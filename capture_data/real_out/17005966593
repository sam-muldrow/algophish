<script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script type="text/javascript">
	var currentBcAccID = "";
	function manageLiveChat(logged, userVisitName, userAccountId, userVisitRef, userBcBdid, userDepartmentId, userWindowId){
		var chatEnabled = "true";
		if(chatEnabled === "true"){
			if(logged){
				if(!window._bcvma && userAccountId != currentBcAccID){
					initLiveChat(userVisitName, userAccountId, userVisitRef, userBcBdid, userDepartmentId, userWindowId);
					currentBcAccID = userAccountId;
				}
				$("#chatLi").show();
			}
			else{
				/*hide LiveChat for not logged*/
				$("#chatLi").hide();
				$("#chatLi").empty();
				currentBcAccID = "";
			}
		}
	}
	
	function startChat(){
		var p = document.querySelector(".bcFloat img");
		if (!document.getElementById("bc-frame")
				&& !document.getElementById("nanorep-fw") && !!p)
			p.click();
	}
	function initLiveChat(userVisitName, userAccountId, userVisitRef, userBcBdid, userDepartmentId, userWindowId) {
		var bcAccountID = "159106769686747994";
		var bcWebsiteID = "160058034463408041";
		var bcInvitationID = "159804181480500699";
		var bcBdidDefaultLogged = "159804181422302553";
		var bcBdidDefaultNotLogged = "159804181422302553";
		var chatDomainBoldChat = "vmss-eu.boldchat.com/aid/";
		var visitName = userVisitName ?  userVisitName : "";
		var accountId = userAccountId ? userAccountId : "";
		var visitRef = userVisitRef ? userVisitRef : "";
		var bcBdid = bcBdidDefaultNotLogged;
		var windowId = userWindowId ? userWindowId : "";
		var departmentId = userDepartmentId ? userDepartmentId : "";
		
		if (visitName && accountId) {
			bcBdid = userBcBdid ? userBcBdid : bcBdidDefaultLogged;
		}
		$("#chatLi").html("<a href='#'><div id=''><span>CHAT</span><span class='Euicoico_chat'></span></div></a>");
		$("#chatLi").off("click",callBoldChatClick);
		$("#chatLi").on("click",callBoldChatClick);
		obtainLiveChat(bcAccountID, bcWebsiteID, bcInvitationID, visitName, accountId, visitRef, bcBdid, windowId, departmentId, chatDomainBoldChat); 
	}
	function callBoldChatClick(){
		if(typeof startChat == 'function'){
			startChat();
		}else{
			console.log("function startChat boldchat doesn't exists ");
		}
	}
	function obtainLiveChat(bcAccountID, bcWebsiteID, bcInvitationID, visitName, accountId, visitRef, bcBdid, windowId, departmentId,chatDomainBoldChat) {
		

	//var bccbId = "chatLi";
		window._bcvma = window._bcvma || [];
		_bcvma.push([ "setAccountID", bcAccountID ]);
		if (bcWebsiteID)
			_bcvma.push([ "setParameter", "WebsiteID", bcWebsiteID ]);
		_bcvma.push([ "setParameter", "CustomUrl", "" ]);
		_bcvma.push([
				"setParameter",
				"WindowParameters",
				"vr=" + visitRef + "&vi=" + accountId + "&ve=&vp=&vn= "
						+ encodeURIComponent(visitName) + "&lc=" + siteSettings.languageCode ]);
		if (bcBdid)
			_bcvma.push([ "addFloat", {
				type : "chat",
				id : bcBdid
			} ]);
		if (bcInvitationID)
			_bcvma.push([ "setParameter", "InvitationID", bcInvitationID ]);

		var bcLoad = function() {
			if (window.bcLoaded)
				return;
			window.bcLoaded = true;
			var vms = document.createElement("script");
			vms.type = "text/javascript";
			vms.async = true;
			vms.src = ('https:' == document.location.protocol ? 'https://'
					: 'http://')
					+ chatDomainBoldChat
					+ bcAccountID
					+ "/bc.vms4/vms.js";
			var s = document.getElementsByTagName('script')[0];
			s.parentNode.insertBefore(vms, s);
		};
		var BCaddEventHandler = function(elem, eventType, handler) {
			if (elem.addEventListener)
				elem.addEventListener(eventType, handler, false);
			else if (elem.attachEvent)
				elem.attachEvent('on' + eventType, handler);
		}
		if (window.pageViewer && pageViewer.load)
			pageViewer.load();
		else if (document.readyState == "complete")
			bcLoad();
		else
			BCaddEventHandler(window, "load", bcLoad);
	}
</script>
