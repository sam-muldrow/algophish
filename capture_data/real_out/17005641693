		  	function clickHitRecord(queryId, objectId, position, link){
		  		aa('clickedObjectIDsAfterSearch', {
			      eventName: 'CLICK_HIT',
			      index: 'deals_ph',
			      queryID: queryId,
			      objectIDs: [objectId],
			      positions: [parseInt(position, 10)],
			    });
			    
			    if(navigator.cookieEnabled){
				    setCookies('queryId', queryId);
				    setCookies('objectId', [objectId]);
				    setCookies('position', [parseInt(position, 10)]);
				    setCookies('clickedDeal', link);
				    console.log('cookie: ' + getCookie('queryId'));
				    console.log('cookie: ' + getCookie('objectId'));
				    console.log('cookie: ' + getCookie('position'));
				    console.log('cookie: ' + getCookie('clickedDeal'));
				}
		  	}

		  	function setCookies(cookieName, cookieValue){
		  		var exdate=new Date()
	            exdate.setDate(exdate.getDate()+1);
		  		document.cookie = cookieName + "=" + cookieValue + ";expires="+exdate.toGMTString() + "; domain=metrodeal.com; path=/";
		  	}

		  	function getCookie(cName){
		  		var re = new RegExp(cName + "=([^;]+)");
			    var value = re.exec(document.cookie);
			    return (value != null) ? unescape(value[1]) : null;
		  	}

		  	function checkConversion(){
		  		if (/order/.test(window.location.href)){
					aa('convertedObjectIDsAfterSearch', {
				      	eventName: 'CONVERT_HIT',
				      	index: 'deals_ph',
				      	queryID: getCookie('queryId'),
				      	objectIDs: [getCookie('objectId')],
				    });
				    // console.log('done');
			  	}
			  	else console.log('not order');
		  	}
