	    <script>

	            var $head = $( '#ha-header' );
	            var $topbar = $( '#top-bar' );
	            $( '.ha-waypoint' ).each( function(i) {
	                var $el = $( this ),
	                    animClassDown = $el.data( 'animateDown' ),
	                    animClassUp = $el.data( 'animateUp' );

	                $el.waypoint( function( direction ) {
	                    if( direction === 'down' && animClassDown ) {
	                        $head.attr('class', 'ha-header ' + animClassDown);
	                        $head.addClass('custom-logo');
	                    }
	                    else if( direction === 'up' && animClassUp ){
	                        $head.attr('class', 'ha-header ' + animClassUp);
	                        $head.addClass('custom-logo');
	                    }
	                }, { offset: '100%' } );
	            } );

	            $(document).scroll(function () {
	                //Show element after user scrolls 800px
	                var y = $(this).scrollTop();
	                if(isDeal()){
		                if (y > 300) {
		                    $('#summary').fadeIn();
		                } else {
		                    $('#summary').fadeOut();
		                }
		            }
	            });
	            window.onscroll = function() {scrollFunction()};

	            function isDeal(){
	            	if(window.location.href.indexOf("/deals") > -1){
	            		return true;
	            	}
	            	return false;
	            }

	            function scrollFunction() {
	              if (document.body.scrollTop > 20 || document.documentElement.scrollTop > 450) {
	              	$( 'div#metroheader' ).addClass("shrink");
	              	$( '.tab-links' ).addClass("shrink");
	              	(document.getElementById("ha-header")).classList.remove("ha-header-large");
	              	$('header#ha-header').addClass("ha-header-small");
	              	if(isDeal()){
	                	document.getElementById("summary").style.bottom = "0";
	                }
	              } else {
	                (document.getElementById("ha-header")).classList.remove("ha-header-small");
	              	$('header#ha-header').addClass("ha-header-large");
	              	$( '.tab-links' ).removeClass("shrink");
	              	(document.getElementById("metroheader")).classList.remove("shrink");
	              	if(isDeal()){
	                	document.getElementById("summary").style.bottom = "-109px";
	                }
	              }
	            }
