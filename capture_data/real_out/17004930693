    function grt_ri(ele) {
        var $ele = $(ele);
        if ($ele.length > 0 && $.trim($ele.val()).length > 0 && $ele.valid() && !isInternalAccess() && $("#googleretargeting").size() == 0) {
            g_track();
        }
    }

