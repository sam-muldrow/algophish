<!--<script type="text/javascript">
	function countTime( a ) {
		//获取当前时间
		var date = new Date();
		var now = date.getTime();
		//设置截止时间
		var str= $(a).data('time');
		var endDate = new Date(str);
		var end = endDate.getTime();

		//时间差
		var leftTime = end-now;
		//定义变量 d,h,m,s保存倒计时的时间
		var d,h,m,s;
		if (leftTime>=0) {
			d = Math.floor(leftTime/1000/60/60/24);
			h = Math.floor(leftTime/1000/60/60%24);
			m = Math.floor(leftTime/1000/60%60);
			s = Math.floor(leftTime/1000%60);
			//将倒计时赋值到div中
			$(a).find("p").html("倒计时：");
			$(a).find(".time_d").html(d+"天");
			$(a).find(".time_h").html(h+"时");
			$(a).find(".time_m").html(m+"分");
			$(a).find(".time_s").html(s+"秒");
			//递归每秒调用countTime方法，显示动态时间效果
			setTimeout(function(){
				countTime(a);
			},1000);
		}else{
			$(a).html("欢迎加入新社群");
		}




	}
	countTime(".time_1");
	countTime(".time_2");
	countTime(".time_3");
	countTime(".time_4");
</script>-->

