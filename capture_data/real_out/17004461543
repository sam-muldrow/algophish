<script defer src="https://cdn.liga.net/assets/js/global.js?v=2"></script>
<script defer src="https://cdn.liga.net/pages/index/index.js?v=20-11-2023-04-09-14"></script>
<script defer src="https://cdn.liga.net/pages/index/slider.js?v=2"></script>
<script>


    if (!isHideAdvert()) {

        var head = document.getElementsByTagName('head')[0];
        var script = document.createElement('script');
        script.type = 'text/javascript';
        script.async = 'true';
        script.src = "https://securepubads.g.doubleclick.net/tag/js/gpt.js";
        head.appendChild(script);

        let catfishHidden = false,
            catfishShow = false;

        function hideCatfishAd() {
            document.getElementById('catfish-wrapper').style.cssText = "display: none;visibility: hidden";
            catfishHidden = true
        }

        document.addEventListener("scroll", function(){
            let scroll = window.scrollY;
            let adInAdBLock = document.getElementById('div-gpt-ad-RICHNEW').childNodes[0];
            if (!adInAdBLock) return;

            if (scroll >= 300 && !catfishHidden && !catfishShow && adInAdBLock.childNodes[0] !== undefined){
                console.log('rich rendered');
                document.getElementById('catfish-close-btn').style.cssText = "display: block;visibility: visible";
                document.getElementById('catfish-wrapper').style.cssText = "display: block;visibility: visible";
                document.getElementById('div-gpt-ad-RICHNEW').style.display = "block";
                catfishShow = true;
            }

        })
    } else {
        const blocksToRemove = document.querySelectorAll('.js-b-block');
        blocksToRemove.forEach(block => {
            block.remove();
        });
    }

</script>
<script>

    let flagA = false;

    function moveAdBlock() {
        const screenWidth = window.innerWidth;

        const pcBanner = document.querySelector('.js-premium-banner');
        const mobBanner = document.querySelector('.js-mob-banner-mv');

        if (mobBanner) {
            const pcBannerInner = pcBanner.querySelector('.js-b-block__banner');
            const mobBannerInner = mobBanner.querySelector('.js-b-block__banner');

            if (mobBanner && screenWidth < 992) {
                if (!flagA) {
                    mobBanner.insertBefore(pcBannerInner, mobBanner.firstChild);
                    flagA = true;
                }
            } else {
                if (flagA) {
                    pcBanner.insertBefore(mobBannerInner, pcBanner.firstChild);
                    flagA = false;
                }
            }
        }
    }


    document.addEventListener('DOMContentLoaded', moveAdBlock);
    window.addEventListener('resize', moveAdBlock);

    var googletag = googletag || {};
    googletag.cmd = googletag.cmd || [];
    googletag.cmd.push(function () {

        if (!isHideAdvert()) {
            console.log('!isHideAdvert banners')

            var mapping_top2 = googletag.sizeMapping().addSize([1500, 0], [[960, 90], [980, 120], [970, 90], [980, 90], [960, 180], [728, 90], [970, 250]]).//desktop
                addSize([1000, 0], [[960, 90], [980, 120], [970, 90], [980, 90], [960, 180], [728, 90], [970, 250]]).//desktop
                addSize([768, 0], [728, 90]).//tablet
                addSize([480, 0], [[300, 50], [300, 100]]).//mobile
                addSize([0, 0], [[300, 50], [300, 100]]).//other
                build();

            var mapping_premium = googletag.sizeMapping().addSize([1500, 0], [[300, 600], [300, 250]]).//desktop
                addSize([1024, 0], [[300, 600], [300, 250]]).//desktop
                addSize([768, 0], [640, 220]).//tablet
                addSize([480, 0], [[300, 600], [300, 250]]).//mobile
                addSize([0, 0], [[300, 600], [300, 250]]).//other
                build();

            var mapping_premium2 = googletag.sizeMapping().addSize([1050, 0], [300, 250]).//desktop
                addSize([1024, 0], [300, 250]).//desktop
                addSize([768, 0], [300, 250]).//tablet
                addSize([480, 0], [300, 250]).//mobile
                addSize([0, 0], [300, 250]).//other
                build();
        }



        if (page_url == "mainpage" || page_url == "/ua" || page_url == "/en") {
            var mapping_top = googletag.sizeMapping().addSize([1500, 0], [960,180]).//desktop
                addSize([1000, 0], [960,180]).//desktop
                addSize([768, 0], [728, 90]).//tablet
                addSize([480, 0], []).//mobile
                addSize([0, 0], []).//other
                build();

            var mapping_rich = googletag.sizeMapping().
            addSize([1500, 0], []). //desktop
                addSize([1024, 0], []). //desktop
                addSize([768, 0], []). //tablet
                addSize([480, 0], [[320, 240], [320, 100], [320,50], [300,100]]). //mobile
                addSize([0, 0], [[320, 240], [320, 100],[320,50], [300,100], [320,200]]). //other
                build();
        } else {
            var mapping_top = googletag.sizeMapping().addSize([1500, 0], [960,180]).//desktop
                addSize([1000, 0], [960,180]).//desktop
                addSize([768, 0], [728, 90]).//tablet
                addSize([480, 0], []).//mobile
                addSize([0, 0], []).//other
                build();

            var mapping_rich = googletag.sizeMapping().
            addSize([1500, 0], [[1440, 180], [960, 200], [960, 120]]). //desktop
                addSize([1024, 0], [[960, 200], [960, 120], [728, 90], [728, 180]]). //desktop
                addSize([768, 0], [[728, 90], [728, 180]]). //tablet
                addSize([480, 0], [[320, 240], [320, 100], [320,50], [300,100]]). //mobile
                addSize([0, 0], [[320, 240], [320, 100],[320,50], [300,100], [320,200]]). //other
                build();
        }


        googletag.pubads().enableSingleRequest();
        googletag.pubads().collapseEmptyDivs();
        googletag.pubads().setTargeting('domain', page_domain);
        googletag.pubads().setTargeting('page_url', page_url);
        googletag.pubads().setTargeting('razdel', page_razdel);
        googletag.pubads().setTargeting('rubric', page_rubric);
        googletag.pubads().setTargeting('tags', page_tags);
        googletag.pubads().setTargeting('news_id', page_id);
        googletag.pubads().setTargeting('theme_id', page_theme_id);
        googletag.enableServices();

        // DFP units
        const windowWidth = document.documentElement.clientWidth;

        var premium = googletag.defineSlot('/2966392/liganet_premium', [[300, 600], [300, 250], [640, 220]], 'div-gpt-ad-PREMIUM')
        .defineSizeMapping(mapping_premium)
        .addService(googletag.pubads());

        function moveAupdatedBlock() {
            googletag.pubads().refresh([premium]);
        }

