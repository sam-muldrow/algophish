                            //googletag.pubads().refresh([premium2]);
                            document.querySelector('.premium-2d').setAttribute('style', 'display: block !important');
                        }
                        if(premuimFirstHeightD >= 590 || premuimFirstHeightD == 0){
                            document.querySelector('.premium-1d').parentNode.nextElementSibling.setAttribute('style', 'display: none !important');
                            document.querySelector('.premium-1d').classList.add('premium-plashka');
                            console.log('premium2d close');
                        }}

                    var premium1x_el_next=document.querySelector('.premium-1x > div');
                    if (premium1x_el_next !== undefined && premium1x_el_next) {
                        var premuimFirstHeightX = premium1x_el_next.getBoundingClientRect().height;
                        if(premuimFirstHeightX < 400 && premuimFirstHeightX > 0){
                            premium2 = googletag.defineSlot('/2966392/liganet_premium2', [300, 250], 'div-gpt-ad-1501856944836-7').defineSizeMapping(mapping_premium2).addService(googletag.pubads());
