        googletag.pubads().addEventListener('slotRenderEnded', function(event) {
            var slotElementId = event.slot.getSlotElementId();
            var newsid = page_id;

            if (slotElementId == "div-gpt-ad-BRANDING") {
                if(event.isEmpty == false) {
                    document.querySelector('body').setAttribute("data-branding", true);
                }
            }


            if (slotElementId == "div-gpt-ad-PREMIUM") {
                // do something related to the slot
                if(event.isEmpty == false) {
                    var premium_width = document.getElementById("google_ads_iframe_/2966392/liganet_premium_0").getAttribute("width");
                    var premium_height = document.getElementById("google_ads_iframe_/2966392/liganet_premium_0").getAttribute("height");
                    document.getElementById("google_ads_iframe_/2966392/liganet_premium_0").setAttribute('style', 'width:' + premium_width + 'px !important;' + ' height:' + premium_height + 'px !important;');
                    var premium1d_el_next=document.querySelector('.premium-1d > div');
                    if (premium1d_el_next !== undefined && premium1d_el_next) {
                        var premuimFirstHeightD = premium1d_el_next.getBoundingClientRect().height;
                        if(premuimFirstHeightD < 400 && premuimFirstHeightD > 0){
                            premium2 = googletag.defineSlot('/2966392/liganet_premium2', [300, 250], 'div-gpt-ad-1501856944836-7').defineSizeMapping(mapping_premium2).addService(googletag.pubads());
