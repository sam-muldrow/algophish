    <script>
        $(function () {$h.a2s.sendContentsLog('POTALTOPJP' , '{"Name":"PotalTopPage"}');})

        function letsPlayDetailOpen(obj, nameOption) {
            if (!nameOption) {
                return;
            }

            if ($(obj).parent("li").hasClass("open")) {
                return;
            }

            $h.a2s.sendContentsLog('POTALTOPGAMEJP', '{"Name":"' + nameOption + '"}');
        }

        function bannerAllow_Click(nameOption) {
            if (!nameOption) {
                return;
            }
            $h.a2s.sendClickLog('POTALTOPGAMEJP', '{"Name":"' + nameOption + '"}');
        }

        function letsPlayDetailAllow_Click(nameOption) {
            if (!nameOption) {
                return;
            }
            $h.a2s.sendClickLog('POTALTOPGAMEJP', '{"Name":"' + nameOption + '"}');
        }
    </script>

	
