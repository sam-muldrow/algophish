@media (min-width: 1300px) {
    .top-banner--above-header + .lp-header .menu-wrapper {
        top: 116px;
    }
}
</style>
<script>
(() => {
    let banner = document.querySelector('.top-banner--above-header')
    if(banner !== null && window.innerWidth > 1299) {
        let dropdowns = [...document.querySelectorAll('.menu-wrapper')]
        let headerHeight = document.querySelector('.lp-header').offsetHeight;
        const bannerHeight = banner.offsetHeight;

        function position() {
            const scrollDepth = window.scrollY;
            const subtract = (bannerHeight - scrollDepth) > -1 ? bannerHeight - scrollDepth : 0;
            if(window.innerWidth > 1299) {
                dropdowns.forEach((drop) => {
                    drop.setAttribute('style', `top: ${subtract + headerHeight}px`);
                })
            } else {
                dropdowns.forEach((drop) => {
                    drop.setAttribute('style', `top: 0px`);
                })
            }
        }

        window.addEventListener('scroll', position)
        window.addEventListener('resize', position)
    }
})();

(()=>{
    const popup = document.querySelector('.side-popup');
    const alreadySeen = sessionStorage.getItem('sticky-banner-shown');
    if(alreadySeen && popup !== null) {
        popup.style.display = 'none'
    }
})();
</script><script type="text/javascript">window.optmzrData=window.optmzrData||{};optmzrData["7"] = "(topbanner fix):(Control)";</script>
