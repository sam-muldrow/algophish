<script>
    window.jobbio_widgets = window.jobbio_widgets || { cmd: [] };
    function initJobbioWidget(id) {
      const $jobbio = $('#' + id);
      if (typeof jobbio_display === 'undefined' || $jobbio.length === 0) {
        return;
      }

      setTimeout(() => {
        const showWidget = typeof window.jobbio_widgets[id] === 'function' ? window.jobbio_widgets[id]($jobbio)
            : true;
        if (!showWidget) {
          return;
        }

        jobbio_display.widget({
          container: id,
          style: $jobbio.data('widget'),
          slug: $jobbio.data('slug'),
          count: $jobbio.data('count'),
          type: $jobbio.data('type'),
          content: 'jobs'
        })
      }, 0);
    }

    function jobbioLoaded() {
      $(() => {
        window.jobbio_widgets.cmd.forEach(initJobbioWidget);
        window.jobbio_widgets.cmd = {push: initJobbioWidget};
      })
    }
    </script>
