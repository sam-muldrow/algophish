<script>
  $(".Swiperbanner-slide a").on('click',function(){
    var lsindex = $(this).closest(".Swiperbanner-slide").index(".Swiperbanner-slide");
    var lspos = lsindex;
    var lsOrd;
    switch (lspos) {
      case 1:
        lsOrd = "primo";
        break;
      case 2:
        lsOrd = "secondo";
        break;
      case 3:
        lsOrd = "terzo";
        break;
      case 4:
        lsOrd = "quarto";
        break;
      case 5:
        lsOrd = "quinto";
        break;
      case 6:
        lsOrd = "sesto";
        break;
      case 7:
        lsOrd = "settimo";
        break;
      case 8:
        lsOrd = "ottavo";
        break;
      case 9:
        lsOrd = "nono";
        break;
      case 10:
        lsOrd = "decimo";
        break;
    }
    if(window.dataLayer) {
      dataLayer.push({
        'event':'GAEvent',
        'event_name':'slider_banner',
        'link_text':'lsOrd'
      });
      //console.log ("slider_banner, " + lsOrd);
    }    
  });
</script>
</div>

