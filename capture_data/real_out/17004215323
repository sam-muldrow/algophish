</div><script>  
$(document).ready(function(){ 
    var countswiperenterprise = 0;
    //console.log(countswiperenterprise);
    $("#HomeTabsNavA2").on('click',function(){ 
      $("#tabhome2").addClass("active");
        countswiperenterprise++;
        if ($("#tabhome2").hasClass("active")) {
          if((countswiperenterprise) == 1) {
            initSwiper(); 
            //console.log('click initSwiper');
            //console.log(countswiperenterprise);
          }
        };
    });
});
  
function initSwiper(){
    var swiperenterprisehome = new Swiper('.SwiperEnterpriseHome', {
      // Disable preloading of all images
      preloadImages: false,
      // Enable lazy loading
      lazy: true,
      slidesPerView: 1,
      spaceBetween: 30,
      slidesPerGroup: 1,
      // init: false,
      pagination: {
        el: '.swiper-pagination',
        clickable: true,
      },
      a11y: {
        paginationBulletMessage: 'Vai alla slide {{index}}',
      },
      breakpoints: {
        481: {
          slidesPerView: 2,
          spaceBetween: 30,
          slidesPerGroup: 2,
        },
        992: {
          slidesPerView: 3,
          spaceBetween: 30,
          slidesPerGroup: 3,
        },
        1200: {
          slidesPerView: 3,
          spaceBetween: 30,
          slidesPerGroup: 3,
        },
      }
  })
};

</script></div>

