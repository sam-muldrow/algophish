    (function (d, s, id, uri) {
        const lang = "it";
        const banner = init_Promo_Black_Friday(lang);
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) {
            return;
        }
        js = d.createElement(s);
        js.id = id;
        js.onload = function () {
            try {
                const options = {
                    htmlBanner: banner['html'],
                    cssBanner: banner['css'],
                    footerEl: '#Footer',
                    viewAfterScroll: 200,
                    cookieName: "promoBlackFriday",
                    deniedURLs: ["/en/", "/es/"]
                }
                const sticky = new Sticky(options);
            } catch (error) {
                console.log(error);
            }
        };
        js.src = uri;
        fjs.parentNode.insertBefore(js, fjs);
