<script>
    document.addEventListener('DOMContentLoaded', function () {

        // 1秒後にクラスを付与
        setTimeout(function () {
            document.getElementById("floating").classList.add('floatingShow');
        }, 1000);

        // ×クリック
        document.getElementById('floatingBtn').addEventListener('click', () => {
            const keyName = 'tabviewed';
            const keyValue = true;

            if (!sessionStorage.getItem(keyName)) {
                sessionStorage.setItem(keyName, keyValue);
                document.getElementById("floating").classList.add('floatingNone');
            }
        });

        // ×クリック後
        const keyName = 'tabviewed';
        const keyValue = true;
        if (sessionStorage.getItem(keyName)) {
            document.getElementById("floating").classList.add('floatingNone');
        }

    });

</script>
    
