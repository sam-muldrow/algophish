jQuery(function($) {
$('.pb_news_tab li a').click(function(){
var parent = $(this).parent();
$('.pb_news_tab li.curr').removeClass('curr');
parent.addClass('curr');
$('.pb_news_container').stop().animate({'margin-left':parent.index() * -672});
return false;
});
$('.pt_rtsb li a.text').click(function(){
var parent = $(this).parent();
if (parent.hasClass('curr')) {return false;}
var curr = $('.pt_rtsb li.curr');
curr.find('img').fadeOut();
curr.removeClass('curr').stop().animate({'height':40});
parent.stop().addClass('curr').animate({'height':227}).find('img').fadeIn();
return false;
});
$('.pt_rthot_tab li a').click(function(){
var parent = $(this).parent();
$('.pt_rthot_tab li.curr').removeClass('curr');
parent.addClass('curr');
$('.top_news_container').stop().animate({'margin-left':parent.index() * -275});
return false;
});
$('.forum_top_sub li a').click(function(){
var parent = $(this).parent();
$('.forum_top_sub li.curr').removeClass('curr');
parent.addClass('curr');
$('.forum_top').stop().animate({'margin-left':(parent.index() - ($('.forum_top_sub li').length - 1)) * 1000});
return false;
});
$(".pt_rtsb li:first-child a, .pb_news_tab li:first-child a, .pt_rthot_tab li:first-child a, .forum_top_sub li:nth-child(2) a").trigger("click");
