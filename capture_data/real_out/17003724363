            "link": "https://e.cn.miaozhen.com/r/k=2364247&p=8POyq&dx=__IPDX__&rt=2&pro=s&ns=__IP__&ni=__IESID__&v=__LOC__&xa=__ADPLATFORM__&tr=__REQUESTID__&mo=__OS__&m0=__OPENUDID__&m0a=__DUID__&m1=__ANDROIDID1__&m1a=__ANDROIDID__&m2=__IMEI__&m4=__AAID__&m5=__IDFA__&m6=__MAC1__&m6a=__MAC__&m11=__OAID__&m14=__CAID__&m5a=__IDFV__&mn=__ANAME__&m5b=__IDFA1__&m11a=__OAID1__&m14a=__CAID1__&o=https://dev.coc.10086.cn/coc3/canvas/rightsmarket-h5-canvas/online/ppydyjtm?channelCode=P00000052081",
            "startTime": "2023/06/26 0:0:0",
            "endTime": "2023/06/30 0:0:0",
			//"pos": ".g_part1.page_width.m_top35.m_clearfix",    // å¨æ­¤ä½ç½®ä¹ä¸ææ¾ç¤ºï¼ç¨äºé¿å¼æå®åºåå
            "showTime": 5000
        }];
        var curTime = new Date().getTime();
        var isMob = /AppleWebKit.*Mobile/i.test(navigator.userAgent) || (/MIDP|SymbianOS|NOKIA|SAMSUNG|LG|NEC|TCL|Alcatel|BIRD|DBTEL|Dopod|PHILIPS|HAIER|LENOVO|MOT-|Nokia|SonyEricsson|SIE-|Amoi|ZTE/.test(navigator.userAgent));

        // å¾ªç¯å¹¿ååå®¹æ°ç»ï¼éä¸å¤æ­å¹¿åç±»ååæ¾ç¤ºæ¶é´ï¼æ ¹æ®å®éæåµåå«æ¾ç¤º
        for (var i = 0; i < adInfos.length; i++) {
            if (adInfos[i].adType == "float") {
                adFloat(adInfos[i]);
            } else if (adInfos[i].adType == "popup") {
                adPopup(adInfos[i]);
            } else if (adInfos[i].adType == "couplet") {
                adCouplet(adInfos[i]);
            }
        }

        // æµ®å±å¹¿å
        function adFloat(adInfo) {
            if (_isExpires(adInfo.startTime, adInfo.endTime)) {
                // è®¾ç½®å¹¿ååç´ 
                $(".m-ad-float .bigimg").attr("src", adInfo.bigImg);
                $(".m-ad-float .smallimg").attr("src", adInfo.smallImg);
                $(".m-ad-float a").attr("href", adInfo.link);
                var pos = $(adInfo.pos).offset().top;  // å¨æ­¤ä½ç½®ä¹ä¸ææ¾ç¤ºï¼ç¨äºé¿å¼æå®åºåå
                var isShowed = false;
                window.onscroll = function () {
                    if ($(window).scrollTop() >= pos) {
                        $(".m-ad-float").show();
                        if(!isShowed){
                            isShowed = true;
                            setTimeout(function () {
                                $(".m-ad-float-big").fadeOut(200);
                                $(".m-ad-float-small").fadeIn(200);
                            }, adInfo.showTime);
                        }
                    } else {
                        $(".m-ad-float").hide();
                    }
                }
            }
        }
        
        

        // å¼¹çªå¹¿å
        function adPopup(adInfo) {
            if (_isExpires(adInfo.startTime, adInfo.endTime)) {
                // è®¾ç½®å¹¿ååç´ 
                $(".m-ad-popup .m-ad-popup-big .bigimg").attr("src", adInfo.bigImg);
                $(".m-ad-popup .m-ad-popup-small .smallimg").attr("src", adInfo.smallImg);
                $(".m-ad-popup a").attr("href", adInfo.link);
                $(".m-ad-popup").show();
                setTimeout(function(){
                    $(".m-ad-popup-big").fadeOut(200);
                    $(".m-ad-popup-small").fadeIn(200);
                }, adInfo.showTime);
            }
        }
        
        // å¯¹èå¹¿å
        function adCouplet(adInfo) {
            if (_isExpires(adInfo.startTime, adInfo.endTime)) {
                // è®¾ç½®å¹¿ååç´ 
                $(".m-ad-couplet-left .leftimg").attr("src", adInfo.leftImg);
                $(".m-ad-couplet-right .rightimg").attr("src", adInfo.rightImg);
                $(".m-ad-couplet-right a").attr("href", adInfo.link);
                $(".m-ad-couplet-left a").attr("href", adInfo.link);
				
				/*var pos = $(adInfo.pos).offset().top;  // å¨æ­¤ä½ç½®ä¹ä¸ææ¾ç¤ºï¼ç¨äºé¿å¼æå®åºåå
				var isShowed = false;
				window.onscroll = function () {
				    if ($(window).scrollTop() >= pos) {
				        $(".m-ad-couplet").show();
				        if(!isShowed){
				            isShowed = true;
				        }
				    } else {
				        $(".m-ad-couplet").hide();
				    }
				}*/
				
                $(".m-ad-couplet").show();
                function adSize() {
                    if ($(window).width() <= 1450) {
                        var width = !isMob ? $(window).width() : $("body").width();
                        $(".m-ad-couplet").css({"width": width});
                    } else {
                        $(".m-ad-couplet").css({"width": 1462});
                    };
                };
                adSize();
                $(window).resize(function() {
                    adSize();
                });
            }
        }

        // å³é­å¹¿å
        $(".u-ad-close").on("click", function(){
            if($(this).parent(".m-ad-float-big, .m-ad-popup-big").length > 0){
                // å³é­å¤§å¾ï¼æ¾ç¤ºå°å¾
                $(this).parent(".m-ad-float-big, .m-ad-popup-big").siblings(".m-ad-float-small, .m-ad-popup-small").show();
                $(this).parent(".m-ad-float-big, .m-ad-popup-big").remove();
            }else if($(this).parent(".m-ad-float-small, .m-ad-popup-small").length > 0){
                // å³é­å°å¾ï¼ç´æ¥æ¸é¤åç´ 
                $(this).parents(".m-ad").remove();
            }else if($(this).parent(".m-ad").length > 0){
                // å³é­åä¾§å¯¹èæ¶ï¼ç´æ¥æ¸é¤åç´ 
                $(this).parent(".m-ad").remove();
            }
        });

        // æ¯å¦å¨æææå
        function _isExpires(start, end) {
            var startTime = new Date(start).getTime();
            var endTime = new Date(end).getTime();
            if (curTime >= startTime && curTime < endTime) {
                return true;
            } else {
                return false;
            }
        }
		
		// ä¸´æ¶å¢å 
		// var adDate = new Date();
		// var adDay = adDate.getDate();
		// var adMonth = adDate.getMonth() + 1;
		
		// //å¼¹çªå¹¿å3æ3å·ä¸æ¾ç¤ºç¼©æ¾ä½
		// if(adMonth == 3) {
		// 	if(adDay >= 3) {
		// 		$(".m-ad-popup-small").addClass("m-ad-popup-small_none");
		// 	}
		// }
    });
</script>
