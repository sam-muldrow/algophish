<script>
    $(function() {
        if ( $(".image-gallery-wrap").length > 0) { 
                $('.image-gallery').slick({
                    infinite: true,
                    speed: 400,
                    slidesToShow: 3,
                    slidesToScroll: 1,
                    centerMode: true,
                    focusOnSelect: true,
                    variableWidth: true,
                    mobileFirst: true,
                    prevArrow: "<button class='slick-prev' aria-label='Previous' type='button' id='image-gallery-previous'>Previous</button>",
                    nextArrow: "<button class='slick-next' aria-label='Next' type='button' id='image-gallery-next'>Next</button>"
                });
                $('.image-gallery').slickLightbox({
                    caption: 'caption',
                    itemSelector: 'div > figure > a'
                });
        }
        if ( $(".value-slider-wrap").length > 0) { 
                $('.single-item').slick({
                    dots: true,
                    appendDots:'.dots-container',
                    infinite: true,
                    speed: 450,
                    slidesToShow: 1,
                    slidesToScroll: 1,
                    mobileFirst:true,
                    prevArrow: "<button class='slick-prev' aria-label='Previous' type='button' id='value-slider-previous'>Previous</button>",
                    nextArrow: "<button class='slick-next' aria-label='Next' type='button' id='value-slider-next'>Next</button>"
                });
        }
    }) 
     </script>
