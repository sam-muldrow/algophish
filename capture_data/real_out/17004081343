    <script>
        var mobile = window.innerWidth <768;
        var video = document.getElementById("vid");
        var videoMobile = document.getElementById("vidMobile");
        var button = document.getElementById("pause-play-video");
        button.style.background = "url(/components/img/pause-button.svg) no-repeat";
        window.onload = addAutoplay();
        function addAutoplay() {
            if(window.innerWidth <768){
                video.style.display = "none";
                button.style.background = "url(/components/img/play-button.svg) no-repeat";
                button.setAttribute("aria-label", "Play video");
            };
            if(window.innerWidth >767){
                videoMobile.style.display = "none";
                video.load();
                video.autoplay = true;
            };
        }
        function Play() {
            if(mobile) {
                if(videoMobile.paused && videoMobile.readyState === 4) {                    
                videoMobile.play();
                button.style.background = "url(/components/img/pause-button.svg) no-repeat";
                button.setAttribute("aria-label", "Pause video");
                } else if(videoMobile.paused && videoMobile.readyState !== 4) {
                videoMobile.load();
                videoMobile.play();
                button.style.background = "url(/components/img/pause-button.svg) no-repeat";
                button.setAttribute("aria-label", "Pause video");
                } else {
                videoMobile.pause();
                button.style.background = "url(/components/img/play-button.svg) no-repeat";
                button.setAttribute("aria-label", "Play video");
            } 
            } else {
                if(video.paused) {
                video.play();
                button.style.background = "url(/components/img/pause-button.svg) no-repeat";
                button.setAttribute("aria-label", "Pause video");
                } else {
                    video.pause();
                    button.style.background = "url(/components/img/play-button.svg) no-repeat";
                    button.setAttribute("aria-label", "Play video");
                }
            }            
        }
    </script>
</section>
