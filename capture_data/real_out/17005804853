        $mainSlider.slick('slickFilter', function(index){
            var $this = $(this);
            if ($this.find('.banner-item').hasClass('non-login-only')) {
                if (!isMember) {
                    return $this.eq(index);
                }
            } else {
                return $this.eq(index);
            }
            if (isMember && $this.hasClass('non-login-only')) {
            }
        });

