                        $('#recommend-package-list-area').find('.btn-badge-bookmark').sokmilBookmarkButton({
            token: csrfToken,
            gaEventAction: 'Top',
            gaEventLabel: 'Badge',
            delegateSelector: '#recommend-package-list-area .horizontal-list'
        });

        $('#recommend-package-list-area').on('click.areaEvent', '.horizontal-link', function (ev) {
            ev.stopPropagation();
            var $linkElm = $(this);
            var url = $linkElm.attr('href');
            var itemIndex = parseInt($linkElm.data('index'));
            ga_record_event('TopPageIdol', '#recommend-package-list-area', 'Item:' + itemIndex, {
                'hitCallback': function () {
                    window.location = url;
                }
            });
            return false;
        });
        $('#promotion-banner-area1').on('click.areaEvent', '.banner-item-link', function (ev) {
            ev.stopPropagation();
            var $linkElm = $(this);
            var url = $linkElm.attr('href');
            ga_record_event('TopPageIdol', '#promotion-banner-area1', 'Item:1:' + encodeURI(url.removeURLParam('ref')), {
                'hitCallback': function () {
                    window.location = url;
                }
            });
            return false;
