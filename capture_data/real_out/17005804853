        $('#promotion-banner-area4').on('click.areaEvent', '.banner-item-link', function (ev) {
            ev.stopPropagation();
            var $linkElm = $(this);
            var url = $linkElm.attr('href');
            ga_record_event('TopPageIdol', '#promotion-banner-area4', 'Item:4:' + encodeURI(url.removeURLParam('ref')));
            window.open(url, '_blank');
            return false;
        });
        var $starRecommendArea = $('#star-tag-list-area');
        var initStarRecommendArea = false;

        function topStarRecommend() {
            if (initStarRecommendArea) {
                return;
            }

            initStarRecommendArea = true;
            $.ajax({
                url: '/action/recommend_star_list/',
                data: {'section': 'idol'},
                dataType: 'json'
            }).done(function (ret) {
                if (!ret || ret.length <= 0) {
                    $starRecommendArea.remove();
                    return;
                }

                var elmId = ret.dataSource + '-star-tag-list-area';

                var $tagRecommend = $starRecommendArea.find('.horizontal-list-area')
                    .attr('id', ret.dataSource + '-star-horizontal-list')
                    .sokmilTagList({
                        addClasses: [''],
                        tagDataList: ret.tagInfos,
                        refCode: 'top_actress',
                        linkCallBack: null,
                        sectionCode: 'idol',
                        tagType: 'staff',
                        isOpenBlankWindow: false,
                        hasPaginateControl: true
                    });

                $starRecommendArea.on('click.areaEvent', '.horizontal-link', function (ev) {
                    ev.stopPropagation();
                    var $linkElm = $(this);
                    var url = $linkElm.attr('href');
                    var itemIndex = parseInt($linkElm.data('index'));
                    ga_record_event('TopPageIdol', '#' + elmId, 'Item:' + itemIndex, {
                        'hitCallback': function () {
                            window.location = url;
                        }
                    });
                    return false;
                }).on('click.areaEvent', '.go-to-list', function (ev) {
                    ev.stopPropagation();
                    var $linkElm = $(this);
                    var url = $linkElm.attr('href');
                    ga_record_event('TopPageIdol', '#' + elmId, 'GoToList', {
                        'hitCallback': function () {
                            window.location = url;
                        }
                    });
                    return false;
                });
            }).fail($.noop());
        }

        window.requestAnimationFrame(function () {
            topStarRecommend();
        });
        $('#promotion-banner-area5').on('click.areaEvent', '.banner-item-link', function (ev) {
            ev.stopPropagation();
            var $linkElm = $(this);
            var url = $linkElm.attr('href');
            ga_record_event('TopPageIdol', '#promotion-banner-area5', 'Item:5:' + encodeURI(url.removeURLParam('ref')), {
                'hitCallback': function () {
                    window.location = url;
                }
            });
            return false;
        });
    });

        var packageSliderList = {};

    function generatePackageSlickSlider(slickSliderId) {
        var winScrollTop = window.pageYOffset || document.documentElement.scrollTop;
        var winHeight = window.innerHeight || document.documentElement.clientHeight || 0;

        if (packageSliderList[slickSliderId].init) {
            return;
        }

        var $slickSliderArea = packageSliderList[slickSliderId].sliderAreaElm;

        if (($slickSliderArea.offset().top > winScrollTop + (winHeight * 1.5))
            || ($slickSliderArea.offset().top + $slickSliderArea.outerHeight(true) < winScrollTop - (winHeight * 0.5))
        ) {
            window.cancelAnimationFrame(packageSliderList[slickSliderId].animationId);
            packageSliderList[slickSliderId].animationId = window.requestAnimationFrame(function () {
                generatePackageSlickSlider(slickSliderId);
            });
            return;
        }

        packageSliderList[slickSliderId].init = true;
        setTimeout(function () {
            var $slickSlider = $slickSliderArea.find('.horizontal-list');
            var responsiveData = getPackageSlickSliderResponsiveData('large');

            $slickSlider
                .show()
                .on('init', function () {
                    $slickSliderArea.find('.loader-img').remove();
                })
                .on('afterChange', function (slick, currentSlide) {
                                        if (!window.IntersectionObserver) {
                        $(this).find('.lazy-img.b4-load').sokmilLazyLoad();
                    }
                                    })
                .slick({
                    infinite: false,
                    prevArrow: '<span class="list-ctrl-area prev"></span>',
                    nextArrow: '<span class="list-ctrl-area next"></span>',
                    respondTo: 'slider',
                    responsive: responsiveData,
                    zIndex: 0
                })
                .find('.btn-badge-bookmark').sokmilBookmarkButton({
                    token: csrfToken,
                    gaEventAction: 'Top',
                    gaEventLabel: 'Badge',
                    delegateSelector: '#' + slickSliderId
                });
                    if (!window.IntersectionObserver) {
                $slickSlider.find('.lazy-img.b4-load').sokmilLazyLoad();
            }
                }, 500);
    }

    $(function () {
        if (typeof $.fn.slick === 'function') {
            if (!packageSliderList['sale-39906-horizontal-list']) {
                packageSliderList['sale-39906-horizontal-list'] = {
                    init: false,
                    animationId: false,
                    sliderAreaElm: $('#sale-39906-horizontal-list')
                };
            }

            packageSliderList['sale-39906-horizontal-list'].animationId = window.requestAnimationFrame(function() {
                generatePackageSlickSlider('sale-39906-horizontal-list');
            });
        } else {
            var $packageListArea0 = $('#sale-39906-horizontal-list');
            $packageListArea0.find('.loader-img').hide();
            $packageListArea0.find('.horizontal-list').show();
            $packageListArea0.ctrlHorizontalList('init');
                        $packageListArea0.find('.btn-badge-bookmark').sokmilBookmarkButton({
                token: csrfToken,
                gaEventAction: 'Top',
                gaEventLabel: 'Badge',
                delegateSelector: '#sale-39906-horizontal-list'
            });
        }
    });

