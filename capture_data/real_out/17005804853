<script>
    $(function () {
        var $linkItems = $('.js-menu-account-link');
        $linkItems.each(function () {
            var $linkItem = $(this);
            var gaEventSuffix = $linkItem.data('ga-event-suffix');
            $linkItem.on('click', function (ev) {
                ev.preventDefault();
                ga_record_event('Click', 'Menu--' + gaEventSuffix, '', {
                    'hitCallback': function () {
                        location.href = $linkItem.data('url');
                    }
                });
            })
        })
    })
</script>
<script>
        $(function () {
        var $window = $(window);
        var $header = $('.js-header');
        var $headerGlobal = $('.js-header-global');

        var setHeaderSizeToCssProperty = function () {
            var heightHeader = 50;
            var heightHeaderGlobal = 50;
            if ($header[0]) {
                heightHeader = $header.outerHeight();
                heightHeaderGlobal = $headerGlobal.outerHeight();
            } else {
                heightHeader = heightHeaderGlobal = $headerGlobal.outerHeight();
            }
            document.body.style.setProperty('--height-header', heightHeader + 'px');
            document.body.style.setProperty('--height-header-global', heightHeaderGlobal + 'px');
        };

        setHeaderSizeToCssProperty();
        var isRunning = false;
        $window.on('resize orientationchange', function () {
            if (!isRunning) {
                isRunning = true;
                window.requestAnimationFrame(function () {
                    setHeaderSizeToCssProperty();
                    isRunning = false;
                });
            }
        });
    });
</script>
<script>
    $(function () {
        var $buttonMenuService = $('.js-menu-service-button');
        var $menuService = $('.js-menu-service');
        var $overlayMenuService = $('.js-overlay-menu-service');
        var $buttonMenuAccount = $('.js-menu-account-button');
        var $menuAccount = $('.js-menu-account');
        var $overlayMenuAccount = $('.js-overlay-menu-account');
        var $menuCtrl = $('.js-menu-ctrl');

        var ctrlVisibleMenu = function ($menu, $overlay, motion) {
            if (motion === 'open') {
                $menu.addClass('is-visible');
                if ($overlay) {
                    $overlay.addClass('is-visible');
                }
                operateFixBody(true);
            } else {
                $menu.removeClass('is-visible');
                if ($overlay) {
                    $overlay.removeClass('is-visible');
                }
                operateFixBody(false);
            }
        };

        $buttonMenuService.on('click', function (ev) {
            ev.preventDefault();
            ctrlVisibleMenu($menuAccount, $overlayMenuAccount, 'close');
            ctrlVisibleMenu($menuCtrl, null, 'close');
            if ($menuService.hasClass('is-visible')) {
                ctrlVisibleMenu($menuService, $overlayMenuService, 'close');
            } else {
                ctrlVisibleMenu($menuService, $overlayMenuService, 'open');
            }
        });
        $overlayMenuService.on('click', function (ev) {
            ev.preventDefault();
            ctrlVisibleMenu($menuService, $overlayMenuService, 'close');
        });

        $buttonMenuAccount.on('click', function (ev) {
            ev.preventDefault();
            ctrlVisibleMenu($menuService, $overlayMenuService, 'close');
            ctrlVisibleMenu($menuCtrl, null, 'close');
            if ($menuAccount.hasClass('is-visible')) {
                ctrlVisibleMenu($menuAccount, $overlayMenuAccount, 'close');
            } else {
                ctrlVisibleMenu($menuAccount, $overlayMenuAccount, 'open');
            }
        });
        $overlayMenuAccount.on('click', function (ev) {
            ev.preventDefault();
            ctrlVisibleMenu($menuAccount, $overlayMenuAccount, 'close');
        });

        var $linkItems = $('.js-global-account-link');
        $linkItems.each(function () {
            var $linkItem = $(this);
            var gaEventSuffix = $linkItem.data('ga-event-suffix');
            $linkItem.on('click', function (ev) {
                ev.preventDefault();
                ga_record_event('Click', 'Global-' + gaEventSuffix, '', {
                    'hitCallback': function () {
                        location.href = $linkItem.data('url');
                    }
                });
            })
        })
    });
</script>
<script type="text/javascript">
        $(function () {
                var $headerMessageTopArea = $('#header-message-top-area');
        if (!navigator.cookieEnabled || !document.cookie) {
            $headerMessageTopArea.append($('<p class="header-message alert">クッキーをONに設定してください。詳しくは<a href="https://www.sokmil.com/about/service/#q5 ">こちら</a></p>'));
        }

                    });

    $(function () {
        Env.check();
        var isOldBrowser = (Env.isSafari && Env.safariVersion.indexOf('9') === 0);

                if (isOldBrowser) {
            var pageHeader = $('#page-header');
            pageHeader.addClass('is-old-browser');
        }

                var $searchFormElm = $('form.search-form');
        $searchFormElm.find('.btn-search').each(function () {
            var $thisElm = $(this);
            $thisElm.click(function () {
                if ($thisElm.parent().children('.qword').val()) {
                    $thisElm.parent().submit();
                }

                return false;
            });
        });
        $searchFormElm.find('.qword').each(function () {
            var $thisElm = $(this);
            $thisElm.keypress(function (ev) {
                if ((ev.which && ev.which === 13) || (ev.keyCode && ev.keyCode === 13)) {
                    return !!$thisElm.val();
                } else {
                    return true;
                }
            });
        });
    });

    $(function () {
        var isHovered = function (element) {
            return $(element + ':hover').length > 0;
        };

                var $myPageElm = $('#head-my-page');
        var $myPageBoxElm = $('#page-my-page-box');

        $myPageElm.on({
            'mouseenter': function (ev) {
                ev.preventDefault();
                var $thisElm = $(this);
                $.wait(300).then(function () {
                    if (!isHovered('#' + $thisElm.attr('id'))) {
                        return;
                    }
                    if ($myPageElm.hasClass('processing')) {
                        return;
                    }
                    $myPageElm.addClass('processing');
                    $myPageBoxElm
                        .stop(true, true)
                        .slideDown(200, function () {
                            $myPageElm.removeClass('processing').addClass('open');
                        });
                });
            },
            'mouseleave': function (ev) {
                ev.preventDefault();
                var $thisElm = $(this);
                $.wait(300).then(function () {
                    if (isHovered('#' + $thisElm.attr('id'))) {
                        return;
                    }
                    if ($myPageElm.hasClass('processing')) {
                        return;
                    }
                    $myPageElm.addClass('processing');
                    $myPageBoxElm
                        .stop(true, true)
                        .slideUp(200, function () {
                            $myPageElm.removeClass('processing').removeClass('open');
                        });
                });
            },
            'click': function (ev) {
                if (Env.isMobile) {
                    ev.preventDefault();
                    var $thisElm = $(this);
                    if ($thisElm.hasClass('processing')) {
                        return;
                    }
                    $myPageBoxElm.stop(true, true);
                    if ($myPageBoxElm[0] && $myPageBoxElm.is(':visible')) {
                        $myPageBoxElm.slideUp(200, function () {
                            $myPageElm.removeClass('open');
                        });
                    } else {
                        $myPageBoxElm.slideDown(200, function () {
                            $myPageElm.addClass('open');
                        });
                    }
                }
            }
        });
        $myPageBoxElm.on('click', 'a', function (ev) {
            ev.preventDefault();
            location.href = $(this).attr('href');
            return false;
        });

                var $pageCartElm = $('#page-cart');
        var $pageCartBoxElm = $('#page-cart-box');

        if ($pageCartBoxElm.length) {
            $pageCartBoxElm.async_cart({
                shopdomain: domain,
                removeurl: 'https://www.sokmil.com/basket/remove/',
                url: 'https://www.sokmil.com/basket/getall/'
            });

            $pageCartBoxElm.on('click', 'a', function (ev) {
                var pageBoxItemUrl = $(this).attr('href');
                if (pageBoxItemUrl) {
                    ev.preventDefault();
                    location.href = $(this).attr('href');
                    return false;
                }
            });
        }

                        $pageCartElm.on({
            'mouseenter': function (ev) {
                ev.preventDefault();
                var $thisElm = $(this);
                $.wait(300).then(function () {
                    if (!isHovered('#' + $thisElm.attr('id'))) {
                        return;
                    }
                    if ($thisElm.hasClass('processing')) {
                        return;
                    }
                    openHeaderCartBox();
                });
            },
            'mouseleave': function (ev) {
                ev.preventDefault();
                var $thisElm = $(this);
                $.wait(300).then(function () {
                    if (isHovered('#' + $thisElm.attr('id'))) {
                        return;
                    }
                    if ($thisElm.hasClass('processing')) {
                        return;
                    }
                    closeHeaderCartBox();
                });
            },
            'click': function (ev) {
                if (Env.isMobile) {
                    ev.preventDefault();
                    var $thisElm = $(this);
                    if ($thisElm.hasClass('processing')) {
                        return;
                    }
                    if ($pageCartBoxElm[0] && $pageCartBoxElm.is(':visible')) {
                        closeHeaderCartBox();
                    } else {
                        openHeaderCartBox();
                    }
                }
            }
        });
        
        
                var $globalNavAreaElm = $('#global-nav-area');
        var globalNavAreaHeight = $globalNavAreaElm.outerHeight(true);
        var globalNavTop = ($globalNavAreaElm.offset()) ? $globalNavAreaElm.offset().top : 0;

                var $globalNavList = $globalNavAreaElm.find('.global-nav-list');
        $globalNavList.find('.global-nav-item').on({
            'mouseenter': function (ev) {
                ev.preventDefault();
                var $thisElm = $(this);
                $.wait(300).then(function () {
                    if (!isHovered('#' + $thisElm.attr('id'))) {
                        return;
                    }
                    var $innerElm = $thisElm.children('.global-nav-inner');
                    $thisElm.addClass('processing');
                    $innerElm
                        .stop(true, true)
                        .slideDown(200, function () {
                            $thisElm.removeClass('processing').addClass('open');
                            if (!window.IntersectionObserver) {
                                $thisElm.find('.lazy-img.b4-load').sokmilLazyLoadImg();
                            }
                        });
                });
            },
            'mouseleave': function (ev) {
                ev.preventDefault();
                var $thisElm = $(this);
                $.wait(300).then(function () {
                    if (isHovered('#' + $thisElm.attr('id'))) {
                        return;
                    }
                    $thisElm.addClass('processing');
                    var $innerElm = $thisElm.children('.global-nav-inner');
                    if ($innerElm[0]) {
                        $thisElm.children('.global-nav-inner')
                            .stop(true, true)
                            .slideUp(200, function () {
                                $thisElm.removeClass('processing').removeClass('open');
                            });
                    } else {
                        $thisElm.removeClass('processing').removeClass('open');
                    }
                });
            },
            'click': function (ev) {
                ev.preventDefault();
                var $thisElm = $(this);
                if ($thisElm.hasClass('link')) {
                    location.href = $thisElm.find('.nav-link').attr('href');
                    return false;
                }
                if ($thisElm.hasClass('processing')) {
                    return;
                }
                var $innerElm = $thisElm.children('.global-nav-inner');
                $innerElm.stop(true, true);
                if ($innerElm[0] && $innerElm.is(':visible')) {
                    $innerElm.slideUp(200, function () {
                        $thisElm.removeClass('open');
                    });
                } else {
                    $innerElm.slideDown(200, function () {
                        $thisElm.addClass('open');
                    });
                }
            }
        }).find('.global-nav-inner').on('click', function (ev) {
            ev.stopPropagation();
        });

                        var $floatGlobalNavArea = $('<div/>')
            .appendTo($globalNavAreaElm)
            .attr('id', 'floating-global-nav-area')
            .addClass('floating-global-nav-area');

        var $btnMenu = $('<span/>')
            .appendTo($floatGlobalNavArea)
            .addClass('btn-header');
        $('<span/>')
            .appendTo($btnMenu)
            .addClass('icon-menu')
            .addClass('icon-menu-large');
        $('<span/>')
            .appendTo($btnMenu)
            .addClass('btn-name')
            .text('カテゴリ');

        var $floatGlobalNavList = $globalNavList.clone(true);
        $floatGlobalNavList
            .appendTo($floatGlobalNavArea)
            .addClass('floating');
        $floatGlobalNavList.find('a').each(function (index, element) {
            var $thisElm = $(element);
            $thisElm.attr('href', $thisElm.attr('href').addURLParam('ref', 'float_header'));
        });

        $floatGlobalNavArea.on({
            'mouseenter': function (ev) {
                ev.preventDefault();
                var $thisElm = $(this);
                $.wait(300).then(function () {
                    if (!isHovered('#' + $thisElm.attr('id'))) {
                        return;
                    }
                    if ($thisElm.hasClass('processing')) {
                        return;
                    }
                    $thisElm.addClass('processing');
                    $floatGlobalNavList
                        .stop(true, true)
                        .slideDown(200, function () {
                            $thisElm.removeClass('processing').addClass('open');
                            if (!window.IntersectionObserver) {
                                $thisElm.find('.lazy-img.b4-load').sokmilLazyLoadImg();
                            }
                        });
                });
            },
            'mouseleave': function (ev) {
                ev.preventDefault();
                var $thisElm = $(this);
                $.wait(300).then(function () {
                    if (isHovered('#' + $thisElm.attr('id'))) {
                        return;
                    }
                    if ($thisElm.hasClass('processing')) {
                        return;
                    }
                    $thisElm.addClass('processing');
                    $floatGlobalNavList
                        .stop(true, true)
                        .slideUp(200, function () {
                            $thisElm.removeClass('processing').removeClass('open');
                        });
                });
            },
            'click': function (ev) {
                ev.preventDefault();
                var $thisElm = $(this);
                if ($thisElm.hasClass('processing')) {
                    return;
                }
                $floatGlobalNavList.stop(true, true);
                if ($floatGlobalNavList.is(':visible')) {
                    $floatGlobalNavList.slideUp(200, function () {
                        $thisElm.removeClass('open');
                    });
                } else {
                    $floatGlobalNavList.slideDown(200, function () {
                        $thisElm.addClass('open');
                    });
                }
            }
        });

        $floatGlobalNavList.find('.global-nav-item').on('mouseenter click', function (ev) {
            ev.stopPropagation();
            var $thisElm = $(this);
            var $innerElm = $thisElm.children('.global-nav-inner');
            var $textElm = $innerElm.prev('.global-nav-item-text');
            $innerElm
                .css({
                    'top': $thisElm.position().top,
                    'width': $textElm.outerWidth(true)
                });
        });

                        var $goToAdult = $('#floating-go-to-adult')
            .css({
                'box-shadow': '0 0 3px 1px rgba(0, 0, 0, 0.2)',
                'height': 50,
                'right': 0,
                'top': 71,
                'width': 120
            });
        
                var $win = $(window);
        $win.on('scroll', function () {
            // メニュー
            if ($win.scrollTop() > globalNavTop + globalNavAreaHeight) {
                $floatGlobalNavArea.show();
            } else {
                $floatGlobalNavArea.hide();
            }

            // カート
            if ($win.scrollTop() > globalNavTop + globalNavAreaHeight) {
                $pageCartElm.addClass('float');
                $myPageElm.addClass('float');

                                $goToAdult.show();
                
            } else {
                $pageCartElm.removeClass('float');
                $myPageElm.removeClass('float');

                                $goToAdult.hide();
                            }
        });
            });
</script>
<script type="text/javascript">
    $(function () {
        var banners = [];
        banners.push('<a class="nav-link" href="https://www.videx.jp/hobby/sp/pachisev/index.htm?ref=sokmil_banner_seven" target="_blank"><img class="nav-item-img no-border lazy-img b4-load" style="height: 60px; width: 150px;" alt="パチスロ サイトセブンTV" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAYAAAAfFcSJAAAADUlEQVQI12NgYGBgAAAABQABXvMqOgAAAABJRU5ErkJggg==" data-src="https://cc.sokmil.com/img/banner/bnr_v_seven.gif" /></a>');
        banners.push('<a class="nav-link" href="https://www.videx.jp/hobby/sp/entermeitele/index.htm?ref=sokmil_banner_entermeitele" target="_blank"><img class="nav-item-img no-border lazy-img b4-load" style="height: 60px; width: 150px;" alt="エンタメ～テレ" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAYAAAAfFcSJAAAADUlEQVQI12NgYGBgAAAABQABXvMqOgAAAABJRU5ErkJggg==" data-src="https://cc.sokmil.com/img/banner/bnr_v_entermeitele.gif" /></a>');
        $('#nav-banner1').html(banners[Math.floor(Math.random() * banners.length)]);
    });

    $(function () {
                var accessed_tags_c = $.cookie("ACCESSED_TAGS_COUNT_IDOL");
        
        var recommend_c = 4;
        if (accessed_tags_c >= recommend_c) {
            $.ajax({
                url: "/action/idol/accessedtags/",
                type: 'GET',
                cache: false
            }).done(function (ret) {
                if (ret.status && ret.status == "ok" && ret.data.length >= recommend_c) {
                    var item_count = 0;
                    var tags = [];
                    var len = 1;
                    var pos = 0;
                    var $accessedTagsElm = $('#nav-accessed-tag');
                    var $listElm = $accessedTagsElm.find('.nav-item-list');
                    var $listItemElm, $linkElm, $textElm;
                    for (var i in ret.data) {
                        item_count++;
                        $listItemElm = $('<li/>');
                        $listItemElm
                            .appendTo($listElm)
                            .addClass('nav-item')
                            .addClass('two-col');
                        if (item_count % 2 == 0) {
                            $listItemElm.addClass('right-col');
                        }

                        $linkElm = $('<a/>');
                        $linkElm
                            .appendTo($listItemElm)
                            .addClass('nav-link')
                            .attr('href', ret.data[i].url.addURLParam('ref', 'menu_rec_tag'));

                        $textElm = $('<span/>');
                        $textElm
                            .appendTo($linkElm)
                            .addClass('nav-item-text')
                            .text(ret.data[i].name);
                    }
                    $accessedTagsElm.slideDown('fast');
                }
            }).fail($.noop());
        }
    });
</script>
<script type="text/javascript">
    var domain = 'sokmil.com';
    
    
    $(function () {
        
    });

    $(function () {
                $('#site-description-area').show();
        $('#site-merit-area').show();
        
        var animation_speed = 300;
        $('div.page-message-box.extend-box .open-message-text').click(function (e) {
            $parent_message_box = $(this).closest('div.page-message-box.extend-box');
            $(this).fadeOut(animation_speed / 2, function () {
                $parent_message_box.find('.close-message-text').fadeIn(animation_speed / 2);
            });
            $parent_message_box.find('.page-message-body').slideDown(animation_speed);
        });
        $('div.page-message-box.extend-box .close-message-text').click(function (e) {
            $parent_message_box = $(this).closest('div.page-message-box.extend-box');
            $(this).fadeOut(animation_speed / 2, function () {
                $parent_message_box.find('.open-message-text').fadeIn(animation_speed / 2);
            });
            $parent_message_box.find('.page-message-body').slideUp(animation_speed);
        });

    });

    $(function () {
                Env.check();
        if (!Env.isMobile) {
            $(document).on({
                    'mouseenter': function () {
                        var $wrapper = $(this);
                        $wrapper.find('.hover-badge').show();
                        if ($wrapper.prop('tagName').toLowerCase() === 'a' || $wrapper.closest('a')[0]) {
                            $wrapper.find('img').css({'opacity': 0.8});
                        }
                    },
                    'mouseleave': function () {
                        var $wrapper = $(this);
                        $wrapper.find('.hover-badge').hide();
                        $wrapper.find('.added').show();
                        if ($wrapper.prop('tagName').toLowerCase() === 'a' || $wrapper.closest('a')[0]) {
                            $wrapper.find('img').css({'opacity': 1});
                        }
                    }
                },
                '.badge-area-wrapper');
        }
    });

    $(function () {
        var $win = $(window);
        var $body = $('html, body');
        var $mainContent = $('#main-content');

        var winOption = {
            'winScrollTop' : window.pageYOffset || document.documentElement.scrollTop,
            'winHeight' : window.innerHeight || document.documentElement.clientHeight || 0,
            'winWidth' : window.innerWidth || document.documentElement.clientWidth || 0
        };

        $('.list-rights-media').find('.btn-add-cart-open').on('click', function (ev) {
                ev.stopPropagation();
                var $cartButton = $(this);
                var targetBoxId = $cartButton.data('target-box');
                var $targetCartBox = $('#' + targetBoxId);

                if ($targetCartBox.is(':visible')) {
                    $targetCartBox.fadeOut('fast', function () {
                        $cartButton.removeClass('open').html('<span class="icon-add-cart">+</span>カートに追加');
                    });
                } else {
                    $('.list-rights-media-button-area').fadeOut('fast', function () {
                        $('.btn-add-cart-open.open').removeClass('open').html('<span class="icon-add-cart">+</span>カートに追加');
                    });

                    var mainWidth = $mainContent.outerWidth(false);
                    var mainOffsetLeft = $mainContent.get(0).offsetLeft;
                    var btnHeight = $cartButton.outerHeight(false);
                    var btnWidth = $cartButton.outerWidth(true);
                    var elmWidth = $targetCartBox.outerWidth(true);
                    var elmVisibleTop = $cartButton.get(0).offsetTop + btnHeight + 15;
                    var elmVisibleLeft = $cartButton.get(0).offsetLeft - Math.floor((elmWidth / 2) - (btnWidth / 2));

                    var orgArrowMarginLeft = -15;
                    var arrowDiff = 0;
                    var orgElmVisibleLeft = elmVisibleLeft;
                    if (mainOffsetLeft + elmVisibleLeft < 0) {
                        elmVisibleLeft = -1 * mainOffsetLeft - 10;
                        arrowDiff = orgElmVisibleLeft - elmVisibleLeft;
                    } else if (winOption.winWidth < elmVisibleLeft + elmWidth + mainOffsetLeft) {
                        elmVisibleLeft = winOption.winWidth - elmWidth - ((winOption.winWidth - mainWidth) / 2);
                        arrowDiff = orgElmVisibleLeft - elmVisibleLeft;
                        arrowDiff = arrowDiff > elmWidth / 2 ? (elmWidth / 2) + orgArrowMarginLeft : arrowDiff;
                    }

                    $targetCartBox.css({
                        left: elmVisibleLeft,
                        top: elmVisibleTop
                    }).fadeIn('fast', function () {
                        $cartButton.addClass('open').text('閉じる');
                    });

                    var newArrowMarginLeft = orgArrowMarginLeft + arrowDiff;
                    $targetCartBox.find('.box-arrow').css({
                        'margin-left': newArrowMarginLeft
                    });
                }

                return false;
            });
        $body.on('click', function () {
            $('.list-rights-media-button-area').fadeOut('fast', function () {
                $('.btn-add-cart-open.open').removeClass('open').html('<span class="icon-add-cart">+</span>カートに追加');
            });
            $('body > div.highimagebox').slideUp(100);
            $('p.product-short-description-box').slideUp(100);
        });
        $win.on('resize', function () {
            $('.list-rights-media-button-area').fadeOut('fast', function () {
                $('.btn-add-cart-open.open').removeClass('open').html('<span class="icon-add-cart">+</span>カートに追加');
            });
            $('body > div.highimagebox').slideUp(100);
            $('p.product-short-description-box').slideUp(100);
            winOption = {
                'winScrollTop' : window.pageYOffset || document.documentElement.scrollTop,
                'winHeight' : window.innerHeight || document.documentElement.clientHeight || 0,
                'winWidth' : window.innerWidth || document.documentElement.clientWidth || 0
            };
        });
        $('.list-rights-media-button-area').find('.btn-add-cart').on('click', function (ev) {
            ev.stopPropagation();
            var $btnAddCart = $(this);
            var ecItem = gaHelper.generateMovieDataToECommerceItem(
                $btnAddCart.data('package-code'),
                $btnAddCart.data('title'),
                $btnAddCart.data('section'),
                $btnAddCart.data('license'),
                $btnAddCart.data('bit-rate-class'),
                $btnAddCart.data('price'),
                1,
                {}
            );
            gaHelper.pushAddToCartEvent(ecItem, function () {
                action_addtocart($btnAddCart.attr('href'));
            });
            return false;
        });

        var $goToTopBtn = $('.go-to-page-top');
        $goToTopBtn
            .hide()
            .on('click', function (e) {
                e.preventDefault();
                $body.animate({
                    scrollTop: 0
                }, 500);
                return false;
            });
        var cssBottomGoToButton = $goToTopBtn.css('bottom');

        $win.on('scroll', function () {
            if ($win.scrollTop() > 100) {
                $goToTopBtn.fadeIn('fast');
            } else {
                $goToTopBtn.fadeOut('fast');
            }
        });

        $('a.scrollto, area.scrollto').click(function (e) {
            e.preventDefault();
            var hash = '#' + (this.href + '').split('#')[1];
            if (hash.length > 1) {
                var $target = $(hash);
                if ($target[0]) {
                    $body.animate({
                        scrollTop: $target.offset().top
                    }, 500);
                }
            }
            return false;
        });

        $('.lazy-img.b4-load').sokmilLazyLoad({addScrollEvent: true});

            });
</script>
