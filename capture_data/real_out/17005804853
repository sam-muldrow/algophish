</script>
<script type="text/javascript">
    $(function () {
        $.ajax({
            url: '/action/package_history/',
            data: {'section': 'idol', 'frmp': 'top'}
        }).done(function (ret) {
            if (ret.historyPackageDetailInfos) {
                var $historyAreaElm = $('#history-package-list-area');
                var $historyPackageListElm = $historyAreaElm.find('.horizontal-list-area')
                    .sokmilPackageList({
                        elmId: $historyAreaElm.attr('id') + '-list-area',
                        sectionCode: 'idol',
                        addClasses: ['large'],
                        packageDataList: ret.historyPackageDetailInfos,
                        refCode: 'top_history',
                        isDisplayAddCart: false,
                        isDisplayMinPrice: true,
                        token: csrfToken,
                        gaEventAction: 'Top'
                    });

                if (!$historyPackageListElm.find('.horizontal-item')[0]) {
                    $historyAreaElm.remove();
                }
            }

            if (ret.historyPackageForRecommend) {
                var $historyRecAreaElm = $('#history-recommend-package-list-area');

                var data = {
                    'type': 'by_packages_packages',
                    'section': 'idol',
                    'pid': JSON.stringify(ret.historyPackageForRecommend),
                    'id': null,
                    'cart': false,
                    'eid': 'product-history-recommend',
                    'frmp': 'top'
                };

                $.ajax({
                    url: '/action/package_recommend/',
                    data: data,
                    dataType: 'json',
                    type: 'get',
                    async: true,
                    cache: true
                }).done(function (ret) {
                    $historyRecAreaElm.find('.loader-area').remove();

                    if (ret === null || typeof ret.packageInfos === 'undefined' || ret.packageInfos.constructor !== Array) {
                        $historyRecAreaElm.remove();
                    } else if (ret.packageInfos.length === 0) {
                        $historyRecAreaElm.remove();
                    } else {
                        var $historyRecPackageListElm = $historyRecAreaElm.find('.horizontal-list-area')
                            .addClass('product-history-list')
                            .sokmilPackageList({
                                sectionCode: 'idol',
                                addClasses: ['large'],
                                packageDataList: ret.packageInfos,
                                refCode: 'top_history_recommend',
                                isDisplayAddCart: false,
                                isDisplayMinPrice: true,
                                token: csrfToken,
                                gaEventAction: 'Top'
                            });
                    }
                }).fail(function (rt, status, error) {
                    if (status !== 'abort') {
                        $historyRecAreaElm.remove();
                    }
                });
            }

        }).fail($.noop());
    });
</script>
<!-- create at 2023/11/21 10:00:05 -->        <div id="left-nav" class="idol">
