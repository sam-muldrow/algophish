        var isRecommendSeparateArea = false;
        var $tagRecommendArea = $('#frequency-tag-area');
        var initTagRecommendArea = false;
        var kickCallbackId = false;

        function topTagRecommend() {
            if (initTagRecommendArea) {
                return;
            }

            initTagRecommendArea = true;
            $.ajax({
                url: '/action/package_list_by_tag_frequency/',
                data: {'section': 'idol', 'frmp': 'top'},
                dataType: 'json'
            }).done(function (ret) {
                if (!ret || ret.length <= 0) {
                    return;
                }

                ret.forEach(function (tagInfo, index) {
                    var $tagArea = $('<div/>')
                        .appendTo($tagRecommendArea)
                        .attr('id', 'frequency-tag-' + tagInfo.tag.tagcode + '-package-list-area')
                        .addClass('partial-area');

                    if (isRecommendSeparateArea || index !== 0) {
                        $tagArea.addClass('area-separate');
                    }
                    var $headlineArea = $('<div/>')
                        .appendTo($tagArea)
                        .addClass('headline-area');

                    var tagTypeName = tagInfo.tag.tagtypename;
                    if (tagInfo.tag.tagtypecode === 'staff') {
                        if (tagInfo.tag.stafftypecode === 'avpornstar') {
                            tagTypeName =  'AV女優';
                        } else if (tagInfo.tag.stafftypecode === 'avpornactor') {
                            tagTypeName =  'AV男優';
                        } else if (tagInfo.tag.stafftypecode === 'model') {
                            tagTypeName =  'グラビアアイドル';
                        } else {
                            tagTypeName =  tagInfo.tag.stafftypename;
                        }
                    }
                    $('<h2/>')
                        .appendTo($headlineArea)
                        .addClass('headline')
                        .text('おすすめ' + tagTypeName + '【' + tagInfo.tag.name + '】');
                    $('<a/>')
                        .appendTo($headlineArea)
                        .addClass('go-to-list')
                        .attr('href', tagInfo.tag.tag_url.addURLParam('ref', 'top_frequency_tag'))
                        .text('もっと見る');

                    $('<div/>')
                        .appendTo($tagArea)
                        .attr('id', tagInfo.tag.tagcode + '-horizontal-list')
                        .addClass('horizontal-list-area')
                        .sokmilPackageList({
                            elmId: 'tag-recommend' + index,
                            sectionCode: 'idol',
                            addClasses: ['large'],
                            packageDataList: tagInfo.package_list,
                            refCode: 'top_frequency_tag',
                            isDisplayAddCart: false,
                            isDisplayMinPrice: true,
                            token: csrfToken,
                            gaEventAction: 'Top'
                        });

                    var row = index + 1;

                    $tagArea.on('click.areaEvent', '.horizontal-link', function (ev) {
                        ev.stopPropagation();
                        var $linkElm = $(this);
                        var url = $linkElm.attr('href');
                        var $item = $linkElm.closest('.slick-slide');
                        var itemIndex = parseInt($linkElm.data('index'));
                        ga_record_event('TopPageIdol', '#frequency-tag-package-list-area' + row, 'Item:' + itemIndex + ':' + tagInfo.tag.tagcode, {
                            'hitCallback': function () {
                                window.location = url;
                            }
                        });
                        return false;
                    }).on('click.areaEvent', '.go-to-list', function (ev) {
                        ev.stopPropagation();
                        var $linkElm = $(this);
                        var url = $linkElm.attr('href');
                        ga_record_event('TopPageIdol', '#frequency-tag-package-list-area' + row, 'GoToList:' + tagInfo.tag.tagcode, {
                            'hitCallback': function () {
                                window.location = url;
                            }
                        });
                        return false;
                    });

                });
            }).fail($.noop());
        }

        window.requestAnimationFrame(function () {
            topTagRecommend();
        });
