    $(function () {
                $('.sokmil_lightbox').sokmilLightbox({
            imgInfos: jacket_urls,
            openImage: function () {
                ga_record_event('Enlarge Image', 'Top', 'Jacket Open');
            },
            nextImage: function () {
                ga_record_event('Enlarge Image', 'Top', 'Jacket Next Slide');
            },
            prevImage: function () {
