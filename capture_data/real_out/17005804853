            var $linkElm = $(this);
            var url = $linkElm.attr('href');
            var itemIndex = parseInt($linkElm.data('index'));
            ga_record_event('TopPageIdol', '#recommend-tag-package-list-area3', 'Item:' + itemIndex + ":" + "v_i_takeshobou", {
                'hitCallback': function () {
                    window.location = url;
                }
            });
            return false;
        }).on('click.areaEvent', '.go-to-list', function (ev) {
            ev.stopPropagation();
            var $linkElm = $(this);
            var url = $linkElm.attr('href');
            ga_record_event('TopPageIdol', '#recommend-tag-package-list-area3', 'GoToList:v_i_takeshobou', {
                'hitCallback': function () {
                    window.location = url;
                }
            });
            return false;
        });
