        var isMember = $body.hasClass('status-ismember');

        var $mainSlider = $('#slider-main');

        $mainSlider.on('init', function(slick){
            $mainSlider.find('.slick-slide').removeClass('changing');
            $mainSlider.find('.list-ctrl-area').fadeIn('fast');
        }).on('reInit', function(slick){
            $mainSlider.find('.slick-slide').removeClass('changing');
            $mainSlider.find('.list-ctrl-area').fadeIn('fast');
        }).on('beforeChange', function(slick, currentSlide, nextSlide){
            $mainSlider.find('.slick-slide').addClass('changing');
            $mainSlider.find('.list-ctrl-area').fadeOut('fast');
        }).on('afterChange', function(slick, currentSlide){
            $mainSlider.find('.slick-slide').removeClass('changing');
            $mainSlider.find('.list-ctrl-area').fadeIn('fast');
        }).on('breakpoint', function (event, slick, breakpoint) {
            slick.$nextArrow.css({right: slick.breakpointSettings[breakpoint].centerPadding});
            slick.$prevArrow.css({left: slick.breakpointSettings[breakpoint].centerPadding});
        }).slick({
//             asNavFor: '#slider-thumbnail',
            autoplay: true,
            autoplaySpeed: 2500,
            centerMode: true,
            centerPadding: '20%',
            infinite: true,
            slidesToShow: 1,
            speed: 1000,
            prevArrow: '<span class="list-ctrl-area prev"></span>',
            nextArrow: '<span class="list-ctrl-area next"></span>',
            respondTo: 'slider',
            responsive: [
                {
                    breakpoint: 2000, // 1361,
                    settings: {
                        centerPadding: '30%'
                    }
                },
                {
                    breakpoint: 1000,
                    settings: {
                        centerPadding: '25%'
                    }
                }
            ],
            zIndex: 0
        });

