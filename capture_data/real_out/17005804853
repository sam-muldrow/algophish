            <script>
        function ga_record_event(category, action, optional_label, optional_value) {
            if (!optional_value) {
                optional_value = [];
            }
            var existsCallback = ('hitCallback' in optional_value && typeof optional_value['hitCallback'] === 'function');
            var callback = existsCallback ? optional_value['hitCallback'] : null;
            gaHelper.pushEvent(
                {
                    'event': 'ua_event',
                    'ua_event_category': category,
                    'ua_event_action': action,
                    'ua_event_label': optional_label,
                    'non_interaction': 'nonInteraction' in optional_value,
                },
                callback
            );
        }
    </script>
    <script type="text/javascript">
    var is_afteronread = false;
    $(document).ready(function () {is_afteronread=true});

    var cart_action = 'ajax';

    function action_addtocart(url) {
        if (cart_action === 'ajax' && is_afteronread) {
            var $a = $('<a href="' + url + '">add</a>');
            $a.async_basket({
                action: 'add',
                callback: function () {
                    $.wait(1000)
                        .then(function () {
                            openHeaderCartBox();

                            return $.wait(2000);
                        })
                        .then(function () {
                            if ($('#page-cart:hover').length == 0) {
                                closeHeaderCartBox();
                            }
                        });
                }
            }).click();
        } else {
            location.href = url;
        }
    }

    $.extend({
        wait: function (msec) {
            return $.Deferred(function (dfd) {
                setTimeout(function () {
                    dfd.resolve();
                }, msec);
            }).promise();
        }
    });

    function openHeaderCartBox() {
        var $parent = $('#page-cart');
        $parent.addClass('processing');
        $('#page-cart-box')
            .stop(true, true)
            .slideDown(200, function () {
                $parent.removeClass('processing').addClass('open');
            });
    }

    function closeHeaderCartBox() {
        var $parent = $('#page-cart');
        $parent.addClass('processing');
        $('#page-cart-box')
            .stop(true, true)
            .slideUp(200, function () {
                $parent.removeClass('processing').removeClass('open');
            });
    }
</script>
