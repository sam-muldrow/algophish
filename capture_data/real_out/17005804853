<script type="text/javascript">
    $(document).ready(function () {
        var itemsIds = [];
        if (Env && Env.check()) {
            if (Env.isIE10OrOlder) {
                itemsIds.push('ie10');
            }
        }
        if (itemsIds.length > 0) {
            for (var i = 0; i < itemsIds.length; i++) {
                $('#top-info-msg-item-' + itemsIds[i]).show();
            }
            $('#top-information-message-list-area').show();
        }
    });
