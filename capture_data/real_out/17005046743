        <script>
            $(document).ready(function() {
                $('#order_house').submit(function() {	
                    var house = $("input[name='house']").val();
                    var name = $("input[name='name']").val();
                    var phone = $("input[name='phone']").val();				
                    $.ajax({
                        type: 'post',
                        data: {'house':house,'name':name,'phone':phone},
                        url: "/house/order_house.html",
                        error: function () {
                            alert('Error!');
                        },
                        success: function (result) {
                            if (result.code) {
                                alert("预约成功");
                                $("input[name='house']").val('');
                                $("input[name='name']").val('');
                                $("input[name='phone']").val('');				
                            } else alert(result.msg);
                        }
                    });
                    return false;
                });
            })
        </script>
			
			
