          if (typeof parentUrl == 'undefined') {
            parentUrl = '';
          }

          var isInIframe = window.location != window.parent.location && parentUrl.indexOf('app.convertexperiments.com') === -1 && parentUrl.indexOf('api.traq.li') === -1;
