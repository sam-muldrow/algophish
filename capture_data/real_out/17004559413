<!-- Recombee track script -->
<script src="https://cdn.jsdelivr.net/gh/recombee/js-api-client/dist/recombee-api-client.min.js"></script>

<script defer>
	(function() {
		window.CNC = window.CNC || {};
        window.CNC.recombee = window.CNC.recombee || {};
        window.CNC.recombee.userId = null;

		// Inicializace
		const client = new recombee.ApiClient(
			"czech-news-center-prod",
			"70U0WxdYzaEMPtAX0IfhiEhiMo6JcI1NtbHvH0nTn38eORv4PDkPTQ1ngsvCBuGf",
			{
				baseUri: "client-rapi-cnc.recombee.com"
			}
		);

		const recombeeUserId = (document.cookie.match(/^(?:.*;)?\s*_bisko\s*=\s*([^;]+)(?:.*)?$/) || [undefined, null])[1];
		window.CNC.recombee.userId = recombeeUserId;
		window.CNC.recombee.client = client;
		window.CNC.recombee.articleId = '';
		// Hodnoty z window.CNC.recombee se používají i ve skriptu pro načítání článků z Recommbee na FE

		const isArticle = false;

		// Tracker se inicializuje pouze pokud má uživatel nastaveno cookie _bisko
		if (recombeeUserId && isArticle) {
			const isRecommId = false;

			// Posílání PV detailu článku
			if (isRecommId) {
				client.send(new recombee.AddDetailView(
					recombeeUserId,
					'',
					{ "recommId": '' }
				));
			} else {
				client.send(new recombee.AddDetailView(recombeeUserId, ''));
			}
		}
	})();
</script>



