<script type="text/javascript">
    function ChangeTheme()
    {
        var l_newTheme = '';
        if (Cookies.get('Theme') == 'dark') {
            l_newTheme = 'light';
        }
        else {
            l_newTheme = 'dark';
        }

        Cookies.set('Theme', l_newTheme, { expires: 30 });
        window.location.reload(true);
    }

    var navVisible = false;

    function ShowNav() {
        if (!navVisible) {
            Hide('NavOpenBTN');
            Hide('middle');
            Show('NavCloseBTN');
            Show('Nav');
            navVisible = true;
        }
        else {
            Show('NavOpenBTN');
            Show('middle');
            Hide('NavCloseBTN');
            Hide('Nav');
            navVisible = false;
        }
    }
</script>
