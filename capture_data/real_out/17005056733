<script type="text/javascript">
    window.addEventListener('load', () => {
        registerSW();
    });
 
    async function registerSW() {
      if ('serviceWorker' in navigator) {
        try {
          await navigator
                .serviceWorker
                .register('/manifest/worker.js');
        }
        catch (e) {
          console.log('SW registration failed');
        }
      }
    }
</script>
