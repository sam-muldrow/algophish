<script type="text/javascript">
  document.querySelector('#growth').addEventListener("click",function (){
    window.location.href = "/yeah-growth-web/#/"
  })

  const videoDom = document.getElementById('growth-video')
  const imgDom = document.getElementById('growth-banner')
  setTimeout(() => {
    videoDom.style.display = 'none'
    imgDom.style.display = 'block'
  }, 10000)

  window.addEventListener('resize', function() {
    if (window.innerWidth > 1200) {
      imgDom.style.backgroundImage = "url('https://cn.yeahmobi.com/uploadfile/ueditor/file/20221201/1669871937807073.jpg')"
    } else if (window.innerWidth > 700) {
      imgDom.style.backgroundImage = "url('https://cn.yeahmobi.com/uploadfile/ueditor/file/20221201/1669871937998245.jpg')"
    } else {
      imgDom.style.backgroundImage = "url('https://cn.yeahmobi.com/uploadfile/ueditor/file/20221201/1669880218139802.jpg')"
    }
  })
</script>

