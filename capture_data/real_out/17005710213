	<script type="text/javascript">
		function chg_mtop_menu(id) {
			for (var i = 1; i <= 8; i++) {
				//document.getElementById("mtop_menu" + i).className = "mainmenu_new";
			}
			document.getElementById(id).className = "mainmenu_new_orange";
		}
		function out_mtop_menu(id) {
			var section = "";
			if (id == "mtop_menu1") {
				section = "talk";
			} else if (id == "mtop_menu2") {
				section = "healthy";
			} else if (id == "mtop_menu3") {
				section = "home";
			} else if (id == "mtop_menu4") {
				section = "mother";
			} else if (id == "mtop_menu5") {
				section = "life";
			} else if (id == "mtop_menu6") {
				section = "town";
			} else if (id == "mtop_menu7") {
				section = "writer";
			} else if (id == "mtop_menu8") {
				section = "gallery";
			} else if (id == "mtop_menu9") {
				section = "coupon";
			}
			if (section != "mainpage") {
				document.getElementById(id).className = "mainmenu_new";
			}
		}
	</script>
