	function chg_tabcontent(v) {
		for (var i = 1; i <= 6; i++) {
			document.getElementById("tabmenutd" + i).className = "tabmenu_black";
			document.getElementById("tabmenu" + i).style.display = "none";
		}
		document.getElementById("tabmenutd" + v).className = "tabmenu_orange";
		document.getElementById("tabmenu" + v).style.display = "block";
	}

//-->
</script>
