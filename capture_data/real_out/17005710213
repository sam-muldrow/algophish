    function viewLoginFrame(vw){

		var scrWidth = window.outerWidth;
		var scrHeight = document.documentElement.scrollTop;
        var left = (scrWidth-320) / 2;
		var top = (scrHeight + 100);

		var dp = "";
		if (vw == "visible") {
			dp = "block";
		} else if (vw == "hidden") {
			dp = "none";
		}
		
        document.getElementById("login_frame").style.left=left+"px";
        document.getElementById("login_frame").style.top=top+"px";
		document.getElementById("login_frame").style.display = dp;
        if (vw=="visible"){
            document.getElementById("loginframe").src="/mainpage/account/login_pop.asp";
        }else if (vw=="hidden"){
            isovercnt = true;
        }

    }

	function cfGetEventPosition(evt) {

		var e = evt || this.event;
		var b = document.body;
		var scroll = cfGetScrollOffset();
		var pos = {
			x : window.event.pageX || e.clientX+scroll.x-b.clientLeft,
			y : window.event.pageY || e.clientY+scroll.y-b.clientTop
		}
		return pos;

	}

	function cfGetScrollOffset(win) {

		if (!win) win = self;
		var x = win.pageXOffset || win.document.body.scrollLeft || document.documentElement.scrollLeft || 0;
		var y = win.pageYOffset || win.document.body.scrollTop || document.documentElement.scrollTop || 0;
		return { x:x, y:y };

	}

