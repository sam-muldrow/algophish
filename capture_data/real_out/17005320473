<script>/*<![CDATA[*/ 
        let existingAssets1489015 = [];
        let blockIds1489015 = ["1488949","1488952","1489018"];
        let currentBlock1489015 = null;
        let blockAssets1489015 = [];
        let i1489015=0,j1489015=0;
        let currentID1489015 = "";
        let thisBlock1489015 = document.querySelector("#block-1489015");
        let parentBlock1489015 = thisBlock1489015.querySelector(".hasExclusions");
        let displayCount1489015 = 5;
        let includedCount1489015 = 10;
        let debugMode1489015 = true;
        if (blockIds1489015.length > 0)
        {
            for(i1489015=0;i1489015<blockIds1489015.length;i1489015++)
            {
                currentBlock1489015=document.querySelector("#block-"+blockIds1489015[i1489015]);
                /* get all asset ids from the block */
                blockAssets1489015 = currentBlock1489015.querySelectorAll("article");
                for (j1489015=0;j1489015<blockAssets1489015.length;j1489015++)
                {
                    currentID1489015 = blockAssets1489015[j1489015].id;
                    currentID1489015 = currentID1489015.replace("card-showcase-","");
                    currentID1489015 = currentID1489015.replace("card-grid-"+blockIds1489015[i1489015]+"-","");
                    currentID1489015 = currentID1489015.replace("card-summary-"+blockIds1489015[i1489015]+"-","");
                    existingAssets1489015.push(currentID1489015);
                }
            }
            if(debugMode1489015){console.log("We have the following existing assets:",existingAssets1489015);}
            
            /* ok now we have exclusions, if there are more than zero, proceed */
            if (existingAssets1489015.length > 0)
            {
                /* we are going to loop through all the assets in our block and remove those that are in the exclusion list, make sure we aren't showing too many, then display the block */
                blockAssets1489015 = thisBlock1489015.querySelectorAll(".exclusionAsset");
                if(debugMode1489015){console.log("We have the following assets in this block",blockAssets1489015);}
                for(i1489015=0;i1489015<blockAssets1489015.length;i1489015++)
                {
                     currentID1489015 = blockAssets1489015[i1489015].dataset.uuid;
                     if(debugMode1489015){console.log("Testing "+currentID1489015);}
                     if(existingAssets1489015.includes(currentID1489015))
                     {
                         if(debugMode1489015){console.log("Found a match for "+currentID1489015);}
                         blockAssets1489015[i1489015].remove();
                         includedCount1489015--;
                     }
                }
                
                /* if includedCount is still larger than displayCount, then we need to remove assets from the bottom up */
                if(debugMode1489015){console.log("Included = "+includedCount1489015+" display = "+displayCount1489015);}
                if(includedCount1489015>displayCount1489015)
                {
                    blockAssets1489015 = thisBlock1489015.querySelectorAll(".exclusionAsset");
                    let numberToDelete1489015 = includedCount1489015-displayCount1489015;
                    if(debugMode1489015){console.log("To delete: "+numberToDelete1489015);}
                    j1489015=1;
                    for(i1489015=blockAssets1489015.length-1;i1489015>0;i1489015--)
                    {
                        blockAssets1489015[i1489015].remove();
                        j1489015++;
                        if(j1489015>numberToDelete1489015)
                        {
                            break;
                        }
                    }
                }
                parentBlock1489015.style.display='block';
            }
        }
        
     /*]]>*/</script>
