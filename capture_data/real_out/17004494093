<script language="javascript" type="text/javascript">
const placeholdersAndElements = [
{
placeholder: document.querySelector('#menu-placeholder'),
element: document.querySelector('ing-menu')
},
{
placeholder: document.querySelector('#nav-placeholder'),
element: document.querySelector('ing-nav')
}
];
async function removePlaceholders() {
placeholdersAndElements.forEach(({ placeholder, element }) => {
if (placeholder) {
if (element || isBot) {
customElements.whenDefined(element.localName).then(() => {
document.body.removeChild(placeholder);
});
} else {
document.body.removeChild(placeholder);
}
}
});
}
removePlaceholders();
</script>
