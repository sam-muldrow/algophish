<script>
<!--dl_view_cart==Main Cart-->
    
<!-- dl_view_cart == Mini Cart -->
jQuery(".header-item .js-drawer-open-cart").click(function(){
  var items = [];
  // dataLayer.push({ ecommerce: null });
    window.dataLayer = window.dataLayer || [];
   jQuery.getJSON('/cart.js', function(cart) {
     var total_price = (cart.total_price/100);
     var item_count = cart.item_count;
     var length = cart.items.length;
      for(var i=0;i<=length-1;i++){
        var obj = {
              class:cart.items[i].properties.metafields.category_class,
              department:cart.items[i].properties.metafields.department,
              division:cart.items[i].properties.metafields.division,
              currency:"USD",
              item_id: ""+cart.items[i].product_id+"",
              item_name: cart.items[i].product_title,
              item_brand: cart.items[i].vendor,
              price: (cart.items[i].price/100),
              quantity:cart.items[i].quantity,
              item_category: cart.items[i].product_type,
              imageURL: cart.items[i].image,
              item_variant:cart.items[i].title,
              shopify_handle:cart.items[i].url,
              shopify_product_id: ""+cart.items[i].product_id+"",
              shopify_sku: cart.items[i].sku,
              shopify_variant_id:""+cart.items[i].id+""
          }
        items.push(obj);
      }
      // console.log(items)
    setTimeout(function(e){
      dataLayer.push({
        event: "dl_view_cart",
        totalPrice:total_price,
        quantity:item_count,
        ecommerce: {
          currencyCode: "USD",
          items:items
        }
      });
    },1300)
  });
});
<!--dl_remove_from_cart update-quantity == Mini Cart-->
$(document).on('click','#CartDrawer #CartContainer .ajaxcart__product .js-qty__wrapper .js-qty__adjust.js-qty__adjust--minus',function(e){  
    var current = $(this);
     var prod_id = jQuery(this).parents('.ajaxcart__row').data('id');
     var prod_name = jQuery(this).parents('.ajaxcart__row').data('title');
      var prod_vendor = jQuery(this).parents('.ajaxcart__row').data('vender');
      var prod_price = jQuery(this).parents('.ajaxcart__row').data('price');
      var prod_type = jQuery(this).parents('.ajaxcart__row').data('type');
      var prod_image = jQuery(this).parents('.ajaxcart__row').data('image');
      var prod_variant_title = jQuery(this).parents('.ajaxcart__row').data('variant_title');
      var prod_handle = jQuery(this).parents('.ajaxcart__row').data('handle');
      var prod_sku = jQuery(this).parents('.ajaxcart__row').data('sku');
      var prod_variant_id = jQuery(this).parents('.ajaxcart__row').data('variant_id');
      var items = [];
      var obj = {
        item_id: ""+prod_id+"",
        item_name: prod_name,
        item_brand: prod_vendor,
        price: prod_price,
        quantity:1,
        item_category: prod_type,
        currency: "USD",
        imageURL: prod_image,
        item_variant: prod_variant_title,
        shopify_handle: prod_handle,
        shopify_product_id: ""+prod_id+"",
        shopify_sku: prod_sku,
        shopify_variant_id: ""+prod_variant_id+"",
      }
      setTimeout(function(e){
        jQuery.getJSON('/cart.js', function(cart) {
          var length = cart.items.length;
          for(var i=0;i<=length-1;i++){
            if (prod_id == cart.items[i].product_id) {
               var obj1 = {
                    class:cart.items[i].properties.metafields.category_class,
                    department:cart.items[i].properties.metafields.department,
                    division:cart.items[i].properties.metafields.division,
                  }
              var itemObject = {...obj, ...obj1};   
              items.push(itemObject);
              break;
              }
              }
          });
        dataLayer.push({ ecommerce: null }); 
         dataLayer.push({
            event: "dl_remove_from_cart", 
            ecommerce: {
              currencyCode: "USD",
              items:items
          }
       });
    },500);
  })
  
<!--dl_add_to_cart update-quantity == Mini Cart-->  
$(document).on('click','#CartDrawer #CartContainer .ajaxcart__product .js-qty__wrapper .js-qty__adjust.js-qty__adjust--plus',function(e){  
    var current = $(this);
     var prod_id = jQuery(this).parents('.ajaxcart__row').data('id');
     var prod_name = jQuery(this).parents('.ajaxcart__row').data('title');
      var prod_vendor = jQuery(this).parents('.ajaxcart__row').data('vender');
      var prod_price = jQuery(this).parents('.ajaxcart__row').data('price');
      var prod_type = jQuery(this).parents('.ajaxcart__row').data('type');
      var prod_image = jQuery(this).parents('.ajaxcart__row').data('image');
      var prod_variant_title = jQuery(this).parents('.ajaxcart__row').data('variant_title');
      var prod_handle = jQuery(this).parents('.ajaxcart__row').data('handle');
      var prod_sku = jQuery(this).parents('.ajaxcart__row').data('sku');
      var prod_variant_id = jQuery(this).parents('.ajaxcart__row').data('variant_id');
      var items = [];
      var obj = {
        item_id: ""+prod_id+"",
        item_name: prod_name,
        item_brand: prod_vendor,
        price: prod_price,
        quantity:1,
        item_category: prod_type,
        currency: "USD",
        imageURL: prod_image,
        item_variant: prod_variant_title,
        shopify_handle: prod_handle,
        shopify_product_id: ""+prod_id+"",
        shopify_sku: prod_sku,
        shopify_variant_id: ""+prod_variant_id+"",
      }
      setTimeout(function(e){
        jQuery.getJSON('/cart.js', function(cart) {
          var length = cart.items.length;
          for(var i=0;i<=length-1;i++){
            if (prod_id == cart.items[i].product_id) {
               var obj1 = {
                    class:cart.items[i].properties.metafields.category_class,
                    department:cart.items[i].properties.metafields.department,
                    division:cart.items[i].properties.metafields.division,
                  }
              var itemObject = {...obj, ...obj1};   
              items.push(itemObject);
              break;
              }
              }
          });
        dataLayer.push({ ecommerce: null }); 
         dataLayer.push({
            event: "dl_add_to_cart", 
            ecommerce: {
              currencyCode: "USD",
              items:items
          }
       });
    },500);
  })  
</script>
<script>
<!--dl_begin_checkout==Mini Cart/Main cart-->
$(document).on('click','.cart__checkout',function(e){
//jQuery(".cart__checkout").click(function(){
  window.dataLayer = window.dataLayer || [];
  dataLayer.push({ ecommerce: null });  // Clear the previous ecommerce object.
  jQuery.getJSON('/cart.js', function(cart) {
    var item_count = cart.item_count;
    var total_price = (cart.total_price/100)
    var length = cart.items.length;
    var items = [];
    for(var i=0;i<=length-1;i++){
      var obj = {
        class:cart.items[i].properties.metafields.category_class,
        department:cart.items[i].properties.metafields.department,
        division:cart.items[i].properties.metafields.division,
        item_id: ""+cart.items[i].product_id+"",
        item_name: cart.items[i].product_title,
        item_brand: cart.items[i].vendor,
        price: (cart.items[i].original_price/100),
        quantity:cart.items[i].quantity,
        item_category: cart.items[i].product_type,
        currency: "USD",
        imageURL: cart.items[i].image,
        item_variant:cart.items[i].title,
        shopify_handle:cart.items[i].url,
        shopify_product_id: ""+cart.items[i].product_id+"",
        shopify_sku: cart.items[i].sku,
        shopify_variant_id:""+cart.items[i].id+""
      }
      items.push(obj);
    }
    setTimeout(function(e){
      dataLayer.push({
        event: "dl_begin_checkout",
        ecommerce: {
          currencyCode: "USD",
          totalPrice:total_price,
          quantity:item_count,
          items:items
        }
      });
    },500);
  });
console.log("GA4 Events: Checkout button clicked!!")
}); 
</script>
