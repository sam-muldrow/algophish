      var classname = document.getElementsByClassName("add-to-cart");
      var addToCart = function() {
        _learnq.push(['track', 'Added to Cart', item]);
        }; for (var i = 0; i < classname.length; i++) {
        classname[i].addEventListener('click', addToCart, false);
      }
