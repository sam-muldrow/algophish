        </div>
      {{/if}}
      <div class="grid grid--full">
        <div class="grid__item one-half">
          <p class="ajaxcart__subtotal">Subtotal</p>
        </div>
        <div class="grid__item one-half text-right">
          <p class="ajaxcart__price">
              {{ 'totalPrice' }}
          </p>
        </div>
      </div>
      <p class="ajaxcart__note">
        Shipping, taxes, and discount codes calculated at checkout.
      </p>
      
      
      <div class="cart__checkout-wrapper ajax-cart__checkout-wrapper">
        <a href="/cart" class="btn btn--secondary view-cart">View Bag</a>
  {{#if hideCheckoutButton}}
     
     <span style="display:none;">Checkout not permitted</span>
     
        {{else}}
     
     <button type="submit" name="checkout" id="cart__checkout" class="btn cart__checkout">
                      Check out
     </button>
  
  {{/if}}
  
      </div>
    </div>
</script>
