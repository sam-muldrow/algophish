    <script defer>
function swymCallbackFn(swat){
  // your API calls go here
  if(algoliaShopify && typeof algoliaShopify.instantsearch !== 'undefined'){ // default algolia object.
    algoliaShopify.instantsearch.search.on('render', function () { 
      if(swat){
 swat.initializeActionButtons(".ais-block");
        SwymUtils.forEachElement(".ais-hits--item > div", function(elem) {
          elem.onclick = function(e) {
            e.stopPropagation();
          }
        });
      }
    });
  }
}
if(!window.SwymCallbacks){
  window.SwymCallbacks = [];
}
window.SwymCallbacks.push(swymCallbackFn);
</script>

    
