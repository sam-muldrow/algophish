    function updatecounterDown() {
        const now = new Date();
        const electionDate = new Date("2024-02-14"); // Change this to the election date
        const timeCounterDownLeft = electionDate - now;

        const daysCounterDown = Math.floor(timeCounterDownLeft / (1000 * 60 * 60 * 24));
        const hours = Math.floor((timeCounterDownLeft % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
        const minutes = Math.floor((timeCounterDownLeft % (1000 * 60 * 60)) / (1000 * 60));
        const seconds = Math.floor((timeCounterDownLeft % (1000 * 60)) / 1000);

        $("#daysCounterDown").html(`${daysCounterDown} HARI`);
        $("#timeCounterDown").html(`${hours} Jam ${minutes} Menit ${seconds} Detik`);
    }

    $(document).ready(function() {
        updatecounterDown();
        setInterval(updatecounterDown, 1000);
    });
</script> </div> <div class="clear h10"></div> <div class="border-box"> <div class="content sponsored-content-sidebar MotoGP Qatar"> <h3><a href="https://www.jpnn.com/tag/motogp-qatar" target="_self" title="MotoGP Qatar">MotoGP Qatar</a> </h3> <ul> <li> <article> <a href="https://www.jpnn.com/news/alex-marquez-dan-diggia-akan-melakukan-start-terbaik-di-main-race-motogp-qatar-2023" title="Alex Marquez dan Diggia Akan Melakukan Start Terbaik di Main Race MotoGP Qatar 2023 - JPNN.com"> <img loading="lazy" src="https://image.jpnn.com/resize/278x156-80/arsip/normal/2023/11/19/pembalap-gresini-racing-di-motogp-qatar-2023-foto-gresini-lv-xw4o.jpg" alt="Alex Marquez dan Diggia Akan Melakukan Start Terbaik di Main Race MotoGP Qatar 2023 - JPNN.com" width="278" height="156" class="rsp loading"> </a> <h2><a href="https://www.jpnn.com/news/alex-marquez-dan-diggia-akan-melakukan-start-terbaik-di-main-race-motogp-qatar-2023" title="Alex Marquez dan Diggia Akan Melakukan Start Terbaik di Main Race MotoGP Qatar 2023 - JPNN.com">Alex Marquez dan Diggia Akan Melakukan Start Terbaik di Main Race MotoGP Qatar 2023</a></h2> <div class="clear"></div> </article> </li> <li> <article> <a href="https://www.jpnn.com/news/2-video-pecco-martin-bersenggolan-di-sprint-motogp-qatar" title="2 Video Pecco & Martin Bersenggolan di Sprint MotoGP Qatar - JPNN.com"> <img loading="lazy" src="https://image.jpnn.com/resize/278x156-80/arsip/normal/2023/11/19/pembalap-pramac-ducati-jorge-martin-memenangi-sprint-motogp-thix.jpg" alt="2 Video Pecco & Martin Bersenggolan di Sprint MotoGP Qatar - JPNN.com" width="278" height="156" class="rsp loading"> </a> <h2><a href="https://www.jpnn.com/news/2-video-pecco-martin-bersenggolan-di-sprint-motogp-qatar" title="2 Video Pecco & Martin Bersenggolan di Sprint MotoGP Qatar - JPNN.com">2 Video Pecco & Martin Bersenggolan di Sprint MotoGP Qatar</a></h2> <div class="clear"></div> </article> </li> <li> <article> <a href="https://www.jpnn.com/news/espargaro-pukul-kepala-mor