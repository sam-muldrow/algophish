               <script>
                  /*$( document ).ready(function() {
                     getTodaysTrendingData();
                  });*/
                  var scrollForHomeheightTrending = 9000;
                  var hometrendsflag = 1; 
                  window.addEventListener("scroll", function() {
                     if(document.body.offsetHeight - window.pageYOffset < scrollForHomeheightTrending && hometrendsflag==1){
                        hometrendsflag=0; 
                        getTodaysTrendingData();
                     }  
                  });
                  function getTodaysTrendingData(){
                     $.ajax({
                        url: window.location.origin+"/google-treanding/ga-4/Mid-Day-Google-Today-Realtime-Page.json", 
                        method: "GET",
                        success: function(resultMDT)
                        {
                           var htmlMDT = ""; 
                           var border_right = "trending-news-border-right";
                           if(resultMDT !=""){
                              for (var i=0; i<resultMDT.length; i++){
                                 if(i<10){
                                    if(resultMDT.length-1 == i || i==9){
                                       border_right = "";
                                    }

                                    var iconImg = "";
                                    if(resultMDT[i]['flag'] == 'photo'){
                                       iconImg = '<i class="fa bg-light-blue camera-over-image-trending color-white fa-camera" aria-hidden="true"></i>';
                                    }else if(resultMDT[i]['flag'] == 'video'){
                                       iconImg = '<i class="fa fa-play bg-light-blue camera-over-image-trending color-white" aria-hidden="true"></i>';
                                    }
                                    
                                    var htmlMDT = htmlMDT+'<div class="swiper-slide mb-1"><div class="'+border_right+'"><div class="row trending-swiper trending-swiper"><div class="col-6 pr-1"><p class="trending-lineclamp mb-0"><a href="'+resultMDT[i]['site_url']+'" class="trending-news-color">'+resultMDT[i]['title']+'</a></p></div><div class="col-6 pl-0"><a href="'+resultMDT[i]['site_url']+'"><picture><img class="imglazy_rec img-height" src="'+resultMDT[i]['img_url']+'" alt="'+resultMDT[i]['title']+'" style="border-radius:2px"></picture>'+iconImg+'</a></div></div></div></div>';
                                  }

                              } 
                              $(".trnding-todays").html(htmlMDT); 
                              var trendingswiper = new Swiper(".TrendingSwiper", {
                                 observer: true,
                                 observeParents: true,
                                 watchSlidesProgress: true,
                                 slidesPerView: 1,
                                 observer: true,
                                 observeParents: true,
                                 navigation: {
                                       nextEl: '.bottomSliderSwipe .swiper-button-next',
                                       prevEl: '.bottomSliderSwipe .swiper-button-prev',
                                 },
                                    breakpoints: {
                                          // when window width is >= 640px
                                          640: {
                                             slidesPerView: 2.5,
                                             spaceBetween: 10,
                                       }
                                    }
                                 });
                           }else{
                              $('.trending-widget').hide();
                           }
                        } 
                     });
                  }  
               </script>
            </section>
