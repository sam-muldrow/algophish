<script>
    function setCookieAgreementCookie(){
        var d = new Date();
        var noOfDays = 90;
        d.setTime(d.getTime() + (noOfDays * 86400 * 1000));
        document.cookie = "cookieAgreement=Accepted;expires=" + d.toUTCString() + ";path=/";
        $(".cookieAgreementBanner").hide();
    }
</script>
