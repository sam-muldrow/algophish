                //å çå¯¼èª å·æ°
                $('.hangyeDir').each(function () {
                    var _this = $(this);
                    var length = _this.children().length - 6;
                    index = Math.floor(Math.random()* length);
                    $(this).children().each(function(key, val){
                        if( key >= index && key < index+6){
                            $(this).show();
                        }else{
                            $(this).hide();
                        }
                    });
                })

