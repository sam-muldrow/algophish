        <script>
            (function() {
                var HotWord = {
                    Version: "0.1.0",
                    createNew: function() {
                        var o = {};
                        o.init = function() {
                            //inputè·åï¼å¤±å»ç¦ç¹
                            $(".import").focus(function() {
                                $(".tab-menu").show();
                            });

                            //tabéé¡¹å¡åæ¢

                            $('body').bind('click', function(event) {
                                // IEæ¯æ event.srcElement ï¼ FFæ¯æ event.target
                                var evt = event.srcElement ? event.srcElement : event.target;
                                if (evt.id == 'kw' || evt.id == 'tab-menu' || evt.id == 'nav') return; // å¦ææ¯åç´ æ¬èº«ï¼åè¿å
                                else {
                                    $("#tab-menu").css('display', 'none');
                                    //$('#kw').hide(); // å¦ä¸æ¯åéèåç´ 
                                }
                            });

                            $('#kw').bind('click', function(event) {
                                $("#tab-menu").css('display', 'block');
                            });

                            $("#nav a").off("hover").on("hover", function() {
                                var index = $(this).index();
                                $(this).addClass("on").siblings().removeClass("on");
                                $("#contentBox .box").eq(index).addClass("active").siblings().removeClass("active");
                            });

                            last_search();

                            $(document).on("click", "#contentBox .box li", function() {
                                var _this = $(this).children("span.thisHotword");
                                var _id = _this.attr('hotwordId');
                                var _word = _this.html();
                                $('#kw').val(_word);
                                $('#kw').attr('_word_id', _id);
                                $('.btn-search').click();
                            });

                            $('.btn-search').click(function() {
                                var _word = $('#kw').val();
                                var _id = $('#kw').attr('_word_id');

                                var _text = {
                                    'id': _id,
                                    'word': _word,
                                };
                                _text = encodeURI(JSON.stringify(_text));
                                $.ajax({
                                    url: '/kw_click/',
                                    type: 'GET',
                                    data: {
                                        text: _text
                                    },
                                    dataType: 'json',
                                    success: function(ret) {
                                        last_search();
                                    }
                                });
                            });
                        };

                        var last_search = function() {
                            var data = get_cookie("userHotword");
                            if (data) {
                                data = urldecode(data);
                                var json = JSON.parse(data);
                                if (typeof json === "object") {
                                    var str = "";
                                    var i;
                                    // for (k in json) {
                                    //     i = k * 1 + 1;
                                    for (var k = 0; k < json.length; k++) {
                                        i = k * 1 + 1;
                                        if (i > 3) {
                                            str += '<li><span>' + i + '</span><span class="thisHotword" hotwordId=0>' + json[k] + '</span></li>';
                                        } else {
                                            str += '<li><span class="color">' + i + '</span><span class="thisHotword" hotwordId=0>' + json[k] +
                                                '</span></li>';
                                        }
                                    }
                                    if (str) {
                                        $('.userSearchWord').html(str);
                                    }
                                }
                            }
                        };

                        var get_cookie = function(name) {
                            name += "=";
                            var carray = document.cookie.split(";");

                            for (var i = 0; i < carray.length; i++) {
                                var c = carray[i];
                                while (c.charAt(0) === " ") {
                                    c = c.substring(1, c.length);
                                }
                                if (c.indexOf(name) === 0) {
                                    return decodeURIComponent(c.substring(name.length, c.length));
                                }
                            }

                            return null;
                        };

                        var urldecode = function(str) {
                            if (null === str) {
                                return "";
                            } else {
                                try {
                                    return decodeURIComponent(str);
                                } catch (e) {
                                    return "";
                                }
                            }
                        };
                        return o;
                    }
                };

                var hw = HotWord.createNew();
                hw.init();
            })();
        </script>

