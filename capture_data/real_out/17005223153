    GoogleAnalytics.autoWireUpEvents = function (jQuery) {

        if (!jQuery) {
            return;
        }

        jQuery('.ga-event').click(function () {
            var $el = jQuery(this);

            var gaCategory = $el.data('ga-category');
            var gaAction = $el.data('ga-action');
            var gaLabel = $el.data('ga-label');
            var gaValue = $el.data('ga-value');

            if (!gaCategory || !gaAction) {
                return;
            }

            GoogleAnalytics.genericEvent(gaCategory, gaAction, gaLabel, gaValue);
        });
    };

    
