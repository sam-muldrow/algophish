            var fadeTime;
            var mySwiper = new Swiper('.picslide .swiper-container', {
                observer: true,
                observeParents: true,
                autoplay: true,
                pagination: {
                    el: '.picslide .swiper-pagination',
                    clickable: true,
                },
                navigation: {
                  nextEl: '.picslide .swiper-button-next',
                  prevEl: '.picslide .swiper-button-prev',
                },
            });
            $('.picslide').mouseenter(function(){
                mySwiper.autoplay.stop();
                clearTimeout(fadeTime);
                $('.picslide .swiper-button-prev').stop(true,true).fadeIn();
                $('.picslide .swiper-button-next').stop(true,true).fadeIn();
            });
            $('.picslide').mouseleave(function(){
                mySwiper.autoplay.start();
                fadeTime = setTimeout(function(){
                    $('.picslide .swiper-button-prev').stop(true,true).fadeOut();
                    $('.picslide .swiper-button-next').stop(true,true).fadeOut();
                },500);
            });
