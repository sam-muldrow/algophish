$(document).ready(function(){
var st = '2023-10-23 9:30:00';
var et = '2023-11-13 00:00:00';
var today = new Date();
var a = new Date(Date.parse(st.replace(/-/g, "/")));
var b = new Date(Date.parse(et.replace(/-/g, "/")));

if ( today >=a && today <= b){
$("#Jia-header-2016").before("<div class='dropDown_ban'><div class='dropDown_limit'><div class='dropDown_ban_wrap'><a href='javascript:void(0);' class='close'></a><div class='dropDown_big'><a href='//tg.jia.com/shanghai/55/' target='_blank' tjjj='sitehome_big_adv'><img src='//tgi13.jia.com/130/115/30115688.jpg' width='1200' height='300' /></a><span></span></div><div class='dropDown_small'><a href='//tg.jia.com/shanghai/55/' target='_blank' tjjj='sitehome_big_adv'><img src='//tgi12.jia.com/130/115/30115685.jpg' width='1200' height='90' /></a><span></span></div></div></div></div>");
}
			//显示图片
		var ban_timer=null;
		var shink_timer=null;
		$(".dropDown_ban_wrap").slideDown(800);
		shink_timer=setTimeout(shink,5800);
		$(".dropDown_ban_wrap .close").click(function(){//关闭
				clearTimeout(shink_timer);
				$(".dropDown_ban").remove();
			});
		
		if(screen.width>1024)  //获取屏幕的的宽度
	    {
	       $('.dropDown_ban_wrap,.dropDown_big,.dropDown_small').css("width","1200px");
	       $('.dropDown_big img,.dropDown_small img').attr({'width':1200})
	    }
	    $('.dropDown_big span').click(function(){//收缩
	    	clearTimeout(shink_timer);
	    	ban_timer=setInterval(h_test,30);
	    	$('.dropDown_ban_wrap').stop().animate({'height':90},500);
	    	
	    });
	    $('.dropDown_small span').click(function(){//展开
	    	$('.dropDown_small').stop().animate({'opacity':0,'z-index':-1});
	    	$('.dropDown_ban_wrap').stop().animate({'height':300},500);
	    	$('.dropDown_big').stop().animate({'opacity':1,'z-index':2});
	    });
	    
	    function shink(){
	    	ban_timer=setInterval(h_test,30);
	    	$('.dropDown_ban_wrap').stop().animate({'height':90},500);
	    }
	    
	    function h_test(){
	    	if(parseInt($('.dropDown_ban_wrap').height())<=200)
	    	{
	    		$('.dropDown_big').stop().animate({'opacity':0,'z-index':-1});
				$('.dropDown_small').stop().animate({'opacity':1,'z-index':1});
				clearInterval(ban_timer);
	    	}
	    }
	    });
</script>
 <script type="text/javascript">
$(document).ready(function(){
    var st = '2017-9-10 18:00:00';
    var et = '2017-10-8 18:00:00';
    var today = new Date();
    var a = new Date(Date.parse(st.replace(/-/g, "/")));
    var b = new Date(Date.parse(et.replace(/-/g, "/")));

    if ( today >=a && today <= b){
        $("#Jia-header-2016").before("<div class='dropDown_ban'><div class='dropDown_limit'><div class='dropDown_ban_wrap'><a href='javascript:void(0);' class='close'></a><div class='dropDown_big'><a href='//tg.jia.com/shanghai/55/' target='_blank' tjjj='Jia.jnh.b'><img src='//fastued1.jia.com/image/adv/dt300.jpg' width='1200' height='300' /></a><span></span></div><div class='dropDown_small'><a tjjj='Jia.jnh.s1' href='//tg.jia.com/shanghai/55/' target='_blank' style='position:absolute;height:90px;left:0;top:0;width:818px;background:#fff;opacity:0;filter:alpha(opacity:0);'></a><a tjjj='Jia.jnh.s2' href='//tg.jia.com/shanghai/55/' target='_blank' style='position:absolute;height:90px;left:819px;top:0;width:167px;background:#fff;opacity:0;filter:alpha(opacity:0);'></a><a tjjj='Jia.jnh.s2' href='//tg.jia.com/shanghai/856/' target='_blank' style='position:absolute;height:90px;right:55px;top:0;width:167px;background:#fff;opacity:0;filter:alpha(opacity:0);'></a><img src='//fastued1.jia.com/image/adv/dt90.jpg' width='1200' height='90' /><span style='z-index: 2;'></span></div></div></div></div>");
    }
    //显示图片
    var ban_timer=null;
    var shink_timer=null;
    $(".dropDown_ban_wrap").slideDown(800);
    shink_timer=setTimeout(shink,5800);
    $(".dropDown_ban_wrap .close").click(function(){//关闭
        clearTimeout(shink_timer);
        $(".dropDown_ban").remove();
    });

    if(screen.width>1024)  //获取屏幕的的宽度
    {
        $('.dropDown_ban_wrap,.dropDown_big,.dropDown_small').css("width","1200px");
        $('.dropDown_big img,.dropDown_small img').attr({'width':1200})
    }
    $('.dropDown_big span').click(function(){//收缩
        clearTimeout(shink_timer);
        ban_timer=setInterval(h_test,30);
        $('.dropDown_ban_wrap').stop().animate({'height':90},500);

    });
    $('.dropDown_small span').click(function(){//展开
        $('.dropDown_small').stop().animate({'opacity':0,'z-index':-1});
        $('.dropDown_ban_wrap').stop().animate({'height':300},500);
        $('.dropDown_big').stop().animate({'opacity':1,'z-index':2});
    });

    function shink(){
        ban_timer=setInterval(h_test,30);
        $('.dropDown_ban_wrap').stop().animate({'height':90},500);
    }

    function h_test(){
        if(parseInt($('.dropDown_ban_wrap').height())<=200)
        {
            $('.dropDown_big').stop().animate({'opacity':0,'z-index':-1});
            $('.dropDown_small').stop().animate({'opacity':1,'z-index':1});
            clearInterval(ban_timer);
        }
    }
});
</script>
