    <script type="text/javascript">
      var ua = window.navigator.userAgent;
      var msie = ua.indexOf('MSIE ');
      if (msie > 0) {document.querySelector('html').className += ' MSIE';}
  
      var trident = ua.indexOf('Trident/');
      if (trident > 0) {document.querySelector('html').className += ' MSIE';}
  
      var edge = ua.indexOf('Edge/');
      if (edge > 0) {document.querySelector('html').className += ' MSIE';}
    </script>

    
    


    

    <script type="text/javascript">
  (function() {
      const tagId = window.contentSquareId || "2d49608e6cc52";
      const srcUrl = "//t.contentsquare.net/uxa/" + tagId + ".js";
      window._uxa = window._uxa || [];
      if (typeof CS_CONF === 'undefined') {
          window._uxa.push(['setPath', window.location.pathname + window.location.hash.replace('#', '?__')]);
          var mt = document.createElement("script");
          mt.type = "text/javascript";
          mt.async = true;
          mt.src = srcUrl;
          document.getElementsByTagName("head")[0].appendChild(mt);
      } else {
          window._uxa.push(['trackPageview', window.location.pathname + window.location.hash.replace('#', '?__')]);
      }
  })();
  </script>
