      <script type="text/javascript">
  (function () {
    function getCookie(name) {
      const value = "; " + document.cookie;
      const parts = value.split("; " + name + "=");
      return parts.length === 2 ? parts.pop().split(";").shift() : undefined;
    }

    const locale = getCookie("locale") || getCookie("region");
    const customerId = getCookie("customer-id") || getCookie("customer_persistentid");
    const accountId = getCookie("account-id");

    if (window.newrelic) {
      if (locale) {
        window.newrelic.setCustomAttribute('locale', locale);
      }
      if (customerId) {
        window.newrelic.setCustomAttribute('customerId', customerId, true);
      }
      if (accountId) {
        window.newrelic.setCustomAttribute('lotteriesAccountId', accountId, true);
      }
    }
  })();
</script>
    

    
    
    
