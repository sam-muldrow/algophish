	<script>
		window.Next = (window.Next == null) ? {} : window.Next;
		window.Next.GlobalVariables = (window.Next.GlobalVariables == null) ? {} : window.Next.GlobalVariables;

		window.Next.GlobalVariables.AllowLazyAds = false;

		//Just incase entries isnt known
		if (!Object.entries) {
			Object.entries = function (obj) {
				var ownProps = Object.keys(obj),
					i = ownProps.length,
					resArray = new Array(i); // preallocate the Array
				while (i--)
					resArray[i] = [ownProps[i], obj[ownProps[i]]];

				return resArray;
			};
		}

		function enableLazyLoadSpaces() {
			//Means there has been no interruption to the loading of the ads library however this doesnt take into account ads being blocked
			Next.GlobalVariables.AllowLazyAds = true;
		}
