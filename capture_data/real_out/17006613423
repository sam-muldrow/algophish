    <script type="text/javascript">
        // inject the input field to pass to the backend validation
        // needs to be dynamic as the captcha wont always be needed
        var injectCaptchaInput655e085e0e7f3 = function () {
            // if the input field does not exist, inject it
            if (!$('recaptchaInput655e085e0e7f3')) {
                var newCaptchaInputField = new Element('input', {
                    id: 'recaptchaInput655e085e0e7f3', 
                    type: 'hidden', 
                    name: 'recaptcha',
                    class: 'validate-ajax-recaptcha'
                });
                newCaptchaInputField.inject($('recaptchaContainer655e085e0e7f3'), 'before');
            }
        };
    
        var verifyCallback655e085e0e7f3 = function (response) {
            // if the input field does not exist, inject it
            if (!$('recaptchaInput655e085e0e7f3')) {
                injectCaptchaInput655e085e0e7f3();
            }
            // set the captcha token value to send to the backend with the form data
            $('recaptchaInput655e085e0e7f3').value = response;
            // remove recaptcha's own value field
            $$("[name='g-recaptcha-response']").dispose();
            
            // hack to hide mmho advice tooltip
            if ($('recaptchaFieldset655e085e0e7f3').hasClass('error')) {
                $('recaptchaFieldset655e085e0e7f3').removeClass('error');
                $('recaptchaFieldset655e085e0e7f3').addClass('success');
            }
            if ($('advice-validate-ajax-recaptcha-recaptchaInput655e085e0e7f3')) {
                $('advice-validate-ajax-recaptcha-recaptchaInput655e085e0e7f3').dispose();
            }
        };
        
        var expiredCallback655e085e0e7f3 = function () {
            // clear the captcha token value
            $('recaptchaInput655e085e0e7f3').value = '';
        }
        
        // remove the captcha with mootools magic
        var recaptchaDestroy655e085e0e7f3 = function () {
            // recaptcha reset to let us re-render later?
            grecaptcha.reset(recaptchaRenderId655e085e0e7f3);
            // remove the hidden form input
            $('recaptchaInput655e085e0e7f3').destroy();
            // remove all the elements injected by recaptcha's code
            //$('recaptchaContainer').empty();
        }
        
        // render the captcha
        var recaptchaRenderId655e085e0e7f3 = undefined;
        var recaptchaRender655e085e0e7f3 = function () {
            if (!$('recaptchaInput655e085e0e7f3')) {
                injectCaptchaInput655e085e0e7f3();
                if (recaptchaRenderId655e085e0e7f3 === undefined) {
                    recaptchaRenderId655e085e0e7f3 = grecaptcha.render('recaptchaContainer655e085e0e7f3', {
                        'sitekey': '6LdFJv8SAAAAAMv7GZnieAIaoaln-PeKJrUTSnOB',
                        'callback': verifyCallback655e085e0e7f3,
                        'expired-callback': expiredCallback655e085e0e7f3,
                        'theme': 'dark'
                    });
                }
            }
        }
        
        // state that captcha code is loaded and ready to render
        var recaptchaReady655e085e0e7f3 = false;
        var recaptchaCallback655e085e0e7f3 = function () {
            recaptchaReady655e085e0e7f3 = true;
            // if this captcha block is inside the registration form, render automatically
            if (document.id('registerForm') && document.id('registerForm').contains(document.id('recaptchaFieldset655e085e0e7f3')) ) {
                recaptchaRender655e085e0e7f3();
            }
        }
