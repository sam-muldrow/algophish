<script type="text/javascript">
    function enableChangePlatform() {
        var element = $('#changePlatform');
        if (element.data('onclick')) {
            element.attr('onclick', element.data('onclick'));
        }
    }

    if (typeof (chipVersion132) != "function") {
        document.addEventListener('jQueryReady', function () {
            $(document).one('asyncFilesReady', function () {
                $.getCachedCSS('https://static.jobs.bg/mobile/assets/css/chips.34c3ed1ad64f767292ea.css');

                $.getCachedScript("https://static.jobs.bg/mobile/assets/js/chips.4ec1b10e11e7c4900d31.js", function () {
                    enableChangePlatform();
                    initChips();
                    
                });
            });
            document.removeEventListener('jQueryReady', arguments.callee);
        });
    }
    else {
        $( document ).ready(function() {
            enableChangePlatform();
            initChips();
        });
    }
</script>

