                    <script type="text/javascript">
            document.addEventListener('jQueryReady', function () {

                registry.host = 'https://www.jobs.bg/';
                registry.appIndex = (typeof appIndex !== "undefined") ? appIndex : '/';
                registry.lang = 'bg';
                registry.platform = 'jobs';
                registry.platformColors = {'jobs': '#75318B', 'itjobs': '#5585EC'};
                registry.platformDefaultColor = '#75318B';

                
                
                if (typeof (jsErrorProcess) !== "function") {
                    jsErrorProcess = function (err) {
                        if ($ ('#jsErrorMessage').length == 0) {
                            $ ('body').append ('<div id="jsErrorMessage" style="display: none;"><div class="page-error-opacity"></div><div class="page-error-message"><div><div id="serverMessageText" style="border-bottom: 1px solid #ddd; padding-bottom: 10px; margin-bottom: 10px;">Необходимо е приложението да бъде презаредено.</div><div style="text-align: center;"><span class="color-blue pointer" onclick="$(\'#jsErrorMessage\').hide(); window.location = registry.appIndex;" class="button" style="font-weight: bold; padding: 10px;">Презареди</span></div></div></div></div>');
                        }
                        $ ('#jsErrorMessage').show ();
                    }
                }

                document.removeEventListener('jQueryReady', arguments.callee);
            });
        </script>
