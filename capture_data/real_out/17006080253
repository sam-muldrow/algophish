                $(document).one('asyncFilesReady', function() {
                    mdc.InitComponentFirst('#navigationDrawer', 'MDCDrawer');
                    document.body.addEventListener('MDCDrawer:closed', function () {
                        $('html').removeClass('scroll-lock');
                    });

                                            initSideDrawerMenus();
                                    });

                document.removeEventListener('jQueryReady', arguments.callee);
            });
