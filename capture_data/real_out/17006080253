                                        <div class="rounded-box bg-white p-16">
                                            <iframe
                                                    id="economyIframe"
                                                    src="https://www.jobs.bg/economy_sandboxed.php"
                                                    sandbox="allow-scripts allow-same-origin allow-popups allow-popups-to-escape-sandbox"
                                                    onload="this.style.height=(this.contentWindow.document.body.scrollHeight)+'px';"                                                    style="min-width: 100%; border: none; width: 100%;" scrolling="no" height="480"></iframe>
                                            <script type="text/javascript">
                                                document.addEventListener('jQueryReady', function () {

                                                    $(document).one('asyncFilesReady', function () {
                                                        $.getCachedScript("https://static.jobs.bg/mobile/assets/js/iframe-resizer/iframeResizer.min.bd1fe20f61d8b9b4a1d0.js")
                                                            .fail(function () {
                                                            })
                                                            .done(function () {
                                                                var iframe = iFrameResize({
                                                                    log: false,
                                                                    bodyPadding: '0px 0px 8px 0px',
                                                                    checkOrigin: ['https://www.jobs.bg']
                                                                }, '#economyIframe');
                                                                if (iframe.length > 0) {
                                                                    window.setTimeout(function () {
                                                                        iframe[0].iFrameResizer.resize();
                                                                    }, 100);
                                                                }
                                                            });

                                                        //$.getCachedScript("https://static.jobs.bg/mobile/assets/js/blog-frame-message.96342e3938175139c067.js");

                                                        //LINK messages
                                                        window.addEventListener("message", receiveMessage, false);
                                                        function receiveMessage (event) {
                                                            // Do we trust the sender of this message?  (might be
                                                            // different from what we originally opened, for example).
                                                            if (event.origin !== 'https://www.jobs.bg')
                                                                return;

                                                            if (event.data && event.data.link && event.data.type) {
                                                                if(event.data.type === "system") {
                                                                    // Stop ghost events
                                                                    event.stopImmediatePropagation ();
                                                                    event.preventDefault ();

                                                                    // Open link in external browser
                                                                    handleOpenLinkInBrowser(event.data.link, '_system');
                                                                }
                                                            }
                                                        }
                                                    });
                                                    document.removeEventListener('jQueryReady', arguments.callee);
                                                });
                                            </script>
                                        </div>

