                <script type="text/javascript">
            var registry = registry || {};
            registry.host = 'https://www.jobs.bg/';
            registry.appIndex = (typeof appIndex !== "undefined") ? appIndex : '/';

            if (typeof StatusBar !== 'undefined') {
                StatusBar.backgroundColorByHexString("#75318B");
            }

            var assets =  assets || {};
            document.addEventListener('jQueryReady', function () {

                $(document).one('asyncFilesReady', function () {
                                        $.getCachedCSS('https://static.jobs.bg/mobile/assets/bundle.1da6162e921fd2c0432d90ab5831633c.css');
                    
                    var page = $('.page.active');
                    if (page.length) {
                                                fixSpaLinks(page);
                                            }
                });
                var mainAssets = [
                                        'https://static.jobs.bg/mobile/assets/modern.bundle.0c815b638d82a6172814.js',
                                        'https://static.jobs.bg/mobile/assets/js/main.535203c7ba465ae0621f.js',
                                        'https://www.jobs.bg/translation.js.php?v=7',
                                    ];
                var queue = mainAssets.map(function(script) {
                    if (typeof window['assets'][script] === "undefined") {
                        window['assets'][script] = $.getScript(script).fail (function () {
                            jsErrorProcess ('Error loading ' + script);
                        });
                        return window['assets'][script];
                    }
                });
                $.when.apply(null, queue).done(function() {
                    $(document).trigger('asyncFilesReady');
                });

                
                document.removeEventListener('jQueryReady', arguments.callee);
            });

                            if (typeof window.cordova !== "undefined" && typeof window.cordova.plugins !== "undefined" && typeof window.cordova.plugins.CookieManagementPlugin !== "undefined") {
                    window.cordova.plugins.CookieManagementPlugin.flush();
                }
            
            
            var event;

            function dispatchJQueryReady() {
                if (typeof(Event) === 'function') {
                    event = new Event('jQueryReady');
                }
                else {
                    event = document.createEvent('Event');
                    event.initEvent('jQueryReady', false, true);
                }

                if (typeof $ === 'undefined') {
                    document.addEventListener('DOMContentLoaded', function () {
                        document.dispatchEvent(event);
                    });
                } else {
                    document.dispatchEvent(event);
                }

                //console.log('dispatch jQueryReady event...');
            }

                            dispatchJQueryReady();
                    </script>
	
    </div><!-- /page -->

        
	<div class="overlay"></div>

</body>
</html>
	