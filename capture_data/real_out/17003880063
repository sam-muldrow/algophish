    // éä¸­è¨­å® row3A mouseover é ç±¤ææ
    //  tab-box  => row3A
    $('.tab-box li').on( "mouseover", function() {
        $('.tab-box li').attr('data-tab','normal');
        $(this).attr('data-tab','active');

        var act_idx = $(this).attr('data-tab-index');
        // åæç·¨è¼¯åå¡
        $(".row3A").each(function( index )
        {
            (act_idx != index )
            ?$( this ).hide()
            :$( this ).show()
            ;
        })
    });

    // scroll body to 0px on click
    $('.gotop-box').click(function () {
            $('body,html').animate({
                    scrollTop: 0
            }, 800);
            return false;
    });

});

