<script>
	var isInApp = false;

	function getCookie(name, defaultValue) {
		match = document.cookie.match(new RegExp(name + '=([^;]+)'));
		return match ? match[1] : defaultValue;
	}

	function getCookieJson(name) {
		var defaultReturn = {
			"loggedIn": false,
			"hasPlusGrant": false,
			"hasZuckergussGrant": false,
			"hasPurGrant": false,
			"default": true
		}

		var grantCookie = getCookie(name, false);

		if (grantCookie && typeof grantCookie === 'string') {
			grantCookie = grantCookie.replace(/(^"|"$)/g, '');
			try {
				return JSON.parse(atob(grantCookie));
			} catch(e) {
				return defaultReturn;
			}
		}

		return defaultReturn;
	}

	function isLoggedIn() {
		var userRights = getCookieJson('_gd1');
		var appRights  = getCookie('iap', false);

		return ('loggedIn' in userRights && userRights.loggedIn) || (appRights === true || appRights === "true");
	}

	function hasPlusGrant() {
		var userRights = getCookieJson('_gd1');
		var appRights  = getCookie('iap', false);

		return ('hasPlusGrant' in userRights && userRights.hasPlusGrant) || (appRights === true || appRights === "true");
	}

	function hasPurGrant() {
		var userRights = getCookieJson('_gd1');
		var appRights  = getCookie('iappur', false);

		return ('hasPurGrant' in userRights && userRights.hasPurGrant) || (appRights === true || appRights === "true");
	}

	function hasZuckergussGrant() {
		var userRights = getCookieJson('_gd1');

		return ('hasZuckergussGrant' in userRights && userRights.hasZuckergussGrant);
	}

	function addClassToElement(className, elementSelector) {
		if (element = document.querySelector(elementSelector)) {
			element.classList.add(className);
		} else {
			document.addEventListener("DOMContentLoaded", function() {
				document.querySelector(elementSelector).classList.add(className);
			});
		}
	}

	// Dynamic script loader with optional callback.
	(function(baseObj) {
		baseObj.scriptReady = function(src, attributes, callback) {
			var s = document.querySelector('script[src="' + src + '"]');

			if (s === null) {
				// Create the script tag.
				var h = document.querySelector('head');
				s = document.createElement('script');
				s.src = src;
				s.type='text/javascript';

				// Add optional attributes.
				if (typeof attributes == 'object') {
					for (var name in attributes) {
						if (Object.prototype.hasOwnProperty.call(attributes, name)) {
							s.setAttribute(name, attributes[name]);
						}
					}
				}

				h.appendChild(s);

				// Add eventlistener for the callback
				if (typeof callback == 'function') {
					s.addEventListener('load', function(e){
						e.target.setAttribute('loaded', 'true');
						setTimeout(callback, 1);
					});
				}
			} else {
				if (typeof callback == 'function') {
					if(s.getAttribute('loaded') == 'true'){
						setTimeout(callback, 1);
					}else{
						s.addEventListener('load', function(e){
							e.target.setAttribute('loaded', 'true');
							setTimeout(callback, 1);
						});
					}
				}
			}
		}
	})(window);
</script>

<script type="application/javascript">
		var UC_UI_SUPPRESS_CMP_DISPLAY = true;

		//load uc script dynamically
		function loadUCScript(id) {
			var cmpTag = document.createElement('script');
			cmpTag.setAttribute('data-settings-id', id);
			cmpTag.setAttribute('id', 'usercentrics-cmp');
			cmpTag.src = 'https://app.usercentrics.eu/browser-ui/latest/loader.js';
			firstScript = document.getElementsByTagName('script')[0];
			firstScript.parentNode.insertBefore(cmpTag, firstScript);
		}

		//show or hide first layer
		function firstLayerToggle(status) {
			if (status) {
				window.wts && window.wts.push(["send", "click", { linkId: "PUR-PURLayer-View" }]);
				window.firstLayerShown = true;
				addClassToElement('showFirstLayer', 'body');
			} else {
				document.querySelector('body').classList.remove('showFirstLayer');
			}
		}

		function isWhiteCardPage() {
			return false;
		}

		if (hasPurGrant() || isWhiteCardPage()) {
			if (hasPurGrant()) {
				localStorage.setItem('aa_consentGroup', 'Pur');
			}
			console.log(isWhiteCardPage() ? 'isWhiteCardPage' : 'User is PurSubscriber');
			addClassToElement('aa-pur-subscriber', 'body');
			window.addEventListener('UC_UI_INITIALIZED', function (event) {
				console.log('Used cmp settingsId: ' + UC_UI.getSettingsCore().id);
			});
			loadUCScript('FYcGWnPFP');	// PUR settings
		} else {
			console.log('User is NO PurSubscriber');
			window.addEventListener('UC_UI_INITIALIZED', function (event) {
				console.log('Used cmp settingsId: ' + UC_UI.getSettingsCore().id);

				if (UC_UI.isConsentRequired() === true) {
					console.log('Consent required - showing modal');
					firstLayerToggle(true);
				} else if (UC_UI.areAllConsentsAccepted() === true) {
					console.log('All Consents are accepted');
				} else {
					console.log('Not all Consents given - showing modal');
					firstLayerToggle(true);
				}
			});
			loadUCScript('xxC_4yYNi');	// common settings
		}
	</script>

