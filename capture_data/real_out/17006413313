<script type="text/plain" data-usercentrics="Google Ad Manager">
		var gdCookie   = getCookie('_gd');
		var userStatus = "guest";

		if (gdCookie) {
			gdCookie     = gdCookie.replace(/\"/g, '');
			var gdValue  = gdCookie.split(',');
			var loggedIn = (gdValue[0] == 'true');
			var hasGrant = (gdValue[1] == 'true');

			if (loggedIn && hasGrant) {
				userStatus = "subscribed";
			} else if (loggedIn) {
				userStatus = "registered";
			}
		}

		if ((userStatus === "guest") || (userStatus === "registered")) {
			var imported = document.createElement('script');
			imported.src = 'https://static.wlct-one.de/p/bundles/nk5XJtZZJrJ87gCvxE9D3itc.js#wbss';
			document.head.appendChild(imported);
		}
	</script>
