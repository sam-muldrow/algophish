        function trackPermissionEvents(event, action) {    
        }
        function promptAndSubscribeUser(displayNativePrompt, slidePromptDisable) {
            window.OneSignal.isPushNotificationsEnabled(function(isEnabled) {
                if (!isEnabled) {
                    window.OneSignal.push(function() {
                        if(displayNativePrompt){
                            window.OneSignal.showNativePrompt();
                            return;
                        }
                        if(!slidePromptDisable){
                            window.OneSignal.showSlidedownPrompt();
                            return;
                        }
                    });
                }
            });
        }
        function tagToUser(key, value) {
            window.OneSignal.push(function() {
                window.OneSignal.sendTag(key, value);
            });
        }
        function registerVisit(topic, numberOfVisits){
            function getCurrentStatusObject(topicTagName, freqTopicName){
                let promise = new Promise(function(resolve, reject){
                    window.OneSignal.push(function() {
                        window.OneSignal.getTags(function(tags) {
                            const currentStatus = {
                                isManuallySuscribed: tags && tags[topicTagName] && tags[topicTagName] === 's',
                                frecuencyValue: tags && tags[freqTopicName] ? parseInt(tags[freqTopicName]) : NaN
                            };
                            resolve(currentStatus);
                        });
                    });
                });
                return promise;
            }
            window.OneSignal.push(function() {
              window.OneSignal.isPushNotificationsEnabled(function(isEnabled) {
                if (isEnabled) {
                    const topicTagName = "t_";
                    const freqTopicName = "tf_";
                    getCurrentStatusObject(topicTagName, freqTopicName).then(function(currentStatusObject) {
                        let { isManuallySuscribed, frecuencyValue } = currentStatusObject;
                        if (!isNaN(frecuencyValue)) {
                            frecuencyValue += 1;
                        } else {
                            frecuencyValue = 1;
                        }
                        tagToUser(freqTopicName, frecuencyValue);
                        if (!isManuallySuscribed && frecuencyValue >= numberOfVisits) {
                            tagToUser(topicTagName, 'as');
                        }
                    });
                }
              });
            });
        }
        
        function deleteUserTags(){
            window.OneSignal.push(function() {
                window.OneSignal.getTags(function (tags) {
                    window.OneSignal.deleteTags(Object.keys(tags).filter(key => key.startsWith("t")))
                })
            })
        }
    }
