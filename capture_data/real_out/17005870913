<script>

                function verifyConsentUsers(euCallback, defaultCallbak) {

            $.ajax({
                url: "/wp-json/lindito/eu/get_cf_ipcountry",
                type: "GET",
                contentType: "application/json",
                success: function(response){
                    if(response.data.isEUResponse){ //If the User is from EU
                        // console.log(response.data.countryCode);

                        //Delete cookies that were implemented before
                        var oldGDPRUSer = getCookieAtFirst("metaspoonTestUser");
                        if(oldGDPRUSer !== ""){
                            delete_cookieAtFirst('metaspoonConsent');
                            delete_cookieAtFirst('metaspoonConsentAgreed');
                            delete_cookieAtFirst('metaspoonTestUser');
                        }

                        var consentCookieV2 = getCookieAtFirst("metaspoonConsent");
                        if(consentCookieV2 === ""){
                            delete_cookie('metaspoonDefaultConsent');

                            setCookieAtFirst("metaspoonConsent", "non-personalized", 24);
                            setCookieAtFirst("metaspoonConsentAgreed", "no-data", 24);
                        }

                        if(typeof euCallback === 'function') euCallback();
                        else return true;

                    }else{
                        delete_cookie('metaspoonConsent');
                        delete_cookie('metaspoonConsentAgreed');
                        delete_cookie('metaspoonGDPRUser');

                        setCookieAtFirst("metaspoonDefaultConsent", "non-EU", 365*24);

                        if(typeof defaultCallbak === 'function') defaultCallbak();
                        else return false;
                    }
                }
            });

            // }
        }

                function loadConsentAssets(){

            //Add Privacy-consent JS file to HEAD
            var jsFile = document.createElement('script');
            jsFile.setAttribute("type","text/javascript");
            jsFile.setAttribute("src", "https://www.lindito.com/wp-content/themes/lindito/js/privacy-consent-v2.js?ver=1.0.692");
            document.getElementsByTagName("head")[0].appendChild(jsFile);

            //CSS for privacy-consent
            var cssFile = document.createElement("link");
            cssFile.setAttribute("rel", "stylesheet");
            cssFile.setAttribute("type", "text/css");
            cssFile.setAttribute("href", "https://www.lindito.com/wp-content/themes/lindito/css/privacy-consent-v2.css?ver=1.0.692");
            document.getElementsByTagName("head")[0].appendChild(cssFile);

        }

                function loadNonEUAssets() {

        }

                var consentCookieV2 = getCookieAtFirst("metaspoonConsent");
        var consentDefault = getCookieAtFirst("metaspoonDefaultConsent");
        if( (consentCookieV2 === "" && consentDefault === "") || (consentCookieV2 !== "" && consentDefault !== "") ){
            verifyConsentUsers(loadConsentAssets, loadNonEUAssets);
        } else if(consentCookieV2 !== "" && consentDefault === ""){
            loadConsentAssets();
        }

    </script>
<script>

        var screenW = window.innerWidth > screen.width ? screen.width  : window.innerWidth;

        var deviceTest = 'd';
        /* This decided what platform is being used depending on screensize and orientation */
        if (screenW <= 480 && screenW >= 0 ){
            deviceTest = 'm'; //sets platform as mobile
        } else if (screenW <= 960 && screenW > 599){
            deviceTest = 't'; //sets platform as tablet
        } else {
            deviceTest = 'd'; //sets platform as desktop
        }

        function escapeHtml(string) {
            return String(string).replace(/[&<>"'\/]/g, function (s) {
                return entityMap[s];
            });
        }

        var params = {};
        if (location.search) {
            var parts = location.search.substring(1).split('&');
            for (var i = 0; i < parts.length; i++) {
                var nv = parts[i].split('=');
                if (!nv[0]) continue;
                params[nv[0]] = nv[1] || true;
            }
        }

        function setTestVar(urlToLoad, queryStr){

            var randNum = Math.floor(Math.random() * 2) + 1;
            var randTstVar = "";

            switch(deviceTest){
                case 'm':
                    break;

                case 't':
                    break;

                case 'd':
                    break;

                default:
                    break;
            }

            if(queryStr !== '' && randTstVar !== ''){
                window.location = urlToLoad + "&testvar="+randTstVar;
            }else{
                window.location = urlToLoad + "?testvar="+randTstVar;
            }

        }

    </script>
