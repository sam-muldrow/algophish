<script>

            var getQueryString = function ( field, url ) {
                var href = url ? url : window.location.href;
                var reg = new RegExp( '[?&]' + field + '=([^&#]*)', 'i' );
                var string = reg.exec(href);
                return string ? string[1] : null;
            };

            var getQstr = getQueryString('testvar');

            function openNav() {

                var element = document.querySelector("#mySidenav");

                if(element.style.left === "0px"){
                    closeNav();
                }else{

                    var _root = document.getElementsByTagName( 'html' )[0];

                    element.style.left = "0px";

                    var menu, name, arr;
                    menu = document.getElementById("mobile_menu");
                    name = "menu-close";
                    arr = menu.className.split(" ");
                    if (arr.indexOf(name) === -1) {
                        menu.className += " " + name;
                    }

                    _root.className += " nav-open";
                }

            }

            function closeNav() {

                if(device === 'd'){
                    document.getElementById("mySidenav").style.left = "-200px";
                }else{
                    document.getElementById("mySidenav").style.left = "-100%";
                }
                var _root = document.getElementsByTagName( 'html' )[0];
                _root.classList.remove("nav-open");

                var menu = document.getElementById("mobile_menu");
                menu.className = menu.className.replace( /(?:^|\s)menu-close(?!\S)/ , '' );

            }
        </script>
