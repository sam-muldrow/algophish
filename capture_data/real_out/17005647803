        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());

        gtag('config', 'UA-108501590-1');
    </script>   <script src="https://trustseal.e-rasaneh.ir/trustseal.js"></script> <script>eRasaneh_Trustseal(74806, false);</script> </body> </html>