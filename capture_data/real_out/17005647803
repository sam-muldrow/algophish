                </script>             </div>         </div>     </div> </div>     <script>
        (()=>{
            const tab4 = document.getElementById('tab-4');
            const tabXTemplate = document.querySelectorAll('#myTabsInner2 > li > a');
            let xTemplate = true;
            if (tab4){
                tab4.addEventListener('click', ()=> {
                    const con3 = document.getElementById('con3');

                    if (con3 && xTemplate ){
                        con3.innerHTML = con3.children[0].innerHTML;
                        xTemplate = !xTemplate;
                    }
                });
            }
            if (tabXTemplate.length > 0){
                tabXTemplate.forEach((item)=>{
                    item.addEventListener('click', function () {
                        const elementTarget = document.querySelector(this.attributes.href.nodeValue);
                        if (elementTarget.firstElementChild.nodeName == "SCRIPT"){
                            elementTarget.innerHTML = elementTarget.firstElementChild.innerHTML;
                        }
                    });
                });
            }
        })();
    </script> <div class="up-hot">      	<div style="display:none;">  	 </div> </div>         <header class="header-new-section white-section">
