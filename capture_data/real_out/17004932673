        function adDetection() {
            if (TftAdEngineBlocked) {
                console.log("AdBlockerDetected");
                ga('send', 'event', 'AdBlocker', 'ads', 'ads');
            } else console.log("AdBlockerNotDetected");
        }

        window.onload = adDetection;

    </script>


