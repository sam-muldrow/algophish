    <script src="/build/frontend/require.js"></script>

                                
    <script type="text/javascript">

        (function() {
            var config = {
                baseUrl: '/app/js',
                waitSeconds: 7,
                paths: {
                    'jquery-master': '/bundles/frontend/resources/bower/jquery/dist/jquery',
                    'backbone-master': '/bundles/frontend/resources/bower/backbone/backbone',
                    'underscore-master': '/bundles/frontend/resources/bower/underscore/underscore',
                    'modernizr' : '/bundles/frontend/resources/bower/modernizr/modernizr',
                    'slick' : '/bundles/frontend/resources/bower/slick-carousel/slick/slick',
                    'swiper' : '/bundles/frontend/resources/swiper.min',
                    'jquery-sticky-kit': '/bundles/frontend/resources/jquery.sticky-kit.min',
                    'lazysizes': '/bundles/frontend/resources/lazysizes.min',
                    'mobile-detect' : '/bundles/frontend/resources/bower/mobile-detect/mobile-detect',
                    'homad' : '/bundles/frontend/resources/modified_sources/homad',
                    'pa-embed' : '/bundles/frontend/resources/pa-embed/PAEmbedController'
                },
                config: {
                    init: {
                        modules: []
                    }
                }
            };

            requirejs.config(config);

            define('init', ['module', '/app/js/config.js'], function (module) {
                var modules = module.config().modules;
                for (var i = 0; i < modules.length; i++) {
                    require([modules[i]]);
                }
            });

            require(['init']);
        })();
    </script>


        
            <script>
        if (displayCleverpush) {
            window.addEventListener("load", function (event) {
                var cleverpushURL = "https://static.cleverpush.com/channel/loader/sCG6u65ZJ8xPxpTPi.js";

                const script = document.createElement('script');
                script.src = cleverpushURL;
                document.body.appendChild(script);
                console.log("Consentmanager: localCall ConsentGranted Executed: Cleverpush");
            }, false);
        }
    </script>

        
        
        
    </body>
