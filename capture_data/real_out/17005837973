<script src="https://www.syracuse.edu/assets-static/CACHE/js/carbon.be95a0ea157f.js"></script>

<!-- body js inject -->


<script type="text/javascript">
/**
* Playback on hover and focus for any and all of these blocks
*/

const heroVideoList = document.getElementsByClassName('hero__video');
const HeroVideoContainers = Array.from(heroVideoList);

HeroVideoContainers.forEach(function(heroVideoContainer) {
    const play = heroVideoContainer.children[0];
    const pause = heroVideoContainer.children[1];
    const video = heroVideoContainer.children[2];
	const image = heroVideoContainer.children[3];

	const sources = video.querySelectorAll('source');
	for (let i=0; i<sources.length; i++) {
		let value = sources[i].getAttribute('data-src');
		sources[i].setAttribute('src', value );
		video.load()
	}
	if (Carbon.utils.reduceMotion === false){
		play.classList.toggle('hidden');
		pause.classList.toggle('hidden');
		// Only autoplay if 'prefers reduce motion' is not set
		video.play()
	}

    play.addEventListener('click', function(){
        video.play()
		play.classList.toggle('hidden');
        pause.classList.toggle('hidden');
        pause.focus()
    });
    pause.addEventListener('click', function(){
        video.pause()
        play.classList.toggle('hidden');
        pause.classList.toggle('hidden');
        play.focus()
    });
    play.addEventListener('keydown', function(event){
        if (event.key === 'Enter'){
            video.play()
            play.classList.toggle('hidden');
			pause.classList.toggle('hidden');
            pause.focus();
        }
    });
    pause.addEventListener('keydown', function(event){
        if (event.key === 'Enter'){
            video.pause();
            play.style.visibility = "visible";
            pause.style.visibility = "hidden";
            play.focus();
        }
    });
});

</script>


