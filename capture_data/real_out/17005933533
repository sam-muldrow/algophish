<script>
	
	$(function(){
		var lastScrollTop=0;
		var timeOut = "";
		$(window).scroll(function(event){
			var st=$(this).scrollTop();
			clearTimeout(timeOut);
			if (st>200)
			{
				if (st>lastScrollTop){
					timeOut  = setTimeout(function() {jQuery('.activeSticky').addClass('activeStickyHide')}, 20);
					//$('.activeSticky').addClass('activeStickyHide'); 
				}
				else if (st<lastScrollTop) {
					timeOut = setTimeout(function() {jQuery('.activeSticky').removeClass('activeStickyHide')}, 20);
					//$('.activeSticky').removeClass('activeStickyHide'); 
				}
			}
			else
			{
				jQuery('.activeSticky').removeClass('activeStickyHide');
			}
			lastScrollTop=st;
		});
	})
</script>
