<script>
/* Адаптация iframe по высоте под ширину страницы при ресайзе страницы */
var BannerInitialHeight;
$(window).resize(function(){
            if (($('#banner_top iframe').width() < 970) && ($('#banner_top iframe').width() > 50))
            {
                 var BannerHeight = $('#banner_top iframe').height()*($('#banner_top iframe').width()/1500)
                 $('#banner_top').css('height', BannerHeight + 'px' );
            }
            else {
                    $('#banner_top').css('height', BannerInitialHeight + 'px' );
            }
})
</script>
			
			

			

			


	
	
	
	
	

	




	

			
