<script>
$(document).ready(function(e) {
	<!-- Изображение в качестве бэкграунда на главной -->
	$('.sectionMain.widget-group-news_articles .non-sidebar-widget .avatarList li:first-child').each(function( index ) {
		if (!$(this).hasClass('thread-1482592'))/*Исключение для конкретной темы*/{
			var bgsource = $(this).find('.widget_thread_content').find('>img').attr('src');
			if (!bgsource) bgsource=$(this).find('.widget_thread_content').find('.LbTrigger').attr('href');
			if (!bgsource) bgsource=$(this).find('.widget_thread_content').find('.bbCodeImage').attr('src');
			if (bgsource) $(this).css('background-image', 'url('+bgsource+')');
		}
		
	});
	<!-- //Изображение в качестве бэкграунда на главной -->
		
	/*слайдер*/
	slider_containers = ['.widget-group-hot_threads .avatarList>ul', '.widget-group-autolavka_top .avatarList>ul', '.widget-group-autolavka_main .avatarList>ul', '.widget-group-dealers_news .avatarList>ul', '.widget-group-articles .avatarList>ul', ];
	jQuery.each(slider_containers, function( index, value ) {
		var a = ($(value).children().not('.slide_show_more_stub').length*$(value).children().filter(':first').width()); //Ширина всех блоков
		if (a > $(value).width())
		{
			$(value).closest('.avatarList').siblings('.flipper').addClass('active');
		}
		else 
		{
			$(value).closest('.avatarList').siblings('.flipper').removeClass('active');
		}
		/*console.log($(value).closest('.avatarList'));*/
		
	});
	$(window).resize(function() {
		slider_reset();
	});
	scrolling = 0;
	$('.flipp_left.active').click(function(){
		if (scrolling == 0)
		{
			scrolling = 1;
			var slider = $(this).siblings('.avatarList').children('ul');
			slider_slide(slider, 'right', scrolling);
		}
	});
	$('.flipp_right.active').click(function(){
		if (scrolling == 0)
		{
			scrolling = 1;
			var slider = $(this).siblings('.avatarList').children('ul');
			slider_slide(slider, 'left', scrolling);
		}
	});
	
	
	$('.widget-group-last_threads_posts .widget').on('scroll', function(){
		var listHeight = $(this).children('.avatarList').height();
		if ($(this).scrollTop() > (listHeight-$(this).height())) 
		{
			$(this).find('.scroll_shadow').css('display', 'none');
		}
		else 
		{
			$(this).find('.scroll_shadow').css('display', 'block');
		}
	});
	/* Подгрузка новостей и тем AJAX */
	$('.a_show_more').on('click', function(){
		var selector = $(this).data('section');
		var show = $(this).data('show');
		if ($(this).data('place')=='inner' || $(this).data('place')=='innerAutolavka')//Добавляем блоки внутрь блока
		{
			var containerSelector = selector + ' .avatarList>ul';
			var cutOffHigher = $(containerSelector + ' li:last-child').data('posttimestamp');//Максимальное время
			var offset=$(containerSelector).children('li').length;
		}
		else if ($(this).data('place')=='outer')//Создаем сестринский блок и подгружаем данные в него - для блоков со слайдером
		{
			var containerSelector = selector;
			var cutOffHigher = $(containerSelector + ' .WidgetFramework_WidgetRenderer_Threads:last-child li:last-child').data('posttimestamp');//Максимальное время
			offset=$(containerSelector).find('li').length;
		}
		if (selector == '.widget-group-autolavka_main') 
		{
			$(selector+' ul').css('flex-wrap', 'wrap');
			$(selector+' ul').removeClass('unwrapped');
			if (offset < 13) {offset = offset + 5;}
		}
		if (cutOffHigher === undefined){cutOffHigher = 0;} 
		else {cutOffHigher = parseInt(cutOffHigher);}
		
		XenForo.ajax(
		'contentroute/?show=' + $(this).data('show') + '&place=' + $(this).data('place') + '&offset=' + offset + '&cutOffHigher=' + cutOffHigher,
		{},
		function(ajaxData, textStatus)
		{
			if (ajaxData.templateHtml)
			{
				new XenForo.ExtLoader(ajaxData, function()
				{
					$(ajaxData.templateHtml).xfInsert('appendTo', containerSelector);
				});
			}
		})
		
		return false;
	})
	/* ///Подгрузка новостей и тем AJAX */
	/* Прячет второй заголовок автолавки */
		var avtolavka_h3 = $('.widget-group-autolavka_top').find('h3');
		if (avtolavka_h3.length > 0)
		{
			console.log(avtolavka_h3);
			$('.widget-group-autolavka_main').find('h3').remove();
		}
	/* ///Прячет второй заголовок автолавки */
});
/*слайдер*/
function slider_reset()
{
	jQuery.each(slider_containers, function( index, value ) {
		$(value).css('left', '0%');
	});
}
function slider_slide(slider, direction, scrolling)
{
	var slider_width =  parseInt(slider.css('width'));
	var slider_elem_width = parseInt(slider.children('li').css('width'));
	var offset = slider_elem_width; //На сколько смещать?
	var currentOffset= parseInt(slider.css('left'));// На сколько уже смещено?
	if(screen.width <= 1024){var subElementsCount = slider.children().length;}
	else{var subElementsCount = slider.children().not('.slide_show_more').length;}
	var maxOffset = -subElementsCount*slider_elem_width+slider_width; //Максимально возможное смещение
	var rightElemNum = (-currentOffset+slider_width)/slider_elem_width; //Номер правого элемента
	var leftElemNum = (-currentOffset)/slider_elem_width+1; //Номер левого элемента
	if(direction == 'right')
	{
		if(rightElemNum>=subElementsCount)
		{
			newOffset = 0; //Новое смещение
		}
		else
		{
			newOffset = currentOffset-slider_elem_width//Новое смещение
		}
	}
	if(direction == 'left')
	{
		if(rightElemNum>=subElementsCount)
		{
			newOffset = 0; //Новое смещение
		}
		else
		{
			newOffset = currentOffset-slider_elem_width//Новое смещение
		}
	}
	if(direction == 'right')
	{
		if(leftElemNum <=1)
		{
			newOffset = maxOffset; //Новое смещение
		}
		else
		{
			newOffset = currentOffset+slider_elem_width//Новое смещение
		}
	}
	slider.animate({
		left: newOffset+'px'
	  }, 200, "linear", function() {
		  window.scrolling=0;
	});
}
</script>

