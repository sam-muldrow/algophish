    $('.countries_version').click(function (e) {
      var _isHidden = $('#all_countries').is(':hidden')
      if (_isHidden) {
        $('#all_countries').show()
      } else {
        $('#all_countries').hide()
      }
    })
    $('#all_countries').hover(
            function () { },
            function () {
              $('#all_countries').hide()
