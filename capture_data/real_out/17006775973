    <script>
        var base_url = window.location.origin;
                $.ajax({
            url: base_url + '/js/categories-sp-menu.js',
            dataTypes: 'script',
            success: function () {
                jQuery.each(categories, (index, item) => {
                let url = item.sections[0].url;
                    $('#categories-for-mobile').append('<li>' +
                        '<a href="' + url + '">' +
                        item.title + '</a></li>');
                });
            }
        })
                var $window = $(window);
        function checkWidth() {
            var windowsize = $window.width();
            if (windowsize > 767) {
                $('#categories-bar').css('display', 'none');
            } else {
                $('#categories-bar').css('display', 'block');
                if ($('#mnav').length == 0){
                    $('#back-to-top').addClass("application-bottom-button");
                }
            }
        }
        // Execute on load
        checkWidth();
        // Bind event listener
        $(window).resize(checkWidth);
    </script>
