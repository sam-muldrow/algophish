<script type="text/javascript">
    let $userAgent = document.getElementById('user-agent');
    let $userAgentInfo = document.getElementById('user-agent-info');

    const navigator = window.navigator;
    const userAgent = navigator.userAgent;
    const normalizedUserAgent = userAgent.toLowerCase();
    const standalone = navigator.standalone;

    const isIos = /ip(ad|hone|od)/.test(normalizedUserAgent) || navigator.platform === 'MacIntel' && navigator.maxTouchPoints > 1;
    const isAndroid = /android/.test(normalizedUserAgent);
    const isSafari = /safari/.test(normalizedUserAgent);
    const isChromeIOS = /CriOS/i.test(userAgent) && /iphone|ipod|ipad/i.test(userAgent);
    const isWebview = (isAndroid && /; wv\)/.test(normalizedUserAgent)) || (isIos && !standalone && !isSafari);
    var showMenu = 0;

    if (isWebview) {
        document.getElementById("mnav").style.display = "none";
        document.getElementById("header-main").style.display = "none";
        document.addEventListener("DOMContentLoaded", function() {
            const BackToTop = document.getElementById("back-to-top");
            const SearchBtn = document.getElementById("searchBtn");
            
            if(BackToTop !== null){
                BackToTop.classList.add("application-bottom-button");
            }
            if(SearchBtn !== null){
                SearchBtn.classList.add("application-bottom-button");
            }
        });
    }
    else {
        if (window.screen.width <= 767) {
            document.getElementById("mnav").style.display = "block";
            
            
            
        }
        // Listen event change screen width
        window.addEventListener('resize', function(event) {
           if (window.screen.width < 767) {
            document.getElementById("mnav").style.display = "block";
           }
           else {
            document.getElementById("mnav").style.display = "none";
            if(showMenu == 1)showPopupMenu();
           }
           
        }, true);

        let lastScrollTop = window.pageYOffset || document.documentElement.scrollTop;

        window.addEventListener(
        'scroll',
        function handleScroll() {
            const scrollTopPosition =
            window.pageYOffset || document.documentElement.scrollTop;

            if (scrollTopPosition > lastScrollTop) {
                if (isIos && isChromeIOS) {
                    document.getElementById("nav-bottom-ios").style.backgroundColor = "#fff";
                    document.getElementById("nav-bottom-ios").style.height = "10px";
                }
            } else if (scrollTopPosition < lastScrollTop) {
                if (isIos && isChromeIOS) {
                    document.getElementById("nav-bottom-ios").style.height = "0px";
                }
            }
            lastScrollTop =
            scrollTopPosition <= 0 ? 0 : scrollTopPosition;
        },
        false,
        );
    }

    const listMenu = document.querySelectorAll("#mnav li");
    let activeMenu = 1; 
    for(let i=0; i<listMenu.length -1; i++){
        if(listMenu[i].classList.contains("nav-active")){
            activeMenu = i+1;
        }
    }

    function showPopupMenu() {
        if (showMenu == 0) {
            document.getElementById("menu" + activeMenu).classList.remove("nav-active");
            document.getElementById("menu5").classList.add("nav-active");
            document.getElementById("popupmenu").style.display = "block";
            document.getElementById("popupcontainer").classList.add("show");
        }
        else {
            document.getElementById("menu5").classList.remove("nav-active");
            document.getElementById("menu" + activeMenu).classList.add("nav-active");
            document.getElementById("popupcontainer").classList.remove("show");
            document.getElementById("popupmenu").style.display = "none";
        }
        showMenu = 1 - showMenu;
    }
</script>

    <div id="general-modal" class="modal fade modal-full" role="dialog">
        <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title"></h4>
                </div>
                <div class="modal-body"></div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">閉じる</button>
                </div>
            </div>

        </div>
    </div>

