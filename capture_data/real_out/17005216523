<script data-cfasync="false">

    window.firebaseApp = null;
    window.firebaseConfig = {
        apiKey: "AIzaSyDyWvo4tv0tExVvsVromj-pJJCLOvy9Aa4",
        authDomain: "pricecheck-messaging.firebaseapp.com",
        projectId: "pricecheck-messaging",
        storageBucket: "pricecheck-messaging.appspot.com",
        messagingSenderId: "868066929270",
        appId: "1:868066929270:web:82151bd4fcfffbd368cd57",
        measurementId: "G-HM3TFEL43D"
    };


    async function deleteToken() {
        if (typeof firebase == 'undefined') {
            await import("https://www.gstatic.com/firebasejs/8.3.2/firebase-app.js");
            await import("https://www.gstatic.com/firebasejs/8.3.2/firebase-messaging.js");
        }

        if (window.firebaseApp == null) {
            window.firebaseApp = firebase.initializeApp(window.firebaseConfig);
        }

        const messaging = firebase.messaging();

        messaging.getToken().then((currentToken) => {
        messaging.deleteToken(currentToken).then(() => {
            if (currentToken != null) {
                let fd = new FormData();
                fd.append("fcm_token", currentToken);
                fd.append("_token", "z2FMEytapJ4e7FZF0no9zmgzZnj8dmO2prwm5KFc")
                fetch("/ws/remove_fcm_token", {
                    "method": "post",
                    "body": fd
                }).then((response) => console.log(response.json()));
            }
        }).catch((err) => {
            console.log('Unable to delete token. ', err);
        });
        }).catch((err) => {
        console.log('Error retrieving registration token. ', err);
        });
    }

    async function showNotificationPrompt() {

        localStorage.setItem("seen_fcm_prompt", "yes")

        if (typeof Sweetalert2 == 'undefined') {
            await import("https://cdn.jsdelivr.net/npm/sweetalert2@11");
        }

        const getDealsConfirmBox = await Sweetalert2.fire({
            title: 'Get notified about great deals',
            text: "Would you like notifications about great deals?",
            iconHtml: '<img src="/images/logo-sm.png" />',
            customClass: {
                icon: 'swal-no-border'
            },
            showCancelButton: true,
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '#d33',
            confirmButtonText: 'Yes, Please',
            cancelButtonText: "No, Thanks.",
            position: "top"
        }).then(result => result);


        if (getDealsConfirmBox.isConfirmed === false) {
            return false;
        }

        return true;
    }

    async function loadFirebaseScripts(setupMessaging = true) {

        if (typeof firebase == 'undefined') {
            await import("https://www.gstatic.com/firebasejs/8.3.2/firebase-app.js");
            await import("https://www.gstatic.com/firebasejs/8.3.2/firebase-messaging.js");
            await import("https://www.gstatic.com/firebasejs/8.3.2/firebase-analytics.js");
        }

        if (window.firebaseApp == null) {
            window.firebaseApp = firebase.initializeApp(window.firebaseConfig);
        }

        const analytics = firebase.analytics();
        const messaging = firebase.messaging();

        if (setupMessaging) {
            if (typeof Sweetalert2 == 'undefined') {
                await import("https://cdn.jsdelivr.net/npm/sweetalert2@11");
            }
            messaging.onMessage(function(payload){
                console.log(payload);
                let body = payload.notification.body;
                let link = "";
                let icon = "";

                if (body.includes("#link:")) {
                    let parts = body.split("#link:");
                    if (parts.length == 1) {
                        body = "";
                        link = parts[0].trim();
                    } else {
                        body = parts[0].trim();
                        link = parts[1].trim();
                    }
                }

                if (typeof payload.notification.image !== 'undefined') {
                    icon = payload.notification.image;
                }

                if (typeof payload.notification.click_action !== 'undefined') {
                    link = payload.notification.click_action;
                }

                if (icon != "") {
                    const img = new Image();
                    img.src = icon;

                    let pushHtml = `<img src="${icon}" style="max-width:360px;"/>`;
                    if (link != "") {
                        pushHtml = `<a href="${link}">${pushHtml}</a>`;
                    }

                    img.onload = function() {
                        Sweetalert2.fire({
                            showConfirmButton: false,
                            padding: 0,
                            showCloseButton: true,
                            title: "<span style='font-size:0.8em;padding:5px'>" + payload.notification.title + "</span>",
                            html: pushHtml,
                        });
                    };
                } else {
                    Sweetalert2.fire({
                        showConfirmButton: true,
                        padding: 0,
                        showCloseButton: true,
                        title: "<span style='font-size:0.8em;padding:5px'>" + payload.notification.title + "</span>",
                        html: "<p>" + body + "</p>",
                        showCancelButton: true,
                        confirmButtonColor: '#3085d6',
                        cancelButtonColor: '#d33',
                        cancelButtonText: "Not interested.",
                        confirmButtonText: 'Learn more'
                    });
                }

            });
        }

        if ('serviceWorker' in navigator) {
            navigator.serviceWorker.register('/firebase-messaging-sw.js');
            navigator.serviceWorker.ready.then((serviceWorkerRegistration) => {

                // Notification.permission == 'granted' || -- add this back when onesignal users are all converted over.
                if (Notification.permission == 'denied') {
                    return true;
                }

                if (localStorage.getItem('seen_fcm_prompt') == "yes") {
                    return true;
                }

                showNotificationPrompt().then(allowed => {
                    if (allowed) {
                        // Native browser prompt
                        Notification.requestPermission().then((permission) => {
                            if (permission != "granted") {
                                console.log("Push messaging: permission denied")
                                return false;
                            }

                            messaging.getToken().then((currentToken) => {
                                if (currentToken) {
                                    let fd = new FormData();
                                    fd.append("fcm_token", currentToken);
                                    let metaEl = document.querySelector('meta[name="csrf-token"]')
                                    if (metaEl) {
                                        fd.append("_token", metaEl.content)
                                    } else {
                                        fd.append("_token", "z2FMEytapJ4e7FZF0no9zmgzZnj8dmO2prwm5KFc")
                                    }

                                    fetch("/ws/save_fcm_token", {
                                        "method": "post",
                                        "body": fd
                                    }).then((response) => console.log(response.json()));
                                } else {
                                    console.log(
                                        'No registration token available. Request permission to generate one.'
                                    );
                                }
                            }).catch((err) => {
                                console.log(
                                    'An error occurred while retrieving token. ',
                                    err);
                            });
                            // end get token
                        });
                        // end native browser prompt
                    }
                });

            });
        }
    }

    window.addEventListener('load', () => {

        loadFirebaseScripts();

    });
</script>
