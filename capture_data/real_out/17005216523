<script data-cfasync="false">
    if (typeof getCookie !== "function") {
        var getCookie = function (name) {
            var match = document.cookie.match(new RegExp('(^|; )' + name + '=([^;]+)'));
            return match ? match[2] : null;
        }
    }

    if (typeof setCookie !== "function") {
        var setCookie = function (name, value) {
            //console.log('f setCookie');
            var now = new Date();
            var time = now.getTime();
            var expireTime = time + 1000 * 60 * 60 * 24 * 30; //30d

            now.setTime(expireTime);
            document.cookie = name + '=' + value + '; expires=' + now + ';path=/';
            localStorage.setItem('expireTime', expireTime);
        }
    }

    if (typeof setCSRFToken !== "function") {
        function setCSRFToken() {
            // add token to form
            let oReq = new XMLHttpRequest();
            //pass the search term in too because we use in in the server-side call to build the RC values
            oReq.onload = function (data) {
                if (data.target.response && data.target.response.token) {
                    var metaEl = document.querySelector('meta[name="csrf-token"]');
                    if (metaEl) {
                        metaEl.content = data.target.response.token;
                    }
                }
            };
            oReq.open('GET', '/csrf', true);
            oReq.responseType = 'json';
            oReq.send();
        }
    }

    if (typeof getCSRFToken !== "function") {
        function getCSRFToken() {
            var metaEl = document.querySelector('meta[name="csrf-token"]');
            if (metaEl && metaEl.content) {
                return metaEl.content;
            }
            return null;
        }
    }

    //critical functions here (can't be deferred)
    if (typeof lazyLoad2021 !== "function") {
        function lazyLoad2021(img) {
            const picture = img.parentElement;
            const src = img.getAttribute('data-src');
            if (picture) {
                const source = picture.querySelector('.lazy-source');
                if (source) {
                    const sourceSrc = source.getAttribute('data-srcset');
                    if (source && sourceSrc) {
                        source.srcset = source.getAttribute('data-srcset');
                    }
                }
            }

            if (src) {
                img.src = src;
            }
        }
    }

    if (typeof lazyLoadBackground2021 !== "function") {
        function lazyLoadBackground2021(element) {
            const src = element.getAttribute('data-background-src');
            if (src !== "") {
                element.style.backgroundImage = "url('"+src+"')";
            }
        }
    }

    //all the lazy loading image stuff is here
    const imgObserver = new IntersectionObserver((entries, self) => {
        entries.forEach(entry => {
            if (entry.isIntersecting) {
                lazyLoad2021(entry.target);
                self.unobserve(entry.target);
            }
        });
    });

    const imgObserverBackground = new IntersectionObserver((entries, self) => {
        entries.forEach(entry => {
            if (entry.isIntersecting) {
                lazyLoadBackground2021(entry.target);
                self.unobserve(entry.target);
            }
        });
    });

    if (typeof importJS2021 !== "function") {
        function importJS2021(src) {
            return new Promise(resolve => {
                var s = document.createElement('script');
                var head = document.getElementsByTagName('head')[0];

                s.onload = resolve;
                s.src = src;

                if (head) {
                    head.appendChild(s);
                } else {
                    document.body.appendChild(s);
                }

            });
        }
    }

    if (typeof importCSS2021 !== "function") {
        function importCSS2021(src) {
            return new Promise(resolve => {
                var s = document.createElement('link');
                s.rel = "stylesheet";
                var head = document.getElementsByTagName('head')[0];

                s.onload = resolve;
                s.href = src;

                if (head) {
                    head.appendChild(s);
                } else {
                    document.body.appendChild(s);
                }

            });
        }
    }

    //we need to do this for cachebusting!!
    setCSRFToken();
</script>
