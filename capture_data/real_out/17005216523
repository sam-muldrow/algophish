<script data-cfasync="false">

    function closeBfForm() {
        document.getElementById("bfslide-in-form").classList.add("bf-slide-out")
    }

    window.addEventListener("load", function(){
        const seenSlideIn = localStorage.getItem("bf_2023");
        if (seenSlideIn == "yes") {
         return;
        }

        if (typeof Sweetalert2 == 'undefined') {
            import("https://cdn.jsdelivr.net/npm/sweetalert2@11");
        }

        if (typeof grecaptcha == 'undefined') {
            const recap = document.createElement('script');
            recap.src = "https://www.google.com/recaptcha/api.js?render=6Lcp5f4UAAAAAIsYB19YsmfqZy7A93TTMp40Oltu";
            document.body.appendChild(recap);
        }
        
        setTimeout(function() {
                showSubscribeForm();
        }, 1000);
   });

   function showSubscribeForm() {
        bfFormHandlerEventSetup = true;
        document.getElementById("bfslide-in-form").classList.add("bf-slide-in")
        localStorage.setItem("bf_2023", "yes");


        document.getElementById("bf_slidein_subscribe_form").addEventListener('submit', (e) => {
        e.preventDefault();
        const loader =  document.getElementById("bf_slidein_subscribe_form_submit_loader");
        loader.style.display = "block";

        const name = document.getElementById("bf_slidein_subscribe_fullname");
        const email = document.getElementById("bf_slidein_subscribe_email");
        const terms_agree = document.getElementById("bf_slidein_subscribe_terms");
        const emailRegex = /^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\.[a-zA-Z0-9-]+)*$/;

        if (name.value.length < 3 || email.value.length < 5 || !email.value.match(emailRegex)) {
            Sweetalert2.fire(
                'Oops! please fill in all required fields',
                'Please fill in your name and email address.',
                'error'
            )
            return;
        }

        if (!terms_agree.checked) {
            Sweetalert2.fire(
                'Oops! please except our terms',
                'Please accept our terms and conditions to continue.',
                'error'
            )
            return;
        }

        grecaptcha.ready(function() {
            grecaptcha.execute('6Lcp5f4UAAAAAIsYB19YsmfqZy7A93TTMp40Oltu', {action: 'newsletter_subscribe'}).then(function(token) {
                const fd = new FormData();
                fd.append("name", name.value);
                fd.append("email", email.value);
                fd.append("return_json", 1);
                fd.append("_token", document.querySelector('meta[name="csrf-token"]').getAttribute("content"))
                fd.append("g-recaptcha-response", token);
                fd.append("source", "bfsub_form_11_2023");

                fd.append("receive_newsletter", 1);

                fetch("/newsletter/subscribeToMailNewsletter", {
                    method: "post",
                    body: fd
                }).then(response => {
                    if (response.status == 201 || response.status == 200) {
                        Sweetalert2.fire(
                            'Success!',
                            "Thank you. Successfully subscribed to our Black Friday mailing list.",
                            'success'
                        );
                        closeBfForm();
                    } else {
                        Sweetalert2.fire(
                            'Oops! something went wrong',
                            'Please check the details submitted and try again.',
                            'error'
                        );
                    }
                }).catch((err) => {
                    Sweetalert2.fire(
                        'Oops! something went wrong',
                        'Please check the details submitted and try again.',
                        'error'
                    );

                }).finally(() => {
                    loader.style.display = "none";
                })

            });
        });
    });
   }

</script>
