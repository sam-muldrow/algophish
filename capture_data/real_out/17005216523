<script data-cfasync="false">
    const pcDetectRobot = (userAgent) => {
        const robots = new RegExp([
            /bot/,/spider/,/crawl/,                            // GENERAL TERMS
            /APIs-Google/,/AdsBot/,/Googlebot/,                // GOOGLE ROBOTS
            /mediapartners/,/Google Favicon/,
            /FeedFetcher/,/Google-Read-Aloud/,
            /DuplexWeb-Google/,/googleweblight/,
            /bing/,/yandex/,/baidu/,/duckduck/,/yahoo/,        // OTHER ENGINES
            /ecosia/,/ia_archiver/,
            /facebook/,/instagram/,/pinterest/,/reddit/,       // SOCIAL MEDIA
            /slack/,/twitter/,/whatsapp/,/youtube/,
            /semrush/,                                         // OTHER
        ].map((r) => r.source).join("|"),"i");               // BUILD REGEXP + "i" FLAG

        return robots.test(userAgent);
    };

    if (typeof mauticTrack !== "function") {
        function mauticTrack() {

            var tags = [];

                            tags.push('device:::desktop');
            
            
                
            

            
            //category
                                        
            //product/offers
            
                
            

            var manufacturerMetaEl = document.querySelector('meta[itemprop="Manufacturer"]');
            if (manufacturerMetaEl && manufacturerMetaEl.content) {
                // tags['brand:::' + manufacturerMetaEl.content] = 1;
                tags.push('brand:::'+manufacturerMetaEl.content);
            }


            (function(w,d,t,u,n,a,m){w['MauticTrackingObject']=n;
                w[n]=w[n]||function(){(w[n].q=w[n].q||[]).push(arguments)},a=d.createElement(t),
                    m=d.getElementsByTagName(t)[0];a.async=1;a.src=u;m.parentNode.insertBefore(a,m)
            })(window,document,'script','/js/mtc.min.js?v=20231011','mt');

            // add token to form
            let oReq = new XMLHttpRequest();
            //pass the search term in too because we use in in the server-side call to build the RC values
            oReq.onload = function (data) {
                if (data.target.response && data.target.response.user) {
                    document.user = data.target.response.user;

                    if (typeof mt === "function") {
                        if (document.user && document.user.user_id) {
                            mt('send', 'pageview', {
                                email: document.user.email,
                                firstname: document.user.first_name,
                                lastname: document.user.last_name,
                                user_id: document.user.user_id,
                                tags: tags.map(tag => tag.replace(",", "_")).join(',')
                            });
                        } else {
                            mt('send', 'pageview', {
                                tags: tags.map(tag => tag.replace(",", "_")).join(',')
                            });
                        }
                    }
                } else {
                    if (typeof mt === "function") {
                        mt('send', 'pageview', {
                            tags: tags.map(tag => tag.replace(",", "_")).join(',')
                        });
                    }
                }
            };
            oReq.open('GET', '/auth/get-user', true);
            oReq.responseType = 'json';
            oReq.send();
        }
    }

    const userAgent = navigator.userAgent;
    const isRobot = pcDetectRobot(userAgent);

    if (!isRobot) {
        window.addEventListener('load',() => {
            mauticTrack();
        });
    }

