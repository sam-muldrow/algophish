<script data-cfasync="false">
function formatProductCardPrice(number) {
    number = new Intl.NumberFormat('en-US', { style: 'currency', currency: 'USD' }).format(number);
    return number.replace("$", "")
}

    window.superSuperDealsPopupEnabled = false;
    if (typeof getScrollHeight == 'undefined') {
        function getDocHeight() {
            var D = document;
            return Math.max(
                D.body.scrollHeight, D.documentElement.scrollHeight,
                D.body.offsetHeight, D.documentElement.offsetHeight,
                D.body.clientHeight, D.documentElement.clientHeight
            )
        }

        function getScrollHeight() {
            var winheight = window.innerHeight || (document.documentElement || document.body).clientHeight
            var docheight = getDocHeight()
            var scrollTop = window.pageYOffset || (document.documentElement || document.body.parentNode || document
                .body).scrollTop
            var trackLength = docheight - winheight
            var pctScrolled = Math.floor(scrollTop / trackLength * 100)
            return pctScrolled
        }
    }

    function showDrawer(e) {
        e.preventDefault();

        //if we have sticky offers on the right, reset their top so they stick nicely
        var stickies = document.getElementsByClassName('sticky-container');
        if (stickies) {
            for (let i = 0; i < stickies.length; i++) {
                stickies[i].style.top = '280px';
            }
        }

        document.getElementById("showDrawerPuller").style.display = "none";
        document.getElementById("super-deals-bar-container").style.marginBottom = "280px";
        document.getElementById("super-deals-bar").classList.add("slidein-super-deals-bar");
        document.getElementById("super-deals-bar-container").classList.remove("slideout-super-deals-bar");
        document.getElementById("super-deals-bar").classList.add("super-deals-bar-fixed");
        document.getElementById("drawer-inner").style.marginTop = "255px";

    }
    function hideDrawer(e) {
        e.preventDefault();

        //if we have sticky offers on the right, reset their top so they stick nicely
        var stickies = document.getElementsByClassName('sticky-container');
        if (stickies) {
            for (let i = 0; i < stickies.length; i++) {
                stickies[i].style.top = '0';
            }
        }

        document.getElementById("super-deals-bar-container").style.marginBottom = 0;
        document.getElementById("super-deals-bar-container").classList.add("slideout-super-deals-bar");
        document.getElementById("super-deals-bar-container").classList.remove("slidein-super-deals-bar");
        document.getElementById("showDrawerPuller").style.display = "block";
        document.getElementById("drawer-inner").style.marginTop = 0;

    }

    function writeOfferHtml(offer) {
        let  offerHtml = `<div class="product-single-card" style="flex-direction: column;justify-content: flex-start;align-items: stretch;position: relative;text-align:left;">
  <div style="align-items: stretch">
    <div style="align-items: stretch">
      <div style="-ms-flex-direction: row;-webkit-box-orient: horizontal;-webkit-flex-direction: row;flex-direction: row;">
        <a href="${offer['track_url']}&rc=SuperDeals" target="_blank" class="image-container-link"><img
             src="${offer['image']}"
             data-src="${offer['image']}"
            class="super-product-image"
            aria-label="${offer['product_title']}"
            style="width: auto;height: auto;"
            alt="${offer['product_title']}"
        /></a>
      </div>
    </div>
    <div style="flex-direction: column;justify-content: flex-start;align-items: stretch;margin-top:5px;">
      <a
        href="${offer['track_url']}&rc=SuperDeals"
        data-nb="0"
        target="_blank"
        ><span data-lines="2" data-truncate="0" style=" display: -webkit-box;-webkit-box-orient: vertical;overflow: hidden;-webkit-line-clamp: 2;text-decoration:underline">${offer['product_title']}</span
        ></a
      >
      <div style="flex-direction: column;justify-content: flex-start;align-items: stretch;">
      <div style="flex-direction: column;margin-top:10px;">
      <div style="flex-direction: row;color:red;"><span><s>R${formatProductCardPrice(offer['total_price'])}</s></span>
      </div>
    </div>
    <div

        <div style="flex-direction: column;margin-top:2px;">
          <div style=" flex-direction: row;">
            <span style="font-size:14px;font-weight:700;">R${formatProductCardPrice(offer['price'])}</span>
          </div>
        </div>
        <div
          style="flex-direction: row;justify-content: flex-start;align-items: stretch;">
          <div style="flex-direction: row;justify-content: flex-start;align-items: center;margin-top:5px;">
            <a data-lines="1" data-truncate="0" style="color:green" href="${offer['track_url']}&rc=SuperDeals" target="_blank">${offer['shop_name']}</a>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

        `;

        document.getElementById('super-deals-glider').innerHTML += offerHtml
    }


    window.addEventListener("load", function(){
        window.addEventListener("scroll", function() {
        let breakpoint = 30;
                if (window.superSuperDealsPopupEnabled || getScrollHeight() < breakpoint) {
            return;
        }

        window.superSuperDealsPopupEnabled = true;
        let endpoint = "/sliders/superdeals.json";
        let prefix = "?";

        
        let title = encodeURIComponent(window.document.title);
        let page_location = encodeURIComponent(window.location.href);

        endpoint = endpoint + prefix + "title=" + title + "&page_location=" + page_location
        fetch(endpoint).then(
            json => json.json()
        ).then(json => {
            if (json && typeof json != "undefined" && json.length > 6) {
                let numberToShow = 10;
        
                for (let offer of json) {
                    writeOfferHtml(offer);
                }

                if (json.length > 12) {
                    numberToShow = 14;
                }else if (json.length > 10) {
                    numberToShow = 12;
                }

                //if we have sticky offers on the right, reset their top so they stick nicely
                var stickies = document.getElementsByClassName('sticky-container');
                if (stickies) {
                    for (let i = 0; i < stickies.length; i++) {
                        stickies[i].style.top = '280px';
                    }
                }

                document.getElementById("super-deals-bar-container").style.marginBottom = "280px";
                document.getElementById("super-deals-bar").classList.add("slidein-super-deals-bar");
                document.getElementById("super-deals-bar").classList.add("super-deals-bar-fixed");
                document.getElementById("drawer-inner").style.marginTop = "255px";


                                    new Glider(document.getElementById('super-deals-glider'), {
                        slidesToScroll: 1,
                        slidesToShow: numberToShow,
                        draggable: false,
                        dots: '.dots',
                        arrows: {
                            prev: '.super-deals-glider-prev',
                            next: '.super-deals-glider-next'
                        }
                    });
                
                document.getElementById('super-deals-glider').addEventListener('glider-slide-visible',
                    function(event) {
                        var imgs_to_anticipate = 20;
                        var glider = Glider(this);
                        for (var i = 0; i <= imgs_to_anticipate; ++i) {
                            var index = Math.min(event.detail.slide + i, glider.slides.length - 1),
                                glider = glider;
                            loadImages.call(glider.slides[index], function() {
                                glider.refresh(true);
                            })
                        }
                    });
            }         });
    });
    });
</script>
