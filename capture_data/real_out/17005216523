<style>
    .footer-subscribe-header {
        font-weight: 500;
        font-size: 18px;
        line-height: 150%;
        color: #27272A;
    }
    .footer-subscribe-message {
        font-weight: normal;
        font-size: 16px;
        line-height: 150%;
        color: #3F3F46;
        vertical-align:bottom;
    }

    #subscribe-bar-inner input[type="text"], #subscribe-bar-inner input[type="email"] {
        border: 1px solid #D4D4D8;
        box-sizing: border-box;
        border-radius: 4px;
        padding: 10px;
        font-size: 16px
    }
    #subscribe-bar-inner input[type="submit"] {
        padding: 11px 31px;
        background: #EF4444;
