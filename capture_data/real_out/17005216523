<script type="text/javascript" data-cfasync="false">
    function showShadow() {
        document.getElementById('home-search-bar').style.backgroundColor = '#fff';
        document.getElementById('home-search-bar').style.boxShadow = '0 1px 6px rgba(32,33,36,.28)';
        document.getElementById('home-search-bar').style.borderColor = 'border-color:rgba(223,225,229,0)';
    }

    function hideShadow() {
        document.getElementById('home-search-bar').style.backgroundColor = '#fff';
        document.getElementById('home-search-bar').style.boxShadow = '';
        document.getElementById('home-search-bar').style.border = '1px solid #D4D4D8';
    }

    function showSuggestions() {
        var searchBar = document.getElementById('home-search-bar');
        var suggestions = document.getElementById('suggestionsDiv');
        var input = document.getElementById('search_input');

        if (input.value.length > 2) {
            searchBar.style.borderBottomLeftRadius = '0';
            searchBar.style.borderBottomRightRadius = '0';

            suggestions.style.width = searchBar.offsetWidth + 'px';
            suggestions.style.display = 'block';
            searchSuggest();
        }
    }

    function showClear() {
        var btn = document.getElementById('clear_button');
        var input = document.getElementById('search_input');

        if (input.value.length > 0) {
            btn.style.display = 'block';
            btn.style.visibility = 'visible';
        } else {
            btn.style.display = 'none';
            btn.style.visibility = 'hidden';
        }
    }

    function clearBox() {
        var btn = document.getElementById('clear_button');
        var input = document.getElementById('search_input');
        var searchBar = document.getElementById('home-search-bar');

        input.value = '';
        btn.style.display = 'none';
        btn.style.visibility = 'hidden';
        closeSuggestions();
        input.focus();
    }

    function closeSuggestions() {
        var searchBar = document.getElementById('home-search-bar');
        var suggestions = document.getElementById('suggestionsDiv');

        suggestions.style.display = 'none';
        searchBar.style.borderBottomLeftRadius = '24px';
        searchBar.style.borderBottomRightRadius = '24px';
    }
</script>
<script data-cfasync="false">
    //listen to a body onclick to close the suggestions
    document.body.addEventListener("click", function (evt) {
        if (evt.target.closest('#search_form') === null) {
            closeSuggestions();
        }
    });

    function searchSuggest() {
        console.log("Done typing.");
        // $("div#suggestionsDiv").hide();
        // $("div#showingResultsDiv").html("");

        var a = document.getElementById('search_input').value;

        if (!a || a == "undefined") return true;

        document.getElementById('searchloaderdiv').style.backgroundImage = "url(/images/loadert.gif)";
        let bloghtml = "";
        let syn = "";
        let sashtml = "";
        let brandblockshtml = "";
        let departmentshtml = "";
        let brandshtml = "";
        let shopshtml = "";
        let ssearcheshtml = "";
        let cmshtml = "";
        let ulshopblockshtml = "";
        let ulbrandblockshtml = "";
        let shopblockshtml = "";
        let ulspecialOffers = "";
        let specialOffers = "";
        let tagshtml = "";
        // var selectedCat = $("div#button-categorydropdown a").attr("data-catid");
        var selectedCat = null;
        console.log("fetching results and blog entries for st [" + a + "] cat [" + selectedCat + "]");

        var oReq = new XMLHttpRequest();
        let url = "/ssuggest";
        oReq.onload = function (e) {
            data = e.target.response;
            if (data.sas.length > 0) {
                for (var i = 0; i < data.sas.length; i++) {
                    sashtml +=
                        '<li class="sas item"><img class="lazy-img" data-src="' + data.sas[i]["sicon"] + '"><a href="' + data.sas[i]["surl"] + '">' +
                        '<span>' + data.sas[i]["stitle"] + '</span>' + '</a></li>';
                }
            }
            let oldst = "";
            if (data.syn) {
                oldst = a;
                a = data.syn;
            }
            if (data.departments.length > 0) {
                data.departments = data.departments.slice(0, 3);
                departmentshtml += '<li class="header"><span>IN DEPARTMENTS</span></li>';
                for (var i = 0; i < data.departments.length; i++) {
                    departmentshtml +=
                        '<li class="item"><a href="/search?search=' + encodeURIComponent(a) + '&category=' + data.departments[i].id + '">' +
                        '<span class="sterm"> ' + a + '</span><span class="in"> - </span><span class="result">' + data.departments[i].name + '</span></a></li>';
                }
                departmentshtml +=
                    '<li class="item"><a href="/search?search=' + encodeURIComponent(a) + '">' +
                    '<span class="sterm">' + a + '</span><span class="in"> - </span><span class="result">All Departments</span>&nbsp;</a></li>';
            }
            if (data.brandsuggestions.length < 1 && data.brands.length > 0) {
                brandshtml += '<li class="header"><span>IN BRANDS</span></li>';
                data.brands = data.brands.slice(0, 3);
                for (var i = 0; i < data.brands.length; i++) {
                    brandshtml +=
                        '<li class="item"><a href="/search?search=' + encodeURIComponent(a) + '&brand=' + encodeURIComponent(data.brands[i].name) + '">' +
                        '<span class="sterm">' + a + '</span><span class="in"> - </span><span class="result">' + data.brands[i].name + '</span></a></li>';
                }
                brandshtml +=
                    '<li class="item"><a href="/search?search=' + encodeURIComponent(a) + '">' +
                    '<span class="sterm">' + a + '</span><span class="in"> - </span><span class="result">All Brands</span>&nbsp;</a></li>';
            }

            if (data.tags.length > 0) {
                tagshtml += '<li class="header"><span>IN TAGS</span></li>';
                for (var i = 0; i < data.tags.length; i++) {
                    tagshtml +=
                        '<li class="item"><a href="/search?search=' + encodeURIComponent(a) + '&tags=' + encodeURIComponent(data.tags[i].name) + '">' +
                        '<span class="sterm"> ' + a + '</span><span class="in"> - </span><span class="result">' + data.tags[i].name + '</span></a></li>';
                }
                tagshtml +=
                    '<li class="item"><a href="/search?search=' + encodeURIComponent(a) + '">' +
                    '<span class="sterm">' + a + '</span><span class="in"> - </span><span class="result">All Tags</span>&nbsp;</a></li>';
            }

            if (data.shopsuggestions.length < 1 && data.shops.length > 0) {
                data.shops = data.shops.slice(0, 3);
                shopshtml += '<li class="header"><span>IN SHOPS</span></li>';
                for (var i = 0; i < data.shops.length; i++) {
                    if (a == data.shops[i].name || data.shops[i].name == "WantItAll.co.za") continue;
                    shopshtml +=
                        '<li class="item"><a href="/search?search=' + encodeURIComponent(a) + '&shop=' + data.shops[i].id + '">' +
                        '<span class="sterm"> ' + a + '</span><span class="in"> - </span><span class="result">' + data.shops[i].name + '</span></a></li>';
                }
                shopshtml +=
                    '<li class="item"><a href="/search?search=' + encodeURIComponent(a) + '">' +
                    '<span class="sterm">' + a + '</span><span class="in"> - </span><span class="result">All Shops</span>&nbsp;</a></li>';
            }


            if (data.special_offers.length > 0) {
                ulspecialOffers += '<li class="header"><span>SPECIAL OFFERS</span></li>';
                                for (var i = 0; i < data.special_offers.length; i++) {
                    let spoffer = data.special_offers[i];
                    let discount_html = "";
                    if (spoffer.discount > 0) {
                        discount_html = '<div style="position:relative;"><div class="discount-ssugest-small">SAVE '+spoffer.discount+'%</div></div>'
                    }

                    specialOffers +='<a href="'+spoffer['track_url']+'" target="_blank" title="'+spoffer['fo_title'].replace(/"/g, "")+'">'
                    specialOffers += '<div style="font-size: 14px;text-align: center;height:125px;border:1px solid #D4D4D8;border-radius:6px;">'
                    specialOffers += discount_html
                    specialOffers +=  spoffer['fo_title'].substring(0, 15)+'...<br />';
                    specialOffers += '<img class="lazy-img" style="height:66px;" data-src="'+spoffer['image_small_url']+ '" />';
                    specialOffers += '<br /> <span>'+spoffer['shop_name']+'</span><br />'
                    specialOffers += '<span style="font-weight: bold;font-size: 14px;line-height: 150%;color: #27272A;">R'+parseFloat(spoffer['price']).toFixed(2)
                    specialOffers += '</span></div></a>';
                }
            }

            if (data.stsuggestions.length > 0) {
                data.stsuggestions = data.stsuggestions.slice(0, 4);
                ssearcheshtml += '<li class="searches header"><span>SUGGESTED SEARCHES</span></li>';
                for (var i = 0; i < data.stsuggestions.length; i++) {
                    ssearcheshtml +=
                        '<li class="item"><a href="/search?search=' + encodeURIComponent(data.stsuggestions[i]["pst"]) + '">' +
                        '<span>' + data.stsuggestions[i]["pst"] + '</span></a></li>';
                }
            }
            if (data.cmspages.length > 0) {
                cmshtml += '<li class="header collections"><span>SUGGESTED COLLECTIONS</span></li>';
                for (var i = 0; i < data.cmspages.length; i++) {
                    cmshtml +=
                        '<li class="item"><a href="' + data.cmspages[i]["cmsurl"] + '"><span>' + data.cmspages[i]["cms"] + '</span></a></li>';
                }
            }
            if (data.brandsuggestions.length > 0) {
                ulbrandblockshtml = '<li class="header"><span>SUGGESTED BRANDS</span></li>';
                for (var i = 0; i < data.brandsuggestions.length; i++) {
                    brandblockshtml +=
                        '<div style="text-align: center;"><a href="/search?brand=' + encodeURIComponent(data.brandsuggestions[i]["bra"]) + '">';
                    if (data.brandsuggestions[i]["image"]) {
                        brandblockshtml +=
                            data.brandsuggestions[i]["bra"] +
                            '<br/><img style="max-height:45px;" class="lazy-img" data-src="https://images.pricecheck.co.za/images/objects/brand/' + data.brandsuggestions[i]["image"] + '">';
                    } else {
                        brandblockshtml += "<br/>" + data.brandsuggestions[i]["bra"];
                    }
                    brandblockshtml += "</a></div>";
                }
            }
            if (data.shopsuggestions.length > 0) {
                ulshopblockshtml = '<li class="header"><span>SUGGESTED SHOPS</span></li>';
                for (var i = 0; i < data.shopsuggestions.length; i++) {
                    shopblockshtml +=
                        '<div style="text-align: center;"><a href="/shops/' + data.shopsuggestions[i]["sid"] + "/" + encodeURIComponent(data.shopsuggestions[i]["psh"]) + '">' +
                        data.shopsuggestions[i]["psh"] +
                        '<br/><img class="lazy-img" data-src="https://images.pricecheck.co.za/images/objects/shop/' + data.shopsuggestions[i]["surl"] + '"></a></div>';
                }
            }
            if (data.syn.length >= 1) {
                document.getElementById("showingResultsDiv").innerHTML =
                    "Showing results for <span style='color:#0664c6;'>" + data.syn + "</span><br/>Search for <a href='/search?search=" + encodeURIComponent(oldst) + "'>" + oldst + "</a> instead"
            }

            document.querySelector('ul#ul-sas').innerHTML = sashtml;
            document.querySelector('ul#ul-departments').innerHTML = departmentshtml;
            document.querySelector('ul#ul-tags').innerHTML = tagshtml;
            document.querySelector('ul#ul-brands').innerHTML = brandshtml;
            document.querySelector('ul#ul-special-offers').innerHTML = ulspecialOffers;
            document.querySelector('div#div-special-offers').innerHTML = specialOffers;
            document.querySelector('ul#ul-shops').innerHTML = shopshtml;
            document.querySelector('ul#ul-cms').innerHTML = cmshtml;
            document.querySelector('ul#ul-ssearches').innerHTML = ssearcheshtml;
            document.querySelector('ul#ul-shopBlocks').innerHTML = ulshopblockshtml;
            document.querySelector('div#div-shopBlocks').innerHTML = shopblockshtml;
            document.querySelector('ul#ul-brandBlocks').innerHTML = ulbrandblockshtml;
            document.querySelector('div#div-brandBlocks').innerHTML = brandblockshtml;
            document.querySelector('ul#ul-blog').innerHTML = "";

            document.getElementById('suggestionsDiv').querySelectorAll('.lazy-img').forEach(img => {
                lazyLoad2021(img);
            });
        };
        oReq.open('POST', url, true);
        oReq.setRequestHeader("Content-Type", "application/x-www-form-urlencoded");
        oReq.responseType = 'json';
        oReq.send("st=" + encodeURIComponent(a));

    }
</script>
