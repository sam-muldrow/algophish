<script data-cfasync="false">
        loginComplete = false;

        let loadLoginSectionAjax = new XMLHttpRequest();
        //pass the search term in too because we use in in the server-side call to build the RC values
        let loadLoginSectionAjaxUrl = "/auth/auth-frontend-2021/?r=https%3A%2F%2Fwww.pricecheck.co.za%2F%3Fcfd%3Ddesktop&_=" + new Date().getTime();
        loadLoginSectionAjax.onload = function (response) {

            if (loadLoginSectionAjax.readyState === loadLoginSectionAjax.DONE) {
                //if we're logged out
                var priceCheckHeader = loadLoginSectionAjax.getResponseHeader('X-PC-LOGGED_OUT');
                if (priceCheckHeader !== null && priceCheckHeader === "1") {
                    importJS2021("https://accounts.google.com/gsi/client");
                } else {
                    if (typeof runLoginFunctions === "function") {
                        runLoginFunctions();
                    } else {
                        document.addEventListener('DOMContentLoaded', (event) => {
                            if (typeof runLoginFunctions === "function") {
                                runLoginFunctions();
                            }
                        });                    }
                    loginComplete = true;
                }

                const loginArea = document.getElementById('header-login-area');
                let html = this.responseXML.getElementsByTagName('body')[0].innerHTML;
                loginArea.innerHTML = html;

                [...this.responseXML.scripts].forEach(function(script) {
                    var scriptEl = document.createElement("script");
                    scriptEl.type = "text/javascript";
                    scriptEl.text = script.innerText;
                    document.getElementsByTagName('head')[0].appendChild(scriptEl);
                });

                [...this.responseXML.getElementsByTagName('style')].forEach(function(style) {
                    loginArea.prepend(style);
                });
            }
        };
        loadLoginSectionAjax.open('GET', loadLoginSectionAjaxUrl, true);
        loadLoginSectionAjax.responseType = 'document';
        loadLoginSectionAjax.send();
    </script>
