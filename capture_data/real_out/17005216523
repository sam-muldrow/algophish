<script data-cfasync="false">
                    const gliderEl = document.getElementById('promo-holder');
                    const promoGlider = new Glider(gliderEl, {
                        slidesToShow: 1,
                        draggable: true,
                        duration: 0.5,
                        // dots: '.dots',
                        // arrows: {
                        //     prev: '.dd-glider-prev',
                        //     next: '.dd-glider-next'
                        // }
                    });

                    let autoplayDelay = 3500;

                    let timeout = -1;
                    let hovering = false;
                    function startPromoSliderTimeout() {
                        clearTimeout(timeout);
                        timeout = setTimeout(() => {
                            if (!hovering)
                                promoGlider.scrollItem((promoGlider.slide + 1) % promoGlider.slides.length);
                        }, autoplayDelay);
                    }

                    let animID = 0;
                    const isAnimating = () => promoGlider.animate_id !== animID;
                    gliderEl.addEventListener("glider-animated", () => {
                        animID = promoGlider.animate_id;
                        if (!hovering) startPromoSliderTimeout();
                    });

                    gliderEl.addEventListener("mouseover", () => {
                        hovering = true;
                        clearTimeout(timeout);
                    });

                    gliderEl.addEventListener("mouseout", () => {
                        hovering = false;
                        if (!isAnimating()) startPromoSliderTimeout();
                    });

                    startPromoSliderTimeout();
                </script>
</div>
