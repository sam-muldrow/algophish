<script data-cfasync="false">
    var drawerIsOpen = false;
    function hideDrawerOverlay() {
        var drawerOverlay = document.getElementById('drawer-overlay');
        drawerOverlay.style.display = "none";
        var drawer = document.getElementById('drawer-inner');
        drawer.removeEventListener("transitionend", hideDrawerOverlay);
    }

    // function showDrawerOverlay() {
    //     var drawerOverlay = document.getElementById('drawer-overlay');
    // }

    function openDrawer() {
        var drawerOverlay = document.getElementById('drawer-overlay');
        drawerOverlay.style.backgroundColor = "rgba(0, 0, 0, 0.5)";
        drawerOverlay.style.display = "block";
        var drawer = document.getElementById('drawer-inner');
        // drawer.addEventListener("transitionend", showDrawerOverlay);
        drawer.style.transform = "translateX(250px)";
        drawerIsOpen = true;
    }

    function closeDrawer(e) {
        // var drawerOverlay = document.getElementById('drawer-overlay');
        // drawerOverlay.style.backgroundColor = "rgba(0, 0, 0, 0)";
        var drawer = document.getElementById('drawer-inner');
        drawer.addEventListener("transitionend", hideDrawerOverlay);
        drawer.style.transform = "translateX(0px)";
        drawerIsOpen = false;
    }
    
</script>
