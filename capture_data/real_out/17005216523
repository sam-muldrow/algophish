<script data-cfasync="false">
    var io = new IntersectionObserver(
        entries => {
            console.log(entries[0]);
            if (entries[0].isIntersecting) {
                var recaptchaScript = document.createElement('script');
                recaptchaScript.src = 'https://www.google.com/recaptcha/api.js?render=6Lcp5f4UAAAAAIsYB19YsmfqZy7A93TTMp40Oltu';
                recaptchaScript.defer = true;
                document.body.appendChild(recaptchaScript);
            }
        },
        {
            root: document.querySelector('.page-wrapper'),
            rootMargin: "0px",
            threshold: 1.0,
        }
    );
    io.observe(document.getElementById('subscribe-bar'));
</script>
<script data-cfasync="false">
    const footer_subscribe_form = document.getElementById("_form_28_");

    footer_subscribe_form.addEventListener('submit', (e) => {
        // on form submission, prevent default
        e.preventDefault();
        const subscribe_form_area = document.getElementById("subscribe_form_area");
        const footer_subscribe_thanks = document.getElementById("footer_subscribe_thanks");
        const footer_subscribe_error = document.getElementById("footer_subscribe_error");
        const name = document.getElementById("footer_newsletter_name");
        const email = document.getElementById("footer_newsletter_email");
        const nameError = document.querySelector('#footer_newsletter_name + span.error');
        const emailError = document.querySelector('#footer_newsletter_email + span.error');

        //reset these
        nameError.textContent = '';
        emailError.textContent = '';
        nameError.className = 'error';
        emailError.className = 'error';
        footer_subscribe_error.style.display = 'none';

        if(!email.validity.valid || !name.validity.valid) {
            // If it isn't, we display an appropriate error message
            showError();
            // Then we prevent the form from being sent by canceling the event
            event.preventDefault();
        } else {
            let loader = document.createElement("img");
            let subscribeLoadingContainer = document.querySelector(".subscribe-loading");
            loader.src = "/images/pulse.gif"
            loader.height = "45";
            loader.width = "45";
            subscribeLoadingContainer.appendChild(loader);

            grecaptcha.ready(function() {
                // do request for recaptcha token
                // response is promise with passed token
                grecaptcha.execute('6Lcp5f4UAAAAAIsYB19YsmfqZy7A93TTMp40Oltu', {action: 'newsletter_subscribe'}).then(function(token) {
                    // add token to form
                    let oReq = new XMLHttpRequest();
                    //pass the search term in too because we use in in the server-side call to build the RC values
                    let url = "/newsletter/subscribeToMailNewsletter";
                    oReq.onload = function (data) {

                        console.log(data.target.response);
                        if (data.target.response && data.target.response.errors) {
                            let errors = data.target.response.errors;

                            if (errors && errors.name) {
                                subscribeLoadingContainer.innerHTML = "";
                                footer_subscribe_error.innerText = 'Please provide your first name';
                            }

                            if (errors && errors.email) {
                                subscribeLoadingContainer.innerHTML = "";
                                footer_subscribe_error.innerText = 'Please provide a valid email address';
                            }

                            footer_subscribe_error.style.display = 'block';
                        }
                        else if (data.target.response && data.target.response.message) {
                            if (data.target.response.message === 'CSRF token mismatch.') {
                                subscribeLoadingContainer.innerHTML = "";
                                footer_subscribe_error.style.display = 'block';
                            }
                        }
                        else {
                            subscribe_form_area.style.display = 'none';
                            footer_subscribe_thanks.style.display = 'flex';
                            subscribeLoadingContainer.innerHTML = "";
                        }
                    };
                    oReq.open('POST', url, true);
                    oReq.responseType = 'json';
                    oReq.setRequestHeader( 'Content-Type', 'application/x-www-form-urlencoded' );
                    oReq.setRequestHeader( 'X-Requested-With', 'XMLHttpRequest' );
                    oReq.setRequestHeader( 'X-CSRF-TOKEN', getCSRFToken() );
                    let postVars = "_token="+getCSRFToken();
                    postVars = "&g-recaptcha-response="+encodeURIComponent(token);
                    postVars += "&name=" + encodeURIComponent(name.value) + "&email=" + encodeURIComponent(email.value);
                    oReq.send(postVars);
                });
            });
        }
    });

    function showError() {
        const name = document.getElementById("footer_newsletter_name");
        const email = document.getElementById("footer_newsletter_email");
        const nameError = document.querySelector('#footer_newsletter_name + span.error');
        const emailError = document.querySelector('#footer_newsletter_email + span.error');

        if(email.validity.valueMissing) {
            // If the field is empty,
            // display the following error message.
            emailError.textContent = 'You need to enter an e-mail address.';
            // Set the styling appropriately
            emailError.className = 'error active';
        } else if(email.validity.typeMismatch) {
            // If the field doesn't contain an email address,
            // display the following error message.
            emailError.textContent = 'Entered value needs to be an e-mail address.';
            // Set the styling appropriately
            emailError.className = 'error active';
        }

        if(name.validity.valueMissing) {
            // If the field is empty,
            // display the following error message.
            nameError.textContent = 'Please enter your name';
            // Set the styling appropriately
            nameError.className = 'error active';
        }

    }</script>
