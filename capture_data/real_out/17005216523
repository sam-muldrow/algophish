        cursor: pointer;
    }
    /* This is our style for the invalid fields */
    input:invalid{
        border-color: #900;
        background-color: #FDD;
    }

    input:focus:invalid {
        outline: none;
    }

    /* This is the style of our error messages */
    .error {
        width  : 100%;
        padding: 0;

        font-size: 80%;
