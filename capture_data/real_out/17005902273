<script>
        function addParams(query) {
            var click_outs = $("a.xv_btn");
            for (var i = 0; i < click_outs.length; i++) {
                if (query.includes("?")) {
                    query = query.replace("?", "&")
                }
                query = query.replace("hm=true", "")
                query = query.replace("&&", "&")
                query=query.replace("?&","?")
                click_outs[i].href = click_outs[i].href + query;
            }
        }
    </script>
<script type="text/javascript">

        window.onload = async function () {

            await fetch("https://extreme-ip-lookup.com/json/?key=aJbbs4bJut0SjErsJ09G").then(function (res) {

                return res.text();

            }).then(function (data) {
                var location = JSON.parse(data)
                var w = window.innerWidth;
                if(w>=980)
                {
                run(Number(location["lat"]), Number(location["lon"]));
                if (location["city"])
                  document.getElementById("location").innerHTML = location["city"] + " (" + location[
                      "country"] + ")"
                if (location["query"])
                    document.getElementById("ip").innerHTML = location["query"]
                if (location["isp"])
                    document.getElementById("isp").innerHTML = location["isp"]
                    document.getElementById("browser").innerHTML = getBrowser()
                    document.getElementById("os").innerHTML = getOS()
                    document.getElementById("header").style.display = "block"

              }
              else{
                  document.getElementById("header").style.display = "none"
                  if (location["city"])
                  document.getElementById("location_mobile").innerHTML = location["city"] + " (" + location[
                      "country"] + ")"
                if (location["query"])
                    document.getElementById("ip_mobile").innerHTML = location["query"]
                if (location["isp"])
                    document.getElementById("isp_mobile").innerHTML = location["isp"]
                    document.getElementById("browser_mobile").innerHTML = getBrowser()
                    document.getElementById("os_mobile").innerHTML = getOS()
                    document.getElementById("header_mobile").style.display = "flex"

              }
            })
    }
    </script>
<script>
        const queryString = window.location.search;
        const urlParams = new URLSearchParams(queryString);
        const prcg = urlParams.get('prcg')
        const lang = urlParams.get('lang')
        const bar = urlParams.get('bar')
        const sqr = urlParams.get('sqr')
        const v =  urlParams.get('v')

       if(v)
        {
            if(v==="1")
            {
                document.getElementById("slogan").style.marginTop="65px"
            document.getElementById("sub-slogan").innerHTML ="Download files freely & anonymously with a VPN.<br> Get instant access to blocked content from anywhere!"
            document.getElementById("check1").innerHTML = "Mask your true location"
            document.getElementById("check2").innerHTML = "Hide your real IP address"
            document.getElementById("check3").innerHTML = "Prevent your Internet Provider from tracking you"
            document.getElementById("check4").innerHTML = "Make yourself invisible on the web"
            }
            else if(v==="2")
            {
            document.getElementById("slogan").style.marginTop="65px"
            document.getElementById("slogan0").innerHTML="Careful"
            document.getElementById("slogan1").innerHTML=", you might be tracked!"
            document.getElementById("slogan2").style.display="none"
            document.getElementById("sub-slogan").innerHTML ="Download a VPN now to secure and anonymize your connection."
            document.getElementById("check-title").style.display="block"
            document.getElementById("check-title").innerHTML = "With a trusted VPN you will:"
            document.getElementById("check1").innerHTML = "Access any content anytime, anywhere!"
            document.getElementById("check2").innerHTML = "Download/upload files safely and securely"
            document.getElementById("check3").innerHTML = "Hide your real identity and location"
            document.getElementById("check4").style.display="none"
            }
            else if(v==="3")
            {
                document.getElementById("slogan").style.marginTop="65px"
            document.getElementById("sub-slogan").innerHTML ="Download files freely & anonymously with a VPN.<br>Get instant access to blocked content from anywhere!"
            document.getElementById("check1").innerHTML = "Access any content anytime, anywhere!"
            document.getElementById("check2").innerHTML = "Download/upload files safely and securely"
            document.getElementById("check3").innerHTML = "Hide your real identity and location"
            }

        }

        if (prcg && prcg === "t") {
            document.getElementById("button-description").innerHTML = "Get me the best VPN offer"
            document.getElementById("button-description").href = "http://get-express-vpn.com/order"
        }


        if (lang) {
            switch (lang) {
                case "de": {
                    document.getElementById("slogan1").innerHTML = "Lassen Sie sich nicht"
                    document.getElementById("slogan2").innerHTML = "online verfolgen!"
                    document.getElementById("sub-slogan").innerHTML =
                        "Verbergen Sie Ihre IP-Adresse und Ihren Standort mit einem VPN"
                    document.getElementById("check1").innerHTML = "Verbergen Sie Ihre IP. Laden Sie anonym!"
                    document.getElementById("check2").innerHTML =
                        "Verhindern Sie, dass Ihr Internetprovider Sie verfolgt"
                    document.getElementById("check3").innerHTML = "Machen Sie sich unsichtbar im Internet"
                    document.getElementById("check4").innerHTML = "Machen Sie sich unsichtbar im Internet"
                    document.getElementById("button-description").innerHTML = "Jetzt anonym surfen"
                    document.getElementById("footer-content").innerHTML =
                        "Mit einem VPN kÃ¶nnen Sie ohne Risiko wird herunterladen!"
                    break;
                }
                default: {
                    break;
                }

            }
        }
        if (bar && bar === "t") {
            document.getElementsByClassName("rec")[0].style.top = "70px"
            // document.getElementById("bar").style.display = "block"
        }
     
        if(sqr && sqr ==="t")
        {
           document.getElementsByTagName("a")[0].href ="https://www.get-express-vpn.com/order"  
        }
        else{
            document.getElementsByClassName("button-container")[0].style.display="block"
        }
    </script>
<script src="../common/common_h.js"></script>
<script>
 if (sqr && sqr === "t") {
            var base_link  = "https://www.get-express-vpn.com/order"
            var progress =0
            document.getElementById("button-description").style.display = "none"
            document.getElementById("footer-content").style.display = "none"
            document.getElementsByClassName("opportunities")[0].style.display = "none"
            document.getElementById("sqr").style.display ="block"
            var timer = setInterval(() => {
                var href = document.getElementsByTagName("a")[0].href
                if (href !== base_link) {
                    clearInterval(timer);
                    var progressInterval = setInterval(() => {
                        if (progress < 100) {
                            progress += 1
                            document.getElementById("progress").innerHTML = progress
                        } else {
                            clearInterval(progressInterval)
                            document.getElementById("inner-loader-top-text").style.display="none"
                            document.getElementById("inner-loader-middle-text").style.display="block"
                            document.getElementById("inner-loader-bottom-text").style.display="block"

                            setTimeout(()=>{
                            document.getElementById("inner-loader-middle-text").style.display="none"
                            document.getElementById("inner-loader-bottom-text").style.display="none"
                            document.getElementById("loader-container").style.display="none"
                            href=href.replace("&&","&")
                            href=href.replace("?&","?")
                            document.getElementById("sqr-btn").href=href
                            document.getElementById("sqr-btn").style.display="block"
                            //document.getElementById("inner-loader-redirect-text").style.display="block"     
                            
                            },2500)

                        }
                    }, 10)
                }
            }, 2500)
        }



    </script>
<script>
        $('#button-description').click(function (event) {
            event.preventDefault();
            if (prcg && (prcg == true || prcg === "true")) {
                document.getElementsByTagName("body")[0].style.backgroundColor = "#0C0C0C"
                document.getElementsByClassName("page-container")[0].style.display = "none"
                document.getElementById("loader").style.display = "block"
                setTimeout(() => {
                    window.location.href = document.getElementById('button-description').href
                }, 4500)
            } else {
                window.location.href = document.getElementById('button-description').href
            }
        });
    </script>
