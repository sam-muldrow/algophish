                var top_days = new Array(1,3,7);
                function showtab(num) {
                    top_days.forEach(function(element, i) { 
                        if (element != num) {
                            document.getElementById("top"+element).style.display = "none";
                            document.getElementById("tabhref"+element).classList.add("section_menu_active");
                        } else {
                            document.getElementById("top"+element).style.display = "flex";
                            document.getElementById("tabhref"+element).classList.remove("section_menu_active");
                        }
                    });
                }
