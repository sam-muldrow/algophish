<!-- ▼ 2カラムモジュールで1カラム表示ページの補正css ▼ -->

<!-- ▲ 2カラムモジュールで1カラム表示ページの補正css ▲ -->

<!-- ▼ ページに適したid+入口フラグを付与 ▼ -->
<script>
    var webId = "";
    var entFlg = "";
    var uniqueFlg = "";
    var hasBotChat = "";

    $(window).on('load', function(){
        $('#repl-ai-chatboard-icon,.repl-ai-chatboard-close-button').on('click', function(){
            if ($('#repl-ai-chatboard').hasClass('close')) {
                $('#chatToolEntry').removeClass('chat-tool__area--btn-on');
                $('#chatToolEntry').addClass('chat-tool__area--btn-off');
            } else {
                $('#chatToolEntry').removeClass('chat-tool__area--btn-off');
                $('#chatToolEntry').addClass('chat-tool__area--btn-on');
            }
        });
        $('#chatToolEntry').on('click', function(){
            $('#repl-ai-chatboard-icon').removeClass('chatbot__area--btn-on');
            $('#repl-ai-chatboard-icon').addClass('chatbot__area--btn-off');
        });
        $('#chatToolBox-min,#chatToolBox-min02').on('click', function(){
            if (!$('#chatToolBox').is(':visible') && !$('#chatToolBox-max').is(':visible')) {
                $('#repl-ai-chatboard-icon').removeClass('chatbot__area--btn-off');
                $('#repl-ai-chatboard-icon').addClass('chatbot__area--btn-on');
            }
        });
        if(hasBotChat) {
            $('#chatToolOn').show();
        }
    });
</script>
<!-- ▲ ページに適したid+入口フラグを付与 ▲ -->

<!-- ▼ チャットエリア　※オペレーター着可時に表示 ▼ -->
