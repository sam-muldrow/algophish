<script type="text/javascript" src="./html/js/modernizr.js"></script>

<script type="text/javascript">
/* Setting cross domain liker parameter to cookie variable  */
var url = location.search;
var GA_cookie_param = getParameterByName('_ga');
if (GA_cookie_param) document.cookie = "GA_cookie_param="+GA_cookie_param;
function getParameterByName(name, url) {
    if (!url) url = window.location.href;
    name = name.replace(/[\[\]]/g, '\\$&');
    var regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)'),
        results = regex.exec(url);
    if (!results) return null;
    if (!results[2]) return '';
    return decodeURIComponent(results[2].replace(/\+/g, ' '));
}
/* End */

  /* Method that iterates over the Modernizr library and builds it up into a format that the server then stores for use server side */
  function fetchBrowserFeatures(p_formName) {
    var featuresList = '';
    var form = document.forms[p_formName];
    var objModernizr = Modernizr;
    var propertyType;

    if (form && form.BROWSER_FEATURES && objModernizr) {
      for (var property in objModernizr) {
        if (objModernizr.hasOwnProperty(property)) {
          if (property[0] === '_') {
            continue;
          }
          propertyType = typeof objModernizr[property];
          if (propertyType === 'function') {
            continue;
          }
          featuresList += (featuresList ? '|' : '') + property + ':';
          if (propertyType === 'object') {
            for (var subProperty in objModernizr[property]) {
              if (objModernizr[property].hasOwnProperty(subProperty)) {
                featuresList += '/' + subProperty + ':' + (objModernizr[property][subProperty] ? '1' : '0');
              }
            }
          } else {
            featuresList += objModernizr[property] ? '1' : '0';
          }
        }
      }
      featuresList += ';';
      form.BROWSER_FEATURES.value = featuresList;
    }
  }

  function readCookie(name) {
    name += '=';
    for (var ca = document.cookie.split(/;\s*/), i = ca.length - 1; i >= 0; i--)
      if (!ca[i].indexOf(name))
        return ca[i].replace(name, '');
  }

  window.onload = function() {
    fetchBrowserFeatures('browserFeatures');
    document.getElementById('compact').value = readCookie('compactMode') || '';
    document.getElementById('box').value = readCookie('boxView') || 'tc-box-on';
    document.getElementById('headerLock').value = readCookie('lockHeader') || '';
    document.getElementById('footerLock').value = readCookie('lockFooter') || '';
    document.getElementById('fontSize').value = readCookie('fontSize') || 'tc-small';
    document.getElementById('showSideBar').value = readCookie('showSideBar') || 'tc-unshow-sidebar';
	

    document.forms['browserFeatures'].submit();
  };

</script>

