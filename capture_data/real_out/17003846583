    translationsHash = '6880934b',
    /**
     * This variable is used to convert the generic "portal" keyword in the data-jsl10n attributes
     * e.g. 'data-jsl10n="portal.footer-description"' into a portal-specific key, e.g. "wiktionary"
     * for the wiktionary portal.
