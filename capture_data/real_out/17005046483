<script type="text/javascript" nocombine="true">

    function initPersonalizer() {
        if (Carnival.Personalizer) {
            Carnival.Personalizer.personalizePage().then(function () {
                Carnival.Personalizer.homepageInit();
            });
        } else {
            console.log("Carnival.Personalizer not ready");
            $(document).on("CCL-Personalizer-loaded", function(event) {
                initPersonalizer();
            });
        }
    }

    initPersonalizer();
    
</script>

