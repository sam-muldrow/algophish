            function UpScroll(Id,nm,sec,fg){//éæ¡æ»å¨ææ
                var o=$('#'+Id),
                    Len=o.find('.IndPnDv').length,
                    P=o.find('.IndPnLst'),
                    T=o.find('.IndPnDv').outerHeight(),
                    H=T*(Len-1),
                    ST=T,
                    Tmr;
                if(Len<2){return;}
                Tmr=setInterval(function(){
                    P.stop().animate({'top':'-'+ST+'px'},400);
                    ST=(ST>=(H-(T*nm)))?0:ST+T;
                },(sec)?sec:5000);
            }
        </script>

