  <script src="https://www.sanguosha.com/static/pc/dist/js/pages/index-420153d293.js"></script>
  <script src="/js/swiper-3.4.2/js/swiper.min.js"></script>
  <script src="/js/h5.js"></script>
  <script>
      function onClick(val, secondId) {
          var keyword = $("input[name=" + val + "]").val();
          if (!keyword) {
              alert('请输入关键词');
              return ;
          }
          var str = secondId ? '/secondid/' + secondId : ''
          var url = 'https://www.sanguosha.com' + '/news/list/id/8000' + str + '/keyword/' + keyword
          window.open(url, '_blank');
      }
      $(function(){
        var swiper = new Swiper('.js_swiper1', {
          pagination: '.js_page1',
          paginationClickable: true,
        });

        var swiper = new Swiper('.js_swiper2', {
          pagination: '.js_page2',
          paginationClickable: true,
        });

        //新闻切换
        $('#newsTab .n-tab').click(function () {
          var t = $(this),
            idx = t.index();
          t.addClass('tab-on').siblings().removeClass('tab-on');
          t.parent().siblings('.news-ul').hide().eq(idx).show();
        });
        
        //身份切换
        jQuery('.heros-box').slide({ titCell: '.hd li', mainCell: '.bd', effect: 'fold' }); //身份玩法
        jQuery('.general-box').slide({ titCell: '.hd li', mainCell: '.bd', effect: 'fade', trigger: 'click', delayTime: 500, interTime: 4000, autoPlay: true });

        $("#code1").click(function(){
          $(".fades,#pop1").fadeIn();
        })
        $(".fades").click(function(){
          $(".fades,#pop1,#pop2").fadeOut();
        })
        $("#code2").click(function(){
          $(".fades,#pop2").fadeIn();
        })
      })
  </script>
</body>
</html>
