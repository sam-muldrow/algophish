    e.preventDefault();
    SgsCensus.ReportData('205_200','1200','','Official_1_1','','官网开始游戏','');
    __sgsYT.sendData('sgs_gw_start')
    setTimeout(function(){
      window.location.href = e.currentTarget.href;
    },500);
  });

