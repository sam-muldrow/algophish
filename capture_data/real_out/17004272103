$.ajax({
type: "GET",
url: "https://log.kienthuc.net.vn/ajax/tool.ashx?type=search&phrase=" + window.encodeURIComponent($(this).val()),
cache: true
}).done(function (msg) {
window.location = msg;
});
}
}
});
$('.txtsearch').focus(function () { if ($(this).val() == 'Tìm trong Kiến thức') { $(this).val(''); } });
$('.txtsearch').blur(function () { if ($(this).val() == '') { $(this).val('Tìm trong Kiến thức'); } });
</script>

