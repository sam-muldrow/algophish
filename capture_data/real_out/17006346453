} else if (webPathname === "/offre-paybox-mail/") {
	inputValue = "Offre Paybox Mail";
} else if (webPathname === "/offre-paybox-start/") {
	inputValue = "Offre Paybox Start";
}
package.value = inputValue;
</script>
</p><div class="wpcf7-response-output" aria-hidden="true"></div>
</form>
</div>
</div>
			</div>
			</div>
				
				
				
				
			</div>
				
				
			</div>		</div>
	</div>
						</div>

				
				</article>

			

