    <script>
        $('.owl-demo-gt').each(function(){
            if ($(".owl-demo-gt").find('.item-gt').length > 1) {
                $(".owl-demo-gt").owlCarousel({
                    rtl : true,
                    autoplay : false,
                    autoplayHoverPause : true,
                    loop : true,
                    margin : 0,
                    navigation : true,
                    navigationText : false,
                    items : 1,
                    dots:false,
                    nav: true,
                    responsive : {
                        0 : {
                            items : 1
                        },
                        768 : {
                            items : 1
                        },
                        1000 : {
                            items : 1
                        }
                    }
                });
            }
            if($(this).find('.item-gt').length > 1) {
                $(this).on('changed.owl.carousel', function (event) {
                    var index_gt = event.item.index;
                    var total = $(this).find('.owl-stage').children(':eq(' +  index_gt + ')').find('.counter_box_hidden').children('span').text();
                    $(this).parents('.gozaresh-pic').children('header').children('.blue-header').find('.counter_box').html(total);


                });
            }
            $(".owl-controls .owl-prev").html('');
            $(".owl-controls .owl-next").html('');
        });
    </script>
