        $('.owl-demo-p').each(function(){
            if ($(".owl-demo-p").find('.item-p').length > 1) {
                $(".owl-demo-p").owlCarousel({
                    rtl : true,
                    autoplay : false,
                    autoplayHoverPause : true,
                    loop : true,
                    margin : 0,
                    navigation : true,
                    navigationText : false,
                    items : 1,
                    dots:false,
                    nav: true,

                    responsive : {
                        0 : {
                            items : 1
                        },
                        768 : {
                            items : 1
                        },
                        1000 : {
                            items : 1
                        }
                    }
                });
            }
            if($(this).find('.item-p').length > 1) {
                $(this).on('changed.owl.carousel', function (event) {
                    var index_p = event.item.index;
                    var total = $(this).find('.owl-stage').children(':eq(' +  index_p + ')').find('.counter_box_hidden').children('span').text();
                    $(this).parents('.pishnahad2').children('header').children('.blue-header').find('.counter_box').html(total);


                });
            }
            $(".owl-controls .owl-prev").html('');
            $(".owl-controls .owl-next").html('');
        });
	</script>

