<script>
        window.addEventListener('DOMContentLoaded', function () {
            (function ($) {
                $("#IframeId1").on('load', function () {
                    $(this).height($(this).contents().find("html").height());
                    var adsFrameGeneral1 = $(this).height()
                    if (adsFrameGeneral1 == 0) {
                        $(this).css("display", "none");
                        $('.geminiLB4Ad').addClass('showLB4');
                    }
                });

                $("#IframeId2").on('load', function () {
                    $(this).height($(this).contents().find("html").height());
                    var adsFrameGeneral2 = $(this).height()
                    if (adsFrameGeneral2 == 0) {
                        $(this).css("display", "none");
                        $('.geminiLB5Ad').addClass('showLB5');
                    }
                });
                function isVisible($el) {
                    var winTop = $(window).scrollTop();
                    var winBottom = winTop + $(window).height();
                    var elTop = $el.offset().top;
                    var elBottom = elTop + $el.height();
                    return ((elBottom <= winBottom) && (elTop >= winTop));
                }
                var EventTracked = false;
                $(window).scroll(function () {
                    var visible = isVisible($(".TrackedItem"));

                    if (visible) {
                        if (!EventTracked) {
                            EventTracked = true;
                            //ga('send', 'event', 'Native-Ads', 'Mokhtarat-Section', 'Ad-Displayed');
                            TrackEvent('Native-Ads', 'Mokhtarat-Section', 'Ad-Displayed');

                        }
                    } else {
                        EventTracked = false;
                    }
                });
            })(jQuery);
        });




    </script>
