</div> <script>
        window.addEventListener('DOMContentLoaded', function () {
            (function ($) {
                if (General.GetCookie("BNClosed") != null) {
                    if ('1526967' == General.GetCookie("BNClosed")) {
                        $('body').removeClass('showLastNews');
                    }

                    else {
                        $('body').addClass('showLastNews');
                    }
                }

                else {
                    $('body').addClass('showLastNews');
                }


                $(".close").click(function () {
                    $('body').removeClass('showLastNews');
                    var date = new Date();
                    date.setDate(date.getDate() + 1);
                    General.SetCookie("BNClosed", "1526967", date)
                });
            })(jQuery);
        });


    </script>
