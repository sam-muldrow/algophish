<script type="text/javascript">
    window.addEventListener('DOMContentLoaded', function () {
        (function ($) {
            $('.moreLnksBtn').on('click', function (e) {
                $('.moreLnksBtn').closest('.moreLnks').not($(this).closest('.moreLnks')).removeClass("openLinksMenu");
                $(this).closest('.moreLnks').toggleClass("openLinksMenu");
                e.stopPropagation();
            });



            $(document).on('click', function (e) {
                if ($(e.target).is("body") === false) {
                    $('.moreLnks').removeClass("openLinksMenu");
                }
            });
        })(jQuery);
    });



</script>

