<script>
		//local
		//window.location.href = "/main/index.html";
	
		//server
		//window.location.href = "https://www.seoul.go.kr/main/index.jsp";
		//window.location.href = "/";

		isMobile = {

		  Android: function () {
				   return navigator.userAgent.match(/Android/i) == null ? false : true;
		  },
		  BlackBerry: function () {
				   return navigator.userAgent.match(/BlackBerry/i) == null ? false : true;
		  },
		  IOS: function () {
				   return navigator.userAgent.match(/iPhone|iPad|iPod/i) == null ? false : true;
		  },
		  Opera: function () {
				   return navigator.userAgent.match(/Opera Mini/i) == null ? false : true;
		  },
		  Windows: function () {
				   return navigator.userAgent.match(/IEMobile/i) == null ? false : true;
		  },
		  any: function () {
				   return (this.Android() || this.BlackBerry() || this.IOS() || this.Opera() || this.Windows());
		  }
	};

		var domain 	= window.location.protocol + "//" + window.location.hostname;
		var exec	= (domain.indexOf('localhost') > -1) ? '.html' : '.jsp';
		var goPc	= '/main/index' + exec;
		var goMo	= '/main/indexM' + exec;
		var fullurl = document.URL + '';
		
		if(isMobile.any()){
    		//mobile conntected
			window.location.href = goMo;
			
		}else{
			//pc conntected
			window.location.href = goPc;
		};
		
</script>
