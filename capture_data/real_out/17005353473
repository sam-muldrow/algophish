<script type="text/javascript">
    function showModal(){
        const notificationModal = document.getElementById("notification-modal");
        if (notificationModal.className === "notification-modal"){
            notificationModal.className = "notification-modal-hidden";
        }else{
            notificationModal.className = "notification-modal";
        }
    }
    window.onclick = function(event) {
        const notificationButton = document.getElementById("notification-button");
        const notificationIcon = document.getElementById("notification-icon");
        const notificationModal = document.getElementById("notification-modal");
        if (event.target !== notificationModal && event.target !== notificationButton && event.target !== notificationIcon) {
            notificationModal.className = "notification-modal-hidden";
        }
    }
</script>

    
