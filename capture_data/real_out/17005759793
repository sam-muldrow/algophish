break}};CommunicationNav.prototype.handleLogoutClick=function(){logout()};CommunicationNav.prototype.render=function(){};CommunicationNav.prototype.hydrate=function(){var logoutButton=this.nav.querySelector('ui-navitem[item\x3d"communication.logout"] button');if(logoutButton)logoutButton.addEventListener("click",this.handleLogoutClick.bind(this));this.nav.addEventListener("click",this.handleClickEvent.bind(this))};return CommunicationNav}(SharedComponents.UIElement,Swedbank);
