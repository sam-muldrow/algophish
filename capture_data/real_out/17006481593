<script>
var brtCodeVerification = function(jButton) {
  let regex = /(?<!\S)\d{12}(?!\S)|(?<!\S)\d{14}(?!\S)|(?<!\S)\d{15}(?!\S)|(?<!\S)\d{19}(?!\S)/g;
      jButton.unbind('click').bind('click', function(e) {
            e.preventDefault();
            let value = jButton.parent().find('input').val();
            if (value.match(regex)) {
				let trackingLink = $(this).siblings("span.icon-for-search-parcel-tracking").attr("value");
				var search = trackingLink + value;
				window.open(search, openLink).focus() // openLink ? 
            } else {
				alert('Inserire un codice numerico di 12, 14, 15 o 19 cifre'); // To modify if needed
            }
      }
    );
}

setTimeout(function(){
	brtCodeVerification(jQuery('.widget_search-parcel-tracking-embedded-widget span[id^="search-parcel-tracking-"]'));
}, 500);
</script>
