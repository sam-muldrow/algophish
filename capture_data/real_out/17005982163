    })//end of ready</script><script>$(document).ready(function() {
    $.post('/scripts/ajax_wn_index_tld.pl', function( data ) {
      var tldPrices = {};
      if( data.result && data.result.tld_price ) {
        $.each(data.result.tld_price, function(i, tld) {
          tldPrices[tld.shortname] = tld.price_new.slice(0,-3);
        });
      }
      $('#tld_ru').text(tldPrices['ru']);
      $('#tld_rus').text(tldPrices['рус']);
      $('#tld_rf').text(tldPrices['рф']);
      $('#tld_com').text(tldPrices['com']);
    });
  });</script>
  

