<script>
//关闭对联广告
function hideCouplet(){
setcookie('bbs-couplet',1,6*60*60);//6小时cookie
jq(".couplet").hide();
}
//DOM结构绘制完毕后就执行
jq(document).ready(function(){
if(getcookie('bbs-couplet')==1){
jq(".couplet").hide();//cookie 1，关闭对联广告
}else{
jq(".couplet").show();//默认显示
}
});
</script>
