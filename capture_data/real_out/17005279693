<script>
var slider; 
var pos = 0;

//Включаем
changeSlide();

//Наполняем точками, если баннеров 2 или больше ИНАЧЕ скрываем
if ($('.horizontal_carousel_item_container').length > 1) {
    for (var i=1; i < $('.horizontal_carousel_item_container').length; i++){
        $( "#h-b-btn" ).append( "<span class='circle-btn'></span>" );
    }
    $('#h-b-btn').css('display','flex');
}

//Клик по точке
$('.circle-btn').on('click',function(){
    if ( !$(this).hasClass('active') ){
        pos = $('.circle-btn').index(this);
        $('.circle-btn').removeClass('active');
        $(this).addClass('active');
        $('.horizontal_carousel_item_container').removeClass('active');
        $('.horizontal_carousel_item_container:nth-child(' + (pos+1) + ')').addClass('active');
    }
});

//функция переключения слайдов
function horizontalTimer() {
    if ( pos >= $('.horizontal_carousel_item_container').length ) { 
        pos = 0; 
    }
    $('.horizontal_carousel_item_container').removeClass('active');
    $('.circle-btn').removeClass('active');
    $('.horizontal_carousel_item_container:nth-child(' + (pos+1) + ')').addClass('active');
    $('.circle-btn:nth-child(' + (pos+1) + ')').addClass('active');
    pos++;
}

//Вызов функции
function changeSlide(){
    slider = setInterval( horizontalTimer, 3000);
}

//Остановка функции
function stopChangeSlide() {
    clearInterval( slider );
}

$('#h-b-carousel').hover(function(){
    stopChangeSlide();
}, function(){
    changeSlide();
});

</script></div></div><div style='height: 190px; display: none; text-indent: -9999px; padding: 0px'></div>    <!--/noindex-->

