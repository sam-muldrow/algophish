<script>
$(function () {
    var xhr = null,
        $doc = $(document),
        $wnd = $(window);

    $wnd.on("scroll", function() {
        var doc_height = $doc.height(),
            threshold = (doc_height < 4500) ? 1700 : 400;

        if ((doc_height -  $wnd.height() - $wnd.scrollTop()) < threshold) {
            xhr = xhr || $.ajax({
                url: "/lib/extra_product_news.php",
                type: "GET",
                dataType: "html",
                data: { offset: $('#front_goods').children(':last-child').data('offset'), quantity: 6 },
                beforeSend: function() { $('#front_goods').append('<img style="display:block; margin:40px auto;" src="/art/loading.gif" alt="Подождите...">') },
                success: function(html) { $("#front_goods").append(html) },
                complete: function() { $('#front_goods>img').remove(); xhr = null; }
            });
        }
    });
});

$(function() {
    if (parseInt($('#retail_performance_min').text()) > 0) {
        setInterval(function() { $(".info_slide").delay(3000).toggle("slide") }, 3000);
    }
});

$(function() {
    $('#front_eye .ui-eye').eye({
        href   : { enabled: true, url: '/scripts/eye/?gid=2637#good_id={good_id}' },
        url    : '/scripts/eye/img.php?size={size}&goods={goods}',
        popup  : { url: '/scripts/eye/popup.php?good_id={good_id}' },
        effects: { shift: { url: '/scripts/eye/cut.php?good_id={good_id}' } },
        demo   : { enabled: true },
        good_id : 618073,
        promoted: [],
        goods   : [618073,704484,685986,640911,543296,525038,492941,489424,481026,478222,474160]    });
});
</script>
