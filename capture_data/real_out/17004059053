        <script>
    var path = location.pathname;
    if (path === undefined) {
        path = '/';
    }

    var target = {};

    if (path == '/') {
        $('nav.primary a[href="/"]').addClass('active');
    } else if (path.indexOf('/car/') === 0) {
        $('nav.primary a[href="/sports/"]').addClass('active');
    } else {
        $($('nav.primary a').get().reverse()).each(function () {
            var href = $(this).attr('href');
            
            if (href == '/') {
                return true;
            }

            // プロフィールページの場合はURLからではなくpersonオブジェクトからカテゴリ取得
            
            if (path.indexOf(href) === 0) {
                target[href] = $(this);
            }
        });

        if (Object.keys(target).length > 0) {
            var tmp = Object.keys(target).sort(function(a, b) { return b.length - a.length; });
            var longestKey = tmp[0];
            target[longestKey].addClass('active');
        }
    }
</script>


        
