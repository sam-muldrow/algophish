<script>
    var stickyClosed = false;
    $(window).on("scroll", function() {
        if(stickyClosed==false)
        {
            if($(window).scrollTop() > 0 )
                $("#sticky-ads").css("display", "block");
            else
                $("#sticky-ads").css("display", "none");
        }
    });
    function closestickyAds()
    {
        $("#sticky-ads").css("display", "none");
        stickyClosed = true;
    }
</script>
