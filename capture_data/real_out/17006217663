<script type="text/javascript">
// CookieからAoneのセグメントIDを取得する
var getAoneSegments = function() {
	var cs = document.cookie;
	if (cs) {
    var acs = cs.split('; ');
    for(var i=0; i < acs.length; i++){
		var cs = acs[i].split('=');
		if (cs[0] === 'a1_segment_ids') {
        return unescape(cs[1]);
		}
    }
	}
	return '';
}
</script>

