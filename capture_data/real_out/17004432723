  (function(C,O,R,D,I,A,L){
    C.CordialObject=I,C[I]=C[I]||function(){(C[I].q=C[I].q||[]).push(arguments)};
    C[I].l=1*new Date,C[I].q=[],A=O.createElement(R);
    L=O.getElementsByTagName(R)[0],A.async=1,A.src=D,L.parentNode.insertBefore(A,L);
  })(window, document, "script", "//data.p.modcloth.com/track.v2.js", "crdl");

  crdl("connect", "modc", {
    trackUrl: "//se.p.modcloth.com",
    connectUrl: "//data.p.modcloth.com",
    cookieDomain: "modcloth.com",
    cookieLife: 365
  });
  var cordialUtm = "utm_source,utm_campaign"
  if (cordialUtm !== '') {
    crdl("param-store", cordialUtm)
