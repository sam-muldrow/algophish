  <script>
    function initFreshChat() {
      window.fcWidget.init({
        token: "60cb0700-2023-4b07-b3e9-2e83c009c02c",
        host: "https://wchat.freshchat.com"
      });
    }
    function initialize(i,t) {
      var e;
      i.getElementById(t) ? initFreshChat() :
        ((e=i.createElement("script")).id=t,
          e.async=!0,e.src="https://wchat.freshchat.com/js/widget.js",
          e.onload=initFreshChat,i.head.appendChild(e))
    }
    function initiateCall() {
      initialize(document,"freshchat-js-sdk")
    }

    var userAgent = navigator.userAgent;
    var isMobileApp = false;
    var userAgentMobileValue = 'modcloth_app';

    if (userAgentMobileValue && userAgent && userAgent.includes(userAgentMobileValue)) {
      isMobileApp = true;
    }

    /*
    we can prevent the freshchat widget from rendering on mobile app if user_agent_on_mobile_app is presented.
    On mobile app (ex: modcloth app) the freshchat widget makes a ring tone. We can disable that in this way.
    */
    if (!isMobileApp) {
      window.addEventListener ?
        window.addEventListener("load",initiateCall,!1):
        window.attachEvent("load",initiateCall,!1);

      

    }

  </script>







