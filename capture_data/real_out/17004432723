<script>

  document.addEventListener("DOMContentLoaded", function(event) {

    document.querySelectorAll('.list_currency .currency_option').forEach((el) => {
      el.addEventListener('click', function(e) {

        const selected_currency = el.dataset.currency;
        const localization_form = document.querySelector('.shopify-localization-form');
        let localization_input = document.getElementById('localization_input');        
        localization_input.value = selected_currency;

        if (selected_currency && selected_currency !== 'US') {
          localStorage.setItem('localization', 'en-' + selected_currency.toLowerCase());
        } else {
          localStorage.removeItem('localization');
        }

        localization_form.submit();

      });
    });
  });
</script>


  
  

  

  
  
  

  
  
  

  

        
      
        </div>
      



  
  

  

  
  
  

  
  





























  
  

  

  
  
  

  
  





























  
  

  

  
  
  

  
  





























  </div>
</div>

