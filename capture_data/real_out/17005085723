<script>
  
function load(){
    $('.last-news').click(function() { 
      if($('.last-news').hasClass('active')){
        
      }else{
        $('.last-news').addClass('active');  
        $('.most-read').removeClass('active');   
        $('.tab-last-news').addClass('active');  
        $('.tab-most-read').removeClass('active'); 
      }
     
    });
    $('.most-read').click(function() { 
      if($('.most-read').hasClass('active')){
        
      }else{
        $('.last-news').removeClass('active');  
        $('.most-read').addClass('active');   
        $('.tab-last-news').removeClass('active');  
        $('.tab-most-read').addClass('active'); 
      }
    });

    $('.menu-btn').click(function() { 
      if($('.menu-btn').hasClass('activ')){
        $('.menu-btn').removeClass('activ');   
        $('.search-btn').removeClass('activ');         
        $('.menu-mobil').removeClass('activ');   
        $('.search-mobil').removeClass('activ');
      }else{
        $('.menu-btn').toggleClass('activ');   
        $('.search-btn').toggleClass('activ');         
        $('.menu-mobil').toggleClass('activ');   
        $('.search-mobil').removeClass('activ');
      }

    });
    $('.search-btn').click(function() {
      $('.menu-btn').toggleClass('activ');   
      $('.search-btn').toggleClass('activ');         
      $('.menu-mobil').removeClass('activ'); 
      $('.search-mobil').toggleClass('activ'); 
    });
  
    $('.menu-btn-cancel').click(function() {
      $('.menu-mobil').removeClass('activ');  
    });
    $('.cancel').click(function() {
      $('.Breaking-news').fadeOut();    
    });
    var modal = document.getElementById("myModal");
    var span = document.getElementsByClassName("close")[0];
    window.onload = function () {
        setTimeout(function () {
          modal.style.display = "block";  
        }, 5000);
    }
    window.onclick = function(event) {
      if (event.target == modal) {
        // modal.style.display = "none";
      }
    }
    span.onclick = function() {
      modal.style.display = "none";
    }
    $(function() {
        if (localStorage.getItem("pageloadcount")) 
        {
            $("#myModal").hide();
        } 
        else{
            $("#myModal").show();
        }
        localStorage.setItem("pageloadcount", "1");
        // else(localStorage.setItem("pageloadcount", "1")){
        //      $("#myModal").show();
             
        //  }
    });
        
    // btn.onclick = function() {
    //   modal.style.display = "block";
    // }
}
  </script>
    
