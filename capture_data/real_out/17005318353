//
var uFunc4985 = function(){};　//ページ編集等で呼び出す為にglobalでuFunc(\d+)が必要なのでダミーとして作成しておく。
(function($) {
  "use strict";
  //
  var code = "4985";
  var url = "aHR0cHM6Ly9ob2NoaS5uZXdzL3Jzcy9pbmRleC54bWw=";
  var limit = "6";
  var viewtype = "1";
  //
  if (window.jQuery) {
    // ユニット用関数（描画時に実行されなければならない関数）
    // ※以下のような形式で記述してください
    window["uFunc" + code] = function() {
      var siteURL = bingotopurl; //(("https:" == document.location.protocol) ? "'.WWW_SSL.'" :  "'.WWW.'" );
      var data = {
        date: new Date() - 0,
        url: url,
        limit: limit,
        viewtype: viewtype
      }
      j$.ajax(siteURL + "/usr-data/units/bRssReader/get-RssFeed.php", {
        data: data,
        dataType: "xml"
      }).done(function(e, res, httpObj) {
        window["showList" + code](httpObj);
      });
    }
    window["showList" + code] = function(httpObj) {
      var res = httpObj.responseXML;
      var items = res.getElementsByTagName("items");
      var html = "";
      html += items[0].childNodes[0].nodeValue;
      j$("#rssx" + code).html(html);
      if(window._st) {
          _st = j$(window).scrollTop();
      }
      j$(window).trigger("bRssReaderDone");
    }

    j$(window).on("load", function(event) {
      window["uFunc" + code]();
    });
  } else {
    // ユニット用関数（描画時に実行されなければならない関数）
    // ※以下のような形式で記述してください
    window["uFunc" + code] = function() {
      var siteURL = bingotopurl; //(("https:" == document.location.protocol) ? "'.WWW_SSL.'" : "'.WWW.'");
      new Ajax.Request(
        siteURL + "/usr-data/units/bRssReader/get-RssFeed.php", {
          asynchronous: true,
          parameters: "date=" + (new Date() - 0) + "&url=" + url + "&limit=" + limit + "&viewtype=" + viewtype,
          onComplete: window["showList" + code]
        });
    }

    window["showList" + code] = function(httpObj) {
      var res = httpObj.responseXML;
      var items = res.getElementsByTagName("items");
      var html = "";
      html += items[0].childNodes[0].nodeValue;
      $("rssx" + code).innerHTML = html;
    }

    Event.observe(window, "load", function(event) {
      window["uFunc" + code]();
    }, false);
  }
})(jQuery);
