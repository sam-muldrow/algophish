	<script>
	$(document).on('click', ".itemMovieDonate", function () {
		var post_id = $(this).attr("data-post-id");
		var link = $(this).attr("data-link");
		
		$(".donateModalShowBtn").removeClass("animate__animated animate__headShake");
		$(".donateModalShakeCust").removeClass("animate__animated animate__headShake");
		$("#itemMovieDonateLoader").removeClass("d-none");
		
		$.ajax({
			type: "POST",
			data: {
				'do'      : 'donate_module',
				'post_id' : post_id,
				'action'  : 'item_movie_donate',
			},
			url: "/index.php"
		}).done(function(result) {
			retdata = $.parseJSON(result);

			if (retdata.result == "ok") {
				if (typeof retdata.new_live_time_left !== 'undefined') {
					if (retdata.new_live_time_left != "") {
						$("#live_after_time_left").html(retdata.new_live_time_left);
					}
				}
			
				$(".donateModalPointsHave[data-post-id="+post_id+"]").html(retdata.points_have - retdata.points_used);
				$(".donateModalPointsNeed[data-post-id="+post_id+"]").html(retdata.points_need);
				$(".donateModalShowBtn").addClass("animate__animated animate__headShake");
				$(".donateModalShakeCust[data-post-id="+post_id+"]").addClass("animate__animated animate__headShake");
				$(".upv.points").html(retdata.my_points);
				
				if (retdata.item_donated) {
					if (link) {
						let a= document.createElement('a');
						a.href= link;
						a.click();
					} else {
						location.reload();
					}
				}
			} else {
				if (retdata.error == "pin_locked")
					swal("Oops!", "Jūsų paskyros taškų naudojimas užrakintas.", "error");
				else if (retdata.error == "not_logged")
					swal("Oops!", "Turite būti prisijungęs prie paskyros.", "error");
				else if (retdata.error == "not_enough_points")
					swal("Oops!", "Neužtenka taškų.", "error");
				else if (retdata.error == "not_allow")
					swal("Oops!", "Veiksmas neleidžiamas.", "error");
				else
					swal('', retdata.error, 'error');
			}
			
			$("#itemMovieDonateLoader").addClass("d-none");
		});
	});
</script>
