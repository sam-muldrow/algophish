<script>
	$('#playlistSelectList').on('change', function() {
		if (this.value == "new") {
			playlistInput("text");
		} else {
			var playlist_id = this.value;

			$.ajax({
				type: "POST",
				data: {
					'do'          :'playlist',
					'action'      :'playlist_select',
					'playlist_id' :playlist_id,
				},
				url: "/index.php",
				success: function(result){
					retdata = $.parseJSON(result);

					if (retdata.result == "ok") {
						$("#playlistPlayerContainer").addClass("d-none");
						$("#playlistEmpty").addClass("d-none");
						$("#playlistSpinner").removeClass("d-none").addClass("d-flex");

						playlistIdSet(playlist_id);
						playlistStop();
						playlistItemsLoad();
					} else if (retdata.error == 'only_logged' ) {
						swal('', 'Šis veiksmas galimas tik prisijungus', 'error');
					} else if (retdata.error != 'undefined' ) {
						swal('', retdata.error, 'error');
					} else {
						swal('', result, 'error');
					}
				}
			});
		}
	});

	$(document).on('click', "#playlistDeleteList", function () {
		var playlist_id = $("#playlistSelectList option[selected]").val();

		swal({
			text: "Bus ištrintas šis playlistas ir visi jo įrašai. Tęsti?",
			icon: "warning",
			buttons: {
				cancel: {
					text: "Atšaukti",
					value: null,
					visible: true,
					className: "",
					closeModal: true,
				},
				confirm: {
					text: "Gerai",
					value: true,
					visible: true,
					className: "",
					closeModal: false
				}
			}
		}).then((willDelete) => {
			if (willDelete) {
				$.ajax({
					type: "POST",
					data: {
						'do'          :'playlist',
						'action'      :'playlist_delete',
						'playlist_id' :playlist_id,
					},
					url: "/index.php",
					success: function(result){
						retdata = $.parseJSON(result);

						$("#playlistSelectList option[value='"+playlist_id+"']").remove();

						if (retdata.result == "ok") {
							if ($("#playlistSelectList option").length > 1) {
								$("#playlistEmpty").addClass("d-none");
								$("#playlistSpinner").removeClass("d-none").addClass("d-flex");

								$("#playlistSelectList option").removeAttr('selected');
								$("#playlistSelectList option[value='new']").next().attr('selected', 'selected');
								playlist_id = $("#playlistSelectList option[selected]").val();
								$('#playlistSelectList').val(playlist_id).trigger('change');
							} else {
								playlistInput("text");
								$("#playlistContainer").empty();
								$("#playlistSort").addClass("d-none");
								$("#playlistEmpty").removeClass("d-none");
								$("#playlistPlayerContainer").addClass("d-none");
								$(".md_playlist[data-action=delete_seen]").addClass("d-none");
								$(".md_playlist[data-action=delete_all]").addClass("d-none");
							}
						} else if (retdata.error == 'only_logged' ) {
							swal('', 'Šis veiksmas galimas tik prisijungus', 'error');
						} else if (retdata.error != 'undefined' ) {
							swal('', retdata.error, 'error');
						} else {
							swal('', result, 'error');
						}

						swal.stopLoading();
						swal.close();
					}
				});
			}
		});
	});

	$(document).on('click', "#playlistSaveList", function () {
		var title = $("#playlistSaveTitle").val();

		if (title != "") {
			$(this).prop('disabled', true);

			$.ajax({
				type: "POST",
				data: {
					'do'     :'playlist',
					'action' :'playlist_save',
					'title'  :title,
				},
				url: "/index.php",
				success: function(result){
					retdata = $.parseJSON(result);
					$("#playlistSaveList").prop('disabled', false);

					if (retdata.result == "inserted") {
						$("#playlistSelectList option[value!='new']").each(function() {
							$(this).remove();
						});

						$.each(retdata.playlists_saved_array, function (i, item) {
							$('#playlistSelectList').append($('<option>', {
								value: i,
								text : item
							}));
						});

						$(".md_playlist[data-action=delete_all]").removeClass("d-none");

						playlistIdSet(retdata.playlists_saved_active_id);
						playlistInput("select");

						if (retdata.first_playlist == 0) {
							playlistStop();
							$("#playlistContainer").empty();
							$("#playlistSort").addClass("d-none");
							$("#playlistEmpty").removeClass("d-none");
							$("#playlistPlayerContainer").addClass("d-none");
							$(".md_playlist[data-action=delete_seen]").addClass("d-none");
						} else {
							swal('', 'Ką tik susikurei pirmąjį playlistą. Iš viso gali turėti 20 sąrašų. Įrašai bus įkeliami į tą playlistą, kuris bus aktyvus šiame lange.', 'success');
						}
					} else if (retdata.error == 'only_logged' ) {
						swal('', 'Šis veiksmas galimas tik prisijungus', 'error');
					} else if (retdata.error != 'undefined' ) {
						if (retdata.error == "playlist_disabled") {
							swal('', "Naujo sąrašo sukurti negali, nes playlisto naudojimas nėra aktyvuotas", 'info');
						} else {
							swal('', retdata.error, 'error');
						}
					} else {
						swal('', result, 'error');
					}
				}
			});
		} else {
			if ($("#playlistSelectList option").length > 1) {
				playlistInput("select");

				var playlist_id = $("#playlistPlayer").attr("data-playlist-id");
				playlistIdSet(playlist_id);
			}
		}
	});

	function playlistIdSet(playlist_id) {
		$("#playlistPlayer").attr("data-playlist-id", playlist_id);
		$(".md_playlist[data-action=delete_all]").attr("data-playlist-id", playlist_id);
		$(".md_playlist[data-action=delete_seen]").attr("data-playlist-id", playlist_id);
		$("#playlistSelectList option").removeAttr('selected');
		$("#playlistSelectList option[value='"+playlist_id+"']").attr('selected','selected');
		$('#playlistSelectList').val(playlist_id);
	}

	function playlistInput(input) {
		$("#playlistSaveTitle").val("");

		if (input == "select") {
			$("#playlistSelectList").removeClass("d-none");
			$("#playlistDeleteList").removeClass("d-none");
			$("#playlistSaveTitle").addClass("d-none");
			$("#playlistSaveList").addClass("d-none");
		}

		if (input == "text") {
			$("#playlistSelectList").addClass("d-none");
			$("#playlistDeleteList").addClass("d-none");
			$("#playlistSaveTitle").removeClass("d-none");
			$("#playlistSaveList").removeClass("d-none");
			//$("#playlistSelectList option").removeAttr('selected');
		}
	}

	function playlistStop() {
		document.getElementById("playlistPlayer").controls = false;
		document.getElementById("playlistPlayer").pause();
		document.getElementById("playlistPlayer_link").href = "#";
		document.getElementById("playlistPlayer_src_mp4").removeAttribute("src");
		document.getElementById("playlistPlayer").load();
		$("#playlistPlayer").attr("data-file-id", "");
	}

	function playlistItemsLoad() {
		var playlist_id = $("#playlistPlayer").attr("data-playlist-id");
		$("#playlistContainer").empty();
		$("#playlistSort").addClass("d-none");
		$("#playlistSpinner").removeClass("d-none").addClass("d-flex");

		$.ajax({
			type: "POST",
			data: {
				'do'         :'playlist',
				'action'     :'list_files',
				'playlist_id':playlist_id,
			},
			url: "/index.php",
			success: function(result){
				retdata = $.parseJSON(result);

				if (retdata.result == "ok") {
					$("#playlistSpinner").addClass("d-none").removeClass("d-flex");
					$("#playlistContainer").html(retdata.items_compiled);

					if (retdata.items_count == 0) {
						$(".md_playlist[data-action=delete_all]").addClass("d-none");
						$("#playlistEmpty").removeClass("d-none");
						$("#playlistSort").addClass("d-none");
					} else {
						$(".md_playlist[data-action=delete_all]").removeClass("d-none");
						$("#playlistEmpty").addClass("d-none");
						$("#playlistSort").removeClass("d-none");
					}

					if (retdata.items_seen_count > 0) {
						$(".md_playlist[data-action=delete_seen]").removeClass("d-none");
					} else {
						$(".md_playlist[data-action=delete_seen]").addClass("d-none");
					}

					if (retdata.enabled == 0) {
						$("#playlistDisabled").removeClass("d-none");
						$("#playlistEnabled").addClass("d-none").removeClass("d-inline-block");
						$("#playlistPlayerContainer").addClass("d-none");
					} else {
						$("#playlistDisabled").addClass("d-none");
						$("#playlistEnabled").removeClass("d-none").addClass("d-inline-block");

						if (retdata.items_count == 0) {
							$("#playlistPlayerContainer").addClass("d-none");
						} else {
							$("#playlistPlayerContainer").removeClass("d-none");
						}
					}

					/*
					saved playlists
					*/
					if (retdata.playlists_saved_array.length == 0) {
						playlistInput("text");
					} else {
						$(".md_playlist[data-action=delete_all]").removeClass("d-none");
						playlistInput("select");

						$("#playlistSelectList option[value!='new']").each(function() {
							$(this).remove();
						});

						$.each(retdata.playlists_saved_array, function (i, item) {
							$('#playlistSelectList').append($('<option>', {
								value: i,
								text : item
							}));
						});

						playlistIdSet(retdata.playlists_saved_active_id);
					}
				} else if (retdata.error == 'only_logged' ) {
					$("#playlistSpinner").addClass("d-none").removeClass("d-flex");
					$("#playlistDisabled").removeClass("d-none");
					$("#playlistDisabled .alert").removeClass("d-none").append("<div class='bg-warning text-center p-2 font-weight-bold'>Playlisto funkcija galima naudotis prisijungus prie svetainės.</div>");
					$(".md_playlist[data-action=enable]").addClass("d-none");
				} else if (retdata.error != 'undefined' ) {
					swal('', retdata.error, 'error');
				} else {
					swal('', result, 'error');
				}
			}
		});
	}

	$('#modalPlaylist').on('hide.bs.modal', function (e) {
		playlistStop();
		playlistInput("text");
		$("#playlistPlayer").attr("data-playlist-id", "");
	});

	$('#modalPlaylist').on('show.bs.modal', function (e) {
		$("#playlistPlayerContainer").addClass("d-none");
		$("#playlistContainer").empty();
		$("#playlistSort").addClass("d-none");
		$("#playlistSpinner").removeClass("d-none").addClass("d-flex");
		$("#playlistEmpty").addClass("d-none");
		playlistItemsLoad();
	})

	$(document).on('click', ".modalPlaylistBtn", function () {
		$('#modalPlaylist').appendTo("body").modal('show');
	});

	$(document).on('click', ".md_playlist", function () {
		var fileId	   = $(this).attr("data-file-id");
		var postId	   = $(this).attr("data-post-id");
		var fAction    = $(this).attr("data-action");
		var playlistId = 0;
		var this_      = this;

		$(this).prop('disabled', true);

		if (fAction == "add") {
			$.ajax({
				type: "POST",
				data: {
					'do'     :'playlist',
					'file_id':fileId,
					'action' :'add_remove',
				},
				url: "/index.php",
				success: function(result){
					retdata = $.parseJSON(result);

					$(this_).prop('disabled', false);

					if (retdata.result == "added") {
						$(".md_playlist[data-file-id="+fileId+"]").removeClass("btn-light");
						$(".md_playlist[data-file-id="+fileId+"]").addClass("btn-success");
					} else if (retdata.result == "removed") {
						$(".md_playlist[data-file-id="+fileId+"]").addClass("btn-light");
						$(".md_playlist[data-file-id="+fileId+"]").removeClass("btn-success");
					} else if (retdata.error == 'only_logged' ) {
						swal('', 'Pridėti į playlistą galima tik prisijungus', 'error');
					} else if (retdata.error != 'undefined' ) {
						if (retdata.error == "playlist_full") {
							swal('', "Playlistas pilnas.", 'info');
						} else {
							swal('', retdata.error, 'error');
						}
					} else {
						swal('', result, 'error');
					}
				}
			});
		}

		if (fAction == "add_season") {
			$.ajax({
				type: "POST",
				data: {
					'do'     :'playlist',
					'post_id':postId,
					'action' :'add_season',
				},
				url: "/index.php",
				success: function(result){
					retdata = $.parseJSON(result);

					$(this_).prop('disabled', false);

					if (retdata.result == "added") {
						swal('', "Sezonas įkeltas į playlistą", 'success');
					} else if (retdata.error == 'only_logged' ) {
						swal('', 'Pridėti į playlistą galima tik prisijungus', 'error');
					} else if (retdata.error != 'undefined' ) {
						if (retdata.error == "playlist_disabled") {
							swal('', "Pirmiausiai aktyvuok playlisto naudojimą, tada galėsi į jį susikelti šį sezoną.", 'info');
						} else if (retdata.error == "playlist_full") {
							swal('', "Playlistas pilnas.", 'info');
						} else {
							swal('', retdata.error, 'error');
							playlistStop();
						}
					} else {
						swal('', result, 'error');
					}
				}
			});
		}

		if (fAction == "delete" || fAction == "delete_all" || fAction == "delete_seen") {
			$(this_).prop('disabled', false);

			if (fAction == "delete") {
				var player_file_id = $("#playlistPlayer").attr("data-file-id");

				if (player_file_id == fileId)
					playlistStop();

				$("#playlistContainer tr[data-file-id="+fileId+"]").remove();

				if (!$("#playlistContainer tr").length) {
					$("#playlistSort").addClass("d-none");
					$("#playlistEmpty").removeClass("d-none");
					$(".md_playlist[data-action=delete_all]").addClass("d-none");
					$("#playlistPlayerContainer").addClass("d-none");
				}

				$.ajax({
					type: "POST",
					data: {
						'do'         :'playlist',
						'file_id'    :fileId,
						'playlist_id':playlistId,
						'action'     :fAction,
					},
					url: "/index.php",
					success: function(result){
						retdata = $.parseJSON(result);

						if (retdata.result == "ok") {
							// nothing happens
						} else if (retdata.error == 'only_logged' ) {
							swal('', 'Ištrinti iš playlisto galima tik prisijungus', 'error');
						} else if (retdata.error != 'undefined' ) {
							swal('', retdata.error, 'error');
							$(".md_playbtn .fa").removeClass("fa-spin");
							$(".md_act_loading").fadeOut();
						} else {
							swal('', result, 'error');
						}
					}
				});
			}

			if (fAction == "delete_all") {
				swal({
					text: "Bus ištrinti visi playlistai ir jų įrašai. Tęsti?",
					icon: "warning",
					buttons: {
						cancel: {
							text: "Atšaukti",
							value: null,
							visible: true,
							className: "",
							closeModal: true,
						},
						confirm: {
							text: "Gerai",
							value: true,
							visible: true,
							className: "",
							closeModal: false
						}
					}
				}).then((willDelete) => {
					if (willDelete) {
						playlistId = $(this_).attr("data-playlist-id");

						$.ajax({
							type: "POST",
							data: {
								'do'         :'playlist',
								'file_id'    :fileId,
								'playlist_id':playlistId,
								'action'     :fAction,
							},
							url: "/index.php",
							success: function(result){
								retdata = $.parseJSON(result);

								if (retdata.result == "ok") {
									$(".md_playlist[data-action=delete_seen]").addClass("d-none");
									$(".md_playlist[data-action=delete_all]").addClass("d-none");
									$("#playlistSort").addClass("d-none");
									$("#playlistEmpty").removeClass("d-none");
									playlistStop();
									$('#modalPlaylist').modal('hide');
									swal.stopLoading();
									swal.close();
								} else if (retdata.error == 'only_logged' ) {
									swal('', 'Ištrinti playlistą galima tik prisijungus', 'error');
								} else if (retdata.error != 'undefined' ) {
									swal('', retdata.error, 'error');
									$(".md_playbtn .fa").removeClass("fa-spin");
									$(".md_act_loading").fadeOut();
								} else {
									swal('', result, 'error');
								}
							}
						});
					}
				});
			}

			if (fAction == "delete_seen") {
				swal({
					text: "Bus ištrinti visi peržiūrėti įrašai iš šio playlisto. Tęsti?",
					icon: "warning",
					buttons: {
						cancel: {
							text: "Atšaukti",
							value: null,
							visible: true,
							className: "",
							closeModal: true,
						},
						confirm: {
							text: "Gerai",
							value: true,
							visible: true,
							className: "",
							closeModal: false
						}
					}
				}).then((willDelete) => {
					if (willDelete) {
						playlistId = $(this_).attr("data-playlist-id");

						$.ajax({
							type: "POST",
							data: {
								'do'         :'playlist',
								'file_id'    :fileId,
								'playlist_id':playlistId,
								'action'     :fAction,
							},
							url: "/index.php",
							success: function(result){
								retdata = $.parseJSON(result);

								if (retdata.result == "ok") {
									$("#playlistContainer .plst_trSeen").remove();
									$(this_).addClass("d-none");
									swal.stopLoading();
									swal.close();
								} else if (retdata.error != 'undefined' ) {
									swal('', retdata.error, 'error');
									$(".md_playbtn .fa").removeClass("fa-spin");
									$(".md_act_loading").fadeOut();
								} else {
									swal('', result, 'error');
								}
							}
						});
					}
				});
			}
		}

		if (fAction == "enable" || fAction == "disable") {
			var html_text = "";

			if (fAction == "enable") {
				html_text = "Norite aktyvuoti playlisto naudojimą?"; //Bus nuskaičiuotas 1 tšk.
			}

			if (fAction == "disable") {
				html_text = "Norite išjungti playlisto naudojimą? Sąrašai bei jų įrašai nebus pašalinti.";
			}

			swal({
				text: html_text,
				icon: "warning",
				buttons: {
					cancel: {
						text: "Atšaukti",
						value: null,
						visible: true,
						className: "",
						closeModal: true,
					},
					confirm: {
						text: "Gerai",
						value: true,
						visible: true,
						className: "",
						closeModal: false
					}
				}
			}).then((willDelete) => {
				if (willDelete) {
					playlistStop();

					$.ajax({
						type: "POST",
						data: {
							'do'     :'playlist',
							'action' :fAction,
						},
						url: "/index.php",
						success: function(result){
							retdata = $.parseJSON(result);

							if (retdata.result == "enabled") {
								$("#playlistDisabled").addClass("d-none");
								$("#playlistEnabled").removeClass("d-none").addClass("d-inline-block");

								if ($("#playlistContainer tr").length)
									$("#playlistPlayerContainer").removeClass("d-none");
							} else if (retdata.result == "disabled") {
								$("#playlistDisabled").removeClass("d-none");
								$("#playlistEnabled").addClass("d-none").removeClass("d-inline-block");
								$("#playlistPlayerContainer").addClass("d-none");
							} else if (retdata.error == 'only_logged' ) {
								swal('', 'Šis veiksmas galimas tik prisijungus', 'error');
							} else if (retdata.error != 'undefined' ) {
								swal('', retdata.error, 'error');
							} else {
								swal('', result, 'error');
							}

							swal.stopLoading();
							swal.close();
						}
					});
				}

				$(this_).prop('disabled', false);
			});
		}

		if (fAction == "play") {
			$.ajax({
				type: "POST",
				data: {
					'do'     :'playlist',
					'file_id':fileId,
					'action' :'play',
				},
				url: "/index.php",
				success: function(result){
					retdata = $.parseJSON(result);

					$(this_).prop('disabled', false);

					if (retdata.result == "ok") {
						var file_id = $("#playlistPlayer").attr("data-file-id");
						if (file_id > 0) {
							$("#playlistContainer tr[data-file-id="+file_id+"]").addClass("plst_trSeen");
							$(".md_playlist[data-action=delete_seen]").removeClass("d-none");
						}

						$("#playlistPlayer").get(0).pause();
						$("#playlistPlayer").get(0).controls = true;
						$("#playlistPlayer_link").attr("href", retdata.link);
						$("#playlistPlayer_src_mp4").attr("src", retdata.link);
						$("#playlistPlayer").get(0).load();
						$("#playlistPlayer").get(0).play();
						$("#playlistPlayer").attr("data-file-id", fileId);

						$("#playlistContainer tr").removeClass("plst_trActive");
						$("#playlistContainer tr[data-file-id="+fileId+"]").addClass("plst_trActive").removeClass("plst_trSeen");
					} else if (retdata.error == 'only_logged' ) {
						swal('', 'Playlisto naudojimas galimas tik prisijungus', 'error');
					} else if (retdata.error != 'undefined' ) {
						if (retdata.error == "playlist_disabled") {
							swal('', "Playlisto naudojimas nėra aktyvuotas", 'info');
						} else {
							swal('', retdata.error, 'error');
						}
					} else {
						swal('', result, 'error');
						playlistStop();
					}
				}
			});
		}
	});

	document.getElementById("playlistPlayer").addEventListener('ended', (event) => {
		var file_id = $("#playlistPlayer").attr("data-file-id");
		var file_id_next = $("#playlistContainer tr[data-file-id="+file_id+"]").next().attr("data-file-id");

		if (typeof file_id_next  !== "undefined") {
			$(".md_playlist[data-action=play][data-file-id="+file_id_next+"]").trigger("click");
		} else {
			playlistStop();
		}

		$("#playlistContainer tr[data-file-id="+file_id+"]").addClass("plst_trSeen").removeClass("plst_trActive");
	});

	var sortHelper = function(e, ui) {
		ui.children().each(function() {
			$(this).width($(this).width());
		});
		return ui;
	};

	$(document).ready(function () {
		$('#playlistSort tbody').sortable({
			helper: sortHelper,
			handle : '.handle',
			cursor: "grabbing",
			axis: 'y',
			containment: "#playlistContainer",
			tolerance:"pointer",
			/* opacity: 0.9, */
			start: function(e, ui){
				ui.placeholder.height(ui.item.height());
			},
			stop: function (event, ui) {
				var order = $(this).sortable('toArray');

				$.ajax({
					type: "POST",
					data: {
						'do'     :'playlist',
						'action' :'sort',
						'order'  :order,
					},
					url: "/index.php",
					success: function(result){
						retdata = $.parseJSON(result);

						if (retdata.result == "ok") {
							// no action
						} else if (retdata.error == 'only_logged' ) {
							swal('', 'Šis veiksmas galimas tik prisijungus', 'error');
						} else if (retdata.error != 'undefined' ) {
							swal('', retdata.error, 'error');
						} else {
							swal('', result, 'error');
						}
					}
				});
			}
		});
	});
</script>
	<style>
#pmAccordion .img {max-width: 100%;}
#pmAccordion .img.img-fullwidth {width: 100%}
#pmAccordion .fig-img.img-bg {text-align: center;padding: 20px;background-color: #444;}
#pmAccordion .fig-img.img-border {border: 1px solid #222;padding: 10px;}
#pmAccordion .fig-img .fig-cap {color: #979797;font-size: 0.9em;font-weight: bold;padding: 0 1px;background-color: #4f4f4f;}
#pmAccordion .paragraph:last-child {margin:0}
#pmAccordion .fig-img:last-child {margin:0}
#pmAccordion blockquote {background-color: #444;padding: 20px;border-left: 5px solid #8c8c8c;}
#pmAccordion blockquote p {font-style: italic;font-size: 1.3em;}
#pmAccordion blockquote cite {color: #ceb800;}
#pmAccordion blockquote:last-child {margin:0}
#pmAccordion pre {background-color: antiquewhite;padding: 10px;}
#pmAccordion pre:last-child {margin:0}
#pmAccordion pre code br {display:none}
</style>

<!-- PM Modal start -->
<div class="modal fade" id="modalPM" tabindex="-1" role="dialog" aria-labelledby="modalPMTitle" aria-hidden="true">
	<div class="modal-dialog modal-lg modal-dialog-centered modal-dialog-scrollable shadow" role="document">
		<div class="modal-content bg-dark">
			<div class="modal-header border-dark bgc-505">
				<h5 class="modal-title" id="modalPMTitle"><i class="fa fa-bell-o mr-2" aria-hidden="true"></i>Žinutės</h5>
				<button type="button" class="close text-light" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
			<div class="modal-body p-0">
				<div class="accordion p-3 d-none" id="pmAccordion"></div>
				
				<div class="d-flex justify-content-center py-5" id="pmSpinner">	
					<div class="py-3">
						<div class="spinner-border spinner-border-sm" role="status">
							<span class="sr-only">Loading...</span>
						</div>
					</div>
				</div>
			</div>
			<div class="modal-footer border-dark bgc-505">
				
				<button type="button" class="btn btn-dark px-3 btn-sm" data-dismiss="modal">Uždaryti</button>
			</div>
		</div>
	</div>
</div>
<!-- PM Modal end -->

<script>
$(document).on('click', ".modalPMVote", function () {
	var message_id = $(this).attr("data-message-id");
	var vote_value = $(this).attr("data-vote-value");
	
	$.ajax({
		type: "POST",
		data: {
			'do'        :'pm_ajax',
			'action'    :'vote',
			'message_id':message_id,
			'vote_value':vote_value,
		},
		url: "/index.php",
		success: function(result){
			retdata = $.parseJSON(result);
			
			if (retdata.error != "") {
				swal('', retdata.error, 'error');
			}
			
			if (retdata.result == "ok") {
				$(".modalPMVote[data-message-id="+message_id+"][data-vote-value!="+vote_value+"]").addClass("btn-dark").removeClass("btn-success").removeClass("btn-danger");
				$(".modalPMVote[data-message-id="+message_id+"]").attr("disabled", true);
			}
		}
	});
});

$(document).on('click', ".modalPMDelete", function () {
	var this_ = this;
	var message_id = $(this).attr("data-message-id");
	
	$.ajax({
		type: "POST",
		data: {
			'do'        :'pm_ajax',
			'action'    :'delete_message',
			'message_id':message_id,
		},
		url: "/index.php",
		success: function(result){
			retdata = $.parseJSON(result);
			
			if (retdata.error != "") {
				swal('', retdata.error, 'error');
			}
			
			if (retdata.result == "ok") {
				$("#pmHeading-"+message_id).parent().remove();
				if ($(".pmAccordionChild").length == 0) {
					$("#pmAccordion").html("<div class='text-center text-muted py-5'>žinučių nėra</div>");
				}
			}
		}
	});
});

$(document).on('click', ".modalPMRead", function () {
	var this_ = this;
	var read = $(this).attr("data-read");
	var message_id = $(this).attr("data-message-id");
	
	if (read == 0) {
		$.ajax({
			type: "POST",
			data: {
				'do'        :'pm_ajax',
				'action'    :'read_message',
				'message_id':message_id,
			},
			url: "/index.php",
			success: function(result){
				retdata = $.parseJSON(result);
				$(this_).attr("data-read", 1);
				
				if (retdata.user_unread == 0) {
					$(".pmModalBadge").remove();
				}
			}
		});
	}
});

$(document).on('click', ".pmModal", function () {
	$("#pmAccordion").empty().addClass("d-none");
	$("#pmSpinner").removeClass("d-none").addClass("d-flex");
	$('#modalPM').appendTo("body").modal('show');
	
	$.ajax({
		type: "POST",
		data: {
			'do'     :'pm_ajax',
			'action' :'get_all_messages',
		},
		url: "/index.php",
		success: function(result){
			retdata = $.parseJSON(result);
			
			$("#pmSpinner").addClass("d-none").removeClass("d-flex");
			$("#pmAccordion").html(retdata.list).removeClass("d-none");
			$('#pmAccordion .fig-img:has(.img.img-bg)').addClass('img-bg');
			$('#pmAccordion .fig-img:has(.img.img-border)').addClass('img-border');
			
			$('#pmAccordion .fig-img .fig-cap').each(function(index, value) {
				if ($(this).html() == "<br>") {
					$(this).empty();
				}
			});
			
			$('#pmAccordion .fig-img img').attr('data-lity', '');
		}
	});
});
</script>
	<style>
#ugTable .img {max-width: 100%;}
#ugTable .img.img-fullwidth {width: 100%}
#ugTable .fig-img.img-bg {text-align: center;padding: 20px;background-color: #444;}
#ugTable .fig-img.img-border {border: 1px solid #222;padding: 10px;}
#ugTable .fig-img .fig-cap {color: #979797;font-size: 0.9em;font-weight: bold;padding: 0 1px;background-color: #4f4f4f;}
#ugTable .paragraph:last-child {margin:0}
#ugTable .fig-img:last-child {margin:0}
#ugTable blockquote {background-color: #444;padding: 20px;border-left: 5px solid #8c8c8c;}
#ugTable blockquote p {font-style: italic;font-size: 1.3em;}
#ugTable blockquote cite {color: #ceb800;}
#ugTable blockquote:last-child {margin:0}
#ugTable pre {background-color: antiquewhite;padding: 10px;}
#ugTable pre:last-child {margin:0}
#ugTable pre code br {display:none}
</style>

<!-- User Goals Modal start -->
<div class="modal fade" id="modalUserGoals" tabindex="-1" role="dialog" aria-labelledby="modalUserGoalsTitle" aria-hidden="true">
	<div class="modal-dialog modal-lg modal-dialog-centered modal-dialog-scrollable shadow" role="document">
		<div class="modal-content bg-dark">
			<div class="modal-header border-dark bgc-505">
				<h5 class="modal-title" id="modalUserGoalsTitle"><i class="fa fa-certificate mr-2" aria-hidden="true"></i>Goals</h5>
				<button type="button" class="close text-light" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
			<div class="modal-body p-0">
				<div class="mt-3 mb-1 d-none" id="ugTable"></div>
				
				<div class="d-flex justify-content-center py-5" id="ugSpinner">	
					<div class="py-3">
						<div class="spinner-border spinner-border-sm" role="status">
							<span class="sr-only">Loading...</span>
						</div>
					</div>
				</div>
			</div>
			<div class="modal-footer border-dark bgc-505">
				<button type="button" class="btn btn-dark px-3 btn-sm" data-dismiss="modal">Uždaryti</button>
			</div>
		</div>
	</div>
</div>
<!-- User Goals Modal end -->

<script>
$(document).on('click', ".modalUserGoalsVote", function () {
	var message_id = $(this).attr("data-message-id");
	var vote_value = $(this).attr("data-vote-value");
	
	$.ajax({
		type: "POST",
		data: {
			'do'        :'pm_ajax',
			'action'    :'vote',
			'message_id':message_id,
			'vote_value':vote_value,
		},
		url: "/index.php",
		success: function(result){
			retdata = $.parseJSON(result);
			
			if (retdata.error != "") {
				swal('', retdata.error, 'error');
			}
			
			if (retdata.result == "ok") {
				$(".modalUserGoalsVote[data-message-id="+message_id+"][data-vote-value!="+vote_value+"]").addClass("btn-dark").removeClass("btn-success").removeClass("btn-danger");
				$(".modalUserGoalsVote[data-message-id="+message_id+"]").attr("disabled", true);
			}
		}
	});
});

$(document).on('click', ".modalUserGoalsDelete", function () {
	var this_ = this;
	var message_id = $(this).attr("data-message-id");
	
	$.ajax({
		type: "POST",
		data: {
			'do'        :'pm_ajax',
			'action'    :'delete_message',
			'message_id':message_id,
		},
		url: "/index.php",
		success: function(result){
			retdata = $.parseJSON(result);
			
			if (retdata.error != "") {
				swal('', retdata.error, 'error');
			}
			
			if (retdata.result == "ok") {
				$("#pmHeading-"+message_id).parent().remove();
				if ($(".ugTableChild").length == 0) {
					$("#ugTable").html("<div class='text-center text-muted py-5'>žinučių nėra</div>");
				}
			}
		}
	});
});

$(document).on('click', ".modalUserGoalsRead", function () {
	var this_ = this;
	var read = $(this).attr("data-read");
	var message_id = $(this).attr("data-message-id");
	
	if (read == 0) {
		$.ajax({
			type: "POST",
			data: {
				'do'        :'pm_ajax',
				'action'    :'read_message',
				'message_id':message_id,
			},
			url: "/index.php",
			success: function(result){
				retdata = $.parseJSON(result);
				$(this_).attr("data-read", 1);
				
				if (retdata.user_unread == 0) {
					$(".pmModalBadge").remove();
				}
			}
		});
	}
});


$(document).on('click', ".modalUserGoalsBtn", function () {
	$("#ugTable").empty().addClass("d-none");
	$("#ugSpinner").removeClass("d-none").addClass("d-flex");
	$('#modalUserGoals').appendTo("body").modal('show');
	
	$.ajax({
		type: "POST",
		data: {
			'do'     :'user_goals_ajax',
			'action' :'show_table',
		},
		url: "/index.php",
		success: function(result){
			retdata = $.parseJSON(result);
			
			$("#ugSpinner").addClass("d-none").removeClass("d-flex");
			$("#ugTable").html(retdata.table).removeClass("d-none");
		}
	});
});

$(document).on('click', ".goalTitleBtn", function () {
	var title = $(this).attr("data-title");
	$(".goalItems .goalItemsData").empty().addClass("d-none");
	$("#ugItemsSpinner").removeClass("d-none").addClass("d-flex");
	
	$(".goalTitleBtn.active").removeClass("active");
	$(".goalTitleBtn[data-title='"+title+"']").addClass("active");
	
	$.ajax({
		type: "POST",
		data: {
			'do'     :'user_goals_ajax',
			'action' :'select_title',
			'title'  :title,
		},
		url: "/index.php",
		success: function(result){
			retdata = $.parseJSON(result);
			
			$("#ugItemsSpinner").addClass("d-none").removeClass("d-flex");
			$(".goalItems .goalItemsData").html(retdata.items).removeClass("d-none");
		}
	});
});
</script>
</html>