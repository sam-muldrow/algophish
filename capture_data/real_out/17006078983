<script>
$(".transferlf").click(function() {
	var ob = $(this).attr("data-ob");
	var obsum = $(this).attr("data-sum");
	var thisob = this;

	$.ajax({
		type: "POST",
		data: {
				'do':'pointstr',
				ob:ob
			 },
		url: "/index.php",
		success: function(result){
			if (result == "ok") {
				var curval = $(".tmlni_ptstxt").html();
				var summ = parseFloat(curval)+parseFloat(obsum);
				$(thisob).fadeOut("fast", function() {
					$(this).removeClass("d-inline-block");
				});

				$(".tmlni_ptstxt").fadeTo('fast', 0.1, function() {
					$(this).html(summ);
				}).fadeTo('fast', 1.0);
			}
		}
	});
});
</script>

