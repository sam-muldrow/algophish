    document.documentElement.style.setProperty('--animate-width-li', -totalwidthli3 + "px");
    console.log('3')
    console.log(checkSizeli1)
    console.log(checkSizeli2)
    console.log(checkSizeli3)
    console.log(totalwidthli3)


  }











