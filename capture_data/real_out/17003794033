                                a.substr(0, 4)
                            )
                        )
                            check = true;
                    })(navigator.userAgent || navigator.vendor || window.opera);
                    return check;
                }
                var bMobile = mobileAndTabletcheck();
                if (bMobile) {
                    var playerWidth = 300;
                    var playerHeight = 169;
                } else {
                    var playerWidth = 640;
                    var playerHeight = 360;
                }

                const videoDiv = bid.adUnitCode;
                let script = window.document.createElement("script");
