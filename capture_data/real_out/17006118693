<script type="text/javascript">
        $(document).ready(function()
        {
            // Verifica se eh mobile
            var isMobile = $.browser.device = (/android|webos|iphone|ipad|ipod|blackberry|iemobile|opera mini/i.test(navigator.userAgent.toLowerCase()));
        
            if (isMobile)
                $('.topo').remove(); // Garantir que nao mostra mais a versao antiga no mobile
            else
                $('topo_box').remove(); // Remove o html renderizado do mobile
        
            // Busca
            $('#bx_busca_pp').click(function()
            {
                if (!$(this).hasClass('aberto'))
                {
                    $(this).addClass('aberto');
                    $('.box_suspenso_busca').show();
                    $('.icone_busca').hide();
                    $('.icone_fechar_busca').show();
                    $('.tx_input_busca').focus();
                }
                else
                {
                    $(this).removeClass('aberto');
                    $('.box_suspenso_busca').hide();
                    $('.icone_busca').show();
                    $('.icone_fechar_busca').hide();
                }
            });
        
            // Valida busca mobile
            $('.bt_input_busca').click(function()
            {
                var str = $('.tx_input_busca').val();
                
                if (str === '' || str === 'O que busca?' || str.length <= 4)
                {
                    alert('Digite algum valor para sua busca!');
        
                    $('.tx_input_busca').focus();
                    
                    return false;	
                }
            
                return true;
                
            });
        
            // Menu
            $('#bx_menu_pp').click(function()
            {
                if (!$(this).hasClass('aberto'))
                {
                    $(this).addClass('aberto');
                    $('.box_suspenso_menu').show();
                    $('.icone_menu').hide();
                    $('.icone_fechar_menu').show();
                }
                else
                {
                    $(this).removeClass('aberto');
                    $('.box_suspenso_menu').hide();
                    $('.icone_menu').show();
                    $('.icone_fechar_menu').hide();
                }
            });
        
            if (isMobile)
            {
                // Moeda
                $.ajax({
                    type: 'GET',
                    url: 'https://economia.awesomeapi.com.br/USD-BRL/1?format=json',
                    dataType: 'json',
                    //error: function()
                    //{
                    //    alert('Ocorreu um erro ao carregar os dados.');
                    //},
                    success: function(data)
                    {
                        $('.box_info').show();
        
                        var vlr = data[0]['ask'],
                            pct = data[0]['pctChange'];
        
                        var vl_numerico = parseFloat(vlr).toFixed(3);
        
                        var vl_final = 'R$ ';
                            vl_final += vl_numerico.replace('.', ',');
        
                        if (pct > 0)
                        {
                            $('.box_moeda').addClass('valor_subiu');
                            $('.box_moeda').removeClass('valor_caiu');
                        }
                        else
                        {
                            $('.box_moeda').addClass('valor_caiu');
                            $('.box_moeda').removeClass('valor_subiu');
                        }
                        
                        $('.box_moeda .tx_valor').html(vl_final);
                    }
                });
            }
        });
        </script>
</head>
