    var $jq = jQuery.noConflict();
    var $sliderInitialized = false;

    function spclicked( btn ) {
            $jq("#sp_hl_1").hide();
            $jq("#sp_hl_2").hide();
            $jq("#sp_hl_3").hide();
            $jq("#sp_hl_4").hide();
            $jq("#sp_hl_5").hide();
            $jq("#sp_hl_6").hide();
            
            var uphl = btn.replace(/_btn_/,"_hl_");
            $jq(uphl).show();
    }

    document.addEventListener("DOMContentLoaded", function(event) {
        var target = $jq('.slider-pro','#spet .jet-slider')
        target.on("showLayersComplete", function(event) {
          if (! $sliderInitialized ) {
            //console.log("target inited");
            $sliderInitialized = true;
            var slider = $jq('.slider-pro','#spet .jet-slider').data( 'sliderPro' );
            slider.on( 'gotoSlide', function( event ) {
              switch (event.index) {
                case 0:
                    spclicked("#sp_btn_1");
                    $jq("#spet").on("click", function() {
                        Countly.q.push(['add_event',{
	                        "key": "function",
                            "count": 1,
	                        "segmentation": {
                            "action": "click_slide1",
                            "label": "organic"
	                        }
                        }]);
                      window.location = "/product/feature-supernote/";
                    });
                    break;
                case 1:
                    spclicked("#sp_btn_2");
                    $jq("#spet").click(function() {
                        Countly.q.push(['add_event',{
	                        "key": "function",
                            "count": 1,
	                        "segmentation": {
                            "action": "click_slide2",
                            "label": "organic"
