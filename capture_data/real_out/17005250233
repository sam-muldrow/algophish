				<div class="elementor-element elementor-element-f3a201d elementor-widget elementor-widget-html" data-id="f3a201d" data-element_type="widget" data-widget_type="html.default">
				<div class="elementor-widget-container">
			<script type="text/javascript">

    var $jq = jQuery.noConflict();
    var $msliderInitialized = false;

    function mclicked( btn ) {
            $jq("#msp_hl_1").hide();
            $jq("#msp_hl_2").hide();
            $jq("#msp_hl_3").hide();
            $jq("#msp_hl_4").hide();
            $jq("#msp_hl_5").hide();
            $jq("#msp_hl_6").hide();
            $jq("#msp_hl_7").hide();
            
            var uphl = btn.replace(/_btn_/,"_hl_");
            $jq(uphl).show();
    }

    document.addEventListener("DOMContentLoaded", function(event) {
        var target = $jq('.slider-pro','#mspet .jet-slider')
        target.on("showLayersComplete", function(event) {
          if (! $msliderInitialized ) {
            //console.log("target inited");
            $msliderInitialized = true;
            var slider = $jq('.slider-pro','#mspet .jet-slider').data( 'sliderPro' );
            slider.on( 'gotoSlide', function( event ) {
              switch (event.index) {
                case 0:
                    mclicked("#msp_btn_1");
                    $jq("#mspet").on("click", function() {
                      window.location = "/product/feature-supernote/";
                    });
                    break;
                case 1:
                    mclicked("#msp_btn_2");
                    $jq("#mspet").click(function() {
                      window.location = "/product/webclipper1/";
                    });
                    break;
                case 2:
                    mclicked("#msp_btn_3");
                    $jq("#mspet").click(function() {
                      window.location = "/product/feature-task/";
                    });
                    break;
                case 3:
                    mclicked("#msp_btn_4");
                    $jq("#mspet").click(function() {
                      window.location = "/product/feature-mindmap1/";
                    });
                    break;
                case 4:
                    mclicked("#msp_btn_5");
                    $jq("#mspet").click(function() {
                      window.location = "/product/feature-template/";
                    });
                    break;
                case 5:
                    mclicked("#msp_btn_6");
                    $jq("#mspet").click(function() {
                      window.location = "/product/feature-ocr/";
                    });
                    break;
              }
	          //console.log( event.index );
            });
            $jq("#msp_btn_1").on("click", function() {
              slider.gotoSlide( 0 );
              return false;
            });
            $jq("#msp_btn_2").on("click", function() {
              slider.gotoSlide( 1 );
              return false;
            });    
            $jq("#msp_btn_3").on("click", function() {
              slider.gotoSlide( 2 );
              return false;
            });
            $jq("#msp_btn_4").on("click", function() {
              slider.gotoSlide( 3 );
              return false;
            });
            $jq("#msp_btn_5").on("click", function() {
              slider.gotoSlide( 4 );
              return false;
            });

            $jq("#msp_btn_6").on("click", function() {
              slider.gotoSlide( 5 );
              return false;
            });
          }
        });
        mclicked("#msp_btn_1");
    });
    
</script>		</div>
				</div>
						</div>
					</div>
		</div>
								</div>
					</div>
		</section>
