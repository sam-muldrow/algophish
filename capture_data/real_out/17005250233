                        Countly.q.push(['add_event',{
	                        "key": "function",
                            "count": 1,
	                        "segmentation": {
                            "action": "click_slide6",
                            "label": "organic"
	                        }
                        }]);
                      window.location = "/product/feature-ocr/";
                    });
                    break;
              }
	          //console.log( event.index );
            });
            $jq("#sp_btn_1").on("click", function() {
              slider.gotoSlide( 0 );
              return false;
            });
            $jq("#sp_btn_2").on("click", function() {
              slider.gotoSlide( 1 );
              return false;
            });    
            $jq("#sp_btn_3").on("click", function() {
              slider.gotoSlide( 2 );
              return false;
            });
            $jq("#sp_btn_4").on("click", function() {
              slider.gotoSlide( 3 );
              return false;
            });
            $jq("#sp_btn_5").on("click", function() {
              slider.gotoSlide( 4 );
              return false;
            });

            $jq("#sp_btn_6").on("click", function() {
              slider.gotoSlide( 5 );
              return false;
            });
          }
        });
        spclicked("#sp_btn_1");
        $jq("#spet").on("click", function() {
                        Countly.q.push(['add_event',{
	                        "key": "function",
                            "count": 1,
	                        "segmentation": {
