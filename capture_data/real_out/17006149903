function showImg($node) {
	for(var i = 0,len = $node.length; i < len; i++){
		$node.eq(i).attr("src",$node.eq(i).attr("data-src"));
	}
}
if (IEVersion == 8) {
	showImg($(".lazyload"));
}
</script>
