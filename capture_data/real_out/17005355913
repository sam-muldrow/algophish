	$( document ).ready(function() {
		setTimeout(function(){
			console.log($('#desktop_ad'));
			document.getElementsByClassName('footer_sticky_ad')[0].style.display='block';
			document.getElementsByClassName('footer_sticky_ad_mob')[0].style.display='block';
