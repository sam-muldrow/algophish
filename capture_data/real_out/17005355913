<script type="text/javascript">
	var name = "cp_token=";
	var decodedCookie = decodeURIComponent(document.cookie);
	var ca = decodedCookie.split(';');
	for(var i = 0; i <ca.length; i++) {
		var c = ca[i];
		while (c.charAt(0) == ' ') {
			c = c.substring(1);
		}
		if (c.indexOf(name) == 0) {
			$('#userDetailsDiv').css('display','block');
			$('#login-topnav').css('display','none');
		}
	}

/*login refresh end */

/* sso login with lead form start */

	$(".Click-here").on('click', function() {
		$(".custom-model-main").addClass('model-open');
	}); 
	$(".close-btn, .bg-overlay").click(function(){
		$(".custom-model-main").removeClass('model-open');
	});

	$(".Click-heree").on('click', function() {
		$(".custom-model-mainn").addClass('model-open');
	}); 
	$(".close-btn, .bg-overlay").click(function(){
		$(".custom-model-mainn").removeClass('model-open');
	});

/* popup leadform start */

/* sso enchament start */

	function adda247_lead_form_getCookie(name) {
		const value = `; ${document.cookie}`;
		const parts = value.split(`; ${name}=`);
		if (parts.length === 2) return parts.pop().split(';').shift();
	}

	$( document ).ready(function() {
		// popup leader from start
		function readCookie(name) {
			var nameEQ = name + "=";
			var ca = document.cookie.split(';');
			for(var i=0;i < ca.length;i++) {
				var c = ca[i];
				while (c.charAt(0)==' ') c = c.substring(1,c.length);
				if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
			}
			return null;
		}

	$('.local-widget .lead-form-btn').click(function(){
		
		var email = readCookie('lead_email');
		if(email){
			$('.local-widget .downloadPDF').attr('download','download');
			var localpdf = $('.local-widget .downloadPDF').attr('data');
			$('#adda247_lead_form #adda247_lead_form_pdflink').val(localpdf);
			$('.local-widget .downloadPDF').attr('href',localpdf);
			var localTitle = $('.local-widget h2.get-all-detils').text();
			$('#adda247_lead_form #adda247_lead_form_keyword').val(localTitle);
			var localButtonText = $('.local-widget .downloadPDF').find('button').text();
			$('#adda247_lead_form #adda247_lead_form_extraField1').val(localButtonText);
			$("#adda247_lead_form_name").val(adda247_lead_form_getCookie("lead_name"));
			$("#adda247_lead_form_email").val(adda247_lead_form_getCookie("lead_email"));
			$('#adda247_lead_form_submit').trigger('click');
			$('.custom-model-main.final-success').addClass('model-open');
		}
		else{
			$('.custom-model-main.new-user').addClass('model-open');
			$('#adda247_lead_form_submit').addClass('localpdfdownload');
			$('.local-widget .downloadPDF').removeAttr('download', 'download');
			$('.local-widget .downloadPDF').attr('href','javascript:void(0)');
			
		}
	});



	});

	/* 	
		sso enhancment end 
		sso login with lead form end 
		menu fix start 
	*/ 

