				$(".dpsp-facebook").click(function(){
			var post_ids = 103821;
			$.ajax({
				url: "https://www.bankersadda.com/wp-admin/admin-ajax.php", 
				type: 'POST',
				data: { 
					action : 'insert_records',
					fb_count: '1',  
					pos_tid: post_ids,  
				},
				success: function(data){
					if (data.res == true){
            console.log('insert');    // success message
        }else{
            console.log('error');    // fail
        }
    }
});
		});


