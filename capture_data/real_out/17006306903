<script>
    $(document).ready(function () {
        $('.shop').select2();
        $('.select2-search').hide();
    });
    $(function () {
        var checked = function () {
            $("#chosen").html($(".dropdown-menu > li.active-item").attr("id"));
            var $activeItem = $("li.active-item > a");
            $('form.search').attr('action', $activeItem.data('url'));
            $(".search-dropdown > .chosen-item").html($activeItem.html());
        };

        checked();

        $(".dropdown-menu > li.option").click(function () {
            $(".dropdown-menu > li.active-item").removeClass("active-item");
            $(this).addClass("active-item");
            checked();
        });

        $('[data-role="trend"]').on({
            'mouseenter': function () {
                $(".trends-list").attr('data-background', $(this).data('background'));
            },
            'mouseleave': function () {
                $(".trends-list").attr('data-background', '');
            }
        });
    });
</script>

            


