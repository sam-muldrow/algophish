<script>
    $('.fotorama-fast-img').css('opacity', 1);
    var fr = $('.fotorama');
    fr.fotorama();
    fr.on('fotorama:load', function () {
        $('.fotorama-fast-img').hide();
    });

    fr.on('fotorama:show', function (e, fr) {
        if(typeof analytics !== 'undefined') {
            analytics.watch();
        }
        if($('.banner-big__link').hasClass('load_first_banner')){
            $('.banner-big__link').removeClass('load_first_banner')
        }
    });

    $(document).on('ready', function(){
        $('.banner-big__link').addClass('load_first_banner');
    })

    function fotoramaClick(elem){
        analytics.gtmActionPromotion(elem[0], elem[0].closest('.banners-list').dataset.title, 'select_promotion', false)

        if(!elem.hasClass('load_first_banner')){
            elem.data('creative', $('.banners-list').data('title'));
            analytics.analyticsManager.sendEventSimple({eventType: Analytics.EVENTS_TYPE.clickToPromo}, elem.data());
        }
    }
</script>        <div class="autodiscounts autodiscounts_main">
