<script>
    if (document.querySelectorAll('.js-autobanner')) {

        document.querySelectorAll('.js-autobanner').forEach((banner, index) => {
            let autobannerContent = banner.querySelector('.js-autobanner-content');
            let divImgCenter = banner.querySelector('.js-img-center');

            let mediaTypeImage = autobannerContent.getAttribute('data-media-type-image');
            let mediaTypeVideo = autobannerContent.getAttribute('data-media-type-video');

            let mediaPath = autobannerContent.getAttribute('data-image-src');
            let videoPath = autobannerContent.getAttribute('data-video-src');
            let imgCenterSrc = divImgCenter.getAttribute('src');
            let imgCenterAlt = divImgCenter.getAttribute('alt');

            window.addEventListener('load', (event) => {
                let imgCenter = document.createElement('img');
                imgCenter.setAttribute('src', imgCenterSrc);
                imgCenter.setAttribute('alt', imgCenterAlt);

                if (divImgCenter.children.length < 1) {
                    divImgCenter.append(imgCenter);
                }

                if (mediaTypeImage && mediaPath) {
                    let img = document.createElement('img');
                    img.setAttribute('src', mediaPath);
                    img.setAttribute('alt', '');
                    img.classList.add('autobanner__left-img');

                    if (autobannerContent.children.length < 1) {
                        autobannerContent.append(img);

                        img.onload = function () {
                            banner.classList.add('animation__slide-in-top');
                        }
                    }
                }

                if (mediaTypeVideo && videoPath) {

                    if (videoPath.includes('youtube.com') && window.innerWidth > 768) {
                        autobannerContent.innerHTML = `<div id=iframeVideo${index} class='autobanner__left-iframe'></div>`;

                        //вставляется script api youtube
                        var tag = document.createElement('script');
                        var firstScriptTag = document.getElementsByTagName('script')[0];
                        tag.src = 'https://www.youtube.com/iframe_api';
                        firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

                    } else {
                        autobannerContent.innerHTML = `<iframe class='autobanner__left-iframe' src='${videoPath}'
                                               allow='autoplay; encrypted-media; fullscreen; accelerometer; gyroscope;
                                               picture-in-picture' allowfullscreen></iframe>`;

                        imgCenter.onload = function () {
                            autobannerContent.children[0].onload = () => {
                                banner.classList.add('animation__slide-in-top');
                            }
                        }
                    }
                }

            });

        })
    }

    //вызывается в script api youtube
    function onYouTubeIframeAPIReady() {
        if (document.querySelectorAll('.js-autobanner')) {

            document.querySelectorAll('.js-autobanner').forEach((banner, index) => {
                let intervalId;
                var player;
                let autobannerContent = banner.querySelector('.js-autobanner-content');
                let videoPath = autobannerContent.getAttribute('data-video-src');
                let youtubeId = youtubeParser(videoPath);
                let idFrame = `iframeVideo${index}`;

                player = new YT.Player(idFrame, {
                    youtubeId: youtubeId,
                    playerVars: {
                        'autoplay': 1,
                        'loop': 1,
                        'playlist': youtubeId,
                        'controls': 0,
                        'showinfo': 0,
                        'disablekb': 1,
                        'modestbranding': 1,
                        'mute': 1
                    },
                    events: {
                        'onReady': onPlayerReady,
                        'onStateChange': onPlayerStateChange
                    }
                });

                function onPlayerReady(event) {
                    player.mute();
                    event.target.playVideo();

                    // для firefox так как там не срабатывает onPlayerStateChange
                    let counterCall = 0;
                    intervalId = setInterval(() => {
                        if (counterCall > 5) {
                            clearTimeout(intervalId);
                        }
                        counterCall++;
                        onPlayerStateChange();
                    }, 500);
                }

                function onPlayerStateChange() {
                    if (player.getPlayerState() === 1) {
                        clearTimeout(intervalId);
                        banner.classList.add('animation__slide-in-top');
                    }
                }

                // поиск youtube id
                function youtubeParser(url) {
                    const regExp = /^.*((youtu.be\/)|(v\/)|(\/u\/\w\/)|(embed\/)|(watch\?))\??v?=?([^#&?]*).*/;
                    const match = url.match(regExp);
                    return (match && match[7].length == 11) ? match[7] : false;
                }
            })
        }
    }

</script>
