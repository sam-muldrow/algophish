<script>
	new Swiper('.autodiscounts-swiper', {
		slidesPerView: 'auto',
		freeMode: true,
		freeModeMomentum: true,
		mousewheel: false,
		forceToAxis: true,
		pagination: {
			el: '.autodiscounts-swiper .carousel-trends__pagination',
			type: 'custom',
			clickable: true,
			renderCustom: function(swiper, current, total) {
				var left = (current - 1) / total * 100;
				width = 100 / total;
				$(swiper.$el).find('.carousel-trends__pagination-item').css('left', left + '%').css('width', width + '%');
			}
		},
		navigation: {
			prevEl: ".autodiscounts-swiper .swipe-arrow-left",
			nextEl: ".autodiscounts-swiper .swipe-arrow-right"
		}
	});
