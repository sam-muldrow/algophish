        function getElementCountByParameters(selector) {
            var value = 0,
                elems = document.querySelectorAll(selector);
            if (elems) {
                value = elems.length;
            }
            if (value >= 2) {
                var articleType = '';
                if (articleType === 'BBC' || articleType == 'DW') {
                    value = value - 2;
                }
            }
            return value;
        }

        document.addEventListener('DOMContentLoaded',
            function () {

