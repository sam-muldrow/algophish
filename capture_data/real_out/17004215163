    <!-- Initialize Swiper --><!-- AboveSpot -->
    <script>

        var container_4B78705E = '#slider_4B78705E .swiper-container';
    var parameters_4B78705E = {
    preloadImages: false,
            lazy: {
        loadPrevNext: true,
            },
            loop: true,
            pagination: {
        el: '#slider_4B78705E .swiper-pagination',
                renderBullet: function (index, className) {
                var links = document.querySelectorAll("#slider_4B78705E  .swiper-slide__link"),
                        currentIndex = index + 1;

                var findHref = function(crr) {
                    var elem = document.querySelector(`#slider_4B78705E  div[data-swiper-slide-index="${crr}"] .swiper-slide__link`);
                        return elem;
                };

                 return '<a data-slider-index="' + currentIndex + '" data-slider-name="slider_4B78705E" class="swiper-pagination-link go-to-link ' + className + '" href="' + findHref(index) + '" target="_blank" onmouseover="slObj_4B78705E.slideTo(' + currentIndex + ')"  >' + currentIndex + '</a>';


                    

                },
            },


            navigation: {
                nextEl: '#slider_4B78705E .swiper-slide__arrow--next',
                prevEl: '#slider_4B78705E .swiper-slide__arrow--prev',
            },
            
            autoplay: {
                delay: 10000,
                disableOnInteraction: false
            },
            
        };

        var slObj_4B78705E = {};

         function addHomeCarouselItem(item) {
            var swiper = document.querySelector('#slider_4B78705E .swiper-container').swiper, isCarouselAd = document.querySelector('#slider_4B78705E .swiper-container .carouselAd');
            if (!isCarouselAd && item.url !== "") {

                var newItem = `<div class="swiper-slide carouselAd"><a href="${item.url}" title="${item.title}" class="swiper-slide__link" target="_blank"><img src="${item.image}" title="${item.title}" alt="${item.title}" width="700" height="600" class="swiper-slide__image lazyload"></a></div>`;
                swiper.addSlide(item.index, newItem);

                const slideDot = document.querySelector(`#slider_4B78705E .swiper-pagination-bullets a[data-slider-index="${(item.index + 1)}"]`);
                slideDot ? slideDot.href = item.url : null
            }
        }

       

        window.addEventListener('DOMContentLoaded', function (event) {/*d_slider-headline*/
            var slider_4B78705E_swiper = new Swiper(container_4B78705E, parameters_4B78705E);
            slObj_4B78705E = slider_4B78705E_swiper;
        });/*d_slider*/


    </script>
