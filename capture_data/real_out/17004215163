                    dataLayer[0].ffoto = '';
                    dataLayer[0].ffotocount = '';
                    dataLayer[0].fvideo = '';
                    dataLayer[0].fvideocount = '';
                }
                else if (1 == "0") {




                    if (0 < 1) {
                        var imageCount = getElementCountByParameters("[data-inline-image='true']");

                        dataLayer[0].ffoto = '0';
                        dataLayer[0].ffotocount = '0';
                        if (imageCount > 0) {
                            dataLayer[0].ffoto = imageCount > 0 ? '1' : '0';
