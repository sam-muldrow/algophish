<script>
var timeout;
var currentline = 1;
var max_items = 3;
function changeClass(line) {
	var block = $("#BlockCalendar");
	block.find("[data-line]").removeClass('conf-active');
	block.find("[data-line="+line+"]").addClass('conf-active');
	block.find("#square-image").find("img").attr({"src": block.find("[data-line="+line+"]").data('picture')});
}
function activeLine() {
	changeClass(currentline);
	currentline++;
	if(currentline > max_items) {
		currentline = 1;
	}
}
$(document).ready(function(){
	var confnum = 3;
	if(confnum > 1) {
		activeLine();
	}
	timeout = setInterval(activeLine, 5000);
	$("[data-line]").on('click', function() {
		var url = $(this).data("url");
		window.open(url);
	});
	if(confnum > 1) {
		$("[data-line]").on('mouseenter', function() {
			var block = $("#BlockCalendar");
			block.find("[data-line]").removeClass('conf-active');
			$(this).addClass('conf-active');
		});
	}
});
</script>
</section>    						    					    				    					       						       													    
