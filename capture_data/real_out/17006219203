            function invokeSmartbanner() {
                $.smartbanner({
                    daysHidden: 1,
                    daysReminder: 1
                });
            }

            const ARTICLE = 0;
            $(function() {
                setTimeout(function(){
                    if(ARTICLE) {
                        var cmp_visible = $('#_ao-cmp-ui').length;

                        if(IS_DESKTOP == 1) {
                            if((cmp_visible == 1 && sticky_footer_visible == 0) || (cmp_visible == 0 && sticky_footer_visible == 1)) {
                                invokeNotificationHandler();
                            }
                            if(cmp_visible == 0 && sticky_footer_visible == 0) {
                                invokeNotificationHandler();
                                invokeArticleFBPopup();
                            }
                        } else {
                            if((cmp_visible == 1 && sticky_footer_visible == 0) || (cmp_visible == 0 && sticky_footer_visible == 1)) {
                                invokeNotificationHandler();
                            }
                            if(cmp_visible == 0 && sticky_footer_visible == 0) {
                                invokeNotificationHandler();
                                var notification = $('.notification-popup').length;
                                if(notification == 1) {
                                    invokeArticleFBPopup();
                                } else {
                                    invokeSmartbanner();
                                }
                            }
                        }
                    }
                }, 1000);
            });
        </script>
   	</body>
</html>
