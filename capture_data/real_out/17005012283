		function noSunday(date) {
			var day = date.getDay();
			return [ (day > 0), '' ];
		}
		;

		function parseDate(input) {
			var parts = input.split('-');
			return new Date(parts[2], parts[0], parts[1]); // months are 0-based
		}

		/* POP UP */
