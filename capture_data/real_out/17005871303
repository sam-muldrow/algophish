    <script>
        var domain = '.tianjihr.com';

        $(document).ready(function () {

            var token = $.cookie('access_token');
            var cilckurl = window.location.href;
            if (token != null && token != "") {
                $('#delivery_layout').hide();
            } else {
                //注册
                $('#delivery_layout').show();
                if (cilckurl.indexOf("zhaopin") > 0 && cilckurl.indexOf(".html") > 0) {
                    $('#delivery_layout').hide();
                    return false;
                }
                var isscroll = true;
                var $document = $(document).outerHeight();
                var $innerHeight = window.innerHeight;
                var actuai = $document - ($(this).scrollTop() + $innerHeight);
                if (actuai < 101) {
                    $('#delivery_layout').removeClass('active');
                } else {
                    $('#delivery_layout').addClass('active');
                }
                $(document).scroll(function () {
                    if (isscroll == true) {
                        actuai = $document - ($(this).scrollTop() + $innerHeight);
                        if (actuai < 101) {
                            $('#delivery_layout').removeClass('active');
                            $(".close-fixed").hide();
                            $(".tianjihr-footer").css({ "margin-top": "0px" });
                        } else {
                            $('#delivery_layout').addClass('active');
                            $(".close-fixed").show();
                        }
                    }

                })
                //$(document).bind("click", function (e) {
                //    var target = $(e.target);
                //    if (target.closest("#menu").length == 0) {
                //        $("#menu ul").hide();
                //    } else {
                //        $("#menu ul").toggle();
                //    }
                //    if (target.closest("#searchtype").length == 0) {
                //        $("#searchtype ul").hide();
                //    } else {
                //        $("#searchtype ul").toggle();
                //    }
                //});

                $('.close-fixed').click(function () {

                    $('#delivery_layout').removeClass('active');
                    $(".close-fixed").hide();
                    isscroll = false;
                    $(".tianjihr-footer").css({ "margin-top": "0px" });
                });
            }




            //判断用户登陆状态
            var resumeid = $.cookie('resumeid');
            var talent_site_url = 'https://talent.tianjihr.com/#/app/resume/details/' + resumeid;
            var is_talent = $.cookie('is_talent');
            if (is_talent == "true") {
                $("#user_resume").attr("href", talent_site_url);
            }
            var has_employer = $.cookie('has_employer');
            if (has_employer == "True") {
                $("#ep_zhaopin").attr("href", "https://employer.tianjihr.com/#/app/position/publish/");
                $("#ep_zhaopin_manage").attr("href", "https://employer.tianjihr.com/#/app/dashboard/index");
            }

            $("#txt_keyword").focus(function () {
                $("#txt_keyword").attr("placeholder", "");
            });
            $("#txt_keyword").focusout(function () {
                if ($("#txt_keyword").val() == "") {
                    $("#txt_keyword").attr("placeholder", "请输入职位或公司名称");
                }

            });
            function search_type(_city, text) {
                $("#searchtype ul").hide();
                $("#searchtype h2").html(text + "<span></span>");
                $("#type").val(_city);
                event.stopPropagation();
            };
            $("#searchtype h2").click(function () {
                $("#searchtype ul").toggle();
            });


            toTop('toTop', false);
            $(".tianjihr-sidebar .qq").click(function () {
                window.open('http://b.qq.com/webc.htm?new=0&sid=776588238&o=dfd&q=7', '_blank', 'height=502, width=644,toolbar=no,scrollbars=no,menubar=no,status=no');
            });
            $(".personal").click(function () {
                clearCookie(domain);
                clearCookie("");
            });
            $(".enterprise").click(function () {
                clearCookie(domain);
                clearCookie("");
            });

        });


        var clearCookie = function (_domain) {
            var keys = document.cookie.match(/[^ =;]+(?=\=)/g);
            if (keys) {
                for (var i = keys.length; i--;)
                    document.cookie = keys[i] + '=0;path=/;expires=' + new Date(0).toUTCString() + ';domain=' + _domain;
            }
        };
        function logout() {
            var searchkeyword = $.cookie('searchkeywords');
            clearCookie(domain);
            clearCookie("");
            window.localStorage.clear();

            $.cookie('searchkeywords', searchkeyword, { expires: 7, path: '/' });
            window.location.reload();
        }
    </script>
    <script>
        (function () {
            var bp = document.createElement('script');
            var curProtocol = window.location.protocol.split(':')[0];
            if (curProtocol === 'https') {
                bp.src = 'https://zz.bdstatic.com/linksubmit/push.js';
            }
            else {
                bp.src = 'http://push.zhanzhang.baidu.com/push.js';
            }
            var s = document.getElementsByTagName("script")[0];
            s.parentNode.insertBefore(bp, s);
        })();
    </script>

    <script>
