			function startAdsense() {
				setTimeout(() => {
					mmLoader({
						"data-ad-client": "ca-pub-7835681492882497",
						src: "https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js",
						async: true
					}, "AdsenseChain").then(
						element => {
							let injectedAd = document.querySelectorAll('.ad-st-injected');
							if (device == 'desktop' && injectedAd.length>0) {
								injectedAd.forEach((el)=>{
									el.remove();
								});
							}
							let blankAdSlots = document.querySelectorAll(".adsbygoogle:not([data-adsbygoogle-status='done'])");
							if(blankAdSlots.length > 0){
								for (var i = 0; i < blankAdSlots.length; i++) {
									(adsbygoogle = window.adsbygoogle || []).push({});
								}
							}
						}
				    );
				}, 3000);
			}
			postTcfReady(startAdsense);


			let YoutubeVideos = document.querySelectorAll(".containervideo.youtube");
			if(YoutubeVideos.length > 0){
				// YouTube Player API
				mmLoader({
					src: "https://www.youtube.com/iframe_api",
					async: true
				});
				var player;	

				// YouTube video
				let videoWrappersInfo = [];
					let videoWrappers = document.querySelectorAll(".videowrapper");
					videoWrappers.forEach(function(videoContainer) {
						videoWrappersInfo.push({
							id: videoContainer.id
						});
					});

					//console.log('ARRAY: ' + JSON.stringify(videoWrappersInfo));

					function onYouTubeIframeAPIReady() {
					// Script for more than one video on the page
					videoWrappersInfo.forEach(function(videoInfo, index) {
						let videoid = videoInfo.id.slice(16);
						let videoAutoplay = 0;
						let videoMute = 0;
						if(index == 0) {
							videoAutoplay = 1;
							videoMute = 1;
						}

						let player = new YT.Player(videoInfo.id, {
							width: '640',
							height: '360',
							videoId: videoid,
							embedConfig: {
								'adsConfig': {
								'adTagParameters': {
								'iu': '/4758/studenti/videopfp',
								'pp': null
								},
								'nonPersonalizedAd': false
								},
							},
							playerVars: {
								mute: videoMute,
								autoplay: videoAutoplay
							}
						});
					});
				}
			}
			
			// Web component mh2021
			mmLoader({
            	src: "https://dafne.sirio.stbm.it/utility-fe/prod/web-components/ame-mh.min.js",
          		defer: true
       		});
