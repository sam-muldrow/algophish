// PERMUTIVE CONSENT CONFIG
var permutiveEnabled = false;
// GET CONSENT TOKEN (euconsent-v2)
let token = (document.cookie.match(/(?:iub)?euconsent-v2=([^;]*)/) || [])[1];

function setupPermutive() {
    __tcfapi('getTCData', 2, (tcData, success) => {
        if (success && tcData) {
            if(tcData.purpose.consents["1"] && tcData.purpose.consents["3"] && tcData.purpose.consents["5"] && tcData.purpose.consents["7"] && tcData.purpose.consents["9"] && tcData.vendor.consents["361"]) {
                permutiveEnabled = true;
                window._exaudiMediaset = window._exaudiMediaset || [];
                window._exaudiMediaset.push(["S",["_cm","PERMUTIVE"]]);
            }

            if(!token) {
                token = tcData.tcString;
            }
        }	
    });
}

postTcfReady(setupPermutive);	

let adMantxLoaded = false;

function startAdvChain2022() {
    let startTime = performance.now();

    const startAdmantx = function () {
        /* ADMANTX CONF */
        let admantxOptions = `{"tag": "studenti.it","key": "a986fe5c0072f82c7c51012fe8aba4e77a80a19e0ff5f90985b3b0f8d86e5ecf","method": "descriptor","filter": "default","decorator": "template.studenti","type": "URL","mode": "async","body": "${escape(document.location.href)}"}`;
        let decodedAdmantx = 'https://euasync01.admantx.com/admantx/service?request='+admantxOptions;
        let urlAdmantx = encodeURI(decodedAdmantx);

        return mmLoader({
            src: urlAdmantx,
            async: true
        },"advChain");
    }

    const startNeodata = function () {
		/* NEODATA CONF */
		let urlNeodata = `https://c.neodatagroup.com/mediaset.js`;
		
        if(permutiveEnabled) {
            return mmLoader({
                src: urlNeodata,
                async: true
            },"advChain");
        }

        return false;
    }

    const promises = [startAdmantx(), startNeodata()];

    Promise.allSettled(promises).
    then((results) => {
        console.log('[startAdvChain2022] admantx elaborato', performance.now() - startTime);
        console.log('[startAdvChain2022]',results)

        // SEND adMantxReady EVENT
        adMantxLoaded = true;
        var adMantxReady = new Event("adMantxReady");
        window.dispatchEvent(adMantxReady);

        mmLoader({
            src: "//adv.mediamond.it/hpsponsor/studenti/header_cls.v4.js",
            async: true
        },"advChain")
        .then(()=> window.dispatchEvent(new Event("introCanStart")));

    }).then(e => {
        console.log('[startAdvChain2022] mm caricato', performance.now() - startTime);
        const advChainEvent = new Event('advChainLoaded');
        window.dispatchEvent(advChainEvent);
    })
}

postTcfReady(startAdvChain2022);

function startPermutive() {
    if(permutiveEnabled) {
        // SETUP CONSENT ON PERMUTIVE 
        permutive.consent({ "opt_in": true, "token": token });
        console.log(`[permutive] setup consent with token`, token);

        // SETUP PERMUTIVE USER IDENTITIES
        let identities = [
            {"id": getGaClientId(), "tag": "ame_publisher_id", "priority": 1} // AME PUBLISHER ID
        ];
        //console.log('perm id ', getGaClientId() )
        permutive.identify(identities);
        console.log(`[permutive] setup user identities`, identities);
        // ADDON PERMUTIVE
        adMantxLoaded ? addonPermutive() : window.addEventListener('adMantxReady', () => addonPermutive());
    }
}

function postPermutiveIdGeneratorReady() {
  if(typeof permutiveIdGeneratorReady !== 'undefined') {
      startPermutive();
  } else {
      window.addEventListener('permutiveIdGeneratorReady', function () {
          startPermutive();
      });
  } 
}
postTcfReady(postPermutiveIdGeneratorReady);


function startOutbrain() {
  let blankOBSlots = document.querySelectorAll(".OUTBRAIN[data-ob-template]");
  if(blankOBSlots.length > 0) {
    setTimeout(() => {
      mmLoader({
        src: "//widgets.outbrain.com/outbrain.js",
        async: true
      },"OutbrainChain");
    }, 3000);
  }
}
postTcfReady(startOutbrain);


