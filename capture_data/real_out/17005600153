<script>

	var tabmenu = $('.jsmenu a');

    tabmenu.on(        
		'click',
		function(e) {
          e.preventDefault();
          $(this).closest('menu.tabmenu').find('.jsmenu.active').removeClass('active');            
          $(this).parent().addClass('active');   
          $(this).closest('.box-with-filter').find('.box-listato-list').removeClass('jsactive');
          $(this).closest('.box-with-filter').find('.top-searches-list').removeClass('jsactive');
          $(this).closest('.box-with-filter').find('.notizie-tab').removeClass('jsactive');
                
          var fil = $(this).data('filtrotab');       
          
          if ($(this).hasClass('filter-' + fil))
            $(this).closest('.box-with-filter').find('.list-' + fil).addClass('jsactive');
         
		}
	);

</script>

		

<script>
  function initCarousels() {
      var newsSwiper = new Swiper('.news-swiper-standard', {
          loop: true,
          autoplay: {
            delay: 6000,
          },
          slidesPerView: "auto",
          spaceBetween: 15,

          // Pagination
          pagination: {
            el: '.swiper-pagination',
            clickable: true
          },
          // Navigation arrows
          navigation: {
            nextEl: '.swiper-button-next',
            prevEl: '.swiper-button-prev',
          }
      });

      var newsSwiperHp = new Swiper('.news-swiper-hp', {
          loop: true,
          autoplay: {
            delay: 6000,
          },
          slidesPerView: "auto",
          spaceBetween: 15,

          // Pagination
          pagination: {
            el: '.swiper-pagination',
            clickable: true
          },
          // Navigation arrows
          navigation: {
            nextEl: '.swiper-button-next',
            prevEl: '.swiper-button-prev',
          }
      });
       
      /*const firstSlideHp = document.querySelectorAll('.news-swiper-hp .swiper-slide')[0];
      if(firstSlideHp) {
        const carouselBtn = document.querySelector('.news-swiper-hp .btn');
        let btnLink = firstSlideHp.dataset.link;
        carouselBtn.href = btnLink;

        newsSwiperHp.on('slideChange', function () {
          btnLink = document.querySelector('.swiper-slide-active').dataset.link;
          console.log('SLIDE ACTIVE LINK', btnLink)
          carouselBtn.href = btnLink;
        });
      }*/
  }

  mmLoader({
      src: "/js/swiper-9.3.2.min.js",
      async: true
  }).then(
      mmLoader({
        rel: 'stylesheet',
        href: "/css/swiper-9.3.2.min.css"
      }, "",'link')
  ).then(initCarousels);
</script>


        <script>
      var isLeaf = false;
    var JSsessionID = new Date().getTime() + '.' + Math.random().toString(36).substring(5);
  var JSNomespeciale = document.querySelector('.testata-speciale .special-name, .tpl-dafne h1');
  if (JSNomespeciale !== null) {
    JSNomespeciale = JSNomespeciale.textContent.replace(/\s+/g, " ").trim();
  }
  
  // event "Articolo Correlato Click"
  var JSArticoliCorrelati = document.querySelectorAll('.box-correlati article');
  var JSArticoliCorrelatiLink = document.querySelectorAll('.box-correlati article a');
  var JSNrArticoliCorrelati = JSArticoliCorrelati.length;
  var JSHaveCorrelati = JSNrArticoliCorrelati > 0 ? 1 : 0;
  var trackCorrelati = '';

  if (JSHaveCorrelati !== 0) {
    Array.from(JSArticoliCorrelatiLink).forEach( function(el,i) {
      el.addEventListener('click', function(event) {
        let clickTitle = el.closest('article').querySelector('h3 a').textContent;
        let clickPos = el.closest('article').dataset.pos;
        gtag('event', 'click', {
          'event_category': 'Correlati',
          'event_label': 'pos: '+ clickPos +', link: '+ el.href +', referral: '+ document.location.href,
          'non_interaction': false
        });
      });
    });
  } else {
    trackCorrelati = 'No correlati';
  }

  // event "Bottone Vota"
  var JSVoteBtn = document.querySelectorAll('.rating_item');
  var JSHaveVoteBtn = JSVoteBtn.length > 0 ? 1 : 0;

  if (JSHaveVoteBtn !== 0) {
    Array.from(JSVoteBtn).forEach( function(el,i) {
        el.addEventListener('click', function(event) {
          var JSValoreVoto = JSVoteBtn.dataset.index;
          gtag('event', 'Voto', {
            'event_category': 'Rating',
            'event_label': document.location.href,
            'value': JSValoreVoto,
            'non_interaction': false
          });
      });
    });
  }
  
  // pageview per "sfoglio gallery"
  var JSGalleryLink = document.querySelectorAll('.gallery-launch');
  var JSHaveGallery = JSGalleryLink.length > 0 ? 1 : 0;

  if (JSHaveGallery !== 0) {
    Array.from(JSGalleryLink).forEach( function(el,i) {
      el.addEventListener('click', function(event) {
        var galleryId = el.dataset.gallery;

        if(typeof galleryId != "undefined") {
          var galleryUrl = location.pathname + location.search;
          galleryUrl = galleryUrl.replace(/gallery=[^&]+/gi, "");
          if(galleryUrl.indexOf("?") > -1) galleryUrl += "gallery=" + galleryId;
          else galleryUrl += "?gallery=" +galleryId;   

          gtag('event', 'page_view', {
            page_title: document.title,
            page_location: JSGalleryLink.href,
            page_path: galleryUrl
          });
        }

      });
    });
  }
  
  /* tracciamento click infocommerce */
  const infocommerceLink = document.querySelectorAll(".modInfoCommerce__item a");
  if (infocommerceLink.length > 0) {
      Array.from(infocommerceLink).forEach(function(el){
          el.addEventListener('click', function(e) {
              gtag('event', 'widget - ' + el.title + ' - ' + el.dataset.varsBrand, {
                  'event_category': 'Affiliazione Contenuto',
                  'event_label': el.dataset.varsMerchant
              });
          });
      });
  }


  /* tracciamento box-seo */
  window.addEventListener('load', function() {
    const SEOlinks = document.querySelectorAll(".box-seo a");
    if (SEOlinks.length > 0) {
        // console.debug(SEOlinks);
        Array.from(SEOlinks).forEach(function(el){
            el.addEventListener('click', function(event) {
              // event.preventDefault(); // DEBUG ONLY!!!!111!!1
                gtag('event', 'Click box SEO', {
                    'event_category': 'User Interaction',
                    'event_label': el.title
                });
            });
        });
    }
  });


  /* tracciamento indice in contenuti pro */
  window.addEventListener('load', function() {
    const INDICElinks = document.querySelectorAll(".toolheaderIndiceSource li a, .indiceHeader li a");
    if (INDICElinks.length > 0) {
        // console.debug(INDICElinks);
        Array.from(INDICElinks).forEach(function(el){
            el.addEventListener('click', function(event) {
              // event.preventDefault(); // DEBUG ONLY!!!!111!!1
              // console.debug('click su '+el);
                gtag('event', 'Click su indice', {
                    'event_category': 'User Interaction',
                    'event_label': 'Click paragrafo'
                });
            });
        });
    }
  
    const INDICEtoggle = document.querySelector("#toolHeader .indiceButton");
    if (INDICEtoggle !== null) {

      INDICEtoggle.addEventListener('click', function(event) {
        INDICEtoggle.action = INDICEtoggle.classList.contains('open') ? 'Apertura' : 'Chiusura';
        // console.debug(INDICEtoggle.classList);
        // event.preventDefault(); // DEBUG ONLY!!!!111!!1
          gtag('event', 'Click su indice', {
              'event_category': 'User Interaction',
              'event_label': INDICEtoggle.action
          });
      });

    }
  });



  /* tracciamento sharing */
  const share_links = document.querySelectorAll(".azioni-tools a, .social-tools a");
  if (share_links.length > 0) {
      Array.from(share_links).forEach(function(el){
          // si, ma quale?
          el.shareaction = el.classList.contains('print') ? 'Stampa' : 'Sharing'
          el.shareplatform = el.classList.contains('fb') ? 'Facebook' : el.classList.contains('tw') ? 'Twitter' : el.classList.contains('wa') ? 'Whatsapp' : 'Stampa';
          el.shareplatform = el.parentNode.parentNode.classList.contains('azioni-tools') ? el.shareplatform+' top' : el.shareplatform+' bottom';
          // console.debug(el.shareaction);

          el.addEventListener('click', function(event) {
            // event.preventDefault(); // DEBUG ONLY!!!!111!!1
              gtag('event', 'Click su '+el.shareaction, {
                  'event_category': 'User Interaction',
                  'event_label': el.shareplatform
              });
          });
      });
  }


  /* tracciamento utilizzo icona materie */
  MatBtn = {};
  MatBtn.header = document.getElementById("materie_handle");
  MatBtn.home = document.getElementById("materie_handle_big");
  MatBtn.close = document.querySelector("#materie-overlay .close-overlay");
  MatBtn.items = document.querySelectorAll("#materie-overlay a.materie_listing_item");
  if (MatBtn.header) {
    MatBtn.header.addEventListener('click', function(event) {
      // event.preventDefault(); // DEBUG ONLY!!!!111!!1
        gtag('event', 'Click su icona materie', {
            'event_category': 'User Interaction',
            'event_label': 'Materie - Click su icona in header'
        });
    });
  }
  if (MatBtn.home) {
    MatBtn.home.addEventListener('click', function(event) {
      // event.preventDefault(); // DEBUG ONLY!!!!111!!1
        gtag('event', 'Click su icona materie', {
            'event_category': 'User Interaction',
            'event_label': 'Materie - Click su icona in hp'
        });
    });
  }
  if (MatBtn.close) {
    MatBtn.close.addEventListener('click', function(event) {
      // event.preventDefault(); // DEBUG ONLY!!!!111!!1
        gtag('event', 'Click su icona materie', {
            'event_category': 'User Interaction',
            'event_label': 'Materie - Click su x'
        });
    });
  }
  if (MatBtn.items) {
    Array.from(MatBtn.items).forEach(function(el){
      let matName = el.innerText.toLowerCase();
      //console.debug(matName);
      el.addEventListener('click', function(event) {
        // event.preventDefault(); // DEBUG ONLY!!!!111!!1
          gtag('event', 'Click su icona materie', {
              'event_category': 'User Interaction',
              'event_label': 'Materie - Click su '+matName
          });
      });
    });
  }


</script>

