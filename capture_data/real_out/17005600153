<script>
    /* Setta una variabile per inibire la pushdown su categorie ed HP*/
    let showPushDown = true;
        showPushDown = false;
    
    // Escludo Matematica (che non ha i div flyfirst-mobile-placement e flyfirst-placement) e le pagine con video
    let iframeVideo = document.querySelector('.show-video, .box-video-speciale');
    if( showPushDown && (window.location.href.indexOf('/matematica/') != -1 || iframeVideo != null)) {
        showPushDown = false;
    }

    //Il parametro "website" è differente per doc e desk, mob. android, mob. ios
    let website = 'stpv';
    if (window.location.href.indexOf('doc.studenti') != -1) {
        website = "st-doc";
    } else {
        if (/(iPad|iPhone|iPod)/g.test(navigator.userAgent)) {
            website = "stpm-ios";
        } else if (navigator.userAgent.toLowerCase().indexOf("android") !== -1){
            website = "stpm-android";
        }
    }
    let isMobile =  window.innerWidth < 1000; // la var si chiama isMobile per evitare conflitti con la var mobile nelle pagine con video in pagina
    //Il parametro "targetElementId" indica all'interno di quale ID erogare la push e/o dove riposizionarla (desk/mob) in caso di chiusura
    let targetElementId = 'flyfirst-mobile-placement';
    if(!isMobile) targetElementId = 'flyfirst-placement';
</script>



