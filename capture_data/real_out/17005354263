</style><link data-rh="true" href="https://static.westwingnow.de/bella/ci_apple-touch-icon.png" rel="apple-touch-icon" sizes="180x180"/><link data-rh="true" href="https://static.westwingnow.de/bella/ci_favicon-32x32.png" rel="icon" type="image/png" sizes="32x32"/><link data-rh="true" href="https://static.westwingnow.de/bella/ci_favicon-16x16.png" rel="icon" type="image/png" sizes="16x16"/><link data-rh="true" href="https://static.westwingnow.de/bella/ci_safari-pinned-tab.svg" rel="mask-icon" color="#8EC5B7"/><link data-rh="true" rel="manifest" href="/manifest.json"/><link data-rh="true" rel="canonical" href="https://www.westwing.it/"/><link data-rh="true" rel="alternate" hrefLang="de" href="https://www.westwing.de/"/><link data-rh="true" rel="alternate" hrefLang="de-DE" href="https://www.westwing.de/"/><link data-rh="true" rel="alternate" hrefLang="de-CH" href="https://www.westwing.ch/"/><link data-rh="true" rel="alternate" hrefLang="it" href="https://www.westwing.it/"/><link data-rh="true" rel="alternate" hrefLang="nl" href="https://www.westwing.nl/"/><link data-rh="true" rel="alternate" hrefLang="pl" href="https://www.westwing.pl/"/><link data-rh="true" rel="alternate" hrefLang="es" href="https://www.westwing.es/"/><link data-rh="true" rel="alternate" hrefLang="fr" href="https://www.westwing.fr/"/><link data-rh="true" rel="alternate" hrefLang="cs" href="https://www.westwing.cz/"/><link data-rh="true" rel="alternate" hrefLang="sk" href="https://www.westwing.sk/"/><script type="text/javascript">(function WestwingEventHandler() {
  var queue = [];
  var subscribers = {};
  var publish = function publish(eventId, data) {
    queue.push({
      eventId: eventId,
      data: data
    });
    if (!subscribers[eventId]) {
      return;
    }
    subscribers[eventId].forEach(function (subscriberCallback) {
      return subscriberCallback(data);
    });
  };
  var subscribe = function subscribe(eventId, callback) {
    if (!subscribers[eventId]) {
      subscribers[eventId] = [];
    }
    var index = subscribers[eventId].push(callback) - 1;
    var queuedEvent = queue.find(function (event) {
      return event.eventId === eventId;
    });
    if (queuedEvent) {
      callback(queuedEvent.data);
    }
    return {
      unsubscribe: function unsubscribe() {
        subscribers[eventId].splice(index, 1);
      }
    };
  };
  var events = {
    queue: queue,
    subscribers: subscribers,
    subscribe: subscribe,
    publish: publish
  };
  var initWindow = function initWindow() {
    window.ww.events = events;
  };
  return {
    events: events,
    initWindow: initWindow
  };
})().initWindow()</script><script type="application/ld+json">[{"@context":"http://schema.org","@type":"Corporation","url":"https://www.westwing.it/","legalName":"Westwing GmbH","contactPoint":[{"@type":"ContactPoint","telephone":"+39 (02) 30568095","email":"E-Mail","contactType":"customer service","hoursAvailable":[{"@type":"OpeningHoursSpecification","dayOfWeek":["Monday","Tuesday","Wednesday","Thursday","Friday"],"opens":"08:00","closes":"18:00"}]},{"@type":"ContactPoint","telephone":"+39 (02) 30568095","email":"E-Mail","contactType":"customer service","hoursAvailable":[{"@type":"OpeningHoursSpecification","dayOfWeek":["Saturday"]}]}],"address":{"@type":"PostalAddress","addressCountry":"IT","addressLocality":"Monaco","addressRegion":"Region","postalCode":"80809","streetAddress":"Moosacher Straße 88"},"sameAs":["","https://www.pinterest.de/westwingitalia/_saved/","https://www.instagram.com/westwing.it/"]},{"@context":"http://schema.org","@type":"WebSite","url":"https://www.westwing.it/","potentialAction":{"@type":"SearchAction","target":"https://www.westwing.it/all-products/?q={search_term_string}","query-input":"required name=search_term_string"}}]</script></head><body class="bella"><img style="width: 0; height: 0; margin: 0; padding: 0; display: block;" alt="" src="https://www.westwing.it/c/api/cookie/register/?key=ww_lvp&value=shop&samesite=None&responsetype=image&expiretime=1732147200" /><div id="app-root"><div class="pageContainer"><div><div data-testid="header-bar-banner-v2" class="BarBannerV2__StyledWrapper-appshell__sc-1lnoknc-0 dLwbYF qa-header-promotion-banner"><a class="HybridLink__StyledLink-appshell__sc-w39pjn-1 RKncF BarBannerV2__BannerLink-appshell__sc-1lnoknc-3 cPvaEB" href="/n/deals/"><div><span data-testid="header-banner-text-mobile" class="BarBannerV2__MobileText-appshell__sc-1lnoknc-1 lfawTo">LA PRE-SALE INIZIA ORA! APPROFITTA DEGLI SCONTI FINO AL -50% SULLE NOSTRE LAMPADE</span><span data-testid="header-banner-text-desktop" class="BarBannerV2__