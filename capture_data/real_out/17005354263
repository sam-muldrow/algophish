                    function ClientLogger (loggerConfig) {
                        this.loggerConfig = loggerConfig;

                        this.log = function(logType, entry, customData) {
                            var message = entry instanceof Error ? entry.message : entry;

                            if (logType === 'debug') {
                                console.info(message, customData);
                                return;
                            }

                            console[logType](message, customData);

                            var data = {
                                clientTimestamp: new Date().toISOString(),
                                buildVersion: this.loggerConfig.buildVersion
                            };

                            if (customData && typeof customData === 'object') {
                                for (var key in customData) {
                                    data[key] = customData[key]
                                }
                            }

                            if (customData instanceof Error) {
                                data.error = {
                                    message: customData.message,
                                    stack: customData.stack,
                                    name: customData.name
                                };
                            }

                            if (entry instanceof Error) {
                                data.error = { message: entry.message, stack: entry.stack, name: entry.name };
                            }

                            if(window.fetch){
                                window.fetch('/bella-api/log', {
                                    method: 'POST',
                                    headers: {
                                        'content-type': 'application/json',
                                    },
                                    body: JSON.stringify({ logType: logType, message: message, customData: data }),
                                });
                            }
                        };

                        this.debug = function(message, customData){
                            this.log('debug', message, customData);
                        };

                        this.info = function(message, customData){
                            this.log('info', message, customData);
                        };

                        this.warn = function(message, customData){
                            this.log('warn', message, customData);
                        };

                        this.error = function(error, customData){
                            if(error){
                                if(error.isRedirect || error === 'A Redirect Occurred'){
                                    this.info(error, customData)
                                    return;
                                }
                            }
                            this.log('error', error, customData);
                        };

                        var allowedErrors = [
                            'Script error.',
                            'ResizeObserver loop limit exceeded',
                            'ResizeObserver loop completed with undelivered notifications.'
                        ]

                        this.registerGlobalErrorHandler = function() {
                            var state = this;
                            window.onerror = function (message, source, fileno, column, error) {
                                if(true) {
                                    return;
                                }
                                var stack = '';
                                if(error && error.stack){
                                    stack = error.stack;
                                }
                                var customData = { source: source, fileno: fileno, column: column, stack: stack, nonBellaErrors: true };
                                var stringMessage = message.toString();
                                if (!allowedErrors.includes(stringMessage)) {
                                    state.error(stringMessage, customData);
                                }
                            };
                        };
                    }

                    if(!window.clientLogger){
                        var clientLogger = new ClientLogger(loggerConfig);
                        clientLogger.registerGlobalErrorHandler();
                        window.clientLogger = clientLogger;
