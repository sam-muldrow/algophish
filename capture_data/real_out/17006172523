        <script type="text/javascript">
            let jinjaVars = {
                lang: "en",
                customUrlLangs: ["de"]            };
        </script>

        <script type="text/javascript">
            const httpGet = (url, callback, failCallback) => {
                const request = new XMLHttpRequest();
                request.onreadystatechange = () => {
                    if (request.readyState === 4 && request.status === 200) callback(request);
                    if (request.readyState === 4 && request.status !== 200 && typeof failCallback !== "undefined") {
                        failCallback(request);
                    }
                };
                request.open("GET", url, true);
                request.send(null);
            };

            const addScriptTag = (src, async = false, defer = false) => {
                var scriptTag = document.createElement("script");
                scriptTag.defer = defer;
                scriptTag.async = async;
                scriptTag.src = src;
                document.body.appendChild(scriptTag);
            }

            const addScriptOnLoad = (src, async = false, defer = false) => {
                if (document.readyState !== "loading") {
                    addScriptTag(src, async, defer);
                } else {
                    document.addEventListener("DOMContentLoaded", () => {
                        addScriptTag(src, async, defer);
                    });
                }
            }

            function TimeRetrievalError (message, statusCode, errorBody) {
                this.message = message;
                this.statusCode = statusCode;
                this.errorBody = errorBody;
                this.type = "TimeRetrievalError";
            }

            let timeRetrievalTimeout;

            const retrieveServerTime = callback => {
                clearTimeout(timeRetrievalTimeout);
                const hostname = document.location.hostname;

                if (hostname.indexOf("24timezones.com") === -1 && hostname.indexOf("24tz.live") === -1) {
                    console.info("Redirecting to 24 Timezones...");
                    document.location.replace("https://24timezones.com" + "/");
                }

                const retry = () => {
                    console.warn("Failed to retrieve time, retrying in 2 seconds...");
                    timeRetrievalTimeout = setTimeout(() => retrieveServerTime(callback), 2000);
                }

                const validateServerTime = response => {
                    try {
                        const serverTime = JSON.parse(response).server_time;
                        if (typeof serverTime === "number") {
                            console.info("Server time retrieved. Loading clock...");
                            callback(serverTime);
                        } else throw new TimeRetrievalError("Invalid server time!", null, response);
                    } catch (e) {
                        retry();
                        if (e.type === "TimeRetrievalError") throw e;
                        else throw new TimeRetrievalError("Failed to retrieve time.", null, e);
                    }
                }

                const successCallback = (response) => validateServerTime(response.responseText);
                const failCallback = (response) => {
                    retry();
                    throw new TimeRetrievalError("Server did not return time.", response.status, null);
                }
                const url = window.location.protocol + "//" + window.location.host + "/t";
                httpGet(url, successCallback, failCallback);
            }

            const addLeadingZeros = number => {
                return number < 10 ? `0${number}` : number;
            }

            const getTime = (dateObject, is24Hr = false, utcHours = true) => {
                const is24format = is24Hr ? true : getTimeFormat() === 24;
                const hoursToUse = utcHours ? dateObject.getUTCHours() : dateObject.getHours();
                const minutes = utcHours ? dateObject.getUTCMinutes() : dateObject.getMinutes();

                if (is24format) return `${addLeadingZeros(hoursToUse)}:${addLeadingZeros(minutes)}`;
                else {
                    const hours = hoursToUse > 12 ? hoursToUse - 12 : hoursToUse;
                    return `${hours}:${addLeadingZeros(minutes)}`;
                }
            }

            const getMeridiem = dateObject => {
                if (getTimeFormat() === 12) return dateObject.getUTCHours() >= 12 ? "PM" : "AM";
                else return "";
            };
        </script>

