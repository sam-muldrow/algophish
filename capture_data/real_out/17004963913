        (function($){
            // component type is: 
            // set up Event listener for height
            function handler(payload){
                var data = payload.data || null,
                    info = data.data || null;
                if (data && data.type === '' && info && info.event === 'resize') {
                     $('. > iframe:first').css('height', info.data);
                 }
            }

            try {
                window.addEventListener("message", handler, false);
            } catch (error) {
                window.attachEvent("onmessage", handler);
            }
        })(window.jQuery);
        </script>
