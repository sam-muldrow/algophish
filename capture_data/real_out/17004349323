<!-- 20190527 레이어팝업 추가 스크립트 // -->
<script type="text/javascript">
        function setCookie( name, value, expiredays ) {
            var todayDate = new Date();
            todayDate.setDate( todayDate.getDate() + expiredays ); 
            document.cookie = name + "=" + escape( value ) + "; path=/; expires=" + todayDate.toGMTString() + ";"
        }
		//레이어 팝업 닫기
		function closeLayer(IdName){
			$("#"+IdName+"").fadeOut();
		}		
		
		
		//레이어 팝업 닫기 - 1일 (광군제 임시)
		function closeLayerToday3(IdName){

			setCookie( "singlesDay", "done" , 1 );

        	$(".singlesDay").fadeOut();
		}

		//레이어 팝업 닫기 - 1일 (블랙프라이데이 임시)
		function closeLayerToday4(IdName){

			setCookie( "blackFriDay", "done" , 1 );

        	$(".blackFriDay").fadeOut();
		}
		
		//레이어 팝업 닫기 - 1일
		function closeLayerToday2(IdName){
			if($('#no_show_today').is(':checked')==true){
				setCookie( "amazonDelivery", "done" , 900 );
			}
		
        	$("#Misdelivery_popup").fadeOut();
		}
		
</script>
<!-- // 20190527 레이어팝업 추가 스크립트 -->

<script>

// 현황판용 
function board_show(loc) {
	if(loc == "DE") {
		$("#CA_board").hide();
		$("#infoDE").addClass("on");
		$("#infoCA").removeClass("on");
	}
	if(loc == "CA") {
		$("#DE_board").hide();
		$("#infoDE").removeClass("on");
		$("#infoCA").addClass("on");
	}
	
	$("#"+loc+"_board").show();
}
</script>
<script>
// 오샵 상품 리스트
$(document).ready(function(){
	$('.shopList ul li.us_Clearance').click(function(){
		$(this).addClass('active');
		$('.shopList ul li.us').removeClass('active');
		$('.shopList div.us').removeClass('on');
		$('.shopList ul li.costco').removeClass('active');
		$('.shopList div.costco').removeClass('on');
		$('.shopList div.us_Clearance').addClass('on');
	})
	$('.shopList ul li.us').click(function(){
		$(this).addClass('active');
		$('.shopList ul li.us_Clearance').removeClass('active');
		$('.shopList div.us_Clearance').removeClass('on');
		$('.shopList ul li.costco').removeClass('active');
		$('.shopList div.costco').removeClass('on');
		$('.shopList div.us').addClass('on');
	})
	$('.shopList ul li.costco').click(function(){
		$(this).addClass('active');
		$('.shopList ul li.us').removeClass('active');
		$('.shopList div.us').removeClass('on');
		$('.shopList ul li.us_Clearance').removeClass('active');
		$('.shopList div.us_Clearance').removeClass('on');
		$('.shopList div.costco').addClass('on');
	})
});

//배송절차
$(document).ready(function(){
	$('.cProcedure p.Shipping').click(function(){
		$(this).addClass('active');
		$('.cProcedure p.Purchase').removeClass('active');
		$('.cProcedure ul.s_procedure').addClass('on');
		$('.cProcedure ul.p_procedure').removeClass('on');
	})
	$('.cProcedure p.Purchase').click(function(){
		$(this).addClass('active');
		$('.cProcedure p.Shipping').removeClass('active');
		$('.cProcedure ul.p_procedure').addClass('on');
		$('.cProcedure ul.s_procedure').removeClass('on');
	})
});

</script>

