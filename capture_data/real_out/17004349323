<script type='text/javascript'>
var x_styleLeft, y_styleTop, x_accept, y_accept;

$(document).ready(function(){ 
	$(".LPOP2").each(function(){
		
		popupId = $(this).attr('id');

		$('#'+popupId).css('cursor', 'move');
		$("#"+popupId).draggable();
		
		if($.cookie(popupId) == "hide" ) {
			$("#"+popupId).hide();
		}
	});

	// 팝업 안보이게
	$("#pop2, #pop4").hide();
	
	//$("#pop3").show();
});


function closePop(popId, days) {
	$("#"+popId).hide();
 	if(days>0) {
 		$.cookie(popId, 'hide', {expires : days*1});		
 	}
}
	
</script>

<!-- 20190527 팝업 내리면 여기부터 -->
  
<script>

cookiedata = document.cookie;
if ( cookiedata.indexOf("singlesDay=done") < 0 ){     
	$(".singlesDay").show();
} else {
	$(".singlesDay").hide();
}

if ( cookiedata.indexOf("blackFriDay=done") < 0 ){     
	$(".blackFriDay").show();
} else {
	$(".blackFriDay").hide();
}

// 광군제 오늘하루 열지않기 임시 
if ( cookiedata.indexOf("amazonDelivery=done") < 0 ){     
	$("#Misdelivery_popup").show();
} else {
	$("#Misdelivery_popup").hide();
}

