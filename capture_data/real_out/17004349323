<script>
$(".hBtn").hide();
var btnHTML = $('#shopBtn a span').html();
	btnHTML = btnHTML.replace('()', '');
$('#shopBtn a span').html(btnHTML);

if( $("#Btn").length > 0 ) {
	$("#Btn").show(); // 메뉴에 해당하는 신청버튼
} else if( $("#Btn").length > 0 ) {
	$("#Btn").show(); // 메뉴에 해당하는 신청버튼
} else if(""=="amazon" || ""=="assist") { // 아마존일경우,
	$("#shopBtn").show(); // 메뉴에 해당하는 신청버튼
} else {
	$("#appBtn").show(); //  디폴트 배송대행 버튼
}
</script>
