                "promotionSlotCategories": [{"displayText":"好店一試","code":"PersonalcarenhealthLanding-PromotionSlot1Section","visible":false},{"displayText":"女神保養秘訣","code":"PersonalcarenhealthLanding-PromotionSlot2Section","visible":false},{"displayText":"年紀大，機器不壞","code":"PersonalcarenhealthLanding-PromotionSlot4Section","visible":false},{"displayText":"HKTVmall小學堂","code":"PersonalcarenhealthLanding-PromotionSlot5Section","visible":false}],
                "zone": "personalcarenhealth",
                "gaCategory": "Personalcarenhealth"
            };
            if (typeof promotionSlotCategories != "undefined" && promotionSlotCategories.promotionSlotCategories.length > 0) {
                ReactDOM.render(React.createElement(UIPromotionSlotContainer, promotionSlotCategories), document.getElementById("promotion-slot"));
            } else {
                Logger.warn("No Promotion Slots found, skipping rendering");        
            }
        </script>

        <script>
            var impressionCheck = {};
            $(document).on("getLastAppenedScrollingText", function(e,eventTargetObj){
                storageIndex = eventTargetObj.children[0].className.split(' ')[1];
                if (hktvCommon.storageAvailable('localStorage')) {
                    localStorage.setItem('scrollingTextLastIndex', storageIndex);
                }
            })
            
            $(document).ready(function(){ 
                $("#navScrollingBarWrapper").on("click", '.scrollingContainer .scrollingText', function(e){
                    var liveDataTargetUrl = $(e.currentTarget).find('a').attr('href');
                    if (typeof liveDataTargetUrl != 'undefined' && liveDataTargetUrl){
                        var liveDataAnalyticsString = e.currentTarget.children[0].innerText;
                        var liveDataAnalyticsIndex = e.currentTarget.className.split(' ')[1].split('-')[1];
                        analytics.trackEventV2(analytics.getGaRevamp2020AnalyticsZone(), "live_data", liveDataAnalyticsString+" | mabs_false | "+liveDataTargetUrl);
                    }
                });
            });

            if (!hktvCommon.isSupportCssAnimation()){
                $("#navScrollingBarWrapper").hide();            
            }
        </script>
        
        <script>
    
    $(".rotating-image-slider").on('click', 'a', function(e) {
        var mabsRefId = $(this).attr('mabsref');
        var lPrefix = 'mabs_' + (mabsRefId ? mabsRefId: 'false');
        var bannerName = e.target.alt? e.target.alt:'';
        var url = $(this).attr('href');
        analytics.trackEvent(analytics.getGaRevamp2020AnalyticsZone(),
            'slider_a',
            bannerName+" | "+url+" | "+lPrefix);
        analytics.trackEcommercePromoClick(
            $(this).attr('href'),
            mabsRefId ? mabsRefId : $(this).find('img').eq(0).attr('alt'),
            analytics.getGaRevamp2020AnalyticsZone() + '_slider_a',
            $(this).data('position')
        );
    });

    $(".image-nav-container").on('click', 'span', function(e) {
         analytics.trackEvent(analytics.getGaRevamp2020AnalyticsZone(),'slider_a_click_page',"NA");
    });
</script>
<script>
    
    analytics.trackPage('/_ga/zh/personalcare_and_health/landing');

    
    $("body").on('click','#cboxWrapper .productQuickViewWrapper .addToCartButton', function() {
        analytics.trackEvent('Personalcarenhealth','Overlay_Product_Add_to_Cart', $(this).data('category')+'_'+$(this).data('productcode'));
    });
    $("body").on('click','#cboxWrapper .productQuickViewWrapper .addToWishlistButton', function() {
        analytics.trackEvent('Personalcarenhealth','Overlay_Product_ClickAddToMylist', $(this).data('category')+'_'+$(this).data('productcode'));
        hktvCommon.addStringResources({'eventCategories': 'Personalcarenhealth', 'eventActionPrefix': 'Overlay_Product_'});
    });
    $("body").on('click', '#cboxWrapper .productQuickViewWrapper .tooltipsContainer a' ,function(event){
        analytics.trackEvent('Personalcarenhealth','Overlay_Product_Checkout', $(this).parents('.tooltips').siblings('.addToCartButton').data('category')+'_'+$(this).parents('.tooltips').siblings('.addToCartButton').data('productcode'));
    });
    $("body").on('click','#cboxClose', function() {
        if($('#cboxLoadedContent .productQuickViewWrapper').length>0){
            analytics.trackEvent('Personalcarenhealth','Overlay_Product_Close','N/A');
        }
        else{
            try{
                if($('iframe.cboxIframe')[0].contentWindow){
                    
                    if($('iframe.cboxIframe')[0].contentWindow.location.pathname.match('snsTokenError')){
                        analytics.trackPage('/_ga/Web/click/facebook/fail/cancel');
                        analytics.trackEvent('Account_Registration','Cancel','Step_Facebook_Fail');
                    }
                    else if($('iframe.cboxIframe')[0].contentWindow.location.pathname.match('snsAgeNotEnough')){
                        analytics.trackPage('/_ga/Web/click/facebook/fail/age_cancel');
                        analytics.trackEvent('Account_Registration','Age_Cancel','Step_Facebook_Fail');
                    }
                    
                }
            }
            catch (e){
                console.log('This cbox does not contain any iFrame.');
            }
        }
    });
</script><input type="hidden" value="page-PersonalcarenhealthLandingPage" class="landingCurrentPage">


