<script>
function showhide(divname){
var x = document.getElementById(divname);
var y = document.getElementById("show-hide-control");
  if (x.style.display === "none") {
    x.style.display = "block";
    y.innerHTML="Hide";
  } else {
    x.style.display = "none";
    y.innerHTML="Show";
  }
}
</script>

