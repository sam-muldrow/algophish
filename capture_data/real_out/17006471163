<script type="text/javascript">
var $=jQuery.noConflict();
$(document).ready(function() {
	//Use outerHeight() instead of height() if have padding
    var stickyad = $("#header").outerHeight()+ 2200+ $("#floatingbar").outerHeight();
    var postscript = $(document).height()-600; //50000; // 

    // important chrome and Firefox
    $(window).scroll(function() {
        if ($(window).scrollTop() > stickyad){
              if ($(window).scrollTop() < postscript){
                 $("#floatingbar").addClass('fixed-ad');
              } else {
              	 $("#floatingbar").removeClass('fixed-ad');
              }
        } else {
            $("#floatingbar").removeClass('fixed-ad');
        }	
    });
});

if(document.getElementById('sk')){
	document.getElementById("sk").style.top=600 + "px";
}
	
</script>



