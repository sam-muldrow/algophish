function hideTimer(){
	document.querySelectorAll('.tab-timer').forEach(target => target.style.display = 'none');
}
function cycle_once(tab, prev_tab,target){
  var tar = document.getElementById(tab);
  var prev_tar = document.getElementById(prev_tab);
  var targetNumber = target;
  hideTimer();
  tar.querySelector('.tab-timer').style.display = 'block';
  tar.classList.add('active');
  tar.querySelectorAll('.tab_link-bottom').forEach(target => target.classList.add('active'));
  prev_tar.classList.remove('active');
  prev_tar.querySelectorAll('.tab_link-bottom').forEach(target => target.classList.remove('active'));
 $(".w-tab-link").removeClass("w--current");
 $(".w-tab-pane").removeClass("w--tab-active");
 $(".w-tab-link:nth-child("+targetNumber+")").addClass("w--current");
 $(".w-tab-pane:nth-child("+targetNumber+")").addClass("w--tab-active");
}
var clicked = 0;
function clearTimers(){
	if(typeof timer2 !== 'undefined'){
  	clearTimeout(timer2);
  }
  if(typeof timer3 !== 'undefined'){
  	clearTimeout(timer3);
  }
  if(typeof timer4 !== 'undefined'){
  	clearTimeout(timer4);
  }
  if(typeof timer5 !== 'undefined'){
  	clearTimeout(timer5);
  }
}
function masterTimer() {
	clearTimers();
  if(clicked == 0){
		cycle_once('tab1','tab5',1);
  	window.timer2 = setTimeout("cycle_once('tab2','tab1',2)",7000);
  	window.timer3 = setTimeout("cycle_once('tab3','tab2',3)",14000);
  	window.timer4 = setTimeout("cycle_once('tab4','tab3',4)",21000);
  	window.timer5 = setTimeout("cycle_once('tab5','tab4',5)",28000);
  } else {
    clearInterval(master);
  }
}
const master = setInterval(masterTimer,35000);
masterTimer();
const clearFirst = setTimeout(clearTimers,35000);
