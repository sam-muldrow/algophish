<script>
var ppbutton = document.getElementById("vidbutton");
if (w <= 668){
ppbutton.innerHTML = '<img src="/wp-content/themes/whsk_megatv.com/common/imgs/play-button.png" />';  
} else {
ppbutton.innerHTML = '<img src="/wp-content/themes/whsk_megatv.com/common/imgs/video-pause-button.png" />';  
}
  
  
 function playPause() {
  var ppbutton= document.getElementById("vidbutton");
  var isPaused = kwikMotion('homeplayer').paused();
  var isEnded = kwikMotion('homeplayer').ended();
  if (isPaused){
  kwikMotion('homeplayer').play();
  ppbutton.innerHTML = '<img src="/wp-content/themes/whsk_megatv.com/common/imgs/video-pause-button.png" />';
  } else {
  kwikMotion('homeplayer').pause();
  ppbutton.innerHTML = '<img src="/wp-content/themes/whsk_megatv.com/common/imgs/play-button.png" />';
  }

   
  }
  

 function MuteUnmute() {
  var mbutton=document.getElementById("mbutton");
  var isVolumeMuted = kwikMotion('homeplayer').muted();
  if (isVolumeMuted){
  kwikMotion('homeplayer').muted(false);
  mbutton.innerHTML = '<img src="/wp-content/themes/whsk_megatv.com/common/imgs/volume.png" />';
  } else {
  kwikMotion('homeplayer').muted(true);    
  mbutton.innerHTML = '<img src="/wp-content/themes/whsk_megatv.com/common/imgs/mute.png" />';
  }
 }
   
var player = kwikMotion('homeplayer');
var focused = true; 
var player_state = false;
document.addEventListener("visibilitychange", function () {
   focused = !focused;
   if (!focused) {
      if (typeof player !== 'undefined') { 
         if(kwikMotion('homeplayer').paused()) player_state=true;
            kwikMotion('homeplayer').pause();
         }
      } else {
         if(player_state!=true) {
             kwikMotion('homeplayer').play();
         }
      }
});

//     kwikMotion('homeplayer').on('ended', function(){
//         kwikMotion('homeplayer').play();
// //       kwikMotion('homeplayer').loop(true);
// //       kwikMotion('homeplayer').autoPlay(true);
//     })
  
