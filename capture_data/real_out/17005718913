    $(document).ready(function () {
        var ajaxEvent = 'click';

        $('#alerts-popup-li').one(ajaxEvent, function () {
            $.get('account/alerts-popup?_xfNoRedirect=1&_xfToken=&_xfResponseType=json')
                    .done(function (data) {
                        $('#alerts-popup-li').html(data['templateHtml']);
                        if ($('#alerts-popup-li .media-body').length > 0) {
                            var myScroll = new IScroll('.scrollable-alerts', {
                                mouseWheel: true,
                                click: true,
                                scrollbars: true
                            });
                        }
                    })
                    .fail(function (jqXHR, textStatus, errorThrown) {
                        alert(errorThrown);
                    });
        });

        $('#conversations-popup-li').one(ajaxEvent, function () {
            $.get('account/conversations-popup?_xfNoRedirect=1&_xfToken=&_xfResponseType=json')
                    .done(function (data) {
                        $('#conversations-popup-li').html(data['templateHtml']);
