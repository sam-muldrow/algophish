		}, 1000);
	}
	
	function fireEmiBondGA(){
		if(category == 'R' || category =='S' || category == 'B'){
			var eventAction = "ecsrow";
			var cd = "banner_impression | hp-buy";
			var saleOrRent = 'Sale';
			if(category == 'R'){
				eventAction = "eqaro";
				cd = "banner_impression | hp-rent";
				saleOrRent = 'Rent';
			}
			eMIBondGA(eventAction,'banner_impression | home-page', cd,saleOrRent,'', true);
		}
	}
	
