<script>/*** BYU Meru Data Cookie Consent Banner and Cookie AutoBlocking ***/

const merudata_scripts = (function() {

  const byu_merudataID = "c5d6912a-d8fc-4138-8d1d-6daeb3056f57";
  const byuh_merudataID = "3ef2e724-39fd-43dd-bcf0-9a418af50116";
  const merudata_url = "https://ccm.merudata.app/assets/";

  let domain_merudataID = null; // Set in isBYU() or isHawaii()

  // If this is a production BYU or BYU-Hawaii site, load the scripts
  if ( !isPreview() ) {
    if ( isBYU() || isHawaii() ) {
      loadConfig();
    }
  }

  // Preview site?
  function isPreview() {
    return window.location.hostname.includes( "brigham-young.psdops.com" );
  }

  // BYU site?
  function isBYU() {
    let isBYU = window.location.hostname.includes( "byu.edu" );
    if ( isBYU ) { domain_merudataID = byu_merudataID; } 
    return isBYU;
  }

  // BYU-Hawaii site?
  function isHawaii() {
      const isByuH_url = window.location.hostname.includes( "byuh.edu" );
      const styleEl = document.querySelectorAll("style[data-cssvarsponyfill='true']")[0];
      let isBYUH = isByuH_url || (styleEl ? styleEl.textContent.includes("--primaryColor: #9e1b34;") : false);

      if ( isBYUH ) { domain_merudataID = byuh_merudataID; }
      return isBYUH;
  }

  // Load scripts
  function loadConfig() {
    let scriptConfig = document.createElement("script");
    scriptConfig.src = merudata_url + domain_merudataID + "/ccmconfig.js";
    scriptConfig.onload = function() { 
      loadScript(); // Load the script file
      loadStylesheet(); // Load the stylesheet file
    };
    document.head.appendChild(scriptConfig);
  }

  function loadScript() {
      let script = document.createElement("script");
      script.src = merudata_url + domain_merudataID + "/d_ccm.min.js";
      document.head.appendChild(script);
  }

  function loadStylesheet() {
      let stylesheet = document.createElement("link");
      stylesheet.rel = "stylesheet";
      stylesheet.href = merudata_url + domain_merudataID + "/ccm.min.css" ;
      stylesheet.media = "print";
      stylesheet.onload = function() { 
        this.media = 'all'; 
      };
      document.head.appendChild(stylesheet);
  }

})();</script>
