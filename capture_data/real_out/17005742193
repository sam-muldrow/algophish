<script>
    // Add line numbers for code blocks with Class Name : code-black
    const codeBlackBlocks = document.querySelectorAll('pre.code-black');
    codeBlackBlocks.forEach(codeBlack => {
        const codeLines = codeBlack.innerText.split('\n');
        const blacknumberedLines = codeLines.map((line, index) => {
            const blineNumber = index + 1;
            return `${blineNumber}. ${line}`;
        });
        codeBlack.innerHTML = blacknumberedLines.join('\n');
    });
    // Add line numbers for code blocks with Class Name : code
    const codeBlocks = document.querySelectorAll('pre.code');
    codeBlocks.forEach(codeBlock => {
        const codeLines = codeBlock.innerText.split('\n');
        const numberedLines = codeLines.map((line, index) => {
            const lineNumber = index + 1;
            return `${lineNumber}. ${line}`;
        });
        codeBlock.innerHTML = numberedLines.join('\n');
    });
</script>