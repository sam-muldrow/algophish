        $(".gtarcade_plat_frontpoint_new").click(function (data) {
            toTakeTag($(this).attr('data_gtarcade_plat_frontpoint'));
        })
        function toTakeTag(data) {
            var _DotStr = data,
                _DotArr = _DotStr.split(','),
                _module = _DotArr[0],
                _function = _DotArr[1],
                _target_url = _DotArr[3],
                _value = _DotArr[2];
