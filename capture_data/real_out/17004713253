<script type="text/javascript">
//<![CDATA[
document.addEventListener('DOMContentLoaded', function() {
  var topPrs = document.getElementsByClassName('top_pr');
  for (var i = 0;i < topPrs.length;i++) {
    topPrs[i].addEventListener('click', function() {
      var myXml = new XMLHttpRequest();
      var url = '/topics/pr_click/' + this.getAttribute('data-id') + '/1/' + this.getAttribute('data-device');
      myXml.open('GET', url);
      myXml.send(null);
    });
  }
});
//]]>
</script>
