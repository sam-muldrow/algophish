    <script type="text/javascript">
      function getPush(param) {
        var elementtext = param.getAttribute("data-label");
        var elementsection =  $(param).parents(".footer__linkrecap-block.accordion").find(".hidden-xs.weight--mediumbold").text();
        var elementposition =  $(param).parents("li").index() + 1;
        var elementlinkurl =  $(param).prop('href');
        window.dataLayer.push({
          "event" : 'click_footer',
          "element_text" : elementtext,
          "element_section" : elementsection,
          "element_position" : elementposition, 
          "element_linkurl" : elementlinkurl
        });
      }

      function getPushSocial(param) {
          var element_text       = param.getAttribute("data-label");
          var element_position   = $(param).index() + 1;
          var element_linkurl    = $(param).prop('href');
          window.dataLayer.push({
              "event": 'click_footer',
              "element_text": element_text,
              "element_section": 'Social',
              "element_position": element_position,
              "element_linkurl": element_linkurl
          });
      }
    </script>
    



