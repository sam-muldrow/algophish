<script type="text/javascript">

    loadGTM();
    window.addEventListener('CookiebotOnAccept', function (e) {
    if (Cookiebot.consent.necessary) {
            enablenecessaryCookies();
        } 
    }, false);

    function CookiebotCallback_OnLoad() {
        if( !/CookieConsent=1/.test(document.cookie) ) {
            $(document).on("click", "#acceptAll,.second-step .onlytext,.second-step .transparent", function() {
                setTimeout(function() {
                    if( Cookiebot.consent.marketing ) {
                        UTK.init()
                        loadAccessToken()
                    }
                }, 200);
            });
        }
        if( /CookieConsent/.test(document.cookie) ) {
            if( Cookiebot.consent.marketing ) {
                UTK.init();
                loadAccessToken()
            }
        }

       if (Cookiebot.consent.necessary) { 
           enablenecessaryCookies();
        } 
   }
   
    function enablenecessaryCookies(){
        function setCookieold(name,value,days) {
            var expires = "";
            if (days) {
                var date = new Date();
                date.setTime(date.getTime() + (days*24*60*60*1000));
                expires = "; expires=" + date.toUTCString();
            }
            document.cookie = name + "=" + (value || "")  + expires + "; path=/";
        }
        setCookieold("wind_disclaimer","0",365); 
    } 
</script>
