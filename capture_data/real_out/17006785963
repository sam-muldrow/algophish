                googletag.cmd.push(function () {
                    var _mastheadCheckSizes = googletag.sizeMapping()
                        .addSize([0, 0], [[300, 100], [300, 50], [320, 50], [320, 100]])
                        .addSize([768, 200], [728, 90])
                        .addSize([1190, 200], [[728, 90],[970, 250], [1240, 220]])
                        .build();

                    COF.pubApi.configMastheadCheckSizes(_mastheadCheckSizes);
                });


            var kwpag = [];
            kwpag.push('homepage');
                    kwpag.push('Mercados');
                    kwpag.push('empresas');
                    kwpag.push('economia');
                    kwpag.push('banca');
                    kwpag.push('acções');
                    kwpag.push('noticias');
                    kwpag.push('negócios');
                    kwpag.push('bancos em portugal');
                    kwpag.push('administração pública');
                    kwpag.push('negócios online');
                    kwpag.push('trabalho em portugal');
                    kwpag.push('emprego em portugal');
                    kwpag.push('ganhar dinheiro');
                    kwpag.push('pme');
                    kwpag.push('inovação');
                    kwpag.push('empreendedorismo');

            function adsRenderEnded(adsEvent) {

                var slotElementID = adsEvent.slot.getSlotElementId();

                //remove empty grey div
                if (adsEvent.isEmpty) {
                    if (slotElementID.indexOf('BTFvert') > -1 || slotElementID.indexOf('vertATF') > -1 ) {
                        $('#' + slotElementID).parent().hide();
                    }
                    if (slotElementID.indexOf('BTFhorz') > -1 || slotElementID.indexOf('horzATF') > -1 ) {
                        $('#' + slotElementID).parent().hide();
                        $('#' + slotElementID).parents('.masthead_BG').hide();
                    }
                    if (slotElementID.indexOf('OutOfPage') > -1) {
                        $('#' + slotElementID).hide();
                    }
                }

                //acrescentar palavra pub
                if (!adsEvent.isEmpty) {
                    if (slotElementID.indexOf('BTFhorz') > -1 || slotElementID.indexOf('horzATF') > -1) {
                        if ($('#' + slotElementID).siblings('span').length == 0) {
                            $('#' + slotElementID).after('<span class="info_pub">Pub</span>');
                        }
                    }
                    else if (slotElementID.indexOf('BTFvert') > -1 || slotElementID.indexOf('vertATF') > -1) {
                        if ($('#' + slotElementID).siblings('span').length == 0)
                            $('#' + slotElementID).after('<span class="info_pub">Pub</span>');
                    }
                    else if (slotElementID.indexOf('BTFInContent') > -1) {
                        if ($('#' + slotElementID).length > 0) {
                            $('#' + slotElementID).append('<span class="info_pub">Pub</span>');
                            $('#' + slotElementID).addClass('m-rec publicidadeInContent');
                        }
                    }
                }
            }

            var indexOptin = 0;
            googletag.cmd.push(function () {
                googletag.pubads()
                    .setTargeting('end', [window.location.href.match(/([^\?]*)\/*$/)[0]])
                    .setTargeting('kw', kwpag);

                
                googletag.pubads().setTargeting('up', 'Anonymous');
                



                googletag.pubads().addEventListener('slotRenderEnded', function (event) {
                    adsRenderEnded(event);
                });

                googletag.pubads().enableSingleRequest();
                googletag.pubads().enableAsyncRendering();
                googletag.pubads().collapseEmptyDivs(true, true);
                googletag.pubads().setCentering(true);
                googletag.companionAds().setRefreshUnfilledSlots(true);
                googletag.pubads().disableInitialLoad();


                googletag.enableServices();

                //OOP
                COF.pubApi.renderOOP($('#div-gpt-ad-oop-0'));

                //MASTHEAD
                if ($('#div-gpt-ad-masthead-0').length > 0) {
                    COF.pubApi.renderMasthead($('#div-gpt-ad-masthead-0'), '', 'horzATF', 'ATF', false);
                }

                //MREC
                
                if ($('#div-gpt-ad-mrec-0').length > 0) {
                    COF.pubApi.renderMrec($('#div-gpt-ad-mrec-0'), '', 'vertATF', 'ATF', false);
                }
                


            });
        });

