    require(["main", "adservice"], function () {
        require(["slotplacement/topic", "amd/browser-message"], function (Topic, BrowserMessage) {
          var options = {
            url: "https://static.chasecdn.com",
            audienceIds: ""
          };
          Topic.subscribe('adUpdate', function (pageId, data) {

