				const bannerTitle = document.getElementById('js-banner-title');

				function addBannerClick(type, name, niveau1, niveau2, niveau3) {
					const pageTitle = document.querySelector('meta[property=\'og:title\']').getAttribute('content');
					if (typeof NPO !== 'undefined') {
						NPO.click({
							type: type,
							name: name,
							pageLinked: pageTitle || '',
							niveau1: niveau1,
							niveau2: niveau2,
							niveau3: niveau3
						});
					}
				}

