        <div id="home-pagination" class="pagination"></div>
<script>
    jQuery(document).ready(function(){
        tn_custom_paginate();     });

    function tn_custom_paginate()
    {
                const tnUser = tn_get_user_cookie();
        const homeUrl = window.location.protocol + '//' + window.location.host;
        const currentPage = isNaN(window.location.pathname.split('/')[2]) ? 1 : parseInt(window.location.pathname.split('/')[2]);
        const allPager = (tnUser.is_login && tnUser.permission >= 60) ? true : false;
        const totalPage = allPager ? 8228 : 200;
        const pageRange = 4;
        const startPage = (currentPage - pageRange <= 0) ? 1 : (currentPage - pageRange);
        const endPage = (totalPage - currentPage <= pageRange) ? totalPage : (currentPage + pageRange);

        let paginationDiv = document.createElement('div');
        paginationDiv.className = 'pagination';

        if (startPage > 1) {
            let homeLink = document.createElement('a');
            homeLink.href = homeUrl;
            homeLink.textContent = '« 首頁';
            paginationDiv.appendChild(homeLink);
        }

        if (currentPage > 1) {
            let previousLink = document.createElement('a');
            previousLink.href = homeUrl + '/page/' + (currentPage - 1) + '/';
            previousLink.textContent = '‹上一頁';
            paginationDiv.appendChild(previousLink);
        }
        for (let i = startPage; i <= endPage; i++) {
            if (i === currentPage) {
                let pageLink = document.createElement('span');
                pageLink.className = 'current';
                pageLink.textContent = i;
                paginationDiv.appendChild(pageLink);
            } else {
                let pageLink = document.createElement('a');
                pageLink.href = homeUrl + '/page/' + i + '/';
                pageLink.className = 'inactive';
                pageLink.textContent = i;
                paginationDiv.appendChild(pageLink);
            }
        }

        if (currentPage < totalPage) {
            let nextPageDiv = document.createElement('a');
            nextPageDiv.href = homeUrl + '/page/' + (currentPage + 1) + '/';
            nextPageDiv.textContent = '下一頁›';
            paginationDiv.appendChild(nextPageDiv);
        }

        if (totalPage > 5 && currentPage < totalPage) {
            let lastPageLink = document.createElement('a');
            lastPageLink.href = homeUrl + '/page/' + totalPage + '/';
            lastPageLink.textContent = '最後一頁»';
            paginationDiv.appendChild(lastPageLink);
        }

        document.getElementById('home-pagination').appendChild(paginationDiv);
    }
</script>
