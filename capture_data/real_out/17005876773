        var currentminuts = new Date().getMinutes();
        var currentday = new Date().getDay();
        var currentdate = serverDateTime.getDate();

        if (currentdate == 7 || currentdate == 30) {
            document.getElementById("marquee").innerHTML = data[7].toString();
        }
        if (currentdate == 8 || currentdate == 31) {
            document.getElementById("marquee").innerHTML = data[8].toString();
