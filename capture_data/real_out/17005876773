		  $('.window .close').click(function (e) {
            //Cancel the link behavior
            e.preventDefault();
            $.cookie('feedback_Cookie', 'Yes', { expires: 365, path: '/' });
            $('#maskWN').hide();
            $('.window').hide();
            var scroll = $(window).scrollTop();
            $("html").scrollTop(scroll);
        });
		
		 $('#maskWN').click(function () {
            $(this).hide();
            $.cookie('feedback_Cookie', 'Yes', { expires: 365, path: '/' });
            $('.window').hide();

        });

