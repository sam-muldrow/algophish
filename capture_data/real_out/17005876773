            <section>
                <div class="grid_3 omega">
                <h3><span class="blue">Functionwise</span> Sites</h3>
                 <div role="menubar" id="wrapper1" class="grid_subtree archives alpha">
    <div role="menuitem" aria-haspopup="true" aria-controls="MPContent" class="accordionButton"
        id="MP">
        <a href="javascript:void(0);">Monetary Policy</a></div>
    <div role="menu" aria-hidden="true" aria-owns="MP" class="accordionContent" id="MPContent">
        <ul class="accordionlist">
            <li role="menuitem"><a href="../scripts/FS_Overview.aspx?fn=2752" id="MPOverview">Overview</a></li>
            <li role="menuitem"><a href="../scripts/Annualpolicy.aspx" id="MPIDs">Monetary Policy
                Statements</a></li>
            <!--<li role="menuitem"><a href="#" id="MPLF">Legal Framework</a></li>-->
