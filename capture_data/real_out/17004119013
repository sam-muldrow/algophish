<script id="imh-promo-banner-js-after">
window.addEventListener('DOMContentLoaded', () => {
	var pClass = '';
	var pCtaType = 'chat';
	var pDataProd = 'business-hosting';
	var pLink = '';
	var hidePromo = 'true';
	var pBgImg = 'https://design.inmotionhosting.com/assets/images/promos/hero_promo_banner_new.webp';
	var btnLabel = 'Chat Now';
	var pStyle = 'standard';	if(!pClass) {
		var attrs = false;
		var el = false;
		var rostrum = document.querySelector('imh-product-rostrum-v3');
		var table = document.querySelector('imh-product-spec-table-v1');

		if(rostrum) {
			attrs = rostrum.getAttributeNames();
			el = rostrum;
		} else if(table) {
			attrs = table.getAttributeNames();
			el = table;
		}
		if(attrs) {
			attrs.forEach((attr, i) => {
				if(attr.includes('product-class')) {
					pClass = el.getAttribute(attr);
				}
			});
		}
	}
	
	if(pClass && !document.querySelector('.promo-container')) {
		var promos = catalog.promos[pClass];
		var pricing = catalog.pricing[pClass];
		var atcBtns = getAtcButtons(pClass, btnLabel, 'atc btn-secondary promo-cta');
		var promoEnd = false;
		var bannerHTML = ''
		if(promos && promos['featured']) {
			Object.entries(promos['featured']).forEach(featured => {
			var promoProduct = featured[0];
			var date = new Date(featured[1]['CampaignEndDate']);
			var d = date.toString().split(' ');
			promoEnd = d[1]+' '+d[2]+', '+d[3];
			var promoTerm = featured[1]['TermLength'];
			if(featured[1]['TermType'] === 'year'){
				promoTerm = featured[1]['TermLength'] * 12;
			}
			var promoBtnKey = promoProduct+promoTerm;
			var pt = featured[1]['PricingDescription'].replaceAll('<p>','').replaceAll('</p>','/n');
			var promoTextArray = pt.split('/n');
			var discountPrice = featured[1]['DiscountPrice'];
			var normalPrice = pricing[promoTerm][promoProduct][0]['Price'];
			var promoDiscountPercent = ((normalPrice - discountPrice) / normalPrice) * 100;
			
			bannerHTML += `
			<div class="container promo-container" style="background-image: url('${pBgImg}');">
				<div class="row">
					<div class="imh-4-col banner-section-1">
					<div class="pr-1"><p>${promoTextArray[2]}<p><p>${promoTextArray[3]}</p></div>
					<div class="pr-2"><p>${promoTextArray[4]}</p></div>
					</div>
					<div class="imh-4-col pr-3">
					<p>${promoTextArray[5]}</p>
					<p class="promo-banner-subtext">New Customer Accounts Only. Valid through ${promoEnd} at 12:00pm PST.</p>
					</div>
					<div class="imh-4-col pr-4">`;
					if(pCtaType !== 'hide') {
						switch(pCtaType) {
							case 'atc':
							var btn = atcBtns[promoBtnKey];
							bannerHTML += btn;
							bannerHTML += `
							<div id="timer"></div>
							`;
							break;
							case 'chat':
							bannerHTML += `
							<a class="btn-secondary hero-btn chat-btn-popup promo-cta" data-value="sales" aria-label="Promo Banner Chat" listening-for="click" data-prodvalue="${pDataProd}">${btnLabel}</a>
							<div id="timer"></div>
							`;
							break;
							case 'link':
							bannerHTML += `
							<a href="${pLink}" class="btn-secondary hero-btn promo-cta">${btnLabel}</a>
							<div id="timer"></div>
							`;
							break;
							case 'hide':
							bannerHTML += ``;
							break;
						}
					}
				bannerHTML += `
					</div>
				</div>
				</div>
			`;
			});
			
			if(!hidePromo ||hidePromo === 'false') {
			let pContainer = document.createElement("div");
			pContainer.classList.add('promo-banner');
			pContainer.classList.add(pStyle);
			pContainer.innerHTML = bannerHTML;
			
			var timer = pContainer.querySelector("#timer");
			
			if(window.matchMedia('(max-width: 959.98px)').matches) {
				pContainer.querySelector('.promo-banner .promo-container').style.backgroundImage = `none`;
				pContainer.querySelector('.promo-banner .banner-section-1').style.backgroundImage = `url(${pBgImg})`;
			}
			
			if(timer) {

				var countdownDate = new Date(promoEnd + ' 12:00:00').getTime();

				var x = setInterval(function() {

					var nowDate = new Date();
					var now = new Date(nowDate.toLocaleString("en-US", { timeZone: "America/Los_Angeles" })).getTime();
					var distance = countdownDate - now;

					var days = Math.floor(distance / (1000 * 60 * 60 * 24));
					var hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
					var minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
					var seconds = Math.floor((distance % (1000 * 60)) / 1000);
					
					timer.innerHTML = "Deal Ends in:<div><div>" + days + "<span>Days</span> </div> :<div>" + hours + "<span>Hours</span> </div> :<div>" + minutes + "<span>Minutes</span> </div> :<div>" + seconds + "<span>Seconds</span></div></div></div>";
					
					if (distance < 0) {
						clearInterval(x);
						timer.innerHTML = "DEAL EXPIRED";
					}
				}, 1000);

				}
			
			if(!document.querySelector('.promo-banner')) {
				document.querySelector('.imh-hero').after(pContainer);
			}
			
			window.addEventListener('resize', function(event) {
				if(document.querySelector('.promo-banner .promo-container')) {
					if(window.matchMedia('(max-width: 959.98px)').matches) {
					document.querySelector('.promo-banner .promo-container').style.backgroundImage = `none`;
					document.querySelector('.promo-banner .banner-section-1').style.backgroundImage = `url(${pBgImg})`;
					} else {
					document.querySelector('.promo-banner .banner-section-1').style.backgroundImage = `none`;
					document.querySelector('.promo-banner .promo-container').style.backgroundImage = `url(${pBgImg})`;
					}
				}
				});
			}

			let promoBanner = document.querySelector('.promo-banner');

			if(promoBanner) {
				if(promoBanner.classList.contains('sticky-1')
				|| promoBanner.classList.contains('sticky-2')
				|| promoBanner.classList.contains('sticky-3')) {
					let promoBannerCurrentPos = promoBanner.offsetTop - promoBanner.offsetHeight;
					window.addEventListener('scroll', () => {
						if(window.pageYOffset > promoBannerCurrentPos) {
							promoBanner.classList.add('collapsed');
						} else {
							promoBanner.classList.remove('collapsed');

						}
					});
				}
			}
			
		}
	}

	
});
</script>
