	function goBoardDetailLink(pageURL, boardId, articleId) {
		document.boardViewFrm.action = pageURL;
		document.boardViewFrm.BOARD_ID.value = boardId;
		document.boardViewFrm.ARTICLE_ID.value = articleId;
