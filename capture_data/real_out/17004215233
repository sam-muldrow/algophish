  var ScreeenWidth = $(window).innerWidth();
   $(document).ready(function () {
           if(digitalData.page.pageInfo.pageID == '185343'){
              if (/iPad|iPhone|iPod/.test(navigator.platform)){
                  console.log('iphone');
                  $(".digital_platform_cookie_table thead th").attr('aria-hidden','true');
              }
              else{
                  $(".digital_platform_cookie_table thead th").removeAttr('aria-hidden');
                if(window.innerWidth < 922){
                    $(".digital_platform_cookie_table thead th").attr('aria-hidden','true');
                  }
