        var ctaText = "Schedule a free consultation"
            consultationURL = "https://agentcalendar-a6e4g7cueme2hueb.z01.azurefd.net/SMBEntryForm.aspx?trg=UNIFIEDNA"
            consultationURL = consultationURL.toString();
            $("#btn-consultation").attr("href", consultationURL);
            $('#spn-consultation').html(ctaText);
            var existingButton = $('.hero-beta__btn span').html();
          if(ctaText.toLowerCase() == "none" || consultationURL.toLowerCase() == "none" || (existingButton != null && existingButton.toLowerCase() == "schedule a free consultation"))
          {
              $('#btn-consultation').hide();
          } 
      else
        {
          $('#btn-consultation').show();
        }
    }
        
         var hideSignUpBtn = function()
         {
          if(digitalData.page.pageInfo.nodeID == '24457')
            {
              $('#c-uhf-nav-cta').hide();
            }
           if((digitalData.page.pageInfo.pageID == '183328' || digitalData.page.pageInfo.nodeAliasPath == '/main/get-started/sign-up-with-microsoft-advertising') && digitalData.page.attributes.market == 'zh-cn')
            {
              $('#c-uhf-nav-cta').hide();
            }
         }
</script>
