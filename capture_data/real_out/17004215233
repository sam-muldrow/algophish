<script>
$(window).scroll(function () {

    if (digitalData.page.attributes.market == "en-us") {

        /*CHECKING THE CONSENT BANNER AVAILABILITY*/

        if ($("#wcpConsentBannerCtrl").hasClass('_23tra1HsiiP6cT-Cka-ycB') == true) {

            /*CHECKING THE CONSENT BANNER AND PROMOTION BANNER*/

            if ($('#epb').hasClass('x-hidden') == false) {

                var consentbannerHeight = $('#cookie-banner').height();

                var bannerHeight = $('#epb').height();

                if (bannerHeight == undefined) {

                    emptyvalue();

                } else {

                    if ($(this).scrollTop() > (consentbannerHeight + bannerHeight)) {

                        $('header').addClass('uhf-Sticky')

                    } else {

                        $('header').removeClass('uhf-Sticky')

                    }

                }

            } else {

                /*IF ONLY CONSENT BANNER AVAILABLE*/

                var consentbannerHeight = $('#cookie-banner').height();

                if ($(this).scrollTop() > consentbannerHeight) {

                    $('header').addClass('uhf-Sticky')

                } else {

                    $('header').removeClass('uhf-Sticky')

                }

            }

        } else {

            /*IF ONLY PROMOTION BANNER AVAILABLE*/

            if ($('#epb').hasClass('x-hidden') == false) {

                var bannerHeight = $('#epb').height();

                if (bannerHeight == undefined) {

                    nobanner();

                } else {

                    if ($(this).scrollTop() > bannerHeight) {

                        $('header').addClass('uhf-Sticky')

                    } else {

                        $('header').removeClass('uhf-Sticky')

                    }

                }

            } else {

                /*WITHOUT BANNER*/

                if ($(this).scrollTop() > 0) {

                    $('header').addClass('uhf-Sticky')

                } else {

                    $('header').removeClass('uhf-Sticky')

                }

            }

        }

    }

});

 

function emptyvalue() {

    var consentbannerHeight = $('#cookie-banner').height();

    if ($(this).scrollTop() > consentbannerHeight) {

        $('header').addClass('uhf-Sticky')

    } else {

        $('header').removeClass('uhf-Sticky')

    }

}

 

function nobanner() {

    if ($(this).scrollTop() > 0) {

        $('header').addClass('uhf-Sticky')

    } else {

        $('header').removeClass('uhf-Sticky')

    }

}
</script>

</head>
