<script async="false" type="text/javascript">
  var siteConsent= null;
  function manageConsent() {
    if(siteConsent.isConsentRequired){
      siteConsent.manageConsent();
    }
  }
  function onConsentChanged(categoryPreferences) {      
    console.log("onConsentChanged", categoryPreferences); 
    var currentConsent = siteConsent.getConsent();
    if (currentConsent.Analytics && currentConsent.Advertising) {
      LoadAdobeScripts(); 
      console.log("consent accepted. Adobe scripts added");  
    }
    const ConsentChangedEvent= new Event("ConsentChanged");
    document.dispatchEvent(ConsentChangedEvent);
    
  }
  window.WcpConsent && WcpConsent.init("en-US", "cookie-banner", function (err, siteConsentParam) {
    if (!err) {
      siteConsent= siteConsentParam;      
    } else {
      console.log("Error initializing WcpConsent: "+ err);
    }
  }, onConsentChanged, WcpConsent.themes.light);
  
  if(siteConsent.isConsentRequired){ 
    var consentState= siteConsent.getConsent();
    if (consentState.Analytics && consentState.Advertising) {
      LoadAdobeScripts();
      console.log("Returning user - Adobe scripts added");  
    }
  }
  
  if(!siteConsent.isConsentRequired){
    LoadAdobeScripts();
    console.log("Consent not required - Adobe scripts added "); 
  }
  
  
  
  function LoadAdobeScripts()
  {
    var currentBuildFoldername= $('#live-person-script').attr('src');
    currentBuildFoldername= currentBuildFoldername.slice(0, currentBuildFoldername.lastIndexOf('/'));
    var atMinJS= currentBuildFoldername + "/at.min.js";
    var atConfigMinJs= currentBuildFoldername + "/at-config.min.js";
    var atConfigMinJSScript= document.createElement("script");
    atConfigMinJSScript.type= "text/javascript";
    atConfigMinJSScript.async= false;
    atConfigMinJSScript.src= atConfigMinJs;
    atConfigMinJSScript.id= "atconfig-script"
      $("head").append(atConfigMinJSScript);
    var atMinJSScript= document.createElement("script");
    atMinJSScript.type= "text/javascript";
    atMinJSScript.src= atMinJS;
    atMinJSScript.async= false;
    atMinJSScript.id= "at-script"
      $("head").append(atMinJSScript);
    setTimeout(launch_load, 1000);
  }
  
  function launch_load()
  {
    var launch_js_url="";
    dd_EnvironmentCode= digitalData.page.siteInfo.siteEnvCode;
    if (dd_EnvironmentCode== "prod") { 
      launch_js_url="https://advertiseonbing-blob.azureedge.net/blob/launch/search/prd/5ef092d1efb5/3bfc36d154cc/launch-dd02e12af77d.min.js";} 
    else { 
      if (dd_EnvironmentCode== "dev") { 
        launch_js_url="https://assets.adobedtm.com/5ef092d1efb5/3bfc36d154cc/launch-4b133cd5c940-development.min.js";} 
      else { 
        launch_js_url="https://advertiseonbing-blob.azureedge.net/blob/launch/search/stg/5ef092d1efb5/3bfc36d154cc/launch-677cca724c8c-staging.min.js";} } 
    var launchJSScript= document.createElement("script");
    launchJSScript.type= "text/javascript";
    launchJSScript.src= launch_js_url;
    launchJSScript.async= false;
    launchJSScript.id= "launch-script";
    $("head").append(launchJSScript);
  }
  
</script>

<script>
  
  $(document).ready(function () { 
    if($(".fixed-newsletter.noprint").length > 1){    
      $('.fixed-newsletter.noprint').not(':last').remove();
    }
    if($('#uhf-swp').length > 0){
      $('#uhf-swp').removeAttr('aria-label');
      var ariaLabelText = $("#uhf-swp p")[0].innerText.replace(">", " ")
          ariaLabelText = ariaLabelText + "Opens in new window";
      $('#uhf-swp').attr('aria-label', ariaLabelText);
    }
    
    function addConsultationLabel() {	   
      if($('#btn-consultation').length > 0){
        $('#btn-consultation').removeAttr('aria-label');
        var ariaLabelconsultation = $('#btn-consultation span')[0].innerText;
        ariaLabelconsultation = ariaLabelconsultation + " Opens in new window";
        $('#btn-consultation').attr('aria-label', ariaLabelconsultation);
	  }
    }
    setTimeout(addConsultationLabel, 1500);
    
    $('#primary-nav-item-signin').attr('role','link');
    
    if (siteConsent.isConsentRequired)
    {
      $('#c-uhff-footer_managecookies').toggleClass("x-hidden");
      $('#c-uhff-footer_managecookies').find("a").attr("href", "javascript:manageConsent()");
    }
 
  });
</script>


     


