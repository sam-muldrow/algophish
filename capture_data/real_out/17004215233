    <script>
        $(document).ready(function () {
          if($(".hrtab_paragraph").val() == ""){
                $(".hrtab_paragraph").addClass("insights_hiden");
            }
            /*HORIZONTAL TAB ORDER CHANGE SCRIPT*/
            function divPositionChangeLL() {
                if (($(".tab-content")) && ((window.screen.availWidth) < 769)) {
                    const firstItem = document.querySelector('.tab_lft_column');
                    const secondItem = document.querySelector('.tab_rgt_column');
                    $(firstItem).insertAfter(secondItem);
                } else if (($(".tab-content")) && ((window.screen.availWidth) < 992)) {
                    const firstItem = document.querySelector('.tab_lft_column');
                    const secondItem = document.querySelector('.tab_rgt_column');
                    $(firstItem).insertAfter(secondItem);
                }
                else {
                    const firstItem = document.querySelector('.tab_lft_column');
                    const secondItem = document.querySelector('.tab_rgt_column');
                    $(secondItem).insertAfter(firstItem);
                }
            }

            $(window).resize(function () {
                divPositionChangeLL();
            });
          
          $(window).on('resize', function(){
              var win = $(this); //this = window
              if (win.width() < 992) { 
                  console.log("Tab and mobile resolution");
                  $(".tablist_li").removeAttr("tabindex");
                  $(".tablist_li").attr("aria-hidden","true");
                  $(".tablist_li.active").attr("tabindex","0");
                  $(".tablist_li.active").removeAttr("aria-hidden");
              } else{
                   $(".tablist_li").attr("tabindex","0");
                   $(".tablist_li").removeAttr("aria-hidden");
              }
          });
          
          var windowsizetab = $(window).width();
          if (windowsizetab < 992) { 
                  console.log("refresh page");
                  $(".tablist_li").removeAttr("tabindex");
                  $(".tablist_li").attr("aria-hidden","true");
                  $(".tablist_li.active").attr("tabindex","0");
                  $(".tablist_li.active").removeAttr("aria-hidden");
                  
              } else{
                   $(".tablist_li").attr("tabindex","0");
                   $(".tablist_li").removeAttr("aria-hidden");
              }
            /*HORIZONTAL TAB ONCLICK SCRIPT*/
        $(document).on('keypress click', '.tabs-container li', function () {
            $(this).blur();
            $('.tabs-container li').removeClass('active');
            $('.tabs-container ul').toggleClass('expanded');
            $(this).addClass('active');
            var tab_id = $(this).attr('data-tab');
            $('.tab-content').removeClass('current');
            $(this).addClass('current');
            $('#' + tab_id).addClass('current');
			$(window).on('resize', function(){
              var win = $(this); //this = window
              if (win.width() < 992) { 
                  console.log("Tab and mobile resolution");
                  if($(".tabs ul").hasClass("expanded") == true){
                    $(".tablist_li").attr("tabindex","0");
                    $(".tablist_li").removeAttr("aria-hidden");
                } else{
                    $(".tablist_li").removeAttr("tabindex");
                    $(".tablist_li").attr("aria-hidden","true");
                    $(".tablist_li.active").attr("tabindex","0");
                    $(".tablist_li.active").removeAttr("aria-hidden");
                    $(".tablist_li.active").focus();
                }
              }
          });
		  
		  var windowsizetab = $(window).width();
          if (windowsizetab < 992) { 
                  console.log("refresh page");
                  if($(".tabs ul").hasClass("expanded") == true){
                    $(".tablist_li").attr("tabindex","0");
                    $(".tablist_li").removeAttr("aria-hidden");
                } else{
                    $(".tablist_li").removeAttr("tabindex");
                    $(".tablist_li").attr("aria-hidden","true");
                    $(".tablist_li.active").attr("tabindex","0");
                    $(".tablist_li.active").removeAttr("aria-hidden");
                    $(".tablist_li.active").focus();
                }
              }
            
        });
 });
         </script>
