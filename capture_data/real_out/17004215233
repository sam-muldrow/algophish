                    $(".digital_platform_cookie_table thead th").removeAttr('aria-hidden');
                  }
              }
          }
          $("a.btn-primary-xandrcolor").attr("role","button");
          //switch mobile option enable code
   function divPositionChangexandr(){
	//Its for fifty fifty class name
	if(($(".two-col.two-col--switch-mobile.bg-red-light-shade-xandr")) && ((window.screen.availWidth) < 769) && (ScreeenWidth < 769)){
		console.log("mobile");
        const firstItem = document.querySelector('.two-col .flex-row .two-col__left');
        const secondItem = document.querySelector('.two-col .flex-row .two-col__right');
        $(firstItem).insertAfter(secondItem);
    }
    else{
        const firstItem = document.querySelector('.two-col .flex-row .two-col__left');
        const secondItem = document.querySelector('.two-col .flex-row .two-col__right');
        $(secondItem).insertAfter(firstItem);
    }
	if(($(".two-col.two-col--switch-mobile.third-container")) && ((window.screen.availWidth) < 769) && (ScreeenWidth < 769)){
        const firstItem = document.querySelector('.two-col--switch-mobile.third-container .flex-row .two-col__left');
        const secondItem = document.querySelector('.two-col--switch-mobile.third-container .flex-row .two-col__right');
        $(firstItem).insertAfter(secondItem);
    }
    else{
        const firstItem = document.querySelector('.two-col--switch-mobile.third-container .flex-row .two-col__left');
        const secondItem = document.querySelector('.two-col--switch-mobile.third-container .flex-row .two-col__right');
        $(secondItem).insertAfter(firstItem);
    }
}
$(window).resize(function(){
  if(digitalData.page.pageInfo.pageID != "185337"){
      divPositionChangexandr();
    }
