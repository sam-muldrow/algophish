     <script>
        $(document).ready(function () {
          function xandruhfmenu(){
                if (window.innerWidth > 992) {
                    $(".xandr_logo").removeAttr("tabindex");
                    //$(".xandr_logo").removeAttr("role");
                    //$(".xandr_logo").removeAttr("type");
                } else {
                    $(".xandr_logo").attr("tabindex", "0");
                    //$(".xandr_logo").attr("role","button");
                    //$(".xandr_logo").attr("type","button");
                }
              }
          $(window).resize(function () {
                xandruhfmenu();
            });
          
         if (window.innerWidth > 992) {
                    $(".xandr_logo").removeAttr("tabindex");
                }
          });
   

        /*UHF HEADER SUB DROPDOWN ONCLICK SCRIPT*/
        var elements = document.getElementsByClassName("xandr_logo");

		var myFunction = function() {
			if (window.innerWidth < 922) {
				if ($(".xandr_logo i").hasClass("down") == true) {
					$(".xandr_logo i").removeClass("down");
					$(".xandr_logo i").addClass("up");
					$(".xandr_nav").css("display", "block");
				} else {
					$(".xandr_logo i").removeClass("up");
					$(".xandr_logo i").addClass("down");
					$(".xandr_nav").css("display", "none");
				}
			}
		};

		for (var i = 0; i < elements.length; i++) {
			elements[i].addEventListener('click', myFunction, false);
		}
      
        //keypress for xandr logo
        var myFunctionkeypress = function() {
			if (window.innerWidth < 922) {
				if ($(".xandr_logo i").hasClass("down") == true) {
					$(".xandr_logo i").removeClass("down");
					$(".xandr_logo i").addClass("up");
					$(".xandr_nav").css("display", "block");
				} else {
					$(".xandr_logo i").removeClass("up");
					$(".xandr_logo i").addClass("down");
					$(".xandr_nav").css("display", "none");
				}
			}
		};
		
		for (var i = 0; i < elements.length; i++) {
			elements[i].addEventListener('keypress', myFunctionkeypress, false);
		}

        $(document).on('click', '#subnavdropdown', function () {
            if ($("span i").hasClass("down") == true) {
                $("span i").removeClass("down");
                $("span i").addClass("up");
                $(".sub_nav_xandr").css("display", "block");
            } else {
                $("span i").removeClass("up");
                $("span i").addClass("down");
                $(".sub_nav_xandr").css("display", "none");
            }
        });
        
        window.addEventListener('click', (event) => {
            if (!event.target.closest('#subnavdropdown')) {
                $("#subnavdropdown span i").removeClass("up");
                $("#subnavdropdown span i").addClass("down");
                $(".sub_nav_xandr").css("display", "none");
                if (window.screen.width < 922) {
                    if (!event.target.closest('.xandr_logo')) {
                        $(".xandr_logo i").removeClass("up");
                        $(".xandr_logo i").addClass("down");
                        $(".xandr_nav").css("display", "none");
                    }
                }
            }
        });
    </script>

