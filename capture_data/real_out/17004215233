   function divPositionChange(){
	//Its for md class name
    if(($(".two-col--two-thirds-right--md.two-col--switch-mobile")) && ((window.screen.availWidth) < 769)){
        const firstItem = document.querySelector('.two-col--two-thirds-right--md .flex-row .two-col__left');
        const secondItem = document.querySelector('.two-col--two-thirds-right--md .flex-row .two-col__right');
        $(firstItem).insertAfter(secondItem);
    }
    else{
        const firstItem = document.querySelector('.two-col--two-thirds-right--md .flex-row .two-col__left');
        const secondItem = document.querySelector('.two-col--two-thirds-right--md .flex-row .two-col__right');
        $(secondItem).insertAfter(firstItem);
    }
    if(($(".two-col--two-thirds-left--md.two-col--switch-mobile")) && ((window.screen.availWidth) < 769)){
        const firstItem = document.querySelector('.two-col--two-thirds-left--md .flex-row .two-col__left');
        const secondItem = document.querySelector('.two-col--two-thirds-left--md .flex-row .two-col__right');
        $(firstItem).insertAfter(secondItem);
    }
    else{
        const firstItem = document.querySelector('.two-col--two-thirds-left--md .flex-row .two-col__left');
        const secondItem = document.querySelector('.two-col--two-thirds-left--md .flex-row .two-col__right');
        $(secondItem).insertAfter(firstItem);
    }
	//Its for without md class name
    if(($(".two-col--two-thirds-right.two-col--switch-mobile")) && ((window.screen.availWidth) < 769)){
        const firstItem = document.querySelector('.two-col--two-thirds-right .flex-row .two-col__left');
        const secondItem = document.querySelector('.two-col--two-thirds-right .flex-row .two-col__right');
        $(firstItem).insertAfter(secondItem);
    }
    else{
        const firstItem = document.querySelector('.two-col--two-thirds-right .flex-row .two-col__left');
        const secondItem = document.querySelector('.two-col--two-thirds-right .flex-row .two-col__right');
        $(secondItem).insertAfter(firstItem);
    }
    if(($(".two-col--two-thirds-left--md.two-col--switch-mobile")) && ((window.screen.availWidth) < 769)){
        const firstItem = document.querySelector('.two-col--two-thirds-left .flex-row .two-col__left');
        const secondItem = document.querySelector('.two-col--two-thirds-left .flex-row .two-col__right');
        $(firstItem).insertAfter(secondItem);
    }
    else{
        const firstItem = document.querySelector('.two-col--two-thirds-left .flex-row .two-col__left');
        const secondItem = document.querySelector('.two-col--two-thirds-left .flex-row .two-col__right');
        $(secondItem).insertAfter(firstItem);
    }
	//Its only having switchmobile
	if(($(".two-col.two-col--switch-mobile")) && ((window.screen.availWidth) < 769)){
        const firstItem = document.querySelector('.two-col .flex-row .two-col__left');
        const secondItem = document.querySelector('.two-col .flex-row .two-col__right');
