 	  	  var adv_sizes = [];
		  var check_adv_size = function(frcd){
			  var sizeWP = 0, sizeMPU = 0, mobile = (window.innerWidth < 768);
			  if (document.getElementById('page').offsetTop) sizeWP = document.getElementById('page').offsetTop;
			  if (sizeWP>30 && !mobile) sizeWP-=30;
			  if (mobile) sizeWP-=10;
			  var o = document.getElementById((mobile ? 'admputop2' : 'admputop'));
			  if (o) if (o.clientHeight) sizeMPU = o.clientHeight;
			  if ((sizeMPU > 310)&&(sizeWP > 90)) adv_sizes['wp600']=1;
			  else {
				  if (sizeWP > 90) adv_sizes['wp']=1;
				  if (sizeMPU > 310) adv_sizes['h600']=1;
			  }
			  if (document.getElementById('adv_left_div')) adv_sizes['skin']=1;
			  if (document.getElementById('eyeDiv')) adv_sizes['eyediv']=1;
			  if (document.getElementById('ayads-html')) adv_sizes['ayads']=1;
			  if (document.getElementById('ayads-video')) adv_sizes['ayadsvideo']=1;
			  if (typeof frcd === 'undefined') setTimeout(check_adv_size, 8000);
		  }
		  setTimeout(check_adv_size, 2000);
		  webVitals.getCLS(function(d){ cls_trk = Math.floor(d.value * 1000); });
		  webVitals.getLCP(function(d){ lcp_trk = Math.floor(d.value); lcp_data = d; });
		  webVitals.getTTFB(function(d){ ttfb_trk = Math.floor(d.value); });
		  if (document.addEventListener) document.addEventListener("visibilitychange", function() {
			  if (document.visibilityState === 'hidden') {
				  if (!cwv_once) return;
				  cwv_once = false;
				  if (!(cls_trk === false)) {
					  var labs = '';
						  try {
								check_adv_size(true);
								if (adv_sizes['eyediv']) labs = 'eyediv';
								else if (adv_sizes['ayadsvideo']) labs = 'ayadsvideo';
								else if (adv_sizes['ayads']) labs = 'ayads';
								else if (adv_sizes['wp600']) labs = 'masthead_600';
								else if (adv_sizes['wp']) labs = 'masthead';
								else if (adv_sizes['h600']) labs = '300x600';
								else if (adv_sizes['skin']) labs = 'skin';
								else labs = 'none';
						 } catch(e){ labs = ''; }
					 iol_track("CLS", cls_trk, labs);
				  }
				  if (!(lcp_trk === false)) {
