								  lcp_org = "MULTI";
							  } else {
								  var o = lcp_data.entries[0].element;
								  if (o.nodeName) oo += o.nodeName;
								  else  oo += "NONE";
								  if (o.classList) if (o.classList[0])  oo += encodeURIComponent("." + o.classList[0]);
								  if (o.id) oo += encodeURIComponent("#" + o.id);
								  lcp_org = oo;
							  }
						  }
					  } catch(e) {
						  lcp_org = '';
					  }
					  iol_track("LCP", lcp_trk, lcp_org);
				  }
				  if (!(ttfb_trk === false)) iol_track("TTFB", ttfb_trk);
			  }
		  });
	  }
	  setTimeout(function(){
		  var s = document.getElementsByTagName('script')[0];
		  s.parentNode.insertBefore(ct, s);
	  }, 5000);
