var ocm_page_type = 'Home';
!(function () {
var sub_paths = window.location.pathname.replace("/", "").split("/") || [];
sub_paths = sub_paths.filter(String);
if (sub_paths.length === 0) {
ocm_page_type = "Home";
} else if (sub_paths.length === 1) {
if (sub_paths[0] == 'efsyn-city') {
ocm_page_type = 'Efsyn City Home';
} else {
ocm_page_type = "Category";
}
} else if (sub_paths.length === 2) {
if (sub_paths[0] === "node") {
ocm_page_type = "Article";
} else if (sub_paths[0] == 'efsyn-city') {
ocm_page_type = 'Efsyn City Category';
} else {
ocm_page_type = "Category";
}
} else if (sub_paths.length >= 3) {
if (sub_paths[0] == 'efsyn-city') {
ocm_page_type = 'Efsyn City Article';
} else {
ocm_page_type = "Article";
}
}
})();
