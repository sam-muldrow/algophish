				{ title: 'Building a Keyboard: Ducky ProjectD', url: '/review/building-a-keyboard-14-ducky-projectd-outlaw-65-cherry-mx2a-red-rgb/', ts: '1699283220', small: '/review/building-a-keyboard-14-ducky-projectd-outlaw-65-cherry-mx2a-red-rgb/images/small.png', small1x: '/review/building-a-keyboard-14-ducky-projectd-outlaw-65-cherry-mx2a-red-rgb/images/small@1x.png', class: 'review-carousel-item__link ' },
				{ title: 'KLEVV CRAS V RGB DDR5-6400', url: '/review/klevv-cras-v-rgb-ddr5-6400-cl32-2x-16-gb/', ts: '1699043108', small: '/review/klevv-cras-v-rgb-ddr5-6400-cl32-2x-16-gb/images/small.png', small1x: '/review/klevv-cras-v-rgb-ddr5-6400-cl32-2x-16-gb/images/small@1x.png', class: 'review-carousel-item__link ' },
				{ title: 'Gizaudio x Binary Acoustics Chopin IEMs', url: '/review/quick-look-gizaudio-x-binary-acoustics-chopin-in-ear-monitors/', ts: '1699035780', small: '/review/quick-look-gizaudio-x-binary-acoustics-chopin-in-ear-monitors/images/small.png', small1x: '/review/quick-look-gizaudio-x-binary-acoustics-chopin-in-ear-monitors/images/small@1x.png', class: 'review-carousel-item__link ' },
				{ title: 'Lexar NM790 2 TB', url: '/review/lexar-nm790-2-tb/', ts: '1698957600', small: '/review/lexar-nm790-2-tb/images/small.png', small1x: '/review/lexar-nm790-2-tb/images/small@1x.png', class: 'review-carousel-item__link ' },
				{ title: 'Alphacool Core RTX 4090 Strix/TUF', url: '/review/alphacool-core-geforce-rtx-4090-strix-tuf-gpu-block/', ts: '1698937860', small: '/review/alphacool-core-geforce-rtx-4090-strix-tuf-gpu-block/images/small.png', small1x: '/review/alphacool-core-geforce-rtx-4090-strix-tuf-gpu-block/images/small@1x.png', class: 'review-carousel-item__link ' },
				{ title: 'Thermaltake Ceres 300 TG ARGB', url: '/review/thermaltake-ceres-300-tg-argb/', ts: '1698850440', small: '/review/thermaltake-ceres-300-tg-argb/images/small.png', small1x: '/review/thermaltake-ceres-300-tg-argb/images/small@1x.png', class: 'review-carousel-item__link ' },
				{ title: 'RCA Evolution M27PG135F', url: '/review/rca-evolution-m27pg135f-240-hz-gaming-monitor/', ts: '1698757216', small: '/review/rca-evolution-m27pg135f-240-hz-gaming-monitor/images/small.png', small1x: '/review/rca-evolution-m27pg135f-240-hz-gaming-monitor/images/small@1x.png', class: 'review-carousel-item__link ' },
			];

	var reviewCarousel = {
		swiper: undefined,
		init: function() {
			var initialSlide=0;
			var hash = location.hash.substr(1);
			if (hash.startsWith('review-')) {
				hash=hash.split('-');
				hash=hash[1];
				$.each(slides, function(key, val) {
					if (val['ts']!=hash)
						return;
					initialSlide=key;
				});
			}

			this.swiper = new Swiper('.review-carousel__items', {
				slidesPerView: 3,
				spaceBetween: 0,
				initialSlide: initialSlide,
				threshold: 1,
				breakpoints: {	// When updating these, also update the values in css for non-JS
					320: {
						slidesPerView: 3,
					},
					640: {
						slidesPerView: 4,
					},
					768: {
						slidesPerView: 5,
					},
					900: {
						slidesPerView: 3,
					},
					950: {
						slidesPerView: 4,
					},
					1100: {
						slidesPerView: 5,
					},
					1250: {
						slidesPerView: 6,
					},
					1400: {
						slidesPerView: 7,
					},
				},
				virtual: {
					slides: slides,
					renderSlide: this.renderSlide,
				},
				on: {
					beforeInit: function() { $('.review-carousel-item-placeholder').remove(); },
					slideChange: function() { reviewCarousel.addMoreHeightIfThreeLineItemFound(); },
					resize: function() { reviewCarousel.addMoreHeightIfThreeLineItemFound(); },
					imagesReady: function() { reviewCarousel.addMoreHeightIfThreeLineItemFound(); },
				}
			});
			this.swiper.on('progress', function() { reviewCarousel.checkAjax.call(reviewCarousel); });	// set the listener after init, so first init won't trigger ajax check

			$('.review-carousel__button--prev').on('click', function(e) { e.preventDefault(); reviewCarousel.clickPrev(); });
			$('.review-carousel__button--next').on('click', function(e) { e.preventDefault(); reviewCarousel.clickNext(); });

			$('.review-carousel__items').on('click', '.review-carousel-item__link', function() {
				var current=slides[reviewCarousel.swiper.activeIndex]['ts'];
				window.history.replaceState(null, '', '/#review-'+current);
			});

			this.markNewReviews();
			this.workaroundForSmallClickMovement();
		},
		checkAjax: function() {
			var swiper=this.swiper;
			var delta = swiper.virtual.slides.length - swiper.activeIndex;
			if (delta>10)
				return;

			if ($('.review-carousel').hasClass('review-carousel--is-loading'))
				return;

			this.loadMore();
		},
		loadMore: function() {
			var swiper=this.swiper;
			var $container=$('.review-carousel');
			$container.addClass('review-carousel--is-loading');

			var lastId=swiper.virtual.slides[swiper.virtual.slides.length - 1].ts;

			$.getJSON('/lastreviews/'+lastId+'?cnt=20', function(data) {
				var items=[];

				$.each(data, function(key, val) {
					swiper.virtual.appendSlide(val);
				});

				$container.removeClass('review-carousel--is-loading');
			});
		},
		markNewReviews: function() {
			try {
				var key='reviewcarousel_last';
				var time=Math.round((new Date()).getTime() / 1000);

				var lastActivity=parseInt(localStorage.getItem(key));
				localStorage.setItem(key, time);
				if (isNaN(lastActivity))
					lastActivity=0;

				if (lastActivity<time-24*3600)
					lastActivity=time-24*3600;

				$('.review-carousel-item').each(function(i ,e) {
					if ($(this).data('ts')>lastActivity)
						$(this).addClass('review-carousel-item--new');
					if ($(this).data('ts')>time-3600)
						$(this).addClass('review-carousel-item--new');
				});
			} catch(e) {}
		},
		renderSlide: function(slide, index) {
			result='<div class="swiper-slide review-carousel-item" data-ts="'+slide['ts']+'">';
			result+='<a class="'+slide['class']+'" href="'+slide['url']+'">';
			result+='<img class="review-carousel-item__image" alt="" src="'+slide['small1x']+'" width="100" height="100" sizes="100px" srcset="'+slide['small1x']+' 100w, '+slide['small']+' 256w" />';
			result+='<span class="review-carousel-item__title">'+slide['title']+'</span><span class="review-carousel-item__bottom"></span></a></div>';

			return result;
		},
		clickPrev: function() {
			var swiper=this.swiper;
			var limit = swiper.params.slidesPerView != undefined ? swiper.params.slidesPerView : swiper.slidesPerView;
			var target = swiper.activeIndex - limit;
			if (target<=0)
				target=0;
			if (target==swiper.activeIndex)
				return;

			function keepScrolling() {
				if ((swiper.activeIndex>target) && (!swiper.isBeginning))
					swiper.slidePrev(33);
				else {
					$('.review-carousel .swiper-wrapper').css('transition-timing-function', '');
					swiper.off('transitionEnd', keepScrolling);
				}
			}

			$('.review-carousel .swiper-wrapper').css('transition-timing-function', 'linear');
			swiper.on('transitionEnd', keepScrolling);
			swiper.slidePrev(33);
		},
		clickNext: function() {
			var swiper=this.swiper;
			var limit = swiper.params.slidesPerView != undefined ? swiper.params.slidesPerView : swiper.slidesPerView;
			var target= swiper.activeIndex + limit;
			if (target>=swiper.virtual.slides.length)
				return;

			function keepScrolling() {
				if ((swiper.activeIndex<target) && (!swiper.isEnd))
					swiper.slideNext(33);
				else {
					$('.review-carousel .swiper-wrapper').css('transition-timing-function', '');
					swiper.off('transitionEnd', keepScrolling);
				}
			}

			$('.review-carousel .swiper-wrapper').css('transition-timing-function', 'linear');
			swiper.on('transitionEnd', keepScrolling);
			swiper.slideNext(33);
		},
		workaroundForSmallClickMovement: function() {
			var mouseDownData = undefined;

			$('.review-carousel .swiper-wrapper').on('mousedown', 'a', function(e) {
				if (e.which!=1)
					return;
				mouseDownData = e.originalEvent;
			});

			$('.review-carousel .swiper-wrapper').on('mouseup', 'a', function(e) {
				var debug = false;

				if (debug) console.log(e);

				if (e.which!=1)
					return;

				e = e.originalEvent;
				var deltaT = e.timeStamp - mouseDownData.timeStamp;
				if (deltaT>300) {
					if (debug) console.log('deltaT');
					return;
				}

				var deltaX = e.pageX - mouseDownData.pageX;
				if (deltaX > 10) {
					if (debug) console.log('deltaX');
					return;
				}

				var velocity = Math.abs(deltaX / deltaT);
				if (velocity > 0.05) {
					if (debug) console.log('velocity');
					return;
				}

				var $link = $(e.target).closest('a');

				$link.trigger('click');
				window.location = $link.attr('href');
			});
		},
		addMoreHeightIfThreeLineItemFound: function() {
			// Only add more height, never remove it, or carousel gets jumpy during scrolling
			if ($('.review-carousel__items').hasClass('review-carousel__items-has-tall-item'))
				return;

			$el=$('.review-carousel-item__title').filter(function() { return $(this).height()>=30; });

			if ($el.length>0)
				$('.review-carousel__items').addClass('review-carousel__items-has-tall-item');
		},
	};

	loadjs.ready(['jquery', 'swiper'], function() {
		reviewCarousel.init();
	});
</script>

