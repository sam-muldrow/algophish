	function poll_ajax(action, data) {
		$.ajax({
			'url': '/frontpage-poll/'+action,
			'data': data
		}).done(function(msg) {
			$('#frontpagepoll-content').html(msg);
		});
	}

	function getvoteoptions() {
		poll_ajax('options');
	}

	function getvoteresults() {
		poll_ajax('results');
	}

	function votesubmit(form) {
		var value=-1;
		for (var i=0;i<form.vote.length;i++) {
			if (form.vote[i].checked)
				value=form.vote[i].value;
		}
		if (value!=-1) {
			form.submit.disabled=true;
			poll_ajax('vote', { vote: value });
		}
	}
</script>

