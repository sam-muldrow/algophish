				{ title: 'AVerMedia Live Gamer Ultra 2.1 Capture Card', url: '/review/avermedia-live-gamer-ultra-2-1-capture-card/', ts: '1699382428', small: '/review/avermedia-live-gamer-ultra-2-1-capture-card/images/small.png', small1x: '/review/avermedia-live-gamer-ultra-2-1-capture-card/images/small@1x.png', class: 'review-carousel-item__link ' },
				{ title: 'Darmoshark M3S Varun', url: '/review/darmoshark-m3s-varun/', ts: '1699371180', small: '/review/darmoshark-m3s-varun/images/small.png', small1x: '/review/darmoshark-m3s-varun/images/small@1x.png', class: 'review-carousel-item__link ' },
