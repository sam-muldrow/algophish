<!-- <script src="https://cdn.auth0.com/js/auth0-spa-js/1.2/auth0-spa-js.production.js"></script> -->
<script type="text/javascript">
window.addEventListener("load", function() {
var webAuth;
$(document).ready(function () {
webAuth = new auth0.WebAuth({
domain: "auth.subaru.jp",
//redirectUri: "https://ec-dev.subaru.jp/site1/testpage.html",
redirectUri: "https://www.subaru.jp", //"https://www-qa.subaru.jp/levorg/levorg/outTestheader",
clientID: "oTfOr8phTXJ69KH3Y2hF224OjU1X9XvK",
responseType: 'token id_token',
//SCM-ID audiencef
audience: 'https://api.subaru.co.jp',
plugins: [new CordovaAuth0Plugin()]
});
webAuth.checkSession({
nonce: '1234',
}, function (err, authResult) {
console.log(err);
console.log(authResult);
if (typeof (authResult) == "undefined") {
console.log('请登录');
$('.log-box button').attr("style", "display:none;");
} else {
//验证成功
console.log('已登陆，按钮显示');
$('.log-box button').attr("style", "display:inline-block;");
}
});
$('#btnCheckSession').click(function (e) {
e.preventDefault();
webAuth.checkSession({
nonce: '1234',
}, function (err, authResult) {
console.log(err);
console.log(authResult);
});
});
//SCM-ID Universal login
$('#btnLogin').click(function (e) {
e.preventDefault();
let url = webAuth.authorizeSilent({});
console.log(url);
ajax_method(url, {}, "get", function (data) {
console(data);
alert(data);
});
});
//退出代码
$('#btnLogout').click(function (e) {
e.preventDefault();
webAuth.logoutIframe({
returnTo: 'https://www.subaru.jp' //'https://ec-dev.subaru.jp/site1/testpage.html'
});
});
$('#btnLogoutsp').click(function (e) {
e.preventDefault();
webAuth.logoutIframe({
returnTo: 'https://sp.subaru.jp'//'https://ec-dev.subaru.jp/site1/testpage.html'
});
});
});
ajax_method = function(url, data, method, success) {
var ajax = new XMLHttpRequest();
if (method === 'get') {
if (data) {
url += '?';
url += data;
}
ajax.open(method, url);
ajax.send();
} else {
ajax.open(method, url);
ajax.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
if (data) {
ajax.send(data);
} else {
ajax.send();
}
}
ajax.onreadystatechange = function() {
if (ajax.readyState == 4 && ajax.status == 200) {
console.log(ajax.responseText);
// 将 数据 让 外面可以使用
// return ajax.responseText;
// 当 onreadystatechange 调用时 说明 数据回来了
// ajax.responseText;
// 如果说 外面可以传入一个 function 作为参数 success
success(ajax.responseText);
console.log(ajax.responseURL);
}
}
}
});
</script>
