    <section id="contents_pickup">
      <div class="pickup_inner">
	<h2><img src="/img/top/pickup_header_ttl.png?date=20230330" alt="インフォメーション" />
		<div class="btn">
			<a href="/information"><img src="/img/top/top_pick-up_btn.jpg?date=20230330" alt="インフォメーション一覧"></a>
		</div>
	</h2>
