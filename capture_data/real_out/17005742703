<script src="/responsive/js/utils/loginAuth.js?20220207"></script>
<script src="/responsive/js/wwwLogin.js?20191203"></script>

<script>
    $(function(){
        // ú»
        WwwLogin.default();
        Loading.start($(".login"));
        try {
            LoginAuth.getUserInfo()
                .then(function(res) {
                    var path = window.location.pathname.split('/'); 
                    var file_name = path.pop();
                    var info = res.data[0];
                    if(info) {
                        if (file_name == "/" || file_name == "") {
                            WwwLogin.home(info);
                        } else {
                            WwwLogin.general(info);
                        }
                    }
                    Loading.end($(".login"));
                })
                .catch(function(err) {
                    Loading.end($(".login"));
                });
        }
        catch(e) {
            Loading.end($(".login"));
        }
    });
</script>
