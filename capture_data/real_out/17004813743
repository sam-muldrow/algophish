tvline"></a><img src="https://img.artlebedev.ru/svalka/index-line/z-i-n-m-m/m.svg" class="z-ico" />Мокапус – трехмерные мокапы онлайн</div></div></div></div></div>
<script>var indexLine = {
	$line: null,
	$items: null,
	$itemsInner: null,
	x: 0,
	padding: 0,
	init: function () {
		var $l = $('#als-bottom-line'),
			self = this;
		if (!$l.length) return;
		this.$line = $l;
		this.$items = $('.index-line-items', $l);
		this.$itemsInner = $('.index-line-items-inner', $l);
		this.checkFill();
		$(window).on('resize', function () {
			self.checkFill();
		});
		window.setInterval(function () {
			self.onTot()
		}, 2000);
		self.onTot();
		this.videoInit();
	},
	checkFill: function () {
		var lineWidth = this.$line.width(),
			$items = $('.index-line-item', this.$items),
			firstItemWidth = $items.first().width();
		function itemsWidth () {
			var res = 0;
			$('.index-line-item', this.$items).each(function (i) {
				res += $(this).width();
			});
			return res;
		}
		while (lineWidth + firstItemWidth > itemsWidth()) {
			$items.first().clone().appendTo(this.$itemsInner);
		}
	},
	onTot: function () {
		this.x += 100;
		this.$itemsInner.css('transform', 'translateX(' +  (-this.x) + 'px)');

		var $first = $('.index-line-item', this.$itemsInner),
			width = $first.width(),
			self = this;
		if (Math.abs($first.offset().left) >= width) {
			self.padding += width;
			$first.appendTo(this.$itemsInner);
			this.$itemsInner.css('padding-left', self.padding + 'px');
		}
	},
	videoInit: function () {
		var $video = $('video', this.$line);
		if (!$video.length) return;
		var video = $video[0];
		if (video.webkitPlaysInline !== undefined) video.webkitPlaysInline = true;
		if (video.playsInline !== undefined) video.playsInline = true;
		try {video.pause()} catch (e) { }
		try {video.play()} catch (e) { }
	}
}
indexLine.init();</script>
