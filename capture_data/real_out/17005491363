    <script type="text/javascript" class='optanon-category-C0001'>
    function refreshAuthorBio(article_id,auth_bio,auth_url,reviewer_bio,reviewer_url) {
        //Seems like we may not want this CSS rule since the min-width is set to 100%
        if(auth_bio && reviewer_bio) { 
            document.querySelectorAll('#'+article_id+' .auth-info .by-line .by-line-author > *').forEach(function(e) { e.style.width = '49%'});
            //$('#'+article_id+' .auth-info .by-line .by-line-author').children().css('width', '49%');
        }
        if(auth_bio){
            auth_box = document.querySelector('#'+article_id+' #auth-bio');
            auth_bio_text = auth_box.innerText;
            splitContent(auth_bio_text, auth_box, 2, 6, auth_url);
        }
        if(reviewer_bio){
            reviewer_box = document.querySelector('#'+article_id+' #reviewer-bio');
            reviewer_bio_text = reviewer_box.innerText;
            splitContent(reviewer_bio_text, reviewer_box, 2, 6, reviewer_url);
        }
    }

    function splitContent(text, box, maxRow, offset, url = "") {
        var re = /[^\x00-\xff]/g;
        var style = getComputedStyle(box, null);
        var w = parseInt(style.width);
        var mSize = parseInt(style.fontSize);

        var count = Math.floor(w / mSize);
        var hasDouble = text.match(re);
        var len = hasDouble ? (hasDouble.length + text.length) / 2 : text.length / 2;
        var maxSize = count * maxRow;
        if (len > maxSize) {
            for (var i = maxSize; i < text.length; i++) {
                var mText = text.substr(0, i);
                var has = mText.match(re);
                var mLen = has ? (has.length + mText.length) / 2 : mText.length / 2;
                if (mLen >= maxSize - offset) {
                    text = mText + '...' + '<a href="' + url + '">Read More</a>';
                    break;
                }
            }
        }
        box.innerHTML = text;
    }

    if(window.AuthorBio && Object.keys(window.AuthorBio).length > 0) {
        for(let article_id in window.AuthorBio ) {
            if(window.AuthorBio[article_id].proccessed === false) {
                window.AuthorBio[article_id].proccessed = true;
                refreshAuthorBio(
                    window.AuthorBio[article_id].article_id,
                    window.AuthorBio[article_id].auth_bio,
                    window.AuthorBio[article_id].auth_url,
                    window.AuthorBio[article_id].reviewer_bio,
                    window.AuthorBio[article_id].reviewer_url
                );
            }
        }
    }
</script><!-- End Footer Script -->
