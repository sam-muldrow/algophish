	window.dataLayer = window.dataLayer || [];
	new function(){
		'use strict';

		let obj = {};
		let member_id;
		let cookie_key = 'u-demographics';
		if(document.cookie.match(new RegExp(cookie_key + '=([^;]+)'))){
			member_id = RegExp.$1;
		}
		
		if(member_id) obj.member_id = member_id;

		let content_group = getContentGroup();
		let content_group_genre = getContentGroupGenre();
		let content_group_subgenre = getContentGroupSubgenre();

		if(content_group) obj.content_group = content_group;
		if(content_group_genre) obj.content_group_genre = content_group_genre;
		if(content_group_subgenre) obj.content_group_subgenre = content_group_subgenre;

		dataLayer.push(obj);

		function getContentGroup(){
			let path = location.pathname;
			let val;
			if(/^\/(index.html)?$/.test(path)){
				val = "TOP";
			}else if(/^\/(movie|drama|sports|music|stage|anime|documentary|extra)\/(index.html)?$/.test(path)){
				val = "ã¸ã£ã³ã«TOP";
			}else if(/^\/detail\/\d{6}.*$/.test(path)){
				val = "çªçµè©³ç´°";
			}else if(/^\/(program_list|ranking|program|wowow_original|prg_request)\//.test(path)){
				//ä¸å±¤ãå«ã
				val = "çªçµæ¤ç´¢";
			}else if(/^\/schedule\//.test(path)){
				val = "çªçµè¡¨";
			}else if(/^\/(wod|service\/4k|plusw|service)\/(index.html)?$/.test(path)){
				//æå®ã®ãã£ã¬ã¯ããªã®ã¿
				val = "ãµã¼ãã¹ç´¹ä»";
			}else if(/^\/(eventinfo)\//.test(path)){
				//ä¸å±¤ãå«ã
				val = "ãµã¼ãã¹ç´¹ä»";
			}else if(/^\/(support\/tetsuzuki|service\/chatguide)\/(index.html)?$/.test(path)){
				val = "ãµãã¼ã";
			}else if(/^\/join\/\w+/.test(path)){
				val = "å å¥æå";
			}else if(/^\/search\//.test(path)){
				val = "ãµã¤ãåæ¤ç´¢";
			}
			return val;
		}
		function getContentGroupGenre(){
			let path = location.pathname;
			let val;
			if(/^\/movie\//.test(path)){
				val = "æ ç»";
			}else if(/^\/drama\//.test(path)){
				val = "ãã©ã";
			}else if(/^\/sports\//.test(path)){
				val = "ã¹ãã¼ã";
			}else if(/^\/music\//.test(path)){
				val = "é³æ¥½";
			}else if(/^\/stage\//.test(path)){
