					);
				});
				simulContentBox.ready(function() {
					$("header .__ondemand .container .__credit").text(credit);
					if(programLength === 0) {
						$("header .__ondemand .container .__navi").css({"display" : "none"});
						$("nav .__slide-menu .__mod .__head").css({"display" : "none"});
					}
					resolve();
				});
			});
		});

		(function() {
			var renderFlg = false;
			var renderFlgSp = false;
			$("header .__ondemand").hover(function() {
				if(!renderFlg) {
					clipSimulContents();
					renderFlg = true;
				}
			});
			$("nav .__slide-menu .-net-viewing").on("click", function() {
				if(!renderFlgSp) {
