<script type="text/javascript">
	"use strict";
	document.addEventListener("DOMContentLoaded",function(eve){
		var registObj = new XMLHttpRequest();
		registObj.open("get", "/common/header/v1/header_user_info.php", true);
		registObj.onload = function(){
			var registData = JSON.parse(this.responseText);
			//WOWOWãªã³ã©ã¤ã³ã¦ã¼ã¶ã¼å¤å®
			var userFlg = registData.login_flg === "1" ? true : false;
			// console.log("userFlg:" + userFlg);
			var userClass = userFlg?'__closed':'__open';
			//å å¥èå¤å®
			var registFlg = registData.link_flg !== "" ? true : false;
			// console.log("registFlg:" + registFlg);
			var registClass = registFlg?'__registed':'__unregistered';
			document.body.classList.add(userClass, registClass);
		}
		registObj.send(null);

		// ãã©ãã­ã³ã°URLåå¾
		window.globalTrackingUrl = {};
		window.globalTrackingUrl.isAvailable = false;
		var joinLinkUrl = "https://wod.wowow.co.jp/";
		$.ajax({
			type:"POST",
			url: "/API/new_prg/get_tracking_url.php",
			data: JSON.stringify({ "url": getRequestUrl() }),
			contentType: 'application/json',
			dataType:"json",
			timeout : 30000,
			success: function(data) {
				var responseData = data;
				if (responseData &&
					responseData.hasOwnProperty("result") &&
					responseData.result === "0" &&
					responseData.hasOwnProperty("tracking_url") &&
					responseData.tracking_url.length > 0
					) {
					joinLinkUrl = responseData.tracking_url;
				}
				setTrackingUrl(joinLinkUrl);
			},
			error: function(data) {
				setTrackingUrl(joinLinkUrl);
			}
		});

		var resizeTitle = [];
		var spClass = "";
		var pcClass = "";

		// ãããã¼ååæéä¿¡ä¸­çªçµè¡¨ç¤º
		var getSimulContents = new Promise(function(resolve, reject){
			$.ajax({
				type: 'POST',
				url: '/API/new_prg/programlist.php',
				contentType: 'application/json',
				data: JSON.stringify({
					mode: "24"
				}),
				cache: false
			}).done(function (data) {
				if (data.result !== "0") {
					console.error("Error(API) ", data);
					$("header .__ondemand .container .__navi").css({"display" : "none"});
					$("nav .__slide-menu .__mod .__head").css({"display" : "none"});
					return;
				}
				var channelHTML = {
					"191": '<img class="__icn_channel" src="/assets/img/icon/icon_ch_prime.svg">',
					"192": '<img class="__icn_channel" src="/assets/img/icon/icon_ch_live.svg">',
					"193": '<img class="__icn_channel" src="/assets/img/icon/icon_ch_cinema.svg">'
				};

				var channelName = {
					"191": "prime",
					"192": "live",
					"193": "cinema"
				};

				var credit = "";

				var programLength = 0;

				var ww = window.windowWidth || window.innerWidth; // _head_link.ejsã§å®ç¾©ï¼ãã©ã¦ã¶ã®åæç»ãæããããï¼ window.windowWidth = window.innerWidth
				if(ww > 800) {
					var simulContentBox = $("header .__ondemand .container .__content");
					pcClass = "text-orver-flow";
				} else {
					var simulContentBox = $("nav .__slide-menu .__mod .__content");
					spClass = "js-text-elp";
				}

				data.onair_list.forEach(function (value) {
					if(value.simulcast !== "2") {
						return false;
					}
					programLength++;

					var episode = value.story_no && value.story_no !== "0" ? "#" + value.story_no : "";
					episode += value.episode ? " " + value.episode : "";
					//episode = episode !== "" ? "<br>" + episode : episode;
					credit += "ã" + value.title + "ã" + value.credit + " ";

					var simulcastUrl = "";
					if ($('html').hasClass('ios') || $('html').hasClass('android') || $('html').hasClass('android-default') || $('html').hasClass('android-chrome')) {
						simulcastUrl = value.prg_branch_cd
							? '/detail/' + value.prg_cd + '/' + value.prg_branch_cd + '/' + value.prg_mode_branch_cd
							: '/detail/' + value.prg_cd + '/-/' + value.prg_mode_branch_cd;
					} else {
						var wodUrl = "https://wod.wowow.co.jp/simulcast/";
						var channelStr = "";
						switch (value.channel) {
							case "191":
								channelStr = "Prime";
								simulcastUrl = wodUrl + channelStr;
								break;
							case "192":
								channelStr = "Live";
								simulcastUrl = wodUrl + channelStr;
								break;
							case "193":
								channelStr = "Cinema";
								simulcastUrl = wodUrl + channelStr;
								break;
							default:
								// 004: 4Kã¯åæéä¿¡ãªã
								break;
						}
					}

					resizeTitle.push(value.title + episode);

					simulContentBox.append(
						'<a href="' + simulcastUrl + '" class="__item col-8__md-24 js-simulcast" target="_blank" onclick="gaCustom' + '(this, \'send\', \'event\', \'header\', \'Click\', \'wol_pc_simulcast_' + channelName[value.channel] + '\');">' +
						'<div class="row">' +
						'<div class="__thumb col-12 is-hidden-md">' +
						'<img src="' + value.image_a_half + '">' +
						'</div>' +
						'<div class="__desc col-12__md-24">' +
						'<div class="__icn">' + channelHTML[value.channel] + '</div>' +
						'<div class="__text-wrap ' + spClass + ' ">' +
						'<div class=" ' + pcClass + ' ">' +
							value.title +
							episode +
