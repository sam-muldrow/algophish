<script>
$(function(){
	setTimeout(function(){
		$('.unveil').off('unveil').unveil({}, function() {
			this.onerror = function() {
				$(this).removeClass('img-not-loaded');
				$(this).attr('src', 'wpub/default_thumb.png');
				$(this).addClass('img-missing');
			};
			this.onload = function() {
				$(this).addClass('img-loaded');
			};
		});
	}, 250);
});
</script>
