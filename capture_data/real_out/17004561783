        externalLink: "tel:08000310800",
        phoneModalTitle: "Atendimento por telefone",
        phoneNumber: "0800 031 0800",
      },
    },
  };

  addEventListener("focusin", (event) => {
    const target = event.target;
    if (
      isPhoneModalOpen ||
      isModalOpen ||
      (target && target.getAttribute("floating-modal-focus") === "true")
    )
      return;

    lastFocused = target;
  });

  function updateModalFocus(event) {
    const modalFocusElements = [
      ...document.querySelectorAll(
        `[${isPhoneModalOpen ? "phone" : "floating"}-modal-focus="true"]`
      ),
    ];
    const isLastElement =
      event.target == modalFocusElements[modalFocusElements.length - 1];
    const isFirstElement = event.target == modalFocusElements[0];
    const shiftKeyPressed = event.shiftKey;
    if (isFirstElement && shiftKeyPressed) {
      event.preventDefault();
      modalFocusElements[modalFocusElements.length - 1].focus();
    }
    if (isLastElement && !shiftKeyPressed) {
      event.preventDefault();
      modalFocusElements[0].focus();
    }
  }

  document.addEventListener("keydown", function (event) {
    if (event.key === "Tab" && isModalOpen) updateModalFocus(event);
  });

  function openFloatingModal() {
    modalContent.style.display = "flex";
    modalOverlay.style.display = "block";
    allElements.style.overflowY = "hidden";
    isModalOpen = true;
    const modalFocusElements = [
      ...document.querySelectorAll('[floating-modal-focus="true"]'),
    ];
    modalFocusElements[0].focus();
  }

  function closeFloatingModal() {
    allElements.style.overflowY = "initial";
    modalContent.classList.add("floatingButtonContactModalDisappear");
    modalOverlay.classList.add("floatingButtonContactModalOverlayDisappear");
    setTimeout(() => {
      modalContent.classList.remove("floatingButtonContactModalDisappear");
      modalOverlay.classList.remove(
        "floatingButtonContactModalOverlayDisappear"
      );
      modalContent.style.display = "none";
      modalOverlay.style.display = "none";

      changeModalPageTo("INITIAL", true);
      isModalOpen = false;

      lastFocused.focus();
    }, 500);
  }

  function openOrCloseModal() {
    if (!isModalOpen) return openFloatingModal();
    closeFloatingModal();
  }

  function setButtonOnClick(button, onClickFunction) {
    button.removeAttribute("href");
    button.onclick = onClickFunction;
    button.onkeydown = (event) => {
      if (event.key === "Enter") onClickFunction(event);
    };
  }

  function setButtonFlow(button, infos, event) {
    setButtonOnClick(button, (event) => {
      event.preventDefault();
      changeModalPageTo(infos.flowHref);
    });
  }

  function closePhoneModal(event) {
    const phoneModalOverlay = document.querySelector("#phone-modal-overlay");
    const modalFocusElements = [
      ...document.querySelectorAll('[floating-modal-focus="true"]'),
    ];
    phoneModalOverlay.style.display = "none";
    phoneModal.style.display = "none";
    isPhoneModalOpen = false;
    modalFocusElements[0].focus();
  }

  function triggerAnalyticsWhatsappPageviewEvent(buttonName) {
    dataLayer.push({
      event: "page_view",
      page: {
        page_referrer: document.referrer,
        page_location: window.location.href,
        page_title: document.title,
        page_path: `${window.location.origin}${window.location.pathname}`,
        buttonOrigin: buttonName,
      },
    });
  }

  function openPhoneModal(button, infos, event) {
    setButtonOnClick(button, (event) => {
      const phoneModalOverlay = document.querySelector("#phone-modal-overlay");
      event.preventDefault();
      const phoneModalTitle = document.querySelector(
        "#floating-button-phone-title"
      ).children[1];
      const phoneModalCta = document.querySelector(
        "#floating-button-phone-cta"
      );
      const phoneModalNumber = document.querySelector(
        "#floating-button-phone-number"
      );

      const modalFocusElements = [
        ...document.querySelectorAll('[phone-modal-focus="true"]'),
      ];

      phoneModalOverlay.style.display = "block";
      phoneModal.style.display = "flex";

      phoneModalTitle.innerHTML = infos.phoneModalTitle;
      phoneModalNumber.innerHTML = infos.phoneNumber;
      phoneModalCta.href = "tel:" + infos.phoneNumber.replace(" ", 0);

      isPhoneModalOpen = true;
      modalFocusElements[0].focus();

      triggerAnalyticsWhatsappPageviewEvent(infos.name);
    });
  }

  function setButtonHyperLink(button, infos) {
    button.onclick = null;
    button.onkeydown = null;
    button.href = infos.externalLink;
  }

  function conditionalPhoneModal(button, infos) {
    if (window.innerWidth >= 768) {
      openPhoneModal(button, infos);
    } else {
      setButtonHyperLink(button, infos);
    }
  }

  function changeButtonInfos(buttonId, infos) {
    let button = document.querySelector(buttonId);
    [...button.classList].forEach((className) => {
      if (className.substring(0, 4) == "btn_")
        button.classList.remove(className);
    });
    const buttonClass = `btn_${infos.name
      .toLowerCase()
      .replaceAll(" ", "_")
      .replace("/", "_")
      .normalize("NFD")
      .replace(/[\u0300-\u036f]/g, "")}`;
    button.classList.add(buttonClass);
    button.innerHTML = infos.buttonIconSrc + infos.name;

    infos.nextStep(button, infos, event);
  }

  function changeModalPageTo(flowHref, isBackAction = false) {
    let newModalPage = floatingButtonContactModalAllFlow[flowHref];
    let modalTitle = document.querySelector("#floating-button-title");
    modalTitle.classList.add("floatingButtonContactModalChangeTitle");
    setTimeout(() => {
      modalTitle.classList.remove("floatingButtonContactModalChangeTitle");
    }, 1300);

    if (newModalPage) {
      if (!isBackAction) floatingButtonContactModalCurrentFlow.push(flowHref);

      modalTitle.innerHTML = newModalPage.title;
      changeButtonInfos("#floating-button-first-cta", newModalPage.firstBtn);
      changeButtonInfos("#floating-button-second-cta", newModalPage.secondBtn);
    }
  }
  changeModalPageTo("INITIAL");

  function backOrCloseModalFlow() {
    if (floatingButtonContactModalCurrentFlow.length == 1)
      return closeFloatingModal();

    floatingButtonContactModalCurrentFlow.pop();
    changeModalPageTo(
      floatingButtonContactModalCurrentFlow[
        floatingButtonContactModalCurrentFlow.length - 1
      ],
      true
    );
  }
</script>
