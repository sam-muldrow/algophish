<script>
// Logica Abrir e fechar Modal
function openModalEvent(modal = "") {
  if (!modal) {
    const modalId = event.target.getAttribute("data-modal");
    modal = document.getElementById(modalId);
  }
  modal.style.display = "flex";
  trapFocusableElements(modal);
}

function closeModalEvent(modal = "") {
  if (!modal) {
    const modalId = event.target.getAttribute("data-modal");
    modal = document.getElementById(modalId);
  }
  closeModal(modal);
}

function closeModal(modal) {
  if (!modal) {
    const modalId = event.target.getAttribute("data-modal");
    modal = document.getElementById(modalId);
  }
  const previousModalParser = {
    "modal-contato": "floating-button",
    "modal-chat": "modal-contato",
    "modal-telefone": "modal-contato",
  }[modal.id];
  const previousModal = document.getElementById(previousModalParser);
  modal.style.display = "none";
  previousModal && trapFocusableElements(previousModal);
}

function trapFocusableOnClose(modal) {
  let focusableElements = modal.querySelectorAll("input,button:not([disabled])");
  let firstFocusableElement = focusableElements[0];
  firstFocusableElement.focus();
}

function trapFocusableElements(modal) {
  let focusableElements = modal.querySelectorAll("input,button:not([disabled])");
  let firstFocusableElement = focusableElements[0];
  firstFocusableElement.focus();
}

const modalContato = document.querySelector(".modalContato");
const modalChat = document.querySelector(".modalChat");
const modalTelefone = document.querySelector(".telefoneModal");

function modalTrapFocus(modal) {
  return function (e) {
    let focusableElements = modal.querySelectorAll("a,input,button:not([disabled])");
    let isEscPressed = e.key === "Escape";
    let isTabPressed = e.key === "Tab";

    if (isEscPressed) return closeModal(modal);
    if (!isTabPressed) return;
    focusableElements = modal.querySelectorAll("a,input,button:not([disabled])");
    let firstFocusableElement = focusableElements[0];
    let lastFocusableElement = focusableElements[focusableElements.length - 1];
    if (e.shiftKey) {
      if (document.activeElement === firstFocusableElement) {
        lastFocusableElement.focus();
        e.preventDefault();
      }
    } else {
      if (document.activeElement === lastFocusableElement) {
        firstFocusableElement.focus();
        e.preventDefault();
      }
    }
  };
}

modalContato.addEventListener("keydown", modalTrapFocus(modalContato));
modalChat.addEventListener("keydown", modalTrapFocus(modalChat));
modalTelefone.addEventListener("keydown", modalTrapFocus(modalTelefone));

// Logica Modal Chatbot:
const modalChatbot = {
  validateNameInput: function (name) {
    return name;
  },
  validatePhoneInput: function () {
    return this.getPhone().length == 10 || this.getPhone().length == 11;
  },
  validateFields: function (name, phone) {
    const isNameValid = this.validateNameInput(name);
    const isPhoneValid = this.validatePhoneInput(phone);
    return isNameValid && isPhoneValid;
  },
  createPhoneMask: function (phoneNumber) {
    return phoneNumber.replace(/^(\d{2})(\d)/g, `($1) $2`).replace(/(\d)(\d{4})$/, "$1-$2");
  },
  setPhoneMask: function () {
    const phone = this.getPhone();
    phoneInput.value = this.createPhoneMask(phone);
  },
  getDDDAndNumber: function (phone) {
    const ddd = phone.substring(0, 2);
    const number = phone.substring(2);
    return [ddd, number];
  },
  submitForm: function (event) {
    event.preventDefault();
    const name = modalChatbot.getName();
    const fullPhone = modalChatbot.getPhone();
    const areInputsValid = modalChatbot.validateFields(name, fullPhone);
    if (areInputsValid) {
      const [ddd, number] = modalChatbot.getDDDAndNumber(fullPhone);
      let destinationUrl = `https://www.oi.com.br/chatbot/?nome=${encodeURIComponent(
        name
      )}&ddd=${ddd}&telefone=${number}`;
      window.location.href = destinationUrl;
    }
  },
  getName: function () {
    return nameInput.value;
  },
  getPhone: function () {
    return phoneInput.value.replace(/\D/g, "");
  },
  setNameMessage: function (isValid) {
    const nameErrorMessageElement = document.querySelector("#error-name");
    if (!isValid) {
      nameErrorMessageElement.innerHTML = "O Nome é obrigatório";
      nameInput.classList.add("modalChatInputError");
    } else {
      nameErrorMessageElement.innerHTML = "";
      nameInput.classList.remove("modalChatInputError");
    }
  },
  setPhoneMessage: function (isValid) {
    const phoneErrorMessageElement = document.querySelector("#error-phone");
    if (!isValid) {
      phoneErrorMessageElement.innerHTML = "Telefone Inválido";
      phoneInput.classList.add("modalChatInputError");
    } else {
      phoneErrorMessageElement.innerHTML = "";
      phoneInput.classList.remove("modalChatInputError");
    }
    this.setPhoneMask();
  },
  validateNameKeys: function () {
    const name = modalChatbot.getName();
    const isValidName = modalChatbot.validateNameInput(name);
    modalChatbot.setNameMessage(isValidName);
    modalChatbot.enableSubmitButton();
  },
  validatePhoneKeys: function () {
    const phone = modalChatbot.getPhone();
    const isValidPhone = modalChatbot.validatePhoneInput(phone);
    modalChatbot.setPhoneMessage(isValidPhone);
    modalChatbot.enableSubmitButton();
  },
  enableSubmitButton: function () {
    const name = this.getName();
    const phone = this.getPhone();
    const areInputsValid = this.validateFields(name, phone);
    const submitButton = document.getElementById("submit-form-button");
    if (areInputsValid) {
      submitButton.disabled = false;
      submitButton.classList.add("modalChatButtonEnabled");
      submitButton.classList.remove("modalChatButtonDisabled");
    } else {
      submitButton.disabled = true;
      submitButton.classList.add("modalChatButtonDisabled");
      submitButton.classList.remove("modalChatButtonEnabled");
    }
  },
};

const chatForm = document.getElementById("chat-form");
chatForm.addEventListener("submit", modalChatbot.submitForm);

const nameInput = document.getElementById("name");
const phoneInput = document.getElementById("phone");

nameInput.addEventListener("keyup", modalChatbot.validateNameKeys);
phoneInput.addEventListener("keyup", modalChatbot.validatePhoneKeys);

</script><script>
  let ctaIntervalId;
  let bannerIntervalId;
  let bannerGloboPlayIntervalId;

  const handleCTAEventListener = () => {
    const ctaElement = document.querySelector(
      ".institutional-banner__content-cta"
    );
    if (ctaElement) {
      ctaElement.addEventListener("click", () => {
        dataLayer.push({
          event: "select_content",
          content_type: "video",
          content_id: "video_banner_home",
        });
      });
      clearInterval(ctaIntervalId);
    }
  };

  const handleBannerEventListener = () => {
    const bannerElement = document.querySelector(".institutional-banner");
    if (bannerElement) {
      function onVisible(element, callback) {
        new IntersectionObserver((entries, observer) => {
          entries.forEach((entry) => {
            if (entry.isIntersecting) {
              callback(element);
            }
          });
        }).observe(element);
      }
      onVisible(bannerElement, (element) => {
        dataLayer.push({
          event: "view_banner",
          creative_name: "Banner Experiencia de Fibra",
          items: [
            {
              item_id: "banner_home_oi_experiencia_fibra",
              item_category: "banner",
            },
          ],
        });
      });
      clearInterval(bannerIntervalId);
    }
  };

  const handleBannerGloboPlayEventListener = () => {
    const bannerElement = document.querySelector(".globoplay-banner") || document.querySelector(".bannerGuerrilhaSul");
    if (bannerElement) {
      document.querySelector(".globoplay-banner__cta")?.addEventListener("click", () => {
          const offerValue = `${offersComponent.ofertaHero.globoPlayPrefixo},${offersComponent.ofertaHero.globoPlaySufixo}`;
          dataLayer.push({
            event: "select_promotion",
            currency: "BRL",
            value: offerValue,
            items: [
              {
                item_id: offersComponent.ofertaHero.code,
                item_name: `${offersComponent.ofertaHero.name} + GloboPlay`,
                item_brand: "Oi Fibra",
                item_category: "Home",
                promotion_name: "Banner Oi Fibra",
                promotion_id: "banner_oi_fibra",
                price: offerValue,
                quantity: 1,
              },
            ],
          });
        });
      function onVisible(element, callback) {
        new IntersectionObserver((entries, observer) => {
          entries.forEach((entry) => {
            if (entry.isIntersecting) {
              callback(element);
            }
          });
        }).observe(element);
      }
      onVisible(bannerElement, (element) => {
        const isGloboPlay = offersComponent.ofertaHero.prefixo > 70;
        let offerValue;
        let offerName;

        if(isGloboPlay) {
          offerValue = `${offersComponent.ofertaHero.globoPlayPrefixo},${offersComponent.ofertaHero.globoPlaySufixo}`;
          offerName = `${offersComponent.ofertaHero.name} + GloboPlay`
        } else {
          offerValue = `${offersComponent.ofertaHero.prefixo},${offersComponent.ofertaHero.sufixo}`
          offerName = offersComponent.ofertaHero.name
        }

        dataLayer.push({
          event: "view_promotion",
          currency: "BRL",
          value: offerValue,
          items: [
            {
              item_id: offersComponent.ofertaHero.code,
              item_name: offerName,
              item_brand: "Oi Fibra",
              item_category: "Home",
              promotion_name: "Banner Oi Fibra",
              promotion_id: "banner_oi_fibra",
              price: offerValue,
              quantity: 1
            },
          ],
        });

		dataLayer.push({
          event: "view_item_list",
          item_list_id: offerName,
          item_list_name: offerName,
          currency: "BRL",
          value: offerValue,
          items: [
            {
              item_id: offersComponent.ofertaHero.code,
              item_name: offerName,
              item_variant: "Disponível",
              item_category: "Home",
              price: offerValue,
              item_type: "Internet"
            },
          ],
        });
      });
      clearInterval(bannerGloboPlayIntervalId);
    }
  };

  ctaIntervalId = setInterval(handleCTAEventListener, 1000);
  bannerIntervalId = setInterval(handleBannerEventListener, 1000);
  bannerGloboPlayIntervalId = setInterval(
    handleBannerGloboPlayEventListener,
    1000
  );
  
  //push page_view
  dataLayer.push({
    event: "page_view",
    page: {
      page_referrer: document.referrer,
      page_location: window.location.href,
      page_title: document.title,
      page_path: window.location.origin + window.location.pathname,
      buttonOrigin: ""
    }
  });

  function handleSelectPromotion () {
    const offerValue = `${offersComponent.ofertaHero.prefixo},${offersComponent.ofertaHero.sufixo}`;
    dataLayer.push({
      event: "select_promotion",
      currency: "BRL",
      value: offerValue,
      items: [
        {
          item_id: offersComponent.ofertaHero.code,
          item_name: `${offersComponent.ofertaHero.name}`,
          item_brand: "Oi Fibra",
          item_category: "Home",
          creative_name: "fibra_home_banner",
          promotion_name: "Banner Oi Fibra",
          promotion_id: "banner_oi_fibra",
          price: offerValue,
          quantity: 1,
        },
      ],
    });
  }
</script></body></html>