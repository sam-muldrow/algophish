<script>

function handleWhatsappButtonTestAB(shouldRenderTestAB) {
  if (shouldRenderTestAB) {
    document.querySelector("#button-whatsapp-contact-container").style.display ="block";
    document.querySelector("#floating-button").style.display = "none";
  } else {
    document.querySelector("#floating-button").style.display = "block";
    document.querySelector("#button-whatsapp-contact-container").style.display = "none";
  }
}
  
const getOfferWithSVAValue = (offerValue = 0, SVAValue = 0) => {
let SVAPrefix = Math.floor(SVAValue + offerValue);
let SVASufix = Math.floor(
    (SVAValue + offerValue - Math.floor(SVAValue + offerValue)) * 100
);
return { SVAPrefix, SVASufix, SVAValue };
};


offersComponent.apiCityOffers = function(data) { 
    var that = this; 
    var dadosRetorno = {};
    var grupoDeMenus = []; 

    that.setLocalStorage({
        'estado': data.uf, 
        'cidade': data.cityId,
        'cidade-nome': data.city
    })  

    if(!data.error){			


        if(data.data){
            that.ofertas = data.data.offers;
            dadosRetorno = data.data;
        }else{
            that.ofertas = data.offers;
            dadosRetorno = data;
        }  

        var linkOferta = dadosRetorno['salesforceLink']?dadosRetorno['salesforceLink']:data['salesforceLink']; 
        localStorage.setItem('salesforcelink', linkOferta);
        that.ofertas.forEach(function(offer, index) {
            offer.childProducts.forEach(function(item) {
                item["showBeneficio"] = (item.name.toLowerCase() != "taxa de habilitação")
                item["indexPai"] = index
            });

            offer.childProducts[0]["showBeneficio"] = false;
            var valor = offer.amount; //.replace( '.',',').trim().split(',');
            prefixo = Math.trunc(valor);
            decimalPart = valor - Math.trunc(valor);
            sufixo = parseFloat(decimalPart.toFixed(2) * 100) 
            var velocidadeDownload = offer.childProducts[0].velocidadeDownload;
            var velocidadeUpload = that.valorUpload(offer.childProducts[0].velocidadeDownload);

            offer["prefixo"] = prefixo 
            offer["sufixo"] = sufixo 
            offer["velocidadeDown"] = that.velocidade(velocidadeDownload); 
            offer["tipoDown"] = that.velocidadeTipo(velocidadeDownload);
            offer["velocidadeUp"] = that.velocidade(velocidadeUpload);
            offer["tipoUp"] = that.velocidadeTipo(offer["velocidadeUp"]);
            offer["linkCarrinho"] = `${linkOferta}?produtoCode=${offer.code}&speed=${velocidadeDownload}`;
            const globoPlayValue = getOfferWithSVAValue(
                valor,
                data?.addons?.SVOD_GLOBOPLAY?.amount // data?.addons?.SVOD_GLOBOPLAY?.amount
            );
            offer["globoPlayPrefixo"] = globoPlayValue.SVAPrefix;
            offer["globoPlaySufixo"] = globoPlayValue.SVASufix; 

            if (data.heroOffer == offer.code) {
                that.ofertaHero = offer;
                localStorage.setItem('salesforceofertas', offer["linkCarrinho"]);
            }  

            offer["isFeaturedCardOffer"] = (data.featuredCardOffer == offer.code)
            offer["isPromotionalCard"] = (data.featuredCardOffer == offer.code && data["isPromotional"]) 
        })  

        if (data.grupoDeMenus) {
            grupoDeMenus = data.grupoDeMenus;
            if(this.ofertaHero.downloadSpeed === "100" || this.ofertaHero.downloadSpeed === "200") {
              this.ofertaHero = this.ofertas[1];
            }
            that.getBanner(grupoDeMenus, false);
        }

      	handleWhatsappButtonTestAB(data.whatsappButton);
    }else{
        grupoDeMenus = [];
        that.getBanner(grupoDeMenus, true)
    }
}, 

offersComponent.getBanner = function (grupoDeMenus, error) {
    var that = this;
    grupoDeMenus.push('default')
    //console.log( grupoDeMenus ); 
    
    var dados =  grupoDeMenus;
    //console.log('dados ' +  dados ); 
    
    $.each($(".hide-contexto"), function () {
            var contexto = $(this).attr("data-integracao");
            var arrayContexto = contexto.split(",");
            const intersection = dados.filter((item1) => arrayContexto.some((item2) => item1 === item2));
            if ( intersection.length > 0 )
                $(this).removeClass("hide-contexto");				
        });
    

    var ownerDocument = document.implementation.createHTMLDocument('virtual');
    var htmlTeste = $("#template-banners").html()
    var jsonBanner = []
    var bannerText = '' 
    $.each($(htmlTeste, ownerDocument).filter('.banner-home-html'), function (pos, item) {  

        tags = $(item).data("integracao").split(",")
        const intersection = grupoDeMenus.filter((item1) => tags.some((item2) => item1 === item2));


        if (intersection.length > 0) {
            //console.log(intersection)
            titulo = $(htmlTeste, ownerDocument).filter(`#banner-title${pos}`).html()
            nome = $(htmlTeste, ownerDocument).filter(`#banner-nome${pos}`).html()

            antigo = "lumis/portal/file/fileDownload.jsp" 
            novo = "/" + antigo  
            html = $(item).html().replaceAll(antigo, novo);
            jsonBanner.push({ titulo, nome, html })
            return;
        }  

    });

    var htmlTesteTemplate = $("#app1").clone();
    //console.log(htmlTesteTemplate)
    $("#banner").html("") 
    $("#banner").html($(htmlTesteTemplate).html())  

    var app6 = new Vue({ 
        el: '#banner',
        data: { 
            banner: jsonBanner 
        } 
    }); 

    if(!error){
        that.mudarValorBanner(".valor-banda", that.ofertaHero["velocidadeDown"]);
        that.mudarValorBanner(".valor-tipo", that.ofertaHero["tipoDown"]);
        that.mudarValorBanner(".valor-prefixo", that.ofertaHero["globoPlayPrefixo"]);
        that.mudarValorBanner(".valor-sufixo", that.ofertaHero["globoPlaySufixo"]);
        $('.banner-integracao').removeClass('banner-integracao-hidden');

        $.each($(".carrinho-default"), function () {
            var url =  localStorage.getItem('salesforcelinkhome');					
            $(this).attr('href', url );

        });

        $.each($(".carrinho-default-ofertas"), function () {
            var url =  localStorage.getItem('salesforceofertashome');				
            $(this).attr('href', url );

        });
        
    }
    
    $(".pushevent-offer").on("click", function () {		
            var datacontent = $(this).attr("data-context");
            var content = datacontent.split("||");
            jsonSend = {
                event: "home-portal_event",
                eventCategory: content[0]+'_'+content[1],
                eventAction: content[2],
                eventLabel: content[3]
            };		
            dataLayer.push(jsonSend);
            //console.log("json 2 enviado:" + JSON.stringify(jsonSend));	
        });
    
$.each($(".hide-contexto"), function () {
            var contexto = $(this).attr("data-integracao");
            var arrayContexto = contexto.split(",");
            const intersection = dados.filter((item1) => arrayContexto.some((item2) => item1 === item2));
            if ( intersection.length > 0 )
                $(this).removeClass("hide-contexto");				
        });


    initCarousel();
}
