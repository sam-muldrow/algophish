<script>
    function addSejaParceiroMobileVersion() {
      const mobileFooterUL =
        document.querySelector("#menu-rodape-1").children[0];

      const newMobileAnchor = document.createElement("a");
      newMobileAnchor.classList.add("arrow-down");
      newMobileAnchor.classList.add("seja-parceiro-mobile");
      newMobileAnchor.innerText = "Seja Parceiro";

      const newMobileLi = document.createElement("li");
      newMobileLi.classList.add("list-inline-item");
      newMobileLi.appendChild(newMobileAnchor);

      mobileFooterUL.insertBefore(newMobileLi, mobileFooterUL.firstChild);

      const newMobileUL = document.createElement("ul");
      newMobileUL.classList.add("menu-rodape-seja-parceiro-list");
      newMobileUL.style.display = "none";
      newMobileUL.innerHTML = `
        <li><a href="http://oi.com.br/seja-parceiro-oi/televendas/">TeleAgentes</a></li>
        <li><a href="http://oi.com.br/seja-parceiro-oi/porta-a-porta/">Porta a Porta</a></li>
        <li><a href="http://oi.com.br/seja-parceiro-oi/empresas/">Empresarial</a></li>
        `;

      const footer = mobileFooterUL.parentNode.parentNode;
      const ulNextSibling = mobileFooterUL.parentNode.nextSibling;
      footer.insertBefore(newMobileUL, ulNextSibling);
    }
    function addSejaParceiroDesktopVersion() {
      const desktopFooterUL = document.querySelector("#menu-rodape-2");

      const newDesktopLi = document.createElement("li");
      newDesktopLi.classList.add("list-inline-item");
      newDesktopLi.innerHTML = `
          <ul class="menu-rodape-seja-parceiro-list-desktop" style="display: none;">
            <li><a href="http://oi.com.br/seja-parceiro-oi/televendas/">TeleAgentes</a></li>
            <li><a href="http://oi.com.br/seja-parceiro-oi/porta-a-porta/">Porta a Porta</a></li>
            <li><a href="http://oi.com.br/seja-parceiro-oi/empresas/">Empresarial</a></li>
          </ul>
          <a class="arrow-down seja-parceiro-desktop">Seja Parceiro</a>
          `;

      desktopFooterUL.insertBefore(newDesktopLi, desktopFooterUL.firstChild);
    }

    function addEventListernerMobile() {
      document
        .querySelector(".seja-parceiro-mobile")
        .addEventListener("click", (event) => {
          event.preventDefault();
          const elem = document.querySelector(
            ".menu-rodape-seja-parceiro-list"
          );
          elem.style.display == "none"
            ? (elem.style.display = "block")
            : (elem.style.display = "none");
        });
    }

    function addEventListernerDesktop() {
      document
        .querySelector(".seja-parceiro-desktop")
        .addEventListener("click", (event) => {
          event.preventDefault();
          const elem = document.querySelector(
            ".menu-rodape-seja-parceiro-list-desktop"
          );
          elem.style.display == "none"
            ? (elem.style.display = "block")
            : (elem.style.display = "none");
        });
    }
    function addSejaParceiro() {
      addSejaParceiroMobileVersion();
      addEventListernerMobile();
      addSejaParceiroDesktopVersion();
      addEventListernerDesktop();
      document.querySelector("body > main > div > section:nth-child(3) > div:nth-child(1)").innerText = "";
    }
    addSejaParceiro();
  </script></footer>





