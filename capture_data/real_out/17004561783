  var hasFibraXEventListenerBeenAdded = false;

  function triggerClickEvent() {
    const integerPrice = Number(
      document.querySelector(".valor-prefixo").innerText
    );
    const floatPrice = Number(
      document.querySelector(".valor-sufixo").innerText
    );
    const priceValue = integerPrice + floatPrice / 100;
    const price = Number(priceValue.toFixed(2));

    const productCode = offersComponent.ofertaHero.code;

    return (
      window &&
      window.dataLayer &&
      window.dataLayer.push({
        event: "select_promotion",
        currency: "BRL",
        value: price,
        items: [
          {
            item_id: productCode,
            item_name: `${offersComponent.ofertaHero.downloadSpeed} MEGA`,
            price: price,
            promotion_id: productCode,
            promotion_name: `${offersComponent.ofertaHero.downloadSpeed} Mega`,
            location_id: "hero-banner",
          },
        ],
      })
    );
  }

  function initCarousel () {
    const speedElem = document.querySelector(".valor-banda");
    if(speedElem) speedElem.setAttribute("data-blur-value", speedElem.innerText);
    
    const fibraXKnowMoreBtnElem = document.querySelector(".fibrax-know-more-btn")
    if(fibraXKnowMoreBtnElem) fibraXKnowMoreBtnElem.addEventListener("click", function (event) {
      window &&
        window.dataLayer &&
        window.dataLayer.push({
        event: "click",
        page_referrer: document.referrer,
        page_location: window.location.href,
        page_title: "Home Oi",
        page_type: "Home Oi",
        page_path: "/",
        click_type: "banner_btn_fibra-x_saiba-mais",
      });
    });
    
    

    // old function
    myCarousel = document.getElementById("carouselExampleIndicators");
	carouselIndicators = myCarousel.querySelectorAll(".carousel-indicators button .animation span");
	carousel = new bootstrap.Carousel(myCarousel,{  });
	fillCarouselIndicator(1); 
	window.addEventListener("load", function () { 
		fillCarouselIndicator(1);
	}); 

	myCarousel.addEventListener("slide.bs.carousel", function (e) {
		let index = e.to; 
		fillCarouselIndicator(++index); 
	}); 

	$('#carouselExampleIndicators').carousel({
		pause: false,
		interval: 15000, 
	});
  }
