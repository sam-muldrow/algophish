/*** VISIBILITY ***/
.ATheSectionHead {
  width: calc(100% - 0.5rem);
  overflow: hidden;
  display: flex;
  border-color: #353B42;
  padding: 1rem 0.25rem;
}
.ATheSectionHead--light {
  border-color: #E3E4E5;
}
.ATheSectionHead--light .ATheSectionHead__Headline {
  color: #E3E4E5;
}
.ATheSectionHead__Line {
  border-bottom: 2px solid;
  flex-grow: 1;
  position: relative;
  bottom: 0.375rem;
  margin-right: 0.5rem;
  margin-left: 0.5rem;
  border-color: inherit;
}
.ATheSectionHead__Line--right {
  margin-right: 0;
  margin-left: 0.5rem;
}
.ATheSectionHead__Headline {
  font-size: 1.25rem;
  font-weight: 700;
  line-height: 1.2;
  margin: 0;
  /** default and colorTitle='DARK' */
  color: var(--grayDarkerOrLigher);
}
@media (min-width: 1280px) {
.ATheSectionHead__Headline {
    font-size: 1.375rem;
}
}
.ATheSectionHead__Headline--reduced {
  font-size: 1.5rem;
  font-weight: 700;
  line-height: 1.2;
  bottom: 0;
  padding: 0;
}
@media (min-width: 1280px) {
.ATheSectionHead__Headline--reduced {
    font-size: 1.625rem;
}
}

