  var divCollections = $(".swiper-wrapper");
  var container = $(divCollections).find('.swiper-slide');
  var randomDivs = container.get().sort(function() {
    return Math.random() - .5;
  });
  $(divCollections).html(randomDivs);
  var swiper = new Swiper('.swiper-container', {
    autoplay: 4200,
    autoplayDisableOnInteraction: false,
