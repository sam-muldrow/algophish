-breaking-news.js"></script> <script> $(document).ready(function () { if ($('.newsTicker2').length) { $.each($('.newsTicker2'), function (index, element) { $(element).breakingNews({ direction: 'rtl' }); }); } function getRandomInt(min, max) { min = Math.ceil(min); max = Math.floor(max); return Math.floor(Math.random() * (max - min + 1)) + min; } $('.banners-slider').each(function () { var _slides = $(this).find('.banners-slide'); var _slidesCount = _slides.length; if (_slidesCount > 1) { var _activeIndex = getRandomInt(0, _slidesCount - 1); _slides.hide(); _slides.eq(_activeIndex).show(); } }); }); </script> <style> .goog-te-gadget a, .goog-te-gadget{ display: flex; align-items: center; } </style> <script type="text/javascript"> function googleTranslateElementInit() { new google.translate.TranslateElement({pageLanguage: 'ar'},'google_translate_element'); } </script> <script type="text/javascript" src="//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit"></script> </body> </html>