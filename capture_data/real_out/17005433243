$( document ).ready(function() {
	setTimeout(function(){
		$('#Ittefaq_popups').hide(); 
		setCookieMinutes('Ittefaq_popup_confirm','visited',10);
	}, 10000);
});

$('#closeed').live('click', function () {
	$('.mobiledesktopAnchorContainer').animate({height: "0px",padding: "0"}, 1000 );
	$('#closeed').hide(500);
});

//anchor ad interactive
var change_for_bottom_sticky = true;
var sticky_bottom_interactive = function(){
	if($(window).scrollTop() > 600) {
		if( change_for_bottom_sticky ){
			$('.mobiledesktopAnchorContainer').animate({height: "0px",padding: "0"}, 1000 );
			$('.closeed').hide(500);
			change_for_bottom_sticky = false;
		}	
	}
	else{
		if( !change_for_bottom_sticky ){
			$('.mobiledesktopAnchorContainer').animate({height: "100px",padding: "3px 0"}, 1000 );
			$('.closeed').show(500);
			change_for_bottom_sticky = true;
		}
	}
}
$(window).scroll(sticky_bottom_interactive).resize(sticky_bottom_interactive);

