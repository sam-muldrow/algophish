		$('.mobiledesktopAnchorContainer').html(anchorAdDisplayAdbeliv+closeButtonAdBlive);
	} else {
		$('.mobiledesktopAnchorContainer').html(anchorAdDisplayAdbeliv+closeButtonAdBlive);
	}
