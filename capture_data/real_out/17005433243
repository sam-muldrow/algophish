				_jw_progresssive_load_data[323] = "<div class=\"inner\"><div id=\"div_324\" class=\"p_d        _col\"><div class=\"inner\"><div id=\"widget_326\" class=\"widget_color_ widget_wrap\"><div class=\"contents_pagetab    widget\">\t\t\t<div id=\"page_tabs_326\" class=\"page_content_tabs\">\r\n\t\t\t\t<div class=\"tabheadings\"><div class=\"each_heading active\" data-index=\"1\">\u09aa\u09cd\u09b0\u099c\u09a8\u09cd\u09ae<\/div><div class=\"each_heading \" data-index=\"2\">\u0995\u09cd\u09af\u09be\u09ae\u09cd\u09aa\u09be\u09b8<\/div><div class=\"each_heading \" data-index=\"3\">\u099a\u09be\u0995\u09b0\u09bf<\/div><\/div>\r\n\t\t\t\t<div class=\"tabitems\">\t\t\t<div class=\"each_block datatab-1 active\">\r\n\t\t\t\t<div class=\"row\">\r\n\t\t\t\t\t<div class=\"col col2\">\r\n\t\t\t\t\t\t<div class=\"col_in\">\r\n\t\t\t\t\t\t\t\t\t<div class=\"contents  summery_view \r\n\t\t \r\n\t\t\t \r\n\t\t\t \r\n\t\t\t \r\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\r\n\t\t\t\r\n\t\t\t\t\t\t\t\t\tbig_content_title\t\t\t\t\t\t\r\n\t\t\t\r\n\t\t\t\t\t\t\r\n\t\t\t \r\n\t\t\t\">\r\n\t\t\t<div class=\"row\"><div class=\"col col1\">\t\t<div class=\"each col_in \r\n\t\t\thas_image \r\n\t\t\timage_top \r\n\t\t\tcontent_capability_blog \r\n\t\t\tcontent_type_news \r\n\t\t\tresponsive_image_hide_\t\t\t \r\n\t\t\t\t\t\t \r\n\t\t\t\t\t\t \r\n\t\t\t\t\t\t\t\t\t\r\n\t\t\t\t\t\">\r\n\t\t\t\r\n\r\n\r\n\t\t\t\r\n\t\t\t\t\t\t\r\n\t\t\t<!--image-->\r\n\t\t\t\t\t\t\t<div class=\"image\">\r\n\t\t\t\t\t\t\t\t\t\t\t<span class=\"jwImgWrap\" style=\"padding-bottom:83.333333333333%\">\r\n\t\t\t\t\t\t\t<span id=\"ari-image-jw655c3b1b84afd\" data-ari=\"{&quot;path&quot;:&quot;media\\\/2023\\\/11\\\/20\\\/53aac04a2eedc4569f2f05540ebb6830-655ae347d4d4d.jpg?jadewits_media_id=140513&quot;,&quot;ratio&quot;:[300,250],&quot;alt&quot;:&quot;\\u099a\\u09cd\\u09af\\u09be\\u099f\\u099c\\u09bf\\u09aa\\u09bf\\u099f\\u09bf\\u2019\\u09b0 \\u09b8\\u09bf\\u0987\\u0993 \\u09ae\\u09bf\\u09b0\\u09be \\u09ae\\u09c1\\