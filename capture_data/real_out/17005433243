				_jw_progresssive_load_data[286] = "<div class=\"inner\"><div id=\"div_287\" class=\"p_d        _col\"><div class=\"inner\"><div id=\"wrapper_290\" class=\"wrapper special_33_3_66_7      \"><div class=\"hf_wrap\"><div class=\"color_theme_\"><div class=\"headbar\"><a href=\"https:\/\/www.ittefaq.com.bd\/national\">\u099c\u09be\u09a4\u09c0\u09df<\/a><\/div><\/div><\/div><div class=\"inner\"><div id=\"div_291\" class=\"p_d_d  brash      _col\"><div class=\"inner\"><div id=\"widget_293\" class=\"widget_color_ widget_wrap\"><div class=\"contents_listing    widget\">\t\t<div id=\"contents_293_ajax_container\" class=\"contents  summery_view \r\n\t\t\t \r\n\t\t\t \r\n\t\t\t \r\n\t\t\t \r\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t\r\n\t\t\t\t\t\t\t\t\t\r\n\t\t\t\t\t\t\t\t\tbig_content_title\t\t\t\t\t\t\r\n\t\t\t\r\n\t\t\t\t\t\t\r\n\t\t\t \r\n\t\t\t\">\r\n\t\t\t<div class=\"row\"><div class=\"col col1\">\t\t<div class=\"each col_in \r\n\t\t\thas_image \r\n\t\t\timage_top \r\n\t\t\tcontent_capability_blog \r\n\t\t\tcontent_type_news \r\n\t\t\tresponsive_image_hide_\t\t\t \r\n\t\t\tm_show_image_as_top\t\t\t \r\n\t\t\t\t\t\t \r\n\t\t\t\t\t\t\t\t\t\r\n\t\t\t\t\t\">\r\n\t\t\t\r\n\r\n\r\n\t\t\t\r\n\t\t\t\t\t\t\r\n\t\t\t<!--image-->\r\n\t\t\t\t\t\t\t<div class=\"image\">\r\n\t\t\t\t\t\t\t\t\t\t\t<span class=\"jwImgWrap\" style=\"padding-bottom:56.25%\">\r\n\t\t\t\t\t\t\t<span id=\"ari-image-jw655c3b1b68f7a\" data-ari=\"{&quot;path&quot;:&quot;media\\\/2023\\\/08\\\/19\\\/0a095f5ac806c62ce875ca924ec01a23-64e0dada8b2e5.jpg?jadewits_media_id=123331&quot;,&quot;ratio&quot;:[16,9],&quot;alt&quot;:&quot;\\u099b\\u09ac\\u09bf: \\u09b8\\u0982\\u0997\\u09c3\\u09b9\\u09c0\\u09a4&quot;,&quot;caption&quot;:&quot;\\u099b\\u09ac\\u09bf: \\u09b8\\u0982\\u0997\\u09c3\\u09b9\\u09c0\\u09a4&quot;}\">\r\n\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t<\/span>\r\n\t\t\t\t\t\t<\/span>\r\n\t\t\t\t\t\t\t\t\t\t\t\t<script data-id=\"ari-image-jw655c3b1b68f7a\">\r\n\t\t\t\t\t\t\t\tjwARI.fetch