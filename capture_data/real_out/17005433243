	if(cat_Name_Pick == 'home'){
		anchorAdSlotName = 'Ittefaq_HP_Anchor_Mobile';
	} else {
		anchorAdSlotName = 'Ittefaq_Inner_Anchor_Mobile';
	}
} else {
	if(cat_Name_Pick == 'home'){
		anchorAdSlotName = 'Ittefaq_HP_Anchor';
	} else {
		anchorAdSlotName = 'Ittefaq_Inner_Anchor';
	}
}

