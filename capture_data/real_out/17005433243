\t} else {\r\n\t\t\/\/alert(\"Please Select Division\");\r\n\t}\r\n\r\n});\r\n\r\n$(\"#get_news_division\").on('click',function(){\r\n\tvar bt_divi = $(\"#division\").val();\r\n\tvar bt_dist = $(\"#district\").val();\r\n    var bt_subdist = $(\"#subdistrict\").val();    \r\n\tif(bt_divi){\r\n\t\tif(bt_dist){\r\n            if(bt_subdist){\r\n\t\t\t    window.location.href=jw_bt_dist_url+bt_subdist+\"?divs=\"+bt_divi+'&dist='+bt_dist+'&subdis='+bt_subdist;\r\n            } else{\r\n                window.location.href=jw_bt_dist_url+bt_dist+\"?divs=\"+bt_divi+'&dist='+bt_dist+'&subdis='+bt_subdist;\r\n            }\r\n\t\t} else{\r\n\t\t\twindow.location.href=jw_bt_dist_url+bt_divi+\"?divs=\"+bt_divi+'&dist='+bt_dist+'&subdis='+bt_subdist;\r\n\t\t}\r\n\t} else {\r\n\t\talert(\"Please Select Division\");\r\n\t}\r\n    return false;\r\n});\r\n$('.divisional_input').parent().parent().parent().parent().css({'z-index':'0'});\r\n\r\nvar jw_bt_vs = window.location.href.split('divs=')[1];\r\n\r\nif( jw_bt_vs ){\r\n    jw_bt_vs = jw_bt_vs.split('&dist=');\r\n}\r\n\r\nif( jw_bt_vs ){\r\njw_bt_vs[0] = decodeURIComponent(jw_bt_vs[0]);\r\njw_bt_vs[1] = decodeURIComponent(jw_bt_vs[1]);\r\nif( jw_bt_vs[0] ){\r\n    $('#division').val(jw_bt_vs[0]).change();\r\n    $('#district').val(jw_bt_vs[1]);\r\n    $('.divisional_input').show();\r\n    }\r\n}\r\n\r\n\r\n\r\n\r\nvar jw_bt_bangladesh_pages = {'bangladesh':'bangladesh','dhaka':'dhaka','khulna':'khulna','chattogram':'chattogram','barisal':'barisal','mymensingh':'mymensingh','rangpur':'rangpur','rajshahi':'rajshahi','sylhet':'sylhet'}\r\n\r\nvar jw_bt_page = window.location.href.split('www.ittefaq.com.bd')[1];\r\n\r\nif( jw_bt_bangladesh_pages[jw_bt_page] ){\r\n    $('.divisional_input').show();\r\n}\r\n\r\n$('.divisional_input').show();\r\n\r\n<\/script><\/div>\r\n\t\t\t\t<\/div>\r\n\t\t\t<\/div><\/div><div id=\"widget_436\" class=\"widget_color_ widget_wrap\"><div class=\"section_header    widget\">\t\t<div class=\"titlebar \r\n\t