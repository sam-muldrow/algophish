<script>
// inner page's extra article's ad functions
//a2
var adSlotInnerA2C1, adSlotInnerA2T1, adSlotInnerA2R1, adSlotInnerA2R2;
function moreAd_Ittefaq_A2() {
	googletag.cmd.push(function() {
		adSlotInnerA2T1 = googletag.defineSlot('/21687517116/Ittefaq_Inner_A2_T1', [[970, 90], [728, 90], [320, 100], [320, 50]])
			.addService(googletag.pubads());
		adSlotInnerA2T1.defineSizeMapping(mapping_970x90);
		
		adSlotInnerA2C1 = googletag.defineSlot('/21687517116/Ittefaq_Inner_A2_C1', [300, 250])
			.addService(googletag.pubads());
		
		adSlotInnerA2R1 = googletag.defineSlot('/21687517116/Ittefaq_Inner_A2_R1', [300, 250])
			.addService(googletag.pubads());
		
		adSlotInnerA2R2 = googletag.defineSlot('/21687517116/Ittefaq_Inner_A2_R2', [300, 250])
			.addService(googletag.pubads());

		let divInnerA2T1 = document.createElement('div');
		let divInnerA2C1 = document.createElement('div');
		let divInnerA2R1 = document.createElement('div');
		let divInnerA2R2 = document.createElement('div');
		divInnerA2T1.id = adSlotInnerA2T1.getSlotElementId(); // auto-generated by GPT
		divInnerA2C1.id = adSlotInnerA2C1.getSlotElementId(); // auto-generated by GPT
		divInnerA2R1.id = adSlotInnerA2R1.getSlotElementId(); // auto-generated by GPT
		divInnerA2R2.id = adSlotInnerA2R2.getSlotElementId(); // auto-generated by GPT
		document.getElementById('iT_inner_A2_T1_cont').appendChild(divInnerA2T1);
		document.getElementById('iT_inner_A2_C1_cont').appendChild(divInnerA2C1);
		document.getElementById('iT_inner_A2_R1_cont').appendChild(divInnerA2R1);
		document.getElementById('iT_inner_A2_R2_cont').appendChild(divInnerA2R2);
		googletag.pubads().disableInitialLoad();
		googletag.enableServices();
		googletag.display(adSlotInnerA2T1);
		googletag.display(adSlotInnerA2C1);
		googletag.display(adSlotInnerA2R1);
		googletag.display(adSlotInnerA2R2);
	});
}
