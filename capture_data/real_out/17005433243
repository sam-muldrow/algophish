<script>
//ad codes
//details page ad codes
//article 1 after image
let ittefaqInnerA1C1 = '<div class="ittefaq_Inner_A1C1 aligncenter pt10 pb10 w100"><div class="aw300">'
	+"<div id='Ittefaq_Inner_Article1_Content1'>"
		+'<scr'+'ipt>'
			+"googletag.cmd.push(function() { googletag.display('Ittefaq_Inner_Article1_Content1');});"
		+'</scr'+'ipt>'
