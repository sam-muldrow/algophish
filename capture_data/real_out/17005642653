    <script>
    $(document).ready(function() {
        function sleep(time) {
            return new Promise((resolve) => setTimeout(resolve, time));
        }

        async function demo() {

            var i;
            for (i = 1; i < 9; i++) {
                var str = $(".diwali" + i).html();

                randnumber = Math.floor(Math.random() * 2) + 1;

                $(".diwali" + i).html("<img src=/img/diwalicracker" + randnumber + ".gif>");

                await sleep(1500);



                $(".diwali" + i).text(str);



            }
            //demo();
        }

        //demo();
    });
    </script>
    <script>
    function linkdiv() {
        $(".triggermodal").on("click", function(e) {
            e.preventDefault();
            $("#modalframe").html("<p align='center'><img src=/img/loading.gif></p>");
            var thisuri = $(this).attr("href");
            var modname = $(this).attr("modname");
            $("#modalframe").html('<iframe src=' + thisuri + ' width="100%" height="520px" frameborder="0" allowtransparency="true"></iframe>');
            $("#exampleModalLabel").html(modname);
            $('#newModal').modal('show');
            $("#newModal").unbind("hidden.bs.modal");
        });
    }


    $(document).ready(function() {
        linkdiv();
        $("a[href*='https://']:not([href*='https://www.caclubindia'],[href*='https://news.caclubindia'])").attr("target", "_blank");
        // validate the comment form when it is submitted
        // $("#commentForm").validate();
        //setTimeout(function(){  $("#gsc-i-id1").attr("placeholder", "Site Search");}, 3000);
        //$("#sidebar").mCustomScrollbar({        theme: "minimal"    });
        $('#dismiss, .overlay-sidebar').on('click', function() {
            $('#sidebar').removeClass('active');
            $('.overlay-sidebar').removeClass('active');
        });
        $('#sidebarCollapse').on('click', function() {
            $('#sidebar').addClass('active');
            $('.overlay-sidebar').addClass('active');
            $('.collapse.in').toggleClass('in');
            $('a[aria-expanded=true]').attr('aria-expanded', 'false');
        });

        // $(".mega-dropdown").click(function() {
        //     window.location = "/coaching/"
        // }), $(".incomesstax").click(function() {
        //     window.location = "/category/income-tax.asp"
        // }), $(".accountancy").click(function() {
        //     window.location = "/category/accounts.asp"
        // }), $(".corporatelaw").click(function() {
        //     window.location = "/category/corporate-law.asp"
        // }), $(".audit").click(function() {
        //     window.location = "/category/audit.asp"
        // }), $(".gst").click(function() {
        //     window.location = "/category/gst.asp"
        // }), $(".students").click(function() {
        //     window.location = "/category/students.asp"
        // });
        $(".myBox").click(function() {
            window.location = $(this).find("a").attr("href");
            return false;
        });
        $(".myBox_blank").click(function() {
            window.open($(this).find("a:first").attr("href"));
            return false;
        });
    });
    </script>
