	var f = document.createElement('script');
	f.type = 'text/javascript';
	f.async = true;
	f.src = (location.protocol === 'https:' ? 'https:' : 'http:') + '//cdn.insurads.com/bootstrap/EMOIYLRT.js';
	s.parentNode.insertBefore(f, s);
}
