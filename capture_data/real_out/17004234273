    <script type="text/javascript">
        window.addEventListener("load", function () {
            var getUrl = window.location.href;
            var amp = false;
            if (getUrl.indexOf('facet=amp') != -1) {
                amp = true;
            }

            var ofrecidoUnit = document.getElementsByClassName("patrocinio-especial-ofrecidopor-container");

            if (ofrecidoUnit.length > 0 && amp === false) {
                var s = document.createElement('span');
                s.innerHTML = '<div class="ofrecido-por-text-pre2 sponsor-text" wf-group="board_free_html_ads"><span class="sponsor-text" style="color:#000000">Ofrecido por:</span></div>';
                s.className = 'ofrecido-por-text-large';
                ofrecidoUnit[0].parentNode.insertBefore(s, ofrecidoUnit[0]);
            }
            var ofrecidoUnitAmp = document.getElementsByClassName("patrocinio-especial-mobile-ofrecidopor-container");
            if (ofrecidoUnitAmp.length > 0 && amp === true) {
                var s = document.createElement('span');
                s.innerHTML = '<div class="ofrecido-por-text-amp sponsor-text" wf-group="board_free_html_ads"><span class="sponsor-text" style="color:#000000">Ofrecido por:</span></div>';
                s.className = 'spec-text-amp';
                ofrecidoUnitAmp[0].parentNode.insertBefore(s, ofrecidoUnitAmp[0]);
            }
        });
    </script>
