                    <td data-iso-com="ES-CE"><span class="flagicon"><img alt="Bandera de Ceuta" src="https://t.resfu.com/media/widgets/img/flags/ceuta-20.png" width="20" height="13" class="thumbborder" srcset="https://t.resfu.com/media/widgets/img/flags/ceuta-30.png 1.5x, https://t.resfu.com/media/widgets/img/flags/ceuta-40.png 2x"  /></span> <a href="/zona/ceuta" title="Ceuta">Ceuta</a></td>
                </tr>
                <tr>
                    <td data-iso-com="ES-ML"><span class="flagicon"><img alt="Bandera de Melilla" src="https://t.resfu.com/media/widgets/img/flags/melilla-20.png" width="20" height="13" class="thumbborder" srcset="https://t.resfu.com/media/widgets/img/flags/melilla-30.png 1.5x, https://t.resfu.com/media/widgets/img/flags/melilla-40.png 2x" /></span> <a href="/zona/melilla" title="Melilla">Melilla</a></td>
                </tr>
            </table>
        </div>

    </div>
    <!-- end Gallery -->

    <!-- footer del widget -->
    <div class="clearfix"></div>
    <!-- end footer del widget --></div><script>




function rf_map_functions562121032() {
    var spain_map_parent562121032 = document.getElementById('widget-562121032');
    var spain_map = document.getElementById('spain_map562121032');
    var spain_map_areas = spain_map_parent562121032.getElementsByTagName('area');
    var spain_map_img = document.getElementById('countryImage562121032');

    for(sma=0;sma<spain_map_areas.length;sma++){
        var sma_area = spain_map_areas[sma];

        map_original_x = 393;
        map_original_y = 316;

        map_new_x = spain_map_img.offsetWidth;
        map_new_y = spain_map_img.offsetHeight;

        var coords = sma_area.getAttribute('coords');
        coords = coords.split(',');

        var new_coords = '';
        for(c=0;c<coords.length;c++) {
            if(c%2==0) {
                var tmp = parseInt( parseInt(parseInt(coords[c])*map_new_x)/map_original_x );
            } else {
                var tmp = parseInt( parseInt(parseInt(coords[c])*map_new_y)/map_original_y );
            }
            new_coords = (new_coords==''?tmp:new_coords+','+tmp);
        }
        sma_area.setAttribute('coords', new_coords);

        sma_area.onmouseover=function(n){
            deactive_province562121032();
            var target = n.target || n.srcElement;

            if(target)
            {
                var data_iso = target.getAttribute('data-iso');
                var data_isocom = target.getAttribute('data-iso-com');
            }

            active_province562121032(data_iso, false, data_isocom);
        }
    }

     for(sma=0;sma<spain_map_areas.length;sma++){
        var sma_area = spain_map_areas[sma];

        sma_area.onmouseout=function(n){
            deactive_province562121032();
        }
    }

    function deactive_province562121032() {
        var spain_map_map = spain_map_parent562121032.getElementsByClassName('spain-map');
        spain_map_map_div = spain_map_map[0].getElementsByTagName('div');
        for(smmd=0;smmd<spain_map_map_div.length;smmd++){
            var smmd_class = spain_map_map_div[smmd].getAttribute('class');
            spain_map_map_div[smmd].setAttribute('class', smmd_class.replace(' hover','').replace(' disabled','').replace(' com','') );
        }
    }

    function active_province562121032(data_iso, provinces, data_iso_com) {
        var spain_map_map = spain_map_parent562121032.getElementsByClassName('spain-map');

        var spain_map_map_class = spain_map_map[0].getAttribute('class');
        var spain_map_map_id = spain_map_map[0].getAttribute('id');
        var spain_map_map_divs = spain_map_map[0].getElementsByTagName('div');
        if(provinces || spain_map_map_class.indexOf('provinces')>=0) {
            var data_iso_com = provinces===true?data_iso:data_iso_com;
            if(provinces)
                data_iso = false;
            for(smmd=0;smmd<spain_map_map_div.length;smmd++){
                var smmd_class = spain_map_map_div[smmd].getAttribute('class');
                var smmd_dataiso = spain_map_map_div[smmd].getAttribute('data-iso');
                var smmd_dataiso_com = spain_map_map_div[smmd].getAttribute('data-iso-com');

                if(smmd_dataiso==data_iso)
                    spain_map_map_div[smmd].setAttribute('class', smmd_class + ' hover');
                else
                    if(data_iso_com) {
                        if( smmd_dataiso!=data_iso && smmd_dataiso_com!=data_iso_com )
                            spain_map_map_div[smmd].setAttribute('class', smmd_class + ' disabled');
                        if( smmd_dataiso_com==data_iso_com )
                            spain_map_map_div[smmd].setAttribute('class', smmd_class + ' com');
                    }
                    else
                    {
                        spain_map_map_div[smmd].setAttribute('class', smmd_class + ' disabled');
                    }
            }
        } else {
            for(smmd=0;smmd<spain_map_map_div.length;smmd++){
                var smmd_class = spain_map_map_div[smmd].getAttribute('class');
                var smmd_dataiso = spain_map_map_div[smmd].getAttribute('data-iso');
                var smmd_dataiso_com = spain_map_map_div[smmd].getAttribute('data-iso-com');

                if(smmd_dataiso==data_iso)
                    spain_map_map_div[smmd].setAttribute('class', smmd_class + ' hover');
                else
                    if(data_iso_com){
                        if(smmd_dataiso!=data_iso && smmd_dataiso_com!=data_iso_com )
                            spain_map_map_div[smmd].setAttribute('class', smmd_class + ' disabled');
                    }
                    else
                    {
                        spain_map_map_div[smmd].setAttribute('class', smmd_class + ' disabled');
                    }
            }

        }
    }


    var spain_map_links = spain_map_parent562121032.getElementsByClassName('map-links');
    var spain_map_links_td = spain_map_parent562121032.getElementsByTagName('td');
    for(sml=0;sml<spain_map_links_td.length;sml++){
        var sm_link = spain_map_links_td[sml];
        sm_link.onmouseover=function(n){
            deactive_province562121032();
            var data_isocom = n.currentTarget.getAttribute('data-iso-com');
            active_province562121032(data_isocom, true, data_isocom);
        }

    }

    for(sml=0;sml<spain_map_links_td.length;sml++){
        var sm_link = spain_map_links_td[sml];

        sm_link.onmouseout=function(n){
            deactive_province562121032();
        }

    }

}

window.addEventListener('load', rf_map_functions562121032);
</script>	</div>
