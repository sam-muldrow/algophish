<script type="text/javascript">

function selDisrights(){
	var marval=document.getElementById('hid_dt_vlue').value;
	var n=marval;
	document.getElementById("seldlt").className="feed-carousel-control feed-left";
	//document.getElementById('selPre').style.display='block';
	if(marval>=1){
		//document.getElementById('selNex').style.display='none';
		document.getElementById("seldrd").className="feed-carousel-control feed-right feed-control-disabled";
	}
	if(marval<=1)
	{
		
	marval=parseInt(marval)+1;
	document.getElementById('hid_dt_vlue').value=marval;
	marval=(marval*580)-580;
	$('#content5').animate({
     marginLeft: "-"+marval+"px"						
  });
	}
	n=parseInt(n)+1;
	ajxn_dist(n);
}
 

function selDislefts(){
	var marval=document.getElementById('hid_dt_vlue').value;
	if(marval<=3){
		document.getElementById("seldrd").className="feed-carousel-control feed-right";
			//document.getElementById('selNex').style.display='block';
	}
	if(marval<=2){
		//document.getElementById('selPre').style.display='none';
		document.getElementById("seldlt").className="feed-carousel-control feed-left feed-control-disabled";
	}
	if(marval>1)
	{
	marval=parseInt(marval)-1;
	document.getElementById('hid_dt_vlue').value=marval;
	marval=(marval*580)-580;
	$('#content5').animate({
     marginLeft: "-"+marval+"px"						
  });
	}
	
}

function gethttpdist(){try{httpdist=new XMLHttpRequest}catch(a){try{httpdist=new ActiveXObject("Msxml2.XMLHTTP")}catch(a){try{httpdist=new ActiveXObject("MICROSOFT.XMLHTTP")}catch(a){httpdist=null,alert("your browser doesnot support ajax")}}}}
var n;
function ajxn_dist(n)
{
		//document.getElementById("selload").innerHTML="<div align='center'><img src='//stat.dinamalar.com/new/2012/images/process.gif'></div>";
	var today = new Date();
	var datet = today.getFullYear()+'-'+(today.getMonth()+1)+'-'+today.getDate()+'-'+today.getHours();
	var dstajxhidfin=document.getElementById('dstajxhidfin'+n).value;
	if((n==2 || n==3) && dstajxhidfin==0){
			document.getElementById("selload").className="selload";

  	httpdist.open("GET","district_section_ajx.asp?n="+n+"&dt="+datet,true);
	httpdist.send(null);
  	httpdist.onreadystatechange=function(){
	if(httpdist.readyState==4){
	document.getElementById("selload").className="";
	   document.getElementById('dstcntdiv').innerHTML=document.getElementById('dstcntdiv').innerHTML+httpdist.responseText;
	   document.getElementById('dstajxhidfin'+n).value='1';
			}
	   }
	}
	
}
</script>
