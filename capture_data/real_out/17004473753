<script type="text/javascript">
var trndtime;
function selRRight(){
	var marval=document.getElementById('temple').value;
	marval=parseInt(marval)+1;
	if(marval==7){marval=0;
		document.getElementById("selTemple").style.marginLeft = "0px";
		document.getElementById('temple').value=marval;
	}else{
		document.getElementById('temple').value=marval;
	marval=(marval*350)-350;
	$('#selTemple').animate({
     marginLeft: "-"+marval+"px"						
  },1500);
	}
	trndtime=setTimeout("selRRight()",5000);
}
var trndtime;
function MMrasi(a)
{	if(a==1){
		clearTimeout(trndtime);
		}
	else{
		clearTimeout(trndtime);
			trndtime=setTimeout("selRRight()",1000);
		}
}
</script>
