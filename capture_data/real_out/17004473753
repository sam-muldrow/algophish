        ]
    }
});
			}
		}
	}
	}
}
function fn_rnvd(){
	var cnvd=document.getElementById('rnvd').value;
	if(cnvd==0){
		document.getElementById('rinvdo1').style.display='none';
		document.getElementById('rivdo1').style.display='block';
		document.getElementById('rnvd').value=1;
		/*if(mobyes==1) {
		fullpath="";
		}else{*/
		fullpath="";
		//}
		var vras_id=document.getElementById('vras_id').value;	
			var sourcelnk=document.getElementById('plysrc').value;
			
			if (sourcelnk.indexOf("www.youtube") >=0) {
		document.getElementById('youtubeElementrasi').style.display ="block";
		document.getElementById('myElementrasi').style.display ="none";
	document.getElementById('youtubeElementrasi').innerHTML='<iframe width="325" height="195" src="'+sourcelnk+'?rel=0&showinfo=0&autoplay=1" allow="autoplay"  frameborder="0" allowfullscreen></iframe>';
	}
	else{
document.getElementById('youtubeElementrasi').style.display ="none";
