<script type="text/javascript">
var Astrovdo;
function astroajaxvdo(Astrovdo){
	var today = new Date();
	var datet = today.getFullYear()+'-'+(today.getMonth()+1)+'-'+today.getDate()+'-'+today.getHours();
	http.open("GET","astroAjaxvdo.asp?cat="+Astrovdo+"&dt=r"+datet,true);
	http.send(null);
	http.onreadystatechange=function(){
		if(http.readyState==4) {
			document.getElementById('astrodata').innerHTML=http.responseText;
			if(document.getElementById('rasvdos').value=='1'){
		/*if(mobyes==1) {
		fullpath=document.getElementById('rsimobpth').value;
		}else{*/
		fullpath=document.getElementById('rsivpath').value;
		//}
		var vrasa_id=document.getElementById('vrasa_id').value;	
		
		var sourcelnk=fullpath;
			
			if (sourcelnk.indexOf("www.youtube") >=0) {
		document.getElementById('youtubeElementrasi').style.display="block";
		document.getElementById('myElementrasi').style.display="none";
	document.getElementById('youtubeElementrasi').innerHTML='<iframe width="325" height="195" src="'+sourcelnk+'?rel=0&showinfo=0&autoplay=1" allow="autoplay"  frameborder="0" allowfullscreen></iframe>';
	}
	else{
		document.getElementById('youtubeElementrasi').style.display="none";
		document.getElementById('myElementrasi').style.display ="block";
		var newvdofle=fullpath;
			var vdf=newvdofle.split("_,");
			var file1=vdf[0]+"_450k.mp4";

			var container = document.getElementById("myElementrasi");
flowplayer(container, {
    key: "$688227841480965",
	 autoplay: true,
	ratio: 9/16,
	share: false,
	analytics: "UA-2346178-4",
    clip: {
		hlsjs: {
        safari: true,
