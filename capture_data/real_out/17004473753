<script>

var vss,trndtime;
 function selapps(){
	 var cks=document.getElementById("cc").value;
	 if(cks=2){
	 $("#container1").fadeOut(1000);
	 $("#container2").fadeOut(1000);
	 $("#container3").fadeOut(1000);
	  var vv= document.getElementById("app").value
	    $("#container"+vv).fadeIn(1000);
	  vi=parseInt(vv)+1;
	 if(vi==4) vi=1;
	 document.getElementById("app").value=vi;
	 trndtime=setTimeout("trendn('"+vi+"')",30000);
	 
 }
 }

 function trendn()
{
	selapps();
}
    
selapps(0);

function trmouse(a)
{
	if(a==1){
		document.getElementById("cc").value=1;
		clearTimeout(trndtime);
		}
	else{
		document.getElementById("cc").value=2;
		trndtime=setTimeout("trendn('"+vi+"')",30000);
		}
		}

</script>
