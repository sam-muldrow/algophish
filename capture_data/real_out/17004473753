<script type="text/javascript">

var sps
function selSps(sps){

	for(i=1;i<=5;i++){
	document.getElementById("spl"+i).style.display="none";
	document.getElementById("selspls"+i).className="";
	
	}
	document.getElementById("spl"+sps).style.display="block";
	document.getElementById("selspls"+sps).className="active";
}

 

