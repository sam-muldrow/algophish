ns[sl.selectedIndex].value + '.htm ')} else{ fa=fa.replace('/(' + wwwServer + ')(.+)(-.+).htm$/', '$1$2.htm ')} sl.form.action=fa; sl.form.submit()} function verify(f){ var errors=''; if (f.nume.value.length < 3){ errors +="Please type in your name. It should be at least 3 characters long.\n"} else if (f.nume.value.match(/[^\w .]/)){ errors +="Field name may contain only letters, spaces, digits and underscore\n"} if (f.comentariu.value.length < 10){ errors +="Review should be at least 10 characters long\n"} if (errors.length >0){ alert(errors); return !1} return !0}</script>
