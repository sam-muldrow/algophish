<script>
    // jQuery to handle the click event for the parent item
    $(document).ready(function () {
        var container = $("body");
        var scrolledClass = "view--scrolled";
        var scrollThresholdPercent = 10; // Adjust the scroll threshold as needed (percentage of viewport height)
        var viewportHeight = $(window).height();
        var scrollThreshold = (viewportHeight * scrollThresholdPercent) / 100;

        var isScrolled = false; // Flag to track whether the class is already added
        var isAnimating = false; // Flag to track CSS animations

        $(window).scroll(function () {
            // Check the scroll position
            var scroll = $(window).scrollTop();
            
            if(!isAnimating){
                // Add or remove the "navbar--scrolled" class based on scroll position
                if (scroll >= scrollThreshold && !isScrolled) { // Adjust the scroll threshold as needed
                    container.addClass(scrolledClass);
                    isAnimating = true; // Flag to track CSS animations
                    isScrolled = true; // Set the flag to true
                } else if (scroll < scrollThreshold && isScrolled) {
                    container.removeClass(scrolledClass);
                    isAnimating = true; // Flag to track CSS animations
                    isScrolled = false; // Set the flag to false
                }
            }
        });

        // Listen for the "transitionend" event on the navigation bar
        $(".navbar").on("transitionend", function (event) {
            // Check if the event target is the navigation bar
            if ($(event.target).hasClass("navbar")) {
                // Allow scroll checking after the CSS animations are complete
                isAnimating = false;
            }
        });

        // Toggle off-canvas menu when mobile menu button is clicked
        $(".mobile-menu-btn").on("click", function () {
            $("body").toggleClass("offcanvas-expanded");
        });
    });
</script></body>

</html>