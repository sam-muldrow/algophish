<script>
//Tri-promo js
    (function () {	
		var dropdownurl = document.getElementById("custom-account-dropdown");
		var btn = document.getElementById("custom-account-dropdown-submit");
		//click event function for the Go button
		btn.addEventListener("click", function(e){

                        //get url for Celebrus
                        let serviceType = document.getElementById('custom-account-dropdown').value;
                        //send Celebrus, selection from self-service dropdown for report - number of people selecting each type of sercice
                        if (Bootstrapper.Celebrus && Bootstrapper.Celebrus.sendCelebrusCustomAttribute) { 
                        Bootstrapper.Celebrus.sendCelebrusCustomAttribute("selfservice_Drp_value", serviceType);
                        }
                        //end Celebrus
			window.open(dropdownurl.value);
			e.preventDefault();
		});
    })();
</script>









