      <script type="text/javascript">$(function(){


    var tab = $('.tabs-wrap .tab');
    var tabimage = $('.tab-content .tab-content__image');
    var tabcopy = $('.tab-content__copy');
    var selector = $('.selector');

    tab.on('click', function(){
        tab.removeClass('active');
        $(this).addClass('active');
        var data = $(this).data('selected');

        tabimage.removeClass('active');
        $('.tab-content__image--'+data).addClass('active');
        tabcopy.removeClass('active');
        $('.tab-content__copy--'+data).addClass('active');

        //update select dropdown for mobile
        selector.val(data);
    });

    selector.on('change', function(){
        var t = $(this);
        var selected = t.find(':selected').data('selector');

        
        tab.removeClass('active');
        selector.val(selected);

        tabimage.removeClass('active');
        tabcopy.removeClass('active');
        $('.tab-content__image--'+selected).addClass('active');
        $('.tab-content__copy--'+selected).addClass('active');
    });


    var num = 2; //set to 2 since first one is already shown
    changeImage();
    setInterval(changeImage, 8000);

    function changeImage() {

        $('.experience__image-container').removeClass('play');

        // add delay to restart timer
        setTimeout(function(){
            $('.experience__image-container').addClass('play');
        },100);

        $('.experience__image').removeClass('active');
        $('.experience__image--'+num).addClass('active');

        $('.experience__pricing-image').removeClass('active');
        $('.experience__pricing-image--'+num).addClass('active');

        $('.experience__check-image').removeClass('active');
        $('.experience__check-image--'+num).addClass('active');

        if(num == 3) {
            num = 1;
        } else {
            num++;
        }
        
    }

});
</script>
  	
  


