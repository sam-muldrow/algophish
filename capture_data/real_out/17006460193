function AddRowsToTable() { 
 var tbl = document.getElementById('table1'); 
 var lastRow = tbl.rows.length - 1; 
 var f=1;
 var iteration = lastRow;    
 if (iteration < 50) {    
  var row = tbl.insertRow(lastRow); 
 var leer = row.insertCell(0);
 leer.setAttribute('colspan', '5'); 
 leer.setAttribute('height', '10');

   var row = tbl.insertRow(lastRow + 1);
   var leer1 = row.insertCell(0);
   leer1.innerHTML='<b>Original-Url:</b>';
   var leer2 = row.insertCell(1);
   
   var cellRight = row.insertCell(2);
   var el = document.createElement('input'); 
   el.setAttribute('type', 'text'); 
   el.setAttribute('name', 'url[]'); 

   cellRight.setAttribute('id',iteration) 
   cellRight.appendChild(el); 
   
   var row = tbl.insertRow(lastRow + 2); 
 var leer = row.insertCell(0);
 leer.setAttribute('colspan', '5'); 
 leer.setAttribute('height', '10');
 
 var row = tbl.insertRow(lastRow + 3);
   var leer1 = row.insertCell(0);
   leer1.innerHTML='<b>Wartezeit:</b>';
   var leer2 = row.insertCell(1);
   
   var cellRight = row.insertCell(2);
  
   var el = document.createElement('select'); 
   el.setAttribute('name', 'wait[]');
   var el2 = document.createElement('option');    
   theText=document.createTextNode("1");
   el2.appendChild(theText);
   el2.setAttribute('name', 'wait');
