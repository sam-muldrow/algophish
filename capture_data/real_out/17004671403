<script type="text/javascript">
    function loadAsync(src, callback){
        var s = document.createElement('script'),
            r = false;
        s.src = src;
        if (callback !== null) {
            s.onload = s.onreadystatechange = function() {
                if (!r && (!this.readyState || /loaded|complete/.test(this.readyState))) {
                    r = true;
                    callback();
                }
            };
        }
        document.getElementsByTagName('head')[0].appendChild(s);
    }

    loadAsync("/staticrl/scripts/golem_consent_190-min.js", function () {
        GolemConsent.setCustomConfig({"cookieName":"golem_consent20","cookieExpire":90,"cookieVersion":"|220101","fallback":true,"fallbackExpire":1,"fallbackTimeout":5000,"destination":"https:\/\/www.golem.de\/"});
        GolemConsent.init("inline");
    });
</script>

