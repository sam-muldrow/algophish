jQuery.noConflict();
jQuery(document).ready(function($){
   var container = $('#allmode-id603');

   container.after('<div id="allmode-second" />').next().html(container.html());
   $("#allmode-id603 .allmode-item:odd").remove();
   $("#allmode-second .allmode-item:even").remove();

   container.imagesLoaded(function(){
      container.carouFredSel({

         auto: false,
         synchronise: "#allmode-second",
         width: '100%',
         height: 'auto',


         scroll: {
            items         : 1,
            duration      : 600,
            pauseOnHover   : true,
            timeoutDuration   : 3000
         },

         // start responsive mode
         responsive: true,
         items: {
            visible: {
               min: 1,
               max: 6
            },
         },
         swipe: {
            onTouch: true,
            items: 2
         },
         onCreate : function () {
            $(window).on('resize', function () {
               container.parent().height(container.height());
            }).trigger('resize');
         },
         // end responsive mode

         prev: '.allmode-id603-prev',
         next: '.allmode-id603-next'

      });
   });

   $("#allmode-second").carouFredSel({
      auto: false,
      responsive: true,
      items: {
         visible: {
            min: 1,
            max: 6
         },
      }
   });

});
