function getParameterByName(name) {
	name = name.replace(/[\[]/,"\\[").replace(/[\]]/,"\\]");
var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
	results = regex.exec(location.search);
return results === null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
}

var cjeventid = getParameterByName('cjevent');

var now = new Date();
var time = now.getTime();
var expTime = time + 1000*31536000;
	now.setTime(expTime);

if(cjeventid !=''){
	document.cookie = "cje=" + cjeventid + "; expires=" + now.toGMTString() + "; path=/";
}
</script><script async src="https://www.googletagmanager.com/gtag/js?id=G-DW9QC6NP9P"></script>
  <script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());

    gtag('config', 'G-DW9QC6NP9P');
  </script>
<script>

function checkCart () {

  let currentCart = localStorage.getItem("bcCart");
  if (currentCart) currentCart = JSON.parse(currentCart);
  const options = {method: 'GET', headers: {'Content-Type': 'application/json'}};
  fetch('https://dailysteals.com/api/storefront/carts', options)
    .then(response => response.json())
    .then(carts => {
      const newCart = {"currency": "USD", "items": []}
      if (carts && carts.length > 0) {
          const cart = carts[0];
          let cartValue = 0;
          for (const item of cart.lineItems.physicalItems) {  
            const newCartItem = {
              item_id: item.sku,
              item_name: item.name,
              price: item.salePrice,
              quantity: item.quantity
            }     
            cartValue += item.extendedSalePrice;
            newCart.items.push(newCartItem);
          }
          newCart.value = cartValue;
          localStorage.setItem("bcCart",JSON.stringify(newCart));
          }
      const events = compareCart(newCart, currentCart);
      if (events.add.items.length > 0) {
      	gtag("event","add_to_cart",events.add);
          console.log('gtag add')
      }
      // need to check for purchase
      if (events.remove.items.length > 0) {
        //  gtag("event","remove_from_cart",events.remove);
          console.log('gtag remove')
      }
  })
    .catch(err => console.error(err));

}

function compareCart (newCart, oldCart) {

  const addEvent = {"currency": "USD", "items": []}
  const removeEvent = {"currency": "USD", "items": []}
  let addValue = 0;
  let removeValue = 0;

  // need to compare the two sets and find changes
  const checked = [];
  for (const item of newCart.items) {
    checked.push(item.item_id)
    const eventItem = JSON.parse(JSON.stringify(item));
    const itemChange = isInCart(eventItem, oldCart);
    if (itemChange === 0) continue;
    if (itemChange > 0) {
      addValue += item.price * Math.abs(itemChange);
      eventItem.quantity = Math.abs(itemChange);
      addEvent.items.push(eventItem)
    }
    if (itemChange < 0) {
      removeValue += item.price * Math.abs(itemChange);
      eventItem.quantity = Math.abs(itemChange);
      removeEvent.items.push(eventItem)
    }
  }
  // need to only get items that are no longer in the newCart
  for (const item of oldCart.items) {
    if (checked.includes(item.item_id)) continue; // we've already compared this item
    const eventItem = JSON.parse(JSON.stringify(item));
    removeValue += item.price * item.quantity;  
    removeEvent.items.push(eventItem)
  }

  addEvent.value = addValue;
  removeEvent.value = removeValue;

  return {add: addEvent, remove: removeEvent};
  
}

function isInCart (item, currentCart) {

  const currItems = currentCart.items;
  for (const currItem of currItems) {
    if (currItem.item_id === item.item_id) {
      return (item.quantity - currItem.quantity)
    }
  }
  return item.quantity;
}

addEventListener('click', () => {checkCart();});


</script>
