        const Add_To_Cart_button = document.querySelectorAll(".btnATC");
        Add_To_Cart_button.forEach((button) => {
        button.addEventListener("click", function handleClick(event) {
          const proudct_id = event.target.getAttribute("data-product-id");
          if (proudct_id !== null) {
            window.dataLayer.push({
              event: "add_to_cart",
              product_id: proudct_id,
            });
          }
        });
