<script>
    setTimeout( function() {
        window.location.reload( true );
    }, 60*60*1000 );

    $(document).ready( function() {
        $('#yr').text(new Date().getFullYear());
        $.ajax({
            dataType: "json",
            url: '/lch.php',
            data: { ltoken: $("#zbiornik-ltoken").val() },
            timeout: 10*1000,
            success: function( resp ){
                if( !resp.ok ) {
                    setTimeout( function() {
                        location.reload( true );
                    }, 1000 );
                }
            }
        });

    });
</script>




