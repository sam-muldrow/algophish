}});

</script>
<script>

function granking($scope, $http, $log) {
	var params = {
		"callback":'JSON_CALLBACK',
	};

	$http.jsonp('https://pmall.gpoint.co.jp/g-ranking/get.php', {"params": params}).success(function(data) {
		$scope.topics = data;
	});
}

</script>
