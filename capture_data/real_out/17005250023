      }).done(function(json) {
      $("#prize_box").show();
      var len = json.length;
      var maxloop = 2; //JèÔµ½¢ðãü
        if (len < maxloop ) {
          maxloop = len;
        }
        for(var i=0; i < maxloop; i++){ //JèÔµðwèµÈ¢ê ÏmaxloopÍlenÉÏXµÄ29`32sÚsv
          var inshtml = '<li>';
          inshtml += '<a href="'+json[i].prizelink+'&utm_source=gpoint&utm_medium=gtop&utm_campaign=prize_frame">';
          inshtml += '<div class="prize_img"><img class="pirze_img" src="'+json[i].prizeimage+'" alt=""></div>';
          inshtml += '<div class="prize_details"><p class="prize_name">'+json[i].prizename+'</p>';
          inshtml += '<div class="btn_link">';
          inshtml += '<span class="lottery-number">_1û ½è<span>'+json[i].unitprice+'f</span>^</span><strong>å·é</strong></div>';
          inshtml += '<p class="countdown">cèÔF<span id="matter'+[i+1]+'countdownD">0</span>ú<span id="matter'+[i+1]+'countdownH">00</span>Ô<span id="matter'+[i+1]+'countdownM">00</span>ª<span id="matter'+[i+1]+'countdownS">00</span>b</p>';
          inshtml += '<p class="totalcount">»ÝÌåûF<strong>'+json[i].totalcount+'</strong></p>';
          inshtml += '<div class="triangle"><span class="squ-txt"><span id="matter'+[i+1]+'status">JÃ</span></span></div>';
          inshtml += '</div>';
          inshtml += '</a></li>';
        $('#prize').before(inshtml);
        todatestr.push(json[i].todate);
          }
        countdownTodate();
        }).fail(function() {
      $('#prize_box').hidden();
      }
    );
    function myFunc(datestr,target){
      limitY = Number(datestr.substr(0, 4));
      limitM = Number(datestr.substr(4, 2));
      limitD = Number(datestr.substr(6, 2));
      limitH = Number(datestr.substr(8, 2));
      limitMI = Number(datestr.substr(10, 2));
      myLimit = new Date(limitY ,limitM -1 ,limitD ,limitH, limitMI);
      myD   = new Date();										// »Ý
      keepSecond = myLimit.getTime() - myD.getTime();			// úÀÆ»ÝÌ·

      if (keepSecond <= 0) {
        myLimitDay = "0";
        myLimitHour = "00";
        myLimitMin = "00";
        myLimitSec = "00";
      } else {
        myMsec = myLimit.getTime() - myD.getTime();				// úvZ
        myLimitDay  = Math.floor ( myMsec / (1000*60*60*24) );
        myMsec     -= ( myLimitDay * (1000*60*60*24) );
        myLimitHour = Math.floor ( myMsec / (1000*60*60) );
        myMsec     -= ( myLimitHour * (1000*60*60) );
        myLimitMin  = Math.floor ( myMsec / (1000*60) );
        myMsec     -= ( myLimitMin * (1000*60) );
        myLimitSec  = Math.floor ( myMsec / 1000 );

        if ( myLimitHour <= 9 ) myLimitHour = "0" + myLimitHour;	// 1Ìê0ðâ®
        if ( myLimitMin  <= 9 ) myLimitMin  = "0" + myLimitMin;
        if ( myLimitSec  <= 9 ) myLimitSec  = "0" + myLimitSec;

        // ÜàÈ­I¹
        limitDate = myLimit.getFullYear()+myLimit.getMonth()+myLimit.getDate();
        sysDate = myD.getFullYear()+myD.getMonth()+myD.getDate();
        if(limitDate == sysDate && $("#" + target + "countdownD").parents(".prize li").hasClass("soonToend") == false){
          $("#" + target + "countdownD").parents(".prize li").addClass("soonToend");
          document.getElementById(target +"status").innerHTML = 'à¤·®</br>I¹';
        }
      }

      myDispD = myLimitDay;	// úð\¦
      myDispH = myLimitHour;
      myDispM = myLimitMin;
      myDispS = myLimitSec;

      document.getElementById(target +"countdownD").innerHTML = myDispD;		// cú
      document.getElementById(target +"countdownH").innerHTML = myDispH;		// c
      document.getElementById(target +"countdownM").innerHTML = myDispM;		// cª
      document.getElementById(target +"countdownS").innerHTML = myDispS;		// cb
    }
    function countdownTodate() {
        for(var i=0; i < todatestr.length; i++){
            var datestr = todatestr[i] ;
            var target = 'matter' + (i+1);
            myFunc(datestr,target);
       }
        setTimeout('countdownTodate()', 1000);
    }

</script>
</section>
