<script>
// ¶NúüÍ»è
var dd = new Date();

function getCookie(key){
    var tmp=document.cookie+";";
	var tmp1=tmp.indexOf(key, 0);
    if(tmp1!=-1){
        tmp=tmp.substring(tmp1,tmp.length);
        var start=tmp.indexOf("=",0)+1;
        var end=tmp.indexOf(";",start);
        return(unescape(tmp.substring(start,end)));
    }
    return("" + dd.getTime());
}
document.write('<SCR' + 'IPT LANGUAGE="JavaScript" SRC="https://www.gpoint.co.jp/scripts/bdBanner.jsp?&f=2&hid=BDCARD&' + dd.getTime() + getCookie('fuserno') + '"></SCR' + 'IPT>');
</script>

