                    var url = json[i].serviceurl;
                }
                $('.servicelist').append('<dd><a href="' + url + '" target="_blank">' + json[i].servicename + '</a></dd>');
                if (json[i].servicename == 'fLO') {
                    $('.pointanswer_granking').append(json[i].pointanswer);
                }
                if (json[i].servicename == '¾êÇ±') {
                    $('.pointanswer_daredoko').append(json[i].pointanswer);
                }
            }
        }
    });
});

