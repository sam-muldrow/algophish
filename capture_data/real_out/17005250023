function getCookieValue(key){
    var tmp=document.cookie+";";
    var tmp1=tmp.indexOf(key, 0);
    if(tmp1!=-1){
        tmp=tmp.substring(tmp1,tmp.length);
        var start=tmp.indexOf("=",0)+1;
        var end=tmp.indexOf(";",start);
        return(unescape(tmp.substring(start,end)));
    }
    return("" + dd.getTime());
}
function disptokupon(){

  $.ajax({
     url: 'https://pmall.gpoint.co.jp/kokangen/kokangen.json',
     scriptCharset: 'UTF-8',
     type    : 'GET',
     dataType: 'jsonp',
     jsonpCallback: 'kokangen',
     success: function(result){

                 var cnt = 0;
                 for( matter in result) {
                     if(cnt <8){
                         cnt++;
                                    var inshtml  = '<a href="'+result[matter].detailurl+'" class="block-link" onClick="javascript:ga(\'send\', \'event\', \'www.gpoint.co.jp/index.html\', \'click\', \''+result[matter].title+'\');setCtti(\''+result[matter].displayid+'\');"><div>';
                                    if(result[matter].pointbefore != 0){
                                       inshtml += '<strong><span class="timesale-point"><span class="point-cancel">'+Number(result[matter].pointbefore).toLocaleString()+result[matter].ishendou+'f</span>¨<span class="font-size24">'+Number(result[matter].point).toLocaleString()+result[matter].ishendou+'f</span></span></strong>';
                                       if(getCookieValue('specgGA0000131138' + getCookieValue('fuserno')) == '0' && result[matter].registbonusflag != null && result[matter].registbonusflag == '1'){
                                          inshtml += '<p class="icon-welcomecp">³çÉ<span>+500f</span>ÎÛ</p>';
                                       } else {
                                          inshtml += '<span class="timesale-multiple redfont">|CgUP<span class="font-size24">'+Math.floor((parseInt(result[matter].point)/parseInt(result[matter].pointbefore)) * 10 )/10+'</span>{</span>';
                                       }
                                    } else {
                                       inshtml += '<strong><span class="timesale-point"><span class="font-size24">'+Number(result[matter].point).toLocaleString()+result[matter].ishendou+'f</span></span></strong>';
                                       if(getCookieValue('specgGA0000131138' + getCookieValue('fuserno')) == '0' && result[matter].registbonusflag != null && result[matter].registbonusflag == '1'){
                                          inshtml += '<p class="icon-welcomecp">³çÉ<span>+500f</span>ÎÛ</p>';
