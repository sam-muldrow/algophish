$(function(){
    $.ajax({
        type: 'GET',
        url: 'https://img.gpoint.co.jp/common/json/servicelist.json',
        scriptCharset: 'UTF-8',
        dataType: 'jsonp',
        jsonpCallback: 'servicelist',
        success: function(json) {
            var len = json.length;
            for(var i=0; i<len; i++){
                if (json[i].parameter == '1'){
