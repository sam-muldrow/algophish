												if($.inArray('BEST_OFFER', value['buyingOptions'])!=-1)
													details += ' <small>'+__('or Best Offer')+'</small>';
												var imageurl = (typeof value['image']!=='undefined' && typeof value['image']['imageUrl']!=='undefined') ? value['image']['imageUrl'].replace('https://i.ebayimg.com/', 'https://www.picclickimg.com/').replace('/thumbs/images/g/', '/').replace('/s-l225.jpg', '/'+slug+'.webp') : '';
												html += '<li id="item-'+value['legacyItemId']+'"><div class="shadow"></div>'+
														'<a href="/'+slug+'-'+value['legacyItemId']+'.html" target="_blank"><div class="sq loading">'+((imageurl!='') ? '<picture><source srcset="'+imageurl+'" type="image/webp"><source srcset="'+imageurl.replace('.webp', '.jpg')+'" type="image/jpeg"><img src="'+imageurl+'" onload="sq(this)" title="'+titlehtml+'" alt="'+titlehtml+'" /></picture>' : '<img src="/img/imgNoImg.gif" onload="sq(this)" title="'+titlehtml+'" alt="'+titlehtml+'" />')+'<div class="controls" onmouseenter="enter(this)" onmouseleave="leave(this)"><span class="glyphicon glyphicon-triangle-left" aria-hidden="true" onclick="thumbprev(this, arguments[0]);return false;"></span><span class="glyphicon glyphicon-triangle-right" aria-hidden="true" onclick="thumbnext(this, arguments[0]);return false;"></span></div>'+((typeof value['watchCount']!=='undefined' && value['watchCount']>0) ? '<div class="watchcount">'+((0&&value['watchCount']/Math.max(Math.ceil((Date.now()-Date.parse(value['itemCreationDate']))/86400000), 1)>3) ? '<span class="trending text-uppercase" style="color:#ff0000;"><strong>trending</strong> </span>' : '')+value['watchCount'].toString().replace(/\B(?=(\d{3})+(?!\d))/g, ((price['currency']=='EUR') ? '.' : ','))+' <span class="glyphicon glyphicon-heart" aria-hidden="true"></span><span class="text"> '+__('watcher'+((value['watchCount']>1) ? 's' : ''))+'</span></div>' : '')+((typeof value['qualifiedPrograms']!=='undefined' && $.inArray('AUTHENTICITY_GUARANTEE', value['qualifiedPrograms'])!=-1) ? '<div class="authenticityguarantee" title="'+__('Authenticity Guarantee')+'"><span class="glyphicon glyphicon-ok-sign" aria-hidden="true"></span> '+__('Authenticity Guarantee')+'</div>' : '')+'</div>'+
														'<h3'+((value['topRatedBuyingExperience']) ? ' class="toprated"' : '')+' title="'+titlehtml+'">'+((newlylisted) ? '<strong>'+timeago(value['itemCreationDate'])+'</strong> - ' : '')+(($.cookie('sort')=='DistanceNearest' && typeof value['distanceFromPickupLocation']!=='undefined') ? Math.round(value['distanceFromPickupLocation']['value'])+' '+value['distance']['unitOfMeasure']+' - ' : '')+((watchcount) ? '<strong>'+value['watchCount']+' watching</strong> - ' : '')+((value['topRatedBuyingExperience']) ? '<img src="/img/toprated'+((window.location.hostname=='picclick.com') ? 'plus' : '')+((window.location.hostname=='picclick.de') ? 'DE' : '')+'.png" width="14" height="13" alt="Top-Rated'+((window.location.hostname=='picclick.com') ? ' Plus' : '')+' Seller" title="Top-Rated'+((window.location.hostname=='picclick.com') ? ' Plus' : '')+' Seller" /> ' : '')+titlehtml+'</h3></a>'+
														'<div class="price">'/*+((typeof value['sellingStatus'][0]['timeLeft']==='undefined') ? ((value['sellingStatus'][0]['sellingState'][0]=='EndedWithoutSales') ? '<span class="label label-success text-uppercase">'+__('Unsold')+'</span> ' : '<span class="label label-danger text-uppercase">'+__('Sold')+'</span> ') : '')*/+((!newlylisted && !watchcount) ? '<strong>' : '')+((discounthtml!='') ? discounthtml : '')+pricehtml+((!newlylisted) ? '</strong>' : '')+details+((typeof value['itemEndDate']!=='undefined') ? timeleft2(value['itemEndDate']) : '')+'</div>'+
														'<div class="button"><button class="btn btn-primary btn-xs" onclick="v(this);" rel="nofollow"><span class="glyphicon glyphicon-arrow-right hidden-xs" aria-hidden="true"></span> '+__('See')+' '+__('Details')+'</button>'+(($.cookie('directbuy')!==undefined) ? ' &nbsp;<small>eBay</small>' : '')+((1||window.location.hostname!='picclick.com') ? ' <button class="btn btn-success btn-xs" onclick="v3(this)" rel="nofollow" title="'+__('Search')+' Amazon"><span class="glyphicon glyphicon-search hidden-xs" aria-hidden="true"></span><img src="/img/amazon-logo-white-png-transparent-icon.png" width="13" height="13" alt="Amazon"></button>' : '')+'</div>'+
														'</li>';
											}
											if(html!='')    // Need to add this due to continue above
											{
												$('div#category-'+searchnum+' ul.items').append(html);
												$('div#category-'+searchnum).removeClass('hidden');
												$('.nav-tabs').removeClass('hidden');
											}
										}
										else if(typeof data.findItemsAdvancedResponse!=='undefined' && typeof data.findItemsAdvancedResponse[0].searchResult!=='undefined' && typeof data.findItemsAdvancedResponse[0].searchResult[0].item!=='undefined')
										{
											var html = '';
											var newlylisted = (urls[0].indexOf('&sortOrder=StartTimeNewest')!=-1) ? true : false;
newlylisted = true;
											var watchcount = (urls[0].indexOf('&sortOrder=WatchCountDecreaseSort')!=-1) ? true : false;
watchcount = false;
											for(var i=0, len=data.findItemsAdvancedResponse[0].searchResult[0].item.length; i<len; i++)
											{
												var value = data.findItemsAdvancedResponse[0].searchResult[0].item[i];
												if(i>0 && data.findItemsAdvancedResponse[0].searchResult[0].item[i-1]['itemId'][0]==data.findItemsAdvancedResponse[0].searchResult[0].item[i]['itemId'][0]) continue;
												value['title'][0] = $.trim(value['title'][0]);
												if(value['title'][0].toUpperCase()==value['title'][0])
													value['title'][0] = value['title'][0].toLowerCase().replace(/\b[a-z]/g, function(letter){ return letter.toUpperCase(); });
												var slug = urlstring(word_limit(value['title'][0], 8));
												var titlehtml = escapeHtml(value['title'][0]);
												var price = (value['listingInfo'][0]['buyItNowAvailable'][0]=='false' || $.cookie('type')!='BuyItNow') ? value['sellingStatus'][0]['convertedCurrentPrice'][0] : value['listingInfo'][0]['buyItNowPrice'][0];
												var pricehtml = ((price['@currencyId']!='USD' && price['@currencyId']!='GBP' && price['@currencyId']!='AUD' && price['@currencyId']!='CAD') ? price['@currencyId']+' ' : '')+((price['@currencyId']=='USD' || price['@currencyId']=='CAD' || price['@currencyId']=='AUD') ? '$' : '')+((price['@currencyId']=='GBP') ? '£' : '')+parseFloat(price['__value__']).toFixed(2).toString().replace('.', ((price['@currencyId']=='EUR') ? ',' : '.')).replace(/\B(?=(\d{3})+(?!\d))/g, ((price['@currencyId']=='EUR') ? '.' : ','));
												var discounthtml = '';
												if(typeof value['discountPriceInfo']!=='undefined' && typeof value['discountPriceInfo'][0]['originalRetailPrice']!=='undefined')
												{
													var discount = value['discountPriceInfo'][0]['originalRetailPrice'][0];
													discounthtml = ((discount['@currencyId']!='USD' && discount['@currencyId']!='GBP' && discount['@currencyId']!='AUD' && discount['@currencyId']!='CAD') ? discount['@currencyId']+' ' : '')+((discount['@currencyId']=='USD' || discount['@currencyId']=='CAD' || discount['@currencyId']=='AUD') ? '$' : '')+((discount['@currencyId']=='GBP') ? '£' : '')+parseFloat(discount['__value__']).toFixed(2).toString().replace('.', ((discount['@currencyId']=='EUR') ? ',' : '.')).replace(/\B(?=(\d{3})+(?!\d))/g, ((discount['@currencyId']=='EUR') ? '.' : ','));
													discounthtml = '<small><strike class="text-danger">'+discounthtml+'</strike></small> ';
												}
												var details = '';
												if(typeof value['sellingStatus'][0]['bidCount']!=='undefined')
													details = ' <small> '+value['sellingStatus'][0]['bidCount'][0]+' '+((value['sellingStatus'][0]['bidCount'][0]!=1) ? __('Bids') : __('Bid'))+((value['listingInfo'][0]['buyItNowAvailable'][0]=='true') ? ' '+__('or')+' '+__('Buy It Now') : '')+'</small>';
												else if(value['listingInfo'][0]['buyItNowAvailable'][0]=='true' || typeof value['listingInfo'][0]['listingType']==='undefined' || value['listingInfo'][0]['listingType'][0]=='StoreInventory' || value['listingInfo'][0]['listingType'][0]=='FixedPrice')
													details = ' <small>'+__('Buy It Now')+'</small>';
												if(value['listingInfo'][0]['bestOfferEnabled'][0]=='true')
													details += ' <small>'+__('or Best Offer')+'</small>';
												html += '<li id="item-'+value['itemId'][0]+'"><div class="shadow"></div>'+
														'<a href="/'+slug+'-'+value['itemId'][0]+'.html" target="_blank"><div class="sq loading"><img src="'+((typeof value['galleryURL']!=='undefined') ? 'https://www.picclickimg.com/d/l400/pict/'+value['itemId'][0]+'_/'+slug+'.jpg' : '/img/imgNoImg.gif')+'" onload="sq(this)" title="'+titlehtml+'" alt="'+titlehtml+'" /><div class="controls" onmouseenter="enter(this)" onmouseleave="leave(this)"><span class="glyphicon glyphicon-triangle-left" aria-hidden="true" onclick="thumbprev(this, arguments[0]);return false;"></span><span class="glyphicon glyphicon-triangle-right" aria-hidden="true" onclick="thumbnext(this, arguments[0]);return false;"></span></div></div>'+
														'<h3'+((value['topRatedListing'][0]=='true') ? ' class="toprated"' : '')+' title="'+titlehtml+'">'+((newlylisted) ? '<strong>'+timeago(value['listingInfo'][0]['startTime'][0])+'</strong> - ' : '')+((typeof value['distance']!=='undefined') ? Math.round(value['distance'][0]['__value__'])+' '+value['distance'][0]['@unit']+' - ' : '')+((watchcount) ? '<strong>'+value['listingInfo'][0]['watchCount'][0]+' watching</strong> - ' : '')+((value['topRatedListing'][0]=='true') ? '<img src="/img/toprated'+((window.location.hostname=='picclick.com') ? 'plus' : '')+((window.location.hostname=='picclick.de') ? 'DE' : '')+'.png" width="14" height="13" alt="Top-Rated'+((window.location.hostname=='picclick.com') ? ' Plus' : '')+' Seller" title="Top-Rated'+((window.location.hostname=='picclick.com') ? ' Plus' : '')+' Seller" /> ' : '')+titlehtml+'</h3></a>'+
														'<div class="price">'+((typeof value['sellingStatus'][0]['timeLeft']==='undefined') ? ((value['sellingStatus'][0]['sellingState'][0]=='EndedWithoutSales') ? '<span class="label label-success text-uppercase">'+__('Unsold')+'</span> ' : '<span class="label label-danger text-uppercase">'+__('Sold')+'</span> ') : '')+((!newlylisted && !watchcount) ? '<strong>' : '')+((discounthtml!='') ? discounthtml : '')+pricehtml+((!newlylisted) ? '</strong>' : '')+details+((typeof value['sellingStatus'][0]['timeLeft']!=='undefined') ? timeleft(value['sellingStatus'][0]['timeLeft'][0]) : '')+'</div>'+
														'<div class="button"><button class="btn btn-primary btn-xs" onclick="v(this);" rel="nofollow"><span class="glyphicon glyphicon-arrow-right hidden-xs" aria-hidden="true"></span> '+__('See')+' '+__('Details')+'</button>'+((1||window.location.hostname!='picclick.com') ? ' <button class="btn btn-success btn-xs" onclick="v3(this)" rel="nofollow" title="'+__('Search')+' Amazon"><span class="glyphicon glyphicon-search hidden-xs" aria-hidden="true"></span><img src="/img/amazon-logo-white-png-transparent-icon.png" width="13" height="13" alt="Amazon"></button>' : '')+'</div>'+
														'</li>';
											}
											$('div#category-'+searchnum+' ul.items').append(html);
											$('div#category-'+searchnum).removeClass('hidden');
											$('.nav-tabs').removeClass('hidden');
										}
									}
								})());
							}
