	<script>
		$('#q').typeahead({
			highlight:true,
			hint:false,
			minLength:0
		},
		{
			name:'suggestions',
			displayKey:'value',
			source:suggestions.ttAdapter(),
			templates:{
				suggestion:function(data){
					return '<p'+((typeof data.recent!=='undefined') ? ' class="recent"' : '')+'>'+data.value+((typeof data.category!=='undefined') ? '<small class="text-muted"> in <span class="text-primary">'+data.category[1]+'</span></small>' : '')+'</p>';
				}
			}
		}).on('typeahead:selected', function(ev, suggestion){
			if(typeof suggestion.category!=='undefined')
			{
				$('<input>').attr({
					type:'hidden',
					name:'categoryId',
					value:suggestion.category[0]
				}).appendTo('#search');
			}
			$('#search').submit();
		});
		if(!isTouch || (!isMobile.iOS() && !isMobile.Android()))
			moveCursorToEnd(document.getElementById('q'));
		$('#q').on('click', function(e){
			if(isMobile.any() && $(window).width()<=414 && document.getElementById('q').value.length>22)
				moveCursorToEnd(document.getElementById('q'));
			if($('.tt-dropdown-menu').css('display')=='none')
			{
				ev = $.Event('keydown');
				ev.keyCode = ev.which = 40;
				$(this).trigger(ev);
			}
		});
	</script>
