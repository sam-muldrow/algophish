            <script>
                (function () {
                    var token = (Math.floor(Math.random() * 10000000000)).toString(16) + (Math.floor(Math.random() * 10000000000)).toString(16) + (Math.floor(Math.random() * 10000000000)).toString(16),
                        $iframe = document.currentScript.previousElementSibling, $ifVal = 0, $hg = 0;
                    $ifVal = setInterval(function () {
                        $iframe.contentWindow.postMessage({ type: "resizeIframe", token: token }, "*");
                    }, 200);
                    window.addEventListener('message', function (ev) {
                        if (ev.data.type && ev.data.type.indexOf && ev.data.type.indexOf('resize::') != -1) {
                            if (ev.data.token == token) {
                                clearInterval($ifVal);
                                $hg = ev.data.type.replace('resize::', '');
                                $iframe.style.height = $hg + 'px';
                            }
                        }
                    });
                })();
            </script>
