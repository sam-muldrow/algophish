<script type='text/javascript'>
(function () {
    function addScript(src, async, onload) {
        const s = document.createElement('script');
        s.src = src;
        s.onload = onload;
        s.type = "text/javascript";
        s.async = async === void 0 ? true : async;
        document.head.appendChild(s);
    }

    function initESW(gslbBaseURL) {
        embedded_svc.settings.displayHelpButton = true; //Or false
        embedded_svc.settings.language = ''; //For example, enter 'en' or 'en-US'
        embedded_svc.settings.defaultMinimizedText = 'Need Help?';
        //embedded_svc.settings.disabledMinimizedText = '...'; //(Defaults to Agent Offline)

        //embedded_svc.settings.loadingText = ''; //(Defaults to Loading)
        //embedded_svc.settings.storageDomain = 'yourdomain.com'; //(Sets the domain for your deployment so that visitors can navigate subdomains during a chat session)

        // Settings for Chat
        //embedded_svc.settings.directToButtonRouting = function(prechatFormData) {
        // Dynamically changes the button ID based on what the visitor enters in the pre-chat form.
        // Returns a valid button ID.
        //};
        //embedded_svc.settings.prepopulatedPrechatFields = {}; //Sets the auto-population of pre-chat form fields
        //embedded_svc.settings.fallbackRouting = []; //An array of button IDs, user IDs, or userId_buttonId
        //embedded_svc.settings.offlineSupportMinimizedText = '...'; //(Defaults to Contact Us)

        embedded_svc.settings.enabledFeatures = ['LiveAgent'];
        embedded_svc.settings.entryFeature = 'LiveAgent';
        embedded_svc.settings.extraPrechatFormDetails = [
            {"label": "Last Name", "transcriptFields": ["Visitor_Last_Name__c"]},
            {"label": "First Name", "transcriptFields": ["Visitor_First_Name__c"]},
            {"label": "Email", "transcriptFields": ["Visitor_Email__c"]},
            {"label": "Company", "transcriptFields": ["Visitor_Company__c"]}
        ];

        embedded_svc.init(
            'https://icontact.my.salesforce.com',
            'https://icontact.force.com/customers',
            gslbBaseURL,
            '00D80000000KqLk',
            'iContact_Sales_Chat',
            {
                baseLiveAgentContentURL: 'https://c.la3-c1-ph2.salesforceliveagent.com/content',
                deploymentId: '57280000000TNsB',
                buttonId: '5732H0000004DNB',
                baseLiveAgentURL: 'https://d.la3-c1-ph2.salesforceliveagent.com/chat',
                eswLiveAgentDevName: 'iContact_Sales_Chat',
                isOfflineSupportEnabled: true
            }
        );

        if (autoChat) {
            embedded_svc.addEventHandler("onSettingsCallCompleted", function () {
                embedded_svc.liveAgentAPI.startChat();
            });
        }

        //  privacy policy
        var show = false;
        embedded_svc.addEventHandler("afterMaximize", function (data) {
            if (!show) {
                show = true;

                var p = document.createElement('p');
                p.innerHTML = 'By submitting this form, you agree that iContact may process data according to our <a href="https://www.icontact.com/legal/privacy" title="Privacy Policy">Privacy Policy</a>.';

                var li2 = document.createElement("li2");
                li2.style.fontSize = "12px";
                li2.style.color = "grey";
                li2.style.backgroundColor = "white";
                li2.style.padding = "10px 10px 10px 10px";
                li2.style.lineHeight = "16px";
                li2.appendChild(p);

                var sidebar = document.querySelector("div.formContent.embeddedServiceSidebarForm");

                sidebar.appendChild(li2, sidebar.childNodes[2]);
            }
        });
    }


    var params = new URLSearchParams(location.search);
    var autoChat = params.get('startchat');
    var isSupport = autoChat === 'support';

    if (isSupport) {
        if (!window._laq) {
            window._laq = [];
        }

        var buttonId = '57380000000TNsL';
        window._laq.push(function () {
            var dummyButton = document.createElement('div');
            liveagent.showWhenOnline(buttonId, dummyButton);
            liveagent.showWhenOffline(buttonId, dummyButton);
        });
        addScript('https://c.la4-c2-chi.salesforceliveagent.com/content/g/js/37.0/deployment.js');
        if (window.LoadingIndicator) {
					setTimeout(function() {
						LoadingIndicator.enable();
						LoadingIndicator.start();
					}, 100);
					setTimeout(function() {
						LoadingIndicator.stop();
					}, 3000);	
				}

        function checkReady() {
            if (window.liveagent && window.liveagent.init) {
                liveagent.setChatWindowHeight(640);
                liveagent.setChatWindowWidth(485);

                liveagent.addButtonEventHandler(buttonId, function (e) {
                    if (e == liveagent.BUTTON_EVENT.BUTTON_AVAILABLE) {
                        startChat(buttonId, true)
                    }
                });
                liveagent.init('https://d.la4-c2-chi.salesforceliveagent.com/chat', '57280000000TNsB', '00D80000000KqLk');
            } else {
                setTimeout(checkReady, 100);
            }
        }

        checkReady();
        function startChat(id, thisWindow) {
            if (thisWindow) {
                // Rebind window.open to stop opening a dummy window
                var orig = window.open;
                window.open = function() {};
                liveagent.startChatWithWindow(id, window.name);
                window.open = orig;
            } else {
                liveagent.startChat(id);
            }
        }
    } else {
        // Sales
        if (!window.embedded_svc) {
            addScript('https://icontact.my.salesforce.com/embeddedservice/5.0/esw.min.js', false, function () {
                initESW('https://service.force.com');
            });
            ;
        } else {
            initESW('https://service.force.com');
        }
    }

})();

</script>
