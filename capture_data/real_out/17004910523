<script>
loadR2B2Adapter = function(){
    var mobileView = document.documentElement.clientWidth < 992;
    var r2b2DesktopUrl = "//delivery.r2b2.cz/hb/cet21/tv.nova.cz_desktop";
    var r2b2MobileUrl = "//delivery.r2b2.cz/hb/cet21/tv.nova.cz_mobile";
    var r2b2ScriptElement = document.createElement('script');
    var head = document.getElementsByTagName('head')[0];
    r2b2ScriptElement.type='text/javascript';
    r2b2ScriptElement.src=mobileView?r2b2MobileUrl:r2b2DesktopUrl;
    head.append(r2b2ScriptElement);
}
loadR2B2Adapter();
</script>
