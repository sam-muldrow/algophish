<!-- additional js -->
<script>
    window.dataLayer = window.dataLayer || [];

    /*======== protect internal GTM storage against flooding ==== */
      var eventBuffer = [];
      var emittingDelayMs = 1;
      var eventQueue = {
          push: function(obj) {
              eventBuffer.push(obj);
              processEventStorage();
          }
      };

      function processEventStorage() {
          if (processEventStorage.last > +new Date() - emittingDelayMs) {
              setTimeout(function() {
                  processEventStorage();
              });
          } else {
              processEventStorage.last = +new Date();
              dataLayer.push(eventBuffer.shift());
          }
      }



    function videoPushToDataLayer (dataObject, params){

        var airAt = new Date().toISOString().split("T")[0];
        var contentOnlineReleaseTimeFrom = dataObject.type === 'live' ? airAt : dataObject.custom.air_at;
        var contentTitle = dataObject.custom.contentTitle ? dataObject.custom.contentTitle : typeof PageData.title === 'undefined' ? dataObject.title : PageData.title;
        var contentLength = dataObject.type === 'live' ? undefined : dataObject.duration < 0 ? undefined : dataObject.duration;
        var classInfo = params.isAd ? window.trackVideoPlayers[dataObject.streamInfo.videoID] : window.trackVideoPlayers[dataObject.id];

        
        if (dataObject.custom.preview){
          return
        }
        if (dataObject.type === 'live' && dataObject.playerType === 'smallPlayer') {
          return
        }

        eventQueue.push({
            'event': params.event,
            'matrixID': params.matrixId,
            'gaEventCategory': 'video',
            'gaEventAction': params.gaEventAction,
            'gaEventLabel': contentTitle,
            'contentLength': contentLength,
            'videoTag': PageData.videoTag ? PageData.videoTag : 'empty',
            'contentShowTitle': PageData.contentShowTitle ? PageData.contentShowTitle : dataObject.custom.contentShowTitle ? dataObject.custom.contentShowTitle : undefined,
            'contentType': dataObject.type === 'live' ? dataObject.type : dataObject.custom.contentType ? dataObject.custom.contentType : PageData.contentType,
            'contentID': dataObject.custom.contentID ? dataObject.custom.contentID : dataObject.id,
            'contentEpisodeID': PageData.contentEpisodeID,
            'contentEpisodeTitle': PageData.contentEpisodeTitle,
            'contentChannelID': dataObject.custom.contentChannelID,
            'contentChannelTitle': dataObject.custom.contentChannelTitle,
            'contentTitle': contentTitle,
            'contentScreenMode': dataObject.contentScreenMode,
            'contentEventPercentageProgress': params.progress ? (params.progress * 100) + '%' : dataObject.type === 'live' ? undefined : dataObject.duration < 0 ? undefined : Math.round(dataObject.currentTime/dataObject.duration*100) + '%',
            'contentEventCurrentTime': params.progress ? Math.round(dataObject.duration * params.progress) : dataObject.duration < 0 ? undefined : Math.round(dataObject.currentTime),
            'contentOnlineReleaseTimeFrom': contentOnlineReleaseTimeFrom ? contentOnlineReleaseTimeFrom : PageData.articleDatePublished,
            'contentSource': dataObject.distributionType === 'live' ? 'live' : 'on-demand',
            'contentTimeConsumed': window.trackVideoPlayers[dataObject.id] ? Math.round(window.trackVideoPlayers[dataObject.id].consumedTime) : undefined,
            'contentVolumeStatus': dataObject.volume > 0 ? 1 : 0,
            'contentVolumePercentageLevel': dataObject.volume ? Math.round(dataObject.volume * 100) + '%' : undefined,
            'contentTabStatus': document.visibilityState === 'visible' ? '1' : '0',
            'contentAsset': 'video',
            'contentDistributionType': dataObject.distributionType,
            'contentPlayerType': dataObject.playerType ? dataObject.playerType : 'player',
            'contentNonCumulativeTimeConsumed' : params.nonCumulativeTimeConsumed && typeof params.nonCumulativeTimeConsumed != "undefined"  ? Math.round(params.nonCumulativeTimeConsumed) : undefined,
             //new
            'contentSeasonTitle' : dataObject.custom.contentSeasonTitle,
            'contentSeasonNumber' : dataObject.custom.contentSeasonNumber,
            'contentSeasonID' : dataObject.custom.contentSeasonID,
            'contentShowID' : dataObject.custom.contentShowID ? dataObject.custom.contentShowID : PageData.contentShowID,
            'contentUserActive': dataObject.userActive ? '1' : '0',
            'contentAutoplayStatus': dataObject.contentAutoplayStatus ? '1' : '0',
            'contentPlayerPreviewStatus': dataObject.custom.preview ? '1' : '0',
            'contentIdec': PageData.VideoIDEC ? PageData.VideoIDEC[dataObject.id] : undefined,
            'contentVideoTag': dataObject.custom.videoTag,
            'contentAuthor': PageData.contentAuthor,
            'contentPublishDate': PageData.contentPublishDate,
            'contentTvReleaseTime': PageData.contentTvReleaseTime ? PageData.contentTvReleaseTime : dataObject.custom.contentTvReleaseTime ? dataObject.custom.contentTvReleaseTime : undefined,
            //other
            'doNotTrackStatus': dataObject.custom.doNotTrackStatus,
            'adBlockStatus': PageData.adBlockStatus,
            'pagePlayerEnabled': PageData.pagePlayerEnabled,
            'pagePlayerAdsEnabled': PageData.pagePlayerAdsEnabled,
            'pageMiniplayerEnabled': PageData.pageMiniplayerEnabled,
            'pageMiniplayerAdsEnabled': PageData.pageMiniplayerAdsEnabled,
            'pageType': PageData.pageType,
            'pageID': PageData.pageId,
            'articleTopics': PageData.articleTopics,
            'articleAuthor': PageData.articleAuthor,
            'articleSponsored': PageData.articleSponsored,
            'articleDatePublished': PageData.articleDatePublished,
            'ai_content': PageData.ai_content ? PageData.ai_content : false,
            'contentAdPlayed': classInfo.prerollAdsPlayed ? 1 : 0,
            'brandSafetyStatus': Utils.GetBrandSafety()
        });

        if (params.event === 'video_player_terminate'){
          delete window.trackVideoPlayers[dataObject.id];
        }
    };

    var VideoEvents = function (data) {
        this.consumedTime = 0;
        this.nonCumulativeTimeConsumed = 0;
        this.afterAdConsumedTime = 0;
        this.data = data;
        this.debounced = true;
        this.half = true;
        this.finished = true;
        this.oneMinute = true;
        this.fiveMinute = true;
        this.heartbeatCounter = 1;
        this.afterAdsDebounced = true;
        this.prerollAdsPlayed = false;
    };

    VideoEvents.prototype.consumptionUpdate = function (data) {
        var timer = data.currentTime - this.data.currentTime;

        if (timer > 0 && timer < 1.9) {
            this.consumedTime += timer;
            this.nonCumulativeTimeConsumed += timer;
            if(this.afterAdsDebounced && this.prerollAdsPlayed){
              this.afterAdConsumedTime += timer;
            }
        }

        if (this.afterAdConsumedTime >= 3 && this.afterAdsDebounced && this.prerollAdsPlayed) {
            videoPushToDataLayer(data,{
              event: 'video_preroll_ads_debounced',
              gaEventAction:'preroll_ads_debounced',
              progress:undefined,
              matrixId: 64
            });
            this.afterAdsDebounced = false;
        }

        if (this.consumedTime >= 9.5 && this.debounced) {
            videoPushToDataLayer(data,{
              event: 'video_debounced',
              gaEventAction:'debounced',
              nonCumulativeTimeConsumed: this.nonCumulativeTimeConsumed,
              progress:undefined,
              matrixId:5
            });
            this.debounced = false;
            this.nonCumulativeTimeConsumed = 0;
        }
        if (
            Math.round((this.consumedTime / data.duration) * 100) >= 50 &&
            this.half && data.type !== 'live'
        ) {
            videoPushToDataLayer(data,{
              event: 'video_half',
              gaEventAction:'half',
              nonCumulativeTimeConsumed: this.nonCumulativeTimeConsumed,
              progress:undefined,
              matrixId:6
            });
            this.half = false;
            this.nonCumulativeTimeConsumed = 0;
        }
        if (
            Math.round((this.consumedTime / data.duration) * 100) >= 95 &&
            this.finished && data.type !== 'live'
        ) {
            videoPushToDataLayer(data,{
              event: 'video_finished',
              gaEventAction:'finished',
              nonCumulativeTimeConsumed: this.nonCumulativeTimeConsumed,
              progress:undefined,
              matrixId:7
            });
            this.finished = false;
            this.nonCumulativeTimeConsumed = 0;
        }
        if (this.consumedTime >= 59.5 && this.oneMinute) {
            videoPushToDataLayer(data,{
              event: 'video_one_minute_stream',
              gaEventAction:'one_minute_stream',
              nonCumulativeTimeConsumed: this.nonCumulativeTimeConsumed,
              progress:undefined,
              matrixId:30
            });
            this.oneMinute = false;
            this.nonCumulativeTimeConsumed = 0;
        }
        if (this.consumedTime >= 299.5 && this.fiveMinute) {
            videoPushToDataLayer(data,{
              event: 'video_five_minute_stream',
              gaEventAction:'five_minute_stream',
              nonCumulativeTimeConsumed: this.nonCumulativeTimeConsumed,
              progress:undefined,
              matrixId:31
            });
            this.fiveMinute = false;
            this.nonCumulativeTimeConsumed = 0;
        }
        if (data.type === 'live' && this.consumedTime >= (300 * this.heartbeatCounter) - 0.5) {
            videoPushToDataLayer(data,{
              event: 'video_heartbeat',
              gaEventAction:'heartbeat',
              nonCumulativeTimeConsumed: this.nonCumulativeTimeConsumed,
              progress:undefined,
              matrixId:36
            });
            this.heartbeatCounter ++;
            this.nonCumulativeTimeConsumed = 0;
        }
        this.data = data;
    };

    VideoEvents.prototype.adsPlayed = function (data) {
      if(!this.prerollAdsPlayed){
        this.prerollAdsPlayed = true;
        
        videoPushToDataLayer(this.data,{
          event: 'video_first_ad_start',
          gaEventAction:'first_ad_start',
          progress:undefined,
          matrixId:69
        });
      }
    };

    VideoEvents.prototype.consumptionReset = function (data) {
        this.consumedTime = 0;
        this.nonCumulativeTimeConsumed = 0;
        this.data = data;
        this.debounced = true;
        this.half = true;
        this.finished = true;
        this.oneMinute = true;
        this.twoMinute = true;
        this.afterAdsDebounced = true;
        this.prerollAdsPlayed = false;
        this.heartbeatCounter = 1;
    };

    window.trackVideoPlayers = {};
</script>
    <script>window.ajaxTimeouts={userTimeout:PageData.ajaxUserTimeout,globalTimeout:PageData.ajax_global_timeout}</script>
