<script>
    const hideElements = () => {
        const elements = document.getElementsByClassName('sssp-hide');
        if (!elements.length) {
            return;
        }
        for (let i = elements.length - 1; 0 <= i; i--) {
            let el = elements[i];
            if (el && el.parentNode) {
                el.parentNode.removeChild(el);
            }
        }
        replaceRteVoyoLinks();
        replaceNovaLogoLinks();
    };
    const replaceRteVoyoLinks = () => {
        // get RTE Voyo links
        const voyoLinks = document.querySelectorAll('.c-rte a[href*="voyo.nova.cz"], .c-hero a[href*="voyo.nova.cz"]');
        // replace RTE Voyo links one by one
        voyoLinks.forEach((item) => {
            const node = document.createElement('span');
            node.innerHTML = item.innerHTML;
            item.replaceWith(node);
        });
    };
    const replaceNovaLogoLinks = () => {
        const novaLogoHeaderLink = document.querySelector('.main a[href*="tv.nova.cz"]');
        const novaLogoFooterLink = document.querySelector('.c-footer a[href*="tv.nova.cz"]');
        
        const nodeHeader = document.createElement('span');
        nodeHeader.classList.add('c-logo');
        nodeHeader.innerHTML = novaLogoHeaderLink.innerHTML;
        novaLogoHeaderLink.replaceWith(nodeHeader);
        
        const nodeFooter = document.createElement('span');
        nodeFooter.classList.add('img');
        nodeFooter.innerHTML = novaLogoFooterLink.innerHTML;
        novaLogoFooterLink.replaceWith(nodeFooter);
    }
    const getPositions = () => {
        let positions = [
            // leaderboard desktop
            {'zoneId': 262177, 'id': "ssp-zone-262177", 'width': 970, 'height': 210, 'desktop': true, 'mobile': false},
            // board desktop
            {'zoneId': 262189, 'id': "ssp-zone-262189", 'width': 970, 'height': 310, 'desktop': true, 'mobile': false},
            // rectangle right 1 desktop
            {'zoneId': 262180, 'id': "ssp-zone-262180", 'width': 300, 'height': 300, 'desktop': true, 'mobile': false},
            // rectangle right 2 desktop
            {'zoneId': 262183, 'id': "ssp-zone-262183", 'width': 300, 'height': 600, 'desktop': true, 'mobile': false},
            // square article desktop
            {'zoneId': 262186, 'id': "ssp-zone-262186", 'width': 300, 'height': 300, 'desktop': true, 'mobile': false},
            // square article mobile
            {'zoneId': 262192, 'id': "ssp-zone-262192", 'width': 480, 'height': 480, 'desktop': false, 'mobile': true},
            // rectangle article mobile
            {'zoneId': 262201, 'id': "ssp-zone-262201", 'width': 480, 'height': 480, 'desktop': false, 'mobile': true},
            // rectangle after article mobile
            {'zoneId': 262198, 'id': "ssp-zone-262198", 'width': 480, 'height': 480, 'desktop': false, 'mobile': true},
            // pop up mobile
            {'zoneId': 262204, 'id': "ssp-zone-262204", 'width': 480, 'height': 480, 'desktop': false, 'mobile': true}
        ];

        if (window.innerWidth >= 1366) {
            positions = positions.map(position => {
                if (position.id === "ssp-zone-262177") {
                    delete position.zoneId
                }
                return position
            })
            positions.push({
                'zoneId': 262177,
                'id': "ssp-zone-262177",
                'width': 2000,
                'desktop': true,
                'mobile': false,
                'elements': [
                    {'id': 'ssp-zone-262177', 'width': 970},
                    {'id': 'ssp-zone-262177-branding', 'width': 2000, 'height': 1400},
                ]
            })
        }
        return positions
    };

    const isValidDevicePosition = (position) => {
        return (position.desktop && window.innerWidth >= 1000) || (position.mobile && window.innerWidth < 1000);
    };
    const getActivePositions = (elements, position) => {
        if (elements.length > 1) {
            let positions = [];
            for (let i = 0; i <= elements.length - 1; i++) {
                let activePosition = {
                    'id': elements[i].id,
                    'width': position.width,
                    'height': position.height,
                    'desktop': position.desktop,
                    'mobile': position.mobile
                }

                if (position.zoneId) activePosition.zoneId = position.zoneId,
                    positions.push(position);
            }
            return positions;
        }
        return [position];
    };

    const seznamAds = () => {
        hideElements();
        const positions = getPositions();
        let adPositions = [];
        for (const position of positions) {
            if (!isValidDevicePosition(position)) {
                continue;
            }
            const elements = document.querySelectorAll('*[id^="ssp-zone-' + position.zoneId + '"]');
            if (!elements.length) {
                continue;
            }
            let activePositions = getActivePositions(elements, position);
            adPositions.push(...activePositions);
        }

        if (adPositions) {
            // sssp.getAds(adPositions)
            adPositions.map(adPosition => {
                sssp.getAds(adPosition)
            })

        }
    };

    const useSeznamAds = sssp && sssp.displaySeznamAds();

    document.addEventListener("DOMContentLoaded", function(e) {
        if (useSeznamAds) {
            seznamAds();
        } else {
            googletag.cmd.push(function() { googletag.pubads().refresh(); });
        }
    });
    
    // hide elements immediately by styles
    if(useSeznamAds) {
        const head = document.getElementsByTagName('head')[0];
        const styleElement = document.createElement('style');
        styleElement.innerHTML = 
            '/*hide sssp-hide elements by styles*/'
            +'.sssp-hide{ display: none !important; }'
        ;
        head.append( styleElement );
    }
    
</script>

