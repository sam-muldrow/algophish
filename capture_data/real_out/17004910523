<div id="MobileBranding"></div>
<!--DigitalAd mobile branding-->
<div id="da-sticky"></div>

<script>

    // temporary styles - workaround 20.7.23
    let tmpStylesR2B2 = document.createElement('style');
    tmpStylesR2B2.innerHTML = 'body.mobile-menu-active #da-sticky>*, body.mobile-menu-active [data-adtrack-container]>* { bottom: 0px !important; }';
    document.querySelector('head').append(tmpStylesR2B2);



document.addEventListener('DOMContentLoaded', function() {
    if(!Utils.IsResponsive()) { return false; }
    
    if (!useSeznamAds && !PageData.adsDisabled?.mobileBranding) {
        abcBrandingTestDecision();
    } else {
        console.info('mobile branding disabled on this page');
    }
}, false);

abcBrandingTestDecision = function() {
    
    let abcValue = Math.floor( Math.random() * 3 );

// Just test for new DigitalAd ad server
//useDAIBB_Branding_NewAdserver2();
//return;

    switch (abcValue) {
        case 0: 
            useR2B2_Branding();
            break;
        case 1:
            //useDAIBB_Branding();
            //useDAIBB_Branding_NewAdserver();
            useDAIBB_Branding_NewAdserver2();
            break;
        case 2: 
            usePerformax_Branding();
            break;
    }
    
}

useR2B2_Branding = function() {
    console.info('use R2B2 Branding');
    var afterElement = document.getElementById("MobileBranding");
    var appendedDivWrapper = document.createElement('div');
    var wrapperClass = 'MobileBrandingWrapper';
    var HTML = '<script src="//delivery.r2b2.cz/get/tv.nova.cz/generic/fixed"><\/script>';
    appendedDivWrapper.setAttribute('class', wrapperClass)
    afterElement.parentNode.insertBefore(appendedDivWrapper, afterElement.nextSibling);
    setInnerHTML(document.querySelector('.' + wrapperClass), HTML);
}


