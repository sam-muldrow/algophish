// New DigitalAd ad server 2
useDAIBB_Branding_NewAdserver2 = function() {
    console.info('use DAIBB Branding New AdServer 2 Test');
    const daScriptElement = document.createElement('script');
    const bodyElement = document.getElementsByTagName('body')[0];
    daScriptElement.src = 'https://ads.richmedia.cz/js/da-nova.js';
    bodyElement.append(daScriptElement);
}
//New DigitalAd ad server 2 END

usePerformax_Branding = function() {
    console.info('use Performax Branding');
    window.__pxPageConfig = window.__pxPageConfig || {};
    if(typeof PageData.mobileMenuBottom !== 'undefined' ) {
        window.__pxPageConfig.anchor = { bottom: "60"};
    }
    const performaxScriptElement = document.createElement('script');
    const bodyElement = document.getElementsByTagName('body')[0];
    performaxScriptElement.async = true;
    performaxScriptElement.src = 'https://cdn.performax.cz/yi/adsbypx/px_autoads.js';
    bodyElement.append(performaxScriptElement);
}

    
