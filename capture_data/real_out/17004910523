    sasControll = {
        disable: typeof useSeznamAds!=='undefined' && useSeznamAds,
        banners: null,
        trackCalled: false,
        sasObjectFull: window.AdsObject || {},
        lazyload: false,
        init: () => {
            
            // workaround - remove questionmarks from names in targeting - START
            // - replacing questionmarks with url encoded questionmarks "%3F"
            if(typeof AdsObject.ball.show_episode_name !== 'undefined') {
                AdsObject.ball.show_episode_name = AdsObject.ball.show_episode_name.replaceAll('?', '%3F');
            }
            // workaround - remove questionmarks from names in targeting - END
            
            sasControll.separeBanners();
            sasControll.setLayoutTargeting();
            sasControll.prepareSizes();
            sasControll.tagBanners();
            //sasControll.requestAll();
        },
        separeBanners: () => {
            sasControll.banners = { ...window.AdsObject};
            delete sasControll.banners.ball;
        },
        setLayoutTargeting: () => {
            const deviceSasTargeting = Utils.IsResponsive() ? 'mobile' : 'desktop';
            window.AdsObject.ball.layout = deviceSasTargeting;
        },
        prepareSizes: () => {
            for (const [key, value] of Object.entries(sasControll.banners)) {
                const sizeObj = JSON.parse(window.AdsObject[key].size);
                if(sizeObj[window.AdsObject.ball.layout].length) {
                    window.AdsObject[key].size = sizeObj[window.AdsObject.ball.layout];
                    if(sasControll.lazyload) {
                        window.AdsObject[key].options = 'lazy';
                    }
                }else {
                    delete window.AdsObject[key];
                }
            }
        },
        requestAll: () => {
            if(sasControll.disable) { return; }
            sasTracker.track();
        },
        requestOneBx: (bx) => {
            if(sasControll.disable) { return; }
            sasTracker.loadBx(bx);
        },
        tagBanners: () => {
            for (const [key, value] of Object.entries(sasControll.banners)) {
                let banner = document.querySelector(`#${value.divId}`);
                if(!banner) { continue; }
                banner.setAttribute('data-bx', key);
                banner.setAttribute('data-area', value.area);
                banner.setAttribute('data-pos', value.pos);
            }
        },
        enableDebug: () => {
            localStorage.setItem('sas-debug-enable',1);
            console.info('[SAS DEBUG] Debug mód byl povolen, můžete jej aktivovat pomocí příkazu "sasDebugger.init();"');
        }
    }
    
    sasControll.init();
    
    </script>

    <!-- SAS DEBUGGER INIT -->
    <script>
    
    // load sasdebug from external place
    loadSasDebug = () => {
        if(typeof sasDebugger.sasObject !== 'undefined') {
            return;
        }
        const body = document.querySelector('body');
        const script = document.createElement('script');
        script.src = '/html/modules/sas-debugger/sasDebugger.js?v=2';
        script.type='text/javascript';
        script.setAttribute('onload', 'sasDebugger.init()');
        body.append(script);
    };
    
    // check if sasdebugger exist
    if(typeof sasDebugger === 'undefined') {
        sasDebugger = {
            init: () => {
                console.info('LOADED FROM TMP DEBUGGER');
                loadSasDebug();
            }
        }
    }
    
    // sas debugger init
    if(new URL(document.location).searchParams.has('sasdebug') || !!sessionStorage.getItem('sass_debug')) {
        sasDebugger.init();
    }

    // enable SAS Debug
    if(new URL(document.location).searchParams.has('enable-sasdebug')) {
        sasControll.enableDebug();
    }
    
