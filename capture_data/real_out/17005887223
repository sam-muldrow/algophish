<script type="text/javascript">
    console.log("DisclaimerDisplay", DisclaimerDisplay);
    //var sRGPDcguTTL = "RGPD.cgu.TTL";
    //var s = localStorage.getItem(sRGPDcguTTL);
    //console.log("lecture RGPdcgu", s);
    //if (s != "1") {
    if(DisclaimerDisplay) {
        var sRGPDcookieVIP = "VIP";
        var s = readCookie(sRGPDcookieVIP);
        console.log("cookie VIP", sRGPDcookieVIP, s);
        if (s == "1") 
            $('#RGPDpartCOM').attr('checked', false);
        else
            $('#RGPDpartCOM').attr('checked', true);

        var id = '#RGPDmodal';
        $('#RGPDpartDisclaimerHref').attr("href", DisclaimerUrl + "#CGU" );
        // Effet de transition     
        $('#fade').fadeIn(1000);   
        $('#fade').fadeTo("slow",0.8);
        // Effet de transition   
        $('#RGPDpartCGU').removeAttr('checked');
        checkCGUrefresh();
        $(id).fadeIn(2000);
        
        $('#RGPDbuttonOK').click(function (e) {
            e.preventDefault();
            //localStorage.setItem(sRGPDcguTTL, "1");
            var bCom = $('#RGPDpartCOM').attr('checked')=="checked";
            console.log("Offres commerciales",bCom);
            AFP.MediaForum.ScriptServices.APIService.DisclaimerValidate(bCom);
            DisclaimerDisplay = false;
            hideModalRGPD();
        });
        $('#RGPDpartCGU:checkbox').on("change", function () {
            checkCGUrefresh();
        });
    }   
</script>
