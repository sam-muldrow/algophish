                        <!--loading 開始 -->
                        <div class="loading"></div>
                        <!--loading 結束 -->

                        <!--無限滾動相關模組 開始-->
                        <script>
                            (function () {
                                var iWinH = $(window).innerHeight();
                                var iWinScrollTop = 0;
                                var iPoint = null; //觸發點
                                var iLimit = 20; //最大觸發次數
                                var iCount = 0; //觸發次數
                                var iBoxH; //容器高
                                var iBoxOffsetTop; //容器距離視窗頂部
                                var iBoxOffsetBottom; //容器距離視窗底部

                                //偵測到底 => 載入下一批資料
                                function fnBottom() {
                                    iWinScrollTop = $(this).scrollTop();
                                    iBoxH = $(".r3 .c2 ").innerHeight();
                                    iBoxOffsetTop = $(".r3 .c2 ").offset().top;
                                    iBoxOffsetBottom = iBoxOffsetTop + iBoxH;
                                    iPoint = iBoxOffsetBottom - iWinH - 500;
                                    if (iCount <= iLimit && iWinScrollTop > iPoint) {
                                        $(window).off("scroll.myWinScroll", fnBottom);
                                        fnAjax();
                                    }
                                }

                                //載入資料
                                function fnAjax() {
                                     $(".r3 .c2 .loading").show();
                                     $.post("//www.ettoday.net/show_roll_2023i.php", {
                                         offset: iCount,
                                         smark: "mark_2023i",
                                         sarea: 0,
                                         sitem: 100
                                       })
                                       .done(function(data){
                                         $(".infinite_scroll .part_list_8").append(data); //加入資料
                                         $(".r3 .c2 .loading").hide();
                                         iCount++;
                                         if(iCount > iLimit) return false; //滿足最大觸發次數,不綁scroll.myWinScroll,不偵測到底
                                         $(window).on("scroll.myWinScroll", fnBottom);
                                       });
                                }

                                //初始
                                $(window).on("scroll.myWinScroll", fnBottom);
                            })();
                        </script>
                        <!--無限滾動相關模組 結束-->
                    </div>
                    <!--直欄 c2 結束-->

                    <!--直欄 c3 開始-->
                    <div class="c3">
