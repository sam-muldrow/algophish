		function ShowAllBanner() {

			var elem1 = $("#show_all_banner_btn").find($("span.arr"));
			var elem2 = $("#show_all_banner_btn").find($("span.switch-text"));

			if($('#banner_bottom').attr('class') == 'hide')
			{
				$('#banner_bottom').removeClass('hide');
				$(elem1).addClass('m-close').removeClass('m-open');
				$(elem2).text('Свернуть');
			}
			else
			{
				$('#banner_bottom').addClass('hide');
				$(elem1).addClass('m-open').removeClass('m-close');
				$(elem2).text('Все информационные партнеры');
			}
			return false;
		}
		</script>
