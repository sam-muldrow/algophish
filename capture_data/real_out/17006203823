    function decorateURL(url) {
        var linkerParam = null;
        ga(function() {
            var trackers = ga.getAll();
            linkerParam = trackers[0].get('linkerParam');
        });
        return (linkerParam ? url + '?' + linkerParam : url);
    }
</script>
