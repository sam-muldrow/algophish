<script>
  function include(src, async, defer, type){
      if(type === 'text/javascript') {
          var d = document,
              h = d.getElementsByTagName('head')[0],
              s = d.createElement('script');
          s.type = type;
          s.async = async;
          s.defer = defer;
          s.src = src;
          h.appendChild(s);
      } else if (type === 'text/css') {
          var d = document,
              h = d.getElementsByTagName('head')[0],
              s = d.createElement('link');
          s.type = type;
          s.async = async;
          s.defer = defer;
          s.href = src;
          s.rel = 'stylesheet'
          h.appendChild(s);
      }
  }
</script>


