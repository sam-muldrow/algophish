var elementExists = document.getElementById("required-jobnum");

if ( elementExists ){

function getURLParameter(name) {
  return decodeURIComponent((new RegExp('[?|&]' + name + '=' + '([^&;]+?)(&|#|;|$)').exec(location.search) || [null, ''])[1].replace(/\+/g, '%20')) || null;
}

var url_string = document.URL;
//var url = new URL(url_string);
var url = window.location.search;
console.log(url);

var vacancy  = getURLParameter('vacancy');
var position = getURLParameter('position');
console.log(vacancy);
console.log(position);

document.getElementById('required-jobnum').value = vacancy;
document.getElementById('required-jobtitle').value = position;

}
