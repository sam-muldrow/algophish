<script>
/*-- salesforce --*/
var r = document.cookie.split(';');
var c_email = "";
var logout = "";

r.forEach(function(value) {
	var content = value.split('=');
	if (content[0].trim() == "email") {
		c_email = content[1];
		c_email = c_email.replace(/[\"]/g,"");
	}
	if (content[0].trim() == "op_logout") {
	logout = content[1];
	}
})

if (c_email != "" && logout == "") {
	_etmc.push(["setOrgId", "514001296"]); 
	_etmc.push(["setUserInfo", {"email": c_email}]);
	_etmc.push(["trackPageView"]);
}
/*-- salesforce --*/
</script>


