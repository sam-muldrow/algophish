<script>
let splide;
let pageNum = 0;
const gotPageNums = [];

$(function(){
  // Splide初期化
  splide = new Splide('.splide', {
    pagination: false,
    speed: 720, // default: 400
  });
  splide.mount();
  callApi("");
  gotPageNums.push(pageNum);
  splide.on('move', (newIndex, prevIndex, destIndex) => {
    pageNum = newIndex;
    if ((pageNum + 1) % pageFetchAmount == 0 && !gotPageNums.includes(pageNum + 1)) {
        fetchNewItem($(".tab-nav-selected").attr('value'));
        gotPageNums.push(pageNum + 1);
        console.log("newItemFetched.", gotPageNums);
    }
  });
});
/*
function changeSpeed() {
  $("#speed_label").text($("#splide_speed").val());
