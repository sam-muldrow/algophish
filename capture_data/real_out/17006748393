<script type="text/javascript">
	$(document).ready(function(){
		$(window).scroll(function () {
			var show_cp = 0;
			var cookies = document.cookie;
			
			if (cookies.length > 0 != "") {
				var cookies = cookies.split("; ");
				
				for (var i = 0; i < cookies.length; i++ ) {
					var cookie = cookies[i].split('=');
					if (cookie[0] == "show_cp") {
						show_cp = decodeURIComponent(cookie[1]);
					}
				}
			}
			if (show_cp == "0") {
				if ($(this).scrollTop() > 0) {
					$("#btm_cpn").fadeIn(500);
				} else {
					$("#btm_cpn").fadeOut(500);
				}
			}
		});
		$('.cls_btn span').click(function () {
			document.cookie = "show_cp=1";
			
			$("#btm_cpn").fadeOut(500);
		});
		
		
		var fitting = function() {
			//要素を中心に移動
			var a = $(".slider").width();
			var b = 480 - (a - 480) / 2;
			$(".slider-visible").css('left', -b + 'px');
		};
		$(".slider-for").slick({
			slidesToShow: 1,
			slidesToScroll: $(".slider-for img").length,
			infinite: true,
			centerPadding: '480px',
			centerMode: true,
			autoplay:true,
			autoplaySpeed: 7500,
			speed: 800,
			lazyLoad: 'progressive',
			variableWidth: true,
			slide: 'div'
		});
		
		
		/*
		$(".slider-for").slick({
			slidesToShow: 1,
			slidesToScroll: $(".slider-nav img").length,
			infinite: true,
			centerPadding: '480px',
			centerMode: true,
			autoplay:true,
			autoplaySpeed: 7500,
			speed: 800,
			lazyLoad: 'progressive',
			variableWidth: true,
			slide: 'div',
			asNavFor: '.slider-nav'
		});
		$('.slider-nav').slick({
			slidesToShow: $(".slider-nav img").length,
			focusOnSelect: true,
			variableWidth: true,
			arrows: false,
			lazyLoad: 'progressive',
			slide: 'img',
			asNavFor: '.slider-for'
		});
		$('.slider-for').on('beforeChange', function(){ $('.slider-nav .slick-slide').css('pointer-events','none'); });
		$('.slider-for').on('afterChange', function(){ $('.slider-nav .slick-slide').css('pointer-events','auto'); });
		*/
		fitting();
		
		$(window).resize(function() {
			fitting();
		});
	});

	
