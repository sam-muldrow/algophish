		function loadScript(src, callback) {
			var done = false;
			var head = document.getElementsByTagName('head')[0];
			var script = document.createElement('script');
			script.src = src;
			head.appendChild(script);
						script.onload = script.onreadystatechange = function() {
				if (!done && (!this.readyState ||
						this.readyState === "loaded" || this.readyState === "complete")) {
					done = true;
					callback();
										script.onload = script.onreadystatechange = null;
					if (head && script.parentNode) {
						head.removeChild(script);
					}
				}
			};
		}
		
		function loadImage(image) {
			image.src = image.dataset.src;
			image.className = image.className.replace('lazyload', 'lazyloaded');
		}
		
		function allloadimage(isSticyChange) {
			const lazy_imgs = document.querySelectorAll('.lazyload');
			var nodes = Array.prototype.slice.call(lazy_imgs, 0);
			nodes.forEach(function(a, idx, array) {
				if (isSticyChange && $.fn.matchHeight) {
					a.onload = differMatchHeight;
				}
				return loadImage(a);
			});
		}

				var heightTimer;
		var caschHeight = 0;
		var isSingle = "";

		function heightChange() {
			var sideheight = $('.side_calc_height_sticky_js').outerHeight(true);
			var mainHeightTargets = $('.main_calc_height_sticky_js');
			var mainheight = 0;
			for (var i = 0; i < mainHeightTargets.length; i++) {
				mainheight += $(mainHeightTargets[i]).outerHeight(true);
			}
			if (mainheight == caschHeight) {
								clearInterval(heightTimer);
			} else {
				caschHeight = mainheight;
			}
			$('.height_for_sticky_js').css({
				'height': (mainheight - sideheight)
			});
		}

				var isSingle = "";
		if (!mql.matches) {
			loadScript("//cdn-image.as-web.jp/etc/js/lazysizes.min.js", function() {});
		} else {
