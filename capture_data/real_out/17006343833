		var Nav = (function() {
			var
				nav = $('.tablike'),
				section = $('.tab_item'),
				link = nav.find('.tablike_link'),
				hasT = false,
				heightTarget_img = $('.img_block'),
				heightTarget_date = $('.date_block'),
				tabCon = $('.tab_content'),
				toCal = $('p.toCalendar'),
				today = $('.today_js'),
				weekend = $('.weekend_js'),
				tableft = $('.tab_left'),
				tabright = $('.tab_right');

			var switchPage = function(e) {
				var self = $(this);

				var i = self.parents('.tablike_item').index();
				var s = section.eq(i);
				var a = $('.tab_item_active');
				var t = $(e.target);
				var tab_head_active = $('.tab_head_active');

				if (!hasT) {
					if (i == a.index()) {
						return false;
					}

					var isweekwnd = self.data('weekend');
					if (isweekwnd) {
						$('.raceCalendar').addClass('weekend_js');
					} else {
						$('.raceCalendar').removeClass('weekend_js');
					}

					tab_head_active.removeClass('tab_head_active');
					if (!$(self.children()[0]).hasClass('tab_head_active')) $(self.children()[0]).addClass('tab_head_active');

					a.addClass('tab_item_hidden').removeClass('tab_item_active');
					s.addClass('tab_item_active');

					hasT = true;

					a.on('transitionend webkitTransitionend', function() {
						$(this).removeClass('tab_item_hidden');
						hasT = false;
						a.off('transitionend webkitTransitionend');
					});
				}
				return false;
			};

			var heightControll = function() {
				var p_cal_height = toCal.outerHeight(true);
				var today_height = 0;
				var weekend_height = 0;
				for (var i = 0; i < today.length; i++) {
					if (today_height < today[i].clientHeight) today_height = today[i].clientHeight;
				}

				for (var i = 0; i < weekend.length; i++) {
					if (weekend_height < weekend[i].clientHeight) weekend_height = weekend[i].clientHeight;
				}

				weekend_height += p_cal_height;
				var height = (weekend_height > today_height) ? weekend_height : today_height;

				tabCon.css('height', (height + 'px'));
				section.css('height', (height + 'px'));

				if (weekend_height > today_height) {
					tableft.css("padding-top", (weekend_height - today_height) / 2 + 'px');
					tableft.css("padding-bottom", (weekend_height - today_height) / 2 + 'px');
					//tabright.css("padding-top",'8px');
					//tabright.css("padding-bottom",'8px');
				} else {
					tabright.css("padding-top", (today_height - weekend_height) / 2 + 'px');
					tabright.css("padding-bottom", (today_height - weekend_height) / 2 + 'px');
					//tableft.css("padding-top",'8px');
					//tableft.css("padding-bottom",'8px');
				}
			}

			var bindActions = function() {
				link.on('click', switchPage);
				heightControll();
			};

			var init = function() {
				bindActions();
				$(window).on('resize', function() {
					heightControll();
				});
			};

			return {
				init: init
			};

		}())
		Nav.init();
	});</script> </section> <!-- //レースカレンダー --><!-- specialTextTagOuter --> <!-- 未設定　緑背景のclassはgreen、黒背景のclassはblack、赤背景のclassはred --><div class="specialTextTagOuter"> <span class="specialTextTag tag black"> <a href="/tag/%E3%83%88%E3%83%A8%E3%82%BFgr-2024%E5%B9%B4%E4%BD%93%E5%88%B6%E7%99%BA%E8%A1%A8"> トヨタGR 2024年体制発表まとめ </a> </span> <span class="specialTextTag tag brue"> <a href="/race_summary/f1/2023f1/2023f1-rd22"> F1第22戦ラスベガスGPまとめ </a> </span> <span class="specialTextTag tag green"> <a href="/race_summary/bike/2023-motogp/2023-motogp-rd19"> MotoGP第19戦カタールGPまとめ </a> </span> <span class="specialTextTag tag red"> <a href="/race_summary/rally/2023_wrc/2023_wrc_rd13"> WRC第13戦ラリージャパンまとめ </a> </span></div> <!-- /specialTextTagOuter --><!-- バナーエリア --><div class="p-bannar_payment_old"><p class="c-text__lv3">レースを愛してやまないファンの方々へ<br />autosport web Premiumが登場。</p><p class="c-text__lv4">詳細を見る</p> <a href="/membership/lp" onclick="ga('send', 'event', 'banner', 'click', 'member-lp', 1);"></a></div> <!-- 電子書籍バナー --> <!-- Latest News --><section id="latestNews" class="newsList"><h2><span>Latest </span>News</h2><!-- F1 News --><section id="f1news" class="newsListSub"><div class="subcat_heading"><span>Formula 1</span></div><div class="details"><article><div class="box"><div class="thumbs "> <a href="/f1/1017987"> <img data-src="https://cdn-image.as-web.jp/2023/11/22125202/asimg_XPB_1187087_HiRes_5a655d7ae122117-300x200.jpg" width="300" height="200" alt="F1 | 2023年F1第23戦アブダビGP　TV放送＆タイムスケジュール" class="lazyload" /> </a></div><h3 class="h4"><a href="/f1/1017987">2023年F1第23戦アブダビGP　TV放送＆タイムスケジュール</a></h3><h4 class="h5"> <a class="category" href="/f1">F1</a> <span class="date">2023/11/22</span></h4></div></article><article><div class="box"><div class="thumbs "> <a href="/f1/1017837"> <img data-src="https://cdn-image.as-web.jp/2023/11/22042340/asimg_s-XPB_1254906_HiRes_1e655d03bc0a5b3-300x200.jpg" width="300" height="200" alt="F1 | 【SNS特集】F1ラスベガスGP：レース後もスペースを与えないフェルスタッペン。ルーキー、サージェント3度目の母国GP" c