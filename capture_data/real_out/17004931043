	$(document).ready(function(){
		googleTranslateElementInit();
		googleTranslateElementInit2()
		formBox.selectInit();
		
		/* TradeShows에 icon 추가 */
		$('.iconGnb').eq(0).addClass('icoGnb5');
		$('.iconGnb').eq(1).addClass('icoGnb6');
		
		$('body').click(function(e){
			if(typeof e.originalEvent == "undefined"){
				return false;			  
			}else{
				if(!$(e.originalEvent.target).closest('.searchBox').length){
					// 검색영역외에
					$('.searchInfoView').hide();
					$('.searchInfoView2').hide();
				}			   
			}
		});
		
		lazyImg('.otherLink');
	});
	
	function googleTranslateElementInit() {
		new google.translate.TranslateElement(
			{
				pageLanguage: "en",
				includedLanguages: "en,zh-CN,zh-TW,es,pt,ja,ru",
				layout: google.translate.TranslateElement.InlineLayout.SIMPLE,
				autoDisplay: true,
				multilanguagePage: true
			}, 
			'google_translate_element');
	}
	
	function googleTranslateElementInit2() {
		new google.translate.TranslateElement(
			{
				pageLanguage: "en",
				includedLanguages: "en,zh-CN,zh-TW,es,pt,ja,ru",
				layout: google.translate.TranslateElement.InlineLayout.SIMPLE,
				autoDisplay: true,
				multilanguagePage: true
			}, 
			'google_translate_element2');
	}
	
	function fn_logout() {
		location.href='/membership/logout.do?action=logout';
	}
	
	function fn_searchCategory(url) {
		location.href = encodeURI(url);
	}

	$(function(){
	  formBox.selectInit();
  	});
	
</script>
<script>
	var typingTimer;				//timer identifier
	var doneTypingInterval = 1000;  //time in ms, 1 second for example
	
	$(function(){
	  	// 검색영역 클릭 시 동작
		$('#header .searchBox input').focusin(function(){
		  	// 입력 값 확인
			let word = $(this).val();
			if(word){
			  	// 입력 값 있으면 자동완성
			  	searchAutoComplete();
			 	// 입력 값 있으면 인기검색어,최근검색 숨기기
			  	$('.searchInfoView').hide();
			  	
			 	// 추천상품 1초뒤에 들고오기
			  	typingTimer = setTimeout(function(){			  	  
			  	  recomendProducts();			  
			  	}, doneTypingInterval);
			}else{
			  	// 입력 값 없으면 인기검색어 출력
			  	getPopkeyword();				
			}
		})
		// 검색영역에 입력 시 동작
		$('#header .searchBox input').keyup(function(){		  
		  	// 한국어, 특수문자 입력 방지
		  	 $(this).val( $(this).val().replace( /[ㄱ-ㅎ|ㅏ-ㅣ|가-힣|[\{\}\[\]\/?.,;:|\)*~`!^\-_+<>@#$%&\\\=\(\'\"]/g, '' ) );
		  	/* $(this).val( $(this).val().replace( /[^a-z|A-Z]/g, '' ) ); */
		  	
		 	// 입력 값 확인
			const word = $(this).val();
		 	// 1초안에 입력 하지않으면 실행(입력 초기화 기능)
			clearTimeout(typingTimer);
			
			if(word == '' || !word){
			  	// 검색 영역에 입력 값 없을 시 추천상품, 자동완성 숨기기
				$('.searchInfoView2').hide();
			  	// 인기검색어 출력
				getPopkeyword();
			}else{
			  	// 검색 영역에 입력 값 있을 시 인기검색어, 최근검색 숨기기
			  	$('.searchInfoView').hide();
			  	// 자동완성 출력
			  	searchAutoComplete();
			  	
		  	   // 추천상품 1초뒤에 들고오기
			  	typingTimer = setTimeout(function(){
			  	  recomendProducts();			  
			  	}, doneTypingInterval);
			}
		});

		lazyImg('.otherLink')
	})	
</script>
