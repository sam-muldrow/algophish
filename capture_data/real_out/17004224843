<script>

    /**
     *
     * @param id
     * @returns {*}
     */
    function findMatch(id){
        return $.grep(_bets.bets, function(e) {
            return e.m_id == id;
        })[0];
    }


    var _bets = {
        bets:           [],
        active_express: [],

        /**
         *
         * @param bets
         */
        populateBets: function (bets) {
            var self = this;

            bets.forEach(function(b) {
                var bet = normalizeBet(b);

                if (bet.m_status === 0) {
                    reCalcSumBets(bet);
                } else {
                    if (typeof bet.sumbets !== "undefined" && typeof bet.sumbets[USER_CURRENCY] !== "undefined") {
                        bet.m_bets_a = parseInt(bet.sumbets[USER_CURRENCY][1] || 0, 10);
                        bet.m_bets_b = parseInt(bet.sumbets[USER_CURRENCY][2] || 0, 10);
                    }
                }

                var minBetValue = 100000;
                if (USER_CURRENCY !== 'RUB') {
                    minBetValue = minBetValue * currency_rates['RUB2' + USER_CURRENCY];
                }
                if (bet.m_status === 5 && bet.m_bets_a + bet.m_bets_b < minBetValue) {
                    return;
                }

                bet.user_currency = USER_CURRENCY;

                Object.observe(bet, betLoungeIndexObserver);

                self.bets.push(bet);
            });
        },

        /**
         *
         * @param bet
         * @param prepend
         */
        createNewBlock: function (bet) {
            if (bet.m_status === 0) {
                if (matchWrappers.next.find(".bet_coming").length)
                    matchWrappers.next.find(".bet_coming").first().before(getTmpl('index_match', bet));
                else
                    matchWrappers.next.tmpl('index_match', bet, true);
            }
            else if (bet.m_status === 1) {
                if (matchWrappers.live.find(".bet_now").length)
                    matchWrappers.live.find(".bet_now").last().after(getTmpl('index_match', bet));
                else
                    matchWrappers.live.tmpl('index_match', bet, true);
            }
            else
                matchWrappers.past.tmpl('index_match', bet, false);
        },

        /**
         *
         */
        renderBets: function () {
            var self = this;

            this.bets.forEach(function(bet){

                if (isHistoryMode() && bet.m_status <= 1)
                    return;

                if (!isHistoryMode() && bet.m_status > 1)
                    return;

                self.createNewBlock(bet);
            });

            this.handleNoActiveMessage();
        },

        /**
         *
         */
        handleNoActiveMessage: function() {
            var nomatches = $('<div class="bet-items__not-items sys-no-active-matches"><div class="bet-items__not-items-text">No current matches</div></div>');

            if (!matchWrappers.next.children().length && !matchWrappers.live.children().length && isTotalMode()) {
                matchWrappers.next.append(function() {
                    var t = nomatches.clone();
                    t.find(".bet-items__not-items-text").text("Schedule awaiting confirmation");
                    return t;
                }());
            } else {
                matchWrappers.next.find(".sys-no-active-matches").remove();
            }
        },

