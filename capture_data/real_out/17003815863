<script>
window.ajaxLogin = function() {
var url =  "ajaxLoginUrl" in window ? ajaxLoginUrl:"//www1.pcbaby.com.cn/common/js/pcbaby.login.1.1-min.js";
url && (loadJs(url), url = "");
function loadJs(url) {
var js = document.createElement("script");
js.src = url;
js.charset = "GBK";
document.getElementsByTagName("head")[0].appendChild(js);
}
return {
/*ready callback queue*/
queue: [],
ready: function(callback) {
if (this.queue) {
this.queue.push(callback);
} else {
callback.call(this);
}
}
}
}();
</script>
