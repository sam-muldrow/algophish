var s_input=pc.getElem("#q");
var form_div=pc.getElem("#s-form");
pc.addEvent(form_div,"click",function(e){
form_div.className='form form-active';
s_input.value='';
e.stopPropagation();
})
pc.addEvent(pc.getElem("body"),"click",function(){
form_div.className='form';          
})
function hoverli(){
/*ÅÅÐÐ°ñliµÄÊó±ê¾­¹ý*/
var rankLi = pc.getElems('.rank li');
pc.each(rankLi, function(li){
pc.addEvent(li,"mouseenter", function(){
this.className="hover";
})
pc.addEvent(li,"mouseleave", function(){
this.className="";
})
});
}
</script>
<script>
_fnWinPop = {
init: function() {
var that = this,
cookieName = "pcbaby_sy_full_2021",
cookieVule = '1';
var popWinCookie = that.getCookie(cookieName);
if (!popWinCookie) {
//that.getHtml();
//$('.doc').addClass('hide');
//$('.popWinFull').fadeIn(1);
//var time = document.getElementById("countTime"),i = 3;
//countTime();
} else {
//$('.doc').removeClass('hide');
}
function countTime() {
var fn = arguments.callee;
if (i >= 0) {
setTimeout(function() {
time.innerHTML = i;
fn(--i);
}, 1000);
} else {
that.setCookie(cookieName, cookieVule, that.getExpireTime());
$('.popWinFull').fadeOut(400);
//$('.doc').removeClass('hide');
}
}
document.getElementById("jpopWinBottomClose").onclick = function() {
$('#jpopWinBottom').animate({
width: '0'
}, 1500, 'swing', function() {
$('.popWinBottomInner').hide();
$('.popWinBottomSmall').show();
$('#jpopWinBottom').animate({
width: '180px'
}, 400, 'swing');
})
}
$(".popWinBottomSmall").click(function() {
$('#jpopWinBottom').animate({
width: '0'
}, 400, 'swing', function() {
$('.popWinBottomInner').show();
$('.popWinBottomSmall').hide();
$('#jpopWinBottom').animate({
width: '100%'
}, 1500, 'swing');
})
});
},
getHtml: function() {
var _html = '';
_html += '<div class="popWinFull">' +
'<a href="#" class="link link1"></a>' +
'<a href="#" class="link link2"></a>' +
'<div class="countTime"><i id="countTime">5</i>s</div>' +
'</div>';
$('#Jbody').prepend(_html);
},
getExpireTime: function() {
var date = new Date();
var hour = 23 - date.getHours();
var min = 59 - date.getMinutes();
var ms = (3600 * hour + 60 * min) * 1000;
return ms;
},
getCookie: function(name) {
return unescape(unescape(document.cookie.replace(new RegExp(".*(?:^|; )" + name + "=([^;]*).*|.*"), "$1")));
},
setCookie: function(cookiename, cookievalue, milsecond) {
var date = new Date();
//console.log(milsecond)
date.setTime(date.getTime() + milsecond);
document.cookie = cookiename + "=" + cookievalue + "; path=/;expires = " + date.toUTCString();
}
}
_fnWinPop.init();
</script>   
