function fill_m_ask(data){
var html="";
if(data.length<1)return;
for(var i=0;i<data.length;i++){
html+='<li><a href="//kuaiwen.pcbaby.com.cn/question/'+data[i].questionId+'.html" target="_blank" title="'+data[i].title+'"><p class="tit">'+data[i].title+'</p><p class="des">'+data[i].answerIntro+'...</p></a></li>';
}
document.getElementById("m_ask").innerHTML=html;
}
function fill_b_ask(data){
var html="";
if(data.length<1)return;
for(var i=0;i<data.length;i++){
html+='<li><a href="//kuaiwen.pcbaby.com.cn/question/'+data[i].questionId+'.html" target="_blank" title="'+data[i].title+'"><p class="tit">'+data[i].title+'</p><p class="des">'+data[i].answerIntro+'...</p></a></li>';
}
document.getElementById("b_ask").innerHTML=html;
}
function fill_m_bbs(data){
var html="";
if(data.length<1)return;
for(var i=0;i<data.length;i++){
var link =data[i].topicUrl;
html+='<li><a href="'+link.replace("http:","")+'" target="_blank" title="'+data[i].title+'"><p class="pic"><img src="'+data[i].userFaceUrl.replace("http:","")+'"/></p><div class="fl"><div class="tit">'+data[i].title+'</div><div class="author">'+data[i].userName+'</div></div></a></li>';
}
document.getElementById("m_bbs").innerHTML=html;
hoverli();
}
function fill_b_bbs(data){
var html="";
if(data.length<1)return;
for(var i=0;i<data.length;i++){
var link = data[i].topicUrl;
html+='<li><a href="'+link.replace("http:","")+'" target="_blank" title="'+data[i].title+'"><p class="pic"><img src="'+data[i].userFaceUrl.replace("http:","")+'"/></p><div class="fl"><div class="tit">'+data[i].title+'</div><div class="author">'+data[i].userName+'</div></div></a></li>';
}
document.getElementById("b_bbs").innerHTML=html;
hoverli()
}
