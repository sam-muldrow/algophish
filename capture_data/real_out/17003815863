";domain=.pconline.com.cn;path=/;expires=Mon, 30-Nov-2099 08:00:00 GMT;";
} else {
var now = new Date().getTime();
if (now - uf > 43200000) {
calljs("//ivy.pconline.com.cn/online-self.jsp");
}
}
} else {
