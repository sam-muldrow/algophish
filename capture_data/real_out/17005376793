                    },
                    {
                        text: ['Accept'],
                        lang: 'en'
                    }
                ]
            },
            decline: {
                    enabled: true,
                    style: 'gray-button',
                    onClick: () => {
                    setCookie('tos_consent', 'reject', 3000);
                    window.location = 'https://www.google.es';
                },
                    text: [
                    {
                        text: [' Salir '],
                        lang: 'es'
                    },
                    {
                        text: ['Exit'],
                        lang: 'en'
                    }
                ]
            },
        }
        });
            entryModal.show();
            document.body.classList.add('body-no-scroll');
        }
        if (typeof CookieConfigurePopup !== 'undefined') {
            var cookiePop = new CookieConfigurePopup({
                title: [
                    {
                        text: ['Configurar cookies'],
                        lang: 'es'
                    },
                    {
                        text: ['Cookie options'],
                        lang: 'en'
                    }
                ],
                subtitle: ' ',
                acceptAll: {
                    style: 'nl-button-blue'
                }
            });
            $('.cc-deny').on('click', function(e) {
                e.preventDefault();
                e.stopPropagation();
                cookiePop.show();
                document.querySelector('.cc-window').classList.add('hidden');
            });
            window.addEventListener('cookies.acceptAll', function(e) {
                analyticsEvent(['_trackEvent', 'cookies', 'accept_all', 'desktop']);
                document.body.removeChild(document.querySelector('.cookie-backdrop.osano'));
                document.body.classList.remove('body-no-scroll');
                setCookie('cookieconsent_status', 'allow', 3000);
            });
            window.addEventListener('cookies.declineAll', function(e) {
                analyticsEvent(['_trackEvent', 'cookies', 'decline_all', 'desktop']);
            });

            window.addEventListener('cookies.exitModal', function(e) {
                analyticsEvent(['_trackEvent', 'cookies', 'exit_modal', 'desktop']);
                document.querySelector('.cc-window').classList.remove('hidden');
            });

            window.addEventListener('cookies.saveChanges', function(e) {
                analyticsEvent(['_trackEvent', 'cookies', 'save_changes', 'desktop']);
                document.querySelector('.cc-window').classList.remove('hidden');
            });
        }

    });
</script>
