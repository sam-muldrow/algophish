<script>
    var sentCartLogData = false;
    function sendCartLogData(cartData) {
        if (sentCartLogData) return;
        sentCartLogData = true;
        sendCartLogDataNaviPlus(cartData);
        sendCartLogDataTagLog(cartData);
    }

    function sendCartLogDataNaviPlus(cartData) {
        var items = [];

        $.each(cartData, function(sku, item) {
            items.push({id: sku});
        });

        __snahost = "r6.snva.jp";
        recoConstructer({
            k:"cqPAVZ5tiqzcP",
            uid:"768f017e8da7f437613bd36e7a7212b8fdd9fdbc",
            bcon:{
                heavy:{
                    items:items
                }
            }
        });
    }

    function sendCartLogDataTagLog(cartData) {
        var cartInArray = [];
        $.each(cartData, function(sku, item) {
            cartInArray.push(sku + ':' + item.qty + ':' + item.price);
        });
        var url = 'https://uabizprd.ukw.jp/taglog/__ukclr.gif';
        var params = [
            'log=impression',
            'client=yamadawebcom',
            'UID=',
            'newID=0',
            'reqURL=' + encodeURIComponent('https://www.yamada-denkiweb.com//shop/cart/cart.aspx'),
            'refURL=' + encodeURIComponent(location.href),
            'cartIn=' + encodeURIComponent(cartInArray.join(',')),
            'pageType=cart'
        ];
                params.push('display=pc');
                
        url += '?' + params.join('&');

        $('body').append('<img src="' + url + '">');
    }

    function sendSelectRecommendProductLog() {
        var sku = $(this).attr("data-sku");
        var did = $(this).attr("data-did");
        apiSetCtr(sku, did, this,'cqPAVZ5tiqzcP&uid=768f017e8da7f437613bd36e7a7212b8fdd9fdbc');
    }

    $(function() {
        $(".js-select-recommend-product").click(sendSelectRecommendProductLog);
    });
</script>



