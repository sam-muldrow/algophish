ирался, но спустя время решил остаться здесь подольше. Дело в том, что жителям деревни была нужна элементарная медицинская помощь, а до ближайшей больницы от них нужно было ехать больше 100 километров. Предлагаем посмотреть онлайн первый сезон сериала «Доктор Иванов» в нашем интернет-кинотеатре.","hru":"","subscription_ids":[6,25,26,30,31,32,33,35,37,38,39,41,42,43,44,45,46,50,51,52,53,54,55,56,58,59,60,61,62,63,64,65,66,67,68,69,70,71,104,105,107,109,113,114,115],"ivi_release_info":{"date_interval_min":null,"date_interval_max":null},"season_release_date":1638662400000,"used_to_be_paid":false,"content_paid_types":["SVOD"],"allow_download":true,"allow_download_paid_types":["SVOD"],"min_episode":1,"max_episode":4,"episode_count":4,"fake":false},{"season_id":9280,"number":2,"purchasable":false,"title":"","description":null,"hru":"","subscription_ids":[6,25,26,30,31,32,33,35,37,38,39,41,42,43,44,45,46,50,51,52,53,54,55,56,58,59,60,61,62,63,64,65,66,67,68,69,70,71,104,105,107,109,113,114,115],"ivi_release_info":{"date_interval_min":null,"date_interval_max":null},"season_release_date":1652227200000,"used_to_be_paid":false,"content_paid_types":["SVOD"],"allow_download":true,"allow_download_paid_types":["SVOD"],"min_episode":1,"max_episode":4,"episode_count":4,"fake":false},{"season_id":9281,"number":3,"purchasable":false,"title":"","description":null,"hru":"","subscription_ids":[6,25,26,30,31,32,33,35,37,38,39,41,42,43,44,45,46,50,51,52,53,54,55,56,58,59,60,61,62,63,64,65,66,67,68,69,70,71,104,105,107,109,113,114,115],"ivi_release_info":{"date_interval_min":null,"date_interval_max":null},"season_release_date":1652313600000,"used_to_be_paid":false,"content_paid_types":["SVOD"],"allow_download":true,"allow_download_paid_types":["SVOD"],"min_episode":1,"max_episode":4,"episode_count":4,"fake":false},{"season_id":9282,"number":4,"purchasable":false,"title":"","description":null,"hru":"","subscription_ids":[6,25,26,30,31,32,33,35,37,38,39,41,42,43,44,45,46,50,51,52,53,54,55,56,