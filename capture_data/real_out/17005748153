        var player;

        function onYouTubeIframeAPIReady() {
            player = new YT.Player('player1', {
                videoId: 'RJN1LTfwc5o',
                border: "none",
                position: "absolute",
                top: "0",
                left: "0",
                width: "100%",
                playerVars: {
                    //autoplay: 1,
                    showinfo: 0,
                    controls: 1,
                    rel: 0,
                    modestbranding: 0,
                    loop: 1,
                    fs: 0,
                    cc_load_policy: 0,
                    iv_load_policy: 3,
                    mute: 1,
                    autohide: 0
                },
                events: {
                    onStateChange: function (e) {
                        if (e.data === YT.PlayerState.ENDED) {
                            e.target.playVideo();
                        }
                    }
                }
            });
        }

        var firstEnterTime;

