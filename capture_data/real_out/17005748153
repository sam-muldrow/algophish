<script type="text/javascript">
    $(document).ready(function () {
        $("form#loginForm").validate({
            rules: {
                email: {
                    required: true,
                    email: true
                },
                password: {
                    required: true,
                    minlength: 4
                },
            },
            messages: {
                password: {
                    required: "This field is required.",
                    minlength: "You must enter a valid password."
                },
                email: {
                    required: "This field is required.",
                    email: "Invalid Email Address"
                }
            },
            errorPlacement: function(error, element) {
                error.appendTo( element.parent() );
            },
            submitHandler: async function(form){
                if($(form).hasClass('submitted')){
                    return;
                }
                $(form).addClass('submitted');
                $('.uses-recaptcha').prop("disabled", true);

                await fetchRecaptchaToken('login');

                form.submit();
            }
        });
    });
</script>
                                    </div>
            </div>
        </div>

</div>

<script type="text/javascript">
    $(function() {

        window.Warframe = window.Warframe || {};

	                $('#loginLightbox .formHolder').on('click', '.signupButton', function(e) {
                e.preventDefault();
                lightbox_signup();
            });
        
        function lightbox_signup() {
	                        if(!isLoggedIn) {
                    loadRecaptcha();
                    hideHolders();
                    $('#signupLightbox').show();
                    lightbox_open();
                } else {
                    window.location.href = "https://www.warframe.com/signup";
                }
                    }
        function lightbox_login() {
	                        if(!isLoggedIn) {
                    loadRecaptcha();
                    hideHolders();
                    $('#loginLightbox').show();
                    lightbox_open();
                } else {
                    window.location.href = "https://www.warframe.com/login";
                }
                    }

        function lightbox_open() {
            $('.lightboxOverlay').css('display', 'block');
            $('body').css('overflowY', 'hidden');
            $('.grecaptcha-badge').css('display', 'block');
        }

        function hideHolders() {
            $('.lightboxHolder').hide();
        }

        $('.lightboxOverlay .close').click(closeSignupLightbox);

        $(document).keyup(function(e) {
            if (e.key === "Escape") { // escape key maps to keycode `27`
                closeSignupLightbox(e);
            }
        });

        function closeSignupLightbox(e){
            if(typeof e !== 'undefined') e.preventDefault();
            $('.lightboxOverlay').css('display', 'none');
            $('body').css('overflowY', 'scroll');
            $('.grecaptcha-badge').css('display', 'none');
        }

                    $('.loginLink').click( function(e) {e.preventDefault(); lightbox_login(); return false; } );
        
                    $('.signupLink').click( function(e) {e.preventDefault(); lightbox_signup(); return false; } );
        
        $('body.landing .playFreeNow,body.landing .navSignup').click(function(e){
            e.preventDefault();
            lightbox_signup();
        });

        $('body.landing .haveAccount').click(function(e) {
            e.preventDefault();
            lightbox_login();
        });

        window.Warframe.triggerLoginLightbox = lightbox_login;
        window.Warframe.triggerSignupLightbox = lightbox_signup;
    });
</script>
    
