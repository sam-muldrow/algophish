                    slidesToScroll: 1,
                    centerMode: true,
                    prevArrow: false,
                    nextArrow: false,
                    autoplay: false,
                    autoplaySpeed: 10000,
                    mobileFirst: true,
                    pauseOnFocus: false,
                    pauseOnHover: false,
                    focusOnSelect: true,
                    swipeToSlide: true,
                    variableWidth: true,
                    lazyload: 'progressive',
                    initialSlide: 18,
                });
                slickslide.on('afterChange', function (s, cs) {
                    if (window.innerWidth >= 768) {
                        TweenMax.to(fadeOverlay, .5, {opacity: 1});
                        setTimeout(function () {
                            let frame = $('.slick-center div .frame');
                            if(frame) {
                                document.querySelector('.warframeVideo source').src = frame.attr('data-movie');
                                video.load();
                                document.querySelector('.videoFallback img').src = frame.attr('data-fallback');
                                if(frame.attr('data-name')) {
                                    document.querySelector('.warframeTitle').innerHTML = (frame.attr('data-name')).toUpperCase();
                                }
                                document.querySelector('.gallery .warframeInfo .warframeDesc').innerHTML = frame.attr('data-desc');
                            }
                        }, 500);
                    }
                });
                $('.section2 img.trans').each(function () {
                    lazyLoadInstance.load($(this)[0]);
                });
                var video = document.querySelector('.warframeVideo');
                var fadeOverlay = document.querySelector('.fadeOverlay');
                let frame = $('.slick-center div .frame');
                document.querySelector('.warframeVideo source').src = frame.attr('data-movie');
                video.load();
                $('.videoFallback img').attr('data-src', frame.attr('data-fallback'));
                if(frame.attr('data-name')) {
                    document.querySelector('.warframeTitle').innerHTML = (frame.attr('data-name')).toUpperCase();
                }
                document.querySelector('.gallery .warframeInfo .warframeDesc').innerHTML = frame.attr('data-desc');

                $('.frameMobile img, .watchTrailer img, .lightbox').remove();
                slider.addEventListener("click", centered, false);
            }

            var section6BG = "background-image: url('https://www-static.warframe.com/images/longlanding/mobileSquadBg.jpg')";
            if (window.innerWidth >= 768) {
                section6BG = "background-image: url('https://www-static.warframe.com/images/longlanding/desktopSquadBg.jpg')";
            }
            $('.section6').attr("style", section6BG);

            var scrolledToWarframe,
                scrolledToCombat,
                scrolledToExplore,
                scrolledToArsenal,
                scrolledToSquad;

            //////Sticky Nav//////
            var nav = document.querySelector(".nav");
            var openNav = document.querySelector(".gameDesc");
            var slidersLoaded = false;

            window.onscroll = function () {
                if (window.pageYOffset >= openNav.offsetTop) {
                    TweenMax.to(nav, .5, {top: "23px"});
                } else {
                    TweenMax.to(nav, .5, {top: "-10%"});
                }
                if (typeof player !== 'undefined' && typeof player.getPlayerState === "function" && player.getPlayerState() !== 1) {
                    player.playVideo()
                    $('.slides').slick('slickPlay');
                }
                if (!slidersLoaded) {
                    $('.section2 .warframeSelect img').each(function () {
                        lazyLoadInstance.load($(this)[0]);
                    });
                    slidersLoaded = true;
                }
            };

            function closeSignupLightbox(e) {
                if (typeof e !== 'undefined') e.preventDefault();
                document.querySelector('.darkOverlay').style.display = 'none';
                document.querySelector('body').style.overflowY = 'scroll';
            }

            $('.playFreeNow,.navSignup').click(function (e) {
                if (typeof e !== 'undefined') e.preventDefault();
                if(document.querySelector('.darkOverlay')) {
                    document.querySelector('.darkOverlay').style.display = 'block';
                }
                document.querySelector('body').style.overflowY = 'hidden';
            });
            $('.signupClose').click(closeSignupLightbox);

            $(document).keyup(function (e) {
                if (typeof e !== 'undefined' && e.key === "Escape") { // escape key maps to keycode `27`
                    closeSignupLightbox();
                }
            });

            //////Liset parallax//////
            var controller = new ScrollMagic.Controller();
            var scene1 = new ScrollMagic.Scene({triggerElement: ".section4", duration: "100%"})
                .setTween(".blueSky", {left: "-5%", y: "-5%"});
            var scene2 = new ScrollMagic.Scene({triggerElement: ".section4", duration: "100%"})
                .setTween(".grasshopper", {left: "-15%"});
            var scene3 = new ScrollMagic.Scene({triggerElement: ".section4", duration: "100%"})
                .setTween(".lisetSmall", {y: "20%"});
            var scene6 = new ScrollMagic.Scene({triggerElement: ".section3", duration: "45%"})
                .setTween(".warframe", {right: "5%", opacity: "1"});
            controller.addScene([scene1, scene2, scene3, scene6]);

            //////Desktop carousel//////
            var video = document.querySelector('.warframeVideo');
            var fadeOverlay = document.querySelector('.fadeOverlay');

            video.addEventListener('canplay', fadeIn, false);
            video.addEventListener('abort', fallback, false);

            function fallback() {
                TweenMax.to(fadeOverlay, .5, {opacity: 0}).delay(.5);
            }

            function fadeIn() {
                TweenMax.to(fadeOverlay, .5, {opacity: 0}).delay(.5);
                TweenMax.to(video, .5, {opacity: 1});
            }

            var frameClicked = false;

            function centered() {
                if (!frameClicked) {
                    frameClicked = true;
                }
                if (window.innerWidth >= 768) {
                    TweenMax.to(fadeOverlay, .5, {opacity: 1});
                    setTimeout(function () {
                        let frame = $('.slick-center div .frame');
                        document.querySelector('.warframeVideo source').src = frame.attr('data-movie');
                        video.load();
                        document.querySelector('.videoFallback img').src = frame.attr('data-fallback');
                        if(frame.attr('data-name')) {
                            document.querySelector('.warframeTitle').innerHTML = (frame.attr('data-name')).toUpperCase();
                        }
                        document.querySelector('.gallery .warframeInfo .warframeDesc').innerHTML = frame.attr('data-desc');
                    }, 500);
                }
            }

            $('#languages').superfish();

            
        });

    </script>
    
    
    
