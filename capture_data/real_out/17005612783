        for (var m of afterPv) {
            m();
        }
    }

    this.aComment = function() {
        WP.pwp.customAction({
            "ga": {
                "event": "addComment",
                "content": {
                    "add": {
                        "commentIndicator": "1"
                    }
                }
            }
        });
    }

    this.aUnload = function() {
        WP.pwp.closePage({
            "ga": {
                "event": "unload",
                "eventLabel": false
            }
        });
    }

    this.aPopup = function(c, a, l) {
        WP.pwp.customAction({
            "ga": {
                "event": "popupInteraction",
                "eventCategory": c,
                "eventAction": a,
                "eventLabel": l || ""
            }
        });
    }

    this.aSocialShare = function(platform, action) {
        WP.pwp.customAction({
            "ga": {
                "event": "socialShare",
                "eventAction": action,
                "eventLabel": platform
            }
        });
    }

    this.aScollPost = function(postdata) {
        this.regScrollpostData(postdata);
    }

    this.aAppInstalled = function() {
        WP.pwp.customAction({
            "ga": {
                "event": "addToHomescreen",
                "eventAction": "add"
            }
        });
    }


    this.regSource = function(sa, srt) {
        
        var dl = document.location;
        var url = dl.protocol + '//' + dl.host + dl.pathname + (dl.search ? '&' : '?') + 'wga_sa=' + sa;
        if (srt) {
            url += '&wga_srt=' + srt
        }
        if (window.history) {
            history.pushState({}, '', url);
        }
    }
}

var g360w = new gaWrapper();

		g360w.reg('page.templateType', 'homepage');




if (window.innerWidth >= 1280) {
    g360w.reg('site.responsiveBreakpoint', 'desktop-wide');
} else if (window.innerWidth >= 985) {
    g360w.reg('site.responsiveBreakpoint', 'desktop-narrow');
} else {
    g360w.reg('site.responsiveBreakpoint', 'mobile');
}
window.addEventListener('load', function() {
    if (window.g360w) {
        g360w.aPageView();
    }
});
window.addEventListener('beforeunload', function() {
    //console.log('currentSP', Service._scrollPostsCurrentActive)
    if (window.g360w) {
        g360w.aUnload();
    }
});
window.addEventListener('appinstalled', function() {
    if (window.g360w) {
        g360w.aAppInstalled();
    }
});


</script>

