<script type='text/javascript' id="tiwanon-js" async="async" src='https://mthai.com/app/themes/tiwanon/dist/scripts/main_c1d1e83b.js' id='tiwanon-js-js'></script>

<script src="/app/themes/tiwanon/assets/splide-4.0.0/dist/js/splide.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/js/bootstrap.bundle.min.js"></script>
<script>
    $(function() {
        
        var prevScrollpos = window.pageYOffset;
        window.onscroll = function() {
            var currentScrollPos = window.pageYOffset;
            if(currentScrollPos >= 350) {
                document.getElementById("monohoro-navbar").style.position = "fixed";
                if (prevScrollpos > currentScrollPos) {
                    document.getElementById("monohoro-navbar").style.top = "0";
                } else {
                    document.getElementById("monohoro-navbar").style.top = "-160px";
                }
            } else {
                document.getElementById("monohoro-navbar").style.position = "unset";
            }
            prevScrollpos = currentScrollPos;
        }

        $( ".header-dropdown" ).click(function() {
            var position = $(this).position();
            var nav_class = $(this).attr("id");
            if($(this).hasClass('show')) {
                $(this).removeClass('show');
                $("."+nav_class).hide();
            } else {
                $(".header-dropdownlist").hide();
                $(".header-dropdown").removeClass('show');
                $(this).addClass('show');
                $("."+nav_class).css({'left': position.left});
                $("."+nav_class).show();    
            }
        });
        

        function responsive() {	
            /* iframe seeme & youtube */
            var content_width = $(".entry-content").width() - 60;
            var video_height = Math.floor(content_width / 1.77);
            $("article .entry-content .entry-content-inner iframe").css({"display": "block", "margin": "0 auto"});	
            $("article .entry-content .entry-content-inner iframe").css({"width": content_width + "px", "height": video_height + "px"});		
        }
        $(window).resize(responsive);
        responsive();

        
                
                    new Splide( '.splide', {type : 'loop', autoplay : false, interval : 9000} ).mount();

            $("#new-horodream-search-form").on('submit', function (e) {
            e.preventDefault();
                var dream = $(this).find(':text').val();
                if (!dream) {
                    alert('กรอกสิ่งที่ฝันเห็นด้วยค่ะ');
                    return;
                }
                window.location = "/dream/" + dream + ".html";
            });

            // check result
            function check_result(value) {
                // ga('send', 'event', 'AbaloneMonohoro', 'clickCheckResultBtn', 'popup_result');
                var protocal = window.location.protocol,
                hostname = window.location.hostname;
                var foundin = [],
                scrollToRes = false,
                NumLotto = {};
                
                var win_img = ['w_max1.jpg', 'w_max2.jpg', 'w_max3.jpg'];
                var lose_img = ['l_max1.jpg', 'l_max1.jpg', 'l_max3.jpg'];
                var rand_win_img = Math.floor( Math.random() * win_img.length );
                var rand_lose_img = Math.floor( Math.random() * lose_img.length ); 
        
                /*
                var win_img = ['abalone.png'];
                var lose_img = ['abalone.png'];
                var rand_win_img = 0;
                var rand_lose_img = 0;
                */


                NumLotto.total = value;
                NumLotto.last2 = value.substr( 4 );
                NumLotto.last3 = value.substr( 3 );
                NumLotto.first3 = value.substr( 0, 3 );
                console.log(NumLotto);

                if ($( '#prize-1>.result:contains(' + NumLotto.total + ')' ).length != 0) {
                    foundin.push( 'ที่ 1' );
                }

                if ($( '#prize-n1>.result:contains(' + NumLotto.total + ')' ).length != 0) {
                    foundin.push( 'ข้างเคียงรางวัลที่ 1' );
                }

                if ($( '#prize-2>.result:contains(' + NumLotto.total + ')' ).length != 0) {
                    foundin.push( 'ที่ 2' );
                }

                if ($( '#prize-3>.result:contains(' + NumLotto.total + ')' ).length != 0) {
                    foundin.push( 'ที่ 3' );
                }

                if ($( '#prize-4>.result:contains(' + NumLotto.total + ')' ).length != 0) {
                    foundin.push( 'ที่ 4' );
                }

                if ($( '#prize-5>.result:contains(' + NumLotto.total + ')' ).length != 0) {
                    foundin.push( 'ที่ 5' );
                }

                if ($( '#prize-l3>.result:contains(' + NumLotto.last3 + ')' ).length != 0) {
                    foundin.push( 'เลขท้าย 3 ตัว' );
                }
                if ($( '#prize-l2>.result:contains(' + NumLotto.last2 + ')' ).length != 0) {
                    foundin.push( 'เลขท้าย 2 ตัว' );
                }
                if ($( '#prize-f3>.result:contains(' + NumLotto.first3 + ')' ).length != 0) {
                    foundin.push( 'เลขหน้า 3 ตัว' );
                }

                if (foundin.length > 0) {
                    $('#myModal').find('.modal-title').html('<span style="color:green;">ยินดีด้วย!!!</span>');
                    $('#myModal').find('.modal-body').html('<a target="_blank" href="https://bit.ly/3KwjqIJ"><img class="swal2-image" src="/app/themes/tiwanon/assets/images/monohoro/' + win_img[rand_win_img]+'" style="display: block;width:100%;"></a><p class="text-center" style="font-size:1.2rem;margin-top:1rem;"><strong>ยินดีด้วย!!!<br>เลข ' + NumLotto.total + '</strong><br/>ของคุณถูกสลากกินแบ่งรัฐบาล ในรางวัล <span id="show_win_prize" style="color:red;"> ' + foundin.join( ' และ' ) + ' </span><br/></p>');
                    $('#myModal').find('.modal-footer').html('<a href="https://lotto.mthai.com/#' + NumLotto.total + '"><button type="button" class="btn btn-secondary btn-monohoro">เพิ่มเติม</button></a> <button type="button" class="btn btn-secondary btn-monohoro" data-dismiss="modal">Close</button>');
                    $('#myModal').modal('show');
                } else {
                    $('#myModal').find('.modal-title').html('<span style="color:red;">เสียใจด้วย T_T</span>');
                    $('#myModal').find('.modal-body').html('<p class="text-center" style="font-size:1.2rem;"><strong>เสียใจด้วย T_T</strong><br><span style="font-weight: bold;">เลข ' + NumLotto.total + '</span><br><span style="color: red; font-style: italic;">ของคุณไม่ถูกรางวัล</span></p><a target="_blank" href="https://bit.ly/3KwjqIJ"><img class="swal2-image" src="/app/themes/tiwanon/assets/images/monohoro/' + lose_img[rand_lose_img]+'" style="display: block;width:100%;"></a>');
                    $('#myModal').find('.modal-footer').html('<button type="button" class="btn btn-secondary btn-monohoro" data-dismiss="modal">Close</button>');
                    $('#myModal').modal('show');
                }

            }

            $( "#lotto-check-result-form" ).on(
                'submit', function (e) {
                    e.preventDefault();
                    var value = $( this ).find( '#result_value' ).val();
                    var chkUrl = $( this ).find( '#result_date' ).val();

                    if ( ! Number.isInteger( parseInt( value ) ) || (value == '') || value.length != 6) {
                        $('#myModal').find('.modal-title').html('มีข้อผิดพลาด');
                        $('#myModal').find('.modal-body').html('<p class="text-center">คุณยังไม่ได้ระบุตัวเลขสลากฯ หรือ กรอกไม่ครบถ้วน!</p>');
                        $('#myModal').find('.modal-footer').html('<button type="button" class="btn btn-secondary btn-monohoro" data-dismiss="modal">Close</button>');
                        $('#myModal').modal('show');
                    } else {
                        if (chkUrl == location.protocol + '//' + location.host + '/') {
                            check_result( value );
                        } else {
                            window.location = chkUrl + '/#' + value;
                        }
                        
                    }
                }
            );

            $( ".number" ).on( "keypress change", function(e) {
                var key = String.fromCharCode(e.which);
                var regex = /[0-9]/;
                return regex.test(key) || e.which == 8;
            });

            $( ".char6" ).on( "keypress", function(e) {
                if(this.value.length > 5) {
                    e.preventDefault();
                    return false;
                }
            });

            $( ".char6" ).on( "change", function(e) {
                if(this.value.length > 6) {
                    this.value = this.value.substr(0, 6);
                }
            });
        
        var w = window.innerWidth;
        if(w >= 768) {
            $('.dream-rectangle').remove();   
        }
    });
</script>


