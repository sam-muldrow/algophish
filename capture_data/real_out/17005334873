ino eligió al candidato de La Libertad Avanza como el quincuagésimo cuarto mandatario de su historia. A continuación, te hablamos de su pasión por los autos y de los autos de garaje, en especial, de uno que tiene el logo del león."},"source":{"system":"composer","name":"copesa","source_type":"staff"},"taxonomy":{"primary_site":{"path":"/mtonline","parent_id":"/","name":"MtOnline","description":null,"_id":"/mtonline","additional_properties":{"original":{"parent":{"servicios":"/","default":"/"},"site":{"pagebuilder_path_for_native_apps":null,"site_url":null,"site_keywords":null,"site_about":null,"site_description":null,"site_title":null,"site_tagline":null},"navigation":{"nav_title":null},"_admin":{"alias_ids":["/mtonline"]},"inactive":false,"node_type":"section","social":{"twitter":"https://twitter.com/mtlatercera","rss":null,"facebook":null,"instagram":null},"site_topper":{"site_logo_image":null},"name":"MtOnline","_id":"/mtonline","ancestors":{"servicios":[],"default":[]},"order":{"default":1023}}},"type":"site","version":"0.5.8"},"sites":[{"path":"/mtonline","parent_id":"/","name":"MtOnline","description":null,"_id":"/mtonline","additional_properties":{"original":{"parent":{"servicios":"/","default":"/"},"site":{"pagebuilder_path_for_native_apps":null,"site_url":null,"site_keywords":null,"site_about":null,"site_description":null,"site_title":null,"site_tagline":null},"navigation":{"nav_title":null},"_admin":{"alias_ids":["/mtonline"]},"inactive":false,"node_type":"section","social":{"twitter":"https://twitter.com/mtlatercera","rss":null,"facebook":null,"instagram":null},"site_topper":{"site_logo_image":null},"name":"MtOnline","_id":"/mtonline","ancestors":{"servicios":[],"default":[]},"order":{"default":1023}}},"type":"site","version":"0.5.8"}],"sections":[{"path":"/mtonline","parent":{"default":"/"},"_website":"la-tercera","parent_id":"/","name":"MtOnline","description":null,"_id":"/mtonline","additional_properties":{"original":{"parent":{"servicios":"/","default