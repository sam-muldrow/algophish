        var $zhVersion = document.querySelector('[hreflang="zh-CN"]');
        if ($zhVersion) {
            $zhVersion.addEventListener('click', function (e) {
                e.preventDefault();
                var iframe = document.createElement('iframe');
                iframe.style.display = "none";
                iframe.src = '/boke/';
                document.body.appendChild(iframe);
                setTimeout(() => {
                    window.location.href = '/meiguo-yimin-xinxi/';
                }, 250);
            });
        }
