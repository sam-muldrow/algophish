<script>
	var today = new Date();
	document.getElementById("nowYear").innerHTML = today.getFullYear();
	
	function deleteViewCookie(){
		var expire = new Date();
		expire.setDate(expire.getDate() - 1);
		cookies = "P2M=; domain=.clien.net; path=/; expires="+expire.toGMTString()+";";
		document.cookie = cookies;
		
		var url = document.location.href;
			url = url.replace("www", "m");
			location.href=url;
	}
</script>

