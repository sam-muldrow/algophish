<script>
	function detectBlockers() {
		var ad = document.createElement('div');
		ad.innerHTML = '&nbsp;';
		ad.className = 'adsbox';
		ad.style.position = 'absolute';
		ad.style.fontSize = '0';
		ad.style.height = '10px';
		ad.style.width = '10px';
		ad.style.zIndex = '-1000';
		ad.style.top = '-1000px';
		document.body.appendChild(ad);

		setTimeout(function() {
			if (ad.offsetHeight === 0) {
				trackBlockerUsage();
				console.log("adBolckUse");
			}
			document.body.removeChild(ad);
		}, 100);
	}

	function trackBlockerUsage() {
		if (typeof gtag !== 'undefined') {
			gtag('event', 'adblocker_detected', {
				'event_category': 'adblocker',
				'event_label': 'Ad Blocker Usage'
			});
		}
	}

	window.onload = detectBlockers;
</script>
 

 <script src="/service/js/app/clien.hongbo.js?1103v1"></script> 

<script>
function App() {
	var _this = this;

	_this.mainListBlock = function() {

		var memoListJSON = localStorage.getItem('MY_MEMO_LIST');
		var jsonInfo = JSON.parse(memoListJSON);

		var keywordJSON = localStorage.getItem('KEYWORD');
		var keyword = JSON.parse(keywordJSON);
		var keywordIsVaild = false;

		if(keywordJSON != null) {
			blockList = keyword.blockKeyWord.split(',');
			keywordIsVaild = true;
		};

		$('*[data-role=main-list]').each(function() {
			var row = $(this);
			var authorId = row.data('author-id');

			for(var i in jsonInfo.memoList) {
				var destId = jsonInfo.memoList[i].destId;
				var blockArticleYn = jsonInfo.memoList[i].blockArticleYn;

				if (blockArticleYn && destId == authorId) {
					row.remove();
				};
			}

			if(keywordIsVaild) {
				if(keyword.blockKeyWord != "" && keyword.keywordYn) {
					$.each(blockList, function(index, element) {
						if(element != "") {
							if((row.find('*[data-role=list-title]').text()).match(element)) {
								row.remove();
							}
						}
					});
				}
			}
		});
	};

	_this.init = function() {
		hongbo.hongboLists(); 
		ui.initSymphathy(); 
		ui.initShortCut(); 		if (IS_LOGIN) {
			_this.mainListBlock();
		};
	}();
}
var app = new App();

</script>
</body>
