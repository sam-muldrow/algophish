            return;
            }

    function ajax_dh_cart() {
        if(ajax_ing == false) {
            var realtime_id = $('.dh_popup').find('.realtime_goods').html();
            $.ajax({
                url: "/orders/ajax_dh_cart",
                type: "post",
                dataType: "json",
                data: {"realtime_id" : realtime_id, "gb" : "search"},
                beforeSend: function(){
                    ajax_ing = true;
                    $('.dh_popup').hide();
                },
                success: function(rs) {
				    if(rs.result == true) {
						var pop_msg = "상품이 장바구니에 담겼습니다!";
                    } else {
						var pop_msg = rs.err_msg;
                    }
                    $('.dh_popup02').find('.pop_msg').html(pop_msg);
                    $('.dh_popup02').show();
                },
                error: function(rs) {
                    alert('서버와 통신 중 에러가 발생했습니다. 다시 시도해 주세요.');
                },
                complete: function() {
                    ajax_ing = false;
                }
            });
        }
    }


    $(function() {
        // 메인 컨텐츠 탭
        $('.maincont .tab li a', '#main').on('mouseenter', function(e) {
            $(this).parent('li').addClass('now').siblings('li').removeClass('now');
            $($(this).attr('tab')).show().siblings('.boxing').hide();
        });
        // 인기순위 탭
        $('#tabRank .pick li a', '#main').on('click', function(e) {
            e.preventDefault();
            $(this).parent('li').addClass('now').siblings('li').removeClass('now');
            $($(this).attr('tab')).show().siblings('.rank-boxing').hide();
        });
        $('#tabRank .cate-lst li a', '#main').on('click', function(e) {
            e.preventDefault();
            $('.cate-lst li').removeClass('on');
            $('.cate-lst.type02 li').removeClass('on');
            $(this).parent('li').addClass('on');
            $($(this).attr('tab')).show().siblings('.lst-pro').hide();
        });
        // 엔젤스 탭
        $('#tabAngels .hd li a', '#main').on('click', function(e) {
            e.preventDefault();
            $(this).parent('li').addClass('now').siblings('li').removeClass('now');
            $(this.hash).show().siblings('.donation').hide();
        });
        // 공지사항 / 언론보도 탭
        $('.notice .tab li a', '#main').on('click', function(e) {
            e.preventDefault();
            $(this).parent('li').addClass('now').siblings('li').removeClass('now');
            $(this.hash).show().siblings('.lst').hide();
            $('.notice a.more').attr('href', $(this).attr('more'));
        });
        // 몰테일 쇼핑 탭 ( 사이트 )
        $('.shopping dd li a', '#main').on('mouseenter', function(e) {
            e.preventDefault();
            var before_banners = $('#main .shopping dd li.now a').attr('tab').replace('#shopping','');
                        //if(swipers[before_banners] != undefined) { swipers[before_banners].stopAutoplay(); }
            $('.shopping dd').find('li').removeClass('now');
            $(this).parent('li').addClass('now');
            $($(this).attr('tab')).show().siblings('.tab-shopping').hide();
            var banners = $(this).attr('tab').replace('#shopping','');
            /*
            if(swipers[banners] != undefined) {
                swipers[banners].startAutoplay();
                swipers[banners].resizeFix(true);
            }
            */
        });

        
                $('#tabReview .center-lst li a').on('click', function(e) {
            e.preventDefault();
            $(this).parent('li').addClass('on').siblings('li').removeClass('on');
            $($(this).attr('tab')).show().siblings('.lst-review').hide();
        });

        // 통합결제
        $('#integratedPayment').click(function(e) {
            e.preventDefault();
            location.href = "/orders/index/USpaywait/AL/A/1";
        });
        // 로그인 가이드 문구
        $('.member .login .frm .txt-input', '#main').on('focusin', function(e) {
            $(this).siblings('span').hide();
        }).on('focusout', function(e) {
            if ($(this).val() == '') {
                $(this).siblings('span').show();
            }
        });
        // 로그인 Enter시 submit
        $('form[name="login_form"] input').keypress(function(e) {
            if(e.which == 13) {
                $('#loginId').val($.trim($('#loginId').val()));
                $('#login-submit').trigger('click');
            }
        });
        // 로그인
        $('#login-submit').click(function() {
            $('#loginId').val($.trim($('#loginId').val()));
            if($.trim($('#loginId').val()).length < 1) {
                alert("아이디를 입력하세요.");
                $('#loginId').focus();
                return;
            }
            if($.trim($('#loginPassword').val()).length < 1) {
                alert("패스워드를 입력하세요.");
                $('#loginPassword').focus();
                return;
            }
            // 아이디 저장
            if($("#rememberId").is(':checked') == true) {
                setCookie_Id('remember_id', $('#loginId').val());
            } else {
                setCookie_Id('remember_id','');
            }
            $('form[name="login_form"]').submit();
        });
        // 배송비계산 - 센터 선택
        $('input[name="center"]').click(function() {
            var currency    = "USD";
            var weight      = "lbs";
            var ship        = false;
            switch($(this).val()) {
                case "UK" :
                break;
                case "NK" :
                break;
                case "DE" :
                break;
				case "OK" :
                break;
                case "GK" :
                case "SK" :
                    ship        = false;
                    currency    = "EUR";
                    weight      = "kg";
                break;
                case "AK" :
                    ship        = false;
                    currency    = "AUD";
                    weight      = "kg";
                break;
                case "HK" :
                    ship        = false;
                    currency    = "HKD";
                    weight      = "kg";
                break;
                case "BK" :
                    currency    = "GBP";
                    weight      = "kg";
                break;
                case "JK" :
                    ship        = true;
                    currency    = "JPY";
                    weight      = "kg";
                break;
                case "CK" :
                    ship        = true;
                    currency    = "CNY";
                    weight      = "kg";
                break;
            }
            $('span.currency-unit').text(currency);
            $('span.weight-unit').text(weight);

            if(ship == false) {
                $('select[name="transport"] > option[value="SH"]').attr('disabled', true).prop('selected', false);
            } else {
                $('select[name="transport"] > option[value="SH"]').attr('disabled', false);
            }

            if($(this).val().match(/^(UK|NK|DK|OK)$/)) {
                $(".delivery_calculator_device").show();
            } else {
                $(".delivery_calculator_device").hide();
            }
        });
        // 숫자 클래스
        $('input.number').change(function() {
            // 구매비용, 예상무게는 숫자만 입력받는다.
            var amount = $(this).val().replace(/[^0-9.,]/g, '');
            amount = new Number(amount).toFixed(2);
            $(this).val(amount);
        });
        // 배송비계산 - 예상배송비 측정
        $('#checkDeliveryCalculator').click(function() {
            // 일단 배송비 리셋
            $('input[name="expected_price"]').val("$ 0");

            // 구매비용, 예상무게의값은 한번더 체크해준다.
            $('input[name="total_amount"]').trigger('change');
            $('input[name="total_weight"]').trigger('change');

            if($('input[name="center"]:checked').val() == undefined) {
                alert("물류센터를 선택해주세요.");
                $('input[name="center"]').focus();
                return;
            }
            if($('select[name="grade_rate"]').val() == "") {
                alert("회원 등급을 선택해주세요.");
                $('select[name="grade_rate"]').focus();
                return;
            }
//            if($('select[name="item_type"]:selected').val() == undefined || $('select[name="item_type"]:selected').val() == "") {
//                alert("품목을 선택해주세요.");
//                $('select[name="item_type"]').focus();
//                return;
//            }
            if($('select[name="transport"]').val() == "") {
                alert("운송방법을 선택해주세요.");
                $('select[name="transport"]').focus();
                return;
            }
            if($('input[name="total_amount"]').val().length < 1) {
                alert("총 구매비용을 입력해주세요.");
                $('input[name="total_amount"]').focus();
                return;
            }
            if($('input[name="total_weight"]').val().length < 1) {
                alert("상품 예상무게를 입력해주세요.");
                $('input[name="total_weight"]').focus();
                return;
            }
            if($('input[name="insurance"]:checked').val() == undefined) {
                alert("보험가입 여부를 선택해주세요.");
                $('input[name="insurance"]').focus();
                return;
            }

            if($('input[name="center"]:checked').val().match(/^(UK|NK|DK|OK)$/)) {
                if($('input[name="create_device"]:checked').val() == undefined) {
                    alert("신청방법을 선택해주세요.");
                    return;
                }
            }

            alert('해당 예상비용은 구매하신 상품의 단순 무게를 바탕으로 적용된\n예상 배송금액이며, 부피무게 상품일 경우 배송비용은 차이가\n발생할 수있습니다.');

            var target_url = $('form[name="delivery_calculator"]').attr('action');
            $.ajax({
                url : target_url,
                type: 'post',
                data: $('form[name="delivery_calculator"]').serialize(),
                dataType: 'json',
                success: function(data) {
                    if(data.result == false) {
                        alert(data.msg);
                        return;
                    }
                    if(data.total_price < 1) {
                        alert("측정값에 오류가 있습니다.");
                        return;
                    }
//                    real_price = $('input[name="exchange_rate"]').val() * data.total_price;
//                    real_price = new Number(real_price).toFixed(-2);
                    $('input[name="expected_price"]').val("$ " + data.total_price);
                },
                error: function() {
                    alert('ERROR!!');
                }
            });
        });

        // 배송비계산 - reset
        $('#resetDeliveryCalculator').click(function() {
            $('form[name="delivery_calculator"]')[0].reset();

            //신청방법 초기화
            $(".delivery_calculator_device").hide();
        });

            });

    function close_layer(layer) {
