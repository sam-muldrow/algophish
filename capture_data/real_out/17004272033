        <script type="text/javascript">
            var diff_date = "20231120";
            var diff_txt = "malltail_main";

            $(document).ready(function() {
                var main_modal_layer_yn  = '';
                if(main_modal_layer_yn != diff_date) {
                //if(main_modal_layer_yn != diff_txt) {
                    $('#bg_modal').show();
                } else {
                    $("#hdrTop").css("z-index", 20);
                    $("#header").css("z-index", 10);
                }//end if
            });

            $(".main_modal_link").on("click", function(e) {
                $('#bg_modal').hide();
                $("#hdrTop").css("z-index", 20);
                $("#header").css("z-index", 10);
            });

            $(".main_modal_close").on("click", function(e) {
                e.preventDefault();
                $('#bg_modal').hide();
                $("#hdrTop").css("z-index", 20);
                $("#header").css("z-index", 10);
                //if($('#today_modal_close:checked').val() != 'undefined' && $('#today_modal_close:checked').val() == "on") {
                    //setCookieMain('main_modal_layer_yn', diff_date, 1);
                    //setCookieMain('main_modal_layer_yn', diff_txt, 7);
                //}
            });

            $(".main_modal_today").on("click", function(e){
                e.preventDefault();
                $("#bg_modal").hide();
                $("#hdrTop").css("z-index", 20);
                $("#header").css("z-index", 10);
                setCookieMain('main_modal_layer_yn', diff_date, 1);
                //setCookieMain('main_modal_layer_yn', diff_txt, 7);
            });

            function setCookieMain(strName,strValue,expiredays) {
                var todayDate = new Date();
                todayDate.setDate( todayDate.getDate() + expiredays );
                document.cookie = strName + "=" + strValue + "; path=/; expires=" + todayDate.toGMTString() + ";";
            }
        </script>

