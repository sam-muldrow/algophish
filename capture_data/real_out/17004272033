    function introduction_center(){
        if(document.getElementById("introduction_center").style.display=="none"){
            document.getElementById("introduction_center").innerHTML="<embed width='662' height='393' src='https://www.youtube.com/v/0jADieFtAZo?autoplay=1' type='application/x-shockwave-flash' allowscriptaccess='always' allowfullscreen='true'></embed>";
            $("#introduction_center").show();
            $("#introduction_center_bg").show();
        }
    }

    function introduction_center_close(){
        document.getElementById("introduction_center").innerHTML="";
        $("#introduction_center").hide();
        $("#introduction_center_bg").hide();
    }

    function setCookie(strName,strValue,expiredays) {
        var todayDate = new Date();
        todayDate.setDate( todayDate.getDate() + expiredays );
        document.cookie = strName + "=" + escape( strValue ) + "; path=/; expires=" + todayDate.toGMTString() + ";";
    }

    function setCookie_Id(strName,strValue) {
        document.cookie = strName + "=" + escape( strValue ) + "; path=/;";
    }

    function getCookie(strName) {
        var strCookieName = strName + "=";
        var objCookie = document.cookie;

        if (objCookie.length > 0) {
            var nBegin = objCookie.indexOf(strCookieName);

            if (nBegin < 0) {
                return;
            }

            nBegin += strCookieName.length;
            var nEnd = objCookie.indexOf(";", nBegin);

            if (nEnd == -1) {
                nEnd = objCookie.length;
            }
        }
        return unescape(objCookie.substring(nBegin, nEnd));
    }

        var time_data = [{"holiday":"1/1|1/2|2/20|5/29|7/4|9/4|11/11|11/23|12/26|","Y":"2023","m":"11","d":"19","a":"pm","H":"12","i":"53","s":"21","N":"7","stat":"holidays","begintime":"09:30","endtime":"18:30","country":"CA"},{"holiday":"1/1|1/2|2/20|5/29|7/4|9/4|11/11|11/23|12/26|","Y":"2023","m":"11","d":"19","a":"pm","H":"15","i":"53","s":"21","N":"7","stat":"holidays","begintime":"09:00","endtime":"18:00","country":"NJ"},{"holiday":"1/1|1/21|1/22|1/23|1/24|1/25|1/26|1/27|6/22|6/23|9/29|10/1|10/2|10/3|10/4|10/5|10/6","Y":"2023","m":"11","d":"20","a":"am","H":"04","i":"53","s":"21","N":"1","stat":"workend","begintime":"08:00","endtime":"17:00","country":"CN"},{"holiday":"1/1|1/23|1/24|5/1|5/5|5/29|6/6|8/15|9/28|9/29|10/2|10/3|10/9|12/25","Y":"2023","m":"11","d":"20","a":"am","H":"05","i":"53","s":"21","N":"1","stat":"workend","begintime":"09:30","endtime":"18:30","country":"KR"},{"holiday":"1/1|1/2|1/3|1/9|2/11|2/23|3/21|4/29|5/3|5/4|5/5|7/17|8/11|9/18|9/23|10/9|11/3|11/23|12/29|12/30","Y":"2023","m":"11","d":"20","a":"am","H":"05","i":"53","s":"21","N":"1","stat":"workend","begintime":"09:30","endtime":"18:30","country":"JP"},{"holiday":"1/1|4/7|4/10|5/1|5/18|5/29|6/8|10/3|12/26|12/31","Y":"2023","m":"11","d":"19","a":"pm","H":"21","i":"53","s":"21","N":"7","stat":"holidays","begintime":"08:00","endtime":"17:00","country":"GR"},{"holiday":"1/1|1/2|4/7|4/10|5/1|5/8|5/29|8/28|12/26|12/27","Y":"2023","m":"11","d":"19","a":"pm","H":"20","i":"53","s":"21","N":"7","stat":"holidays","begintime":"09:30","endtime":"18:00","country":"GB"},{"holiday":"1/1|1/6|4/6|4/7|5/1|5/2|5/8|8/15|10/12|11/1|11/9|12/26","Y":"2023","m":"11","d":"19","a":"pm","H":"21","i":"53","s":"21","N":"7","stat":"holidays","begintime":"09:00","endtime":"18:00","country":"ES"},{"holiday":"1/1|1/26|4/7|4/10|4/25|9/1|10/2|12/25|12/26|12/27","Y":"2023","m":"11","d":"20","a":"am","H":"06","i":"53","s":"21","N":"1","stat":"workend","begintime":"09:00","endtime":"17:00","country":"AU"},{"holiday":"1/1|1/2|