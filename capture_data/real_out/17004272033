<script type="text/javascript">
//quick
var $quick = $('.quick');
if ($quick.length > 0) {
    var quickTop = $('#container').position().top;
    $(window).bind("mousewheel DOMMouseScroll MozMousePixelScroll scroll onmousewheel ", function() {
        if(quickTop < $(window).scrollTop()) {
            $quick.addClass('fixed');
            if(($('#goScroll').attr('value')=="Y") && ($('#goScroll').css('margin-top') == "10px")){    //메인이 아닐 때의 오른쪽 배너 부분 margin-top 줌
                $('#goScroll').css('margin-top', '0px');
            }
        } else {
            $quick.removeClass('fixed');
            if(($('#goScroll').attr('value')=="Y") && ($('#goScroll').css('margin-top') == "0px")){      //메인이 아닐 때의 오른쪽 배너 부분 margin-top 줌
                $('#goScroll').css('margin-top', '10px');
            }
        }
    });
}

