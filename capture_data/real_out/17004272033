var bner_now_idx = 0;
function top_bner_ctr(t){
    var max = bner_imgs.length - 1;
    var top_bner = {};
    var new_bner_idx = 0;

    if(t == "next") {
        if((bner_now_idx + 1) <= max) {
            new_bner_idx = bner_now_idx+1;
        } else {
            new_bner_idx = 0;
        }
    } else if(t == "prev") {
        if((bner_now_idx - 1) >= 0) {
            new_bner_idx = bner_now_idx - 1;
        } else {
            new_bner_idx = max;
        }
    }

    top_bner = bner_imgs[new_bner_idx];
    if(top_bner) {
        bner_now_idx = new_bner_idx;

        var html = "";

        //타입에 따라 나눔
        if(top_bner.type == "AD") {
            html += "<a class='ad-header-top' href='"+top_bner.link+"' data-ad-info-slaves-id='"+top_bner.id+"' target='_blank'>"
                    +"  <img style='width:"+top_bner.img_wh[0]+"px; height:"+top_bner.img_wh[1]+"px;' src='"+top_bner.img+"' alt='"+top_bner.title+"' />"
                    +"</a>";
        } else if (top_bner.type == "TOP") {
            html += "<img src='"+top_bner.img+"' alt='"+top_bner.title+"' usemap='#top_banner' hidefocus='true' border='0'>"
                   +"<map name='top_banner'>"+top_bner.map+"</map>";
        }

        //색상부분 동일처리
        if(top_bner.bg_type == "color"){
            $(".color-box.left-side").attr("style","background-image: none;background:#"+top_bner.bgclr[0]+";");
            $(".color-box.right-side").attr("style","background-image: none;background:#"+top_bner.bgclr[1]+";");
            $("#hdrBner").attr("style","background-color:#"+top_bner.bgclr[1]+";");
        } else {
            $(".color-box.left-side").attr("style","background:rgba(0,0,0,0);"+top_bner.bg_img[0]);
            $(".color-box.right-side").attr("style","background:rgba(0,0,0,0);"+top_bner.bg_img[1]);
            $("#hdrBner").attr("style","background-color:rgba(0,0,0,0);");
        }

        $("#hdrBner #bner_idx").html( new_bner_idx+1 );
        $("#hdrBner .img-box.new").html(html);
        $("#hdrBner").hide().fadeIn();
    }
}


(function($) {
    // 레이어 (상품 센터도착여부 확인하기
    if($("li.search_tracking").length > 0) {
        $("li.search_tracking").click(function(e) {
                    alert("로그인 후 이용해 주시기 바랍니다.");
                });
    }

    // 레이어 닫기 (상품 센터도착여부 확인하기)
    if ($("a.close_tracking").length > 0) {
        $("a.close_tracking").click(function(e){
            if($(".layer-center-c").is(":visible")) {
                $(".layer-center-c").hide();
            } else {
                $(".layer-center-c").show();
            }
        });
    }

    /*top scroll*/
    $("#r-snb .r-side-nav__top").click(function () {
        $('html, body').stop().animate({ scrollTop: 0 }, 400);
    })
})(jQuery);
</script>

