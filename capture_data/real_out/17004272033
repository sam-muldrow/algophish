        swipers['COMMON_TOP_TEXT_BANNER'] = new Swiper('#shoppingCOMMON_TOP_TEXT_BANNER .best-recmd .item .swiper-container', { loop: true, autoplay:4000, autoplayDisableOnInteraction: false });
    $('#shoppingCOMMON_TOP_TEXT_BANNER .best-recmd .item button.prev').on('click', function(e) { e.preventDefault(); swipers['COMMON_TOP_TEXT_BANNER'].swipePrev(); });
    $('#shoppingCOMMON_TOP_TEXT_BANNER .best-recmd .item button.next').on('click', function(e) { e.preventDefault(); swipers['COMMON_TOP_TEXT_BANNER'].swipeNext(); });
    
    function shopping(btn) {
        var tab_site= $('.shopping ul li.now a').attr('tab');
        var site    = tab_site.replace('#shopping', '');
        var tab     = $('.best-recmd .hd a.now', tab_site).attr('tab');
        /*
        var now_page    = parseInt($('.best-recmd .hd a.now', tab_site).attr('page'));
        var total_page  = parseInt($('.total span', tab_site).text());
        var page        = now_page;
        if(btn == "prev") {
            if(now_page == "1") {
                return;
            }
            page = now_page-1;
        } else if(btn == "next") {
            if(now_page == total_page) {
                return;
            }
            page = now_page+1;
        }
        */
        var page        = 1;
        var target_url  = '/ajax_shopping.php';
        $.ajax({
            url : target_url,
            type: 'post',
            data: {
                'site'  : site,
                'tab'   : tab,
                'page'  : page
            },
            dataType: 'json',
            success: function(data) {
                if(data.result == false) {
                    alert(data.msg);
                    return;
                }
                /*
                $('.total strong', tab_site).text(page);
                $('.best-recmd .hd a.now', tab_site).attr('page', page);
                $('.total span', tab_site).text(data.total_page);
                $('.item dl', tab_site).html(data.item_html);
                $('.list', tab_site).html(data.list_html);
                */
                swipers[site].removeAllSlides();
                $.each(data.list, function(key, val) {
                    swipers[site].appendSlide(val);
                });
            },
            error: function() {
                alert('ERROR!!');
            }
        });
    }

