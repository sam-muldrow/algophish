<script type="text/javascript">
    var swipers = [];
    var ajax_ing = false;

    // 로그인 카드배너 롤링
    var swiper_login = new Swiper('.login .frm .swiper-container', {
        loop: true,
        autoplay: 1300,
        autoplayDisableOnInteraction: false,
        mode: 'vertical',
        onInit: function(swiper) {
            var _slides = swiper.slides;
            var _now = swiper.activeIndex;
            var currentSlide = $(_slides[_now]);
            if (currentSlide.children().hasClass("ad-index-login")) {
                currentSlide.children().isOnScreen();
            }
        },
        onSlideChangeEnd: function(swiper) {
            var _slides = swiper.slides;
            var _now = swiper.activeIndex;
            var currentSlide = $(_slides[_now]);
            if (currentSlide.children().hasClass("ad-index-login")) {
                currentSlide.children().isOnScreen();
            }
        },
    });


    // 비주얼 배너 롤링
    var swiper = new Swiper('.visual .swiper-container', {
        loop: true,
        autoplay: 4000,
        autoplayDisableOnInteraction: false,
        paginationClickable: true,
        pagination: '.swiper-pagination',
        onInit: function(swiper) {
            var _slides = swiper.slides;
            var _now = swiper.activeIndex;
            var currentSlide = $(_slides[_now]);

            if (currentSlide.children().hasClass("ad-index-visual")) {
                currentSlide.children().isOnScreen();
            }
        },
        onSlideChangeEnd: function(swiper) {
            var _slides = swiper.slides;
            var _now = swiper.activeIndex;
            var currentSlide = $(_slides[_now]);

            if (currentSlide.children().hasClass("ad-index-visual")) {
                currentSlide.children().isOnScreen();
            }
        },
    });
    // 지사 고객센터 롤링
    var swiper2 = new Swiper('.branch .swiper-container', {
        loop: true,
        autoplay: 4000,
        autoplayDisableOnInteraction: false,
        onSlideChangeStart: function(swiper) {
            var _now = swiper.activeIndex + 1;
            var _name = $('.swiper-wrapper.center .swiper-slide:nth-child(' + _now + ')').find('img').attr('alt');
            $('.branch .name-txt').html(_name);
            $('.branch .name-bg').html(_name);
        }
    });

    // 해외센터 하단 배너
    var swiper3 = new Swiper('.bner-area .swiper-container', {
        loop: true,
        autoplay: 4000,
        autoplayDisableOnInteraction: false,
        onSlideChangeStart: function(swiper) {

        }
    });

    //고객센터 팝업
    var swiper7 = new Swiper('.center-list_wrap.swiper-container', {
        direction: 'horizontal',
        slidesPerView: 'auto'
    });

    $('.branch button.prev').on('click', function(e){
        e.preventDefault();
        swiper2.swipePrev();
    });
    $('.branch button.next').on('click', function(e){
        e.preventDefault();
        swiper2.swipeNext();
    });
    // 베스트 / 추천 상품 롤링
    
        swipers['RATUU'] = new Swiper('#shoppingRATUU .best-recmd .item .swiper-container', { loop: true, autoplay:4000, autoplayDisableOnInteraction: false });
    $('#shoppingRATUU .best-recmd .item button.prev').on('click', function(e) { e.preventDefault(); swipers['RATUU'].swipePrev(); });
    $('#shoppingRATUU .best-recmd .item button.next').on('click', function(e) { e.preventDefault(); swipers['RATUU'].swipeNext(); });
    
