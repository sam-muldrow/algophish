							function newGameDateConvert(dateStr) {
								var timeName = ["åä¸¤å¤©", "åä¸å¤©", "ä»å¤©", "ä¸å¤©å", "ä¸¤å¤©å"];
								var timeArray = dateStr.split("-");
								var ngdate = new Date(),
									ngnow = new Date();
								ngdate.setFullYear(timeArray[0], (new Number(timeArray[1]) - 1), timeArray[2]);
								ngdate.setHours(0, 0, 0, 0);
								ngnow.setHours(0, 0, 0, 0);
								var dayCount = (ngdate.getTime() / 3600000 - ngnow.getTime() / 3600000) / 24 + 2;
								if (dayCount >= 0 && dayCount < timeName.length) {
									return timeName[dayCount]
								}
								return (ngdate.getMonth() + 1) + "." + ngdate.getDate();
							}
							$(function() {
								jQuery.getJSON("//gameapi.uuu9.com/api/indexRank", function(result) {
									var zhuji_await = template.render('zhuji_await_list', {
										list: result.data.await_host
