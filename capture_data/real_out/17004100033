</div>
</div>
</div>
</div>
</div>
</div>
<script type="text/javascript">
$(window).on('load', function() {
// Slick carousel
$("#carosello-comunicati > #mypostenews").slick({
//touchThreshold: 30,
arrows: false,
dots: true,
infinite: false,
slidesToShow: 4,
slidesToScroll: 4,
adaptiveHeight: false,
responsive: [{
breakpoint: 1199,
settings: {
slidesToShow: 3,
slidesToScroll: 3,
}
},
{
breakpoint: 992,
settings: {
slidesToShow: 2,
slidesToScroll: 2
}
},
{
breakpoint: 768,
settings: {
slidesToShow: 1,
slidesToScroll: 1,
arrows: false,
dots: true
}
}
]
});
// POSTE NEWS
var category = "";
var listID = "[]"
if(listID != null && listID!=''){
var obj = JSON.parse(listID); 
}
var res = []; 
for(var i in obj) 
res.push(obj[i]); 
var numNews  =  res.length > 0 ? res.length : "";
var numNewsTGPoste = 0;
var items = [];
if(numNews != null && parseInt(numNews) > 0 &&  category !== null && category !== ''){
$.ajax({
url: "https://www.postenews.it/feed/out/"+category+".xml ",
dataType: "xml",
success: function(data) {
var $xml = $(data),
ct = 0;
$xml.find("item").each(function(i) {
var $this = $(this);
if( $this.find("title").text() !== undefined && $this.find("title").text() !== null && $this.find("title").text() !== '' &&
$this.find("description").text() !== undefined && $this.find("description").text() !== null && $this.find("description").text() !== '' 	&&
$this.find("pubDate").text() !== undefined && $this.find("pubDate").text() !== null && $this.find("pubDate").text() !== ''
) {
if(res.length > 0) { //valido solo se Ã¯Â¿Â½ presente la lista id
var feedID = $this.find("guid").text().split("p=");
if ( res.indexOf(feedID[1]) != -1) {
return; // continue
}
}
ct++;  // item valido	
var pubDate = $this.find("pubDate").text();
var date = new Date(pubDate);
var months = Array("GEN", "FEB", "MAR", "APR", "MAG", "GIU", "LUG", "AGO", "SET", "OTT", "NOV", "DIC");
var completeDate = date.getDay()+' '+months[date.getMonth()]+' '+date.getFullYear().toString().substr(-2);
var guidText = $this.find("guid").text();
var isGuidText = guidText !== undefined && guidText !== null && guidText !== '';
var guidLink = isGuidText ? 'https://www.postenews.it/?p=' + guidText.split("p=")[1] : '';
items.push(
'<div class="slick-elem col-sm-6 col-md-4 col-lg-3">'
+ '<div class="panel panel-cards panel-cards-news panel-cards-edge panel-cards-edge-yellow" style="padding: 0;">'
+ '<div class="panel-wrap">'
+ '<div class="panel-heading">'
+ '<div class="panel-heading-subtitle border-xs-bottom border-sm-bottom clearfix">'
+ '<span class="pull-xs-left">POSTE NEWS</span><span class="pull-xs-right">' + completeDate + ' ' + '</span>'
+ '</div>'
+ '<h5>' + $this.find("title").text() + '</h5>'
+ '</div>'
+ '<div class="panel-body">' + $this.find("description").text() 
+ (isGuidText ? ' <a href="' + guidLink + '" title="Continua la lettura" tabindex="0">(...)</a>' : '')
+ '</div>'
+ (isGuidText ? '<div class="panel-link panel-link-pos1"><a href="' + guidLink + '" title="Scopri di pi&ugrave;" target="_blank" class="btn-card"><span>Scopri di pi&ugrave;</span></a></div>' 
: '')
+ '</div>'
+ '</div>'
+ '</div>'
+ '</div>'
);  //fine push
} else {
if(res.length <= 0) {
ct--; //valido solo se non Ã¯Â¿Â½ presente una lista id
}
}
if(ct === parseInt(numNews)) {
return false; // breaks
}
}); //fine find
// $("#displayNews").html(items.join(''));
// $( "#mypostenews .slick-track" ).prepend(items.join(''));
$("#carosello-comunicati > #mypostenews").slick('slickAdd', items.join(''), numNewsTGPoste, true);
}// fine success
});// fine ajax
} //fine if
}); // fine window load
</script>
</div>
</div>
