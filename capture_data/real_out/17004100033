<script type="text/javascript">
$(document).ready(function() {
if(typeof variantePannelloRicerca !== 'undefined' && variantePannelloRicerca)  { //inizializzato nel layout della pagina 
if (!('remove' in Element.prototype)) {
Element.prototype.remove = function() {
if (this.parentNode) {
this.parentNode.removeChild(this);
}
};
} 
if(document.getElementById("imgBox")) {
document.getElementById("imgBox").remove();
}
} //inizializzato nel layout della pagina 
var element = document.getElementById("panelBody1453895247730");
element.style.cssText  = "margin-bottom:-45px;"
$('#servizio-home-traccia-spedizioni button').click(function() {
searchspedizioni($(this).parent().prev('input'));
});
$('#servizio-home-traccia-spedizioni input').keypress(function(ev) {
var keycode = (ev.keyCode ? ev.keyCode : ev.which);
if ('13' == keycode) { searchspedizioni(this); }
});
function searchspedizioni (element) {
var input = $(element).val();
if (input) {
var loc = "/cerca/index.html#/risultati-spedizioni/"+encodeURIComponent(input.replace("/", "%2F"));
if(typeof Webtrends !== 'undefined' && typeof Webtrends.multiTrack !== 'undefined'){
setTimeout(function() {
window.location.href = loc;
}, 120);
Webtrends.multiTrack({
argsa: ["DCSext.ricerca_sped_da_hp", 1, "WT.dl", 2016],
delayTime: 20, //delay returning to the caller by 20 milliseconds
callback: function (a) {
window.location.href = loc;
}
});
}
else{
window.location.href = loc;
}
}
return false;
}
});
</script>
