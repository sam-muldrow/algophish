<script>
	$(function(){
		$(window).on('load',function(){
			// accordion ---------
			var $trigger = $('#footGuide .accTrigger');
			$trigger.on('click', function () {
				var $this = $(this);
				var $detail = $this.siblings('.accDetail');
				if ($this.hasClass('active')) {
					$detail.stop(true, true).slideUp();
					$this.removeClass('active');
				} else {
					$this.addClass('active');
					$detail.stop(true, true).slideDown();
				}
			});

			// app ---------
			var ua = navigator.userAgent,
				applink = $('#appArea').find('a');

			//アプリの場合
			if (ua.indexOf('magaseek_android_app_2017') > 0 || ua.indexOf('magaseek_iphone_app_2017') > 0 ) {
				$('#appArea').hide();
				return false;
			}

			if (ua.indexOf('iPhone') > 0 || ua.indexOf('Android') > 0 && ua.indexOf('Mobile') > 0) {
				if(ua.indexOf('iPhone') > 0){
					$("#appArea a").attr('onclick', "var s=s_gi('magaseeknewcom'); s.linkTrackVars='eVar59,events'; s.eVar59='190613_iphone_sp'; s.events=''; s.tl(true,'o','appArea1');");
					$("#appArea a").attr('href', "https://go.onelink.me/dtMx/4c7ab30b");
				}
				if(ua.indexOf('Android') > 0){
					$("#appArea a").attr('onclick', "var s=s_gi('magaseeknewcom'); s.linkTrackVars='eVar59,events'; s.eVar59='190613_Android_sp'; s.events=''; s.tl(true,'o','appArea2');");
					$("#appArea a").attr('href', "https://go.onelink.me/dtMx/e5560460");
				}
			} else {
				applink.attr('href', 'https://www.magaseek.com/static/cont/id_APP1307');
			}
		});
	});
	$(document).find('script').each(function () {
		if ($(this).attr('type') === 'application/ld+json') {
			var data = JSON.parse($(this).html());
			if(data['@type'] === 'FAQPage' && $(this).attr('id') != 'faqData'){
				$('#faqData').remove();
			}
		}
	});
</script>
