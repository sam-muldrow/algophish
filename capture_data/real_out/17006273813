    <script nonce="9b4877e9af76a16b1327c5be5ca77f1e92183296">
      window.addEventListener('error', function (evt) {
        if (!evt.target) return;

        var src;
        if (evt.target.tagName == 'SCRIPT' && evt.target.src) {
          src = evt.target.src;
        } else if (evt.target.tagName == 'LINK' && evt.target.rel == 'stylesheet') {
          src = evt.target.href;
        } else {
          return;
        }

        var img = new Image();
        img.src = '/ep/internal/api/errors' +
          '?random=' + Math.random() +
          '&context=' + encodeURIComponent(navigator.userAgent) +
          '&message=' + encodeURIComponent("Error loading " + evt.target.tagName.toLowerCase() + ": " + src) +
          '&extraData=' + encodeURIComponent(JSON.stringify({src: src})) +
          '&url=' + encodeURIComponent(window.location.href) +
          '&isLegacyError=true';
      }, true /* useCapture */);
    </script>
  

