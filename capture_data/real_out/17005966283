<script>//Suppress or enable Zendesk chat based on the "Raven CS" dept being online
zE('webWidget:on', 'chat:departmentStatus', function(dept) {
	if (dept.name === 'Raven CS' && dept.status === 'online') {
		zE('webWidget', 'updateSettings', {
      webWidget: {
        launcher: {
          label: {
            '*': 'Contact',
          }
        },
        chat: {
          departments: {
            enabled: [''],
            select: 'Raven CS'
          },
          suppress: false,
          title: {
            '*': 'Chat with Raven'
          }
        },
        contactForm: {
          suppress: false,
          attachments: false,
          title: {
            '*': 'Contact Raven'
          }
        },
        helpCenter: {
          suppress: true
        }
      }
		});
	} else if (dept.name === 'Raven CS' && dept.status !== 'online') {
		zE('webWidget', 'chat:end');	//End chat
		zE('webWidget', 'updateSettings', {
      webWidget: {
        launcher: {
          label: {
            '*': 'Contact',
          }
        },
        chat: {
          suppress: true,
          title: {
            '*': 'Chat with Raven'
          }
