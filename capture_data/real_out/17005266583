<script>
	
	isTouchDevice = !!("ontouchstart" in document.documentElement);
	docready = [];
	var js2Load = [];

	
	var vm = {
		utils: null,
		AjaxContentLoader: null,
		scaleable: null,
		isIE7: null,
		Constants: {},
		Translations: {}
	};

	function loadScript(options) {
		if (options && options.url) {

			var s = document.createElement('script');

			if (options.callback) {
				addEvent(this, 'load', options.callback);
			}

			s.async = options.async && options.async != undefined ? Boolean(options.async) : true;
			s.src = options.url;
			s.type = 'text/javascript';
			if (options.html) s.text = options.html;

			document.getElementsByTagName("head")[0].appendChild(s);
		}
	}

	function addEvent(obj, type, fn) {
		if (obj == null | obj == undefined) return;
		if (obj.addEventListener) {
			obj.addEventListener(type, fn, false);
		} else if (obj.attachEvent) {
			obj["e" + type + fn] = fn;
			obj[type + fn] = function() {
				obj["e" + type + fn](window.event);
			};
			obj.attachEvent("on" + type, obj[type + fn]);
		}
	}

	function contentLoaded(w, fn) {
		var done = false,
				top = true,
				d = w.document,
				root = d.documentElement,

				add = d.addEventListener ? 'addEventListener' : 'attachEvent',
				rem = d.addEventListener ? 'removeEventListener' : 'detachEvent',
				pre = d.addEventListener ? '' : 'on',

				init = function(e) {
					if (e.type == 'readystatechange' && d.readyState != 'complete') return;
					(e.type == 'load' ? w : d)[rem](pre + e.type, init, false);
					if (!done && (done = true)) fn.call(w, e.type || e);
				},

				poll = function() {
					try {
						root.doScroll('left');
					} catch (e) {
						setTimeout(poll, 50);
						return;
					}
					init('poll');
				};

		if (d.readyState == 'complete') {
			fn.call(w, 'lazy');
		} else {
			if (d.createEventObject && root.doScroll) {
				try {
					top = !win.frameElement;
				} catch (e) {
				}
				if (top) {
					poll();
				}
			}
			d[add](pre + 'DOMContentLoaded', init, false);
			d[add](pre + 'readystatechange', init, false);
			w[add](pre + 'load', init, false);
		}
	}

	
	docready.push(function() {
		$('img.lazy-loading').each(function() {
			var $this = $(this);
			var newSRC = $this.attr('data-original');
			$this.attr('src', newSRC).removeAttr('data-original');
			$this.addClass('lazy-loaded').removeClass('lazy-loading');
		});
	});

	
