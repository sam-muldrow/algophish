    e.preventDefault();
});

if($('.header-bar').length){
    $('.header-bar').click(function() {
        $("html, body").stop().animate({ scrollTop:0 }, 800, 'easeOutExpo');
    });
}

if(modeMobile){
    $(".menu-mobile ul.menu-2>li>a").on('click', function(e){
         e.preventDefault();
         
        var isOpened = $(this).parent().hasClass('opened');
        if(isOpened)
            $(this).parent().removeClass('opened');
        else
            $(this).parent().addClass('opened');
    })

    if($('.product-content-modal').length){
        $('.product-content-modal .product-modal-close').on('click', function(){
            $('html').removeClass('block_scroll');
            $('.product-content-modal').removeClass('show');
            $('.menu-mobile-back').removeClass('show');
            setTimeout(()=>{
                $('.product-content-modal .product-modal-title').html('');
                $('.product-content-modal .product-modal-content').html('');
            },300)
        });
    }
    
    var descriptionPaneContent = ''
    if($('#description-pane').length){
        descriptionPaneContent = $('#description-pane').html();
        $('#description-pane').html('<p>'+$('#description-pane').text()+'</p>');
    }
    
    if(
        $('.section-product-desc').length ||
        $('.section-product-attributes').length
    ){
        $('.body-bottom .show-more').on('click', function(e){
            e.preventDefault();
            
            var theContent = $(this).parent().parent().find('.section-content');
            if(theContent.attr('id') === 'description-pane')
                theContent = descriptionPaneContent
            else
                theContent = theContent.html();
                
            var theTitle = $(this).parent().find('.title-icon')[0].outerHTML + $(this).parent().find('h3')[0].outerHTML
            $('.product-content-modal .product-modal-title').html(theTitle)
            $('.product-content-modal .product-modal-content').html(theContent)
            setTimeout(()=>{
                $('html').addClass('block_scroll');
                $('.menu-mobile-back').addClass('show');
                $('.product-content-modal').addClass('show');
            },200)
        });
    }
    if($('.section-product-comments').length){
        $('.body-bottom').on('click', '.show-more', function(e){
            e.preventDefault();
            
            var theContent = $(this).parent().find('.section-content');
            if(theContent.attr('id') === 'description-pane')
                theContent = descriptionPaneContent
            else
                theContent = theContent.html();
                
            var theTitle = $(this).parent().find('.title-icon')[0].outerHTML + $(this).parent().find('h3')[0].outerHTML
            $('.product-content-modal .product-modal-title').html(theTitle)
            $('.product-content-modal .product-modal-content').html(theContent)
            setTimeout(()=>{
                $('html').addClass('block_scroll');
                $('.menu-mobile-back').addClass('show');
                $('.product-content-modal').addClass('show');
            },200)
        });
    }
}else{
    $(".menu-cats-nav ul>li>ul").on('click', function(e){
        var isOpened = $(this).parent().hasClass('opened');
        if(isOpened)
            $(this).parent().removeClass('opened');
        else
            $(this).parent().addClass('opened');
    })

    if($('.product-features').length && $('.product-features ul li').length>4){
         $('.product-features').addClass('collapsable');
        $('.product .body-top').prepend('<div class="blured-bg"></div>');
        $('.product-features').append('<span class="pf-show-more">بیشتر<i class="ja-arrow-down"></i></span><div class="pf-box"><ul>'+
            $('.product-features ul').html()+'</ul><span class="pf-show-less">کمتر<i class="ja-arrow-down ja-flip-vertical"></i></spa></div>')
            
        $('.product-features').on('click', '.pf-show-more', function(){
            $('.product .body-top .blured-bg').addClass('show');
            $('.product-features .pf-box').addClass('show');
        });
        $('.product .body-top').on('click', '.blured-bg, .pf-show-less', function(){
            $('.product .body-top .blured-bg').removeClass('show');
            $('.product-features .pf-box').removeClass('show');
        });
    }
}

var yposition = $(window).scrollTop(); // should start at 0
var startYPoint = ($('html').hasClass('toolbar_on'))? 48 : 0;
    
if(modeMobile){
    var productBodyPos = false;
    if($('.product .body').length)
        var productBodyPos = $('.product .body').height() + $('.product .body')[0].offsetTop;
    var isDetectDevice = $('.detect-device').length;
    var detectDevice = $('.detect-device')
    var isMiniPopup = $('.mini-jpopup').length;
    var miniPopup = $('.mini-jpopup');
    var headerMobile = $('.header-mobile');
    var addToBasketCont = $('.add-to-basket-cont')
    if($('#article-sticky-ad').length && isDetectDevice)
        detectDevice.addClass('show3');
    if($('#article-sticky-ad').length && isMiniPopup)
        miniPopup.addClass('show3');
    $(window).scroll(function() {
        var scroll = $(window).scrollTop();
        if (scroll > (startYPoint + 50) && scroll > yposition)
            headerMobile.addClass('scrolled');
        else
            headerMobile.removeClass('scrolled');
        if(productBodyPos){
            if (scroll > (startYPoint + 50) && scroll < productBodyPos){
                addToBasketCont.addClass('show');
                if(isDetectDevice) detectDevice.addClass('show2');
                if(isMiniPopup) miniPopup.addClass('show2');
            }else{
               addToBasketCont.removeClass('show'); 
               if(isDetectDevice) detectDevice.removeClass('show2');
               if(isMiniPopup) miniPopup.removeClass('show2');
            }
        }
        yposition = scroll;
    });
}else{
    $(window).scroll(function() {
        var scroll = $(window).scrollTop();
        if(scroll> (startYPoint + 50)) $('.header-1').addClass('scrolled-1');
        else $('.header-1').removeClass('scrolled-1');
        if (scroll > (startYPoint + 100) && scroll > yposition)  $('.header-1').addClass('scrolled-2');
        else $('.header-1').removeClass('scrolled-2');
        yposition = scroll;
    });
    $(".menu-cats").hover(function(){
        $('.menu-cats-back').addClass("show");
    }, function(){
        $('.menu-cats-back').removeClass("show");
    });
}

if($('.product-basket .alternative').length){
    $('.product-basket .alternative').on('click', function(e){
         e.preventDefault();
         var alterPosTop = ($('#box_related').length)? ($('#box_related').offset().top - 75 - startYPoint) : 560;
         $("html, body").stop().animate({ scrollTop:alterPosTop }, 800, 'easeOutExpo');
    });
}

if($('#filter_brand').length){
    if($(".brands_list ul").find("li").length > 10){
        var searchInBrandsHtml = '<input style="margin-bottom:10px" placeholder="جستجو در برند ها ..." id="searchBrand" class="form-control" type="text">';
        $(searchInBrandsHtml).insertAfter("label[for=brand_id]");
        
        $('body').on('input', '#searchBrand', function() { 
            var ul, li, span, span2, i, txtValue, txtValue2, filter;
            filter = $(this).val().toUpperCase();
            ul =  $(this).parent().find('.brands_list ul')[0];
            li = ul.getElementsByTagName("li");
            
            for (i = 0; i < li.length; i++){
                span = li[i].getElementsByTagName("span")[0];
                span2 = li[i].getElementsByTagName("label")[0];
                txtValue = span.textContent || span.innerText;
                txtValue2 = '$';
                txtValue2 = span2.textContent || span2.innerText;
                if (txtValue.toUpperCase().indexOf(filter) > -1 || txtValue2.toUpperCase().indexOf(filter) > -1)
                    li[i].style.display = "";
                else 
                    li[i].style.display = "none";
            }
        });
    }
}

if($('.toggle-slide-area').length){
    
    $('.toggle-slide-area').each(function() {
        var val = Number($(this).find('input').val());
        var activeClass = val === 1 ? ' active' : '';
        $(this).prepend(`<div class="filter-toggler${activeClass}"><div class="filter-toggler-circ"></div></div>`);
    }).on('click', '.filter-toggler', function() {
        var $this = $(this), val = Number($this.siblings('input').val()) ^ 1;
        $this.toggleClass('active').siblings('input').val(val).change();
    });

}

if($('.mode_desktop #blog-article-side').length || $('.mode_desktop #side_right').length){
    var tfromTop = 135;
    if ($('.toolbar_on').length) tfromTop = 183
    
    if($('.mode_desktop #blog-article-side').length)
        $('.mode_desktop #blog-article-side').theiaStickySidebar({
            additionalMarginTop: tfromTop
        });
    
    if($('.mode_desktop #side_right').length)
        $('.mode_desktop #side_right').theiaStickySidebar({
            additionalMarginTop: tfromTop
        });
}

if(modeMobile){
    if($('.section-product-comments').length){
        if($('.rate-section').length)
            $('.rate-section').click(function() {
                var cfromTop = $('.section-product-comments').offset().top - 70;
                if ($('.toolbar_on').length) cfromTop = cfromTop - 48
                $("html, body").stop().animate({ 
                    scrollTop: cfromTop
                }, 800, 'easeOutExpo');
            });
        var commentsLoadtest = 0
        function checkCommentsShowMore() {
            setTimeout(()=>{
                if ($('#comments li').length) {
                    if($('#comments-pane').length && $('#comments-pane').height() > 600){
                        $('#comments-pane').addClass('minimized');
                        $('.section-product-comments .show-more').addClass('show');
                    }
                }else{
                    if(commentsLoadtest <= 15)
                        checkCommentsShowMore()
                    commentsLoadtest++
                }
            }, 300)
        }
        checkCommentsShowMore()
    }
}else{
    if($('.rate-section').length && $('#comments-tab').length){
        $('.rate-section').click(function() {
            
            var cfromTop = $('#comments-tab').offset().top - 70;
            if ($('.toolbar_on').length) cfromTop = cfromTop - 48;
            $("html, body").stop().animate({ 
                scrollTop: cfromTop
            }, 800, 'easeOutExpo');
            setTimeout(()=>{
                $('#comments-tab').click();
            }, 750)
        });
    }
}


// $(document).on('click', 'form[name=signin_form] button', function(){
//     console.log('asasd')
//     $('.alert.alert-danger').remove()
// });
var items = 0;
var cart_url = 'https://janebi.com/cart';
$(document).on("click",'.btn-basket',function () {
	
	if($(this).attr('disabled')=='disabled' )
		return false;
	var basket_data = $(this).closest(".product-basket").serialize();
	//hook('test_basket',basket_data);
	var item = $(this).data('id');
	$("#add_"+item).attr('disabled','disabled');
	loading(1);
	$.ajax({
		type: "POST",
		url: cart_url,
		data: basket_data,
		timeout: (10 * 1000),
		success: function(responses){
			eval (responses);
			if(typeof id !== 'undefined'){
				var this_id = "#basket-" + id;
				if( $(this_id).length > 0){
					$(this_id).animate({ opacity: 0 }, 500, function() {
						$(this_id).before(response).remove();
					});
					$(this_id).animate({ opacity: 0 }, 500);
					$(this_id).animate({ opacity: 1 }, 500);
				}
				else if(response!='')
				{
					
					$("#basket ul").prepend(response);
					// $("#basket li:first").before(response);
					$("#basket ul li:first").hide();
					$("#basket li:first").slideDown("slow");
					
				}
				
				
			}
			
			loading(0);

			$("#add_"+item).removeAttr('disabled');
		},
		error: function( objAJAXRequest, strError  ){
			alert(strError);
		}
	});
	return false;
}).on('click', '.basket-items .btn-delete', function() {
	var item = $(this).data('delete');
	loading(1);
	$.ajax({
		type: "POST",
		url: cart_url,
		data: {
			data: "delete_product",
			id: item
		},
		success: function(responses) {
			eval (responses);
			$("#basket-" + item).slideUp("slow",  function() {
				$(this).remove();
			});
			loading(0);
		}
	});
});


new WOW().init();
		

var current_request = null;    
var search_ms = 0;
var search_min_length = 2;
var timer;
$(".live-search").after("<ul class='live-result'></ul>").keyup(function() {
    clearTimeout(timer);
    var keyword = this.value;
	if (keyword.length >= search_min_length) {
		current_request = $.ajax({
			type: "get",
			url: '/api/system/livesearch',
			cache:true,
			data: {q:keyword,ajax:1} ,
			beforeSend : function()    {           
				if(current_request != null) {
					current_request.abort();
				}
			},
			success: function(data){
				$('.live-result').html('');
				var results = jQuery.parseJSON(data);
				$(results).each(function(key, value) {
					img = (value.image && value.image !== '') ? "<img src='"+value.image+"' alt=''>":"";
					$('.live-result').append('<li><a href="'+ value.link +'">'+img+'<span>'+(value.type ? value.type+": ":"")+value.title +'</span></a></li>');
				});
				$('.live-result li a').click(function() {
					if($(this).attr('href') != '#'){
						loading(1);
						$('.live-search').val($(this).text());
					}
				});
			}
		});
	} else {
		$('.live-result').html('');
	}
}).blur(function(){
	$(".live-result").fadeOut(500);
}).focus(function() {
	$(".live-result").show();
});



$(document.body).on('click', '.btn-wishlist' , function() {
	loading(1);
	$.ajax({
		type: 'POST',
		url: 'https://janebi.com/nano/comment',
		data: {wishlist:$(this).attr('data-id')},
		success: function(data){
			eval(data);
			loading(0);
		},
		error: function( objAJAXRequest, strError  ){
			alert(strError);
			loading(0);
		}
	});
});

if(typeof tooltip === "function"){
	$('[data-toggle="tooltip"]').tooltip();
}
		$(document).on("click",'.btn-dialog',function (event) {
	event.preventDefault();	
	loading(1);
	$.ajax({
		type: "GET",
		url: $(this).attr('href'),
		data:{dialog:true, _go:'https://janebi.com/'},
		success: function(data){
			eval(data);
			loading(0);
		}
	});
});
		
document.addEventListener("mousemove", function handler(e) {
	e.currentTarget.removeEventListener(e.type, handler);
	$('.lazy-code').each(function(i, obj) {
		$(this).after($(this).html());
		$(this).remove();
	});
});
	
});

