    <div class="column is-full section-wrap">
        <h2 class="section-title"><i>Γνώμες</i></h2>
      </div>
    
    <article class="column is-3-desktop is-full-tablet is-full-mobile  prel vert blog-article" id="article-1379282">
    <div class="blocked blog-item">
  <div class="tile-content">
  <div class="wrap-main-tile-content">
    
    <div class="author-box columns is-multiline is-mobile">
            <div class="column is-narrow">
        <div class='author-image-wrapp'>
          <div class="author-image">
            <img alt="Γεώργιος Πιπερόπουλος"
                 width="317"
                 height="318" 
                 loading="lazy" src="data:image/svg+xml,%3Csvg%20xmlns='http://www.w3.org/2000/svg'%20viewBox='0%200%20317%20318'%3E%3C/svg%3E" 
