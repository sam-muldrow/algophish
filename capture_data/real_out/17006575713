(function($){

	$.fn.modal = function(prop){

		var options = $.extend({
			height : "250",
			width : "500",
			title:"Downloading...",
			description: "<p>PS: If you like our templates then, don't forget to ...</p>",
			top: "20%",
			left: "30%",
		},prop);
				
		return this.click(function(e){
			add_block_page();
			add_popup_box();
			
			$('.modal').fadeIn();
		});
		
	
		 function add_block_page(){
			var block_page = $('<div class="modal-block"></div>');
						
			$(block_page).appendTo('body');
		}
		 	
	
		 function add_popup_box(){
			 var pop_up = $('<div class="modal"><a href="#" class="modal-close">X</a><div class="modal-box"><h2>' + options.title + '</h2><section>' + options.description + '<ul class="share"><li class="facebook"><div class="fb-like fb_iframe_widget" data-href="https://www.templateism.com" data-send="false" data-layout="button_count" data-width="100" data-show-faces="true" fb-xfbml-state="rendered" fb-iframe-plugin-query="app_id=&amp;container_width=82&amp;href=https%3A%2F%2Fwww.templateism.com%2F&amp;layout=button_count&amp;locale=en_US&amp;sdk=joey&amp;send=false&amp;show_faces=true&amp;width=100"><span style="vertical-align: bottom; width: 82px; height: 20px;"><iframe name="f120fd0704" width="100px" height="1000px" frameborder="0" allowtransparency="true" allowfullscreen="true" scrolling="no" title="fb:like Facebook Social Plugin" src="https://www.facebook.com/v2.0/plugins/like.php?app_id=&amp;channel=https%3A%2F%2Fstatic.ak.facebook.com%2Fconnect%2Fxd_arbiter%2FX9pYjJn4xhW.js%3Fversion%3D41%23cb%3Dfcae11684%26domain%3Dwww.templateism.com%26origin%3Dhttps%253A%252F%252Fwww.templateism.com%252Ff1635f14a8%26relation%3Dparent.parent&amp;container_width=82&amp;href=https%3A%2F%2Fwww.templateism.com%2F&amp;layout=button_count&amp;locale=en_US&amp;sdk=joey&amp;send=false&amp;show_faces=true&amp;width=100" style="border: none; visibility: visible; width: 82px; height: 20px;" class=""></iframe></span></div></li><li class="twitter"><iframe id="twitter-widget-2" scrolling="no" frameborder="0" allowtransparency="true" src="https://platform.twitter.com/widgets/tweet_button.bd0320cab493e168513c7173184c1d5c.en.html#_=1438104919834&amp;count=horizontal&amp;dnt=false&amp;id=twitter-widget-2&amp;lang=en&amp;original_referer=https%3A%2F%2Fwww.templateism.com%2F&amp;size=m&amp;text=Templateism!%20Professional%20Fully%20Responsive%20Blogger%20template%20for%20free%20@templateism&amp;url=https%3A%2F%2Fwww.templateism.com" class="twitter-share-button twitter-tweet-button twitter-share-button t