    ];


    const bannerBlackFriday = document.querySelector('.js-bannerBlackFriday');

    if (hiddenUrl.includes(window.location.pathname)) {
        bannerBlackFriday.classList.add('pointer');
    }

    if (arrayPage.includes(window.location.pathname)) {
        bannerBlackFriday.classList.add('hidden');
    }

