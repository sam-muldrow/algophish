<script>
    /*
    Используется для отправки событий яндекс метрики с кнопок лендинга,
    имеющих класс - js-headerSendEventYandex и
    data-атрибут - data-headerSendEventYandex, где прописано имя события
    */

    window.addEventListener('load', function () {
        const buttons = document.querySelectorAll(`.js-headerSendEventYandex`);

        function sendEventYandex() {
            const defaultParams = {
                click_URL: document.location.pathname
            };

            const nameEventYandex = this.getAttribute(`data-headerSendEventYandex`);
            const dataEventYandex = JSON.parse(nameEventYandex);
            const targetYandex = dataEventYandex.name;
            const paramsYandex = dataEventYandex.params;

            const params = dataEventYandex.hasOwnProperty(`params`) ? Object.assign(paramsYandex, defaultParams) : defaultParams;

            ym(619945, 'reachGoal', targetYandex, params);
        }

        [...buttons].forEach(button => {
            button.addEventListener(`click`, sendEventYandex);
        });
    });
</script>

<script>
    window.addEventListener('load', function () {
        let openButton = document.querySelector(".js-openButton");
        let contnentMenu = document.querySelector('.js-hideContent');
        const linkMobileMenu = document.querySelectorAll('.js-linkButton');

        if (openButton) {
            openButton.addEventListener('click', function(e) {
                e.preventDefault();
                contnentMenu.classList.toggle('hide-mobile');
            });

            contnentMenu.addEventListener('mouseleave', function(e) {
                contnentMenu.classList.add('hide-mobile');
            });
        }

        for (let i = 0; i < linkMobileMenu.length; i++) {
            let elem = linkMobileMenu[i];
            elem.addEventListener('click', function () {
                contnentMenu.classList.add('hide-mobile');
            });
        }





        const imageElement = document.querySelector('.mdMenu-logo__nav-img');
        const imageDefault = imageElement.src;
        const MobilePrefix = '-mobile.svg';
        let srcImage = imageDefault.replace(/\.[^/.]+$/, "");
        let MobileImage = srcImage + MobilePrefix;

        function handleWindowResize() {
            const width = window.innerWidth;
            let imgSrc = (width <= 1013) ? MobileImage : imageDefault;

            if (imageElement.src !== imgSrc) {
                imageElement.src = imgSrc;
            }
        }

        handleWindowResize();

        window.addEventListener('resize', handleWindowResize);
    });

</script>

<script>
    window.addEventListener('load', function () {
        const openButton = document.querySelector(".js-openButtonMenu");
        const contnentMenu = document.querySelector('.mdMenu-row--anchor');

        if (openButton) {
            openButton.addEventListener('click', function(e) {
                e.preventDefault();
                contnentMenu.classList.toggle('mdMenu-row--visible');
            });

            contnentMenu.addEventListener('mouseleave', function() {
                contnentMenu.classList.remove('mdMenu-row--visible');
            });
        }
    });
</script>

<script>
    function showBackdrop() {
        var backdrop = document.querySelector('.js-backdrop');
        backdrop.style.opacity = 1;
        backdrop.style.zIndex = 5;
        backdrop.style.left = 0;
    }

    function hideBackdrop() {
        var backdrop = document.querySelector('.js-backdrop');
        backdrop.style.opacity = 0;
        backdrop.style.zIndex = 5;
        backdrop.style.left = '-100%';
    }

    window.addEventListener('load', function () {
        var menuElem = document.querySelector('.js-navMenu');

        if (!menuElem) {
            return;
        }

        menuElem.addEventListener('mouseenter', function (e) {
            if (!e.target.classList.contains('active-item')) {
                e.target.classList.add('active-item');
                showBackdrop();
            }
        });

        menuElem.addEventListener('mouseleave', function (e) {
            if (e.target.classList.contains('active-item')) {
                e.target.classList.remove('active-item');
                hideBackdrop();
            }
        });
    });
</script>

<script>
    document.addEventListener('click', function(e){
        var element = e.target;
        if (element && element.classList.contains('js-getAccess')) {
            var valueHref = element.getAttribute('href');
            e.preventDefault();
            e.stopPropagation();
            dataLayer.push({event: 'getaccess'});
            setTimeout(function(){
                window.open(valueHref, '_blank');
            }, 50);
        }
    });

    window.addEventListener('load', function() {
        const popupAuthModal = document.querySelector('.js-authModal');
        var arrElems = [];
        if (!document.querySelector('.js-login-elem')) {
            return false;
        }
        arrElems.push(document.querySelector('.js-login-elem'));
        arrElems.push(document.querySelector('.js-mobile-nav-options--login'));
        for (var i = 0; i < arrElems.length; i++) {
            arrElems[i].addEventListener('click', function(e){
                e.preventDefault();
                e.stopPropagation();
                dataLayer.push({event: 'dialogopen'});

                popupAuthModal.style.display = "block";

                function closePopup() {
                    popupAuthModal.style.display = "none";
                }

                function onClickPopup(e) {
                    const hasClickOutPopup = e.target.classList.contains('js-authModal');
                    const isCloseButton = e.target.classList.contains('js-authModalClose');

                    isCloseButton && closePopup();
                    hasClickOutPopup && closePopup();
                }
                popupAuthModal.addEventListener('click', onClickPopup);
            }, false)
        }
    });
</script>

<script>
    window.addEventListener('load', childHeight);

    function childHeight() {
        const parentItem = document.querySelectorAll('.js-parentItem');
        for (let i = 0; i < parentItem.length; i++) {
            const item = parentItem[i];
            const parentList = item.parentNode;
            item.addEventListener('mouseover', function () {
                const parentListHeight = parentList.clientHeight;
                const child = item.querySelector('.js-child');
                const childItem = item.querySelector('.js-childItem');
                const childHeight = child.clientHeight;
                if (parentListHeight < childHeight) {
                    parentList.style.height = childHeight + 'px';
                } else {
                    childItem.setAttribute('style', 'height: auto; min-height: 100%');
                }
            });

            item.addEventListener('mouseout', function () {
                parentList.style.height = 'auto';
            });
        }
    }
</script>

<!--выпилил facebook, пока он заблокирован в России-->

    <script>
    /* Скрипт по подсвечиванию пунктов меню в зависимости от урла страницы */
    (function(){
        var arrayRegularValueMdBuhgalter = [
            /^\/landing\//i,
            /^\/buhgalterskie-uslugi/i,
            /^\/landingpage\/10-ways/i,
            /^\/landingpage\/accounting-assistance/i,
            /^\/landingpage\/autsorsing-cooperative/i,
            /^\/landingpage\/bonus\/aut/i,
            /^\/landingpage\/buhgalterskie-uslugi/i,
        ];

        var arrayRegularValueRegistartionPage = [
            /^\/buhgalterskie-uslugi\/service\/sno/i,
            /^\/buhgalterskie-uslugi\/service\/registration-ooo/i,
            /^\/mdbuhgalter\/service\/registration-ip/i,
            /^\/landingpage\/ooo-ip-registration/i,
            /^\/registration-business/i,
            /^\/landingpage\/registratsiya-ooo-ip/i,
            /^\/landingpage\/registratsiya-ooo-ip\/ip/i,
            /^\/landingpage\/registratsiya-ooo-ip\/ooo/i
        ];

        var arrayRegularValueKassaPage = [
            /^\/landingpage\/atol/i,
            /^\/landingpage\/evotor/i
        ];

        var arrayRegularValueCommodityAccountingPage = [
            /^\/tovarouchet/i
        ];

        var arrayRegularValueKontragentsPage = [
            /^\/landingpage\/kontragents/i,
            /^\/landingpage\/buro\/price/i,
            /^\/buhgalterskie-uslugi\/blog\/kak-i-na-cto-proverat-kontragenta/i,
            /^\/mdbuhgalter\/blog\/pocemu-zablokirovali-scet-i-kak-ego-razblokirovat/i
        ];

        var arrayRegularValueBookkeepingPage = [
            /^\/online-buhgalteriya/i,
            /^\/internet-buhgalteriya/i,
            /^\/newyear2020/i,
            /^\/main-page/i,
            /^\/mainpage/i,
            /^\/service-for-accountants/i,
            /^\/landing\/buhgalteriya-main/i,
            /^\/company\/tariff/i
        ];

        function helperMatchUrl(arrRegExp) {
            var pathPageName = window.location.pathname;
            var count = 0;
            for(var i = 0, l = arrRegExp.length; i < l; i++) {
                var regularValue = arrRegExp[i];
                if (pathPageName.match(regularValue)) {
                    count++;
                }
            }
            return count;
        }

        function isMdbuhgalterPage() {
            return !!helperMatchUrl(arrayRegularValueMdBuhgalter);
        }

        function isRegistrationPages() {
            return !!helperMatchUrl(arrayRegularValueRegistartionPage);
        }

        function isBookkeepingPages() {
            return !!helperMatchUrl(arrayRegularValueBookkeepingPage);
        }

        function isCommodityAccountingPages() {
            return !!helperMatchUrl(arrayRegularValueCommodityAccountingPage);
        }

        function isKontragentsPage() {
            return !!helperMatchUrl(arrayRegularValueKontragentsPage);
        }

        function isClubPage() {
            return !!window.location.pathname.match(/^\/club/i);
        }

        function isTildaLpPage() {
            return !!window.location.pathname.match(/^\/lp/i);
        }

        function isLandingPage() {
            return !!window.location.pathname.match(/^\/landingpage/i);
        }

        function isCeolandingsPage() {
            return !!window.location.pathname.match(/^\/ceolandings/i);
        }

        function isPage() {
            return !!window.location.pathname.match(/^\/page/i);
        }

        function isCompanyPage() {
            return !!window.location.pathname.match(/^\/company/i);
        }

        function isSpravkaPage() {
            return !!window.location.pathname.match(/^\/spravka/i);
        }

        function isVioPage() {
            return !!window.location.pathname.match(/^\/voprosy-i-otvety/i);
        }
        function isManualPage() {
            return !!window.location.pathname.match(/^\/manual/i);
        }

        function isManualAutPage() {
            return !!window.location.pathname.match(/^\/manual\/autsorsing/i);
        }

        function isBuroPage() {
            return !!window.location.hostname.match(/^buro/i);
        }

        function isUpravlencheskijUchetPage() {
            return !!window.location.pathname.match(/^\/upravlencheskij-uchet/i);
        }

        function isBuroPageLocal() {
            return !!window.location.pathname.match(/^\/buro/i);
        }

        function isMainPage() {
            return !window.location.pathname.match(/[a-z]/i);
        }

        function isIbPages() {
            return  (isLandingPage() || isTildaLpPage() || isCeolandingsPage() || isPage()) && !isCommodityAccountingPages();
        }

        function isProductTuPage() {
            return ;
        }

        function isProductOutPage() {
            return ;
        }

        function isProductAssistantPage() {
            return ;
        }

        function isProductProfbuhPage() {
            return ;
        }

        function isPdPages() {
            if (isMainPage()) {
                return false;
            }

            return (
                !isClubPage() ||
                !isMdbuhgalterPage() ||
                !isRegistrationPages() ||
                !isSpravkaPage() ||
                !isVioPage() ||
                !isManualPage() ||
                !isManualAutPage() ||
                !isCommodityAccountingPages() ||
                !isLandingPage() ||
                !isCeolandingsPage() ||
                !isPage()
            );
        }

        function addActiveClassToItem() {
            var menu = document.querySelector('.js-navMenu');

            if (!menu) {
                return;
            }

            if (isIbPages() && isKontragentsPage()) {
                menu.children[4].classList.add('mdMenu-active-page');
            } else if (isRegistrationPages()) {
                menu.children[1].classList.add('mdMenu-active-page');
            } else if (isMdbuhgalterPage() && isRegistrationPages()) {
                menu.children[1].classList.add('mdMenu-active-page');
            } else if (isMdbuhgalterPage()) {
                menu.children[2].classList.add('mdMenu-active-page');
            } else if ((!isProductAssistantPage() && !isProductTuPage() && !isProductOutPage()) && (isIbPages() || isBookkeepingPages() ||/* isMainPage() || */ isProductProfbuhPage())) {
                menu.children[0].classList.add('mdMenu-active-page');
            } else if (isMdbuhgalterPage() && isKontragentsPage()) {
                menu.children[4].classList.add('mdMenu-active-page');
            } else if (isBuroPage() || isBuroPageLocal()) {
                menu.children[3].classList.add('mdMenu-active-page');
            } else if (isCommodityAccountingPages() || isProductTuPage()) {
                menu.children[6].classList.add('mdMenu-active-page');
            } else if (isUpravlencheskijUchetPage()) {
                menu.children[7].classList.add('mdMenu-active-page');
            } else if (isManualAutPage()) {
                menu.children[2].classList.add('mdMenu-active-page');
            } else if (isManualPage()) {
                menu.children[0].classList.add('mdMenu-active-page');
            } else if (isVioPage() || isSpravkaPage() || isCompanyPage() || isClubPage() || isPdPages()) {
                menu.children[5].classList.add('mdMenu-active-page');
            } else if (isMainPage()) {
                return false;
            } else  {
                menu.children[5].classList.add('mdMenu-active-page');
            }
        }

        function addActiveClassToItemFixed() {
            var menuFixed = document.querySelector('.js-navMenuFixed');

            if (!menuFixed) {
                return;
            }

            if (!isProductAssistantPage() && !isMdbuhgalterPage() && !isProductTuPage() && !isProductOutPage() && (isIbPages() || isMainPage() || isBookkeepingPages())) {
                menuFixed.children[0].classList.add('mdMenu-active-page');
            } else if (isMdbuhgalterPage() || isProductOutPage()) {
                menuFixed.children[1].classList.add('mdMenu-active-page');
            } else if (isCommodityAccountingPages() || isProductTuPage()) {
                menuFixed.children[2].classList.add('mdMenu-active-page');
            } else if (isManualAutPage()) {
                menuFixed.children[1].classList.add('mdMenu-active-page');
            } else if (isManualPage()) {
                menuFixed.children[0].classList.add('mdMenu-active-page');
            }
        }

        window.addEventListener('load', addActiveClassToItem);
        window.addEventListener('load', addActiveClassToItemFixed);
    })();
</script>
