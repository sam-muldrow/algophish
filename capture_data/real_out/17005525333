        const bodyElement = document.querySelector('body');
        const popupLeaveBlackFriday = document.querySelector('.js-popupLeaveBlackFriday');
        const timeoutPopup  = popupLeaveBlackFriday.dataset.timeout;

        function closePopup() {
            popupLeaveBlackFriday.style.display = "none";
            bodyElement.classList.remove('popupVisible');
        }

        function onClickPopup(e) {
            const hasClickOutPopup = e.target.classList.contains('js-popupLeaveBlackFriday');
            const isCloseButton = e.target.classList.contains('js-popupLeaveBlackFridayClose');

            isCloseButton && closePopup();
            hasClickOutPopup && closePopup();
        }

        function showPopup() {
            if (!localStorage.getItem('popupBlackFriday')) {
                localStorage.setItem('popupBlackFriday', 'true');
                popupLeaveBlackFriday.style.display = "flex";
                bodyElement.classList.add('popupVisible');
            }
        }

        function showPopupTimeout() {
            if (!localStorage.getItem('popupBlackFriday')) {
                showPopup();
            }
        }

        if (!arrayPage.includes(window.location.pathname)) {
            setTimeout(showPopupTimeout, parseInt(timeoutPopup) * 1000);

            bodyElement.addEventListener('mouseleave', showPopup);

            popupLeaveBlackFriday.addEventListener('click', onClickPopup);
        }

    })();
</script>

