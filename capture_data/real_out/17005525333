<script>

    function setCookie(name, value, options) {
        options = options || {};
        let expires = options.expires;
        if (typeof expires == "number" && expires) {
            let d = new Date();
            d.setTime(d.getTime() + expires * 1000);
            expires = options.expires = d;
        }
        if (expires && expires.toUTCString) {
            options.expires = expires.toUTCString();
        }
        value = encodeURIComponent(value);
        let updatedCookie = name + "=" + value;

        for (let propName in options) {
            updatedCookie += "; " + propName;
            let propValue = options[propName];
            if (propValue !== true) {
                updatedCookie += "=" + propValue;
            }
        }
        document.cookie = updatedCookie;
    }

    function getCookie(name) {
        let matches = document.cookie.match(new RegExp(
            "(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"
        ));
        return matches ? decodeURIComponent(matches[1]) : undefined;
    }

    if (getCookie("tSession") !== null) {
        setCookie("login_auth", "succses", 2592000);
    }

    if (getCookie("login_auth") !== null) {
        const loginButton = document.querySelector("#login_auth");

        if (loginButton) {
            loginButton.classList.add("js-openAuthModal");
        }
    }

    function navSubElements(items) {
        let result = '';

        items.forEach(function (item) {
            result += '<li class="mdMenu-article-knowledge__chapter-item js-parentItem ">' +
                '<a class="mdMenu-article-knowledge__link" href="' + item.getAttribute('href') + '">' +
                    '<span class="mdMenu-article-knowledge__title">' + item.innerText + '</span>' +
                '</a>' +
            '</li>';
        });

        return result;
    }

    function removeLinkMore() {
        let elem = document.getElementsByClassName('mdMenu-nav__more');
        if (elem[0] !== undefined) {
            elem[0].parentNode.removeChild(elem[0]);
        }
    }

    function linkMore() {
        let navMenu = document.querySelector('.js-navMenu');
        let navLink7 = navMenu.querySelector('.js-navMenu .mdMenu-nav__item:nth-child(7) .mdMenu-nav-elem');
        let navLink8 = navMenu.querySelector('.js-navMenu .mdMenu-nav__item:nth-child(8) .mdMenu-nav-elem');
        let navLinkMore = document.createElement('li');

        navLinkMore.classList.add("mdMenu-nav__item");
        navLinkMore.classList.add("mdMenu-nav__more");

        let navElements = (window.innerWidth > 1013 && window.innerWidth <= 1200)
            ? '<li class="mdMenu-nav-sub__item js-parentItem">' +
                    '<a class="mdMenu-nav-sub-elem only-link" href="' + navLink7.getAttribute('href') + '">' + navLink7.innerText + '</a>' +
                '</li>'
            : '';

        navElements += '<li class="mdMenu-nav-sub__item js-parentItem">' +
            '<a class="mdMenu-nav-sub-elem" href="' + navLink8.getAttribute('href') + '">' + navLink8.innerText + '</a>' +
            '<ul class="mdMenu-article-knowledge__chapter-container js-child" style="height: auto;">' +
                navSubElements(navMenu.querySelectorAll('.js-navMenu .mdMenu-nav__item:nth-child(8) .mdMenu-nav-sub-elem')) +
            '</ul>' +
        '</li>';

        navLinkMore.innerHTML =
            '<span class="mdMenu-nav-elem">Ещё</span>' +
            '<div class="mdMenu-nav-sub-wrap">' +
                '<div class="mdMenu-nav-content">' +
                    '<ul class="mdMenu-nav-sub" style="height: auto;">' +
                        navElements +
                    '</ul>' +
                    '<div class="mdMenu-nav-content-article"></div>' +
                '</div>' +
            '</div>';

        navMenu.append(navLinkMore);
        childHeight();
    }

    if (window.innerWidth > 1013 && window.innerWidth <= 1364) {
        linkMore();
    }

    window.addEventListener("resize", function() {
        removeLinkMore();
        if (window.innerWidth > 1013 && window.innerWidth <= 1364) {
            linkMore();
        }
    });
</script>

<script>
    var header = document.querySelector('.js-scrollMenu');
    const parentElemNav = header.parentElement;
    const headerHeight = header.offsetHeight;

    window.onscroll = function () {
        var scrolled = document.documentElement.scrollTop || document.body.scrollTop;

        var body = document.querySelector('body');

        var isClassNewConcept = header.classList.contains('mdMenuNewModify');

        var threshold = (window.innerWidth > 1013) ? 142 : 1;
        var padding = (window.innerWidth > 1013) ? 142 : 60;

        if (scrolled >= threshold) {
            if (isClassNewConcept) {
                parentElemNav.style.paddingTop = headerHeight + `px`;
            } else {
                body.style.paddingTop = padding + 'px';
            }

            header.classList.add('mdMenu-fixed');
            header.classList.remove('mdMenu-normal');
        } else {
            header.classList.remove('mdMenu-fixed');
            header.classList.add('mdMenu-normal');
            body.style.paddingTop = '0';
            parentElemNav.style.paddingTop = `0`;
        }
    };
</script>

