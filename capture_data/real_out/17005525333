    var lazy = function (attr) {
    attr = attr || 'src';

    var images = document.getElementsByClassName('lazy-img');
    var dataAttr;

    for (var i = 0; i < images.length; i++) {
        var image = images[i];

        if (image.dataset[attr]) {
            dataAttr = image.dataset[attr];
        } else {
            dataAttr = image.dataset.src;
        }

        if (image.getAttribute('src') != dataAttr && dataAttr) {
            image.setAttribute('src', dataAttr);
        }
    }

    var backgrounds = document.getElementsByClassName('lazy-background');
    for (var i = 0; i < backgrounds.length; i++) {
        var background = backgrounds[i];
        if (background.dataset[attr]) {
            dataAttr = 'data-' + attr;
        } else {
            dataAttr = 'data-src';
        }
        if (background.style.backgroundImage != 'url(' + background.getAttribute(dataAttr) + ')') {
            background.style.backgroundImage = 'url(' + background.getAttribute(dataAttr) + ')';
        }
    }
};

var onResize = function () {
    var viewportWidth = window.innerWidth;

    if (viewportWidth > 768) {
        lazy('src');
    } else if (viewportWidth > 480) {
        lazy('src767');
    } else if (viewportWidth > 320) {
        lazy('src479');
    } else {
        lazy('src320');
    }
};

onResize();

};

