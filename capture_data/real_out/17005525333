<script>
    (function() {
        const footerMenuTitles = document.querySelectorAll('.js-footerMenuTitle');
        const footerMenuBlocks = document.querySelectorAll('.js-footerMenuBlock');

        function main() {
            if (window.innerWidth <= 840) {
                [].forEach.call(footerMenuBlocks, function (block) {
                    block.style.height = block.scrollHeight + 'px';
                });
            } else {
                [].forEach.call(footerMenuBlocks, function (block) {
                    block.style.height = 'auto';
                });
            }
        }

        main();

        window.addEventListener("resize", function() {
            main();
        });

        [].forEach.call(footerMenuTitles, function (title) {
            title.addEventListener('click', (e) => {
                if (window.innerWidth <= 840) {
                    e.target.classList.toggle('open');
                    let block = e.target.nextElementSibling;
                    block.classList.toggle('open');

                    if (block.classList.contains('open')) {
                        block.style.height = block.scrollHeight + 'px';
                    } else {
                        block.style.height = 0;
                    }
                }
            });
        });
    })();
</script>


