    <script>
        window.doccloud = window.doccloud || {};
        window.doccloud.Commerce = window.doccloud.Commerce || {};
        window.doccloud.environment = "//commerce.adobe.com";
        window.doccloud.Commerce.checkouturl = window.doccloud.environment + '/checkout';
