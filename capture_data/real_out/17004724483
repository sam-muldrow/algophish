<script>

    var documentsLink = window.location.hostname === 'acrobat.adobe.com' ? 'https://documents.adobe.com'
        : 'https://documentsstage.adobe.com';
    if (window.location.hostname === 'www.adobe.com') {
        documentsLink = 'https://documents.adobe.com';
    }

    var yolo;
    if (window.location.href.indexOf('online') > -1){
        yolo = 'DocumentCloud1Yolo' 
    }
    var adobeid = {
        env: '//ims-na1.adobelogin.com',
        environment: 'prod',
        jumpToken: {
            api: '/ims/jumptoken/v1',
        },
        client_id: yolo || 'DocumentCloud1',
        scope: 'AdobeID,openid,creative_cloud,gnav,sao.cce_private,additional_info.roles,read_organizations',
        uses_redirect_mode: true,
        locale : 'en_US',
        uses_modal_mode: false,
        api_parameters: {
            authorize: {
                state: {
                    ac: '',
                }
            }
        },
        redirect_uri: '' || documentsLink,
        autoValidateToken: true,
        onReady: function () {
            window.dispatchEvent(new Event('dexter:IMSReady'));
            if (window.adobe_dc_sdk) {
                var evt;
                if (!!document.documentMode) {
                    evt = document.createEvent("CustomEvent");
                    evt.initCustomEvent("dc.imslib.ready", true, true, { detail: { instance: window.adobeIMS }});
                } else {
                    evt = new CustomEvent("dc.imslib.ready", { detail: { instance: window.adobeIMS }});
                    evt.initEvent("dc.imslib.ready", true, true);
                }
                document.dispatchEvent(evt);
                window.adobe_dc_sdk.imsReady = true;
            }
        }
    };
</script>
