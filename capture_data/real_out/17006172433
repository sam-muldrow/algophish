<script type="text/javascript" src="https://d287ku8w5owj51.cloudfront.net/inline/products/shared/mdl/material.min.js"></script><script type="text/javascript">
jQuery(function($){
  var newsletterForm_init = function(){
    let txt_userEmail = "";
    let email_isValid = "";
    
    let txt_formName = "form#form_signup";
    let elem_txtField = ".mdl-textfield";
    let txt_txtboxName = "#et_pb_signup_email";

    jQuery(txt_formName).submit(function( event ) {

      email_isValid = !jQuery(txt_formName+" "+elem_txtField).hasClass("is-invalid");
      txt_userEmail = jQuery(txt_formName+" "+txt_txtboxName).val();

      if(txt_userEmail.length == 0 || !email_isValid){
        jQuery(txt_formName+" "+elem_txtField).addClass("is-invalid is-dirty");      
        jQuery(txt_formName+" "+txt_txtboxName).focus();
      }else{
        window.location.href = "/newsletter/?id="+txt_userEmail;      
      }

      event.preventDefault();
    });
  };
  newsletterForm_init();
});
</script></div>
