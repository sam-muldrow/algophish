          <script>
            var getBlogList = function(){
              let json = "";
              const host = window.location.host;
              let bool_isStage = (host.indexOf("stage") != -1 || host.indexOf("localhost") != -1);
              let countrycode = "us";
              let api_domain = (bool_isStage) ? "https://api.stage.creative.com" : "https://api.creative.com";
              let api_url = api_domain+"/blog/api/v1/blogs/"+countrycode+"/";
              fetch(api_url).then(response => response.json()).then(data => {
                json=data.slice(0, 3);
                let bloglist = document.querySelector("ul.blog-list ul");
                for(let i=0;i<json.length;i++){
                  let elem_li = document.createElement("li");
                  let elem_a = document.createElement("a");
                  let blog_url = "/blog/"+json[i].post_title.toLowerCase().replaceAll(" ", "-");
                  elem_a.setAttribute("href", blog_url);
                  elem_a.setAttribute("target","_self");
                  elem_a.innerHTML = json[i].post_title;
                  elem_li.appendChild(elem_a);
                  bloglist.appendChild(elem_li);
                }
              });
            };
            getBlogList();
          </script>
      </div>

