<script src="https://ajax.aspnetcdn.com/ajax/jquery.validate/1.11.1/jquery.validate.min.js"></script>
<script src='https://www.google.com/recaptcha/api.js'></script>

<script>
    $(document).ready(function () {
        $($('#PardotFormId').find('form')[0]).validate({
            errorPlacement: function (error, element) {
                return false;
            }
        });
        $("#btnonSubmit").on("click", function (event) {
            if ($($('#PardotFormId').find('form')[0]).valid()) {
                var response = grecaptcha.getResponse();
                if (response.length === 0) {
                    $("#lblpardotRequiredCaptcha").css('display', 'block');
                    return false;
                }
                else {
                    $("#lblpardotRequiredCaptcha").css('display', 'none');
                }
                $("#btnSubmit").click();
            }
            else {
                $("#btnSubmit").click();
                //$("form").submit();
            }
        });
    });


    function imNotARobot() {
        $.ajax({
            type: "POST",
            url: $("#urltosendDataToServerforpardot").val(),
            data: { "myData": grecaptcha.getResponse() },
            success: function (response) {
            }
        });
    };

</script>

