    function setLoading() {
        $("#mapLoaderId").addClass("mapLoader");
        $('#maindivId').addClass("maindivClass");
    }

    function selectMarker(id, lat, long, address) {
        $(".map-sub-details").css("display", "none");
        $('.city-list > li > a').removeClass('active');
        $("#pnlBranchDetails").css("display", "block");
        $("#sub_details_" + id).css("z-index", "2");
        $("#lst_marker_" + id).find('a').addClass('active');
        $("#sub_details_" + id).css("display", "block");

        if ($(".full-addr-" + id).height() > 45) {
            $(".full-addr-" + id).css("overflow", "hidden");
            $(".full-addr-" + id).css("text-overflow", "ellipsis");
            $(".full-addr-" + id).css("white-space", "nowrap");
        }

        var latitude = lat; // Replace with the actual latitude
        var longitude = long; // Replace with the actual longitude

        // Create the map centered at the specified latitude and longitude
        var center = { lat: latitude, lng: longitude };
        gMap.setZoom(15);
        gMap.panTo(center);

    }


    function closeSubDetails(id) {
        $("#pnlBranchDetails").css("display", "none");
        $("#sub_details_" + id).css("z-index", "0");
    }

    function goToMap(id) {
        $.each(latlongmain, function (key, val) {
            if (val[3] == id) {
                var params = '?q=' + val[1] + ',' + val[2];
                params += '&ll=' + val[1] + ',' + val[2];
                params += '&z=16';
                window.open('http://maps.google.com/maps' + params, '_blank');
            }
        });
    }

    function initGoogleMapSection() {
        var map;
        var bounds = new google.maps.LatLngBounds();
        var mapOptions = {
            mapTypeId: 'roadmap',
            mapTypeControl: false, // Display the map type control
            fullscreenControl: false,
            streetViewControl: false,
        };

        // Display a map on the web page
        map = new google.maps.Map(document.getElementById("Googlemap"), mapOptions);
        map.setTilt(50);


        // Add multiple markers to map
        var infoWindow = new google.maps.InfoWindow(), marker, i;


        // Place each marker on the map
        for (i = 0; i < latlongmain.length; i++) {
            var position = new google.maps.LatLng(latlongmain[i][1], latlongmain[i][2]);
            bounds.extend(position);
            marker = new google.maps.Marker({
                position: position,
                map: map
            });
            markerslog.push(marker);
            // Add info window to marker
            google.maps.event.addListener(marker, 'click', (function (marker, i) {
                return function () {
                    infoWindow.open(map, marker);
                    selectMarker(latlongmain[i][3], latlongmain[i][1], latlongmain[i][2], latlongmain[i][0])
                }
            })(marker, i));

            google.maps.event.addListener(marker, 'mouseover', (function (marker, m) {
                return function () {
                    var title = latlongmain[m][0];
                    var subtitle = latlongmain[m][5];
                    var name = latlongmain[m][4];
                    var content = setContentString(name, title, subtitle);
                    infoWindow.setContent(content);
                    infoWindow.open(map, marker);
                }
            })(marker, i));


            google.maps.event.addListener(marker, 'mouseout', (function (marker, i) {
                return function () {
                    infoWindow.setContent("");
                    infoWindow.close(map, marker);
                }
            })(marker, i));


            // Center the map to fit all markers on the screen
            map.fitBounds(bounds);

            $("#mapLoaderId").removeClass("mapLoader");
            $('#maindivId').removeClass("maindivClass");
        }

        // Set zoom level
        var boundsListener = google.maps.event.addListener((map), 'bounds_changed', function (event) {
            this.setZoom(6);
            google.maps.event.removeListener(boundsListener);
        });
        return map;
    }

    function setContentString(name, title, subtitle) {
        var contentString = '<div class="H_ib_body"><div class="H_ib_close">' +
            '</div>' +
            '<div class="H_ib_content"><span class="col-1"> ' +
            '<span class="amx-cp-text-label not-break amx-mr-office">' + subtitle + '</span> ' +
            '</span> ' +
            '<span class="col-2"><span class="address -lbl">' + name + '</span>' +
            '<span class="address -line-1">' + title + '</span> ' +
            '<span class="address -line-2"></span></span></div>' +
            '</div>';

        return contentString;
    }

    function initializeDirection(lat, long) {
        isClickonDirection = true;
        var map = new google.maps.Map(document.getElementById('Googlemap'), {
            center: { lat: 37.7749, lng: -122.4194 },
            zoom: 8,
            mapTypeControl: false,
            fullscreenControl: false,
            streetViewControl: false,
        });

        var directionsService = new google.maps.DirectionsService();
        var directionsDisplay = new google.maps.DirectionsRenderer({
            map: map,
            panel: document.getElementsByClassName('directions-panel')
        });

        const origin = { lat: curUserlat, lng: curUserlong };
        const destination = { lat: lat, lng: long };

        var request = {
            origin: origin,
            destination: destination,
            travelMode: google.maps.TravelMode.DRIVING
        };

        directionsService.route(request, function (response, status) {
            if (status === google.maps.DirectionsStatus.OK) {
                directionsDisplay.setDirections(response);

                var steps = response.routes[0].legs[0].steps;
                var directionsOutput = '<div class="directiondiv"><ul class="amxdirections amx-cp-directions">';

                for (var i = 0; i < steps.length; i++) {
                    var step = steps[i];
                    var maneuver = step.maneuver || '';
                    var distance = step.distance.value;

                    var sign = '';
                    var arrowClass = ' -depart';
                    if (maneuver === 'turn-left' || step.instructions.trim().toLowerCase().includes('turn <b>left')) {
                        arrowClass = ' -leftTurn';
                    } else if (maneuver === 'turn-right' || step.instructions.trim().toLowerCase().includes('turn <b>right')) {
                        arrowClass = ' -rightTurn';
                    } else if (maneuver === 'sharp-left' || step.instructions.trim().toLowerCase().includes('sharp <b>left')) {
                        arrowClass = ' -sharpleftTurn'
                    } else if (maneuver === 'sharp-right' || step.instructions.trim().toLowerCase().includes('turn <b>right')) {
                        arrowClass = ' -sharpRightTurn'
                    } else if (maneuver === 'uturn-left' || step.instructions.trim().toLowerCase().includes('uturn <b>left')) {
                        arrowClass = ' -leftUTurn';
                    } else if (maneuver === 'uturn-right' || step.instructions.trim().toLowerCase().includes('uturn <b>right')) {
                        arrowClass = ' -rightUTurn'
                    }
                    else if (maneuver === 'keep-left' || step.instructions.toLowerCase().trim().includes('keep <b>left')) {
                        arrowClass = ' -leftFork'
                    }
                    else if (maneuver === 'keep-right' || step.instructions.toLowerCase().trim().includes('keep <b>right')) {
                        arrowClass = ' -rightFork'
                    }
                    else if (maneuver === 'continue' || step.instructions.toLowerCase().trim().includes('continue <b>straight')) {
                        arrowClass = ' -continue'
                    }

                    directionsOutput += '<li class ="amxdirections-list">';
                    directionsOutput += '<i class="amx-i-arrow ' + arrowClass + '" ></i>';
                    directionsOutput += '<strong>' + sign + '</strong> ' + (i + 1) + '.) ';
                    directionsOutput += step.instructions + ' Go for ' + parseInt(distance) + ' m';
                    directionsOutput += '</li>';
                }
                directionsOutput += '</ul> </div>';
                $('.directions-panel').html(directionsOutput);
            } else {
                $('.directions-panel').text('Error: ' + status);
            }
        });
    }

    function getUserLocation() {
        fetch('https://ipapi.co/json/')
            .then(response => response.json())
            .then(data => {
                curUserlat = data.latitude;
                curUserlong = data.longitude;
                console.log("curUserlat:", curUserlat);
                console.log("curUserlong:", curUserlong);
            })
            .catch(error => {
                console.error("Error:", error);
            });
    }

    function OpenDetailsTab(id, lat, long) {
        if (isClickonDirection) {
            gMap = initGoogleMapSection();

            setTimeout(function () {
                selectMarker(id, lat, long);
            }, 200);
            isClickonDirection = false;
        }
    }

    function GetDaysfromResourceKey(day) {
        switch (day) {
            case "SUN":
                return أحد;
            case "MON":
                return الاثنين;
            case "TUE":
                return الثلاثاء;
            case "WED":
                return الاربعاء;
            case "THU":
                return الخميس;
            case "FRI":
                return الجمعة;
            case "SAT":
                return السبت;
            default:
                return أحد;
        }
    }

</script>
    </div>
</div>
        </div>
    </section>





