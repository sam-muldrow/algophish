<!-- Začátek  naslouchání TC string na www.parlamentnilisty.cz -->
<script type="text/javascript">
	var afterTcReadyCalls = [];

var tcIsReady = false;
        var tcReadyListener = (function () {
            var f = {
                getTCData: function () {
                    __tcfapi("getTCData", 2, function (tcData, success) {
                        if (!success) {
                            console.error("Unbale to read getTCData!");
                            return;
                        }
                        console.info("--- TC Data ----------------------------------------------", tcData);
                    });
                }
            }
            if (!tcIsReady) {

                var checkAndRun = function () {
                    if (window.__tcfapi) {
                        tcIsReady = true;
                        processTcReadyCall();

                        f.getTCData();

                        return true;
                    }
                    return false;
                }

                if (checkAndRun()) {
                    return;
                }

                var maxAllowedCalls = 100;
                var listenToTcfApiInterval = window.setInterval(function () {
                    maxAllowedCalls--;
                    console.info("__tcfapi check...", maxAllowedCalls);
                    if (!checkAndRun() && maxAllowedCalls > -1) {
                        return;
                    }

                    if (maxAllowedCalls < 0) {
                        console.info("__tcfapi check canceled due to max call reached");
                    }

                    window.clearInterval(listenToTcfApiInterval);
                    listenToTcfApiInterval = null;
                }, 50);

                return f;
            }
        })();

    function processTcReadyCall() {
        console.info("run:processTcReadyCall");
        for (var i = 0; i < afterTcReadyCalls.length; i++) {
            console.log("invokeTcReadyCall:Execute", afterTcReadyCalls[i].toString());
            afterTcReadyCalls[i]();
        }
    }

	function invokeTcReadyCall(callRef) {
		if(tcIsReady)
		{
			console.log("invokeTcReadyCall:DirectExecute", callRef.toString());
			callRef();
		} else {
			console.log("invokeTcReadyCall:Queued", callRef.toString());
			afterTcReadyCalls.push(callRef);
		}
	}

	
</script>
