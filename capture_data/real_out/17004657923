    <script type="text/javascript">
        $(function () {
            $(".banner-pic .banner").each(function () {
                $(this).attr("style", $(this).data("url"));
            });
            $(".activity_pic img").each(function () {
                $(this).attr("src", $(this).data("url"));
            });

            $("#long_rental input").each(function () {
                var n = $(this).attr("name");
                if (n != undefined) {
                    $(this).attr("name", n.replace("ctl00$mainContent$", ""));
                }
            });
            $(".sp-box").on("mouseover", function () {
                var n = $(this).offset().top + "px"
                    , t = $(this).offset().left + "px";

                $(".ts-aczs .txt").html($(this).html());
                $(this).hasClass("sp-aczs") && $(".ts-aczs").css({
                    top: n,
                    left: t
                }).show();

            });
            $(".sp-box").on("mouseout", function () {
                $(".ts-box").hide();
            });
        });
    </script>
    <script type="text/javascript" async="async">
        var __zp_tag_params =
        {
            "pagetype": "homepage",
            "is_register": "0)" //已注册：1，未注册：0
        }
        // 首页展示底部浮层
        function mbTime() {
            switchmb = setTimeout(function () {
                mbhide();
            }, 5000);
        }
        mbTime();
        mbshow();
    </script>

