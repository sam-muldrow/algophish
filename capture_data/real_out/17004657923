    <script type="text/javascript">
        // 默认日期
        var hour = 4;
        var nowHour = '15';
        var defaultHour = '19';

        if ((Number(nowHour) + hour) <= 22) {
            // 取车时间
            $("#book_StartCldDate_lblgetCurrHour").text((defaultHour < 10 ? "0" : "") + defaultHour + "时");
            $("#book_StartCldDate_J_date_1").val('2023-11-20');

            // 还车时间
            $("#book_EndCldDate_lblretCurrHour").text((defaultHour < 10 ? "0" : "") + defaultHour + "时");
            $("#book_EndCldDate_J_date_2").val('2023-11-22');
        } else {
            // 取车时间
            $("#book_StartCldDate_lblgetCurrHour").text("9时");
            $("#book_StartCldDate_J_date_1").val('2023-11-21');

            // 还车时间
            $("#book_EndCldDate_lblretCurrHour").text("9时");
            $("#book_EndCldDate_J_date_2").val('2023-11-23');
        }

    </script>

