                     var defaultLi = $('.gdg_tab-li').first().attr('data-tab');
                     $('.gdg_tab-cnt').css("display", "none");
                     $('#' + defaultLi).addClass('active').css("display", "block");

                     $('.gdg_tab-li').click(function () {

                         if ($(this).attr('id') != "last") {
