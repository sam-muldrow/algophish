<script type="text/javascript">
    var setCookie=function(e,o,t,domain){
        var i="";
        if(t){
            var r=new Date;
            r.setTime(r.getTime()+24*t*60*60*1e3),i="; expires="+r.toUTCString()
        }
        if (domain) {
            document.cookie=e+"="+(o||"")+i+"; domain="+domain+"; path=/";
        } else {
            document.cookie=e+"="+(o||"")+i+"; path=/";
        }
    };
    agreeButton=document.getElementById("welcome_agree_button");
    agreeButton.addEventListener("click",function(){
        setCookie("trovit_show_gdpr","0",365,upperleveldomain),
        setCookie("trovit_gdpr","1",365,upperleveldomain),
        setCookie("trovit_personalized_google_ads","1",365,upperleveldomain),
        document.body.removeChild(document.getElementById("welcomebar-wrapper")),
        document.body.classList.remove("gdpr_banner_advise")
    });

    var disagreeButton=document.getElementById("welcome_disagree_button");
    disagreeButton.addEventListener("click",function(){
        setCookie("trovit_show_gdpr","0",365,upperleveldomain),
        setCookie("trovit_gdpr","0",365,upperleveldomain),
        setCookie("trovit_personalized_google_ads","0",365,upperleveldomain),
        document.body.removeChild(document.getElementById("welcomebar-wrapper"))
    }),
    setCookie("trovit_personalized_google_ads","1",365,upperleveldomain),
    document.body.classList.add("gdpr_banner_advise");
</script>
    </body>
