  <span class="prev box md_pointer" data-role="calendar-ctrl" data-from="{{= prev_from }}"><i class="fa-light fa-angle-left"></i></span>
  <span class="next box md_pointer" data-role="calendar-ctrl" data-from="{{= next_from }}"><i class="fa-light fa-angle-right"></i></span>
</div>
<table class="md_calendar size_l">
  <tbody>
    <tr>
      {{ _.each(daily_events, function(daily_events) { }}
      <th class="{{= daily_events.week_class }}">
        <div class="md_calendar_week">{{= daily_events.wday }}</div>
        <div class="md_calendar_date">{{= daily_events.day }}</div>
      </th>
      {{ }); }}
    </tr>
    <tr>
      {{ _.each(daily_events, function(daily_events) { }}
      <td class="pt5 {{= daily_events.week_class }} {{= daily_events.today_class }}">
        <div class="event_wrap">
          {{ _.each(daily_events.events, function(event) { }}
          <div class="event {{= event.symbol }}">
            <a href="{{= event.url }}">
              {{= event.label }}
              <span class="number">{{= event.count }}</span>
            </a>
          </div>
          {{ }); }}
        </div>
      </td>
      {{ }); }}
    </tr>
  </tbody>
</table>

