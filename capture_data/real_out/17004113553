                <script type="module">
                    import {
                        createApp,
                        ref,
                        onMounted,
                        computed
                    } from 'https://libs.biobiochile.cl/bbcl/vue/3.2.33/vue.esm-browser.prod.min.js'

                    createApp({
                        setup() {
                            const data = ref([]);
                            const loading = ref(false);

                            function fetchComputo() {
                                loading.value = true;
                                axios.get('https://www.biobiochile.cl/static/computo-teleton.json?v=' + Date
                                        .now())
                                    .then((result) => {
                                        data.value = result.data
                                        loading.value = false;
                                    }).catch((err) => {
                                        data.value = err
                                        loading.value = false
                                    });
                            }

                            onMounted(() => {
                                fetchComputo();
                                setInterval(fetchComputo, 60000)
                            });

                            return {
                                data
                            }
                        }
                    }).mount('#computo-teleton')
                </script>-->
