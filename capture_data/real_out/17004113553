                        <script>
                            const isLiveBiobiotvClosed = +localStorage.getItem('live-biobiotv-box-closed') > Date.now()

                            if (!window.IS_STREAMING_ON && !isLiveBiobiotvClosed) {
                                const containerLiveBiobiotv = document.getElementById('container-live-bbtv')
                                const liveBiobiotv = document.getElementById('live-bbtv')
                                const closeButtonLiveBiobiotv = document.querySelector('#live-bbtv .close-button')

                                window.addEventListener('scroll', () => {
                                    if (containerLiveBiobiotv.getBoundingClientRect().bottom < 0 && !window
                                        .IS_STREAMING_ON) {
                                        liveBiobiotv.classList.add('fixed')
                                    } else {
                                        liveBiobiotv.classList.remove('fixed')
                                    }
                                }, {
                                    passive: true
                                })

                                closeButtonLiveBiobiotv.addEventListener('click', () => {
                                    localStorage.setItem('live-biobiotv-box-closed', Date.now() + 1000 * 60 *
                                        60 * 24) // 24 hr
                                    liveBiobiotv.classList.add('closed')
                                })
                            }
                        </script>
