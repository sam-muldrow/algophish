                <script>
                    ;
                    (function() {
                        var BBCL = (window.BBCL = window.BBCL || {})
                        var plugins = (BBCL.plugins = BBCL.plugins || [])

                        plugins.push(function(Vue, store) {

                            new Vue({
                                el: '#super-titular',
                                store: store,
                                data: {
                                    cuentaregresiva: '',
                                    intervalCR: -1
                                },
                                mounted: function() {
                                    this.calcularCR()
                                    this.intervalCR = setInterval(this.calcularCR, 1000)
                                },
                                destroyed: function() {
                                    clearInterval(this.intervalId)
                                },
                                methods: {
                                    calcularCR: function() {
                                        var date = new Date()
                                        var hora = new Date('')
                                        if (hora > date) {
                                            var diffMs = hora - date
                                            var diffHrs = Math.floor((diffMs % (1000 * 60 * 60 *
                                                24)) / (1000 * 60 * 60));
                                            var diffMins = Math.floor((diffMs % (1000 * 60 *
                                                60)) / (1000 * 60));
                                            var diffSec = Math.floor((diffMs % (1000 * 60)) /
                                                1000);
                                            this.cuentaregresiva = ((diffHrs < 10) ? '0' +
                                                diffHrs : diffHrs) + ":" + ((diffMins <
                                                10) ? '0' + diffMins : diffMins) + ":" + ((
                                                diffSec < 10) ? '0' + diffSec : diffSec)
                                        } else {
                                            this.cuentaregresiva = 'AHORA'
                                            clearInterval(this.intervalCR)
                                        }
                                    }
                                }
                            })
                        })
                    })()
                </script>
