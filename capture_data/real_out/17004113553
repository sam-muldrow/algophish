    <script>
        (function() {
            var BBCL = (window.BBCL = window.BBCL || {})
            var plugins = (BBCL.plugins = BBCL.plugins || [])
            var mixins = (BBCL.mixins = BBCL.mixins || [])

            setTimeout(function() {
                if (window.BBCL_RELOAD) {
                    window.location.reload()
                }
            }, 420000)

            mixins.push({
                data: function() {
                    return {
                        ads: {
                            adTop1: '/1098385/biobiocl/ad_top',
                            adGt: '/1098385/biobiocl/ad_middle1'
                        },
                        barraNuevaConstitucion: {
                            mostrarBanner: false,
                            notasElegidasAlAzar: [],
                            notaFija: {
                                url: 'https://www.biobiochile.cl/especial/una-constitucion-para-chile/noticias/2021/05/07/solo-con-tu-rut-revisa-los-candidatos-de-tu-zona-para-las-proximas-elecciones-del-15-y-16-de-mayo.shtml',
                                titular: "SÃ³lo con tu RUT: revisa los candidatos de tu zona para las prÃ³ximas elecciones del 15 y 16 de mayo",
                                img: 'https://media.biobiochile.cl/wp-content/uploads/2021/05/a_uno_1225267_53192-320x190.jpg'
                            },
                            notasParaElegir: [{
                                    url: "https://www.biobiochile.cl/especial/una-constitucion-para-chile/noticias/2021/02/22/guia-para-entender-como-cuando-y-donde-operara-la-convencion-constituyente.shtml",
                                    titular: "Â¿CÃ³mo funcionarÃ¡ la ConvenciÃ³n Constituyente?",
                                    img: 'https://media.biobiochile.cl/wp-content/uploads/2021/02/como_funcionara_constituyente-320x190.png'
                                },
                                {
                                    url: "https://www.biobiochile.cl/especial/una-constitucion-para-chile/noticias/2021/02/26/como-funcionara-la-paridad-de-genero-en-la-eleccion-de-constituyentes.shtml",
                                    titular: "Â¿CÃ³mo funcionarÃ¡ la paridad de gÃ©nero en la elecciÃ³n de constituyentes?",
                                    img: 'https://media.biobiochile.cl/wp-content/uploads/2021/02/paridad_genero-320x190.png'
                                },
                                {
                                    url: "https://www.biobiochile.cl/especial/una-constitucion-para-chile/noticias/2021/02/23/limites-a-la-constituyente-el-rol-de-los-tratados-internacionales-en-la-nueva-constitucion.shtml",
                                    titular: "Â¿LÃ­mites a la constituyente? El rol de los tratados internacionales en la nueva constituciÃ³n",
                                    img: 'https://media.biobiochile.cl/wp-content/uploads/2021/02/limites_constitucion-320x190.png'
                                },
                                {
                                    url: "https://www.biobiochile.cl/especial/una-constitucion-para-chile/noticias/2021/02/22/asi-opera-el-sistema-que-elige-la-convencion-constitucional-distritaje-reparto-por-lista-y-cuotas.shtml",
                                    titular: "Â¿CÃ³mo se elegirÃ¡ a la ConvenciÃ³n Constituyente?",
                                    img: 'https://media.biobiochile.cl/wp-content/uploads/2021/02/elegir_constituyentes-320x190.png'
                                },
                                {
                                    url: "https://www.biobiochile.cl/especial/una-constitucion-para-chile/noticias/2021/04/01/pueblos-originarios-asi-se-elegiran-los-17-constituyentes-para-los-escanos-reservados.shtml",
                                    titular: "Pueblos originarios: asÃ­ se elegirÃ¡n los 17 constituyentes para los escaÃ±os reservados",
                                    img: 'https://media.biobiochile.cl/wp-content/uploads/2021/03/constituyente_pueblos_indigenas-320x190.png'
                                },
                                {
                                    url: "https://www.biobiochile.cl/especial/una-constitucion-para-chile/noticias/2021/05/10/conoce-las-opciones-de-voto-para-personas-en-situacion-de-discapacidad.shtml",
                                    titular: "Conoce las opciones de voto para personas en situaciÃ³n de discapacidad",
                                    img: 'https://media.biobiochile.cl/wp-content/uploads/2021/05/silla_de_ruedas_voto-320x190.png'
                                },
                                {
                                    url: "https://www.biobiochile.cl/especial/una-constitucion-para-chile/noticias/2021/05/11/que-debo-hacer-si-soy-vocal-de-mesa-revisa-los-pasos-para-estas-elecciones-del-15-y-16-de-mayo.shtml",
                                    titular: "QuÃ© debo hacer si soy vocal de mesa: revisa los pasos para estas elecciones del 15 y 16 de mayo",
                                    img: 'https://media.biobiochile.cl/wp-content/uploads/2021/05/vocal_mesa-320x190.png'
                                }
                            ]
                        }
                    }
                },
                mounted: function() {
                    var cantidadNotasAzar = this.barraNuevaConstitucion.mostrarBanner ? 2 : 3
                    this.barraNuevaConstitucion.notasElegidasAlAzar = this.getMeRandomElements(this
                        .barraNuevaConstitucion.notasParaElegir, cantidadNotasAzar)
                },
                methods: {
                    getMeRandomElements: function(sourceArray, neededElements) {
                        var result = [];
                        var randomIndex = [];
                        for (var i = 0; i < neededElements; i++) {
                            randomIndex = Math.floor(Math.random() * sourceArray.length);
                            result.push(sourceArray[randomIndex]);
                            sourceArray.splice(randomIndex, 1);
                        }
                        return result;
                    }
                }
            })

            plugins.push(function(Vue, store) {

                new Vue({
                    el: "#countdown",
                    data() {
                        return {
                            untilDate: new Date(2021, 11, 19, 0, 0, 0),
                            days: 0,
                            hours: 0,
                            minutes: 0,
                            seconds: 0,
                            shouldCount: false,
                        };
                    },
                    mounted() {
                        this.shouldCount = true;
                        requestAnimationFrame(this.updateCountdown);
                    },
                    destroyed() {
                        this.shouldCount = false;
                    },
                    methods: {
                        updateCountdown() {
                            const diff = this.untilDate.getTime() - Date.now() + 1000;
                            this.days = Math.floor(diff / (1000 * 60 * 60 * 24));
                            this.hours = Math.floor((diff / (1000 * 60 * 60)) % 24);
                            this.minutes = Math.floor((diff / (1000 * 60)) % 60);
                            this.seconds = Math.floor((diff / 1000) % 60);

                            if (diff <= 0) {
                                console.log(diff)
                                this.shouldCount = false;
                                var list = document.getElementsByClassName("cuenta-regresiva")[0];
                                list.getElementsByClassName("cuenta-regresiva-box")[0].style
                                    .display = "none";
                                list.getElementsByClassName("cuenta-regresiva-box")[1].style
                                    .display = "none";
                                list.getElementsByClassName("cuenta-regresiva-box")[2].style
                                    .display = "none";
                                list.getElementsByClassName("cuenta-regresiva-box")[3].style
                                    .display = "none";
                            }

                            if (this.shouldCount) {
                                requestAnimationFrame(this.updateCountdown);
                            }
                        },
                    },
                });

                new Vue({
                    el: '#section-regiones',
                    data: {
                        regionesTabsActiveTab: 'bio_bio'
                    },
                    mounted() {
                        if (localStorage.regionesTabsActiveTab) {
                            this.regionesTabsActiveTab = localStorage.regionesTabsActiveTab;
                        }
                    },
                    watch: {
                        regionesTabsActiveTab(newRegionesTabsActiveTab) {
                            localStorage.regionesTabsActiveTab = newRegionesTabsActiveTab;
                        }
                    }
                })

                new Vue({
                    el: '#section-reportajes',
                    methods: {
                        next: function() {
                            this.$refs['section-reportajes-carousel'].slideNext()
                        },
                        prev: function() {
                            this.$refs['section-reportajes-carousel'].slidePrev()
                        }
                    }
                })

                new Vue({
                    el: '#section-bbcl-investiga',
                    methods: {
                        next: function() {
                            this.$refs['section-bbcl-investiga-carousel'].slideNext()
                        },
                        prev: function() {
                            this.$refs['section-bbcl-investiga-carousel'].slidePrev()
                        }
                    }
                })

                new Vue({
                    el: '#coronavirus-bar',
                    data: function() {
                        return {
                            articles: [{
                                    url: 'https://www.biobiochile.cl/noticias/nacional/chile/2020/03/27/como-han-evolucionado-los-contagios-por-coronavirus-en-cada-region-de-chile.shtml',
                                    title: 'Contagios por regiÃ³n',
                                    icon: 'icono0.png'
                                },
                                {
                                    url: 'https://www.biobiochile.cl/noticias/ciencia-y-tecnologia/ciencia/2020/03/12/las-claves-del-covid-19-lo-que-debes-saber-sobre-el-virus-que-se-convirtio-en-pandemia-global.shtml',
                                    title: 'Lo que debo saber',
                                    icon: 'icono1.png'
                                },
                                {
                                    url: 'https://www.biobiochile.cl/subsidios/',
                                    title: 'Subsidios',
                                    icon: 'icono2.png'
                                },
                                {
                                    url: 'https://www.biobiochile.cl/noticias/vida-actual/cuerpo-y-mente-sanos/2020/03/06/viajaras-en-bus-6-consejos-para-evitar-el-contagio-con-coronavirus.shtml',
                                    title: 'Evitar contagio en viajes',
                                    icon: 'icono3.png'
                                },
                                {
                                    url: 'https://www.biobiochile.cl/noticias/nacional/chile/2020/03/13/que-deben-hacer-los-trabajadores-en-caso-de-cuarentena-por-coronavirus.shtml',
                                    title: 'En caso de cuarentena',
                                    icon: 'icono4.png'
                                },
                                {
                                    url: 'https://www.biobiochile.cl/noticias/nacional/chile/2020/03/13/segun-sintomas-como-se-diferencia-el-coronavirus-de-la-gripe-comun-y-resfriado.shtml',
                                    title: 'Diferencias con gripe',
                                    icon: 'icono5.png'
                                },
                                {
                                    url: 'https://www.biobiochile.cl/noticias/vida-actual/cuerpo-y-mente-sanos/2020/03/03/conoce-los-sintomas-del-nuevo-coronavirus-covid-19.shtml',
                                    title: 'SÃ­ntomas',
                                    icon: 'icono6.png'
                                },
                            ],
                            lastUpdate: '',
                            infected: 0,
                            new_infected: 0,
                            deceased: 0,
                            new_deceased: 0,
                            recovered: 0,
                            restrictions: [],
                            fetching: false,
                            intervalId: -1
                        }
                    },
                    computed: {
                        marqueeDuration: function() {
                            return this.restrictions.reduce(function(previous, current) {
                                return previous + current.length
                            }, 0) / 3.5
                        }
                    },
                    mounted: function() {
                        this.fetch()
                        this.intervalId = setInterval(this.fetch, 45000)
                    },
                    destroyed: function() {
                        clearInterval(this.intervalId)
                    },
                    methods: {
                        fetch: function() {
                            if (!this.fetching) {
                                this.fetching = true
                                var t = this

                                Vue.axios(
                                    'https://www.biobiochile.cl/static/coronavirus-v3.json?t=' +
                                    Date.now()).then(function(data) {
                                    t.lastUpdate = data.data.actualizado
                                    t.infected = data.data.contagios_totales
                                    t.new_infected = data.data.nuevos_contagios
                                    t.restrictions = data.data.restricciones
                                    t.recovered = data.data.recuperados
                                    t.deceased = data.data.fallecidos
                                    t.new_deceased = data.data.nuevos_fallecidos
                                    t.fetching = false
                                }).catch(function() {
                                    t.fetching = false
                                })
                            }
                        },
                    },
                    filters: {
                        twoDigits: function(x) {
                            return x / 10 < 1 ? '0' + x : String(x)
                        },
                        formatNumber: function(n) {
                            return (+n).toLocaleString('en-US').replace(/,/g, '.')
                        }
                    }
                })

                new Vue({
                    el: "#covid-19-calendar",
                    data: {
                        tab: null
                    },
                    methods: {
                        tabClick(tab) {
                            this.tab = this.tab === tab ? null : tab
                        }
                    }
                })
            })
        })()

        ;
        (() => {
            var elemento = document.createElement('script');
            elemento.setAttribute('type', 'module');
            elemento.innerHTML = `  
                import { mountCbbComentarios } from 'https://comunidad-libs.biobiochile.cl/bb-components.js?v=1'
                setTimeout(() => {
                    mountCbbComentarios('#cbbHeader-1');
                    mountCbbComentarios('#cbbHeader-2');
                }, 0)`;
            document.body.appendChild(elemento);
        })();
    </script>
