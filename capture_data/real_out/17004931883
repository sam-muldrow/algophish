    <script>
        // 移动设备[不包括pad]访问pc端
        var isMobile = navigator.userAgent.match(
            /(phone|pod|iPhone|iPod|Android|BlackBerry|IEMobile|MQQBrowser|JUC|Fennec|wOSBrowser|BrowserNG|WebOS|Symbian|Windows Phone)/i,
        );
        var isMatchPage = function (page) {
            var pageUrl = page ? '/' + page : '';
            return (
                document.referrer.indexOf('m.a9vg.com' + pageUrl) === -1 &&
                document.referrer.indexOf('www.a9vg.com' + pageUrl) === -1 &&
                (page ? window.location.href.indexOf('www.a9vg.com' + pageUrl) > 0 : true) &&
                isMobile
            );
        };
        var subDomains = (function () {
            var domainNodes = window.location.hostname.split('.');
            return domainNodes.slice(0, domainNodes.length - 2).join('.');
        })();
        if (isMatchPage('article/')) {
            window.location.href = '//m.a9vg.com' + window.location.pathname;
        } else if (subDomains.indexOf('tgs') > -1 && isMatchPage()) {
            window.location.href = '//m.a9vg.com/tgs';
        } else if (subDomains.indexOf('ps5') > -1 && isMatchPage()) {
            window.location.href = '//m.a9vg.com/ps5';
        } else if (isMatchPage('nyj2022')) {
            window.location.href = '//m.a9vg.com/nyj2022';
        } else if (isMatchPage('niuyoujiang')) {
            window.location.href = '//m.a9vg.com/niuyoujiang';
        } else if (
            isMatchPage() &&
            (!isMatchPage('nyj2020') &&
                !isMatchPage('account/sign-in') &&
                !isMatchPage('account/sign-up') &&
                !isMatchPage('/account/reset-password'))
        ) {
            window.location.href = '//m.a9vg.com';
        }
    </script>

