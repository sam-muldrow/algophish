    var firstTitle = data[0].title
    $($('.news-title').get(0)).html(firstTitle)
    $('#banner_link').attr('href', '/article/'+data[0].nid)
    // document.getElementsByClassName("news-title")[0].innerHTML = data[curSwiperIndex].title;
    var mySwiper = new Swiper('.news-swiper',{
        speed:300,
        loop: true,
        autoplay : {
            delay:3000
        },
        navigation: {
            nextEl: '.swiper-button-next',
            prevEl: '.swiper-button-prev',
        },
        on:{
            slideChange: function(){
                // console.log(this.activeIndex);
                if(this.activeIndex>5) { // 数组长度5
                    curSwiperIndex = 1;
                } else {
                    curSwiperIndex = this.activeIndex;
                }
                $($('.news-title').get(0)).html(data[curSwiperIndex-1].title)
                $('#banner_link').attr('href', '/article/'+data[curSwiperIndex-1].nid)
                // document.getElementsByClassName("news-title")[0].innerHTML = 1;
            },
        },
    })

    // 游戏发售表
    var gameSales_tabs_W = $(".gameSales_tabs_separator").width()
    var cur_gameSales_tabs_W = gameSales_tabs_W / 5
    $(".gameSales_tabs_separator .vd-tabs_indicator").css({"left":"0", "width": cur_gameSales_tabs_W})
    // Tab切换
    $(".gameSales_tabs button").mouseover(gameSalesTabToggleFun)
    function gameSalesTabToggleFun(e) {
        $(this).addClass("is-selected").siblings().removeClass("is-selected");
        var index = $(this).index()
        $(".gameSales_tabs_separator .vd-tabs_indicator").css({"left": cur_gameSales_tabs_W * index, "width": cur_gameSales_tabs_W})
        $(".gameSales_tabs_wrapper").children().eq(index).addClass("is-selected").siblings().removeClass("is-selected");
    }


</script>
