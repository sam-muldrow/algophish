<script>
    var swiper = new Swiper(".mySwiper", {
        slidesPerView: 1, //2
        breakpoints: {
            "@0.00": {
                slidesPerView: 2,
                spaceBetween: 10,
            },
            "@1.00": {
                slidesPerView: 6,
                spaceBetween: 10,
            },
        },
        //direction: "vertical",
        //centeredSlides: true,
        spaceBetween: 10,
