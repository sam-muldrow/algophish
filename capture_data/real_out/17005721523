<script type="text/javascript">
    function toggleNav() {
        //var element = document.getElementById("sidebar");
        document.getElementById("sidebar").classList.toggle("hidden");
        document.getElementById("sidebar-open").classList.toggle("hidden");
        document.getElementById("sidebar-close").classList.toggle("hidden");
        window.scrollTo({ top: 0, behavior: 'smooth' });
    }

    function toggleSearch() {
        //var element = document.getElementById("sidebar");
        document.getElementById("search").classList.toggle("hidden");
        document.getElementById("logo").classList.toggle("hidden");
        //document.getElementById("nav_items").classList.toggle("hidden");
        //document.getElementById("user").classList.toggle("hidden");
        document.getElementById("nav_items").classList.toggle("md:block");
        document.getElementById("search-open").classList.toggle("hidden");
        document.getElementById("search-close").classList.toggle("hidden");
    }


</script>

<div id="ads_outofpage">
<script>
            googletag.cmd.push(function() { googletag.display('ads_outofpage'); });
        </script>
</div>

