<script type="text/javascript">
                    var vrsta = "najnovije";

                    $( document ).on( 'click', '.najbox', function () {
                        var btn = $(this);

                        $.ajax({
                            url: '/loadmorenews',
                            type: 'POST',
                            data: {
                                endpoint: "najbox",
                                vrsta: btn.data("vrsta")
                            },
                            success: function(response){
                                //console.log(response);
                                if(response){
                                    //btn.hide();
                                    $(".najbox").removeClass("border-gray-700");
                                    btn.addClass("border-gray-700");
                                    $(".najbox_display").html(response);
                                } else {
                                    alert("error");
                                }
                            }
                        });
                    });

                </script>
