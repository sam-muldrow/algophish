    (function() {
        var model = {"context":{"currency":"GBP","site":"UK","country":"GB","language":"en-gb","environment":"Production"},"data":[{"type":"home"}]};
        window.tcpl = window.tcpl || {};
        window.tcpl.datalayer = model;
        document.addEventListener('DOMContentLoaded',function(){
            if(window.riDigitalData && Array.isArray(model.data) && model.data.length > 0) {
                var filteredEventNames = ['landing-page', 'content-page', 'search'];
                var isValidEvent = true;
                var type = model.data[0].type;

                for(var i = 0; i < filteredEventNames.length; i+= 1){
                    if(filteredEventNames[i] === type){
                        isValidEvent = false;
                    }
                }

                if(isValidEvent){
                    window.riDigitalData.add(type, model);
                }
                
            }
        });
    })(window);
</script>

