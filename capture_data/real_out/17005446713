    <link rel="preload" href="https://content1.riverisland.com/Assets/RiverIsland/Core/fonts/sang-bleu/SangBleuOGSans-Regular.woff2" as="font" crossorigin>

    
<script type="text/javascript">
            if (!isABot() && (!hasLandedOnHomePageWithinSession())) {
                var winningGender = mostViewedGender();
                if (winningGender != null) {
                    if (["men", "women"].indexOf(winningGender) != -1) {
                        // only redirect if men or women category

                        window.location.href = "/" + winningGender + window.location.search;
                    }
                }
            }

            function isABot() {
                if (/bot|google|aolbuild|baidu|bing|msn|duckduckgo|teoma|slurp|yandex/i
                    .test(navigator.userAgent)) {
                    return true;
                }
                return false;
            }

            function hasLandedOnHomePageWithinSession() {
                if (typeof (Storage) !== "undefined") {
                    var hasredirectThisSession = sessionStorage.getItem("tcpl_hp_hit");
                    if (hasredirectThisSession == null) {
                        sessionStorage.setItem("tcpl_hp_hit", 1);
                        return false;
                    }
                }
                // assume already has
                return true;

            };

            function mostViewedGender() {
                if (typeof (Storage) !== "undefined") {
                    var genderTracking = localStorage.getItem("tcpl-ri-gender-stats");
                    if (typeof genderTracking !== 'undefined' && genderTracking != null) {
                        var obj = JSON.parse(genderTracking);

                        // Create kidswear property (combining boys + girls) if it doesn't exist
                        if (!obj.hasOwnProperty("kidswear")) {
                            var kidswearCount = 0;
                            var kidswearLastUpdate = 0;
                            $.each(["boys", "girls"], function(kidsGender) {
                                if (obj.hasOwnProperty(kidsGender)) {
                                    kidswearCount += obj[kidsGender].count;
                                    kidswearLastUpdate = Math.max(kidswearLastUpdate, obj[kidsGender].lastUpdate);
                                }
                            });
                            obj.kidswear = { count: kidswearCount, lastUpdate: kidswearLastUpdate };
                        }

                        var sortable = [];
                        for (var prop in obj) {
                            if (obj.hasOwnProperty(prop)) {
                                sortable.push({
                                    'key': prop,
                                    'value': obj[prop]
                                });
                            }
                        }
                        if (sortable.length < 1) return null;
                        sortable.sort(function(a, b) {
                            var countA = parseInt(a.value.count);
                            var countB = parseInt(b.value.count);
                            if (countA === countB) {
                                var updatedA = a.value.lastUpdate;
                                var updatedB = b.value.lastUpdate;

                                return (updatedB < updatedA) ? -1 : (updatedB > updatedA) ? 1 : 0;
                            } else {
                                return (countB < countA) ? -1 : (countB > countA) ? 1 : 0;
                            }
                        });
                        var firstElement = sortable[0];
                        if (firstElement.value.count > 0) return firstElement.key;
                        return null;
                    }
                }
                return null;
            }

</script>


