function SearchSubmit(){
	var goto = $("#goto").val();
	//goto = "";
	value = $("#keywords").val();
	value = value.replace(/^\s+|\s+$/g,"");
	if(value == "") { 
		//window.open('//www.lyd.com.cn/search/','search');
		alert("ËÑË÷ÄÚÈÝ£¬²»ÄÜÎª¿Õ"); 
		return false; 
	}else{
		value = $("#keywords").val();
		//window.open('//www.lyd.com.cn/search/?q='+encodeURI(value)+'&goto='+goto,'search');
		//window.open('http://s.lyd.com.cn/cse/search?s=780316685311236820&stp=1&nsid=0&q='+encodeURI(value)+'&goto='+goto,'search');
		window.open('http://s.lyd.com.cn/cse/search?q='+encodeURI(value)+'&s=780316685311236820&nsid=1&','search');
	}
}
</script>
