  $(document).ready(function(){
 $('.son_ul').hide(); //³õÊ¼ulÒþ²Ø
 $('#SI_Search_Type_Hack h3').hover(function(){ //Êó±êÒÆ¶¯º¯Êý
 $(this).parent().find('ul.son_ul').show();  //ÕÒµ½ul.son_ulÏÔÊ¾
 $(this).parent().find('ul.son_ul li').hover(function(){$(this).addClass('lihover')},function(){$(this).removeClass('lihover')}); //liµÄhoverÐ§¹û
 $(this).parent().hover(function(){},
 function(){
 $('.son_ul').hide();
 }
 );

    },function(){}

 );
 $('ul.son_ul li').click(function(){
