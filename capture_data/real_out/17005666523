function check(f){
    //
    location.href='//bbs.lyd.com.cn/member.php?mod=logging&action=login';
    return true;

    if(f.username.value == ""){
        alert("ÇëÌîÐ´ÓÃ»§Ãû!");

        f.username.focus();
        return false;
    }
    if(f.password.value == ""){
        alert("ÇëÌîÐ´ÃÜÂë!");
        f.username.focus();
        return false;
    }

	f.action="http://passport.lyd.com.cn/?action=login";


    f.submit();
	f.username.value = ""
	f.password.value = ""
}
