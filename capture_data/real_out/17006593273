<script>

    function generateGraph(sec_id, sec_slug, sec_name, sec_link, allow_intraday, container) {

        console.log(sec_id, sec_slug, sec_name, sec_link, allow_intraday, container);
        var elem = $('#secid_' + sec_id);
        var icon = "round";
        var key = elem.attr('tab-key');
        var sec_code = elem.attr('sec-code');
        if (parseFloat(elem.attr('price-change')) > 0) icon = "up";
        else if (parseFloat(elem.attr('price-change')) < 0) icon = "down";

        $('#chart_container_top_' + key).attr("href", sec_link);
        $('#chart_container_top_link_' + key).attr("href", sec_link);
        $('#chart_container_top_' + key).removeClass();
        $('#chart_container_top_' + key).addClass("data-icon");
        $('#chart_container_top_' + key).addClass(icon);
        $('#chart_container_top_' + key).addClass("live-"+sec_slug);


        $('#chart_container_top_' + key + ' > #son_fiyat_' + key).html(elem.attr('last-price'));
        $('#chart_container_top_' + key + ' > #yuzde_degisim_' + key).html(elem.attr('percent-change'));

        $('#chart_container_top_' + key + ' > .icon' + key).attr('data-secid', sec_code);
        $('#chart_container_top_' + key + ' > #son_fiyat_' + key).attr('data-secid', sec_code);
        $('#chart_container_top_' + key + ' > #yuzde_degisim_' + key).attr('data-secid', sec_code);


        var highChartsOptions = [];
        highChartsOptions.push({
            sec_id: sec_id,
            sec_slug: sec_slug,
            sec_name: sec_name,
            sec_datatype: allow_intraday,
            credits: {
                enabled: false
            },
            exporting: {
                enabled: false
            },
            rangeSelector: {
                selected: 1,
                inputEnabled: false,
                buttonTheme: {
                    visibility: 'hidden'
                },
                labelStyle: {
                    visibility: 'hidden'
                }
            },
            navigator: {
                enabled: false
            },
            scrollbar: {
                enabled: false
            },
            "xAxis": {
                "type": "datetime"
            }
        });
        $.each(highChartsOptions, function (key, option) {
            option.series = [{
                type: 'area',
                gapSize: 5,
                fillColor: {
                    linearGradient: {
                        x1: 0,
                        y1: 0,
                        x2: 0,
                        y2: 1
                    },
                    stops: [
                        [0, Highcharts.getOptions().colors[0]],
                        [1, Highcharts.Color(Highcharts.getOptions().colors[0]).setOpacity(0).get('rgba')]
                    ]
                },
                threshold: null,
                "index": 0,
                "name": option.sec_name
            }];
            var _dataTypeUrl;
            if (option.sec_datatype && option.sec_datatype == "intraday") {
                _dataTypeUrl = "intradaydata";
            } else {
                _dataTypeUrl = "refdata";
            }

            var _dataContainerId;
            _dataContainerId = "#chartContainer_" + container;

            $(_dataContainerId).html('');
            $.getJSON('/piyasa/' + _dataTypeUrl + '/' + option.sec_slug, function (data) {
                option.series[0].data = data.SeriesData;
                $(_dataContainerId).highcharts("StockChart", option);
                $('.tab-content-bottom-list').css('background-color', '#fff');
                $('#secid_' + sec_id).css('background-color', '#ccc');
            });
        });
    }

    head.ready("layout", function () {
        generateGraph(sec_id, sec_slug, sec_name, sec_link, allow_intraday, 'borsa');
        generateGraph(sec_id, sec_slug, sec_name, sec_link, allow_intraday, 'bitcoin');
    });
</script>
</div>                        <!--Economy Verileri Tabs-->

                    </div>
                </div>
            </div>
        </div>

    </div>
</section>

<script>
    // Main Slider
    head.ready("layout", function() {
        var swiper = new Swiper('.widget-main-slider.type1 .slider', {
            slidesPerView: 1,
            spaceBetween: 10,
            loop: true,
            navigation: {
                nextEl: '.widget-main-slider.type1 .swiper-button-next',
                prevEl: '.widget-main-slider.type1 .swiper-button-prev',
            },
            speed: 1000,
            autoplay: {
                delay: 11000,
                disableOnInteraction: false
            },
            lazy: {
                loadPrevNext: true,
                loadPrevNextAmount: 2
            },
            on: {
                init: function() {
                    var swiper = this;
                    var slideHover = function() {
                        if (!swiper.pagination.bullets || swiper.pagination.bullets.length === 0) {
                            return setTimeout(slideHover, 200);
                        }
                        $(swiper.pagination.bullets).on("mouseover", function() {
                            swiper.slideTo($(this).attr("data-id"));
                        });
                    };
                    setTimeout(slideHover, 50);
                },
            },

            pagination: {
                el: '.widget-main-slider .swiper-pagination',
                renderBullet: function(index, className) {
                    var realIndex = index;
                    if (this.params.loop) {
                        realIndex = index + 1;
                        realIndex = realIndex > (this.slides.length - 3) ?
                            0 : realIndex;
                    }
                    var slideLink = this.slides[realIndex].querySelector("a");
                    var buttonHtml = '<span data-id="' + (index + 1) + '" class="' + className + '">' + (index + 1) + '</span>';

                    if (slideLink) {
                        buttonHtml = '<a href="' +
                            slideLink.getAttribute("href") + '" target="' +
                            slideLink.getAttribute("target") + '">' + buttonHtml + "</a>";
                    }
                    return buttonHtml;
                },
            }

        });

        var swiper = new Swiper('.video-slider.type1 .slider', {
            slidesPerView: 1,
            spaceBetween: 10,
            pagination: {
                el: '.swiper-pagination',
                clickable: true,
            },
            navigation: {
                nextEl: '.video-slider.type1 .swiper-button-next',
                prevEl: '.video-slider.type1 .swiper-button-prev',
            },
        });
        $('.video-slider.type1 .swiper-pagination-bullet').hover(function() {
            $(this).trigger("click");
        });

        var swiper = new Swiper('.gallery-slider.type1 .slider', {
            slidesPerView: 1,
            spaceBetween: 10,
            pagination: {
                el: '.swiper-pagination',
                clickable: true,
            },
            navigation: {
                nextEl: '.gallery-slider.type1 .swiper-button-next',
                prevEl: '.gallery-slider.type1 .swiper-button-prev',
            },
        });
        $('.gallery-slider.type1 .swiper-pagination-bullet').hover(function() {
            $(this).trigger("click");
        });

        var swiper = new Swiper('.program-slider.type1 .slider', {
            slidesPerView: 1,
            spaceBetween: 10,
            pagination: {
                el: '.swiper-pagination',
                clickable: true,
            },
            navigation: {
                nextEl: '.program-slider.type1 .swiper-button-next',
                prevEl: '.program-slider.type1 .swiper-button-prev',
            },
        });
        $('.program-slider.type1 .swiper-pagination-bullet').hover(function() {
            $(this).trigger("click");
        });

    });
</script><script type="text/javascript">
    function initMansetAltBlok() {
        if (typeof head != 'undefined') {
            head.ready("layout", function() {
                var swiper = new Swiper('.widget-slider-full.type1 .slider', {
                    slidesPerView: 2,
                    spaceBetween: 10,
                    freeMode: true,
                    lazy:true,
                    navigation: {
                        nextEl: '.widget-slider-full.type1 .swiper-button-next',
                        prevEl: '.widget-slider-full.type1 .swiper-button-prev',
                    },
                });
            });
        }else{
            var swiper = new Swiper('.widget-slider-full.type1 .slider', {
                slidesPerView: 5,
                spaceBetween: 10,
                freeMode: true,
                lazy:true,
                navigation: {
                    nextEl: '.widget-slider-full.type1 .swiper-button-next',
                    prevEl: '.widget-slider-full.type1 .swiper-button-prev',
                },
            });
        }
    }
</script>


