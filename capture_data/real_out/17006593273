        function initDailySwiper() {
            var tvSliderStartPosition = 0;
            if (typeof head != 'undefined') {
                head.ready("layout", function() {
                    var swiper = new Swiper('.widget-slider-full.type2 .slider', {
                        slidesPerView: 4,
                        spaceBetween: 10,
                        freeMode: true,
                        lazy:true,
                        navigation: {
                            nextEl: '.widget-slider-full.type2 .swiper-button-next',
                            prevEl: '.widget-slider-full.type2 .swiper-button-prev',
                        },
                    });
                });
            }else{
                var swiper = new Swiper('.widget-slider-full.type2 .slider', {
                    slidesPerView: 4,
