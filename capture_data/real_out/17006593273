            var miniEconomyData = function () {
                var items = ['bist-100', 'dolar', 'euro', 'altin-ons'];
                var source = document.querySelector('.widget-economy-data');


                if (source) {
                    var view = document.querySelector('.widget-mini-economy-data');
                    if (view) {
                        for (var i = 0, length = items.length; i < length; i++) {
                            var sourceEl = source.querySelector('#' + items[i]);
                            if (sourceEl) {
                                var viewEl = view.querySelector("[data-id='" + items[i] + "']");
                                var url = sourceEl.getAttribute('href');
                                var name = sourceEl.querySelector('.title').innerText;
                                var value = sourceEl.querySelector('.value').innerText;
                                var percent = sourceEl.querySelector('.value-diff').innerText;
                                var status = sourceEl.querySelector('.data-icon').getAttribute('data-stat');
                                if (viewEl) {
                                    viewEl.querySelector('.name').innerText = name;
                                    viewEl.querySelector('.value').innerText = value;
                                    viewEl.querySelector('.percent').innerText = percent;
                                    viewEl.className = status;
                                    viewEl.setAttribute('href', url);
                                }
                            }
                        }
                    }
                }
            }
            miniEconomyData();

            function socketStart() {
                var colorMapping = {
                    "-1": "downRed",
                    0: "arrowZero",
                    1: "upGreen"
                };
                var classMapping = {
                    "-1": "down",
                    0: "round",
                    1: "up"
                };
                var removeChangeClassTimeouts = {};

                var finansData = function (data) {
                    var $secSlug = $(".live-" + data.SecuritySlug);
                    if ($secSlug.length > 0) {

                        var $lastPriceContainer = $secSlug.find(".LastPrice");
                        var $iconContainer = $secSlug.find(".data-icon");

                        if (data.LastPrice != $lastPriceContainer.text()) {


                            $secSlug.removeClass("up down round");
                            $secSlug.addClass(classMapping[data.ChangeDirection]);
                            $secSlug.addClass("change");

                            $iconContainer.removeClass("up down round");
                            $iconContainer.addClass(classMapping[data.ChangeDirection]);
                            $iconContainer.addClass("change");
                            if(document.querySelector($iconContainer.selector)){
                                $iconContainer[0].setAttribute('data-stat',classMapping[data.ChangeDirection]);
                            }

                            $lastPriceContainer.html(data.LastPrice);
                            $bidContainer = $secSlug.find("." + data.SecuritySlug + "-bid");
                            if ($bidContainer.text() != data.Bid) {
                                $bidContainer.html(data.Bid);
                                $bidContainer.addClass("change");
                            }

                            $askContainer = $secSlug.find("." + data.SecuritySlug + "-ask");
                            if ($askContainer.text() != data.Ask) {
                                $askContainer.html(data.Ask);
                                $askContainer.addClass("change");
                            }

                            if (typeof removeChangeClassTimeouts[data.SecuritySlug] === "undefined") {
                                removeChangeClassTimeouts[data.SecuritySlug] = setTimeout(function () {
                                    $secSlug.removeClass("change");
                                    delete removeChangeClassTimeouts[data.SecuritySlug];
                                }, 1000);
                            }

                            $secSlug.find(".PercentChange").html((data.PercentChange == "%-0,00" || data.PercentChange == "%0,00") ? "% 0" : data.PercentChange);
                            $secSlug.removeClass("upGreen downRed arrowZero");
                            $secSlug.addClass(colorMapping[data.ChangeDirection]);

                            $secSlug.find(".PriceChange").html(data.PriceChange);
                        }
                        miniEconomyData();
                    }
                };

                WebsocketConn.init({
                    websocketUrl: "wss://nc.ciner.com.tr/sub/dot",
                    reconnectInterval: 1000
                }, function (item) {
                    finansData(item);
                });


            }

            if (typeof NchanConn !== "undefined") {
                socketStart();
            } else if (typeof socketIoLoading !== "undefined") {
                head.load(socketIoLoading, socketStart);
            }


        };
	</script>


