<script>
    head.ready("layout", function () {

        $(document).on("click", ".copy-text", function (event) {
            var $tempElement = $("<input>");
            $("body").append($tempElement);
            $tempElement.val($(this).parent().find("span").text()).select();
            document.execCommand("Copy");
            $tempElement.remove();
        });


    });
</script>


