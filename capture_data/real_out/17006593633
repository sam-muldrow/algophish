<script type="text/javascript" src="/_Incapsula_Resource?SWJIYLWA=719d34d31c8e3a6e6fffd425f7e032f3&ns=1&cb=447263099" async></script></body>
<script language="JavaScript" type="text/javascript">
	var subjects = new Array()
	subjects[0] ="languageArt";
	subjects[1] ="math";
	subjects[2] ="scienceHealth";
	subjects[3] ="socialStudies";
	subjects[4] ="worldLanguages";
	subjects[5] ="otherHolt";
	
	var tabView = new YAHOO.widget.TabView('tabview');
	
	var languageArtGroup = new YAHOO.util.ImageLoader.group('languageArtMenu', 'click');	
	var mathGroup = new YAHOO.util.ImageLoader.group('mathMenu', 'click');
	var scienceHealthGroup = new YAHOO.util.ImageLoader.group('scienceHealthMenu', 'click');
	var socialStudiesGroup = new YAHOO.util.ImageLoader.group('socialStudiesMenu', 'click');
	var worldLanguagesGroup = new YAHOO.util.ImageLoader.group('worldLanguagesMenu', 'click');
	var otherHoltGroup = new YAHOO.util.ImageLoader.group('otherHoltMenu', 'click'); 
	
	var hash = document.location.hash;
	var tabIdx = -1;

	for(var i=0; i<subjects.length;i++) {

		if (hash && (hash == '#' + subjects[i]))
			tabIdx = i;
		
		//alert("Processing Subject: " + subjects[i]);
		var hidden_name = subjects[i] + "_hidden";
		var hidden = document.getElementsByName(hidden_name);
		for(var j=0;j<hidden.length ;j++) {
		
			var image_id = subjects[i] + "_img" + j;
			//alert("loop no: " + j + " src id : " + image_id + " /n image location: "  + hidden[j].value);
			switch(i) {
				
				case 0: languageArtGroup.registerSrcImage(image_id, hidden[j].value);
						languageArtGroup.addTrigger('languageArtMenu', 'focus'); 
						break;
				case 1: mathGroup.registerSrcImage(image_id, hidden[j].value);
						mathGroup.addTrigger('mathMenu', 'focus'); 
						break;
				case 2: scienceHealthGroup.registerSrcImage(image_id, hidden[j].value);
						scienceHealthGroup.addTrigger('scienceHealthMenu', 'focus'); 
						break;
				case 3: socialStudiesGroup.registerSrcImage(image_id, hidden[j].value);
						socialStudiesGroup.addTrigger('socialStudiesMenu', 'focus'); 
						break;
				case 4: worldLanguagesGroup.registerSrcImage(image_id, hidden[j].value);
						worldLanguagesGroup.addTrigger('worldLanguagesMenu', 'focus'); 
						break;
				case 5: otherHoltGroup.registerSrcImage(image_id, hidden[j].value);
						otherHoltGroup.addTrigger('otherHoltMenu', 'focus'); 
						break;			
			}
		}	
	}

	if (tabIdx >= 0) {
		hideDefaultPanel();
		tabView.selectTab(tabIdx);
		switch(tabIdx) {		
			case 0: languageArtGroup.fetch();
					break;
			case 1: mathGroup.fetch();
					break;
			case 2: scienceHealthGroup.fetch();
					break;
			case 3: socialStudiesGroup.fetch();
					break;
			case 4: worldLanguagesGroup.fetch();
					break;
			case 5: otherHoltGroup.fetch();
					break;
		}
	}

	function storeAnchor() {
		var href= tabView.get('activeTab').get('href');
		document.location = href;
		return true;	
	}

</script>

</html>