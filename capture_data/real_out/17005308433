<script>
        window.config = {
            endpoints: {
                reseller : '',
                productInfo : '',
                ecommerce : ''
            },
            resource: {
                clearAll: 'Clear All',
                compareSticky: 'Compare',
                compareSameCategory: 'Please compare products from the same category',
                only3Products: 'Only 3 products are allowed to be compared at time.',
                select2Compare: 'Please select at least two products to compare'
            }
        };

        $(function () {
            let firstName = getCookie("_agw-prdfn")
            let name = getCookie("_agw-prdn")
            let token = getCookie("_agw-prdrt")

            if (token) {
                document.querySelector(".menu--account-login").classList.remove("bs5-d-none")
                document.querySelectorAll(".menu--account-mobile-login").forEach(function (item) {
                    item.classList.remove("bs5-d-none")
                })
            }
            else {
                document.querySelector(".menu--account-not-login").classList.remove("bs5-d-none")
                document.querySelectorAll(".menu--account-mobile-not-login").forEach(function (item) {
                    item.classList.remove("bs5-d-none")
                })

                
            }
        })
</script>
