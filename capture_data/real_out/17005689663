    });*/

    	$(document).on('click', '.buy-from-shop', function() {
		var sku = $(this).closest('.product-wrapper').data('modelnumber');
		if (sku) {
			var uri = 'https://ux.mediamarkt.com.tr/store-delivery/index.html?id=' + sku;
			var clsIcon = '<svg class="stock-cls-icn" width="32px" height="32px" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><g data-name="Layer 2"><g data-name="close"><rect width="24" height="24" transform="rotate(180 12 12)" opacity="0"></rect><path d="M13.41 12l4.3-4.29a1 1 0 1 0-1.42-1.42L12 10.59l-4.29-4.3a1 1 0 0 0-1.42 1.42l4.3 4.29-4.3 4.29a1 1 0 0 0 0 1.42 1 1 0 0 0 1.42 0l4.29-4.3 4.29 4.3a1 1 0 0 0 1.42 0 1 1 0 0 0 0-1.42z"></path></g></g></svg>';
			var iframe = ' <div id="buy-frm-shp" class="stock-mdl"><div>' + clsIcon +'<iframe src="' + uri +'" title="Stok Durumu Görüntüle"></iframe></div></div>';
			if(!$('#buy-frm-shp').length) {
				$('body').append(iframe);
			} else {
				$('#buy-frm-shp').show();
			}
			setTimeout( () => {
				$('#cboxOverlay').hide();
			},100);
		}
	});

	$('.box.infobox.availability li').each(function(i,v) {
			if($(v).html().indexOf('Stoktan Gönderi') !== -1) {
				$(v).remove();
			}
		});

    setInterval(function () {
        $('ul.products-list li:not(".with-contenspot")').each(function (i, v) {
            var pName = $(v).find('h2 > a').html();
            if (pName && pName.toLocaleLowerCase().indexOf('outlet') != -1) {
                if (!$(v).find('.outlet-urun').length) {
                    $(v).find('.photo').append('<div class="outlet-urun">OUTLET<span>ÜRÜN</span></div>');
                }
            }
           // if (!$(v).find('.stock-info').length) {
           //    $(v).find('.box.infobox.availability ul').append('<li class="stock-info"><a href="#" class="stok-bilgisi">Mağaza Stok Durumu</a></li>');
           // }

            if (!$(v).find('.buy-from-shop').length) {
               $(v).find('.box.infobox.availability ul').append('<li class="buy-from-shop"><a>Mağazadan Teslim Al</a></li>');
            }
        
        });
    }, 1500);
</script>


