    $(document).on('click', '.stock-cls-icn', function () {
        $('.stock-mdl').hide();
    });

    $(document).on('click', '.stok-bilgisi', function () {
        var sku = $(this).closest('.product-wrapper').data('modelnumber');
        if (sku) {
            var uri = 'https://ux.mediamarkt.com.tr/stok-takip/index.html?id=' + sku;
            $('.stock-mdl iframe').attr('src', uri);
            $('.stock-mdl').show();
