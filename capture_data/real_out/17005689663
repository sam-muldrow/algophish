<script>
/* E-Invoice Email Newsletter Registration Landing */
/* Requires: jQuery & getUrlVars */

function getUrlVars()
  {
      var vars = [], hash;
      var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
   
      for(var i = 0; i < hashes.length; i++)
      {
          hash = hashes[i].split('=');
      hash[1] = unescape(hash[1]);
      vars.push(hash[0]);
          vars[hash[0]] = hash[1];
      }
   
      return vars;
  }
  var urlVars = getUrlVars();

if(getUrlVars()['nlE'])
{
	

	jQuery('input[name=newsletterFirstName]').val(decodeURI(getUrlVars()['lFN'])); 
	jQuery('input[name=newsletterLastName]').val(decodeURI(getUrlVars()['nlLN'])); 
	jQuery('input[name=newsletterEmail]').val(decodeURI(getUrlVars()['nlE']));

}
</script>

