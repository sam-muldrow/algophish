            url: '/search/videos//ajax',
            type: "POST",
            data: {
                isAjax: true
                            },
            dataType: "json",
            beforeSend: function(){
                _overlay.fadeIn();
                _loading_image.fadeIn();
            },
            success: function(data){
                if ($.trim(data) != '') {
                    _container.html(data.videos);
                    _pagination_container.html(data.paginator);
                    $("img[id*='rotate_']").mouseover(function(event) {
                        rotateVideoOn(event.target);
                    }).mouseout(function(event) {
                        rotateVideoOff(event.target);
                    });
                    $("img[id*='rotatehd_']").mouseover(function(event) {
                        rotateHdOn(event.target);
                    }).mouseout(function(event) {
                        rotateHdOff(event.target);
                    });
                    _overlay.fadeOut();
                    _loading_image.fadeOut();
                }
            }
        });
    }
