    function countSidebarCategories() {
        cat = $('.search_filter_sidebar__ch_div input:checked');
        var x = '';
        for (var i = 0; i < cat.length; i++) {
            if ($(cat[i]).val() != '')
                x = x + $(cat[i]).val() + '.';
        }
        $('#search_filter_sidebar__channels').val(x);
    }

    function checkSidebarChChecked() {

        $(".searchCat").each(function () {
            var len = $(this).find('input:checked').length;
            if (len > 1) {
                $("#search_filter_sidebar__ch_input_" + $(this).attr('_name')).attr('selected', 'selected');
            } else {
                $("#search_filter_sidebar__ch_input_" + $(this).attr('_name')).removeAttr('checked');
            }
        });
        countSidebarCategories();
    }


    $(window).scroll(function () {
        if (!$('#search_results').length) return false;
        var scrollTop = $(document).scrollTop(), 
            scrollMin = false;
        if (!scrollMin) scrollMin = $('#search_results').offset().top;
        var delta = scrollTop - scrollMin;
        if (delta<0) delta = 0;
        $('#ajaxLoaderImg').css({
            'top':delta+'px'
        });
    });


    function getAjaxSearch(){

        var _overlay = $('#search_results_overlay'),
            _container = $('#search_results'),
            _pagination_container = $('#search_results_paginator'),
            _loading_image = $('#ajaxLoaderImg');
   
        $.cookie('search_filter_new', $('#search_filter_sidebar__form').serialize(), { expires: 7, path: '/', domain: '.nuvid.com'});

        sort = $('#search_filter_sidebar__form input[name="sort"]').val();
        hq = $('#search_filter_sidebar__form input[name="hq"]').val();

        h1_title = 'New ';
        if (sort == 'rt') {
            h1_title = 'Top Rated ';
        }

        if (hq == 1) {
            h1_title += 'HD'            
        }

        h1_title += ' Porno Videos'

        $('.videos_title').text(h1_title);

