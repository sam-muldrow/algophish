  <script>
    googletag.cmd.push(function() { googletag.display('l-nat'); });

    function renderCriteo (json) {

      var div = document.getElementById('native');
      var htmlcode = '';
      var products_count = 1;
      htmlcode += '<div class="cto_mention">Реклама</div>';
      htmlcode += '<div class="cto_privacy">';
      htmlcode += '<a href="'+ json.privacy.optout_click_url +'" target="_blank"><img src="'+ json.privacy.optout_image_url + '"></a>';
      htmlcode += '</div>';
      htmlcode += '<div class="cto_products">';
      for (var i = 0; i < json.products.length; i++) {
        if (i >= products_count) break;

        htmlcode += '<div class="image">';
        htmlcode += '<a class="thumb-wrap thumb-small-1" href="' + json.products[i].click_url + '" target="_blank">';

        htmlcode += '<img alt="' + json.products[i].title + '" src="'+ json.products[i].image.url +'" class="lazy" style="display: inline;"></a></div>';
        htmlcode += '<a href="' + json.products[i].click_url + '" class="h3">' + json.products[i].title + '</a>';
        htmlcode += '<p>'+ json.products[i].description +'</p>';
        htmlcode += '<div class="cto_product_cta"><a href="' + json.products[i].click_url + '" class="cto_product_cta">' + json.products[i].call_to_action + '</a></div>';
        htmlcode += '<div class="cto_advertiser"><a href="' + json.advertiser.logo_click_url + '" target="_blank">' + json.advertiser.domain +'</a></div>';
      }
      htmlcode += '</div>';
      for (var i = 0; i < json.impression_pixels.length; i++) {
        if (i > json.impression_pixels.length) break;
        htmlcode += '<img src="' + json.impression_pixels[i].url + '" width="0px" height="0px" style="display:none" >';
      }
      div.innerHTML = htmlcode;
      div.style.display = 'block';

    }
  </script>

</div>

 
