	$(document).ready(function() {

		$('.feat-slider').tabs({details: '.face', items: '.item', dtid: 'news-slider-detail-', itid: 'news-slider-item-'});
		$('.feat-slider').sliderRubber({next: '.controls .next', prev: '.controls .prev', overflow: '.slides', slider: '.overflow', displacement: 5, items: '.item'});

		if ( $('.foto-block').length ) {
			$('.foto-block').sliderRubber({next: '.controls .next', prev: '.controls .prev', overflow: '.overflow', slider: '.foto', items: '.image'});
			if($(window).width() > 1230){
				$('.foto-block').sliderRubber('setDis',2);
			} else {
				$('.foto-block').sliderRubber('setDis',1);
			}
		}

		if ( $('.video-block').length ) {
			$('.video-block').sliderRubber({next: '.controls .next', prev: '.controls .prev', overflow: '.overflow', slider: '.foto', items: '.image'});
			if($(window).width() > 1230){
				$('.video-block').sliderRubber('setDis',2);
			} else {
				$('.video-block').sliderRubber('setDis',1);
			}
		}

		$('.fresh-press-main .item').css('width',$('.fresh-press').width()/2);
		$('.fresh-press-main').sliderRubber({next: '.controls .next', prev: '.controls .prev', overflow: '.overflow', slider: '.slider', displacement: 2, items: '.item'});

	});
</script>
