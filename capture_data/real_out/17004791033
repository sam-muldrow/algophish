  <script>
    function mobileCustomModalsBackDrop() {
        // Give modal backdrop an extra class to make it customizable
        $('.promotional-modal').on('show.bs.modal', function(e) {
            setTimeout(function() {
                $('.modal-backdrop').addClass('modal-backdrop-promotional-custom');
            });
        });

        // the custom backdrop class should be removed once in breakpoint-up(md)
        $(window).on('resize', function() {
            // remove custom backdrop class
            if ($(this).width() >= 768) {
                $('.modal-backdrop-promotional-custom').remove();
            }
        });
    }
</script>
<script>
    function getUserAgentType() {
        var userAgent = navigator.userAgent.toLowerCase();
        var userAgentType = "";

        if (userAgent) {
            if (userAgent.match(/(ipod|iphone|ipad)/)) {
                userAgentType = "appleMobile"; // apple devices
            } else if (userAgent.match(/android/)) {

                if (BobeDetectHMSDevices.checkHMSDevices()) { // Huawei device
                    userAgentType = "huaweiMobile";
                } else {
                    userAgentType = "androidMobile";  // other Android devices
                }
            }
        }
        return userAgentType;
    }
</script>

<script>
    var heightWindow;
    var heightDocument;
    var heightScrollable;

    function showAskOnceDailyDealsNewsletterSubscribeModal() {
        var cookieName = 'dotw_popup';
        var cookieTimeout = new Date();
        cookieTimeout.setMonth(cookieTimeout.getMonth() + 1);
        $('#dotw-newsletters-popup').load('/mobilejquery/jsp/newsletters/NewsletterSubscribeAskOnceDailyDealsDisplayModalAJAXHandler.jsp', function(response, status, xhr) {
            if (status === 'success' && response) {
                $('#dotw-newsletters-popup').modal('show');
                $('#dotw-newsletters-popup').on('hide.bs.modal', function(e) {
                    $('#dotw-newsletters-popup').off('hide.bs.modal');
                });
            }
            Cookies.set(cookieName, 'true', {
                path: '/',
                domain: 'bobshop.co.za',
                expires: cookieTimeout
            });
        });
    }

    function showAfterScrollAskOnceDailyDealsNewsletterSubscribeModal() {
        if (getPercentageScrolled() > 25) {
            showAskOnceDailyDealsNewsletterSubscribeModal();
            $(window).off(".dotwpopup");
        }
    }

    function getScrollableHeight() {
        heightWindow = $(window).height();
        heightDocument = $(document).height();
        heightScrollable = heightDocument - heightWindow;
    }

    function getPercentageScrolled() {
        var scrollTop = $(window).scrollTop();
        return Math.floor(scrollTop / heightScrollable * 100);
    }
</script>
