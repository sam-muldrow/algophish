  function trackEvent(category, action, label, value) {
    if(typeof value === 'undefined' || value === null) {
      value = 0;
    }
    if(typeof __gaTracker !== "undefined") {
      __gaTracker('send', {
          hitType: 'event',
          eventCategory: category,
          eventAction: action,
          eventLabel: label,
          eventValue: value
      });
    }
  }
