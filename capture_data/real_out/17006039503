<script>

jQuery(document).ready(function () {

    var _logDownload = function (platform, emailAddress, callback) {
        var downloadType = 'trial';
        var ajaxData = {
            email: emailAddress,
            canEmail: true,
            platform: platform,
            downloadType: downloadType
        };
        $.ajax({
            type: 'GET',
            url: 'https://a.axure.com/log/download',
            //url: 'https://cart.axure.com/LogDownload.aspx',
            data: ajaxData,
            success: function (response) {},
            error: function (response) {},
            dataType: 'jsonp'
        });

        callback();
    };

    function attachDlClick() {
        var btn = $('.nf-form-content');
        if (btn.length == 0) {
            window.setTimeout(attachDlClick, 500);
        } else {

if ($('#nf-form-19-cont').length > 0) return;
// if ($('#nf-form-10_2-cont').length > 0) return;
            btn.on('click', function (event) {
                var $target = $(event.target); 
                if(!($target.hasClass('download-hero__form-btn') || $target.hasClass('btn_download'))) return; 
                var formContent = $(this).closest('.nf-form-content');
                var osList = formContent.find('.js-os-select');
                var OS_MAC;
                if (osList.length == 0) {
                    var macRegex = /Mac/g;
                    OS_MAC = Boolean(macRegex.exec(window.navigator.platform));
                } else {
                    OS_MAC = osList.children('option:selected').val().indexOf('mac') > -1 ? true : false;
                }
                var os = OS_MAC ? 'mac' : 'pc';

                var emailAddress = formContent.find('input[name="email"]').val();
                if (emailAddress.toUpperCase().indexOf('optional') > -1) emailAddress = '';

                _logDownload(os.toLowerCase(), emailAddress, function (result) {
                    location.href = '/downloadthanks?platform=' + os.toLowerCase();
                });
            })
        }
    }
    attachDlClick();
});


    </script>



    


                                                                    
                            
                            
                            
                                                                    
                                                                                        
                    
