  if (e.detail.value == 0.25) {
    trackScrollStops('read 25%');
  };
  if (e.detail.value == 0.50) {
    trackScrollStops('read 50%')
  };
  if (e.detail.value == 0.75) {
    trackScrollStops('read 75%');
  };
  if (e.detail.value == 1) {
    trackScrollStops('read 100%')
