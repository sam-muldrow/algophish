    },
    onRender: function() {
      var s = screen.width
      var p = Math.round((s / 320 - 1) * 100)
      var banner_selector = document.querySelector('#adfox_15925565620405969 + .mobile_full_width')
      if (banner_selector == null){
      } else {
        banner_selector.style.transform = 'scale(1.' + p + ')'
