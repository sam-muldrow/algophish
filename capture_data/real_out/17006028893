        <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>



        <script src="https://pl.zoocdn.com/www/uw/primelocation/static/scripts/main-d81468f0be.js"></script>

        

    <script>

        $(function() {
            load_script( 'https://pl.zoocdn.com/www/uw/primelocation/static/scripts/modules/search-0becd4dd15.js' );

            $('#section_type').on('change', function(e) {
                var section = $(this).val();
                $('#section').val( section );
                if (section == 'to-rent') {
                    $('input[name=new_homes]').prop('disabled', true)
                    $('input[name=include_shared_accommodation]').prop('disabled', false)
                } else {
                    $('input[name=new_homes]').prop('disabled', false)
                    $('input[name=include_shared_accommodation]').prop('disabled', true)
                }

            });


            // Position the autocomplete to be underneath the search input
            $('.geo_autocomplete').autocomplete({
                open: function(event, ui) {
                    var autocomplete = $(".ui-autocomplete");
                    var oldLeft = parseInt(autocomplete.css("left"));
                    var searchbox = $("#search-input-location-wrapper");
                    var newTop = searchbox.offset().top + searchbox.height() - 12;
                    var newLeft = oldLeft + 13;

                    autocomplete.css({
                        "left"  : newLeft + "px",
                        "top"   : newTop + "px",
                        "width" : "445px"
                    });
                }
            });

        });

    </script>




