<script>
        jQuery(document).ready(function ($) {
            var $briefsWrap = $('#briefs .brief-wrap');
            var $detailWrap = $('.detail-list');
            var $conference = $('#conference');
            var $dailyPhoto = $('#dailyphoto');
            var $vote = $('.voting-form-box');

            function rescaleBreifs(victimBlock, targetBlock) {
                if ($(window).width() > 768) {
                    var ua = window.navigator.userAgent;
                    var msie = ua.indexOf("MSIE ");
                    if (msie > 0) {
                        targetBlock.css({'position': 'relative', 'max-height': '950px', 'overflow': 'hidden'});
                    } else {
                        $columnHeight = victimBlock.height();

                        if ($dailyPhoto.length > 0){
                            $dailyHeight = $dailyPhoto.outerHeight()+30; // margin-bottom: 30px;
                        } else {
                            $dailyHeight = 0;
                        }
                        $voteHeight = $vote.outerHeight();
                        targetBlock.css('height', ($columnHeight - $dailyHeight - $voteHeight) * .95);
                        targetBlock.nanoScroller();

                    }
                } else {
                    targetBlock.css({'height': '950px'});
                    targetBlock.nanoScroller({
                        destroy: true
                    });
                }
            }

            rescaleBreifs($detailWrap, $briefsWrap);


            $('.detail-list img').load(function () {
                rescaleBreifs($detailWrap, $briefsWrap);
            });
            $(window).on('resize', function () {
                rescaleBreifs($detailWrap, $briefsWrap);
            });
            let $element = $('.txt_brieflist');
            if($element.length > 0) {
                $element.insertAfter($('ul.content').children().eq(1));
                $element.removeClass('hidden')
            }
        });
    </script>
