                      jQuery(document).ready(function($) {
                        $(window).load(function() {

                          var ua = window.navigator.userAgent;
                          var msie = ua.indexOf('MSIE');
                          $equaledBlocks = $('[data-equal]');
                          $equaledBlocks.each(function() {

                            $current = $(this);
                            $targetBlock = $('#' + $current.data('equal'));

                            if (($(window).width() > 768) && (msie <= 0)) {

                              $currentVerticPadding = $current.outerHeight() - $current.height();
                              $currentContent = $current.find('.brief-wrap');
                              $moreLinkHeight = $current.find('.all-block').outerHeight();

                              $targetHeight = $targetBlock.height();

                              $currentContent.css('height', $targetHeight - $moreLinkHeight - $currentVerticPadding);
                              $currentContent.nanoScroller();

                            }
                            else {
                              $current.css({
                                'position': 'relative',
                                'max-height': '950px',
                                'overflow': 'hidden',
                              });
                            }
                          });
                        });
                      });
