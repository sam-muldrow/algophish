<script type="text/javascript">
/* Отправка форм ------------------------------------------------------------------------------- */
for (let $form of document.querySelectorAll('form')) {
	$form.addEventListener("submit", (event) => {
		let url = "/send.php";

		/* Если форма обращается не к тому URL */
		if (event.target.getAttribute("action") != url)
			return;

		/* Отключаем стандартную обработку форм браузером */
		event.preventDefault();

		/* Получаем элементы */
		let $form = event.target;
		let $btn = $form.getElementsByTagName("button")[0];
		let data = new FormData($form);

		/* Отключаем кнопку */
		$btn.disabled = true;

		/* Отправляем запрос */
		let xhr = new XMLHttpRequest();
		xhr.open("POST", url, true);
		xhr.send(data);
		xhr.onload = function () {
		    if (xhr.status === 200) {
		    	let res = JSON.parse(xhr.response);

		    	/* В зависимости от ответа формируем сообщение */
		    	if (res.success) {
		    		$form.innerHTML = '<div class="form__success">' + res.success + "</div>";
		    		$form.reset();
		    	} else if (res.error) {
		    		$form.querySelector(".form__errors").innerHTML = res.error;
		    	} else {
		    		$form.querySelector(".form__errors").innerHTML = "Произошла неизвестная получения данных. Попробуйте ещё раз.";
		    	}

			/* Включаем кнопку */
			$btn.disabled = false;
		    } else {
		    	$form.querySelector(".form__errors").innerHTML = "Произошла неизвестная ошибка сервера. Попробуйте ещё раз.";
		    }
		}
	});
}
</script>

<script type="text/javascript">
	let loaded = false,
    metricaId = 93505798,
    timerId;

	if (navigator.userAgent.indexOf("YandexMetrika") > -1 ) {
		loadMetrica();
	} else {
		window.addEventListener("scroll", loadScripts);
		window.addEventListener("touchstart", loadScripts);
		document.addEventListener("mouseenter", loadScripts);
		document.addEventListener("click", loadScripts);
		document.addEventListener("DOMContentLoaded", loadFallback);
	}

	function loadFallback() {
		timerId = setTimeout(loadScripts, 6000);
	}

	function loadScripts() {
		if (loaded)
			return;

		let fileJS = document.createElement('script');
		fileJS.src = "/scripts.min.js?v=146";
		document.body.appendChild(fileJS);

		let fileJivo = document.createElement('script');
		fileJivo.src = "//code.jivo.ru/widget/tzBlAIdPkm";
		document.body.appendChild(fileJivo);

		(function(m,e,t,r,i,k,a){m[i]=m[i]||function(){(m[i].a=m[i].a||[]).push(arguments)}; m[i].l=1*new Date();k=e.createElement(t),a=e.getElementsByTagName(t)[0],k.async=1,k.src=r,a.parentNode.insertBefore(k,a)}) (window, document, "script", "https://cdn.jsdelivr.net/npm/yandex-metrica-watch/tag.js", "ym");
		ym(metricaId, "init", {clickmap:true, trackLinks:true, accurateTrackBounce:true});

		loaded = true;

		clearTimeout(timerId);
		
		setTimeout(loadVUZ, 1000);
		setTimeout(loadSSUZ, 1000);

		window.removeEventListener("scroll", loadScripts);
		window.removeEventListener("touchstart", loadScripts);
		document.removeEventListener("mouseenter", loadScripts);
		document.removeEventListener("click", loadScripts);
		document.removeEventListener("DOMContentLoaded", loadFallback);
	}
</script>
