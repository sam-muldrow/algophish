function fillpfserverDropdown(){
removeAllOptions(document.main_submission.pfserverDropdown);
addOption(document.main_submission.pfserverDropdown, "https://mo.proxfree.com/request.php?do=go", "United States Central (MO)");
addOption(document.main_submission.pfserverDropdown, "https://ca.proxfree.com/request.php?do=go", "United States West (CA)");
addOption(document.main_submission.pfserverDropdown, "https://uk.proxfree.com/request.php?do=go", "United Kingdom");
}

function SelectpfipDropdown(){
removeAllOptions(document.main_submission.pfipDropdown);

if(document.main_submission.pfserverDropdown.value == 'https://mo.proxfree.com/request.php?do=go'){
}
if(document.main_submission.pfserverDropdown.value == 'https://ca.proxfree.com/request.php?do=go'){
}
if(document.main_submission.pfserverDropdown.value == 'https://uk.proxfree.com/request.php?do=go'){
}
}

function removeAllOptions(selectbox)
{
	var i;
	for(i=selectbox.options.length-1;i>=0;i--)
	{
		selectbox.remove(i);
	}
}

function addOption(selectbox, value, text )
{
	var optn = document.createElement("OPTION");
	optn.text = text;
	optn.value = value;

	selectbox.options.add(optn);
}

var updatedDropdown = false;

document.addEventListener("DOMContentLoaded", function(event) {
	updatedDropdown = true;
	fillpfserverDropdown();
});
