<div class="pt-[4.25rem] mx-auto  max-w-[99rem] py-3 px-2 md:px-4 min-h-screen bg-neutral-100" 
    x-data="{topads: '', device: 'mobile'}" x-init="() => {
    function handleResize() {
      if (document.getElementById('navtags')) {
        $el.style.paddingTop = document.getElementById('navtags').offsetHeight + 66 + 'px' ;
      }
      if (window.innerWidth < 768) { device = 'mobile' ; } else { device = 'desktop' ; }
    }
    handleResize()
    window.addEventListener('resize', handleResize)
    return () => {
      window.removeEventListener('resize', handleResize)
    }
  }">

