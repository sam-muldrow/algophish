    x-on:click.away="searchShow=false; document.body.classList.remove('overflow-hidden');"
    class="relative" style="display: none !important">
    <div class="fixed inset-0 w-screen h-screen sm:absolute sm:left-0 sm:top-0 sm:w-full sm:max-h-96  z-50 overflow-hidden bg-white flex flex-col"
  x-data="{search: '', suggestions: [],
    fetchSearch() {
      if (this.search.length>1) {
        fetch('/ty/' + this.search, 
          {mode: 'same-origin', credentials: 'same-origin'})
          .then(response => response.json())
          .then(data => {this.suggestions=data;});
      } else {
        this.suggestions=[];
      }
    }
  }"
  >

  <div x-cloak x-on:search-focus.window="
      search=$event.detail ;
      fetchSearch() ;
      setTimeout(()=>{$refs.input.focus()}, 200) ;
      $nextTick(()=>{ setTimeout(() => {$refs.input.focus()}, 300); } ) ;
    ">

    <form x-on:submit.prevent="if (search.length>0) { window.open('/search/' + search, '_self') }">
      <div class="flex w-full h-10 px-3 items-center rounded-xl border border-zinc-600">

        <div class="sm:hidden" x-on:click="$dispatch('closesearch', '')">
