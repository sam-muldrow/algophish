<script type="text/javascript">
    function modelInit(title,text,target) {
        $(".model-style-1 .model-box h3").text(title);
        $(".model-style-1 .model-box p").text(text);
        $(".model-style-1 .model-btns-2").attr("href",target);
        $(".model-style-1").show();
        $("body").addClass("body-cover");
        if($.browser.msie){
            $("body").addClass("p-r-0");
        }

    }
    $(function () {
        $(".model-style-1 .model-btns-1").click(function () {
            $(".model-style-1").hide();
            $("body").removeClass("body-cover");
            if($.browser.msie){
                $("body").removeClass("p-r-0");
            }
        })
    })
</script>
