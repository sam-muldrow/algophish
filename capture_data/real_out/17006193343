<script>
    $(
       function () {

           $(".slider-mobile-main").css({"width": $(".slider-mobile-main>li").length*100+"%"})
           $(".slider-mobile-main>li").css({"width": 1/$(".slider-mobile-main>li").length*100+"%"})
           var initDataX = 0;
           var initDataY = 0;
           var shifting = 0
           var boos = 0;
           $(".slider-mobile-main").on("touchstart", function (e) {
               initDataX = e.originalEvent.changedTouches[0].pageX;//è·åæææä¸æ¶çåæ 
               initDataY = e.originalEvent.changedTouches[0].pageY;//è·åæææä¸æ¶çåæ 
               shifting = $(this).offset().left; //è·åulåç§»çleft

           })
           $(".slider-mobile-main").on("touchmove", function (e) {

               var x = e.originalEvent.changedTouches[0].pageX;//æ»å¨è·ç¦»
               var y = e.originalEvent.changedTouches[0].pageY;//æ»å¨è·ç¦»
               var slidingDistanceX = x - initDataX;
               var slidingDistanceY= y - initDataY;
               boos = slidingDistanceX
               if (Math.abs(slidingDistanceX)>Math.abs(slidingDistanceY)){

                   var left = shifting + slidingDistanceX
                   $(this).css({"left": left+"px"})
                   return false
               }


           })
           var num = 0;
           $(".slider-mobile-main").on("touchend", function (e) {
               var initLeft = $(this).offset().left; //è·åulåç§»çè·ç¦»
               // var num = Math.round(-initLeft/$(".slider-mobile-main>li")[0].offsetWidth)
               //var num = Math.round(-initLeft/$(this).children("li")[0].offsetWidth)
               //var num = parseInt(-initLeft/$(this).children("li")[0].offsetWidth)
               if (boos>80) {
                    num--
                }
                if (boos<-80) {
                    num++
                }
               // num = num >= ($(".slider-mobile-main>li").length-1)?$(".slider-mobile-main>li").length-1:num;//åå¤æ­
               num = num >= ($(this).children("li").length-1)?$(this).children("li").length-1:num;//åå¤æ­

               num = num <= 0 ? 0 : num;

               // $(this).css({"left": -num *  $(".slider-mobile-main li")[0].offsetWidth + "px"})
               $(this).stop().animate({"left": -num *  $(this).children("li")[0].offsetWidth + "px"},400);
               //é¦é¡µå¤æ­
               if($(this).attr("touchend") == "slider-mobile-main-active"){
                   if (num !== 0) {
                       $(this).find("li[touchend='active']").attr("data-toggle","0");
                       $(".index-page .section-cooperation .btn-mobile-more").attr("data-status","hide").text("æ¥çæ´å¤");
                   }
            
               }
               if (num == 1) {
                   $(this).children("li").eq(1).find(".application-ul-1").css({"display": 'flex'})
               } else {
                   $(this).children("li").eq(1).find(".application-ul-1").css({"display": 'none'})
               }
               
               if (num == 3) {
                   $(this).children("li").eq(3).find(".content-control").css({"display": 'block'})
               } else {
                   $(this).children("li").eq(3).find(".content-control").css({"display": 'none'})
               }
               $(this).parents(".slider-mobile-content").next().find("li").removeClass("li-active")

               // $(".slider-mobile-top ul li").children().removeClass("active")
               $(this).parents(".slider-mobile-content").prev().find("li").children().removeClass("active")

               $(this).parents(".slider-mobile-content").prev().find("li").eq(num).children().addClass("active")

               // $(".slider-mobile-bottom ul li").eq(num).addClass("li-active")
               $(this).parents(".slider-mobile-content").next().find("li").eq(num).addClass("li-active")

               // return false
           })

           $(".slider-mobile .slider-mobile-top ul li").click(function () {
               $(".slider-mobile-main").stop().animate({"left": -$(this).index()*$(".slider-mobile-main li")[0].offsetWidth + "px"},400);
               $(this).siblings().children().removeClass("active")
               $(this).children().addClass("active")
               $(this).parents(".slider-mobile").children(".slider-mobile-bottom").find("li").removeClass("li-active")
               $(this).parents(".slider-mobile").children(".slider-mobile-bottom").find("li").eq($(this).index()).addClass("li-active")
               
               if ($(this).index() == 1){
                   $(this).parents(".slider-mobile").find(".application-ul-1").css({"display": "flex"})
               } else {
                   $(this).parents(".slider-mobile").find(".application-ul-1").css({"display": "none"})
               }
               
               
           })
       }
    )
</script>

