;(function($){
    $('#legalwelcome .close').on('click', function() {
      $('#legalwelcome').css('display', 'none');
      localStorage.setItem('legalwelcome', true);
    });

    // show and hide the alert for previous closers.
    var cookiestatus = '';
    if (Storage !== undefined) cookiestatus = JSON.parse(localStorage.getItem('legalwelcome'));
    if (!cookiestatus) {
      $('#legalwelcome').show();
    } else {
      $('#legalwelcome').hide();
    }
})(LITHIUM.jQuery);


