LITHIUM.SearchForm({"asSearchActionIdSelector":".lia-as-search-action-id","useAutoComplete":true,"selectSelector":".lia-search-form-granularity","useClearSearchButton":false,"buttonSelector":".lia-button-searchForm-action","asSearchActionIdParamName":"as-search-action-id","formSelector":"#lia-searchformV32_3ddc2c32545fd6","nodesModel":{"en|category":{"title":"Search Community: PayPal Community","inputSelector":".lia-search-input-message"},"user|user":{"title":"Users","inputSelector":".lia-search-input-user"}},"asSearchActionIdHeaderKey":"X-LI-AS-Search-Action-Id","inputSelector":"#messageSearchField_3ddc2c32545fd6_0:not(.lia-js-hidden)","clearSearchButtonSelector":null});
    (function($){
        $(document).ready(function() {
           var prev = "";
           $( ".lia-search-input-message" ).on('input paste', $.debounce(700,function(e){
                var cur= $( ".lia-search-input-message" ).val();
                if(cur.length > 2){
                    if(prev != cur || $('.help_view-all-link').length <= 0){
                      //var endPoint = '/ppl/plugins/custom/paypal/ppl2022/hc-search?q='+cur+'&experience=consumer&country=US';

                      //var end_point= encodeURI(endPoint);
                      var autocompleteResults;
                      prev = cur;
                      var results;
                      var num_results = '5';
                      $.ajax({
                         type: 'GET',
                         url: '/plugins/custom/paypal/ppl2022/hc-search?tid=5637130491858144331',
                         dataType: 'html',
                         data: {q: cur, country: "US", experience: "consumer"},
                         success: function(data) {
                           if(data && data.trim()){
                               fpti.link='help-articles-loaded';
                               fpti.pglk=fpti.pgrp+'|'+fpti.link;
                               if(PAYPAL && PAYPAL.analytics){
                                   PAYPAL.analytics.Analytics.prototype.recordClick({ data: fpti });
                               }
                               if($('.lia-autocomplete-has-results').length <= 0){
                                 autocompleteResults = $('.lia-autocomplete-empty-result');
                               } else {
                                 autocompleteResults = $('.lia-autocomplete-has-results');
                               }
                               if($('.lia-autocomplete-input-has-results').length <= 0){
                                 inputAutocomplete = $('.lia-autocomplete-input-empty-result');
                               } else {
                                 inputAutocomplete = $('.lia-autocomplete-input-has-results');
                               }
                               if($('.lia-autocomplete-has-results').length > 0){
                                autocompleteResults.find('ul').prepend('<li id="community_list_title"><div>COMMUNITY</div></li>');
                               }
                               results = $.parseJSON(JSON.stringify(data));
                           }
                         }
                      })
                      .done(function(){
                         if(results){
                            JSON.parse(results).forEach(function(eachResult,i){
                                if(i < num_results) {
                                   var href='https://www.paypal.com/us/selfhelp/article/'+eachResult.articleId;
                                   autocompleteResults.find('ul').prepend("<li class='lia-autocomplete-list-even-row'><a class='help-link' href="+href+" id="+eachResult.articleId+">"+eachResult.articleTitle+"</a></li>");
                                }
                            });
                            var rawviewall = 'https://www.paypal.com/us/smarthelp/search?q='+cur+'&experience=consumer&locale.x=en_US';
                            var viewall= encodeURI(rawviewall);
                             autocompleteResults.find('ul').prepend("<li class='help_view-all-row'><span>HELP ARTICLES</span><span class='help_view-all-link'><a class='help-all-results' href="+viewall+">View All</a></span></li>");
                             if($('.lia-autocomplete-has-results').length <= 0){
                                autocompleteResults.addClass('lia-autocomplete-has-results');
                                autocompleteResults.removeClass('lia-autocomplete-empty-result');
                             }
                             if($('.lia-autocomplete-input-has-results').length <= 0){
                               inputAutocomplete.addClass('lia-autocomplete-input-has-results');
                               inputAutocomplete.removeClass('lia-autocomplete-input-empty-result');
                             }
                             $("div.lia-autocomplete-has-results a.help-link").on('click',function(e){
                                 e.preventDefault();
                                 var target = e.target;
                                 if(target){
                                     fpti.link='help-articles-clicked';
                                     fpti.pglk=fpti.pgrp+'|'+fpti.link;
                                     fpti.hpid= target.id;
                                     if(PAYPAL && PAYPAL.analytics){
                                         PAYPAL.analytics.Analytics.prototype.recordClick({ data: fpti });
                                     }
                                     window.open(target.href,'_blank');
                                 }
                             });
                             $("div.lia-autocomplete-has-results a.help-all-results").on('click',function(e){
                                 e.preventDefault();
                                 var target = e.target;
                                 if(target){
                                     fpti.link='view-more-help-results-clicked';
                                     fpti.pglk=fpti.pgrp+'|'+fpti.link;
                                     fpti.hpid= target.id;
                                     if(PAYPAL && PAYPAL.analytics){
                                         PAYPAL.analytics.Analytics.prototype.recordClick({ data: fpti });
                                     }
                                     window.open(target.href,'_blank');
                                 }
                             });
                         }
                      })
                      .fail(function(){
                      });
                    }
                }
               })
            );
        });
    })(LITHIUM.jQuery);

