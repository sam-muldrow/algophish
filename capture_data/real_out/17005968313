        $('#communityAdminLinksTaplet ul').append('<li><a class="lia-link-navigation" id="mod_dashboard" href="/t5/custom/page/page-id/BannedUserDashboard">Auto Ban Dashboard</a></li>');
    })(LITHIUM.jQuery);

