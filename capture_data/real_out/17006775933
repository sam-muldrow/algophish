  <script> $('#cookie-wrap').fadeIn(1000); $('body').on('click', '#cookie-ok, .cookieok', function() { var exdate=new Date(); exdate.setDate(exdate.getDate() + 3650); var c_value=escape('1') + ';expires=' + exdate.toUTCString(); document.cookie='MI_CCTRL' + '=' + c_value; $('#cookie-wrap').fadeOut(); });
</script>
      
