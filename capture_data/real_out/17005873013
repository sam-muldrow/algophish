<script>
    $(function() {
        // Check if the user is on Desktop
        if ($('#homepage_security_box').height() > $('.homepage_teaser_box').height()) {
            // Get the height of the homepage security box and apply it to the teaser boxes so all three of them have the same box size
            $('.homepage_teaser_box').height($('#homepage_security_box').height());
        }

        // Get the biggest headline height and apply it to the smaller one in order for them both to be aligned properly
        var heading_height_1 = $('#homepage_teaser_box_heading_1').height();
        var heading_height_2 = $('#homepage_teaser_box_heading_2').height();
        if (heading_height_1 > heading_height_2) {
            $('#homepage_teaser_box_heading_2').height(heading_height_1)
        } else if (heading_height_1 < heading_height_2) {
            $('#homepage_teaser_box_heading_1').height(heading_height_2)
        }
    })
</script>
