<div class="position-relative">
    <input id="autocompinp" class="form-control autocomplete wgg_input city_loader_bar" style="cursor: default; position: absolute;" tabindex="1" type="text" autocomplete="off" name="autocompinp" value="" placeholder="Stadt eingeben">
