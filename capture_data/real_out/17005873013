    $(".tog").on('mouseenter', function() {
        var presse_img = $(this).attr('id');
        $(this).attr('id', presse_img.substring(0, presse_img.length - 2));
    }).on('mouseleave', function() {
        var presse_img = $(this).attr('id');
        $(this).attr('id', presse_img+'_g');
