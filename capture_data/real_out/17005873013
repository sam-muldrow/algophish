    </div>
</div>

<script src="/js/lib/PartnerMapping.lib.tjb1632485224.js"></script>
<script src="/js/Partner/PartnerSearch.class.tjb1697624698.js"></script>

<script>
    /**
     * Render the Districts and Flatshare Types multiselect dropdowns
     */
    $('.selectpicker').selectpicker(
        {
            noneResultsText: translation_lang_functions.no_results
        },
        'render'
    );

    var mobile_layout  = '';
    var conf_client_id = 'WG-Gesucht';

    window.PartnerSearch = PartnerSearch({
        view: 'homepage',
        mobile_layout: mobile_layout,
        categories: $('#categories.selectpicker').val(),
        city_id: '',
        city_name: replaceSpecialCharacters($('#autocompinp').val()),
        min_log_level: '5'
    });

    window.PartnerSearch.pickPartnerSearch();

    $("#rubrik-dropdown li").on('click', function () {
        var selectedOption = $(this).attr('data-value');
        var selectedOptionText = $(this).attr('data-text');
        $("#rubrik-value").val(selectedOption);
        $("#rubrik-dropdown-menu").html(selectedOptionText+' <span class="caret category_and_ad_type_caret"></span>');
    });

    $('#categories.selectpicker').on('change', function() {
        window.PartnerSearch.updateState({
            categories: $(this).val()
        });
    });

    $("#ang-ges-value").on('change', function () {
        var selectedOption = $(this).val();

        if (selectedOption == '0') {
            window.PartnerSearch.pickPartnerSearch();
        } else {
            window.PartnerSearch.hidePartnerCheckboxes();
        }
    });

    $('#autocompinp').on('change', function() {
        // timeout needed for this to pick the proper value
        setTimeout(
            function() {
                window.PartnerSearch.updateState({
                    city_id: $('#stadt_key').val(),
                    city_name: replaceSpecialCharacters($('#autocompinp').val())
                });
            },
            250
        );
    });

    $('#formPortal').on('submit', function(e) {
        e.preventDefault();

        var categories = $('#categories.selectpicker').val();
        if (typeof categories === 'undefined') {
            categories = [];
        }
        if ((!$('#stadt_key').val() && !$('#city_id').val()) ||
            typeof $('#categories.selectpicker') == "undefined" ||
            categories.length === 0
        ) {
            return;
        }

        formProcessingStart('formPortal');
        var form_data              = $('#formPortal').serializeArray();
        var perform_partner_search = false;
        if (conf_client_id) {
            perform_partner_search = window.PartnerSearch.isPartnerSearchEnabled();
        }

        var window_target = window;
        if (perform_partner_search === true) {
            window_target = window.open(window.location, '_blank');
        }

        submitPortalForm(window_target, mobile_layout, perform_partner_search, form_data);
    });


    /**
     * Submits the portal form
     *
     * @param obj target_window          The targeted window
     * @param str mobile_layout          The mobile layout
     * @param boo perform_partner_search The open airbnb flag
     * @param arr form_data              The form data
     */
    function submitPortalForm(target_window, mobile_layout, perform_partner_search, form_data)
    {
        $.ajax({
            url:"https://www.wg-gesucht.de/",
            method: 'POST',
            data: form_data
        }).done(function(response) {
            if (perform_partner_search === true) {
                setTimeout(function(){
                   window.PartnerSearch.performPartnerSearch(mobile_layout);
                }, 10);
            }

            setTimeout(function(){
                target_window.location.assign(document.location.origin + response)
            }, 10);
        }).fail(function() {
            formProcessingEnd('formPortal');
        });
    }
</script>
                </div>
            </div>
        

