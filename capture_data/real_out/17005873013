                ,                            th: 'Thailand'
                ,                            tn: 'Tunesien'
                ,                            tr: 'Türkei'
                ,                            tw: 'Taiwan'
                ,                            tz: 'Tansania'
                ,                            ua: 'Ukraine'
                ,                            ug: 'Uganda'
                ,                            us: 'USA'
                ,                            uy: 'Uruguay'
                ,                            ve: 'Venezuela'
                ,                            vn: 'Vietnam'
                ,                            ye: 'Jemen'
                ,                            za: 'Südafrika'
                ,                            zm: 'Sambia'
                ,                            zu: 'Usbekistan'
                ,                            zw: 'Simbabwe'
                                    };

        $('#autocompinp').devbridgeAutocomplete({
            serviceUrl: "/ajax/getCities.php",
            params: {
                country_parameter: ""
            },
            autoSelectFirst: true,
            groupBy: 'category',
            onSelect: function(suggestion) {
                updateSelectBoxValue(suggestion);
            },
            transformResult: prepareReceivedData,
            onSearchStart: function() {
                $('#autocompinp').parents('form').find(':input[type="submit"]').prop('disabled', true);
            },
            onSearchComplete: function (query, suggestions) {
                $('#autocompinp').parents('form').find(':input[type="submit"]').prop('disabled', false);
                if (suggestions[0].data.city_id.length) {
                    $('#stadt_key, #city_id').val(suggestions[0].data.city_id);
                    $('#country_code').val(suggestions[0].data.country_code);
                }
            },
            onHint: function(hint) {
                $('#autocomplete-hint').val('')
                var user_input = $('#autocompinp').val().toLowerCase();
                var selected   = $('.autocomplete-selected').text().toLowerCase();
                if (selected.startsWith(user_input)) {
                    $('#autocomplete-hint').val(hint);
                }
            },
            onSearchError: function(query, jqXHR, textStatus, errorThrown) {
                if (jqXHR.status === 404 && $('#no_cities_found').is(':hidden')) {
                    displayCityNotFoundErrorMessage();
                }
                $('#autocompinp').parents('form').find(':input[type="submit"]').prop('disabled', false);
            },
        });


        /*
         * Category changed if city is selected redirect at list view
         */
        $('#categories_wrapper').on('change', function() {
            var categories = $('#categories.selectpicker').val();
            var city_id    = $('#stadt_key').val();
            var city_name  = $('#city_name').val();
            if (categories.length !== 0 && city_name) {
                redirectUserToListView(city_name, city_id, '');
            }
        });
    });

    $('#autocompinp').on('focus', function() {
        old_value = $(this).val();
        $(this).val('');
    });

    $('#autocompinp').on('blur', function() {
        if (!$(this).val().length) {
            $(this).val(old_value);
        }
    });

    // check for a value in the stadt_key input on form submit
    $('#formPortal').on('submit', function(e) {
        if (!$('#stadt_key').val() && !$('#city_id').val()) {
            e.preventDefault();
            $('body').removeClass('wait');
            displayCityNotFoundErrorMessage();
        }

        var categories = $('#categories.selectpicker').val();
        if (typeof categories === 'undefined') {
            categories = [];
        }
        if (categories.length === 0) {
            e.preventDefault();
            $('#no_categories_selected').fadeIn(400).delay(2000).fadeOut(400);
        }
    });
</script>

