   	{name: "windboeen", id: "gust", label: "Windspitzen"}
   	]
     , random_parameter = paras[Math.floor(Math.random() * paras.length)]
     , getImagePath = function(name) {
        name = name || paras[0].name; 
     	return decodeURIComponent(["/?pid=p_modell_expert", "ireq=true", "src=modellb/vermarktung/p_modell_expert/teaser/profikarten-wetterkarte-" + name + ".gif"].join("&")); 
     }
     , getImageAltText = function(label) {
       label = label || "";
       return ["Profikarte ", label, " Europa"].join(""); 
     }
     , img
     , anchor
     , wrapper = $("expertcharts-wrapper")
   ; 
   
   try 
   {
    img = new Element("img", 
    {
	src: getImagePath(random_parameter.name), 
	alt: getImageAltText(random_parameter.label), 
	width: "300", 
	height: "170"
    }); 

    anchor = new Element("a", 
    {
    	href:  decodeURIComponent(["/profiwetter/europa?parameter=",random_parameter.id,"%26model=gfs%26base=23111912%26hour=24"].join("")), 
    	title: getImageAltText(random_parameter.label)
    }); 
  
    anchor.adopt(img);
    if (wrapper) wrapper.adopt(anchor); 
    
    $$("#expertcharts_teaser ul li a").addEvents({
    	"mouseenter": function(e) {
    		img.set("src", getImagePath(e.target.get("data-name")));
    	}
    }); 
   } 
   catch(e) 
   {
	console&&console.log(e); 
   }
 })();
