                var yWingSimgImgtag = $('#yWingSimg .sImg img');
                var yWingBimg = $('#yWingBimg');
                var yWingBimgAtag = $('#yWingBimg > a');
                var yWingBimgImgtag = $('#yWingBimg > a > img');

                // 초기값
                var goods = extendsGoods[0];

                if(goods != null && extendsGoods.length > index){
                    goods = extendsGoods[index];
                }

                yWingSimgAtag.attr('href', goods.href);
                yWingSimgAtag.attr('target', goods.target);
                yWingSimgAtag.bind('click', function () { setWcode('002_0' + goods.wcode + '_001');});

                yWingSimgImgtag.attr('src', goods.src);
                yWingSimgImgtag.attr('alt', goods.alt);

                yWingBimgAtag.attr('href', goods.href);
                yWingBimgAtag.attr('target', goods.target);
                yWingBimgAtag.bind('click', function () { setWcode('002_0' + goods.wcode + '_002');});

                yWingBimgImgtag.attr('src', goods.addSrc);
                yWingBimgImgtag.attr('alt', goods.alt);

                $("#emText1").text(goods.text1);
                $("#emText2").text(goods.text2);

                if(goods.addSrc.length == 0 ){
                    $('#yWingSimg .wingMouseTiny').remove();
                    $('#yWingBimg').remove();
                    $('#yWingClose').remove();
                }
            }

            showLeftExtendsBanner(Math.floor(Math.random() * extendsGoods.length));

        //-->
        </script>
    
	</div>
