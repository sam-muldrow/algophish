	<span class="pagen prev"><a href="javascript:void(0);" onclick="stopYesAllTv();" class="yPgBtn yPgLft bgYUI" style="display: inline;">이전 예스티비 영상</a></span>
	<span class="pagen next"><a href="javascript:void(0);" onclick="stopYesAllTv();" class="pagen yPgBtn yPgRgt bgYUI" style="display: inline;">다음 예스티비 영상</a></span>
	<script type="text/javascript">
		var yesTvIsPlay_0 = false;
		var yesTvIsPlay_1 = false;
		var yesTvIsPlay_2 = false;

		function stopYesAllTv(tvNm) {
			var yesTvMov = $('#yWelYesTv .tvMov iframe');
			for (var i = 0; i < yesTvMov.length; i++) {
				if (i != tvNm) {
					yesTvMov[i].contentWindow.postMessage('{"event":"command","func":"stopVideo","args":""}', '*');

					if (window["yesTvIsPlay_" + i]) {
						window["yesTvIsPlay_" + i] = false;
					}
				}
			}
		}

		function pauseYesAllTv() {
			var yesTvMov = $('#yWelYesTv .tvMov iframe');
			for (var i = 0; i < yesTvMov.length; i++) {
				yesTvMov[i].contentWindow.postMessage('{"event":"command","func":"pauseVideo","args":""}', '*');
			}
		}

		function playYesTv(tvId, tvNm) {
			setWcode('043_002');
			stopYesAllTv(tvNm);
			if (!window["yesTvIsPlay_" + tvNm]) {
				document.getElementById(tvId).contentWindow.postMessage('{"event":"command","func":"playVideo","args":""}', '*');
				window["yesTvIsPlay_" + tvNm] = true;
				for (var i = 1; i < 4; i++) {
					if (i != tvNm) {
						window["yesTvIsPlay_" + i] = false;
					}
				}
			} else {
				window["yesTvIsPlay_" + tvNm] = false;
			}
		}

		$('#yesTvRoll').hiveslide({
			auto: false,
			current: Math.floor(Math.random() * 3),
			wraptag: true
		});

		$('#yWelYesTv').bind('mouseover', function () {
			$(this).find(".yPgBtn").show();
		});

		$('#yWelYesTv').bind('mouseleave', function () {
			$(this).find(".yPgBtn").hide();
		});
	</script>
</div>
