                { 
                    href : 'https://www.yes24.com/campaign/00_Corp/2019/readBook/readBook.aspx', 
                    target : '_self', 
                    src : 'https://image.yes24.com/images/00_Event/2020/0521ReadBook/bn_228x164.jpg', 
                    alt : '법인필독서이벤트' 
                }
            );
                
