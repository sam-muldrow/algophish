<script>
    
    (function (params)
    {
        if(!params.cookieName || !params.checkCookieName)
            return;

        function checkCookie()
        {
            var checkValue = document.cookies.read(params.checkCookieName);
            return !!checkValue;
        }
        if (checkCookie())
            return;

        if(!params.url)
            return;
        if(!params.minTime && params.minTime !== 0 && params.minTime !== "0")
            params.minTime = 24;
        params.minTime = parseInt(params.minTime);
        if(isNaN(params.minTime))
            return;
        if(!params.ignoreRobots)
            params.ignoreRobots = false;
        if(params.ignoreRobots === "false")
            params.ignoreRobots = false;
        if(params.ignoreRobots === "true")
            params.ignoreRobots = true;

        var minTime = (params.minTime*60*60*1000);
        var d = new Date();
        d.setTime(d.getTime() + minTime);

        var storedTime = document.cookies.read(params.cookieName);
        if(storedTime)
            storedTime = parseInt(storedTime);
        if(!storedTime)
        {
            document.cookies.write(params.cookieName, "1");
            if(!document.cookies.read(params.cookieName) && params.ignoreRobots)
                return;
            storedTime = 1;
        }
        if((storedTime + minTime) > (+new Date()))
            return;

        var s = document.createElement("script");
        s.src = params.url;
        s.onload = s.onerror = function ()
        {
            document.cookies.write(params.cookieName, "" + (+new Date()), d);
            if (checkCookie())
                return;
            var failExpire = null; // temporary //new Date(Date.now() + 5*60*1000); // 5 minutes
            document.cookies.write(params.failCookieName, params.failCookieValue, failExpire);
        }
        document.getElementsByTagName("head")[0].appendChild(s);
