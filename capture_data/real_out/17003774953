    <script>
       setInterval(function(){ 
 $(".cyntv_part").find("img.show").removeClass("show").siblings().addClass("show");
     }, 2000);

    $(window).scroll(function() {

        var bottom_h = $("#qwdt").offset().top;

        if ($(window).scrollTop() > 2700) {

            $(".slideBar").css("bottom", "400px")
        } else {
            $(".slideBar").css("bottom", "89px")
        }

    });
    $("#back").click(function() {
        $('body,html').animate({ scrollTop: 0 }, 800);
        return false;
    });


    // url×ª¾ø¶ÔÂ·¾¶
    var toAbsURL = function() {
        var directlink = function(url) {
            var a = document.createElement('a');
            a.href = url;
            return a.href;
        };
        return directlink('') === '' ? function(url) {
            var div = document.createElement('div');
            div.innerHTML = '<a href="' + url.replace(/"/g, '%22') + '"/>';
            return div.firstChild.href;
        } : directlink;
    }();
    $('.magicp img').each(function(k, v) {
        // ¸ù¾ÝÑùÊ½»ñÈ¡Í¼Æ¬¼¯ºÏ
        reg = /^(http:|https:)?\/\//i;
        dsrc = $(v).attr('data-original');
        if (dsrc) {
            src = dsrc;
            if (!reg.test(src)) {
                src = toAbsURL(dsrc);
            }
             src = src.replace(reg, 'https://t.m.youth.cn/magicimg/');
            // console.log(src);

            if ($(this).hasClass("lazy")) {
                $(v).attr('data-original', src);
            } else {
                $(v).attr('src', src);
            }

        }
    })
