        function selectTag(showContent, sl, clj, cljxg) {
            for (i = 1; j = document.getElementById(sl + i); i++) {
                j.style.display = "none";
            }
            document.getElementById(showContent).style.display = "block";
            for (i = 1; j = document.getElementById(clj + i); i++) {
                j.className = "cljys";
            }
            document.getElementById(cljxg).className = "cljysxz";
        }
