    }); //³õÊ¼»¯ END

    //Íâ²¿¼ÓÔØjsÊý¾Ý,ÎÞÊý¾Ý¡¢Òì³£»òIE°æ±¾¹ýµÍHTMLÑùÊ½´¦Àí
    function bbNoShow(type) {


        $(".videoGroup").empty();
        $(".videoGroup").append('<img src="//www.youth.cn/images/20211221bbNoShow.jpg" />');
        $(".todayUl").empty();
        $(".tomorrowUl").empty();
        $(".yesterdayUl").empty();
        //IE°æ±¾liÑùÊ½´¦Àí
        if (type == "ie8") {
            $(".yesterdayUl").append('<li style="text-align: center;margin-top: 20px;"><span>Çë¸üÐÂIE°æ±¾»òÊ¹ÓÃÆäËûä¯ÀÀÆ÷</span></li>');
            $(".todayUl").append('<li style="text-align: center;margin-top: 20px;"><span>Çë¸üÐÂIE°æ±¾»òÊ¹ÓÃÆäËûä¯ÀÀÆ÷</span></li>');
            $(".tomorrowUl").append('<li style="text-align: center;margin-top: 20px;"><span>Çë¸üÐÂIE°æ±¾»òÊ¹ÓÃÆäËûä¯ÀÀÆ÷</span></li>');
        } 
    }

    function zeroize(value, length) {
        if (!length) length = 2;
        value = String(value);
        for (var i = 0, zeros = ''; i < (length - value.length); i++) {
            zeros += '0';
        }
        return zeros + value;
    }
    //Ê±¼ä
    function currentTime(where) {
        var d = new Date(),
            str = '';
        str += d.getFullYear(); //»ñÈ¡µ±Ç°Äê·Ý 
        str += zeroize(d.getMonth() + 1); //»ñÈ¡µ±Ç°ÔÂ·Ý£¨0¡ª¡ª11£© 
        str += zeroize(d.getDate());
        if (where == "cdn") {
            str += zeroize(d.getHours());
            str += zeroize(d.getMinutes());
            str += zeroize(parseInt(d.getSeconds()/5));
        }
        // str += d.getSeconds();
        return str;
    }

    //¼ÓÔØÍâ²¿JS,JS´´½¨Íê³ÉºóÖ´ÐÐcallback()
    function loadScript(src, callback) {
        var script = document.createElement('script'),
            head = document.getElementsByTagName('script')[0];
        script.type = 'text/javascript';
        script.charset = 'UTF-8';
        script.src = src;
        //IE8¡¢Opera 7.0 ¼° Opera ¸üÔç°æ±¾ä¯ÀÀÆ÷²»Ö§³ÖaddEventListener()¿ÉÒÔÊ¹ÓÃattachEvent()
        if (script.addEventListener) {
            script.addEventListener('load', function() {

                callback();
            }, false);
        } else if (script.attachEvent) {
            script.attachEvent('onreadystatechange', function() {
                var target = window.event.srcElement;
                if (target.readyState == 'loaded') {
                    callback();
                }
            });
        }
        head.parentNode.insertBefore(script, head);

    }

    //ÅÐ¶ÏIE
    function isIE() {
        var userAgent = navigator.userAgent; //È¡µÃä¯ÀÀÆ÷µÄuserAgent×Ö·û´®  
        var isIE = userAgent.indexOf("compatible") > -1 && userAgent.indexOf("MSIE") > -1; //ÅÐ¶ÏÊÇ·ñIE<11ä¯ÀÀÆ÷  
        var isEdge = userAgent.indexOf("Edge") > -1 && !isIE; //ÅÐ¶ÏÊÇ·ñIEµÄEdgeä¯ÀÀÆ÷  
        var isIE11 = userAgent.indexOf('Trident') > -1 && userAgent.indexOf("rv:11.0") > -1;
        if (isIE || isIE11) {
            var reIE = new RegExp("MSIE (\\d+\\.\\d+);");
            reIE.test(userAgent);
            var fIEVersion = parseFloat(RegExp["$1"]);
            if (isIE11 && !fIEVersion) {
                fIEVersion = 11;
            }
            if (fIEVersion <= 8) {
                console.log("IE8»òÒÔÏÂ°æ±¾");
                return "ie8";
            }
            console.log("IE" + fIEVersion);
            return true;
        } else if (isEdge) {
            console.log('edge'); //edge
            return false;
        } else {
            console.log('²»ÊÇIEä¯ÀÀÆ÷'); //²»ÊÇieä¯ÀÀÆ÷
            return false;
        }
    }

