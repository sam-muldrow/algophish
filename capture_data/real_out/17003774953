    <script>
    //³õÊ¼»¯
    $(function() {
        // ³õÊ¼»¯UlÇÐ»»¼°ÇÐ»»¹¦ÄÜ
        // $('.scrollView ul').hide()
        // $($('.scrollView ul')[1]).show()
        $($('.date span')[1]).addClass('active')
        $('.date span').click(function(e) {
            // console.log($(this).index())
            $(this).addClass('active').siblings().removeClass('active')
            $($('.scrollView ul')[$(this).index()]).show().siblings().hide()
        });

        //IE°æ±¾¼ì²â£¬Èç¹û<IE8°æ±¾ ½«Ìø³öÏÂÃæµÄli±êÇ©µÄäÖÈ¾£¬Ö±½Ó½øÈëbbNoShow();    
        if (isIE() == "ie8") {
            bbNoShow('ie8');
            return false;
        }

        // Íâ²¿JSµØÖ·£¬2Î»ÄêÔÂÈÕ.js https://t.m.youth.cn/wndbb/20211222.js  images/20211221ls.js
        var scriptSrc = "https://t.m.youth.cn/wndbb/" + currentTime() + ".js?" + currentTime('cdn');
        // var scriptSrc= "//qwcs.youth.cn/images/20211221ls.js?"+currentTime('cdn');

        //¼ÓÔØÍêÍâ²¿jsºó£¬ÔÙcallback£¬»ñÈ¡Êý¾ÝÆ´½ÓäÖÈ¾½ÚÄ¿ÁÐ±íli±êÇ©
        loadScript(scriptSrc, function() {

            var handler = {
                tmpl: function(data) //htmlÖÃ±êÉú³É
                {

                    var dom = new Array();
                    var yesterdayData = "";
                    var todayData = "";
                    var tomorrowData = "";
                    var vdstate = "played";
                    //yesterdayÊý¾Ý»ñÈ¡
                    for (var i = 0; i < data.yesterday.data.data.length; i++) {
                        data.yesterday.data.data[i].pub_time = data.yesterday.data.data[i].pub_time.substring(11, 16);

                        yesterdayData = yesterdayData + handler.list(data.yesterday.data.data[i], 2);

                    }
                    //todayÊý¾Ý»ñÈ¡
                    for (var i = 0; i < data.today.data.data.length; i++) {
                        //NextÎ´²¥·ÅµÄ£¬²»¿ÉÑ¡Ôñ
                        if (data.today.data.next && data.today.data.data[i].post_id == data.today.data.next.post_id) {
                            vdstate = "";
                        }
                        //NowÕýÔÚ²¥·ÅµÄ
                        if (data.today.data.now && data.today.data.data[i].post_id == data.today.data.now.post_id) {
                            vdstate = vdstate + " active";
                        }
                        data.today.data.data[i].state = vdstate;
                        data.today.data.data[i].pub_time = data.today.data.data[i].pub_time.substring(11, 16);
                        // console.log(data.today.data.data[i]);
                        todayData = todayData + handler.list(data.today.data.data[i], 0);

                    }
                    //tomorrowÊý¾Ý»ñÈ¡
                    for (var i = 0; i < data.tomorrow.data.data.length; i++) {
                        data.tomorrow.data.data[i].pub_time = data.tomorrow.data.data[i].pub_time.substring(11, 16);

                        tomorrowData = tomorrowData + handler.list(data.tomorrow.data.data[i], 1);

                    }
                    //yesterdayÊý¾Ý¡¢todayÊý¾Ý¡¢tomorrowÊý¾Ý
                    dom['yesterday'] = yesterdayData;
                    dom['today'] = todayData;
                    dom['tomorrow'] = tomorrowData;

                    return dom;
                },
                list: function(option, whichDay) //Æ´½ÓhtmlÃ¿Ìõ´úÂë
                {
                    var templateString = "";
                    var tmp_state;
                    //½ñÌìºÍÃ÷ÌìµÄli±êÇ©Æ´½Ó  0½ñÌì 1Ã÷Ìì 2×òÌì
                    //ÑùÊ½£º½ñÌì¶àÖÖ×´Ì¬£¬Ã÷ÌìÎª¿Õ£¬×òÌìplayed 
                    if (whichDay == 0) {
                        tmp_state=option.state;
                    } else if (whichDay == 1) {
                        tmp_state='';
                    } else if (whichDay == 2){
                        tmp_state='played';
                    }
                    var islive="false";
                    var video_url;
                    if(option.content_type=="live"){
                        islive="true";
                        video_url=option.payload.live_url ? option.payload.live_url : '';
                    }else{
                        video_url=option.payload.url ? option.payload.url : '';
                    }
                    templateString = '<li class="clearfix bbshowlist ' + tmp_state + '" islive="'+islive+'" data-id="' + option.post_id + '" data-Vdurl="' + video_url + '"><span class="iconvd left"></span><span class="time left">' + option.pub_time + '</span><span class="text left">' + option.title + '</span></li>';
                    return templateString;
                },

                init: function(data) //³õÊ¼»¯ÊÓÆµÁÐ±í
                {
                    if (data.yesterday.data.data == null) {
                        data.yesterday.data.data = 0;
                    }
                    if (data.today.data.data == null) {
                        data.today.data.data = 0;
                    }
                    if (data.tomorrow.data.data == null) {
                        data.tomorrow.data.data = 0;
                    }

                    //domList['yesterday'] , domList['today'] , domList['tomorrow']
                    var domList = handler.tmpl(data);

                    if (domList['yesterday']) {
                        $(".yesterdayUl").empty();
                        $(".yesterdayUl").append(domList['yesterday']);
                    }
                    if (domList['today']) {
                        //Èç¹ûÓÐtoday.data.nowÏÖÔÚ²¥·ÅÊý¾Ý
                        if (domList['today'].state) {
                            $(".videoGroup").empty();
                        }
                        $(".todayUl").empty();
                        $(".todayUl").append(domList['today']);
                    }
                    if (domList['tomorrow']) {
                        $(".tomorrowUl").empty();
                        $(".tomorrowUl").append(domList['tomorrow']);
                    }
                }

            };

            //cyol_wndbb_data²»´æÔÚ  return false
            if (typeof(cyol_wndbb_data) == "undefined") {
                console.log("cyol_wndbb_data²»´æÔÚ");
                return false;
            }

            //cyol_wndbb_dataÊý¾Ý¶ÔÏó²»´æÔÚ  return false
            if (!cyol_wndbb_data.hasOwnProperty("yesterday") ||!cyol_wndbb_data.hasOwnProperty("today") || !cyol_wndbb_data.hasOwnProperty("tomorrow") || !cyol_wndbb_data.yesterday.hasOwnProperty("data") || !cyol_wndbb_data.today.hasOwnProperty("data") || !cyol_wndbb_data.tomorrow.hasOwnProperty("data")) {
                console.log('cyol_wndbb_dataÊý¾Ý¶ÔÏó²»´æÔÚÇë¼ì²é');
                return false;
            }
            //baobaoµÄjsonÊý¾ÝÎª¿Õ  return false
            if ($.isEmptyObject(cyol_wndbb_data)) {
                console.log("cyol_wndbb_data:" + cyol_wndbb_data);
                console.log("Íâ²¿¼ÓÔØjsÊý¾ÝÎª¿Õ");
                return false;
            }

            //³õÊ¼»¯ÊÓÆµÁÐ±í             
            handler.init(cyol_wndbb_data);
            //µã»÷activeÊÓÆµÁÐ±íli±êÇ©£¬´´½¨aliplayer²¥·ÅÆ÷
            $('ul.todayUl li.active').click();
            //ÊÓÆµÁÐ±í³õÊ¼Î»ÖÃ
