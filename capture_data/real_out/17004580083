      react.src = 'https://static.canalrcndigital.com/CanalRCN/react/js/react.17.production.min.js';
      const reactDom = document.createElement( 'script' );
      reactDom.src = 'https://static.canalrcndigital.com/CanalRCN/react/js/react-dom.17.production.min.js';

      div.addEventListener( "load", function ( event ) {
        if ( event.target.nodeName === "SCRIPT" && event.target.getAttribute( "src" ).includes( 'react.17.production.min.js' ) ) {
          //console.log( "Script loaded: " + event.target.getAttribute( "src" ) );
          div.appendChild( reactDom );
        }
        if ( event.target.nodeName === "SCRIPT" && event.target.getAttribute( "src" ).includes( 'react-dom.17.production.min.js' ) ) {
          //console.log( "Script loaded: " + event.target.getAttribute( "src" ) );
          loadProgramacion.setCronStatusProgramacion();
        }
      }, true );

      div.appendChild( react );
    },
    setCronStatusProgramacion: function () {
      this.loadProgramacion();
      setInterval( this.loadProgramacion, ( 5 * 60 ) * 1000 );
    },
    loadProgramacion: async function () {
      const datos = await this.fetchData();

      if ( datos ) {
        const divs = {};
        const padre = document.getElementById( 'presentamos' );

        divs.programacion = React.createElement( React.Fragment,
          null,
          React.createElement( this.Presentando, {
            parrilla: datos.list
          } ),
          React.createElement( this.Sigue, {
            parrilla: datos.list
          } ),
          React.createElement( this.Tarde, {
            parrilla: datos.list
          } )
        );

        ReactDOM.unmountComponentAtNode( padre );
        const res = ReactDOM.render( divs.programacion, padre );
      }
    },
    fetchData: async function () {
      try {
        const time = new Date().getTime();
        const res = await fetch( 'https://objects.canalrcn.com/canalrcn/servicios/parrilla.json?' + time );

        if ( res.status === 200 ) {
          return await res.json();
        } else {
          return null;
        }
      } catch ( error ) {
        return null;
      }
    },
    Presentando: function ( { parrilla } ) {

      const [ lista ] = parrilla;

      if ( !lista ) {
        return '';
      }

      return React.createElement( "div", {
        className: "presentando"
      }, React.createElement( "div", null,
        React.createElement( 'a', {
          href: 'https://www.canalrcn.com/senal-en-vivo/'
        },
          React.createElement( "p", null, "Estamos presentando" ),
          React.createElement( "span", {
            className: "linea"
          }, "" ),
          React.createElement( "div", {
            className: "programa"
          }, lista.programa.title ) ),
        React.createElement( "a", {
          className: 'btn-canal_envivo',
          href: 'https://www.canalrcn.com/senal-en-vivo/'
        }, React.createElement( 'button', {
          className: 'btn',
          type: 'button',
          title: 'SeÃ±al en vivo'
        }, 'Ver en vivo', React.createElement( 'i', {
          className: 'bi bi-broadcast-pin'
        } ) ) ) ) );

    },
    Sigue: function ( { parrilla } ) {

      const [ , lista ] = parrilla;

      if ( !lista ) {
        return '';
      }

      return React.createElement( "div", {
        className: "sigue"
      }, React.createElement( "div", null,
        React.createElement( "p", null, "Sigue" ),
        React.createElement( "span", {
          className: "linea"
        }, "" ),
        React.createElement( "div", {
          className: "programa"
        }, lista.programa.title ),
        React.createElement( 'div', {
          className: 'hora'
        }, lista.programa.hora ) ) );
    },
    Tarde: function ( { parrilla } ) {

      const [ , , lista ] = parrilla;

      if ( !lista ) {
        return '';
      }

      return React.createElement( "div", {
        className: "mas-tarde"
      }, React.createElement( "div", null,
        React.createElement( "p", null, "MÃ¡s tarde" ),
        React.createElement( "span", {
          className: "linea"
        }, "" ),
        React.createElement( "div", {
          className: "programa"
        }, lista.programa.title ),
        React.createElement( 'div', {
          className: 'hora'
        }, lista.programa.hora ) ) )
    },
  };


  window.onload = loadProgramacion.init();

