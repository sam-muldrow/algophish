    <!-- Piwik -->
<script type="text/javascript">
    var _paq = _paq || [];
    _paq.push(['trackPageView']);
    _paq.push(['enableLinkTracking']);
    _paq.push(['enableHeartBeatTimer', 30]);
    _paq.push(['trackAllContentImpressions']);
    (function() {
      var u="//ma.sabay.com/";
      _paq.push(['setTrackerUrl', u+'piwik.php']);
      _paq.push(['setSiteId', 1]);
      var d=document, g=d.createElement('script'), s=d.getElementsByTagName('script')[0];
      g.type='text/javascript'; g.async=true; g.defer=true; g.src=u+'piwik.js'; s.parentNode.insertBefore(g,s);
    })();
</script>
<noscript><p><img src="//ma.sabay.com/piwik.php?idsite=1" style="border:0;" alt="" /></p></noscript>
<!-- End Piwik Code -->
<script>
    AOS.init({
        duration: 1500,
        easing: 'ease',
        once: true
    });

    $(function(){
        var params = new window.URLSearchParams(window.location.search);
        var slug = params.get('slug');
        $('#'+slug).trigger('click');
        $('#'+slug).find('img').removeClass('hide');
        
        if ($('.portfolio-link').hasClass('active')){
            $('.portfolio-link').not($('#'+slug)).find('img').addClass('hide');
        }

        $('.portfolio-link').click(function(){
            $(this).find('img').removeClass('hide');

            if ($('.portfolio-link').hasClass('active')){
                $('.portfolio-link').not(this).find('img').addClass('hide');
            }
        });

        $('.nav-link').click(function(){

            if ($(this).attr('data-toggle') != 'collapse') {

                $('.sub-menu').addClass('collapse').removeClass('show');

                if ($('#navbarNav').hasClass('show')) {
                    // console.log($('#navbarNav').hasClass('show'));
                    // $('#navbarNav').toggleClass('show');
                    $('.navbar-toggler').trigger('click');
                }
            }
        });

        var rellax = new Rellax('.rellax', {
            center: true
        });
        var lazyLoadInstance = new LazyLoad({
            elements_selector: ".lazy"
            // ... more custom settings?
        });
        
        $('.portfolio-item').hover(function(){
            var img = $(this).find('img');
            img.toggleClass('hide');
        });

        $("#carouselExampleControls").swipe({
            swipe: function (event, direction, distance, duration, fingerCount, fingerData) {
                if (direction == 'left') jQuery(this).carousel('next');
                if (direction == 'right') jQuery(this).carousel('prev');
            },
            allowPageScroll: "vertical" 
        });
        
        var photo_wrapper = $('.photos');
        $.each(photo_wrapper, function(index, value) {
            var id = $(this).attr('id');
            var imgs = $("#"+id).find('img');
            var ran_src = 0;
            for(i = 0; i < imgs.length; i++) {
                ran_src = Math.floor(Math.random() * imgs.length);
                break;
            }
            $("#"+id).prepend(imgs[ran_src]);            
        });

        $('.pf-action').click(function(){
            var has_down_ico = $(this).hasClass('fa-arrow-down');
            var has_up_ico = $(this).hasClass('fa-arrow-up');
            if (has_down_ico) {
                $(this).removeClass('fa-arrow-down').addClass('fa-arrow-up');
            }
            if (has_up_ico) {
                $(this).removeClass('fa-arrow-up').addClass('fa-arrow-down');
            }
        });

        $('.profile').click(function(){
            var i = $(this).parent().find('i');
            i.trigger('click');
        });

        $('.btn-profile').click(function(){
            var i = $(this).parent().find('i');            
            i.trigger('click');
        });
    });
    
    var lastScrollTop = 0;
    is_scrolled = true;
    $( window ).scroll(function(event) {
        var st = $(this).scrollTop();
        // var nav_height = $('.navbar.desktop').height();

        // if (st > lastScrollTop) {
        //     $('.navbar.desktop').css('top', -150);
        // } else {
        //     $('.navbar.desktop').css('top', 0);
        // }

        // lastScrollTop = st;

        var slider_height = $('.slider-section').height();
        if (st > slider_height) {
            $('#nv-scrollup').css('display', 'block');
        } else {
            $('#nv-scrollup').css('display', 'none');
        }
    });
    
</script>
    
</body>

</html>