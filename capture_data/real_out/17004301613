	<script>
		let adsObserver;

		function handleIntersectionAds(entries) {
			entries.map((entry) => {
				if (entry.isIntersecting) {
					adsObserver.unobserve(entry.target);
					
					const urlToHit = "https://www.myfonts.com?section_id=ads_sec";
					
					$.ajax({url: urlToHit, success: function(result){
						$("#_adshome").html(result);

						const scriptForAds = document.querySelector('#_scriptforads').getAttribute('script-data');

						const _adsscriptDiv = document.querySelector('#_adsscript');

						if (_adsscriptDiv && scriptForAds !== 3 && scriptForAds !== '') {
							if (scriptForAds.indexOf('<script>') > -1) {
								$('head').append(scriptForAds);
							}
							else {
								const scriptTag = document.createElement('script');

								scriptTag.innerHTML = scriptForAds;
								scriptTag.type = 'text/javascript';

								_adsscriptDiv.appendChild(scriptTag);
							}
						}
					}});  
				}
			})
		}

		const adsObserverIntersectionApi = {
			root: null,
			rootMargin: '0px',
			threshold: 0.02
		}

		const adsDiv = document.querySelector('#_adshome');

		if (adsDiv) {
			adsObserver = new IntersectionObserver(handleIntersectionAds, adsObserverIntersectionApi);
			adsObserver.observe(document.querySelector('#_adshome'));
		}
	</script>
