<script>
  let footerObserver;

  function handleIntersectionFooter(entries) {
    entries.map((entry) => {
      if (entry.isIntersecting) {
        footerObserver.unobserve(entry.target);

        const urlToHit = 'https://www.myfonts.com?section_id=footer';
        $.ajax({
          url: urlToHit,
          success: function (result) {
            $('#_footerhome').html(result);
            $('#_footerhome').css('height', 'max-content');
          }
        });
      }
    });
  }

  const footerObserverIntersectionApi = {
    root: null,
    rootMargin: '0px',
    threshold: 0.02
  };

  const footerDiv = document.querySelector('#_footerhome');

  if (footerDiv) {
    footerObserver = new IntersectionObserver(handleIntersectionFooter, footerObserverIntersectionApi);
    footerObserver.observe(document.querySelector('#_footerhome'));
  }
</script>

    
    
