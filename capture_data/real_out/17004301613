    $(document).ready(function(){
      // Initalize slick-slide
      $('#homecarousel > div').slick({
        dots: true,
        infinite: true,
        autoplay: true,
        draggable: true,
        initialSlide: 0,
        beforeChange: showAriaHiddenTiles(),
        afterChange: showAriaHiddenTiles(),
        speed: 1000,
        slidesToShow: 3,
        slidesToScroll: 3,
        nextArrow: '<button class="slick-arrow slick-next carousel-arrow arrow-right" style="display: block"></button>',
        prevArrow: '<button class="slick-arrow slick-prev carousel-arrow arrow-left" style="display: block"></button>',
        responsive: [
          {
            breakpoint: 1024,
            settings: {
              slidesToShow: 2,
              slidesToScroll: 2,
              dots: true
            }
          },
          {
            breakpoint: 600,
            settings: {
              slidesToShow: 1,
              slidesToScroll: 1
            }
          },
          {
            breakpoint: 480,
            settings: {
              slidesToShow: 1,
              slidesToScroll: 1
            }
          }
        ]
      });

      function showAriaHiddenTiles() {
        setInterval(() => {
          Array.from(document.querySelectorAll(".slick-slide")).forEach((slide) => {
            slide.setAttribute("aria-hidden", "false");
          });
        }, 100);
      }

      $(".homecarousel-container a").each(function() {
        let linkHref = $(this).attr("href").trim();
        let locale = $('html').attr('lang');
        if((linkHref.indexOf("/collections/") || linkHref.indexOf("/products/")) && locale !== "en" && linkHref.indexOf(`/${locale}/`) == -1){
          $(this).attr("href",`/${locale}${linkHref}`);
        }
      });

      window.favorites = {};
      /* Storing user's device details in a variable */
      const details = navigator.userAgent;
      /* Creating a regular expression
      containing some mobile devices keywords
      to search it in details string */
      const regexp = /android|iphone|kindle|ipad/i;
      /* Using test() method to search regexp in details
      it returns boolean value */
      const isMobileDevice = regexp.test(details);
      window.favorites.onHoverMakeItPink = function () {
        if (!isMobileDevice) {
          window.event.target.classList.add("fav_hover");
        }
      };
      window.favorites.onRemoveMakeItGray = function () {
        if (!isMobileDevice) {
          window.event.target.classList.remove("fav_hover");
        }
      };
      window.favorites.addFavorites = function (prodHandle, prodType, customerIDFav, element) {
        window.event.preventDefault();

        // For logged out users store handles in cookie
        if (!customerIDFav && prodHandle) {
          setSessionStorageCollection(prodHandle);
          if (element.href) {
            window.location.replace(element.href);
          }
          return;
        }

        // add to favorites
        $(".toast-wrapper").remove();
        const oldalbumname = "";
        const albumname = "";
        const albumdescription = "";
        const customerid = customerIDFav;
        const favfont = prodHandle;
        const favtype = prodType;
        const fontlist = { id: favfont, type: favtype };
        let data_action = "";
        const fieldvalue = [{ id: favfont, type: favtype }];
        // const buttonId = favbuttonid;
        // const element = window.event.target;
        // const current_class = element.className.replace("searchBarWidget_filter filter albumheart favourite-btn ", "");
        if (element && customerid) {
          if (element.classList.contains("favorites-icon-inactive")) {
            data_action = "Add";
            element.classList.remove("favorites-icon-inactive");
            element.classList.add("favorites-icon-active");
            updateFavorites(
              customerid,
              oldalbumname,
              albumname,
              albumdescription,
              fontlist,
              "customer",
              data_action,
              "json",
              "favorites",
              fieldvalue
            );
            const toastMess = document.querySelector(".toast-message");
            const msgToShow = "Font added to Favorites";
            if (!toastMess || (toastMess && toastMess.innerHTML.trim() !== msgToShow)) {
              messageService.displayToast(msgToShow, 5000);
            }
          } else {
            data_action = "Remove";
            element.classList.remove("favorites-icon-active");
            element.classList.add("favorites-icon-inactive");
            updateFavorites(
              customerid,
              oldalbumname,
              albumname,
              albumdescription,
              fontlist,
              "customer",
              data_action,
              "json",
              "favorites",
              fieldvalue
            );
            const tmsg = 'Font removed from Favorites. <a href="">Undo</a>';
            messageService.displayToast(tmsg, 5000);
          }
        }
      };

      function getSessionStorageCollection() {
        let itemsInSessionStorage = sessionStorage.getItem("onLoginAddToFav");
        if (!itemsInSessionStorage) {
          itemsInSessionStorage = [];
        } else {
          itemsInSessionStorage = itemsInSessionStorage.split(",");
        }
        return itemsInSessionStorage;
      }
    
      function setSessionStorageCollection(handle) {
        const itemsInSessionStorage = getSessionStorageCollection();
        if (!itemsInSessionStorage.includes(handle)) {
          itemsInSessionStorage.push(handle);
        }
        sessionStorage.setItem("onLoginAddToFav", itemsInSessionStorage.join(","));
        window.setTrue = true;
      }
    
      function removeSessionStorageCollection(handle) {
        const itemsInSessionStorage = getSessionStorageCollection();
        const index = itemsInSessionStorage.indexOf(handle);
        if (index > -1) {
          itemsInSessionStorage.splice(index, 1);
        }
        if (itemsInSessionStorage.length === 0) {
          sessionStorage.removeItem("onLoginAddToFav");
        } else {
          sessionStorage.setItem("onLoginAddToFav", itemsInSessionStorage.join(","));
        }
      }

      // Add session favorites after login
      let add_fav_from_session = getSessionStorageCollection();
      if (add_fav_from_session.length > 0) {
        add_fav_from_session.forEach(function (handle) {
          const item = document.getElementById(`fav-${handle}`);
          // check if item is already a favorite
          if (item && item.classList.contains("favorites-icon-inactive")) {
            item.click();
          }
          removeSessionStorageCollection(handle);
        });
      }

      window.favorites.addToAlbum = function (prodHandle, prodType, rotate_counter) {
        // For logged out users store handles in cookie
        const customerIDFav = ''
        if (!customerIDFav && prodHandle) {
          let albumHref = addLocaleToURL('/account/login');
          window.location.replace(albumHref);
          return;
        }

        let targetOffsettop = 0;
        let targetOffsetright = 0;
        const e = window.event;
        const target = $(e.target);
        targetOffsettop = target.offset().top + 20;
        targetOffsetright = $(window).width() - target.offset().left - target.outerWidth() - 10;
        const albumsforproduct = [];
        const presentalbums = document.getElementById("current_albums").value;
        const albumarray = JSON.parse(document.getElementById("current_albums").value);
        const albumproduct = prodHandle;
        const albumproducttype = prodType;
        const arrow = document.getElementById(rotate_counter).style;
        const csstext = arrow.cssText;

        if (csstext === "transform: rotate(-180deg);") {
          arrow.transform = "rotateY(180deg)";
        } else {
          arrow.transform = "rotate(-180deg)";
        }
        if (presentalbums !== "null") {
          albumarray.forEach((item) => {
            const arraylist = item && item.list ? item.list : [];
            if (document.getElementById(item.name)) {
              document.getElementById(item.name).checked = false;
            }
            if (arraylist && arraylist.length > 0) {
              arraylist.forEach((itemlist) => {
                if (itemlist.id === albumproduct) {
                  albumsforproduct.push(item.name);
                }
              });
            }
          });
        }
        document.getElementById("current_product").value = albumproduct;
        document.getElementById("current_product_type").value = albumproducttype;
        const element = document.getElementById("slider_controls");
        const style = window.getComputedStyle(element);
        const display = style.getPropertyValue("display");
        $('#homecarousel > div').slick('slickPlay');
        if (display === "none") {
          $('#homecarousel > div').slick('slickPause');
          $(".album_sidebar_controls").addClass("classalbumhonpeage");
          $(".album_sidebar_controls").css("display", "block");
          $(".album_sidebar_controls").css("position", "absolute");
          $(".album_sidebar_controls").css("right", targetOffsetright);
          $(".album_sidebar_controls").css("top", targetOffsettop);
          if (presentalbums !== "null") {
            albumsforproduct.forEach((itemcheck) => {
              document.getElementById(itemcheck).checked = true;
            });
          }
        } else {
          $(".album_sidebar_controls").css("display", "none");
        }
        $(".album_list_first").css("display", "none");
        $(this).siblings(".album_sidebar_controls").slideToggle(500);
      };

      function createAlbum() {
        $(".album_button_left").on("click", function () {
          $(".toast-wrapper").hide();
          const oldalbumname = "";
          const albumname = document.getElementById("newalbum").value;
          const albumdescription = "";
          const customerid = "";
          const albumproduct = document.getElementById("current_product").value;
          const albumproducttype = document.getElementById("current_product_type").value;
          const data_action = "Createalbum";
          const fontlist = { id: albumproduct, type: albumproducttype };
          const fieldvalue = [{ id: albumproduct, type: albumproducttype }];
          // eslint-disable-next-line no-unused-vars
          const ret_val = updateAlbum(
            customerid,
            oldalbumname,
            albumname,
            albumdescription,
            fontlist,
            "customer",
            data_action,
            "json",
            "albums",
            fieldvalue
          );
          $(".album_button_right").attr("data-message", "New Album Created");
          $(".album_list_first").css({"display":"flex","align-items": "center","gap":"10px"});
          $(".album_button_right").css({"display":"flex","align-items": "center","justify-content": "end", "color": "#1A73E8","padding-right": "3px"});
          $(this).css("cursor", "arrow");
          $(".album_button_left").off("click");
        });
      }
      createAlbum();

      $(".albumcheck").change(function () {
        $(".toast-wrapper").hide();
        if (this.checked) {
          const data_action = "Addstyle";
          const oldalbumname = $(this).data("checkalbumname");
          const newalbumname = $(this).data("checkalbumname");
          const albumproduct = document.getElementById("current_product").value;
          const albumproducttype = document.getElementById("current_product_type").value;
          const newalbumdescription = "";
          const customerid = $(this).data("checkcustomer");
          const fontlist = { id: albumproduct, type: albumproducttype };
          const fieldvalue = [{ id: albumproduct, type: albumproducttype }];
          if (albumproduct !== "") {
            updateAlbum(
              customerid,
              oldalbumname,
              newalbumname,
              newalbumdescription,
              fontlist,
              "customer",
              data_action,
              "json",
              "albums",
              fieldvalue
            );
            $(".album_button_right").attr("data-message", "Font added to album");
            messageService.displayToast("Font added to album", 3000);
          }
        } else {
          const data_action = "Removestyle";
          const oldalbumname = $(this).data("checkalbumname");
          const newalbumname = $(this).data("checkalbumname");
          const albumproduct = document.getElementById("current_product").value;
          const albumproducttype = document.getElementById("current_product_type").value;
          const newalbumdescription = "";
          const customerid = $(this).data("checkcustomer");
          const fontlist = { id: albumproduct, type: albumproducttype };
          const fieldvalue = [{ id: albumproduct, type: albumproducttype }];
          if (albumproduct !== "") {
            updateAlbum(
              customerid,
              oldalbumname,
              newalbumname,
              newalbumdescription,
              fontlist,
              "customer",
              data_action,
              "json",
              "albums",
              fieldvalue
            );
            $(".album_button_right").attr("data-message", "Font removed from albums");
            messageService.displayToast("Font removed from albums", 3000);
          }
          const currLoc = $(location).attr("href");
          if (window.location.href.indexOf("album-details") > -1) {
            setTimeout(() => {
              window.location.replace(currLoc);
            }, 1500);
          }
        }
      });

      function addToDataLayerCarousel(dataToSave) {
        if (dataToSave && dataToSave.length > 0) {
          const { DataController } = window;
          if (DataController) {
            DataController.push({
              event: "eec_impression",
              eventAct: "product impression",
              nonInteraction: true,
              ecommerce: {
                impressions: dataToSave
              }
            });
          }
        }
      }

      let data_to_save = `{"brand": "Deveze Type", "category": "family", "name": "DT Augustina Slab", "list": "DT Augustina Slab", "id": "300718424234"},{"brand": "Avondale Type Co.", "category": "family", "name": "ATC Duel", "list": "ATC Duel", "id": "296996962474"},{"brand": "Set Sail Studios", "category": "family", "name": "Mangrove", "list": "Mangrove", "id": "300860014762"},{"brand": "Julien Fincker", "category": "family", "name": "Grantig", "list": "Grantig", "id": "300890849450"},{"brand": "Grype", "category": "family", "name": "PowerUp", "list": "PowerUp", "id": "300860047530"},{"brand": "Great Studio", "category": "family", "name": "Colagent", "list": "Colagent", "id": "300780585130"},`;
      if (data_to_save.length > 0) {
        try {
          let save_data = data_to_save.slice(0, -1);
          data_to_save = JSON.parse(`[${save_data}]`);
          for (let i = 0; i < data_to_save.length; i++) {
            data_to_save[i].position = i + 1;
          }
          addToDataLayerCarousel(data_to_save);
        } catch (e) {
          console.log('addToDataLayerCarousel error: ', e);
        }
      }

      window.favorites.addToDataLayerCarouselProductClick = function (brand, name, id, category, url) {
        window.event.preventDefault();
        const itemDataWhichIsClicked = {};
        itemDataWhichIsClicked.brand = brand;
        itemDataWhichIsClicked.name = name;
        itemDataWhichIsClicked.id = id;
        itemDataWhichIsClicked.category = category;
        if (Object.keys(itemDataWhichIsClicked).length > 0) {
          const dataToSaveOnClick = {
            event: "eec_click",
            eventAct: "product click",
            nonInteraction: false,
            eventLab: name,
            ecommerce: {
              click: {
                actionField: { list: name },
                products: [itemDataWhichIsClicked]
              }
            },
            // eslint-disable-next-line object-shorthand
            eventCallback: function () {
              url = addLocaleToURL(url);
              window.location.href = url;
            }
          };
          const { DataController } = window;
          if (DataController) {
            console.log(dataToSaveOnClick);
            DataController.push(dataToSaveOnClick);
          }
        }
      }
    });




