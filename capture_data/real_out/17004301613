<script type="application/ld+json">
  {
    "@context": "http://schema.org",
    "@type": "Organization",
    "name": "MyFonts",
    
    "sameAs": [
      "",
      "",
      "",
      "",
      "",
      "",
      "",
      ""
    ],
    "url": "https:\/\/www.myfonts.com"
  }
</script>
<script>
  $(document).ready(function(){
    let redirectUri =`https://auth-api.monotype.com/api/handlelogin?params=mftprod___mftprod___`;
    let pageOrigin = window.location.origin;
    let locale = $('html').attr('lang');
    if (locale !== 'en') {
      pageOrigin = `${pageOrigin}/${locale}/`;
    }
    if($("#mainsignupLink").length){
      let mainsignupHref = $("#mainsignupLink").attr("href");
      mainsignupHref = myfonts.handleAuthZeroLocales(mainsignupHref);
      mainsignupHref = mainsignupHref.replaceAll(redirectUri, `${redirectUri}${btoa(pageOrigin)}`);
      $("#mainsignupLink").attr("href", mainsignupHref);
    }
    if($("#signupLink").length){
      let linkHref = $("#signupLink").attr("href");
      linkHref = myfonts.handleAuthZeroLocales(linkHref);
      linkHref = linkHref.replaceAll(redirectUri, `${redirectUri}${btoa(pageOrigin)}`);
      $("#signupLink").attr("href", linkHref);
    }
    if($("#resend_email").length){
      let linkHref = $("#resend_email").attr("data-href");
      linkHref = myfonts.handleAuthZeroLocales(linkHref);
      $("#resend_email").attr("data-href", linkHref);
    }
    $("#headerCustomerMenu a").each(function(){
      let urlHref = $(this).attr("href");
      let logoutUrl = "/account/logout";
      if(urlHref.indexOf("/v2/logout") !== -1){
        if(urlHref.indexOf(logoutUrl)) {
          urlHref = urlHref.replaceAll(logoutUrl,addLocaleToURL(logoutUrl));
        }
        else{
          urlHref = `${urlHref}&returnTo=https://${Shopify.shop}${addLocaleToURL("/account/logout")}`;
        }
        $(this).attr("href", urlHref);
      }
      else if(urlHref.indexOf("monotype.com") !== -1){
        $(this).attr("target", "_blank");
        if(urlHref.indexOf("locale=en") !== -1 && myfonts.locale !== "en"){
          urlHref = urlHref.replace(`locale=en`,`locale=${myfonts.locale}`);
          $(this).attr("href", urlHref);
        }
        if(urlHref.indexOf("/en/") !== -1 && myfonts.locale !== "en"){
          $(this).attr("target", "_blank");
          urlHref = urlHref.replace(`/en/`,`/${myfonts.locale}/`);
          $(this).attr("href", urlHref);
        }
      }
      
    });
    $(".current_locale").on("click", function() {
        event.preventDefault();
        var asideElement = $(".aside_country_selector");
        
        if (asideElement.hasClass("open")) {
            asideElement.removeClass("open");
            asideElement.removeClass("weg-openleft");

            asideElement.addClass("close");
        } else {
            asideElement.removeClass("close");
            asideElement.addClass("open");
            asideElement.addClass("weg-openleft");
        }
           
    $(".locale-list").toggle();
      });
  });
  $(window).scroll(function() {
    if ($(window).scrollTop() >= 30) {
      $('#shopify-section-header').addClass('fixed-header-mob');
      $('body').addClass('fixed-scroll');
    } else {
      $('#shopify-section-header').removeClass('fixed-header-mob');
      $('body').removeClass('fixed-scroll');
    }
  });

</script>
