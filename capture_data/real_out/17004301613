      <script>
        let specialOffersObserver;
      
        function handleIntersection(entries) {
          entries.map((entry) => {
            if (entry.isIntersecting) {
              specialOffersObserver.unobserve(entry.target);
              const urlToHit = "https://www.myfonts.com?section_id=homespecialoffers";
              
              const slideControlDiv = document.querySelector('#slider_controls');
              if (slideControlDiv) {
                const algoliaScript = document.createElement('script');
                const instantSearchScript = document.createElement('script');
                
                // SHOPIFY-4269 | Removed the CDN and serving these libary from local file
                algoliaScript.setAttribute('src',"//www.myfonts.com/cdn/shop/t/172/assets/algoliasearch-lite.umd@4.10.5.js?v=11686513328097798801700115234");
                instantSearchScript.setAttribute('src',"//www.myfonts.com/cdn/shop/t/172/assets/instantsearch@4.29.1.js?v=50856079152394041111700115236");
                
                slideControlDiv.appendChild(algoliaScript);
                slideControlDiv.appendChild(instantSearchScript);
              }
              $.ajax({url: urlToHit, success: function(result){
                $("#_specialoffershomepage").html(result);
              }});
            }
          })
        }
        const specialOffersIntersectionApi = {
          root: null,
          rootMargin: '0px',
          threshold: 0.05
        }
        const specialOfferDiv = document.querySelector('#_specialoffershomepage');
        if (specialOfferDiv) {
          specialOffersObserver = new IntersectionObserver(handleIntersection, specialOffersIntersectionApi);
          specialOffersObserver.observe(document.querySelector('#_specialoffershomepage'));
        }
