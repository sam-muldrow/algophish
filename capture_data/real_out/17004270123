<script src="/resources/theme/global/js/vendor/prayertimes.min.js"></script>
<script>
function setPrayerTimes(times) {
									var $list = $("ul.prayer-time");
									$list.find("[data-type=fajr] .time").text(times.fajr);
									$list.find("[data-type=dhuhr] .time").text(times.dhuhr);
									$list.find("[data-type=maghrib] .time").text(times.maghrib);
								}
								$.ajax({
									type: 'head'
									, success: function (data, textStatus, request) {
										var serverDate = request.getResponseHeader('Date');
										var d = new Date(serverDate);
										prayTimes.setMethod('Tehran');
										var $select = $(".select-city select:first");
										var coordinations = $select.val().split(',');
										var times = prayTimes.getTimes(d, [coordinations[0], coordinations[1]], 3.5);
										setPrayerTimes(times);
										$(document).on('change', ".select-city select:first", function (e) {
											var coordinations = $select.val().split(',');
											var times = prayTimes.getTimes(d, [coordinations[0], coordinations[1]], 3.5);
											setPrayerTimes(times);
										});

									}
								});
</script>
