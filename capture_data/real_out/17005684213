<script>
  $(document).ready(function() {
    $('body').on('click', '#logout', function(ev) {
      var date = new Date();
      var expiryDays = parseInt(UL.COOKIE_EXIPIRY_IN_DAYS);
      date.setTime(date.getTime() + (expiryDays * 24 * 60 * 60 * 1000));
  
      ulHelper.eraseCookie("ul_user_traits");
      ulHelper.createCookie("ul_auth_status", '0', date);
      window.location.href = "/logout";
    });
    $('.header__topBarIconList').on('click', 'a', function(event){
      try {
        var ele = event.currentTarget
        var iconName = ele.id.replace(/^header-icon-/, '')
        var badgeNumber = 0
        if(iconName == "wishlist") {
          badgeNumber = parseInt($(ele).find('#shortlist-badge')['0'].innerText)
        } else if(iconName == "cart"){
          badgeNumber = parseInt($(ele).find('#cart-badge')['0'].innerText)
        }
        UL.Ecommerce.headerIconClicked({iconName, badgeNumber});
      } catch(err) {
        console.log("Error in adding headerIconClicked event ", err.message)
      }
    });
  });
</script>

