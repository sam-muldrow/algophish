<script type="text/javascript">
(function() {
  function setCookie(event) {
    // disable all cookie buttons
    document.querySelectorAll('.options-button').forEach(item => {
      item.setAttribute('disabled', 'disabled');
    });

    // Set cookie preferences
    let preferences = Context.get('preferences');
    const selection = event.target.dataset.option;
    preferences.privacy.cookies = selection;
    // if user previously opted out of sale of their info but selects all cookies
    // default to `doNotSell: false`
    if (preferences.privacy.doNotSell && selection === 'all') {
      preferences.privacy.doNotSell = false;
    }
    Context.setPreferences(preferences);
    window.location.reload();
  }

  window.addEventListener('load', function() {
    const currentSetting = Context.get('preferences').privacy.cookies;
    if (!['all', 'essential'].includes(currentSetting)) {
      document.querySelector('#privacy-options').classList.remove("m-privacy-options__hidden");
    }
  });

  document.querySelectorAll('.options-button').forEach(item => {
    item.addEventListener('click', event => {
     setCookie(event);
    });
  });

})();
</script>

    
