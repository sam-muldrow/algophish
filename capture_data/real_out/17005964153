	<script>
	$(".section.tracked li a").click( function() {	       
		//ga('send', 'event', 'Section Element', 'click', $(this).parents("ul").siblings("h2.title").text(), 1.00, {nonInteraction: true});
		var label = '<empty>';
		if ( $(this).parents("ul").siblings("h2.title").length){
             
			label =  $(this).parents("ul").siblings("h2.title").text();
		}
		else if ( $(this).parents("ul").siblings("h1").length) { // Flash News
			label = $(this).parents("ul").siblings("h1").text();
		}
      //ga('send', 'event', 'Section Element', 'click', label, 1.00, {nonInteraction: true});
      gtag('event', 'click', {'event_category': 'Section Element','event_label': label,'value': 1.00}); 
	});
	$("i.fa.fa_arrow").click( function() {	       
		// console.log("Top clicked");
      //ga('send', 'event', 'Top Link', 'click',  'Link');
      gtag('event', 'click', {'event_category': 'Top Link','event_label': 'Link'});
	});
  
    $("i.fa.fa_social").click( function() {	       
      //console.log("Article Scoial Icon");
      //ga('send', 'event', 'Article Share Icon Click', 'click',  'Link');
      gtag('event', 'click', {'event_category': 'Article Share Icon Click','event_label': 'Link'});      
	}); 
     $("a#classifieds").click( function() {	       
      //console.log("Classifieds");
       //ga('send', 'event', 'Classifieds', 'click',  'Link');
       gtag('event', 'click', {'event_category': 'Classifieds','event_label': 'Link'});
	}); 
</script>

