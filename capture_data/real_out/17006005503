<script>
    // SORT
    $(document).ready(function () {
        SwitchLayout();
    });

    function SwitchLayout() {
        if ($('#thisContent').is(":visible")) {
            $('#thisPlace').append($('#thisContent'));
        }
    }

    // BOTTOMNAV
    $(document).ready(function () {
        SwitchBottom();
    });

    function SwitchBottom() {
        if ($('#bottomNav').is(":visible")) {
            $('#thisPlaceBottom').append($('#bottomNav'));
        }
    }

    // SORTING WIDTH
    $(document).ready(function () {
        $('.sidebar-scroll').css('height', $('.leftsidebar').height());
        $(window).resize(function () {
            $('.sidebar-scroll').css('height', $('.leftsidebar').height());
        });
        $('.sort-bar').css('width', $('#sectionWidth').width());
        $(window).resize(function () {
            $('.sort-bar').css('width', $('#sectionWidth').width());
        });
    });
</script>





