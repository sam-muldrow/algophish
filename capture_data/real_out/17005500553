	<script type="text/javascript">

	 $(function() {
	     var tgt    = $('#hoge'),
		 tgtOffset = tgt.offset();
	     $(window).scroll(function(){
		 if($(window).scrollTop() > tgtOffset.top - 30) {
		     tgt.css({   position:"fixed",
				 top: "40px"});
		 }else{
		     tgt.css({   position:"relative",
				 top: 0});
		 }
	     });
	 });

	</script>


