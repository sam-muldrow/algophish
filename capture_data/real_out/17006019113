        </script><script data-esi="head-analytics" data-esi-type="static">
            function getCookie(cname) {
                var name = cname + '=';
                var decodedCookie = decodeURIComponent(document.cookie);
                var ca = decodedCookie.split(';');
                for (var i = 0; i < ca.length; i++) {
                    var c = ca[i];
                    while (c.charAt(0) == ' ') {
                        c = c.substring(1);
                    }
                    if (c.indexOf(name) == 0) {
                        return c.substring(name.length, c.length);
                    }
                }
                return '';
            }

            function getVisitorLoginState(langKey) {
                if (getCookie('FlyFE_' + langKey + '_user') != '') {
                    return true;
                }
                return false;
            }

            var localeCookie = getCookie('FA_COMMERCE_LOCALE');
            var pathArray = window.location.pathname.split('/');

            if (googleTagManagerLocaleVars[pathArray[1]] !== undefined && !localeCookie.toLowerCase().includes('_' + pathArray[1])) {
                var locale = [Object.keys(googleTagManagerLocaleVars[pathArray[1]]['optimize'])[0], pathArray[1]];
            } else {
                var locale = localeCookie.split('_').map((name) => name.toLowerCase());
            }

            var initState = googleTagManagerLocaleVars[locale[1]];
            initState['pageCountry'] = locale[1];
            initState['pageLanguage'] = locale[0];
            initState['pagePool'] = 'shop';
            initState['pageCategory'] = 'other';
            initState['pageType'] = 'other';
            initState['visitorLoginState'] = getVisitorLoginState(initState['pageCountry']);
            initState['dataLayerVersion'] = 'v1.0';

            if ('undefined' !== typeof window.globalPageCategory && 'undefined' !== typeof window.globalPageCategory['pageCategory'] && 'undefined' !== typeof window.globalPageCategory['pageType']) {
                initState['pageCategory'] = window.globalPageCategory['pageCategory'];
                initState['pageType'] = window.globalPageCategory['pageType'];
            }

            /* NEW_CONFIGURATOR_TRACKING_PLACEHOLDER */
            window[googleTagManager['dataLayerName']] = window[googleTagManager['dataLayerName']] || [];
            if (googleTagManager['dataLayerName']) {
                window[googleTagManager['dataLayerName']].push(initState);
            }
