<script language="javascript">
var flgstat='false';
function displayMore() {

if (flgstat=='false') {
	flgstat='true';
	document.getElementById('showmore').style.height='auto';
	document.getElementById('showmore').style.display='block';
	document.getElementById('showmore').style.display='block';
	document.getElementById('txtmore').innerHTML='More - '

}else{
	flgstat='false';

	document.getElementById('showmore').style.display='none';
	document.getElementById('showmore').style.display='none';
	document.getElementById('txtmore').innerHTML='More + ';
}

}


</script>

<script>

    function displayScrollToTop() {

	if (window.pageYOffset>10) {
		document.getElementById('toTop').style.display='block';
	} else {

		document.getElementById('toTop').style.display='none';
	}

    }


    function scrollToTop() {

        window.scrollTo(0, 0);
    }
window.addEventListener("scroll", displayScrollToTop);
</script><div id="mainheadcontainer">
