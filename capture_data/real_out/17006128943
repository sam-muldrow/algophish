<section class="block block--kajgana-block block--kajgana-block-default">

  
  <div class="block__content">
    <div class="views-element-container"><div class="view--metro view view-kajgana-metro view-id-kajgana_metro view-display-id-embed_1 js-view-dom-id-03fa433e2e7d3cd28fc702571e3c55fdb1856565cae9004064eb149d10572056">
  
    
          <div class="attachment attachment-before">
      <div class="views-element-container"><div class="view--metro view view-kajgana-metro view-id-kajgana_metro view-display-id-attachment_1 js-view-dom-id-d8510571db29fb2227bdb503ef298eb1ae588184ddb4269828c3c6c88e72771e">
  
    
      
      <div class="view-content">
          <div><div class="teaser teaser--compact">
