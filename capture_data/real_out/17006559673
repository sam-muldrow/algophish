				<script language="JavaScript" type="text/javascript">

				function change_field(mode)
				{
					switch (mode)
					{
						case 1:
								  document.getElementById('upfile1').style.display = 'inline';
								  document.getElementById('upurl1').style.display = 'none';
								  document.getElementById('upfile2').style.display = 'inline';
								  document.getElementById('upurl2').style.display = 'none';
							break;
						case 2:
								  document.getElementById('upfile1').style.display = 'none';
								  document.getElementById('upurl1').style.display = 'inline';
								  document.getElementById('upfile2').style.display = 'none';
								  document.getElementById('upurl2').style.display = 'inline';
				
							break;
					}
				}
				
				function change_RadioButton(id)
				{
					
					if (document.getElementById(id).checked == true)
					{
						document.getElementById(id).checked = false ;
						document.getElementById('gr_' + id).src= 'includes/img/radio_button1.jpg';
					}
					else
					{
						document.getElementById(id).checked = true;
						document.getElementById('gr_' + id).src= 'includes/img/radio_button2.jpg';
					}
				}
				
				function show_progress()
				{
						document.getElementById("upload_div").style.display  = "none";
						//document.getElementById("progress_div").style.visibility  = "visible";
						document.getElementById("progress_div").style.display = "inherit";
						/* document.getElementById("td_upload_form").submit();*/
				}
				
				var submit=0;
				function ch_f() 
				{
					if (  (document.getElementById("bilddatei").value=='') &&  (document.getElementById("image_link").value=="Die Bild-URL bitte hier einfÃ¼gen" )     && (navigator.appName.indexOf("Internet Explorer") == -1)        )
					{
						submit=1;
						document.getElementById("bilddatei").focus();
						document.getElementById("bilddatei").click();
						return false;
					}}
				
				function a_s()
				{
					  if ((submit==1) && (navigator.appName.indexOf("Internet Explorer") == -1))
					  {
							document.getElementById("thirdupload").focus();
							document.getElementById("f_up_form").submit();
					  }	  
				}
				

				</script>
