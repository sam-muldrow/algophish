<script language="javascript">
	var clear;
	clear=false;
	function clear_key_field(clicked)
	{
		if (document.getElementById("image_keys").value=='z.B.: Hund Katze Maus') 
		{
			document.getElementById(clicked).value="";
			if (clicked == 'image_keys'){
				document.getElementById('suchlist').style.display = "block";
				document.getElementById("button").disabled= false;
			}
			if (clicked == 'image_keys2'){
				document.getElementById("button2").disabled= false;
			}		
			document.getElementById(clicked).style.color="#000000";
			clear = true;
		}
			
	}
	function set_key_fields(clicked)
	{
		if (document.getElementById(clicked).value=='')
		{
			document.getElementById(clicked).value="z.B.: Hund Katze Maus";
			if (clicked == 'image_keys'){
				document.getElementById("button").disabled= true;
			}
			if (clicked == 'image_keys2'){
				document.getElementById("button2").disabled= true;
			}
			document.getElementById(clicked).style.color="#666666";
		}	
		
	}
	function close_key_list(){
		document.getElementById('suchlist').style.display = "none";
	}
</script>

