<script type="text/javascript">
    $(function () {
        var model = new Property24.Account({
            loginUrl: 'https://www.property24.com/User/LoginModal',
            registerUrl: 'https://www.property24.com/User/SignUpModal',
            validateVerificationCodeUrl: 'https://www.property24.com/User/ValidateVerificationCodeModal',
            enableRecaptcha: true,
            resendVerificationCodeUrl: '/User/ResendVerificationCode',
        });

        const $togglePasswordButton = $(".js_togglePasswordModal");
        const $passwordInput = $(".js_loginPasswordModal");
        const $toggleIcon = $(".js_togglePassIconModal");

        $passwordInput.on("input", function () {
            var passwordInput = this.value.trim();
            if (passwordInput !== "") {
                $togglePasswordButton.removeClass("hide"); // Show the password toggle icon
            } else {
                $togglePasswordButton.addClass("hide"); // Hide the password toggle icon
            }
        });

        $togglePasswordButton.on("click", function () {
            if ($passwordInput.attr("type") === "password") {
                $passwordInput.attr("type", "text");
                $toggleIcon.attr("title", "Password display: On");
                $toggleIcon.attr("src", "/Content/images/Optimized/Icons/eye-open.svg?z=fd7b3346e87de4e3bf22");
                $toggleIcon.removeClass("p24_eyeClosed").addClass("p24_eyeOpen");
            } else {
                $passwordInput.attr("type", "password");
                $toggleIcon.attr("title", "Password display: Off");
                $toggleIcon.attr("src", "/Content/images/Optimized/Icons/eye-close.svg?z=7cb6c5204355b9aa8e12");
                $toggleIcon.removeClass("p24_eyeOpen").addClass("p24_eyeClosed");
            }
        });


    });
</script>


    <script type="text/javascript">
        var IsDesktopRendered = true;
        $.validator.methods.email = function(value, element) {
            var re = /^(\s*)(?![.-])(?:[-a-zA-Z0-9!#$%&*+/=?^_`{|}~.])*(?:[a-zA-Z0-9!#$%&*+/=?^_`{|}~])+@(?:(?!-)[a-zA-Z0-9-]*[a-zA-Z0-9]\.)+[a-zA-Z]{2,}(\s*)$/;
            return this.optional(element) || re.test(value);
        };
    </script>

    <script>
        runCallbacks();

        window.loadReactComponents = function (callback) { callback(); }
    </script>
