        function hideCookieBanner() {
            let expiryDate = new Date((new Date().getTime()) + 31536000000);
            document.cookie = "P24CPA=2021-06-29T00:27:41.2870000;path=/;domain=.property24.com;SameSite=Lax;expires=" + expiryDate.toUTCString();
            document.querySelector("#cookieConsentWrapper").classList.add("hidden");
        }

        (function () {
            document.getElementById("cookiePolicyLink").addEventListener("click", hideCookieBanner, false);
            document.getElementById("cookieBannerClose").addEventListener("click", hideCookieBanner, false);
        })();
    </script>
