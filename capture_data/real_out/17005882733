<script>
function winopen(url, name) {
var popUpSizeX=650;
var popUpSizeY=500;

var popUpLocationX=150;
var popUpLocationY=150;

var popUpURL=url;


splashWin = window.open("", name ,'toolbar=no,location=no,status=yes,menubar=no,scrollbars=yes,resizable=no');

splashWin.blur();
window.focus();

splashWin.resizeTo(popUpSizeX,popUpSizeY);
splashWin.moveTo(popUpLocationX,popUpLocationY);
splashWin.location=popUpURL;
splashWin.focus();
}

function showContent(contentField)
{
	document.getElementById("contentswitch").innerHTML = document.getElementById(contentField).value;
}
</script>

