      function up(obj) {
        var item = obj.el;
        var num = obj.max;
        var start = obj.start;
        time2 = setInterval(function() {
          start++;
          if (start > num) {
            start = num;
            clearInterval(time2);
          }
          item.text(start)
        }, 500)

      }
    
