        function initAdvClose() {
          var now = +new Date()
          var oldTime = GetCookie('closednkYin_5144960')
          var gap = 24 * 60 * 60 * 1000
          if (oldTime) {
            oldTime = JSON.parse(oldTime)
            if (now - oldTime.time < gap) {
              $N("#closeAdBtn").parent().remove()
            }
          }
          $N("#closeAdBtn").bind('click', function() {
            var time ={time: + new Date()}
            $N("#closeAdBtn").parent().remove()
            SetCookie('closednkYin_5144960', JSON.stringify(time))
          })
        }
        // »î¶¯ÊÂ¼þ
        activityEvent();
        function activityEvent() {
          // ÅÐ¶Ï»î¶¯ÊÇ·ñÐèÒª´ò¿ª
          var now = new Date();
          var nowTime = now.getTime();
          var activitiesTime = localStorage.getItem('activitiesTime') || 0;
          var startBol = nowTime < new Date('2023-09-29').getTime();
          var endBol = nowTime > new Date('2023-10-07').getTime();
          if (startBol || endBol) return
          if (nowTime < activitiesTime) {
            return
          }
          $N('.jsActivities').show();
          $N('body').css('background', 'url(/style/css/new-pc/img/home/guoqingBg.jpg) no-repeat center 56px');
          // ¹Ø±Õ°´Å¥
          $N("#closeBg").bind('click', function() {
            $('.jsActivities').hide();
            $('body').css('background', 'none');
            localStorage.setItem('activitiesTime', nowTime + (1000 * 60 * 60 * 12));
          });
