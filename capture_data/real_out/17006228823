        Common.cors.needCors = function(_url) {
            var url = _url;
            if (typeof (isNeedCors) != 'undefined' && isNeedCors) {
                // url = BaseUrl + url + '&format=jsonp&jsoncallback=?';
                url = BaseUrl + url;
            }
            return url;
        }
        Common.cors.baseUrl = function (_url) {
            var url = _url;
            if (typeof (isNeedCors) != 'undefined' && isNeedCors) {
                url = BaseUrl + url;
            }
            return url;
        }
        Common.cors.ajaxCorsFn = function (_url, _type, _param, callback, otherParam) {
            var url = _url;
            var param1 = {
                type: _type || 'get',
                url: url,
                data: _param,
                dataType: 'json',
                success: callback
            }
            if (typeof (isNeedCors) != 'undefined' && isNeedCors) {
                // param.json = 'jsonp';
