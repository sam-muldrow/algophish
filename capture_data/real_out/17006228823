    if (typeof (Common.cors) == 'undefined') {
        // ¿çÓò¸ÄÔì£¬ÔÚÍ·²¿Ìí¼ÓCommonµÄ·½·¨
        Common.cors = {};
        Common.cors.json = 'json';
