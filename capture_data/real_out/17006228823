<script>
	//µ¼º½²Ëµ¥ÏÂÀ­ÊÂ¼þ
	$(function(){
		var hoverPanelEvent = function (hoverClass, addClass) {
			/*
			hoverClass   string CSSclassName
			addClass     string DOMclassName
			*/
			var hoverTime, outTime;
			$(hoverClass).hover(function () {
				clearTimeout(outTime);
				var $this = $(this);
				$(hoverClass).removeClass(addClass);
				$this.addClass(addClass);
			}, function () {
				var $this = $(this);
				outTime = setTimeout(function () {
					$this.removeClass(addClass);
				}, 100);
			});
		}
		hoverPanelEvent('.ok-header-item', 'ok-active');
		hoverPanelEvent('.ok-nav-item', 'ok-active');
		hoverPanelEvent('.ok-header-acc', 'ok-active');
		hoverPanelEvent('.home-banner-top-left>.top-left-main>.item', 'ok-active');

		// ¶¯Ì¬Ìí¼Óµã»÷ÊÂ¼þ
		$N('.ok-header-item a').each(function (i,v) {
			var $this = $N(v);
			$this.attr("onclick", "google_p(['Ò³Ãæ¶¥²¿¹«¹²µ¼º½', '" + $this.text() + "'])");
		});
	});
</script>
