for (let i=0; i < elementAddFeedbacks.length; i++){
elementAddFeedbacks[i].addEventListener('change', checkTextBox);
}
function checkTextBox (){
let elementCheckCommentBox=document.querySelectorAll('#add_feedbacks [name=\'satisfied\']');
let selectedValue=document.getElementById('add_feedbacks');
if(elementCheckCommentBox&&selectedValue){
for (let i=0; i < elementCheckCommentBox.length; i++){
