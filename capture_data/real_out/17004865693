<div class="html-slot-container">
<script>
function patHideChat() {
var styleTag = document.createElement('style');
styleTag.innerHTML = '.helpButtonEnabled {display:none !important;}';
document.head.append(styleTag);
}

console.log("Chat check");
var d = new Date(); // current time
var z =new Date().toLocaleString("en-US", {
  timeZone: "America/Los_Angeles"
});
d = new Date(z);

// 24hr time
var hours = d.getHours();
var mins = d.getMinutes();

// 0 = Sunday
var day = d.getDay();

var weekendhours = [6, 15, 16, 17];
var weekdayhours = [5, 18, 19, 20];

if((day == 0 || day == 6) && weekendhours.indexOf(hours) > -1) {
  console.log("Chat weekend closing period")
  patHideChat();

} else if ((day > 0 && day < 6) && weekdayhours.indexOf(hours) > -1) {
  console.log("Chat weekday closing period");
  patHideChat();
} else {
  console.log("Not in chat pause period");
}

</script>
</div>
