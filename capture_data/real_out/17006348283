<!-- swiper js -->

<script async>
        
        function addMansetAdvertorialGam(item) {
            if (item != null && typeof item == 'undefined') return;

            var swiperMansetSlider = document.querySelector('.manset-slider').swiper;

            if (item.url !== "") {
                var index = item.Index - 1;

                var newSlideString = '';

                if (index != 0) {
                    newSlideString = `
                       <div class="item swiper-slide carouselAd">
                            <a href="${item.Url}" target="_blank" class="trkvzbnr">
                                <picture>
                                    <source srcset="${item.Path}" media="(max-width: 576px)">
                                    <img src="https://i.tmgrup.com.tr/i/i.gif" data-src="${item.Path}" class='lazyload' alt="${item.Title}">
                                </picture>

                            </a>
                        </div>`;
                }
                else {
                    newSlideString = `
                       <div class="item swiper-slide carouselAd">
                            <a href="${item.Url}" target="_blank" class="trkvzbnr">
                                <picture>
                                    <source srcset="${item.Path}" media="(max-width: 576px)">
                                    <img src="${item.Path}" class="" alt="${item.Title}">
                                </picture>
                            </a>
                        </div>`;
                }


                var htmlObject = $(newSlideString);

                swiperMansetSlider.addSlide(index, htmlObject[0]);

                $('.manset-slider .swiper-pagination-bullet').hover(function () {
                    swiperMansetSlider.slideTo($(this).text())
                });
            }
        }
</script>
