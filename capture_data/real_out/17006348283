<script>
    $(function () {
        function GetCanliBorsa() {
            $.ajax({
                type: "GET",
                url: '/partial/modulgovdeustv2/piyasalarasync',
                dataType: "html",
                success: function (result) {
                    $("#PiyasalarAsync").empty();

                    $("#PiyasalarAsync").html(result);
                }
            });
        };

        setTimeout(function () {
            GetCanliBorsa();
        }, 3000);
        

        setInterval(function () {
            GetCanliBorsa();
        }, 15 * 1000);
    });
</script>

