<script type="text/javascript">
    $(document).ready(function () {
        var _target = !deviceInfo.isMobile && !deviceInfo.isTablet ? "_blank" : "";

        $.getJSON('/json/getflashaber', function (data) {
            if (!data.Success)
                return;

            if (data.List.length > 0) {
                var _html = "",
                    _stylePath = 'https://iahbr.tmgrup.com.tr/site/v3/c/flash-news-module.css?v=16653';
                    _scriptPath = 'https://iahbr.tmgrup.com.tr/site/v3/j/flash-news-module.js?v=16653';

                _html += "<link rel='stylesheet' href='" + _stylePath + "' />";
                _html += "<script type='text/javascript' src='" + _scriptPath + "'><\/script>";

                //_html += "<div class='flash-news-wrapper lazyload' data-link='" + _stylePath + "' data-script='" + _scriptPath + "'>";

                _html += "<div class='flash-news-wrapper'>";
                _html += "  <div class='flash-news'>";
                _html += "      <div class='flash-news-last-minute'>";
                _html += "          <a href='/son-dakika-haberleri' target='" + _target + "'>SON DAKİKA</a>";
                _html += "      </div>";
                _html += "      <div class='flash-news-slider swiper-container'>";
                _html += "          <div class='items swiper-wrapper'>";

                $.each(data.List, function (index, item) {
                    _html += "<div class='item swiper-slide'>";
                    _html += "  <a href='" + item.Url + "'>";
                    _html += "      <span>" + item.TitleShort + "</span>";
                    _html += "  </a>";
                    _html += "</div>";
                });

                _html += "          </div>";
                _html += "          <div class='swiper-button-next'></div>";
                _html += "          <div class='swiper-button-prev'></div>";
                _html += "      </div>";
                _html += "  </div>";
                _html += "</div>";

                $("#divSonDakika").replaceWith(_html);
            }
        });
    });
</script>

                


                


                


