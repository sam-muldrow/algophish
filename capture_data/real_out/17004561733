    // js file loader
    function downloadJSAtOnload() {
         var urlsnum = urls.length;
         for(count = 0; count < urlsnum; count++) {
              var element = document.createElement("script");
              element.src = urls[urlsnum - 1 - count];
              element.type = "text/javascript";
             var head = document.getElementsByTagName("head")[0];
             head.insertBefore(element, head.firstChild);
          }
     }
    function iframeloader(){
         var iframes = document.getElementsByTagName("iframe"); 
         Object.keys(iframes).forEach(function (key){
             if( iframes[key].getAttribute('data-src') ) {
             iframes[key].setAttribute('src', iframes[key].getAttribute('data-src'));
             iframes[key].removeAttribute('data-src');
             }
          });
    }

    document.getElementsByTagName("body")[0].addEventListener("touchstart", loadfunction);
    document.getElementsByTagName("body")[0].addEventListener("mousemove", loadfunction);
