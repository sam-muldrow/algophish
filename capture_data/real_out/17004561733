    var loadcounter = 0;
    function loadfunction() {
    if(loadcounter<1){
        iframeloader();
        downloadJSAtOnload();
        loadcounter=++loadcounter;
     }
    if(loadcounter<5){
         loadcounter=++loadcounter;
     }
