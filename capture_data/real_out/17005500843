<script>
	var showCatfish = function()
	{
		var catfishWrap = document.getElementById('catfishWrap');
		if( window.gpt.catfishAB )
		{
			document.getElementById( 'catfishCloseCountdown' ).remove();
			document.getElementById('catfishClose').classList.remove('hidden');
			var gptAdSlot = googletag.defineSlot('/4152678/KD-Desktop-Catfish-22', []).defineSizeMapping(window.gpt.mappingCatFish).addService(googletag.pubads());
		}
		else
		{
			var gptAdSlot = googletag.defineSlot('/4152678/KD-Desktop-Catfish-23', [[1, 1], [970, 250]]).defineSizeMapping(window.gpt.mappingCatFish).addService(googletag.pubads());
		}
		catfishWrap.appendChild( ( function()
		{
			var div = document.createElement('div');
			div.id = gptAdSlot.getSlotElementId();
			return div;
		} )() );
		googletag.display(gptAdSlot);
		googletag.pubads().refresh([gptAdSlot]);

		googletag.pubads().addEventListener('slotRenderEnded', function(event)
		{
			if( event.slot == gptAdSlot && event.isEmpty )
			{
				document.getElementById( event.slot.getSlotElementId() ).style.display = '';
				document.getElementById( event.slot.getSlotElementId() ).innerHTML = '';
				
				var gptAdSlotSecond	= googletag.defineSlot('/4152678/KD-Desktop-Catfish-second', []).defineSizeMapping(window.gpt.mappingCatFishSecond).addService(googletag.pubads());
				document.getElementById( event.slot.getSlotElementId() ).appendChild( ( function()
				{
					var div = document.createElement('div');
					div.id = gptAdSlotSecond.getSlotElementId();
					return div;
				} )() );
				googletag.display(gptAdSlotSecond);
				googletag.pubads().refresh([gptAdSlotSecond]);
				googletag.pubads().addEventListener('slotRenderEnded', function(event)
				{
					if( event.slot == gptAdSlotSecond && event.isEmpty )
					{
						document.getElementById( event.slot.getSlotElementId() ).style.display = '';
						document.getElementById( event.slot.getSlotElementId() ).innerHTML = '\r\n<!-- KD-Desktop-Catfish --><ins class="adsbygoogle" style="display:inline-block;width:1094px;height:135px" data-ad-client="ca-pub-8746219894182075" data-ad-slot="5648469073"></ins>';
		
						(adsbygoogle = window.adsbygoogle || []).push({});

						showCloseAnimation();
					}
					else if( event.slot == gptAdSlotSecond && !event.isEmpty )
					{
						showCloseAnimation();
					}
				} );
			}
			else if( event.slot == gptAdSlot && !event.isEmpty )
			{
				showCloseAnimation();
			}
		});
	}
	googletag.cmd.push(function() 
	{
		var gptBrandingSlot = googletag.defineSlot('/4152678/KD-branding-22', [1, 1], 'div-gpt-ad-1657101809864-0').defineSizeMapping(window.gpt.mappingBranding).addService(googletag.pubads());
		googletag.display('div-gpt-ad-1657101809864-0');
		
		googletag.pubads().addEventListener('slotRenderEnded', function(event)
		{
			if( event.slot == gptBrandingSlot && !event.isEmpty )
			{
				document.getElementById( 'KD-Top' ).parentNode.setAttribute('style', 'display:none !important');
				
				document.body.classList.add( 'branding' );
				
				showCatfish();
			}
			else if( event.slot == gptBrandingSlot && event.isEmpty )
			{
				document.getElementById( 'KD-Top' ).innerHTML = '';
				
				var gptAdSlotTop = googletag.defineSlot('/4152678/KD-TOP-22', []).defineSizeMapping(window.gpt.mappingTopHeader).addService(googletag.pubads());
				document.getElementById( 'KD-Top' ).appendChild( ( function()
				{
					var div = document.createElement('div');
					div.id = gptAdSlotTop.getSlotElementId();
					return div;
				} )() );
				googletag.display(gptAdSlotTop);
				googletag.pubads().refresh([gptAdSlotTop]);
				googletag.pubads().addEventListener('slotRenderEnded', function(event)
				{
					if( event.slot == gptAdSlotTop && event.isEmpty )
					{
						document.getElementById( 'KD-Top' ).innerHTML = '\r\n<!-- KD-TOP-22 -->\r\n<ins class="adsbygoogle" style="display:inline-block;width:970px;height:90px" data-ad-client="ca-pub-8746219894182075" data-ad-slot="8148865758"></ins>';
						
						(adsbygoogle = window.adsbygoogle || []).push({});
					}
				});
				
				showCatfish();
			}
		});
	});
