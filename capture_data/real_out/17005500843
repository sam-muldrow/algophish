<script>
	var catfishWrap = document.getElementById('catfishWrap');

	var showCloseAnimation = function()
	{
		if( !document.getElementById( 'catfishClose' ).classList.contains('hidden') )
		{
			return;
		}

		var countdownNumberEl = document.getElementById('catfishCloseCountdownNumber');
		var countdown = countdownNumberEl.innerHTML;
		document.getElementById('catfishCloseCountdown').classList.remove('hidden');
		document.getElementById( 'catfishCloseCountdownCircle' ).style.animationDuration = countdown + 's';

		var interval = setInterval( function()
		{
			countdown--;
			
			if( countdown <= 0 )
			{
				clearInterval( interval );
				document.getElementById( 'catfishCloseCountdown' ).remove();
				document.getElementById('catfishClose').classList.remove('hidden');
				return;
			};

			countdownNumberEl.textContent = countdown;
		}, 1000 );
	}
	setTimeout( showCloseAnimation, 10 * 1000 );
	googletag.cmd.push( function()
	{
