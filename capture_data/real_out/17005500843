	window.addEventListener( 'DOMContentLoaded', function()
	{
        ( function( $ )
		{
			var scope = $( '#elMonitoring_widget' );
			
			scope.one( 'update', function()
			{
				$( this ).find( '.preloader' ).hide();
			} );
			
			$.when(
				$.Deferred( function( deferred )
				{
					scope.on( 'ready', function( e, data )
					{
						$( deferred.resolve( data ) );
					} );
				} ),
				$.getScript( "https://cdnjs.cloudflare.com/ajax/libs/jquery.tablesorter/2.31.3/js/jquery.tablesorter.min.js" ),
				$.getScript( "https://cdnjs.cloudflare.com/ajax/libs/mustache.js/4.1.0/mustache.min.js" ),
				$.getScript( "https://cdn.polyfill.io/v3/polyfill.min.js?features=default,Array.prototype.find,matchMedia" ),
				$.getScript( "https://kurs.com.ua/storage/js/utils/number.js?v=1.12" ),
				$.getScript( "https://kurs.com.ua/storage/js/utils/onScreen.js?v=1.0" ),
				$.Deferred( function( deferred )
				{
					var $link = $( '<link/>', {
					   rel: 'stylesheet',
					   type: 'text/css',
					   href: "https://kurs.com.ua/storage/css/monitoring/style.css?v=1.19"
					} ).appendTo('head');
					$( '.page' ).css( 'overflow', 'hidden' );
					$( deferred.resolve );
				} )
			).done( function( data )
