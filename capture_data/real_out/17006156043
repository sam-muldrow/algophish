    (function () {

        // https://attacomsian.com/blog/javascript-dom-detect-internet-explorer-browser
        function isIE() {
            var ua = navigator.userAgent;
            return ua.indexOf('MSIE') > -1 || ua.indexOf('Trident') > -1;
        };

        if (isIE()) {

            var element = document.createElement('div');

            element.className = 'b-ie-support-dropped-banner-content';
            element.innerHTML = 'We no longer support the Internet Explorer browser. Please upgrade to <a href=https://www.microsoft.com/en-us/edge>Microsoft Edge</a> or continue with a different browser for the best experience on AutoTrader.';

            document.body.insertBefore(element, document.body.firstChild);
        }
    })();
</script>



