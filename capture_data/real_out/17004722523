</div><script type="text/javascript">window.addEventListener("load", function(){setTimeout(function(){window.vscomOpenModal("1696523038")},25000);});</script><script src="https://mktdplp102cdn.azureedge.net/public/latest/js/form-loader.js?v=1.84.2007"></script>
<script src="https://mktdplp102cdn.azureedge.net/public/latest/js/ws-tracking.js?v=1.84.2007"></script>
<script>
document.addEventListener("DOMContentLoaded", function () {
    var currentFormStatus = window.localStorage.getItem("homeForm");
    var targetNode = document.querySelector(".home-form");
    const closeBTN = document.querySelector('.vscom-modal-close');
    closeBTN.setAttribute('dat-bi-name', 'modal close');
   
    // Options for the observer (which mutations to observe)
    var config = {
        attributes: false,
        childList: true,
        subtree: true
    };

    function contains(arrList, findValue) {
        var i = arrList.length;
        while (i--) {
            if (arrList[i] === findValue) return true;
        }
        return false;
    }

    // Callback function to execute when mutations are observed
    var callback = function (mutationsList, observer) {
        // traditional 'for loops' for IE 11
        for (var m = 0; m < mutationsList.length; m++) {
            var mutation = mutationsList[m];
            if (mutation.addedNodes.length > 0) {
                for (var n = 0; n < mutation.addedNodes.length; n++) {
                    var node = mutation.addedNodes[n];
                    if (node.className.includes('onboardingform')) {
                        const submitBTN = document.querySelector('.onboardingform button[type="submit"]');
                        const checkBox = document.getElementById('74d14fed-a6fc-e811-a968-000d3a340154');
                        const country = document.getElementById('063e8736-fdac-e811-a966-000d3a34045d');
                        const privacy_policy = document.querySelector('.marketing-field a');
                        const form_heading = document.querySelector('.onboardingform h2.page_one_header').textContent;
                    

                        let elems = [submitBTN, checkBox, country, privacy_policy, closeBTN];
                        elems.forEach(elem => {
                            elem.setAttribute('data-bi-formid', 'form-dynamics');
                            elem.setAttribute('data-bi-formnm', 'GitHub Copilot on VScom Email Sign up form');
                            elem.setAttribute('data-bi-hn', form_heading);
                        });
                        if(checkBox.checked === false) {
                            checkBox.setAttribute('data-bi-name', 'checkbox default off');
                        }
                        checkBox.addEventListener("change", () => {
                            if (checkBox.checked) {
                                checkBox.setAttribute('data-bi-name', 'checkbox default on');
                            } else {
                                checkBox.setAttribute('data-bi-name', 'checkbox default off');
                            }
                        });
                        submitBTN.setAttribute('data-bi-name', 'submit');
                        privacy_policy.setAttribute('data-bi-name', 'privacy policy');
                        country.setAttribute('data-bi-name', 'dropdown-country-United States');
                        country.addEventListener('change', (e) => {
                            let country_select = country.options[country.selectedIndex].text;
                            country.setAttribute('data-bi-name', "dropdown-country-"+country_select);   
                        });


                    } else if (node.className.includes('onFormSubmittedFeedback')) {
                        const reloadBTN = document.querySelector('.onFormSubmittedFeedbackButton');
                        reloadBTN.setAttribute('data-bi-formid', 'form-dynamics');
                        reloadBTN.setAttribute('data-bi-formnm', 'GitHub Copilot on VScom Email Sign up form');
                        reloadBTN.setAttribute('data-bi-name', 'modal reload');
                        reloadBTN.setAttribute('data-bi-hn', 'Join the coding revolution!');
                        localStorage.setItem('homeForm', 'true');
                        observer.disconnect();
                    }
                }
            }
        }
    };

    // Create an observer instance linked to the callback function
    var observer = new MutationObserver(callback);
    if (document.getElementById('a02791bb-a190-ea11-a811-000d3a3b1bf3')) {
        return;
    } else {
        // if the form isn't here yet observing the target node for configured mutations
        observer.observe(targetNode, config);
    }
    // remove the form if user had submitted form
    if (currentFormStatus && currentFormStatus === "true") {
        targetNode.remove();
    }
});

function d365mktConfigureTracking() {
    return {
        Anonymize: true
    };
}
</script>
