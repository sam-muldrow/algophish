const changeUnderline = () => {
    let currentElement;
    setTimeout(
        function() {
            currentElement = jQuery('.bannerBar a');
            currentElement.toggleClass('underline');
            changeUnderline()
        }, 500);
}
jQuery(document).ready(function() {
    jQuery('.iobanner .socialmediabanner a:first-of-type').addClass('underline');

    changeUnderline();

})
