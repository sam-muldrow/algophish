    jQuery('.closeworkpopup').click(function(){
        if(jQuery(this).text() == 'Zamknij'){
        jQuery('.workpopup').css('bottom', '-' + jQuery('.workpopup img').innerHeight() + 'px');
          jQuery(this).text('Otwórz');
        } else{          
        jQuery('.workpopup').css('bottom', '0');
          jQuery(this).text('Zamknij');
        }
      })
});
