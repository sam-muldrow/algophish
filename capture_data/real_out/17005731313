<script>
/*
  jQuery(document).ready(function () {
    setTimeout( function(){
        jQuery('.workpopup').css('bottom', '0px');
    },1500);
    setTimeout( function(){
        jQuery('.workpopup').css('bottom', '-' + jQuery('.workpopup img').innerHeight() + 'px');
          jQuery('.closeworkpopup').text('Otwórz');
