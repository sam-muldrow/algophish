tabChangePc('mgz')
  function tabChangePc(cls){
    $(".magBox ." + cls + "-nav li").mouseover(function(){
        var index = $(this).index();
        $("." + cls + "-nav li,." + cls + "-cons>li").attr("class","")
        $(this).attr("class","tab-selected")
        $("." + cls + "-cons>li").eq(index).attr("class","display")
    })
}
