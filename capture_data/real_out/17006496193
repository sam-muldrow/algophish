<script>
function e(e){var t=!(arguments.length>1&&void 0!==arguments[1])||arguments[1],c=document.createElement("script");c.src=e,t?c.type="module":(c.async=!0,c.type="text/javascript",c.setAttribute("nomodule",""));var n=document.getElementsByTagName("script")[0];n.parentNode.insertBefore(c,n)}function t(t,c,n){var a,o,r;null!==(a=t.marfeel)&&void 0!==a||(t.marfeel={}),null!==(o=(r=t.marfeel).cmd)&&void 0!==o||(r.cmd=[]),t.marfeel.config=n,t.marfeel.config.accountId=c;var i="https://sdk.mrf.io/statics";e("".concat(i,"/marfeel-sdk.js?id=").concat(c),!0),e("".concat(i,"/marfeel-sdk.es5.js?id=").concat(c),!1)}!function(e,c){var n=arguments.length>2&&void 0!==arguments[2]?arguments[2]:{};t(e,c,n)}(window,146,{"multimedia": {}});window.marfeel = window.marfeel || {};
window.marfeel.cmd = window.marfeel.cmd || [];
window.marfeel.cmd.push(['flowcards', 'compass', function(flowcards, compass) {
  flowcards.addEventListener('navigationfromcard', function(event) {
      compass.trackNewPage(); // Track Compass
	  ga('send', 'pageview', event.uri); //Google Analytics
      COMSCORE.beacon({ c1: '2', c2: '6906409' }); // Comscore
  });
}]);
let observer_compass = new MutationObserver((mutations) => {
    mutations.forEach((mutation) => {
        if (mutation.target.innerHTML != '') {
            let Cuid = localStorage.getItem('aws.cognito.identity-id.us-east-1:59cf7619-1bf8-4dc0-a968-718a65a2eb30');
            if (Cuid) {
                window.marfeel.cmd.push(['compass', function(compass) {
                    compass.setUserType('logged');
                }]);
                window.marfeel.cmd.push(['compass', function(compass) {
                    compass.setSiteUserId(Cuid);
                }]);
                console.log('compass sended u');
            }
            observer_compass.disconnect();
        }
    });
});
let ob_config = { childList: true };
let ob_target = document.querySelector('#tyclg_div');
observer_compass.observe(ob_target, ob_config);
</script>

