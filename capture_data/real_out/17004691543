<script type="text/javascript">
    $(function () {
        length = $("#m_pic .mini_pic").length;
        for (var i = 0; i < length; i++) {
            if (i == 0) {
                $(".mall_page").append('<span class="mall_radiu mall_radius">●</span>');
            }
            else {
                $(".mall_page").append('<span class="mall_radiu">●</span>');
            }
        }
        $(".mall_page span").hover(function () {
            $(".mall_page span").removeClass("mall_radius");
            $(this).addClass("mall_radius");
            $("#m_pic .mini_pic").eq($(".mall_radius").index()).show().siblings().hide();
        })
    })
    </script>
