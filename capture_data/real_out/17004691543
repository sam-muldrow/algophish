<script src="/js/jquery-1.7.2.min.js" type="text/javascript"></script>
<script type="text/javascript">
    $(function () {
        $("#serch_level li span").click(function () {
            $("#serch_level li span").removeClass("li_check");
            $(this).addClass("li_check");
            $("#page").attr("data_key", 1);
            $.ajax({
                url: '/ajax/serch_series.ashx',
                data: { level: $("#serch_level li .li_check").attr("data_key"), min_price: $("#serch_price li .li_check").attr("data_min"), max_price: $("#serch_price li .li_check").attr("data_max"), country: $("#serch_nation li .li_check").attr("data_key"), page: $("#page").attr("data_key") },
                cache: false,
                async: false,
                type: "POST",
                dataType: 'html',
                contentType: "application/x-www-form-urlencoded; charset=utf-8",
                success: function (ret) {
                    $("#u_series").html(ret);
                }
            })
            set_num();
            $("#l_level").html($("#serch_level li .li_check").html());
            $("#l_price").html($("#serch_price li .li_check").html());
            $("#l_gj").html($("#serch_nation li .li_check").html());
        })
        $("#serch_price li span").click(function () {
            $("#serch_price li span").removeClass("li_check");
            $(this).addClass("li_check");
            $("#page").attr("data_key", 1);
            $.ajax({
                url: '/ajax/serch_series.ashx',
                data: { level: $("#serch_level li .li_check").attr("data_key"), min_price: $("#serch_price li .li_check").attr("data_min"), max_price: $("#serch_price li .li_check").attr("data_max"), country: $("#serch_nation li .li_check").attr("data_key"), page: $("#page").attr("data_key") },
                cache: false,
                async: false,
                type: "POST",
                dataType: 'html',
                contentType: "application/x-www-form-urlencoded; charset=utf-8",
                success: function (ret) {
                    $("#u_series").html(ret);
                }
            })
            set_num();
            $("#l_level").html($("#serch_level li .li_check").html());
            $("#l_price").html($("#serch_price li .li_check").html());
            $("#l_gj").html($("#serch_nation li .li_check").html());
        })
        $("#serch_nation li span").click(function () {
            $("#serch_nation li span").removeClass("li_check");
            $(this).addClass("li_check");
            $("#page").attr("data_key", 1);
            $.ajax({
                url: '/ajax/serch_series.ashx',
                data: { level: $("#serch_level li .li_check").attr("data_key"), min_price: $("#serch_price li .li_check").attr("data_min"), max_price: $("#serch_price li .li_check").attr("data_max"), country: $("#serch_nation li .li_check").attr("data_key"), page: $("#page").attr("data_key") },
                cache: false,
                async: false,
                type: "POST",
                dataType: 'html',
                contentType: "application/x-www-form-urlencoded; charset=utf-8",
                success: function (ret) {
                    $("#u_series").html(ret);
                }
            })
            set_num();
            $("#l_level").html($("#serch_level li .li_check").html());
            $("#l_price").html($("#serch_price li .li_check").html());
            $("#l_gj").html($("#serch_nation li .li_check").html());
        })
        $("#page").click(function () {
            var all_page = 1;
            $("#page_num").html();
            if (Number($("#page_num").html()) % 6 > 0) {
                all_page = parseInt(Number($("#page_num").html()) / 6) + 1;
            }
            else {
                all_page = parseInt(Number($("#page_num").html()) / 6);
            }
            var page = Number($("#page").attr("data_key")) + 1;
            if ($("#page").attr("data_key") == all_page) {
                alert("没有更多了，重新查看!");
                $("#page").attr("data_key", "1");
            }
            else {
                $("#page").attr("data_key", page);
            }
            $.ajax({
                url: '/ajax/serch_series.ashx',
                data: { level: $("#serch_level li .li_check").attr("data_key"), min_price: $("#serch_price li .li_check").attr("data_min"), max_price: $("#serch_price li .li_check").attr("data_max"), country: $("#serch_nation li .li_check").attr("data_key"), page: $("#page").attr("data_key") },
                cache: false,
                async: false,
                type: "POST",
                dataType: 'html',
                contentType: "application/x-www-form-urlencoded; charset=utf-8",
                success: function (ret) {
                    $("#u_series").html(ret);
                }
            })
        })
        $("#ret").click(function () {
            $("#serch_level li span").removeClass("li_check");
            $("#serch_level li span").eq(0).addClass("li_check");
            $("#serch_price li span").removeClass("li_check");
            $("#serch_price li span").eq(0).addClass("li_check");
            $("#serch_nation li span").removeClass("li_check");
            $("#serch_nation li span").eq(0).addClass("li_check");
            $("#page").attr("data_key", 1);
            $.ajax({
                url: '/ajax/serch_series.ashx',
                data: { level: $("#serch_level li .li_check").attr("data_key"), min_price: $("#serch_price li .li_check").attr("data_min"), max_price: $("#serch_price li .li_check").attr("data_max"), country: $("#serch_nation li .li_check").attr("data_key"), page: $("#page").attr("data_key") },
                cache: false,
                async: false,
                type: "POST",
                dataType: 'html',
                contentType: "application/x-www-form-urlencoded; charset=utf-8",
                success: function (ret) {
                    $("#u_series").html(ret);
                }
            })
            set_num();
            $("#l_level").html($("#serch_level li .li_check").html());
            $("#l_price").html($("#serch_price li .li_check").html());
            $("#l_gj").html($("#serch_nation li .li_check").html());
        })
    })
    function set_num() {
        $.ajax({
            url: '/ajax/serch_series_num.ashx',
            data: { level: $("#serch_level li .li_check").attr("data_key"), min_price: $("#serch_price li .li_check").attr("data_min"), max_price: $("#serch_price li .li_check").attr("data_max"), country: $("#serch_nation li .li_check").attr("data_key")},
            cache: false,
            async: false,
            type: "POST",
            dataType: 'html',
            contentType: "application/x-www-form-urlencoded; charset=utf-8",
            success: function (ret) {
                $("#page_num").html(ret);
            }
        })
    }
</script>
