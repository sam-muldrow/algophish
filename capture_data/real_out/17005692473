 var imp_adv_list = "0";
 function manset_adv_aktivite(id, tur) {
 var req = new XMLHttpRequest();
 if (req) {
 req.open('GET', 'https://ads.yenimedya.com.tr/Service/SaveAdvImpression?advId=' + id + '&isClick=' + tur, true);
 req.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded; charset=iso-8859-9');
 req.send();
 }
 }

 function manset_adv_imp(a) {
 var link = a.find(".rekkklam")
 var adv_id = link.attr("data-adv-id");

 if (adv_id != undefined && adv_id != '' && imp_adv_list.indexOf("," + adv_id) == -1) {
 imp_adv_list += "," + adv_id;
 manset_adv_aktivite(adv_id, false);
 }
 }

