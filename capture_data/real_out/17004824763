              if (location.hostname.indexOf('test') >= 0 || location.hostname.indexOf('localhost') >= 0) {
                script1.src = 'https://api.draftkingstest.com/eventstreams/v1/sdk/webeventtracker.js';
                script2.src = 'https://api.draftkingstest.com/eventstreams/v1/sdk/webeventtrackershim.js';
                
                document.head.appendChild(script1);
                document.head.appendChild(script2);

              } else if((location.hostname.indexOf('preprod') >= 0)){
