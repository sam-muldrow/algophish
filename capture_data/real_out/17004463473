        <script>
            (function() {
                'use strict';

                var $showcaseWidget = $('.js-showcase-3045681'),
                    $cdsBanners = $('.js-wgcds-content-item', $showcaseWidget),
                    options = {
                        id: '3045681',
                        lang: 'ru',
                        cdsScriptUrl: 'https://cds.wgcdn.co/static/client/js/jquery.wgcds.min.js',
                        isAnalyticsEnabled: false,
                        areCDSBannersAvailable: ($cdsBanners.length > 0)
                    };

                $(function() {
                    if ($showcaseWidget.length) {
                        if (typeof ContentSlider === 'function' && typeof Hammer === 'function' && typeof ShowcaseWidget === 'function') {
                            new ShowcaseWidget(options);
                        } else {
                            $.getScript('//eu-wotp.wgcdn.co/static/5.132.0_eb2d57/portal/js/plugins/hammer/hammer.min.js', function() {
                                $.getScript('//eu-wotp.wgcdn.co/static/5.132.0_eb2d57/portal/js/content_slider.js', function() {
                                    $.getScript('//eu-wotp.wgcdn.co/static/5.132.0_eb2d57/showcase/js/showcase.js', function() {
                                        new ShowcaseWidget(options);
                                    });
                                });
                            });
                        }
                    }
                });
            })();
        </script>
    </div>




<script>
    $(function() {
        var CACHE_TIME = 5 * 60 * 1000,
            INBETWEENER_URL = "/ru/inbetweeners/ru/inbetweener/165",
            WIDGET_ID = '3045679',
            CACHE_KEY = 'inbetweener:' + WIDGET_ID + ':' + wgsdk.vars.CURRENT_REQUEST_LANGUAGE,
            $widget = $('.js-inbetweener-content-' + WIDGET_ID),
            model = {
                isLoading: ko.observable(true)
            },
            renderInbetweener = function(data) {
                model.isLoading(false);
                $widget.html(data);
            };

        ko.applyBindings(model, $widget.get(0));

        if (amplify.store(CACHE_KEY)) {
            renderInbetweener(amplify.store(CACHE_KEY));
        } else {
            $.ajax({
                    url: INBETWEENER_URL,
                    dataType: 'html'
                })
                .done(function(data) {
                    /* Cleanup, delete expired cache data */
                    amplify.store();

                    amplify.store(CACHE_KEY, data, {'expires': CACHE_TIME });
                    renderInbetweener(data);
                })
                .fail(function() {
                    model.isLoading(false);
                    $widget.hide();
                });
        }
    });
</script>
<div class="js-inbetweener js-inbetweener-content-3045679">
    <div class="inbetweener_loader" data-bind="spinner: isLoading, spinnerOptions: {top: '50%', color: '#FFFFFF'}"></div>
</div>
        </div>
        
    </div>

            

            
            
