<script>
    (function() {
        'use strict';

        var $carouselWidget = $('.js-carousel-3045683'),
            options = {
                id: '3045683',
                lang: 'ru',
                container: '.js-carousel-3045683',
                cdsScriptUrl: 'https://cds.wgcdn.co/static/client/js/jquery.wgcds.min.js',
                fallbackUrl: '/ru/tankopedia/',
                fallbackImage: '//eu-wotp.wgcdn.co/static/5.132.0_eb2d57/wotp_static/img/core/frontend/scss/common/components/widgets/carousel/img/tankopedia.jpg',
                fallbackTitle: 'Танковедение'
            };

        $(function() {
            if ($carouselWidget.length) {
                if (typeof CarouselSlider === 'function' && typeof Hammer === 'function' && typeof CarouselWidget === 'function') {
                    new CarouselWidget(options);
                } else {
                    $.getScript('//eu-wotp.wgcdn.co/static/5.132.0_eb2d57/portal/js/plugins/hammer/hammer.min.js', function() {
                        $.getScript('//eu-wotp.wgcdn.co/static/5.132.0_eb2d57/carousel/js/carousel_slider.js', function() {
                            $.getScript('//eu-wotp.wgcdn.co/static/5.132.0_eb2d57/carousel/js/carousel_widget.js', function() {
                                new CarouselWidget(options);
                            });
                        });
                    });
                }
            }
        });
    })();
</script>
