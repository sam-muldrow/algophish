  <script>
      /*
      * Checks if a specific advertising/targeting cookie is enabled in the
      * browser's `localStorage`.
      */
    function isAdCookieEnabled(params) {
        let isEnabled = true; // By default, assume 'Advertising/Targeting Cookies' is enabled/allowed.

        /* Iterate over the keys in the `localStorage` object until an Evidon
        object is found. */
        for (const key in localStorage) {
          if (key && key.match(/_evidon/)) {
            let evidonStorageCookie = localStorage.getItem(key);
            const json = JSON.parse(evidonStorageCookie);

            if (json && json.categories && json.categories[1] && json.categories[1]['advertising/targeting cookies'] !== undefined) {
              /* Check the object for a property called
              `'advertising/targeting cookies' within the categories array.
              The boolean value of 'advertising/targeting cookies' will
              indicate whether the user has opted in or out`.
              */
              isEnabled = json.categories[1]['advertising/targeting cookies'];
            } else {
              /**  If the `json` object does not contain
               * 'advertising/targeting cookies' for any reason (likely stale
               * data), then  */
              console.error('Unexpected Evidon Cookie');
              continue;
            }
          }
        }
        return isEnabled;
      }

    /**
     * Global site tag (gtag.js) - Google (Universal) Analytics
     */
    function loadUAGoogleTagManager() {
      if (isAdCookieEnabled()) {
        const script = document.createElement('script');
        script.src = "https://www.googletagmanager.com/gtag/js?id=UA-11345405-1";
        script.type = 'text/javascript';
        script.async = true;
        document.head.appendChild(script);

        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());
        gtag('config', 'UA-11345405-1');
      }
    }

    /** 
     * Global site tag (gtag.js) - Google (GA4) Analytics
    */
    function loadGA4GoogleTagManager() {
      if (isAdCookieEnabled()) {
        const script = document.createElement('script');
        script.src = "https://www.googletagmanager.com/gtag/js?id=G-ETDQ32N89Y";
        script.type = 'text/javascript';
        script.async = true;
        document.head.appendChild(script);

        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());
        gtag('config', 'G-ETDQ32N89Y');
      }
    }
    
    loadUAGoogleTagManager();
    loadGA4GoogleTagManager();
  </script>


