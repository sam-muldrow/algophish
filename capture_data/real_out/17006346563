function setCookie_banner( name, value, expiredays ) {
	var todayDate = new Date();
	todayDate.setDate( todayDate.getDate() + expiredays );
	document.cookie = name + "=" + escape( value ) + "; path=/; expires=" + todayDate.toGMTString() + ";"
}





function close_intercom() {
	
	setCookie_banner( "intercom_popup", "done" , 1);	
	document.getElementById("close_bt").style.display = "none";
	//ChannelIO('shutdown');	
}


function close_intercom_chk() {
	close_intercom();
	$('#chatHidden').modal('hide');
}



function openChannelIO()
{
	document.getElementById("channel_io_button").style.display = "none";
	//document.getElementById("channel_io_button").innerHTML = "";
	
	window.channelPluginSettings = {
//	"mobileOpenUI":"iframe",
	     "pluginKey": "aba1c972-db9a-4b00-851c-3f81acaadaa1"
	   };
  (function() {
    var w = window;
      if (w.ChannelIO) {
      return (window.console.error || window.console.log || function(){})('ChannelIO script included twice.');
    }
    var d = window.document;
    var ch = function() {
      ch.c(arguments);
    };
    ch.q = [];
    ch.c = function(args) {
      ch.q.push(args);
    };
    w.ChannelIO = ch;
    function l() {
      if (w.ChannelIOInitialized) {
        return;
      }
      w.ChannelIOInitialized = true;
      var s = document.createElement('script');
      s.type = 'text/javascript';
      s.async = true;
      s.src = 'https://cdn.channel.io/plugin/ch-plugin-web.js';
      s.charset = 'UTF-8';
      var x = document.getElementsByTagName('script')[0];
      x.parentNode.insertBefore(s, x);
    }
    if (document.readyState === 'complete') {
      l();
    } else if (window.attachEvent) {
      window.attachEvent('onload', l);
    } else {
      window.addEventListener('DOMContentLoaded', l, false);
      window.addEventListener('load', l, false);
    }
  })();
 
  ChannelIO('show');
  ChannelIO('onShow', function() {
//	ChannelIO('shutdown');
	document.getElementById("ch-plugin").style.display = "block";
	document.getElementById("channel_io_button").style.display = "none";
  });
  ChannelIO('onHide', function() {
//	ChannelIO('shutdown');
	document.getElementById("ch-plugin").style.display = "none";
	document.getElementById("channel_io_button").style.display = "block";
  });

}

//-->
</script>
<script>
 $('[data-toggle="collapse"').on('click', function(){  
	 if($(this).find('.bi-chevron-down').hasClass( "fa-arrow-down-animated" ))
	{
		 
		 $(this).find('.bi-chevron-down').removeClass( "fa-arrow-down-animated" );
		 $(this).find('.bi-chevron-down').addClass( "fa-arrow-up-animated" );

	}
	 else
	{	
	 
		 $(this).find('.bi-chevron-down').addClass( "fa-arrow-down-animated" );
		 $(this).find('.bi-chevron-down').removeClass( "fa-arrow-up-animated" );
	}
});
</script>

<script> 

$('.clipboardBtn').on('click', function(e) { 
	// 텍스트값을 가져옴 
	
	var text = $(this).attr("data-clipboard-text");
	if (text == undefined){
		// 타겟이 있으면 
		var target = $(this).attr("data-clipboard-target");
		if (target == undefined) { 
			bs_alert('복사할 대상이 없습니다.','복사할 대상을 확인 후 선택해 주세요.'); 			
			return;
		}
		var text = $(target).text();
	} 
	//숨겨진 input박스 value값으로 text 변수 넣어줌. 
	$('#clip_target').val(text);
	//input박스 value를 선택
	$('#clip_target').select();
	// Use try & catch for unsupported browser 
	try { 
		// The important part (copy selected text) 
		var successful = document.execCommand('copy'); 
		// if(successful) answer.innerHTML = 'Copied!'; 
		// else answer.innerHTML = 'Unable to copy!';		
	} catch (err){ 
		bs_alert('안내','사용하시는 브라우저에서는 지원되지 않는 기능입니다.');
	}
	
	// 액션이 있으면
	if ($(this).attr("data-clipboard-action") == 'select' && successful && target != '' && target != undefined) { 
		SelectText($(target)[0]); 
		successful = document.execCommand('copy'); 
	}}) 
	
	//browser detect 
	// jQuery .browser 미지원 커버 
	var browser = (function() { 
		var s = navigator.userAgent.toLowerCase(); 
		var match = /(webkit)[ \/](\w.]+)/.exec(s) || /(opera)(?:.*version)?[ \/](\w.]+)/.exec(s) || /(msie) ([\w.]+)/.exec(s) || !/compatible/.test(s) && /(mozilla)(?:.*? rv:([\w.]+))?/.exec(s) || []; 
		return { name: match[1] || "", version: match[2] || "0" }; 
	}()); 
	
	function SelectText(text) { 
		// if ($.browser.msie) {
		if (browser.name == 'msie') { 
			var range = document.body.createTextRange(); 
			range.moveToElementText(text); 
			range.select(); 
			// } else if ($.browser.mozilla || $.browser.opera) { 
		} else { 
			var selection = window.getSelection(); 
			var range = document.createRange(); 
			range.selectNodeContents(text); 
			selection.removeAllRanges(); 
			selection.addRange(range); 
			// } else if ($.browser.safari) { 
			// var selection = window.getSelection(); 
		 // selection.setBaseAndExtent(text, 0, text, 1); 
	} 
} 
</script>
