							<script type="text/javascript">
							$(document).ready(function(){
								var height =  $(".rolling_top_prod").height();
								var num = $(".rolling_main li").length;
								var max = height * num;
								var move = 0;
								function noticeRolling(){
									move += height;
									$(".rolling_main").animate({"top":-move},500,function(){
										if( move >= max ){
											$(this).css("top",0);
											move = 0;
										};
									});
								};
								noticeRollingOff = setInterval(noticeRolling,5000);
								$(".rolling_main").append($(".rolling_main li").first().clone());
							});	
