<script>
$(function () {
  $('[data-toggle="tooltip"]').tooltip()
})
/* bootstrap select js all tag*/
/*$(function () {
    $('select').selectpicker();
});*/
/* $(function () {
  $('[data-toggle="tooltip"]').tooltip()
})*/
function openNav(id) {
	var sale_count = 0;
	var buy_count = 0;
	var cart_count = 0;
	var zzim_count = 0;
	var favorite_count=0;
	var notice_data = "";
    var add_tag = "";

	$.ajax({
		url : "/_system/data/my_escrow_json.php",
		dataType:"json",
		mimeType: "application/json",
		success:  function(data) {
 			sale_count = data.sale_order;
			buy_count = data.buy_order;
			cart_count = data.cart_order;
			zzim_count = data.zzim_order;
			favorite_count = data.favorite_order;
			paper_count = data.paper_count;

 
			if(data.notice_subject)
				notice_data = "<a href=\"" + data.notice_url + "\">" + data.notice_subject + "</a>";
		 
		
			if(id == "right") 
			{
				if(sale_count > 0)		$("#sale_count_mypage").removeClass('badge-gray1').addClass('badge-danger');
				if(buy_count > 0)		$("#buy_count_mypage").removeClass('badge-gray1').addClass('badge-danger');

				if(cart_count > 0)		$("#cart_count_mypage").removeClass('badge-gray1').addClass('badge-blue1');
				if(zzim_count > 0)		$("#zzim_count_mypage").removeClass('badge-gray1').addClass('badge-blue1');
				if(paper_count > 0)		$("#paper_count_mypage").removeClass('badge-gray1').addClass('badge-warning white');
				if(favorite_count > 0)		$("#favorite_count_mypage").removeClass('badge-gray1').addClass('badge-blue1');

				$("#sale_count_mypage").html(sale_count);
				$("#buy_count_mypage").html(buy_count);
				$("#cart_count_mypage").html(cart_count);
				$("#zzim_count_mypage").html(zzim_count);
				$("#paper_count_mypage").html(paper_count);
				$("#favorite_count_mypage").html(favorite_count);
		 
			}
			else
			{
				if(sale_count > 0)		$("#sale_count").attr('class','badge badge fsize3 badge-danger text-center');  
				if(buy_count > 0)		$("#buy_count").attr('class','badge badge fsize3  badge-danger  text-center');
 
				if(cart_count > 0)		$("#cart_count").attr('class','blue');
				if(zzim_count > 0)		$("#zzim_count").attr('class','blue');
				if(favorite_count > 0)		$("#favorite_count").attr('class','blue');
				//if(notice_data)		$("#notice_area").attr('class','d-block');

				$("#sale_count").html(sale_count);
				$("#buy_count").html(buy_count);
				$("#cart_count").html(cart_count);
				$("#zzim_count").html(zzim_count);
				$("#favorite_count").html(favorite_count);

				//$("#notice_popup").html(notice_data);
			}

		}

		});
	if(id == "left")
	{
		$('.sidenav-'+id).animate({left: "-50%"}, 0);
		$('.sidenav-'+id).animate({left: "0px",opacity:'1'}, 150);
	}
	else
	{
		$('.sidenav-'+id).animate({right: "-50%"}, 0);
		$('.sidenav-'+id).animate({right: "0px",opacity:'1'}, 150);
	}

//	$('#main_container').addClass('d-none');
//	   $('#main_container').removeClass('d-block');
}

function closeNav(id) {

	if(id == "left")
		$('.sidenav-'+id).animate({left: "-100%",opacity:'0'}, 100);
	else
		$('.sidenav-'+id).animate({right: "-100%",opacity:'0'}, 100);
//	$('#main_container').addClass('d-block');
//	$('#main_container').removeClass('d-none');
}
<!-- 메뉴 슬라이드 -->

</script>

