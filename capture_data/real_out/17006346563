<script>
function market_latest_info()
{
	var  market_counter = 0;
	var time_def = "";
	$.ajax({
		url : "/_system/data/market_latest_json.php",
		dataType:"json",
		mimeType: "application/json",
		success:  function(data) {
			$("#market_counter").html(data.market_counter);
			$("#market_ing_counter").html(data.market_ing_counter);
			$("#singo_total").html(data.singo_total);
			$("#time_def").html(data.time_def);
		}
	});
}
function get_my_escrow()
{

	var sale_count = 0;
	var buy_count = 0;
	var cart_count = 0;
	var zzim_count = 0;

	$.ajax({
		url : "/_system/data/my_escrow_json.php?mode=main_count",
		dataType:"json",
		mimeType: "application/json",
		success:  function(data) {
			if(data.loginChk == "1")
			{
				sale_count = data.sale_order;
				buy_count = data.buy_order;
				cart_count = data.cart_order;
 				
				$("#bottom_sale_count").html(sale_count);
				$("#bottom_buy_count").html(buy_count);
				$("#bottom_cart_count").html(cart_count);


				if(removeComma(sale_count) > 0)		{ $("#bottom_sale_count").attr('class','badge fsize8 mt-1 badge-danger text-center'); }
				if(removeComma(buy_count) > 0)		{ $("#bottom_buy_count").attr('class','badge fsize8 mt-1 badge-danger text-center');  }
				if(removeComma(cart_count) > 0)		{ $("#bottom_cart_count").attr('class','badge fsize7 mt-1 badge-gray0 text-center');  }
				
			
			}
			else 
			{
				$('#box_buy_count').tooltip('hide').attr('data-original-title', '로그인이 필요합니다');
				$('#box_sale_count').tooltip('hide').attr('data-original-title', '로그인이 필요합니다');
				$('#box_cart_count').tooltip('hide').attr('data-original-title', '로그인이 필요합니다');
			}


 		}
	});
}


function fee_product(corp,maker){
		
	 $.ajax({
		url: "/_system/data/product_fee.main.php?corp="+corp+"&maker="+maker,
		cache: false
		})
		.done(function(data) {
			$("#price_data").html(data);
		});	
}

var corp_sl = document.getElementById("price_corp").value;
var maker_sl = document.getElementById("price_maker").value;
 
function check_price(no,group)
{
	var corp_sl = document.getElementById("price_corp").value;
	var maker_sl = document.getElementById("price_maker").value;

	if(group =="corp"){
		document.getElementById("price_corp").value =   no;
		corp_sl = no;
	}else{
		corp_sl = document.getElementById("price_corp").value;
	}

	if(group =="maker"){
		 document.getElementById("price_maker").value =  no;
		 maker_sl = no;
	}else{
		maker_sl = document.getElementById("price_maker").value;
	}

	fee_product(corp_sl,maker_sl);
	
	if(group == "corp"){
		for (i=1; i < 4 ;i++ )
		{
			if(i == corp_sl){
				document.getElementById("corp_"+i).className="black fb";
			}else{
				document.getElementById("corp_"+i).className="gray";
			}
		}
	}
	
	if(group == "maker"){
		var m_arr = new Array(1,12,2);
		for(i = 0; i < m_arr.length; i++){
		
			if(m_arr[i] == no){
				document.getElementById("maker_"+m_arr[i]).className="black fb";
			}else{
				document.getElementById("maker_"+m_arr[i]).className="gray";
			}
		}
	}	
}
$(document).ready(function() {
	market_latest_info();
	get_my_escrow();
	 fee_product(corp_sl,maker_sl);
});
</script>
 
