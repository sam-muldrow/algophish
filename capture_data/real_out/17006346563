<script language=javascript>
var forced_pm = '';
$('#productFindFromListp').on('show.bs.modal', function (e) {

	if(forced_pm != '' )
		$('#pm_modal').val(forced_pm);
	else 
		$('#pm_modal').val('');

	$('#series_modal').val('');
	forced_pm = "";

 	loadingProductList();
})
function ModalShow(pm)
{

	forced_pm = pm;
	$('#productFindFromListp').modal('show');
//	$('#pm_modal').val(pm);

//	loadingProductList();
}
function listBack()
{
	if($('#series_modal').val() != "")
	{
		$('#series_modal').val('');
	}
	else if($('#pm_modal').val() != "")
	{
		$('#pm_modal').val('');
	}

	loadingProductList();
}
function change_next(field,value)
{
	if(field == "pm_modal" && value == "")
	{
		$('#series_modal').val('');
	}

	$('#'+field).val(value);
 
	loadingProductList();
}
function change_Product_submit(product_no)
{
 
	if(product_no == "") return false; 
  
	$.ajax({
		type : 'post',
		url : "/_system/data/get_product_info_json.php?product_no="+product_no,
 
		dataType:"json",
		mimeType: "application/json",
		success:  function(data) {

			$('#product_no').val(data.product_no);
			$('#just_one').val(data.product_no);
			$('#just_one_name').val(data.p_modelno);
			$('#pno').val(data.product_no);
			$('#pw').val(data.pw);
			$('#pw_str').val(data.pw_str);
			$('#pm').val(data.pm);
			$('#m_no').val(data.m_no);
			$('#p_title').val(data.p_title);
			$('#pname').val(data.p_modelno);
			$('#p_key').val(data.p_key);
			$('#p_modelno').val(data.p_modelno);
			$('#p_model').val(data.p_model);
			$('#storage').val(data.storage);
			$('#p_cat').val(data.p_cat);
 
			if($('#'+id).attr("autoclear") == "true")
				$('.typeahead').typeahead('val', '');
			else 
				$('.typeahead').typeahead('val', data.p_modelno);
			cookie_keyword($("#keyword_p").val(), data.product_no, data.p_key + " " + data.storage, data.pw_str);
			check_next_routine();
 		}
	});

	
}
 
function loadingProductList()
{
	/*var queryString = $("form[name=main_search_modal]").serialize() ;*/
	var pm_modal =  $('#pm_modal').val();
	var series_modal =  $('#series_modal').val();
	var datamode = "";
	if($('#'+id).attr("datamode") != undefined)		
	{
		datamode = $('#'+id).attr("datamode");
	}

	if($('#'+id).attr("modal_title") != undefined)		
	{
		$('#productFindFromListpLabel').text($('#'+id).attr("modal_title"));
	}
	else 
		$('#productFindFromListpLabel').text('제품 검색');

	$.ajax({
 
		url : "/_system/data/product_list.php?pm_modal="+pm_modal+"&series_modal="+series_modal+"&datamode="+datamode,
//		data : queryString,
		success:  function(data) {
	 			$("#product_list").html(data);
				 
		}
	});
}
 
