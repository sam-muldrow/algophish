<script>

var id = $('.typeahead').attr("id");
 


function check_next_routine()
{
 
	$('#productFindFromListp').modal('hide');

 
	if($('#'+id).attr("autoscript") == "true")
	{
 
		product_search_endscript();
	}
 
	if($('#'+id).attr("autosubmit") == "true")
	{
	
		if($('#product_no').val() || $('#keyword_p').val()) 
		{	
			if($('#'+id).attr("formname") != "")
			{
			
				var formname = $('#'+id).attr("formname");
		 
				$('#'+formname).submit();
			}
			else 
			{
 
				$('form').submit();
			}
		}
		 
	}
}
// Input 필드 하단에 위치해야 정상 동작 함
var ProductSuggest = new Bloodhound({
  datumTokenizer: Bloodhound.tokenizers.obj.whitespace('value'),
  queryTokenizer: Bloodhound.tokenizers.whitespace,
  remote: {
    url: "/dummy",
//	cache:false,
    wildcard: '%QUERY',
	replace: function(url,uriEncodedQuery)	  {
 
		var val = "/_system/data/ajax_suggest_json.php?form=SearchProduct&keyword=" + encodeURIComponent(uriEncodedQuery);
 
		return val; 
	}
  }
});
var cookie_keywords = "";
var pw_str = "";
if($.cookie('keywords'))
{
	var keyword_arr =  $.cookie('keywords').split(','); 

	for ( var i in keyword_arr ) {
			pw_str = "";
			var keyword_val = keyword_arr[i].split('|'); 
			if(keyword_val[0] != '')
			{
				if(keyword_val[1])
				{
					if(keyword_val[3])	var pw_str = ' <span class=" gray1 fsize1">(' + keyword_val[3] + ')</span>';
					cookie_keywords += '<span class="fsize2    px-2 mx-1 pointer     gray text-nowrap" onClick="change_Product_submit(\''+keyword_val[1]+'\')">' + keyword_val[2] +  pw_str+'</span>';
				}
				else 
					cookie_keywords += '<span class="fsize3   px-1  mx-1 pointer " onClick="change_Keyword_submit(\''+keyword_val[0]+'\')">"' + keyword_val[0]+ '"</span>';
			}
		 }
	if(cookie_keywords != "")
	{
		cookie_keywords = '<div id="key_div"><div class="    text-left px-1 mx-1 my-1 align-self-center lh16  gray text-break" ><span id="key_list" class="text-break">'+cookie_keywords+'</span><i data-toggle=\"tooltip\" data-placement=\"bottom\" title=\"검색어 모두 삭제\" class=\"bi bi-trash gray1 pointer\" onClick="remove_keyword()"></i></div></div>';
	}

}
var SelectfFromnList = cookie_keywords + '<div class="empty-message "  data-toggle="modal" data-target="#productFindFromListp"><div class="fsize4 py-1 btn btn-block text-left  btn-light blue  align-self-center"><span class="border_bottom_blue">또는 제품목록에서 선택하려면</span> &nbsp;<i class=\"bi bi-arrow-right-circle align-self-center\" /></i></div></div>';
$('#custom-templates .typeahead').typeahead({minLength:0}, {
  name: 'products',
  display: 'p_modelno',
  
  highlight: true,
  source: ProductSuggest,
  cancelButton: true, 
  limit: 200,
  cache:false,
  templates: {
    empty: [SelectfFromnList].join('\n'),
// 	header: SelectfFromnList,
	suggestion: function(data) {
		var pw_str = "";
		var storage_val = "";
		if(data.pw_str != null) pw_str = "<span class='blue ml-2'>" + data.pw_str + "</span>";
		if(data.storage) storage_val = " <span class='gray1'>("+data.storage+")</span>";

		if(data.maker_no != null)
		{
			return '<div class="">' + data.maker_name + ' </div>';
		}
		else 
			return '<div class="row  "><div class="col-2 text-center "><img src="' + data.img + '"  class="w-100 px-1"></div><div class="col-9 pt-2 align-self-center"><div class="row  ml-2"><div  class="col-12  fsize5">' + data.p_key + storage_val + '</div><div  class="col-12 pt-1 fsize4 gray1 text-truncate">' + data.p_modelno + pw_str + '</div></div></div><div class="col-1  text-center d-flex align-self-center"><i class="bi bi-star p20 gray1 pointer"  onClick="favorite(\'' + data.product_no + '\')"/></i></div></div><div class="divide_line1 my-1"></div>';
	}
  }
 
});
$('.typeahead').bind('typeahead:close', function () {
	

 
	if($('#'+id).attr("autoclear") == "true")
	{
	 	$('#'+id).typeahead('val', '');
	}
});
$('.typeahead').bind('typeahead:select', function(ev, suggestion) {

	$('#product_no').val(suggestion['product_no']);
	$('#just_one').val(suggestion['product_no']);
	$('#just_one_name').val(suggestion['p_modelno']);
	$('#pno').val(suggestion['product_no']);
	$('#pw').val(suggestion['pw']);
	$('#pm').val(suggestion['pm']);
	$('#m_no').val(suggestion['m_no']);
	$('#p_title').val(suggestion['p_title']);
	$('#pname').val(suggestion['p_modelno']);
	$('#p_key').val(suggestion['p_key']);
	$('#p_modelno').val(suggestion['p_modelno']);
	$('#p_model').val(suggestion['p_model']);
	$('#storage').val(suggestion['storage']);
	$('#p_cat').val(suggestion['p_cat']);
 
	cookie_keyword(suggestion['p_modelno'],suggestion['product_no'],suggestion['p_key'] + " " + suggestion['storage'],suggestion['pw_str']);
 
	check_next_routine();
});

function cookie_keyword(val,product_no,p_key,pw_str)
{
	if(val == '' || val == undefined) return; 
 	val = val.replace(/\<|\>|\"|\'|\%|\;|\(|\)|\&|\+/g, '');
 
	var cookie_keywords = "";
	if($.cookie('keywords'))
	{
		var keyword_arr =  $.cookie('keywords').split(','); 


		for ( var i in keyword_arr ) {

				var keyword_val = keyword_arr[i].split('|'); 
				
				if(val != keyword_val[0] && keyword_val[0] != '') 
					cookie_keywords += keyword_val[0] + '|' + keyword_val[1] + '|' + keyword_val[2] + '|' + keyword_val[3] + ',';

				if(i >= 9) break;
			 }
	}
    cookie_keywords = val + '|' + product_no + '|' + p_key + '|' + pw_str +  "," + cookie_keywords; 
	

	$.cookie('keywords', cookie_keywords,  { expires: 365, path: '/', domain: "cetizen.com" });
}
function remove_keyword()
{
	cookie_keywords = "";
	$.cookie('keywords', cookie_keywords,  { expires: 365, path: '/', domain: "cetizen.com" });
	$('#key_div').html('');
}
function change_Keyword_submit(val)
{
 
	$('#keyword_p').val(val);
	cookie_keyword($("#keyword_p").val(),"","","");
	check_next_routine();
}

function favorite(pno)
		{
 

			if(pno){

				var url = '/market.php?a=favorites_add&mode=ajax&pno='+pno;
				
				  $.ajax({
						url: url,
						cache: false
						})
						.done(function( data ) {
							 var pdb = data.trim();
 							if(pdb == "nologin"){
								location.href="https://www.cetizen.com/login.php";
							}else{
								location.href="https://market.cetizen.com/market.php?tabsection=favorites";
								//zzim_chkeck(auc_no);
							}
						});

			}else{
				//c_alert("로그인이 필요합니다. 화면 제일 하단 로그인버튼을 눌러 로그인후 이용해 주세요.","로그인이 필요한 서비스 입니다.");
				Swal_alert_button("/login.php?referer=", "로그인이 필요한 서비스 입니다", "로그인 페이지로 이동하시겠습니까?",  "","아니오", "");
			}
		}

