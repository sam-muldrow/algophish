                <script type="text/javascript">
        var mouse_is_inside = false;
        $(document).ready(function() {
            let modal = document.getElementById("dialog-popup");
            let cookieName = 'popup_for_page_'+61;
            let showCount = $.cookie(cookieName) ? parseInt($.cookie(cookieName)) : 0;
            let per_action = 0;
            // Get the modal
            if($.cookie(cookieName) ||  showCount >= 1){
                if(showCount < 2){
                    modal.style.display = "block";
                    if(!per_action) writeModalWithCookies(cookieName);
                }
            }
            else{
                modal.style.display = "block";
                if(!per_action) writeModalWithCookies(cookieName);
            }

            $('.modal-content').hover(function(){
                mouse_is_inside=true;
            }, function(){
                mouse_is_inside=false;
            }).on('touchstart touchend', function(e) {
                mouse_is_inside=true;
            });

            //When the user clicks on <span> (x) and outside the modal, close the modal
            $("#dialog-popup").mouseup(function(){
                if(! mouse_is_inside) {
                    $("#dialog-popup").fadeOut();
                    if(per_action) writeModalWithCookies(cookieName);
                }
            });

            $(".popup_image").each(function(){
                $(this).click(function() {
                    $("#dialog-popup").fadeOut();
                    if(per_action) writeModalWithCookies(cookieName);
                    let str = "" +"https://www.biedronka.pl/pl/najtanszy-koszyk-asm";
                    if(str.length > 0)
                        window.open("https://www.biedronka.pl/pl/najtanszy-koszyk-asm","_self");
                });
            });
        });
        function writeModalWithCookies(cookieName){
            let perDay = 1;
            let countOfShowing = $.cookie(cookieName) ? parseInt($.cookie(cookieName)) + 1 : 1;
            $.cookie(cookieName, countOfShowing,{
                path : '/',
                expires : perDay ? 1 : 5
            });
        }

    </script>
    <div id="dialog-popup" class="modal">
