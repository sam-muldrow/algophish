        <nav class="shared_panel_nav" data-view="shared/panel_nav"><ul class="panel-nav">
    <li class="panel-nav-item panel-nav-item-route on">
        <a href="/web/Itineraires"><svg class="icon icon-route"><use xlink:href="#icon-route"></use></svg>Itinéraires</a>
