<script>function alertTicker(){
  const items = $('[id^=important-alert-message-]');
  items.each(function(i, e){
    if($(e).width() < $(e).children().width()){
      $(e).children().addClass('ticker');
    } else {
      $(e).children().removeClass('ticker');
    }
  });
}
$(function() {
  setTimeout(function(){
    if($('.important-alert-default-box').length) alertTicker();
  }, 1500);
});
$(window).resize(function() {
  if($('.important-alert-default-box').length) alertTicker();
});
