<script>
	document.body.style.overflow = 'hidden';

	function AdsCookieManagement() {
	}

	/**
	 * Verificam permisiuni de afisare
	 * @param isCookiePro
	 * @returns {boolean}
	 */
	AdsCookieManagement.areAdsEnabled = function (intIsCookiePro) {
		if (intIsCookiePro == 1) {
			if (AdsCookieManagement.checkCookieProAdsPermited()) {
				return true;
			}
			return false;
		} else {
			if (AdsCookieManagement.getCookie("acceptcookies") == 1) {
				return true;
			}
			return false;
		}
	}

	/**
	 * Verificare acceptre marketing CookiePro
	 * @returns {boolean}
	 */
	AdsCookieManagement.checkCookieProAdsPermited = function () {
		var strCookie = AdsCookieManagement.getCookie("OptanonConsent");

		if (strCookie.includes("C0004:1")) {
			return true;
		}
		return false;
	}


	/**
	 * Setare cookie
	 * @param cname
	 * @returns {string}
	 */
	AdsCookieManagement.getCookie = function (cname) {
		var name = cname + "=";
		var decodedCookie = decodeURIComponent(document.cookie);
		var ca = decodedCookie.split(";");
		for (var i = 0; i < ca.length; i++) {
			var c = ca[i];
			while (c.charAt(0) == " ") {
				c = c.substring(1);
			}
			if (c.indexOf(name) == 0) {
				return c.substring(name.length, c.length);
			}
		}
		return "";
	}


	/**
	 * Initializare ads-uri
	 * https://developers.google.com/publisher-tag/reference#googletag.PubAdsService_refresh
	 */
	AdsCookieManagement.initAds = function () {
		if(typeof googletag !== "undefined"){
			googletag.cmd.push(function () {
				googletag.pubads().refresh();
				// googletag.pubads().display(); // does not work in single request mode
			})
		}
	}


	/**
	 * Initializare ads-uri cu preverificare permisiune marketing
	 * https://developers.google.com/publisher-tag/reference#googletag.PubAdsService_refresh
	 */
	AdsCookieManagement.initAdsAndCheckCookieProCookie = function () {
		if (AdsCookieManagement.checkCookieProAdsPermited()) {
			if(typeof googletag !== "undefined" && typeof googletag.cmd !== "undefined"){
				googletag.cmd.push(function () {
					googletag.pubads().refresh();
					// googletag.pubads().display(); // does not work in single request mode
				})
			}
		}
	}


	/**
	 * Dezactivare ads-uri
	 * https://developers.google.com/publisher-tag/reference#googletag.PubAdsService_clear
	 */
	AdsCookieManagement.disableAds = function () {
		if(typeof googletag !== "undefined"){
			googletag.cmd.push(function () {
				googletag.pubads().clear();
			})
		}
	}

	function initAds() {
		if(typeof googletag !== "undefined"){
			googletag.cmd.push(function () {
				googletag.pubads().refresh();
				// googletag.pubads().display(); // does not work in single request mode
			})
		}
	}
</script>

