<script type="text/javascript">
	
		if( document.cookie.indexOf("OptanonAlertBoxClosed=") >=0){
		document.getElementById('onetrust-consent-sdk').remove();
	}
	
	function OptanonWrapper() {
		//butonul accept
		var elAccept = document.getElementById('onetrust-accept-btn-handler');
		elAccept.addEventListener("click", function (elElement) {
			AdsCookieManagement.initAdsAndCheckCookieProCookie();
			if(typeof window.objUlise !== 'undefined') {
				window.objUlise.init();
			}
			var objDate = new Date();
			objDate.setDate(objDate.getDate() + 90);
			document.cookie = "acceptcookies=1; expires=" + objDate.toUTCString() + ";path=/";
			document.body.style.overflow = 'auto';
		});

		var elPreferinte = document.getElementById('onetrust-pc-btn-handler');
		elPreferinte.addEventListener("click", function (elElement) {
			setTimeout(function () {
				// butonul Permiteti toate
				var elAcceptAll = document.getElementById('accept-recommended-btn-handler');
				elAcceptAll.addEventListener("click", function (elElement) {
					AdsCookieManagement.initAds();
					if(typeof window.objUlise !== 'undefined') {
						window.objUlise.init();
					}
					var objDate = new Date();
					objDate.setDate(objDate.getDate() + 90);
					document.cookie = "acceptcookies=1; expires=" + objDate.toUTCString() + ";path=/";
					document.body.style.overflow = 'auto';
				});
				// butonul Salveaza preferintele
				var elSalveazaPreferinte = document.getElementsByClassName("save-preference-btn-handler")[0];
				elSalveazaPreferinte.addEventListener("click", function (elElement) {
					AdsCookieManagement.initAdsAndCheckCookieProCookie();
					if(typeof window.objUlise !== 'undefined') {
						window.objUlise.init();
					}
					var objDate = new Date();
					objDate.setDate(objDate.getDate() + 90);
					document.cookie = "acceptcookies=1; expires=" + objDate.toUTCString() + ";path=/";
					document.body.style.overflow = 'auto';
				});
			}, 500);
		});
	}
</script>			
	
	

