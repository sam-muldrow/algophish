                        <script type="text/javascript">
                            function infoMarket(cid) {
                                var tagMap = {
                                    "é²ç½®": 251,
                                    "å® ç©": 221,
                                    "é£å": 231,
                                    "ææ¸¸": 233,
                                    "äºæè½¦": 215,
                                    "æè": 225,
                                    "å¹è®­": 209,
                                    "ç¥¨å¡": 212
                                };

                                var modId = (cid == "å¨é¨" || cid == "") ? "" : tagMap[cid];

                                $.getJSON('https://platformapi.cnool.net/market/GetXinxiList?pagesize=40&sort_id=' + modId, function (ret) {
                                    var marketHtml = "";
                                    for(var i=0;i<ret.length;i++){
                                        if(ret[i].cataName.length==2){
                                            ret[i].cataName = ret[i].cataName[0] +"ãã"+ret[i].cataName[1];
                                        }
                                        var style="";
                                        if(ret[i].isBusinessRecommend=="1"){
                                            style=" style='color:red'";
                                        }
                                        marketHtml += "<li><i style='width:85px'>[<a target='_blank' href='"+ret[i].cataUrl+"'>"+ret[i].cataName+"</a>]</i><a "+style+" target='_blank' title='"+ret[i].title+"' href='"+ret[i].url+"'>"+ret[i].title+"<span class='fr'>"+ret[i].content+"</span></a></li>";
                                    };
                                    $(".NumberJiaoyi").append(marketHtml);
                                });
                            };
                            infoMarket("");
                            $(".infoMarket a").click(function(){
                                $(".NumberJiaoyi").html("");
                                var Mcid = $(this).attr("data-id");
                                infoMarket(Mcid);
                            })
                        </script>
