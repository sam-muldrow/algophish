    <script type="text/javascript">
        var _cnoolProtocol = "https://";
        //try {
        //    _cnoolProtocol = navigator.userAgent.indexOf("Windows NT 5.1") >= 0 ? "http://" : "https://";
        //} catch (ex) {
        //    _cnoolProtocol = "https://";
        //}

        function replaceImageProtocol(url) {
            if(!url) {
                return "";
            }
            if(_cnoolProtocol == "https://") {
                var allowHost = ["http://upload2007.cnool.net", "http://upload2016.cnool.net" ];
                for(var i = 0; i < allowHost.length; i++) {
                    if(url.indexOf(allowHost[i]) >= 0) {
                        return url.replace("http://", "https://");
                    }
                }
            }
            return url;
        }

        //document.domain = "cnool.net";
