                <div class="focusNews-tit" style="display: none;">
                    <h3><a id="filter-right-focus" href="http://news.cnool.net" target="_blank">ä»æ¥<b>ç­ç¹</b></a></h3>
                </div>
                <div class="focusNews-body" style="display: none;">
                    <ul class="NumberNews">
                        
                        <li>
                            
                        </li>
                        
                        <li>
                            
                        </li>
                        
                        <li>
                            
                        </li>
                        
                        <li>
                            
                        </li>
                        
                        <script type="text/javascript">
                            var imgReplaceString={List:[]};
                            function ReplaceHOtImgs() {
                                var ranNoList = [];
                                for (var m = 0; m < 4; m++) {
                                    ranNoList.push(Math.random());
                                }
                                for (var i = 0; i < imgReplaceString.List.length; i++) {
                                    var tempRan = Math.random();
                                    var item = imgReplaceString.List[i];
                                    if (tempRan > ranNoList[item.GroupId - 1]) {
                                        ranNoList[item.GroupId - 1] = tempRan;
                                        var repstring = '<a class="pic" href="' + item.Url + '" target="_blank" title="' + item.Summary + '" author="' + item.CreateUser + '" nodeid="">\
                                            <img src="' + item.ImageUrl + '" alt="' + item.Title + '" width="200"><label for="">' + item.Title + '</label>\
                                        </a>';
                                        $("#hotImgNews_" + item.GroupId).html(repstring);
                                    }
                                }
                            }
                            ReplaceHOtImgs();
                            
                        </script>
                    </ul>
                </div>
            </div>
