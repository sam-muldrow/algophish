                        <script>
                            
                            var mobile = /^1[3|4|5|7|8][0-9]{9}$/;
                            $("#frm_activity_apply_btn_1").click(function(){
                                var realname01 = $("#frm_activity_apply").find(".apply-field-input")[0].value;
                                var mobile01 = $("#frm_activity_apply").find(".apply-field-input")[1].value;
                                if(!realname01){$(this).parent().parent().find(".error01").show();}else{$(this).parent().parent().find(".error01").hide();};
                                if(mobile.test(mobile01) && realname01){
                                    window.open('http://jiaju.cnool.net/zxapply/step2.aspx?realname='+ realname01 + '&mobile='+mobile01)
                                }else {
                                    $(this).parent().parent().find(".error").show();
                                }
                            });
                            $("#frm_activity_apply_btn_2").click(function(){
                                var realname02 = $("#frm_activity_apply_2").find(".apply-field-input")[0].value;
                                var mobile02 = $("#frm_activity_apply_2").find(".apply-field-input")[1].value; 
                                if(!realname02){$(this).parent().parent().find(".error01").show();}else{$(this).parent().parent().find(".error01").hide();};
                                if(mobile.test(mobile02) && realname02){
                                    window.open('http://jiaju.cnool.net/freeapply/index.aspx?realname='+ realname02 + '&mobile='+mobile02)
                                }else {
                                    $(this).parent().parent().find(".error").show();
                                }
                            });

                        </script>
