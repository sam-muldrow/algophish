    <script>
        var EventUtil = {
            addHandler: function (element, type, handler) {
                if (element.addEventListener) {
                    element.addEventListener(type, handler, false);
                }
                else if (element.attachEvent) {
                    element.attachEvent("on" + type, handler);
                } else {
                    element["on" + type] = handler;
                }
            },
            removeHandler: function (element, type, handler) {
                if (element.removeEventListener) {
                    element.removeEventListener(type, handler, false);
                }
                else if (element.detachEvent) {
                    element.detachEvent("on" + type, handler);
                } else {
                    element["on" + type] = null;
                }
            }
        }
        ;(function($){
            var ua = navigator.userAgent.toLowerCase(),
                    iphone = (ua.match(/iphone/i) == "iphone"),
                    android = (ua.match(/android/i) == "android"),
                    loc = window.location;
            //--å·¥å·
            var throttle = function(fn,delay, immediate, debounce) {
                var curr = +new Date(),//å½åæ¶é´
                        last_call = 0,
                        last_exec = 0,
                        timer = null,
                        diff, //æ¶é´å·®
                        context,//ä¸ä¸æ
                        args,
                        exec = function () {
                            last_exec = curr;
                            fn.apply(context, args);
                        };
                return function () {
                    curr= +new Date();
                    context = this,
                            args = arguments,
                            diff = curr - (debounce ? last_call : last_exec) - delay;
                    clearTimeout(timer);
                    if (debounce) {
                        if (immediate) {
                            timer = setTimeout(exec, delay);
                        } else if (diff >= 0) {
                            exec();
                        }
                    } else {
                        if (diff >= 0) {
                            exec();
                        } else if (immediate) {
                            timer = setTimeout(exec, -diff);
                        }
                    }
                    last_call = curr;
                }
            };
            var debounce = function(fn, delay, immediate) {
                return throttle(fn, delay, immediate, true);
            };
            var getH = function (d) {
                return d.offsetHeight || d.clientHeight;
            }
            var offsetTop = function(elements){
                var top = elements.offsetTop;
                var parent  = elements.offsetParent;
                while( parent != null ){
                    top += parent.offsetTop;
                    parent = parent.offsetParent;
                };
                return top;
            };
            var hasClass = function (ele, cls) {
                return ele.className.match(new RegExp('(\\s|^)' + cls + '(\\s|$)'));
            }
            //--ç¿»é¡µä¸åºå®
            function ScrollFixed(params){
                this.p = $.extend({
                    win : null,
                    dom : null,    //éè¦åºå®çdomèç¹
                    sideDom : null,
                    domWrap : null,
                    sideDomWrap : null
                },params || {});
                this.top = offsetTop(this.p.dom[0])
                this._winScroll();
            }
            ScrollFixed.prototype.excute = function(t){
                if(t > this.top + getH(this.p.dom[0]) - 1100 && t < this.top + getH(this.p.sideDomWrap[0]) - 1100){
                    this.p.dom.addClass('f-fixed');
                    this.p.dom.removeClass('f-pos');
                    this.p.dom.css('bottom','0');
                }else if(t > this.top + getH(this.p.sideDomWrap[0]) - 1100){
                    this.p.dom.removeClass('f-fixed');
                    this.p.dom.addClass('f-pos').css('bottom', '0');
                }else if(t < this.top + getH(this.p.dom[0]) - 1100){
                    this.p.dom.removeClass('f-fixed');
                    this.p.dom.removeClass('f-pos');
                }
            }
            ScrollFixed.prototype.dosm = function(t){
                this.p.dom.addClass('f-fixed');
                this.p.dom.removeClass('f-pos');
                this.p.dom.css('bottom','0');
            }
            ScrollFixed.prototype._getScroll = function(){
                var t;
                if (document.documentElement && document.documentElement.scrollTop) t = document.documentElement.scrollTop;
                else t = document.body.scrollTop;
                return t;
            }
            ScrollFixed.prototype._winScroll = function(){
                var _t = this;
                EventUtil.addHandler(this.p.win[0], "scroll", debounce(function (e) {
                    var sh = getH(_t.p.sideDomWrap[0]),
                            t = _t._getScroll();

                    if(getH(_t.p.dom[0])+50 < sh){
                        _t.p.domWrap.css('height',sh+'px');
                        _t.excute(t);
                    }
                },1));
            }
            //ç»æåºå®ç»ä»¶å¼å§   æ»å¨
            var collist = $('.Categoryinfo');
            var collistMoreWrap = $('#article_load_more');
            var fixedDomWrap = $('#fixedDomWrap');
            var sideDomWrap = $('.newsBar');
            if ($.browser.mozilla) {
                if($.browser.version >= 9){
                    var scrollFixed = new ScrollFixed({
                        win : $(window),
                        dom : $('.newsMainSide'),
                        domWrap : fixedDomWrap,
                        sideDomWrap : sideDomWrap
                    });
                }else{
                    collistMoreWrap.innerHTML = '';
                }
            }else{
                if(!(iphone || android)){
                    var scrollFixed = new ScrollFixed({
                        win : $(window),
                        dom : $('.newsMainSide'),
                        domWrap : fixedDomWrap,
                        sideDomWrap : sideDomWrap
                    });
                }
            }
        })($);
