                <script>
                    $.getJSON("https://platformapi.cnool.net/magapp/getcircleflow?cat_id=142&step=24&p=1", {
                    }).done(function (ret) {
                        
                        if (ret.errorCode && ret.errorCode == "-999") {
                            $(".mainbody-cercle").hide();
                            return;
                        }
                        var temp = ""
                        for (var i = 0; i < ret.length; i++) {
                            if (ret[i].PicUrl) {
                                var templete = '<li>\
                                <div class="pic" > <a href="'+ ret[i].Url + '" target="_blank"><img src="' + ret[i].PicUrl + '?imageView2/1/w/183/h/250"></a></div>\
                                <div class="title" > <a href="'+ ret[i].Url + '" target="_blank">' + ret[i].Title + '</a></div>\
                                    </li >'
                                temp += templete
                            }
                        }
                        $(".picList").append(temp)
                        jQuery(".picScroll-left").slide({ titCell: ".hd ul", mainCell: ".bd ul", autoPage: true, effect: "leftLoop", scroll: 6, vis: 6, trigger: "click" });
                        })
                </script>
