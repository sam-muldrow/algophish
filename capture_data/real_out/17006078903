var isTehranTimezone = function isTehranTimezone() {
  try {
    var intlName = window.Intl.DateTimeFormat().resolvedOptions().timeZone;
    if (intlName) {
      return intlName.toLowerCase().includes('tehran');
    }
  } catch (e) {}

  var now = new Date();
  return now.toString().toLowerCase().includes('iran');
};

var showPage = function showPage() {
  var errorPage;
  var headTitleElement = document.getElementsByTagName('title');

  if (isTehranTimezone()) {
    errorPage = document.getElementById('fa');
  } else {
    errorPage = document.getElementById('en');
  }

  var lang;
  if (isTehranTimezone()) {
    lang = 'fa';
  } else {
    lang = 'en';
  }

  var pageTitle = errorPage.getElementsByClassName('error-section__title')[0].textContent;
  headTitleElement[0].innerHTML = pageTitle;
  errorPage.classList.remove('error-section--hide');
  afterShowPage(errorPage, lang)
};

window.addEventListener("load", showPage);</script><script type="text/javascript">var page = { current: null };

function onloadCallback() {
    var captchaBox = page.current.getElementsByClassName('redirect__captcha')[0];
    grecaptcha.render(captchaBox, {
        'sitekey': '6Le8dMkUAAAAAEzy7WYOlWbYh0eun-xK0j5aXt6W',
        'theme': 'light',
        'callback': function (response) {
            var token = encodeURIComponent(response);
            if (token.length >= 4096) {
                var urlParams = new URLSearchParams(window.location.search);
                urlParams.set(encodeURIComponent('__arcsrc'), token);
                document.cookie = '__arcsrc=fromSearch; Max-Age=3600; path=/; Domain=' + encodeURIComponent('azmoon.iau.ir') + '';
                window.location.search = urlParams;
            } else {
                document.cookie = '__arcsrc=' + token + '; Max-Age=3600; path=/; Domain=' + encodeURIComponent('azmoon.iau.ir') + '';
                location.reload();
            }
        }
    });
}

function afterShowPage(errorPage, lang) {
    page.current = errorPage;
    document.querySelector('#arvanRecaptcha')
            .setAttribute('src', "https://www.google.com/recaptcha/api.js?hl=".concat(lang, "&onload=onloadCallback&render=explicit"));
}
</script><script id="arvanRecaptcha" defer="defer" async="async"></script></body></html>
