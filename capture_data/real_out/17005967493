<script>
    ///Hide Ads
    let _hide_ads_div_lp = getCookie('_hide_ads_lp');

    if(_hide_ads_div_lp == '1'){

        console.group("Hide Div","Oculta Espacios ADS No Premium");
        const elements = document.getElementsByClassName("ads-custom");
        const elements_2 = document.getElementsByClassName("tads-header-mobile");
        const elements_3 = document.getElementsByClassName("custom-hide-ads");
        const elements_4 = document.getElementsByClassName("ads-slot-custom");
        while (elements.length > 0) elements[0].remove();
        while (elements_2.length > 0) elements_2[0].remove();
        while (elements_3.length > 0) elements_3[0].remove();
        while (elements_4.length > 0) elements_4[0].remove();

    }else {

        console.group("Hide Div","Oculta Espacios ADS Premium");
        const elements = document.getElementsByClassName("ads-custom-premium");
        const elements_2 = document.getElementsByClassName("tads-header-mobile-premium");
        const elements_3 = document.getElementsByClassName("custom-hide-ads");
        const elements_4 = document.getElementsByClassName("ads-slot-custom-prem");
