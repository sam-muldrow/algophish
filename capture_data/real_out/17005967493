            <script>

         var date_diff_indays = function (a, e) {
        return (dt1 = new Date(a)), (dt2 = new Date(e)), Math.floor((Date.UTC(dt2.getFullYear(), dt2.getMonth(), dt2.getDate()) - Date.UTC(dt1.getFullYear(), dt1.getMonth(), dt1.getDate())) / 864e5);
    },
    today = new Date(),
    dd = String(today.getDate()).padStart(2, "0"),
    mm = String(today.getMonth() + 1).padStart(2, "0"),
    yyyy = today.getFullYear(),
    diastomadas = date_diff_indays("08/13/2021", (today = mm + "/" + dd + "/" + yyyy));
document.getElementById("contadordias").innerHTML = diastomadas;
var d = new Date(),
    weekday = new Array(7);
(weekday[0] = "Sunday"), (weekday[1] = "Monday"), (weekday[2] = "Tuesday"), (weekday[3] = "Wednesday"), (weekday[4] = "Thursday"), (weekday[5] = "Friday"), (weekday[6] = "Saturday");
var n = weekday[d.getDay()];
document.getElementById("colorcontador").className = "Saturday" == n || "Monday" == n || "Wednesday" == n ? "contador-dias grey mt-0 mb-3" : "contador-dias blue mt-0 mb-3";
if("Saturday" == n || "Monday" == n || "Wednesday" == n){
    document.getElementsByClassName("grey-yes")[0].style.display = "block";
    document.getElementsByClassName("blue-yes")[0].style.display = "none";
}else{
    document.getElementsByClassName("blue-yes")[0].style.display = "block";
    document.getElementsByClassName("grey-yes")[0].style.display = "none";
}

            </script>
