	    <script type="text/javascript" defer>


        window.addEventListener("load", (event) => {
            <!-- buscamos entre las cookie -->
            var getCookie = function (name) {
                var value = "; " + document.cookie;
                var parts = value.split("; " + name + "=");
                if (parts.length == 2) return parts.pop().split(";").shift();
            };
            let cookie_active_subscription = getCookie('piano_active_subscription');

            if(cookie_active_subscription){
                document.getElementById('portada_1x1_pc').remove();
                document.getElementById('1x1_site').remove();
            }

        });

    </script>

        <script>
        const SITE_ID = 109;
        tp = window.tp || [];

        tp.push(["addHandler", "customEvent", function(event, b, c, d) {
            console.log('Custom Event: ' +  event.eventName)
            switch (event.eventName) {
                case 'email-signup':
                    var email = '';
                    //var params;
                    //var iframeId;

                    // We are parsing the params object sent from the template to find out which iframe triggered it
                    //params = JSON.parse(event.params.params);
                    // And here's the iframeId we're looking for
                    //iframeId = params.iframeId;

                    if ((typeof event.params.email != 'undefined') && (event.params.email.length > 0)) {
                        email = event.params.email;
                    }

                    console.log('Recupera email: ' +  email)

                    const externalId = btoa(email.value)
                    envioemailhash(externalId);

                    break;
            }
        }]);

        function envioemailhash(externalId) {
            // Usa ese hash como externalId en el código que te dieron
            console.log('Envio Email Hash ')
            var cX = cX || {};
            cX.callQueue = cX.callQueue || [];
            cX.callQueue.push(['setSiteId', `${SITE_ID}`]);
            cX.callQueue.push(['invoke', function () {
                if (typeof externalId !== "undefined") {
                    cX.addExternalId({ 'id': externalId, 'type': 'elp' });
                }
            }]);
            cX.callQueue.push(['sendEvent', 'newslettersignup', { 'origin': 'elp-boletines' }]);
            console.log('cX callQueue ')
        }
    </script>
