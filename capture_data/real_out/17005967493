    function pianoAvatarMod() {

        let piano_cookie_mod = getCookie('_psm_lp');/* BR7Y9RLK,RL1NC6N */
        let _psag_lp = getCookie('_psag_lp');/*BRDPGK78,BRJVZNYP */
        let _psas_lp = getCookie('_psas_lp');
        let _psab_lp = getCookie('_psab_lp');

        if (piano_cookie_mod)  {
            let img_avatar_user_desktop  = document.getElementsByClassName('piano-avatar-user-desktop');
            let img_avatar_user_movil  = document.getElementsByClassName('piano-avatar-user-mobil');
            let avatar_accounte = document.getElementsByClassName("avatar_account");
            let piano_array_mod = piano_cookie_mod.split(',');
            let load_avatar = false;
            let _psag_lp_p = _psag_lp.split(',');
            let _psas_lp_p = _psas_lp.split(',');
            let _psab_lp_p = _psab_lp.split(',');

            for (var index in piano_array_mod) {
                for (var index2 in _psag_lp_p) {
                    if (piano_array_mod[index] == _psag_lp_p[index2]){
                        for ( var i = 0; i < img_avatar_user_desktop.length; i++) {
                            img_avatar_user_desktop[i].style.display = 'inline-block';
                            img_avatar_user_desktop[i].innerHTML = '<a><img src="https://www.laprensani.com/wp-content/themes/la_prensa_mix/assets/images/distintivo_premium_desktop.svg" alt="DEFIENDE LA DEMOCRACIA" width="45%"></a>';
                            load_avatar = true;
                        }
                        for ( var j = 0; j < img_avatar_user_movil.length; j++) {
                            img_avatar_user_movil[j].style.display = 'inline-block';
                            img_avatar_user_movil[j].innerHTML = '<a><img src="https://www.laprensani.com/wp-content/themes/la_prensa_mix/assets/images/distintivo_premium_mobile.svg" alt="DEFIENDE LA DEMOCRACIA" width="90%"></a>';
                            load_avatar = true;
                        }
                        for ( var k = 0; k < avatar_accounte.length; k++) {
                            avatar_accounte[k].classList.add("icon_premium");
                            avatar_accounte[k].innerHTML="Reconocemos tu decidido apoyo a la defensa de la justicia,democracia y libertad de expresi&oacute;n"
                            load_avatar = true;
                        }

                    }
                }
            }
        }
    }
