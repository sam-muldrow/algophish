						}
						break;
					} else {
						// if user does not has access to any resource
						_cbq.push(['_acct', 'lgdin']);
					}
				}
			}
		} else {
			// if user does not has access to any resource
			_cbq.push(['_acct', 'lgdin']);
		}
	});
} else {
	// send anon user to chartbeat
	_cbq.push(['_acct', 'anon']);
}

tp.pianoId.init();

// End Legacy code of Piano

						if (pianoReady) {
							pianoReady();
						} else {
							tp.experience.init();
						}
					}]);
				}
			};

			var timerId = setTimeout(function() {
				done();
			}, 3000);

			_matherq.push(['getTracker', function(tracker) {
				tracker.setPaywall('piano');
				if (enableUserDB) {
					tracker.getUserDBSegment({}, function(userDBData) {
						done();
					});
				} else {
					done();
				}
			}]);
