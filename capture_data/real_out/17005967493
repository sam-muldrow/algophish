      <!-- resizeAdsIAB Global Function -->        
      <script>
          const resizeAdsIAB = () => {
              // eslint-disable-next-line no-restricted-globals
              const allFrames = document.getElementsByTagName('iframe');

              // Loop through and find all the iframes
              for (let i = 0; i < allFrames.length; i += 1) {
                  try {
                  const thisFrame = allFrames[i].contentDocument
                      ? allFrames[i].contentDocument
                      : allFrames[i].contentWindow.document;
                  const metaElements = thisFrame.getElementsByTagName('meta');

                  // Loop through and find the 'chatbot' meta
                  for (let j = 0; j < metaElements.length; j += 1) {
                      if (metaElements[j].getAttribute('name') === 'chatbotIAB') {
                      allFrames[i].height = '600';
                      break;
                      }
                  }
                  } catch (error) {
                  // console.error(`ID: ${allFrames[i].id}. Exception: ${error}`);
                  }
              }
          };

          // Make function global
          window.resizeAdsIAB = resizeAdsIAB;
      </script>
        <!-- resizeAdsBubble Global Function -->        
      <script>
          const resizeAdsBubble = (width, height) => {
              // eslint-disable-next-line no-restricted-globals
              const allFrames = document.getElementsByTagName('iframe');

              // Loop through and find all the iframes
              for (let i = 0; i < allFrames.length; i += 1) {
                  try {
                  const thisFrame = allFrames[i].contentDocument
                      ? allFrames[i].contentDocument
                      : allFrames[i].contentWindow.document;
                  const metaElements = thisFrame.getElementsByTagName('meta');

                  // Loop through and find the 'chatbot' meta
                  for (let j = 0; j < metaElements.length; j += 1) {
                      if (metaElements[j].getAttribute('name') === 'chatbotBubble') {
                        allFrames[i].style.width = width;
                        allFrames[i].style.height = height;
                      break;
                      }
                  }
                  } catch (error) {
                  // console.error(`ID: ${allFrames[i].id}. Exception: ${error}`);
                  }
              }
          };

          // Make function global
          window.resizeAdsBubble = resizeAdsBubble;
      </script>
        <!-- floatAdsBubble Global Function -->        
      <script>
          const floatAdsBubble = () => {
              // eslint-disable-next-line no-restricted-globals
              const allFrames = document.getElementsByTagName('iframe');

              // Loop through and find all the iframes
              for (let i = 0; i < allFrames.length; i += 1) {
                  try {
                  const thisFrame = allFrames[i].contentDocument
                      ? allFrames[i].contentDocument
                      : allFrames[i].contentWindow.document;
                  const metaElements = thisFrame.getElementsByTagName('meta');

                  // Loop through and find the 'chatbot' meta
                  for (let j = 0; j < metaElements.length; j += 1) {
                      if (metaElements[j].getAttribute('name') === 'chatbotBubble') {
                        allFrames[i].style.position = 'fixed';
                        allFrames[i].style.bottom = '75px';
                        allFrames[i].style.right = '5px';
                        allFrames[i].style.zIndex = '9999';

                      break;
                      }
                  }
                  } catch (error) {
                  // console.error(`ID: ${allFrames[i].id}. Exception: ${error}`);
                  }
              }
          };

          // Make function global
          window.floatAdsBubble = floatAdsBubble;
      </script>
