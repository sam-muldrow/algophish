<script>
    document.cookie = "__adblocker=; expires=Thu, 01 Jan 1970 00:00:00 GMT; path=/";
    var setNptTechAdblockerCookie = function(adblocker) {
        var d = new Date();
        d.setTime(d.getTime() + 60 * 5 * 1000);
        document.cookie = "__adblocker=" + (adblocker ? "true" : "false") + "; expires=" + d.toUTCString() + "; path=/";
    };
    var script = document.createElement("script");
    script.setAttribute("async", true);
    script.setAttribute("src", "//www.npttech.com/advertising.js");
    script.setAttribute("onerror", "setNptTechAdblockerCookie(true);");
    document.getElementsByTagName("head")[0].appendChild(script);
</script>


<!-- piano composer code -->
<script>
    /**
     * Get value of query var name
     * @param name
     * @returns {RegExpExecArray | string | undefined}
     */
    function getUrlParam (name) {
        var results = new RegExp('[\\?&]' + name + '=([^&#]*)').exec(window.location.href)
        return (results && results[1]) || undefined
    }

    // Chartbeat code with Piano
    var _cbq = window._cbq = (window._cbq || []);

    var cleaned_host = location.host.indexOf('www.') && location.host || location.host.replace('www.', '');

    if (cleaned_host.search('laprensa.com.ni') !== -1) {
        cleaned_host = 'domain=.laprensa.com.ni;';
    } else {
        cleaned_host = '';
    }

    /**
     * Delete a cookie
     * @param {String} coockie_name, cookie name
     */
    function deleteCookie(coockie_name) {
        window.document.cookie = coockie_name + "=;expires=Thu, 01 Jan 1970 00:00:01 GMT;" + cleaned_host + "path=/;";
    }

    /**
     * Set a cookie
     *
     * @param name
     * @param value
     * @param days
     */
    function setCookie(name, value, days) {
        var expires = "";
        if (days) {
            var date = new Date();
            date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
            expires = "; expires=" + date.toUTCString();
        }
        document.cookie = name + "=" + (value || "") + expires + ";" + cleaned_host + "path=/";
    }

    /**
     * Get cookie by name
     * @param cname
     * @returns {string}
     */
    function getCookie(cname) {
        var name = cname + "=";
        var decodedCookie = decodeURIComponent(document.cookie);
        var ca = decodedCookie.split(';');
        for(var i = 0; i <ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    }

    /**
     * Execute Piano Login
     * @param event
     */
    function pianoBtnLoginClick() {
        if (typeof tp.pianoId !== 'undefined') {
            var btn_logins = document.getElementsByClassName('piano-btn-login');

            if ( tp.pianoId !== null ) {
                for (var k = 0; k < btn_logins.length; k++) {
                    btn_logins[k].innerText = 'Cargando...';
                }

                tp.pianoId.show({
                    screen: 'login',
                    loggedIn: function ( data ) {
                        pianoLoginSignUpCallback(data);
                    }
                });
            }

            setTimeout( function () {
                if (!tp.user.isUserValid()) {
                    for (var k = 0; k < btn_logins.length; k++) {
                        btn_logins[k].innerText = 'Iniciar Sesión';
                    }
                }
            }, 6000)
        }
    }

    /**
     * Execute Piano Logout
     * @param event
     */
    function pianoBtnLogoutClick() {
        if (typeof tp.pianoId !== 'undefined' && tp.user.isUserValid()) {
            tp.pianoId.logout(pianoLogoutCallback());
        }
    }

    /**
     * Show password reset screen
     */
    function pianoBtnPasswordReset() {
        if (typeof tp.pianoId !== 'undefined') {
            tp.pianoId.show({screen: 'restore'});
        }
    }

    /**
     * Callback for Piano Signup
     * @param data
     */
    function pianoLoginSignUpCallback( data ) {
        pianoToogleContainer();
    }

    /**
     * Calback for Piano Logout
     */
    function pianoLogoutCallback() {
        pianoToogleContainer();
        deleteCookie('__tae');
        deleteCookie('__tac');
        deleteCookie('piano_user_name');
        deleteCookie('pianobannerfixed');
        deleteCookie('piano_active_subscription');
        deleteCookie('_psm_lp');
        deleteCookie('_psc_lp');
        deleteCookie('_psp_lp');
        deleteCookie('_psag_lp');
        deleteCookie('_psas_lp');
        deleteCookie('_psab_lp');
        setCookie('_hide_ads_lp',"0", 30);
        deleteCookie('piano_name_suscriptor');
        deleteCookie('piano_email_suscriptor');
        $('#modalinfo').modal();

        setTimeout(function () {
            window.location.reload();
        }, 2000);
    }

    function pianoToogleContainer() {
        let piano_cookie_utp = getCookie('__utp');
        let piano_cookie_tac = getCookie('__tac');
        let user_name = '';
        let user_acces = false;

        if ( piano_cookie_utp ) {
            try {
                let piano_array_jwt = piano_cookie_utp.split('.');

                if ( typeof piano_array_jwt[1] !== 'undefined' ) {
                    const user_data = JSON.parse( atob( piano_array_jwt[1] ) );

                    if ( user_data ) {
                        user_name = user_data.given_name;
                    }
                }
            } catch ( e ) {
                console.log(e);
            }
        } else {
            console.log( 'No es un usuario registrado');
        }
        if ( piano_cookie_tac ) {
            try {
                user_acces = true;
            } catch ( e ) {
                user_acces = false;
            }
        }

        let btn_users = document.getElementsByClassName('piano-btn-user');
        let btn_logouts = document.getElementsByClassName('piano-btn-logout');
        let btn_passreset = document.getElementById('password-reset');
        let btn_logins = document.getElementsByClassName('piano-btn-login');
        let btn_suscribes = document.getElementsByClassName('piano-btn-suscribe');
        subscriber_comment();
        if ( user_name ) {
            for ( var i = 0; i < btn_users.length; i++) {
                btn_users[i].style.display = 'inline-block';
                btn_users[i].innerHTML = "<i class='fa fa-home'></i> Hola, " + user_name;
            }

            for ( var j = 0; j < btn_logouts.length; j++) {
                btn_logouts[j].style.display = 'inline';
            }

            btn_passreset.style.display = 'none';

            for (var k = 0; k < btn_logins.length; k++) {
                btn_logins[k].style.display = 'none';
            }

            for ( var l = 0; l < btn_suscribes.length; l++) {
                //si es usuario con suscripcion: No muestra el boton de suscribase
                if (user_acces == true)
                {
                    setCookie('piano_active_subscription',"1", 30);
                    btn_suscribes[l].style.display = 'none';
                }else {
                    //si es un usuario pero no tiene suscripcion: Muetra el botón de suscribirse
                    btn_suscribes[l].style.display = 'inline-block';
                }
            }

            pianoAvatarMod();
            pianoCookiesParameterGeneral();


        } else {
            for ( var i = 0; i < btn_users.length; i++) {
                btn_users[i].style.display = 'none';
            }

            for ( var j = 0; j < btn_logouts.length; j++) {
                btn_logouts[j].style.display = 'none';
            }

            btn_passreset.style.display = 'inline-block';

            for (var k = 0; k < btn_logins.length; k++) {
                btn_logins[k].style.display = 'inline-block';
            }

            for ( var l = 0; l < btn_suscribes.length; l++) {
                btn_suscribes[l].style.display = 'inline-block';
            }

        }
    }

    function subscriber_comment (){
        let _psm_lp = getCookie('_psm_lp');
        let _psc_lp = getCookie('_psc_lp');
        let _piano_name_suscriptor = getCookie('piano_name_suscriptor');
        let _piano_email_suscriptor = getCookie('piano_email_suscriptor');
        let iframe_comment = document.getElementsByClassName('comments-pcm');
        let iframe_commentplan = document.getElementsByClassName('comments-plan');
        let btn_updateplan = document.getElementsByClassName('piano-btn-user-plan');
        let btn_respond = document.getElementsByClassName('comments-respond');
        let show_comment = false;
        let div = document.createElement('div');
        div.innerHTML = ``;
        let nform='<form class="form-content-comment py-3 mb-3"><div class="custom-titles-aling mb-3"><span class="title-comment">COMO BENEFICIO DEL PLAN PREMIUM PODÉS COMENTAR EN TODOS NUESTROS ARTÍCULOS.</span></div> <p class="comment-form-comment custom-aling-content"><textarea class="" rows="5" id="comment" name="text" readonly onkeydown="return false"></textarea> </p> <button type="button" class="custom-aling-content btn-comentar">COMENTAR</button> </form>';

        if (_psm_lp) {

            try {
                let _psm_lp_p = _psm_lp.split(',');
                let _psc_lp_p = _psc_lp.split(',');

                for (var index in _psm_lp_p) {
                    for (var index2 in _psc_lp_p) {
                        if (_psm_lp_p[index] == _psc_lp_p[index2]){
                            for ( var i = 0; i < iframe_comment.length; i++) {
                                iframe_comment[i].style.display = 'block';
                                iframe_comment[i].prepend(div);
                            }
                            for ( var j = 0; j < btn_respond.length; j++) {
                                btn_respond[j].style.display = 'block';
                            }
                            show_comment = true;
                            break;
                        }

                    }
                    if (show_comment){
                        break;
                    }
                }
                if (!show_comment){
                    for ( var i = 0; i < iframe_comment.length; i++) {
                        iframe_comment[i].style.display = 'block';
                        iframe_comment[i].innerHTML = nform;
                    }
                    for ( var n = 0; n < iframe_commentplan.length; n++) {
                        iframe_commentplan[n].style.display = 'block';
                    }
                    for ( var n = 0; n < btn_updateplan.length; n++) {
                        btn_updateplan[n].style.display = 'inline-block';
                        btn_updateplan[n].innerHTML = 'Cambiar Plan';
                        btn_updateplan[n].href = "https://www.laprensani.com/mi-cuenta";
                    }
                }

            } catch ( e ) {
                console.log(e);
            }
            try {
                document.getElementById("author").value = _piano_name_suscriptor;
                document.getElementById("email").value =  _piano_email_suscriptor;
                document.getElementById("url").value = "lp";
            } catch ( e ) {
                console.log(e);
            }
        }else{
            for ( var i = 0; i < iframe_comment.length; i++) {
                iframe_comment[i].style.display = 'block';
                iframe_comment[i].innerHTML = nform;
            }
            for ( var n = 0; n < iframe_commentplan.length; n++) {
                iframe_commentplan[n].style.display = 'block';
            }
            for ( var n = 0; n < btn_updateplan.length; n++) {
                btn_updateplan[n].style.display = 'inline-block';
                btn_updateplan[n].innerHTML = 'SUSCRIBITE';
                btn_updateplan[n].href = "https://www.laprensani.com/suscripcion-digital";
            }
        }
    }
    function hide_ads (){

        let _psm_lp = getCookie('_psm_lp');
        let _psp_lp = getCookie('_psp_lp');
        let _hide_ads_lp = getCookie('_hide_ads_lp');
        let hide_ads = false;

        if (_psp_lp && _psp_lp) {
            if (_hide_ads_lp != 1){
                try {
                    let _psm_lp_p = _psm_lp.split(',');
                    let _psp_lp_p = _psp_lp.split(',');

                    for (var index in _psm_lp_p) {
                        for (var index2 in _psp_lp_p) {
                            if (_psm_lp_p[index] == _psp_lp_p[index2]){
                                setCookie('_hide_ads_lp',"1", 30);
                                hide_ads = true;
                                break;
                            }
                        }
                        if (hide_ads){
                            break;
                        }
                    }
                } catch ( e ) {
                    console.log(e);
                }
            }
        }else { setCookie('_hide_ads_lp',"0", 30);}
    }
    function googleEvents(response) {
        gtag( 'set', { 'user_id': response.user.uid } );
        gtag( 'config', 'UA-9793390-1'); // send user_id for pageview
        tp.push(['addHandler', 'checkoutComplete', function (conversion) {
            gtag('event', conversion.termId, {
                'event_category': 'Conversion Event',
                'event_label': 'Conversion',
                'value': conversion.chargeAmount
            });
        }]);
    }
