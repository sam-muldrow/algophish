    ///Get Modalidad Suscriptor
    tp.push(["init", function () {

        let piano_cookie_mod = getCookie('_psm_lp');
        let piano_cookie_tac = getCookie('__tac');

        if (piano_cookie_tac)  {
            if (!piano_cookie_mod) {

                tp.api.callApi('/access/list', {}, function (response) {
                    var hasAccess = false;
                    var modalidad_suscriptor = new Array();
                    var i = 0;
                    if (response.data.length > 0) {
                        for (var resource in response.data) {
                            if (response.data.hasOwnProperty(resource)) {
                                hasAccess = response.data[resource].granted;
                                if (hasAccess) {
                                    modalidad_suscriptor[i] = response.data[resource].resource.rid;
                                    i += 1;
                                }
                            }
                        }
                        if (hasAccess) {
                            setCookie('_psm_lp',modalidad_suscriptor, 30);
                            hide_ads();
                            pianoAvatarMod();
                            //subscriber_comment();
                        }
                    }
                });
            }else{
                //subscriber_comment();
            }
        }else{
            deleteCookie("_psm_lp");
            //subscriber_comment();
        }
    }]);

