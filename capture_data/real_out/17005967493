	<!-- <div id='laprensa_web_anchor'></div>-->
    <script>
        document.addEventListener("DOMContentLoaded", function() {

            function handleMutations(mutationsList, observer) {
                for (let mutation of mutationsList) {
                    let target = mutation.target;

                    // Si el div est vaco
                    if (target.textContent.trim() === "" && target.children.length === 0) {
                        target.style.height = '0px';
                        continue;
                    }

                    if (mutation.type === 'attributes' && mutation.attributeName === 'style') {
                        let currentHeight = parseInt(window.getComputedStyle(target).height, 10);
                        if (currentHeight !== 50) {
                            target.style.height = '50px';
                        }
                    }
                }
            }

            const observer = new MutationObserver(handleMutations);

            const config = {
                attributes: true,
                attributeFilter: ['style'],
                childList: true
            };

            const targetNode = document.getElementById('laprensa_mob_anchor');

            if(targetNode) {
                observer.observe(targetNode, config);
            }
        });
    </script>

