    const verticals = {"SPORT":{"theme":"blue","brand":"nairabet","predefinedPaths":["/","/bonus","/free-to-play","/quiz-for-cash","/highlights","/coupons","/today","/premier6"]},"CASINO":{"theme":"purple","brand":"nairavegas","predefinedPaths":["/new-casino","/casino"]}};
    
    const Preloader = () => ({
      statusValue: 0,
      get status() {
        return this.statusValue;
      },
      set status(value) {
        this.statusValue = value;
        this.preloadListener(value);
      },
      preloadListener: function (value) {},
      registerListener: function (externalListenerFunction) {
        this.preloadListener = externalListenerFunction;
      },
    });
    
    window['homePagePreload'] = Preloader();
    window['layoutPreload'] = Preloader();
    window['pagesPreload'] = Preloader();
 
    const basePath = '/';
    const locationPath = ('/' + window.location.pathname + '/').replace(basePath, '').replace(/\/\//g, '/').replace(/\/$/, '')
    
    let vertical;
    Object.keys(verticals).forEach(key => {
      if (!vertical && verticals[key].predefinedPaths.indexOf(locationPath) > -1) {
        vertical = key.toLowerCase();
      }
    });
    
    if (!vertical) {
       document.documentElement.classList.add('undefined-vertical-loading');
    }
    
    window['sah'] = {
      set theme({ color, brand }) {
        const overloadingElem = document.getElementById('theme-override-overload');
        if (overloadingElem) {
          overloadingElem.parentNode.removeChild(overloadingElem) ;
        }
      
        const overrideStylesElem = document.createElement('link');
        overrideStylesElem.id = 'theme-override-overload';
        overrideStylesElem.href = 'assets/themes/' + color + '.theme.css';
        overrideStylesElem.rel = 'stylesheet';
        const oldTheme = document.getElementById('theme-override');
        overrideStylesElem.onload = function(){
          if (oldTheme.parentNode) {
            oldTheme.parentNode.removeChild(oldTheme);
          } 
          overrideStylesElem.id = 'theme-override';
          document.documentElement.classList.remove('undefined-vertical-loading');
        };
        overrideStylesElem.onerror = function(){
            document.documentElement.classList.remove('undefined-vertical-loading');
        };
        oldTheme.parentNode.insertBefore(overrideStylesElem, (oldTheme.nextElementSibling || oldTheme));
      
        const favIconEl = document.getElementById('favicon-override');
        favIconEl.href = 'assets/icons/' + brand + '/favicon.ico';
      },
    };
    
    const changeTheme = (verticals, vertical) => {
      const color = verticals[vertical.toUpperCase()].theme;
      const brand = verticals[vertical.toUpperCase()].brand;
      
      window['sah'].theme = { color, brand };
    }
    
     if (vertical && verticals[vertical.toUpperCase()].theme !== 'blue') {
      document.documentElement.classList.add('undefined-vertical-loading');
      changeTheme(verticals, vertical);
    }
    
    const preloadHttpRequest = (preloadKey, endpoint, onload) => {
      window[preloadKey].status = 1;
      const xhr = new XMLHttpRequest();
      xhr.onload = function () {
        window[preloadKey].data = JSON.parse(xhr.responseText);
        window[preloadKey].status = 2;
        
        if (onload) {
          onload(window[preloadKey].data);
        }
      };
      xhr.onerror = function () {
        window[preloadKey].status = 3;
      };
      xhr.open('GET', endpoint, true);
      xhr.send(null);
    };
    
    const defaultLang = 'en';
    const savedLang = window.localStorage.getItem('sah_lang');
    const currentLanguage = savedLang ? savedLang : defaultLang;
    
    const layoutPreloadHandler = (vertical) => {
      preloadHttpRequest('layoutPreload', 'https://cms-ui-data-prd.sahara.editec-online.com/NG/layouts/vertical/'+ vertical + '/desktop/' + currentLanguage);
    };
    
    if (locationPath === '') {
      const onLoadHandler = (data) => {
        if (!vertical && data?.vertical) {
          vertical = data.vertical.toLowerCase();
          changeTheme(verticals, vertical);
          layoutPreloadHandler(vertical);
        }
      }
      preloadHttpRequest('homePagePreload', 'https://cms-ui-data-prd.sahara.editec-online.com/NG/pages/home/desktop/' + currentLanguage, onLoadHandler.bind(this));
    } else if (vertical) {
      preloadHttpRequest('pagesPreload', 'https://cms-ui-data-prd.sahara.editec-online.com/NG/pages/desktop/' + currentLanguage);
    }
    
    if (vertical) {
      layoutPreloadHandler(vertical);
    }
   
    Object.keys(verticals).forEach(key => {
      if (!vertical || key !== vertical.toUpperCase()) {
        const preloadTheme = verticals[key].theme;
        
        if (preloadTheme !== 'blue') {
          const preloadEl = document.createElement('link');
          preloadEl.id = 'theme-prefetch-' + preloadTheme;
          preloadEl.rel = 'prefetch';
          preloadEl.as = 'style';
          preloadEl.href = 'assets/themes/' + preloadTheme + '.theme.css';
          document.head.append(preloadEl);
        }
      } 
    });

    if (savedLang && savedLang !== defaultLang) {
      window['languagePreload'] = Preloader();
      preloadHttpRequest('languagePreload', 'assets/languages/' + savedLang + '.json');
    }
   
  })();
</script>
