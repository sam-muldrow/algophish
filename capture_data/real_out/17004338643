<script>
  let previousCard = "";
  let modal = document.getElementById("t-hp-ShowModal");
  let iconClose = document.getElementsByClassName("t-hp-icon-close")[0];

  function showModal() {
    modal.style.display='block';
  }

  function showModalContent(clicked_id) {
    if(previousCard!==""){
      document.getElementById(previousCard+"-modal").style.display = "none";
    }
    document.getElementById(clicked_id+"-modal").style.display = "block";
    previousCard = clicked_id
  }

  iconClose.onclick = function() {
    modal.style.display = "none";
  }

  window.onclick = function(event) {
    if (event.target == modal) {
      modal.style.display = "none";
    }
  }
</script>



