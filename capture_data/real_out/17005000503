        window.addEventListener('load', function () {
            lazyScript("https://media.ngoisao.vn/static/js/slider.js?v=2", function () {
                $('.accordion').jAccordion({
                    sticky: true,
                    slideMaxWidth: 420,
                    slideDuration: 1200,
                    autoplay: true,
                    event: 'mouseenter',
                    onSlideStartClosing: function (e) {
                        $('.title', this).delay(300).stop(true).animate({left: 0}, 500);
                        $('.description', this).fadeOut();
                        $('.thumb-bw', this).css({'position': 'absolute'}).stop(true).animate({left: -200}, 800);
                        $('.thumb', this).addClass('slideClose');
                        $('.thumb', this).removeClass('slideOpen');
                    },
                    onSlideStartOpening: function (e) {
                        $('.title', this).stop(true).animate({left: -$('.title', this).outerWidth()}, 500);
                        $('.thumb-bw', this).css({'position': 'absolute'}).stop(true).animate({left: 0}, 800);
                        $('.description', this).fadeOut();
                        $('.thumb', this).addClass('slideOpen');
                        $('.thumb', this).removeClass('slideClose');
                    },
                    onSlideOpened: function (e) {
                        $('.description', this).fadeIn();
                    }
                });
            })
        })
    </script>
