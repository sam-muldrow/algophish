.segmented-control .option label::after {
	content: '';
	width: 1px;
	background: rgba(142,142,147,.15);
	position: absolute;
	top: 14%;
	bottom: 14%;
	border-radius: 10px;
	will-change: background;
	-webkit-transition: background .2s ease;
	transition: background .2s ease;
}

