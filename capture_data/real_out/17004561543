		// Voir la documentation de `bytel-api` pour plus de détails
		cache: {
			maxAge: 15*60*1000,
			key: '@bouygues-telecom/wayana/' + window.location.href
		},
		send: 'tc_events_2',
		env: 'PROD',
		site: 'Boutique',
		debug: false
	}).then(function () {
		bytelTC.send(tcVars)
	})
}, 0);
</script></head><body><!DOCTYPE html>


