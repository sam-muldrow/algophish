	left: 0;
	transform: translateX(-.5px);
}

.segmented-control .option label::after {
	right: 0;
	transform: translateX(.5px);
}

.segmented-control .option:first-of-type {
	grid-column: 1;
	grid-row: 1;
	box-shadow: none;
}

