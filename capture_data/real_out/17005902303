<script type="text/javascript">

(function(){

    var showMoreToggle = document.getElementById('more-top-stories-toggle');
    var layout = document.getElementsByClassName('layout')[0];

    function toggleMoreStories(e)
    {
        e.preventDefault();
        var state = showMoreToggle.getAttribute('data-toggle');

        if (state === 'less') {
            showMore();
            return;
        }
        showLess();
    }

    function showMore()
    {
        showMoreToggle.setAttribute('data-toggle', 'more');
        showMoreToggle.innerHTML = 'Show fewer Top Stories <span class="arrow-up"></span>';
        layout.classList.remove('less');
        gtag('event', 'click', {
            'event_category': 'TopStores:show-fewer',
            'event_label': 'TopStories'
        });
    }

    function showLess()
    {
        showMoreToggle.setAttribute('data-toggle', 'less');
        showMoreToggle.innerHTML = 'Show more Top Stories <span class="arrow-down"></span>';
        layout.classList.add('less');
        gtag('event', 'click', {
            'event_category': 'TopStories:show-more',
            'event_label': 'TopStories'
        });
    }

    showMoreToggle.addEventListener('click', toggleMoreStories);

}());

</script>
