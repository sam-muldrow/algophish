<script type="text/javascript">
    document.addEventListener('DOMContentLoaded', function () {
        loadVotedPolls();

        var form = document.getElementById('daily-poll-redesign-form');

        if (form) {
            var pollId = form.elements['poll'].value;

            form.addEventListener('submit', function (event) {
                event.preventDefault();

                loadVotedPolls();

                if (hasVotedPoll(pollId)) {
                    alert("You've already voted!");
                    location.reload();
                    return;
                }

                votedPollAction(pollId);

                form.submit();
            });

            if (hasVotedPoll(pollId)) {
                form.style.display = 'none';
                document.getElementById('daily-poll-redesign-results').style.display = 'block';
            }
        }
    });

    let pollsHistory = 200;
    let pollArray = [];

    let loadVotedPolls = function () {
        let pollList = localStorage.getItem('polls');
        if (pollList != null) {
            pollArray = JSON.parse(pollList);
        }
    };

    let hasVotedPoll = function (pollId) {
        return pollArray.indexOf("poll_" + pollId) != -1;
    };

    let votedPollAction = function (pollId) {
        while (pollArray.length >= pollsHistory) {
            pollArray.shift();
        }
        pollArray.push("poll_" + pollId);
        localStorage.setItem('polls', JSON.stringify(pollArray));
    };

    //Array indexof on prototype, ie issues
    if (!Array.indexOf) {
        Array.prototype.indexOf = function (obj) {
            for (let i = 0; i < this.length; i++) {
                if (this[i] == obj) {
                    return i;
                }
            }
            return -1;
        }
    }
</script>
                            </div>
        
