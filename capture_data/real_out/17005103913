    window.dataLayer = window.dataLayer || [];
    window.eventTracker = window.eventTracker || {};
    eventTracker.cmd = eventTracker.cmd || [];
    var eventMapping = {
      close: { name: 'Close', ga4_event: 'di_click', ga4_clicked: 'close' },
      continue: { name: 'ClickContinue', ga4_event: 'di_click', ga4_clicked: 'continue' },
      display: { name: 'Show', ga4_event: 'di_show' },
      downloadRecommended: { name: 'ClickProgram', ga4_event: 'di_download' },
      downloadLandingPage: { name: 'ClickExternalProgram', ga4_event: 'di_external_download' },
      home: { name: 'ClickHome', ga4_event: 'di_click', ga4_clicked: 'home' },
      support: { name: 'ClickSupportPage', ga4_event: 'di_click', ga4_clicked: 'support' },
      error: {
        name: 'Error',
        labelFn: (ev) => ev.error || 'unknown error',
        nonInteractive: true,
      }
    };

    function trackEvent(action, label, nonInteraction) {
      eventTracker.cmd.push(function() {
        eventTracker.trackEvent("generic", {
          category: "RecommendedAppPopup",
          action,
          label,
          nonInteraction,
        });
      });
    }
    function isModuleScriptSupported(scriptElement) {
      if ('supports' in HTMLScriptElement) {
        return HTMLScriptElement.supports('module');
      }
      return 'noModule' in scriptElement;
    }
    function trackGA4Event(diEvent) {
      var eventMapped = eventMapping[diEvent.kind];
      if (diEvent.kind === 'error') {
        window.dataLayer.push({
          event: 'di_error',
          error_message: diEvent.error || 'unknown error'
        });
      } else {
        window.dataLayer.push({
          event: eventMapped.ga4_event,
          ...(eventMapped.ga4_clicked && { element_clicked: eventMapped.ga4_clicked }),
          di_campaign_name: diEvent.campaign?.id ? diEvent.campaign.id : 'n/a',
          campaign_delivery_logic: diEvent.campaign.logic,
          detected_country: diEvent.countryCode,
        });
      }
    }

    Object.keys(eventMapping).forEach((eventName) => {
      document.addEventListener(`rv:di:${eventName}`, (ev) => {
        var diEvent = ev.detail;
        var eventMapped = eventMapping[diEvent.kind];
        if (!eventMapped) {
          return;
        }
        var campaignIdLabelFn = (ev) => ev.campaign?.id ? ev.campaign?.id : 'n/a';
        var labelFn = eventMapped.labelFn ? eventMapped.labelFn : campaignIdLabelFn;
        if (diEvent.campaign) {
          window.dataLayer.push({
            cpiSelectionLogic: diEvent.campaign.logic,
            diCampaign: diEvent.campaign.id
          });
        }
        trackEvent(
          eventMapped.name,
          labelFn(diEvent),
          !!eventMapped.nonInteractive
        );
        trackGA4Event(diEvent);
      });
    });
    var revampScript = document.createElement('script');
    revampScript.crossorigin = 'anonymous';
