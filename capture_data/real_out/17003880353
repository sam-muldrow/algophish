    <script type="text/javascript">
        var MarketingData;
        var DAWideData34, DAWideData35;
        var DAWideData34LodingComplete = false, DAWideData35LodingComplete = false;   // WideData¸¦ ·ÎµåÇÏ¸é 1,, ±×Àü±îÁö´Â ·ÎµùÈ­¸éÀÌ ³ª¿À°Ô Ã³¸®(0), ¿¡·¯ ¹ß»ý½Ã -1

	    var getDABanners = function (reqNo) {
	        if (window.daAdController === undefined || daAdController === null) return;

	        var request = {
	            reqNo: reqNo,
	            pageType: daAdController.PageType.MAIN,
	            context: this,
	            successCb: function (banners) {
	                if (banners === undefined || banners === null || banners.length == 0) {
	                    return;
	                }

	                if (reqNo == 9000000034) {
	                    
	                    // DAWide34 Ã³¸®
	                    DAWideData34 = banners;
	                    DAWideData34LodingComplete = 1;
	                    WideBannerBiz.refreshAdBannerImage(5);
	                } else if (reqNo == 9000000035) {
	                    
	                    // DAWide35 Ã³¸®
	                    DAWideData35 = banners;
	                    DAWideData35LodingComplete = 1;
	                    WideBannerBiz.refreshAdBannerImage(6);
	                }
	            },
	            errorCb: function (err) {
	                if (reqNo == 9000000034) {
	                    DAWideData34LodingComplete = -1;
	                    WideBannerBiz.refreshAdBannerImage(5);
	                } else if (reqNo == 9000000035) {
	                    DAWideData35LodingComplete = -1;
	                    WideBannerBiz.refreshAdBannerImage(6);
	                }
	            }
	        };

	        daAdController.getDABanners(request);
	    };


	    getDABanners(9000000034);
	    getDABanners(9000000035);
	            
    </script>
    
