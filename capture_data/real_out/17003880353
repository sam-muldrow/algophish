    var lineBannerDisplay = function(isSellerMember, smileClubType) {
	    try {
		    $.ajax({
			    url: "https://m.auction.co.kr/home/GetLineBannerInfo",
			    jsonp: "callback",
			    type: 'GET',
			    data: { memberType: isSellerMember ? "B" : "A", smileClubType: smileClubType },
			    dataType: 'jsonp',
			    contentType: "application/json; charset=utf-8",
			    crossdomain: true
		    }).done(function (data) {
			    if (data.dispSeq > 0) {
				    $("#line_banner").find("a").attr("href", data.landingUrl);
				    $("#line_banner").find("a").attr("target", data.landingType);
				    $("#line_banner").find("img").attr("src", data.imageUrl);
				    $("#line_banner").find("img").attr("alt", data.title);
				    $("#line_banner").show();
			    }
		    }).fail(function (data) {
			    $("#line_banner").hide();
		    });
	    } catch (e) {
		    $("#line_banner").hide();
	    }
    }
	
	var getHashedCguid = function() {
    	    try {
    		    $.ajax({
    			    url: "https://m.auction.co.kr/home/GetCguidHash",
    			    jsonp: "callback",
    			    type: 'GET',
    			    dataType: 'jsonp',
    			    contentType: "application/json; charset=utf-8",
    			    crossdomain: true
    		    }).done(function (data) {
					// Google Tag Manager 
					window.hashedCguid =  data ;   
					dataLayer.push({ event: 'home', userId: window.hashedCguid });

					//setting click_menu
					$("#core_gnb .corenavi.primary li > a").each(function (){
						var menu_nm = $(this).text();
					  	var dataLayerText = "dataLayer.push({event: 'click_menu',name: '" + menu_nm +"',location: 'top',userId:'"+window.hashedCguid+"'});"
						$(this).attr('onclick', dataLayerText);
					});
					
    		    }).fail(function (data) {
					console.log("fail", data);
					window.hashedCguid =  "" ;    
    		    });
    	    } catch (e) {
									
    	    }
        }
    
    if (window !== 'undefined') {
  		window.onchange = function () { };
  	}
	
	

  	$(window).load(function () {
		getHashedCguid();			

  		if (uaControl.util.isTablet()) {
  			$("#ibody").addClass("tablet");
  		}

  		if (typeof (layerAd) == "function") layerAd();

  	        $.cachedScript("//script.auction.co.kr/common/ie6upgrade.js", true).done(function ()
			{ 
				var version = ie6upgrade.getIEVersion();
				if (version <= 0 || version >= 7 )
					return;
				var announcePopup;
				announcePopup = window.open("http://www.auction.co.kr/popup/IEPopUp.html", "announce", "title=no,menubar=no,location=no,toolbars=no,scroll=no,status=yes,width=600,height=480,resizable=no");
		    
			
			 });
			$.cachedScript("//script.auction.co.kr/common/help.js", false);
  		    $.cachedScript("//script.auction.co.kr/homepage/log.js", false).done(function (script, textStatus) { window.onerror = Logger.ErrorHandler;; });
  	});

	var img = document.createElement("img");
	img.src = "https://www.facebook.com/tr?id=322230407984978&ev=PixelInitialized";
  </script>

