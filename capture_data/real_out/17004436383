<script>
$(function(){
    let maxSlidesToShow = 4;

    $('.home-cat-container').find('.skeleton').fadeOut('slow');
    $('.home-cat-container').find('.scroller').fadeIn('slow');;

    $('.scroller').slick({
          infinite: false,
          speed: 300,
          slidesToShow: maxSlidesToShow,
          slidesToScroll: 3,
          variableWidth: true,
          prevArrow: '<span class="arrow-left"><i class="fal fa-chevron-left"></i></span>',
          nextArrow: '<span class="arrow-right"><i class="fal fa-chevron-right"></i></span>',
    })

    $('.scroller').on('beforeChange', function(event, slick, currentSlide, nextSlide){

        var nextSlide = parseInt( nextSlide ),
        currentSlide = parseInt( $('.scroller').slick('slickCurrentSlide') );

        if( $('.slick-track .item-scroller').eq(nextSlide).find('.item-date').length === 0){
            $('.slick-track .item-scroller').eq(nextSlide).find('.item-date-hidden').removeClass('item-date-hidden').addClass('item-date check');
        }

        if( nextSlide < currentSlide){

            if( $('.slick-track .item-scroller').eq(currentSlide).find('.check').length > 0){
                $('.slick-track .item-scroller').eq(currentSlide).find('.check').removeClass('check item-date').addClass('item-date-hidden');
            }
        }
    });

})
</script>
