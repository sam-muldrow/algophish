  const observers = fonts.map(function (font) {
    // eslint-disable-next-line no-undef
    var observer = new FontFaceObserver(font.name, { weight: font.weight });
    return observer.load(null, 10000)
      .then(function () {
        return {
          name: font.name,
          weight: font.weight,
          loaded: true
        }
      })
      .catch(function (err) {
        return {
          name: font.name,
          weight: font.weight,
          loaded: false,
          error: err
        }
      });
  });

  Promise.all(observers)
    .then(function (results) {
      results.forEach(function (result) {
        var camalize = function camalize(str) {
          return str.toLowerCase().replace(/[^a-zA-Z0-9]+(.)/g, function(match, chr) {
            return chr.toUpperCase();
          });
        }
        var className = camalize(result.name) + 'Loaded';
        if (result.loaded && !document.documentElement.classList.contains(className)) {
          document.documentElement.classList.add(className);
          sessionStorage.setItem(className, true);
        }
      })
    });
