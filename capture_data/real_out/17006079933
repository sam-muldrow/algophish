    <script>
      function addToCart(data,qty) {
        jQuery.post('/cart/add.js', {
            quantity: qty,
            id: data
          },
          function(result, status) {
            if(status == 'success'){
              document.querySelector(".modal.is-visible").classList.remove(isVisible);
              document.getElementById('miniPopup').classList.add(isVisible); 
            }
          }, 'json'
        );  
      }
      function addToCartVariant(pid) {
        let variantId = $('#v-' + pid).val()
        let quantity = $('#q-' + pid).val() ?? 1;
        if(variantId) {
          addToCart(variantId,quantity)   
        }    
      }
      function clearCart() {
        jQuery.post('/cart/clear.js', {});  
      }
      function selectVariant(vid) {
        var citem = $('#v-' + vid).val();
        $('span.' + vid).hide();
        if(citem) {
          $("#b-" + vid).prop( "disabled", false );
          $('span#' + citem).show(); 
        } else {
          $("#b-" + vid).prop( "disabled", true );  
        }
      }
      $(".mini-button").prop( "disabled", true );
      function minicart(pid,has_only_default_variant) {
        if(has_only_default_variant) {
          $(".mini-button").prop( "disabled", false );
        }
        let miniTitle = $("#mt-" + pid).val();
        $(".modal-header").show();
        document.getElementById('m-' + pid).classList.add(isVisible);
        $("#miniTitle").html(miniTitle);
      }
      function closeModal() {
        document.querySelector(".modal.is-visible").classList.remove(isVisible);  
      }
      const openEls = document.querySelectorAll("[data-open]");
      const closeEls = document.querySelectorAll("[data-close]");
      const isVisible = "is-visible";
      for (const el of openEls) {
        el.addEventListener("click", function() {
          const modalId = this.dataset.open;
          document.getElementById(modalId).classList.add(isVisible);
        });
      }
      for (const el of closeEls) {
        el.addEventListener("click", function() {
          this.parentElement.parentElement.parentElement.classList.remove(isVisible);
        });
      }
      document.addEventListener("click", e => {
        if (e.target == document.querySelector(".modal.is-visible")) {
          document.querySelector(".modal.is-visible").classList.remove(isVisible);
        }
      });
      document.addEventListener("keyup", e => {
        if (e.key == "Escape" && document.querySelector(".modal.is-visible")) {
          document.querySelector(".modal.is-visible").classList.remove(isVisible);
        }
      });      
  </script>
