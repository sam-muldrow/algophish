<script>
  var currencyCart = 'USD';
  setTimeout(function() {
    jQuery('div#lion-loyalty-panel-custom-css > div.lion-loyalty-widget').on('click', function() {
      setTimeout(function() {
        jQuery('header.lion-loyalty-panel-tier-item__header div.lion-loyalty-panel-tier-item__name').each(function( index ) {
          if(jQuery(this).text() == 'VIP') {
            var vip_text_ll = jQuery(this).parent().next().next().find('div.lion-loyalty-panel-tier-item__list-container:nth-child(1) > ul.lion-loyalty-panel-tier-item__list');
            vip_text_ll.prepend('<li class="lion-loyalty-panel-tier-item__list-item lion-loyalty-panel-tier-item__list-item--rule">Extra 5% off on every purchase</li>');
          }
        });
        jQuery('nav.lion-loyalty-panel-sidebar__menu a.lion-loyalty-panel-sidebar__menu-item--tiers').on('click', function() {
          setTimeout(function() {
            jQuery('header.lion-loyalty-panel-tier-item__header div.lion-loyalty-panel-tier-item__name').each(function( index ) {
              if(jQuery(this).text() == 'VIP') {
                var vip_text_ll = jQuery(this).parent().next().next().find('div.lion-loyalty-panel-tier-item__list-container:nth-child(1) > ul.lion-loyalty-panel-tier-item__list');
                vip_text_ll.prepend('<li class="lion-loyalty-panel-tier-item__list-item lion-loyalty-panel-tier-item__list-item--rule">Extra 5% off on every purchase</li>');
              }
            });
          }, 500);
        });
      }, 500);
    });
  }, 3000);
