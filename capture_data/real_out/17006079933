                  return text.replace('Give a friend $15 off their first purchase and earn 20,000 points if they spend over $30', 'Give a friend ' + format[0] + ' ' + (parseFloat(rate) * parseFloat(15)).toFixed(2)  + ' ' + format[1] + ' off their first purchase and earn 20,000 points if they spend over ' + format[0] + ' ' + (parseFloat(rate) * parseFloat(30)).toFixed(2)  + ' ' + format[1]); 
                });
              }
            }, 500);
          });
          
          jQuery('div.lion-history-table__bubble').on('click', function() {
            setTimeout(function() {
              if(checkCustomLocale == 'zh-CN') {
                jQuery('div.lion-history-info-modal-content__points').text(function(index, text) { 
                  return text.replace('points', '积分'); 
                });
