  src: url("//www.sasa.com/cdn/fonts/avenir_next_rounded/avenirnextrounded_n5.fedb3cf870180f3c438d61eb06b061fc851f4da3.woff2?h10=dGguc2FzYS5jb20&h1=c2FzYS1pbnRsLmFjY291bnQubXlzaG9waWZ5LmNvbQ&h2=YnV5LnNhc2EuY29t&h3=d2ViMS5zYXNhLmNvbQ&h4=c2FzYS5jb20&h5=c3NkY3FhLnNhc2EuY29t&h6=dXMuc2FzYS5jb20&h7=YXUuc2FzYS5jb20&h8=Y2Euc2FzYS5jb20&h9=Z2Iuc2FzYS5jb20&hmac=5736098a77510f56f6249f2fce92ae26494613857674ec80fbe3b16a58022f8b") format("woff2"),
       url("//www.sasa.com/cdn/fonts/avenir_next_rounded/avenirnextrounded_n5.55da6e736355e5866071900fe6ace4d67082a2b8.woff?h10=dGguc2FzYS5jb20&h1=c2FzYS1pbnRsLmFjY291bnQubXlzaG9waWZ5LmNvbQ&h2=YnV5LnNhc2EuY29t&h3=d2ViMS5zYXNhLmNvbQ&h4=c2FzYS5jb20&h5=c3NkY3FhLnNhc2EuY29t&h6=dXMuc2FzYS5jb20&h7=YXUuc2FzYS5jb20&h8=Y2Euc2FzYS5jb20&h9=Z2Iuc2FzYS5jb20&hmac=2eb5a728ce13f94fdc0aef183820b90d5655bd26f38c5409f688fe62a2c12c08") format("woff");
}

    :root {
      --font-body-family: "Avenir Next", sans-serif;
