                  }
                });
                jQuery('span.lion-currency__value').each(function( index ) {
                  if(jQuery(this).text() == '$1') {
                    jQuery(this).html(format[0] + ' ' + rate + ' ' + format[1]);
                  }
                  if(jQuery(this).text() == '$' + v) {
                    jQuery(this).html(format[0] + ' ' + (parseFloat(rate) * parseFloat(v)).toFixed(2)  + ' ' + format[1]);
                  }
                });
