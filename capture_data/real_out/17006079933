                  return text.replace('for rewards from Sasa Global eShop', ' '); 
                });
              }
              jQuery.each(rewards, function(i, v) {
                jQuery('div.lion-loyalty-splash-item__title').each(function( index ) {
                  if(jQuery(this).text().indexOf('$1 voucher') > -1) {
                    jQuery(this).html(format[0] + ' ' + rate + ' ' + format[1] + ' ' + voucherTranslate);
                  }
                  if(jQuery(this).text().indexOf('$' + v + ' voucher') > -1) {
                    jQuery(this).html(format[0] + ' ' + (parseFloat(rate) * parseFloat(v)).toFixed(2)  + ' ' + format[1] + ' ' + voucherTranslate);
                  }
                });
                jQuery('div.lion-loyalty-panel-reward-item__title').each(function( index ) {
