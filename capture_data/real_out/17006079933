                      jQuery('span.lion-currency__value').each(function( index ) {
                        if(jQuery(this).text() == '$1') {
                          jQuery(this).html(format[0] + ' ' + rate + ' ' + format[1]);
                        }
                        if(jQuery(this).text() == '$' + v) {
                          jQuery(this).html(format[0] + ' ' + (parseFloat(rate) * parseFloat(v)).toFixed(2)  + ' ' + format[1]);
                        }
                      });
                    }
                  }, 500);
                });
                jQuery('nav.lion-loyalty-panel-sidebar__menu a.lion-loyalty-panel-sidebar__menu-item--tiers').on('click', function() {
                  setTimeout(function() {
                    jQuery('span.lion-currency__value').each(function( index ) {
                      if(jQuery(this).text() == '$1') {
                        jQuery(this).html(format[0] + ' ' + rate + ' ' + format[1]);
                      }
                      if(jQuery(this).text() == '$' + v) {
                        jQuery(this).html(format[0] + ' ' + (parseFloat(rate) * parseFloat(v)).toFixed(2)  + ' ' + format[1]);
                      }
                    });
                    jQuery('li.lion-loyalty-panel-tier-item__list-item--reward').each(function( index ) {
                      if(jQuery(this).text().indexOf('$1 voucher') > -1) {
                        jQuery(this).html(format[0] + ' ' + rate + ' ' + format[1] + ' ' + voucherTranslate);
                      }
                      if(jQuery(this).text().indexOf('$' + v + ' voucher') > -1) {
                        jQuery(this).html(format[0] + ' ' + (parseFloat(rate) * parseFloat(v)).toFixed(2)  + ' ' + format[1] + ' ' + voucherTranslate);
                      }
                    });
                  }, 500);
                });
