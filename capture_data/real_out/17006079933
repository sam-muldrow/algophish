<script>
  var countdown_datetime = '22 November 2023 12:00:00';
  
  function makeCountdownTimer() {	
    var endTime = new Date(countdown_datetime + " UTC+08:00");			
    endTime = (Date.parse(endTime) / 1000);
    
    var now = new Date();
    now = (Date.parse(now) / 1000);
    
    var timeLeft = endTime - now;
    
    var days = Math.floor(timeLeft / 86400); 
    var hours = Math.floor((timeLeft - (days * 86400)) / 3600);
    var minutes = Math.floor((timeLeft - (days * 86400) - (hours * 3600 )) / 60);
    var seconds = Math.floor((timeLeft - (days * 86400) - (hours * 3600) - (minutes * 60)));
    
    if (hours < "10") { hours = "0" + hours; }
    if (minutes < "10") { minutes = "0" + minutes; }
    if (seconds < "10") { seconds = "0" + seconds; }

    if((hours == 0) && (minutes == 0) && (seconds == 0)) {
      clearInterval(cInterval);
    }
    
    $("#cdtdays").html("<strong>" + days + "</strong>" + " <span>Days</span>");
    $("#cdthours").html("<strong>" + hours + "</strong>" + " <span>Hours</span>");
    $("#cdtminutes").html("<strong>" + minutes + "</strong>" + " <span>Minutes</span>");
    $("#cdtseconds").html("<strong>" + seconds + "</strong>" + " <span>Seconds</span>");
  }
  const cInterval = setInterval(function() { 
    makeCountdownTimer(); 
  }, 1000);
</script>
