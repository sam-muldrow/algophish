<script>
    //  LI tab scroll js
    var button = document.getElementById('slide');
button.onclick = function () {
    var container = document.getElementById('life-insurence-tab-wrapper');
    sideScroll(container,'right',5,1030,5);
};

var back = document.getElementById('slideBack');
back.onclick = function () {
    var container = document.getElementById('life-insurence-tab-wrapper');
    sideScroll(container,'left',5,1030,5);
};

function sideScroll(element,direction,speed,distance,step){
    scrollAmount = 0;
    var slideTimer = setInterval(function(){
        if(direction == 'left'){
            element.scrollLeft -= step;
        } else {
            element.scrollLeft += step;
        }
        scrollAmount += step;
        if(scrollAmount >= distance){
            window.clearInterval(slideTimer);
        }
        
    }, speed);
}

// LI tab Active

$(".tab").click(function(){
  $(this).addClass('active_tab');
  $(this).siblings().removeClass('active_tab');
  $(this).parents().siblings().children().removeClass('active_tab');
});

$(function() {
  $('.tab').click(function() {
    $('.tab_details').hide();
    $('#tab' + $(this).attr('target')).show();
  });
});

// accordion view in mobile 

// Retirement Benifits Accordion orientation JS
jQuery(document).ready(function($) {
  var alterClass = function() {
    var ww = document.body.clientWidth;
    if (ww <= 767) {
      $('.tab_details > h3').addClass('accordion-title');
	var i = 1;
      $('.tab_details').each(function() {
        $(this).attr("target",  i);
        i++;
        });
       // Onclick accordion in mobile
        $(".tab_details").click(function(){
        $('html, body').animate({
                'scrollTop' : $('#tab' + $(this).attr('target')).position().top - 70
            });  
        });
    } else if (ww > 767) {

      $('.tab_details').removeAttr("target");
      $('.tab_details > h3').removeClass('accordion-title');
    };
  };
  $(window).resize(function(){
    alterClass();
  });
  alterClass();
}); 

// Onclick accordion expand in mobile 
$(".tab_details h3").click(function(){
  $(this).toggleClass("expand");
  $(this).parents().siblings().children().removeClass("expand");
  
  $(this).siblings().toggleClass("show_info");
  $(this).parents().siblings().children().siblings().removeClass("show_info");
  
});


      </script></div>


             </div>
</section>             





















