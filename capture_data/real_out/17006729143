<script>
var icicilfObj = {};
var rootObj = this;

// Function to Swiper slider
icicilfObj.slider = function(obj, opt) {
    rootObj[obj] = new Swiper($(obj).find('.swiper-container'), opt);
}

    // Function to slider if Payout
    if ($("#cust-testimonals-swiper.js-swiper-review").length != 0) {
        var opt ={
slidesPerView: 2,
spaceBetween: 30,
slidesPerGroup: 2,
loop: true,
speed: 800,
autoplay: {
        delay: 8000,
   },
pagination: {
el: '.swiper-pagination',
clickable: true,
},
