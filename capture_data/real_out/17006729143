<script type="text/javascript" src="/etc/designs/icicipru/quotejs/swiper/swiper.js"></script>
<script>
var icicilfObj = {};
var rootObj = this;
var opt1 = {
        autoplay: {
          delay: 15000,
          disableOnInteraction: true
        },

        navigation: {
          nextEl: "#hp-banners-first-fold-swiper  .swiper-button-next",
          prevEl: "#hp-banners-first-fold-swiper .swiper-button-prev",
        },
        pagination: {
          el: "#hp-banners-first-fold-swiper .swiper-pagination",
 clickable: true,
        },
        keyboard: true,
breakpoints: {
              
                767: {
                    slidesPerView: 1,
                    slidesPerGroup: 1,
                }
    }
}  ;

// Function to Swiper slider
icicilfObj.slider = function(obj, opt) {
    var swipobj= '.swiper-container';
    rootObj[obj] = new Swiper($(obj).find(swipobj), opt);
}

    // Function to slider if Payout
    if ($("#hp-banners-first-fold-swiper").length != 0) {
        icicilfObj.slider("#hp-banners-first-fold-swiper", opt1);
    };


</script>




