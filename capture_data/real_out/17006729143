var icicilfObj = {};
var rootObj = this;

// Function to Swiper slider
icicilfObj.slider = function(obj, opt) {
    rootObj[obj] = new Swiper($(obj).find('.swiper-container'), opt);
}

    // Function to slider if Payout
    if ($("#demoid.js-swiper-review").length != 0) {
        var opt ={ 
speed: 800,
            slidesPerView: 3,
            spaceBetween: 60,
            slidesPerGroup: 3,
autoplay: {
        delay: 8000,
        disableOnInteraction: false,
      },
            pagination: {
                el: '#demoid .swiper-pagination',
                type: 'bullets',
                clickable: true
            },

            breakpoints: {
                // when window width is <= 320px
                767: {
                    slidesPerView: 1,
                    slidesPerGroup: 1,
                    spaceBetween: 10
                },
               1024: {
                    slidesPerView: 2,
                    slidesPerGroup: 2,
                    spaceBetween: 20
                },
            }
        }  ;
        icicilfObj.slider("#demoid.js-swiper-review", opt);
    };

</script>
