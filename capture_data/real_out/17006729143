<script type="text/javascript"></script>
<script>
var icicilfObj = {};
var rootObj = this;

// Function to Swiper slider
icicilfObj.slider = function(obj, opt) {
    rootObj[obj] = new Swiper($(obj).find('.swiper-container'), opt);
}

    // Function to slider if Payout
    if ($("#all-insurance-slider-wrapper.js-swiper-review").length != 0) {
        var opt ={
slidesPerView: 3,
spaceBetween: 13,
slidesPerGroup: 1,
loop: false,
speed: 800,
autoplay: {
        delay: 8000,
   },
pagination: {
el: '#all-insurance-slider-wrapper .swiper-pagination',
clickable: true,
},
navigation: {
nextEl: '#all-insurance-slider-wrapper .swiper-button-next',
prevEl: '#all-insurance-slider-wrapper .swiper-button-prev',
},
breakpoints: {
                              
          767: {
            slidesPerView: 1,
            spaceBetween: 5,
          },           
          990: {
            slidesPerView: 2,
            spaceBetween: 10,
          },            
          1024: {
            slidesPerView: 3,
            spaceBetween: 10,
          }

    }
}  ;
        icicilfObj.slider("#all-insurance-slider-wrapper.js-swiper-review", opt);
    };

</script>
