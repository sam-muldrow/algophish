                // when window width is <= 320px
                767: {
                    slidesPerView: 1,
                    slidesPerGroup: 1,
                    spaceBetween: 10
                }
    }
}  ;
        icicilfObj.slider("#cust-testimonals-swiper.js-swiper-review", opt);
    };

</script>
