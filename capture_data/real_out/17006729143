.persona-tiles-wrapper.persona-mobile-tiles .persona-tiles-list .persona-tiles {width: 100%;height: 90px;margin-bottom: 10px;border-radius: 12px;box-shadow: none;border: 1px solid #e3e3e3;padding: 20px 10px;}
.persona-tiles-wrapper .persona-tiles-list .persona-tiles > i {width: 47px;height: 47px;background-size: 47px;}
.persona-tiles-wrapper.persona-mobile-tiles .persona-tiles-list .persona-tiles h2 {height: 47px;text-align: left;padding-left: 7px;}
.persona-tiles-wrapper .persona-tiles-list .persona-tiles svg {width: 8px;right: 10px;bottom: 5px;}
.persona-tiles-wrapper.persona-mobile-tiles .persona-tiles-list .persona-tiles.persona-tiles-info + .more-discription a.goal-lnk-desk {
    display: none;
}
}
@media (max-width: 360px){
.persona-tiles-wrapper .persona-tiles-list {
    width: calc(50% - 10px);
}

}


/*Goal Tag Line*/

.goal-tag-line {display: block;position: absolute;top: -9px;left: 25px;margin: 0;background: #ef8023;border-radius: 15px;color: #fff;font-size: 12px;line-height: 12px;padding: 5px 15px;z-index: 9;}
@media (max-width: 767px){
.persona-tiles-wrapper.persona-mobile-tiles {padding-top: 10px;}
    .goal-tag-line {font-size: 10px;padding: 5px 10px;left: 10px;z-index: 0;}
}
