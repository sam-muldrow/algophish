<script>

// mobile persona tiles JS
jQuery(document).ready(function($) {
  var alterClass = function() {
    var ww = document.body.clientWidth;
    if (ww <= 767) {
      $('.persona-tiles-wrapper').addClass('persona-mobile-tiles');
      $(".persona-tiles").click(function(){
            $(this).addClass("persona-tiles-info");           
        });
        $(".close-tile").click(function(){
            $(this).parent().siblings().removeClass("persona-tiles-info");
        });
        
    } else if (ww > 767) {
      $('.persona-tiles-wrapper').removeClass('persona-mobile-tiles');
    };
  };
  $(window).resize(function(){
    $(".persona-tiles").removeClass("persona-tiles-info");
    alterClass();
  });
  alterClass();
});  


$(document).ready(function(){
  if(screen.width<1025){
    $('.goal-lnk-desk').remove();
  }
});
</script></div>
