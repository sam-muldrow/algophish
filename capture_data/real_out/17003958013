        <script type="text/javascript">
            (function(){
                var add_div = document.getElementById('add_div1');
                var arr = '';
                var els = add_div.getElementsByTagName('div');
                if(!els) return;
                for (var i=0; i<els.length; i++) {
                    var classes = els[i].className.split(' ');
                    if (arrIndexOf( classes,  'wrap__AD') != -1 ) {
                        arr = els[i];
                        break;
                    }
                }
                if(arr=='') return;
                //是否半通栏
                if(arrIndexOf( arr.parentNode,  'w1200') != -1  ){
                    arr.style.marginTop = '5px';
                }else{
                    arr.parentNode.parentNode.style.marginTop = '5px';
                }


                //查找css
                function arrIndexOf(arr, v) {
                    for (var i=0; i<arr.length; i++) {
                        if (arr[i] == v) {
                            return i;
                        }
                    }
                    return -1;
                }
            }())
        </script>

