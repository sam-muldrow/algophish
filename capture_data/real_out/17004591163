				<script type="text/javascript">
				$(document).ready(function() {
					if($('#HomeIndexSlidesContainer > ul').length > 1){
						var slideBox = document.getElementById('HomeIndexSlidesContainer');
						var delay = 5000,speed = 20,h = 19;
						var timer = null;
						var setTimer = function(){timer=setInterval(slide, speed);}
						var slide = function(){
							slideBox.scrollTop += 1;
							if(parseInt(slideBox.scrollTop) > 0 && parseInt(slideBox.scrollTop)%h == 0){
								clearInterval(timer);
								slideBox.appendChild(slideBox.getElementsByTagName('ul')[0]);
								slideBox.scrollTop = 0;
								setTimeout(setTimer, delay);
							}
						}
						setTimeout(setTimer, delay);
					}
				});
				</script>
