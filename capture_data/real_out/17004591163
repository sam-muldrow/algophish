<!-- End highlight article -->
<script>
var channel_flag = 'home';
var ga_label_prefix = 'desktop|home';

$(document).ready(function(){
	highlighteventActive();
});

function highlightchangeSlide(slide_idx, isInitialCall, type) {
	isInitialCall = (typeof isInitialCall == 'undefined') ? false : isInitialCall;
	type = (typeof type == 'undefined') ? 'auto' : type;

	var max_slide_idx = $('#HighlightContainer .SliderIndicator > div').length;

	if (slide_idx > max_slide_idx) slide_idx = 0;

	var currentTypeDiv = $('.SliderWrapper').children().get(slide_idx);
	var currentType = $(currentTypeDiv).data('slider');
	$('#MiddleTabControl li[data-type="'+currentType+'"]').addClass('Selected').siblings().removeClass('Selected');
	var left = -100* slide_idx + '%';
	$('#HighlightArticleWrapper .SliderWrapper').css('left', left);
	if (typeof ga === "function") {
		if(!isInitialCall){
			var ga_label = ga_label_prefix +  '|MiddleTab|' + currentType;
			//ga('send', 'event', type, 'click', ga_label, {'nonInteraction': 1});
			gtag('event', 'tab', { 'event_category': 'click', 'event_label': ga_label});
		}
	}
}

function highlightchangeSlideLeft(){
	var max_slide_idx = $('#HighlightContainer .SliderIndicator > div').length;
	var current_index = $('#HighlightContainer .SliderIndicator div[class*="Selected"]').index();
	if(current_index == 0){
		next_index = max_slide_idx - 1;
	}else{
		next_index = current_index - 1;
	}
	highlightchangeSlide(next_index, false, 'arrow');
	$('#HighlightContainer .SliderIndicator div:eq('+next_index+')').addClass('Selected').siblings().removeClass('Selected');
}

function highlightchangeSlideRight(){
	var max_slide_idx = $('#HighlightContainer .SliderIndicator > div').length;
	var current_index = $('#HighlightContainer .SliderIndicator div[class*="Selected"]').index();
	if(current_index == (max_slide_idx - 1)){
		next_index = 0;
	}else{
		next_index = current_index + 1;
	}
	highlightchangeSlide(next_index, false, 'arrow');
	$('#HighlightContainer .SliderIndicator div:eq('+next_index+')').addClass('Selected').siblings().removeClass('Selected');
}

function highlighteventActive(){
	randomSliderHandler();

	$('#HighlightContainer .SliderIndicator div').click(function(){
		$(this).addClass('Selected').siblings().removeClass('Selected');
		highlightchangeSlide($(this).index(), false, 'dot');
	});

	$('#HighlightArticleWrapper .Slider.Left div').click( function(){
		highlightchangeSlideLeft();
	});

	$('#HighlightArticleWrapper .Slider.Right div').click( function(){
		highlightchangeSlideRight();
	});

	$('#MiddleTabControl li').click( function(){
		var tag_type = $(this).data('type');
		var sliderByType = $('.SliderWrapper > div[data-slider="'+tag_type+'"]');
		var sliderIndex = $(sliderByType[0]).index();
		highlightchangeSlide(sliderIndex, false, 'tab');
		$('#HighlightContainer .SliderIndicator div:eq('+sliderIndex+')').addClass('Selected').siblings().removeClass('Selected');
	});
}

/* Random Slider */
function randomSliderHandler(){
	var tag_type = $('#MiddleTabControl li.Selected').data('type');
	var sliderByType = $('.SliderWrapper > div[data-slider="'+tag_type+'"]');
	var countSlider = $('.SliderWrapper > div[data-slider="'+tag_type+'"]').length;
	var x = Math.floor(Math.random() * 2);
	var sliderIndex;
	if(countSlider == 2){
		sliderIndex = $(sliderByType[x]).index();
	}else{
		sliderIndex = $(sliderByType[0]).index();
	}
	highlightchangeSlide(sliderIndex,true); //isInitialCall=true
	$('#HighlightContainer .SliderIndicator div:eq('+sliderIndex+')').addClass('Selected').siblings().removeClass('Selected');
}
</script>
