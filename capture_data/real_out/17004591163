										<style type="text/css">
	.NewsTVContainer{width:206px; height:174px; overflow:hidden; margin:0px; padding:0px; position:relative; background-size:100%; background-repeat:no-repeat; background-position:center center; background-image:url("/ajax/live_video/hottalk/images/newstv_bg_desktop_v2.jpg"); }
	.NewsTVTopic{width:126px; height:44px; position:absolute; bottom:6px; right:11px; display:table; border-spacing:0px; text-decoration:none;}
    .NewsTVTopic p{font-size:12px; line-height:13px; pointer-events:none; font-weight:bold; font-family:Helvetica,Arial,'微軟正黑體','Microsoft JhengHei',sans-serif; color:#000000; display:table-cell; vertical-align:middle;}
	.NewsTVSensor{width:100%; height:56px; position:absolute; left:0%; bottom:0%; z-index:3; cursor:pointer;}
	.NewsTVLiveIcon{width:40px; display:inline-block; position:absolute; top:5px;right:5px;  z-index:2; -webkit-animation:BLINK 1s infinite alternate ease-in-out both; -moz-animation:BLINK 1s infinite alternate ease-in-out both; -o-animation:BLINK 1s infinite alternate ease-in-out both; animation:BLINK 1s infinite alternate ease-in-out both;}
	.NewsTVLiveIcon img{width:100%;}
    .NewsTVContainer #live_youtube_video{width:200px; height:113px; overflow:hidden; position:absolute; top:4px; left:3px; margin:0px; padding:0px; font-size:0px;}
	.etnetTVButtonTop{width:206px; height:24px;}
</style>
<div class="etnetTVButtonTop"><a href="/www/tc/video/" target="_blank"><img src="/www/tc/common/images/etnettv_button_top.png" width="206" /></a></div>
<div class="NewsTVContainer">
	<div class="NewsTVSensor"></div>
	<div class="NewsTVLiveIcon"><img src="/ajax/live_video/NewsTV/images/live_icon.png" alt="Live"/></div>	<div class="NewsTVTopic"></div>	<iframe id="live_youtube_video" width="200" height="113" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen style="display:none;"></iframe></div>
<script>
var video_title = '';
var max_title_length = 26;
var needLiveYTVideo = true;
var video_lv_link = '';
var video_click_link = '/www/tc/video/hottalk/';
var livevideo_ga_event_label = 'desktop|home|liveVideo|hottalk';
var live_programid='4';
var param_trial = '';
var ajax_live_video_server_domain = 'www.etnet.com.hk';

$(document).ready(function(){
	$.getJSON("//"+ajax_live_video_server_domain+"/ajax/live_video/entry_points.php?programid="+live_programid+param_trial, function(data) {
		if(data.entry_points_status == 'ON'){
			var video_title = data.programme.title;
			var video_title_len = (typeof video_title !== 'undefined') ? video_title.length : 0;
			if(typeof data.programme.entry_point_link !== 'undefined') {
				var entry_point_link = data.programme.entry_point_link;
				if(entry_point_link == 'youtube' && typeof data.programme.youtube_link !== 'undefined' && data.programme.youtube_link != '') {
					video_lv_link = data.programme.youtube_link;
				} else if(entry_point_link == 'fb' && typeof data.programme.fb_link !== 'undefined' && data.programme.fb_link != '') {
					video_lv_link = data.programme.fb_link;
				}
			}
			if(video_click_link == '') video_click_link = video_lv_link;

			if(needLiveYTVideo && typeof data.programme.entry_point_link != 'undefined' && data.programme.entry_point_link == 'youtube') {
				var youtube_link = video_lv_link;
				youtube_link = youtube_link.replace('youtube.com/live/', 'youtube.com/watch?v=');

				var youtube_link_search = new URL(youtube_link).search;
				var youtube_link_parameters = new URLSearchParams(youtube_link_search);
				var video_id = youtube_link_parameters.get('v');
				if(typeof video_id !== 'undiefined' && video_id != null) {
					$('#live_youtube_video').attr('src', 'https://www.youtube.com/embed/'+video_id+'?rel=0&autoplay=1&mute=1');
					$('#live_youtube_video').css('display', 'block');
				}
			}

			if(video_title_len > max_title_length) video_title = video_title.substring(0, max_title_length-2) + '...';

			$('.NewsTVTopic').html('<p>'+video_title+'</p>');
			$('.NewsTVContainer').click(function(){
				gaTracking('link', 'click', livevideo_ga_event_label, video_click_link, true, true);
				// stop playing video
				if (live_programid == '4') {
					$('#live_youtube_video').attr('src', video_link.replace('www.youtube.com/watch?v=','www.youtube.com/embed/'));
				}
			});
		}
	});
});
</script>
									</div>
