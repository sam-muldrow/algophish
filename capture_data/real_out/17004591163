<script type="text/javascript">
$(document).ready(function() {
	if($("#IPOSlidesContainer > h2").length > 1 || $("#IPOSlidesContainer > div").length > 1){
		var slideBox = document.getElementById('IPOSlidesContainer');
		var delay = 5000,speed = 20,h = 33;
		var timer = null;
		var setTimer = function(){timer=setInterval(slide, speed);}
		var slide = function(){
			slideBox.scrollTop += 1;
			if(parseInt(slideBox.scrollTop) > 0 && parseInt(slideBox.scrollTop)%h == 0){
				clearInterval(timer);
				//slideBox.appendChild(slideBox.getElementsByTagName('h2')[0]);
				$(slideBox).append($(slideBox).children().first());
				slideBox.scrollTop = 0;
				setTimeout(setTimer, delay);
			}
		}
		setTimeout(setTimer, delay);
	}
});
</script>
