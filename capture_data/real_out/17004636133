<script>
            require(["jquery", "owl", "domReady!"], function($, owl){
                var cards = $('#cards');
                var carouselSettings = {
                    nav: false,
                    loop: false,
                    items: 2,
                    dots: false,
                    autoplay: false,
                    navRewind: false,
                    stagePadding: 40,
                    margin: 10,
                    slideBy: 1,
                    smartSpeed: 850,
                    autoHeight: true,
                    responsive:{
                        0: {
                            items: 1
                        },
                        576: {
                            items: 2
                        },
                        970: {
                            items: 3
                        }
                    }
                };

                function resizeCarousel() {
                    if ($(window).width() < 991) {
                        $('#cards').owlCarousel(carouselSettings);
                    } else {
                        $('#cards').owlCarousel('destroy');
                    }
                }

                $(document).ready(function() {
                    $('#cards').owlCarousel(carouselSettings);
                    resizeCarousel();
                });

                $(window).resize(resizeCarousel);
            });
        </script>
<script>
    require([
        'jquery',
        'domReady!'
    ], function($){
        $('.used-by a').on('click', function(e) {
            e.preventDefault();
            const offSetheight = $('#site-header').outerHeight() - 1;
            $('html, body').animate({ scrollTop: $('#scrollToPoint').offset().top - offSetheight}, 500);
        })
    });
</script>
<script>
    require(["jquery", "domReady!"], function($){
        const barFooter = $('#productNavFooter');
        const barPage = $('.used-by');
        const nav = $('#productNavFooter .fixed-bar');

        function isFixed() {
            const eHeight = nav.height();
            const wHeight = $(window).height();
            const scrollTop = $(window).scrollTop();
            const pageHitPos = (barPage.offset().top - ($('#site-header').height() - barPage.height()));
            const footerHitPos = (barFooter.offset().top - wHeight + eHeight);

            if (scrollTop > pageHitPos) {
                barFooter.addClass('show');
            } else {
                barFooter.removeClass('show');
            }

            if (scrollTop > footerHitPos) {
                barFooter.removeClass('fixed');
            } else {
                barFooter.addClass('fixed');
            }
        }

        $(document).ready(isFixed);
        $(window).on('resize scroll orientationchange', function () {
            isFixed()
        });

    });
</script>
