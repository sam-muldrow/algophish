<script>
    (function () {

        // ËÑË÷ÂñµãÊÂ¼þ
        function searchCollect(data) {
            try {
                if (params_buried_public) {
                    params_buried_public.stay_time =
                        new Date().getTime() - params_buried_public.device_info.local_time
                }
                // ×Ô¶¯²¹È«Â·¾¶
                if (data.jumpUrl.indexOf('.html') == -1) {
                    data.jumpUrl = location.origin + location.pathname + data.jumpUrl
                }
                var propertyMap = {
                    '101107000200004001': data.buttonText,
                    '101107000200004002': 1,
                    '101107000200004003': data.searchParams,
                    '101107000200004004': data.jumpUrl
                }
                console.log(propertyMap)
                event_collect &&
                    event_collect(
                        '101',
                        '107',
                        '0002',
                        '101107000200004',
                        propertyMap,
                        params_buried_public
                    )
            } catch (error) {
                console.log(error)
            }
        }

        // Çå³ý¿Õ¸ñ
        function strTrim(str) {
            return str.replace(/^\s*|\s*$/g, "");
        }
        var actionArr = new Array(), inputNameArr = new Array();
        actionArr = [
            "//search.mysteel.com/so.html",
            "//search.mysteel.com/price.html",
            "//search.mysteel.com/article.html",
            "//search.mysteel.com/fastcomment.html",
            "//e.mysteel.com/zy",
            "//e.mysteel.com/gq/gy",
            "//e.mysteel.com/gq/qg",
        ];
        inputNameArr = ["kw", "kw", "kw", "kw", "keyword", "searchKeyWords", "searchKeyWords"];
        function footSearchFun() {
            // Î²²¿ËÑË÷ÈÕÆÚ
            function tow(o) {
                return o = o >= 10 ? o : "0" + o;
            }
            // ÄêÔÂÈÕ
            var d = new Date(), y = d.getFullYear(), m = d.getMonth() + 1, day = d.getDate();
            e = new Array(" ÐÇÆÚÈÕ", " ÐÇÆÚÒ»", " ÐÇÆÚ¶þ", " ÐÇÆÚÈý", " ÐÇÆÚËÄ", " ÐÇÆÚÎå", " ÐÇÆÚÁù"),
                Stamp = new Date();
            var nowDate = y + "Äê" + tow(m) + "ÔÂ" + tow(day) + "ÈÕ" + e[Stamp.getDay()]
            document.querySelector("#footer-search-date").innerHTML = nowDate
            var urlIndex = "0", inputNameIndex = "0";
            // ËÑË÷tabÇÐ»»ÊÂ¼þ
            var btns = document.querySelectorAll("#footer-search-menu li");
            for (var i = 0; i < btns.length; i++) {
                (function (index) {
                    btns[index].onclick = function (e) {
                        urlIndex = index;
                        inputNameIndex = index;
                        tabInit()
                        this.setAttribute('class', 'cur')
                        // this.classList.add('cur');
                    }
                })(i);
            }
            //³õÊ¼»¯tab
            var tabInit = function () {
                for (var i = 0; i < btns.length; i++) {
                    // btns[i].classList.remove('cur')
                    btns[i].setAttribute('class', '')
                }
            }
            // ËÑË÷°´Å¥µã»÷ÊÂ¼þ
            var searchBtn = document.querySelector('#footer-search-btn')
            searchBtn.onclick = function () {
                var searchValue = document.querySelector('#footer-search-input').value,
                    jumpUrl = ""

                if (strTrim(searchValue) !== "") {
                    jumpUrl = actionArr[urlIndex] + "?" + inputNameArr[inputNameIndex] + "=" + encodeURI(searchValue)
                } else {
                    jumpUrl = actionArr[urlIndex]
                }

                searchCollect({
                    buttonText: 'µã»÷µ×²¿ËÑË÷°´Å¥',
                    searchParams: {
                        keyWord: searchValue
                    },
                    jumpUrl: jumpUrl
                })

                window.open(jumpUrl);
            }
        }
        footSearchFun();
    })();
</script></div>
