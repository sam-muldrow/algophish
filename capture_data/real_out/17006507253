        function getCity(startProvinceCode, startProvince, startCityCode, startCity) {
            window.open("https://www.56jiyun.com/freight_query.html?startProvinceCode=" + encodeURIComponent(
                startProvinceCode) + "&startProvince=" + encodeURIComponent(startProvince) + "&startCityCode=" +
                encodeURIComponent(startCityCode) + "&startCity=" + encodeURIComponent(startCity), "_blank");
        }

        // »ñÈ¡µ±Ç°Ê±¼äºÍ¶¥²¿¹ã¸æµÄÆðÊ¼Ê±¼äºÍ½áÊøÊ±¼ä
        var nowTime = new Date().getTime();
        var noticeStartTime = new Date("2022/12/17 12:00:00").getTime();
        var noticeEndTime = new Date("2022/12/18 13:00:00").getTime();

        // ÅÐ¶Ïµ±Ç°Ê±¼äÊÇ·ñÔÚÉè¶¨µÄÊ±¼ä·¶Î§ÄÚ
        if (nowTime >= noticeStartTime && nowTime <= noticeEndTime) {
            $("#notice-tip").show(); //¶¥²¿Í¨ÖªÏÔÊ¾
        } else {
            $("#notice-tip").hide(); //¶¥²¿Í¨ÖªÒþ²Ø
