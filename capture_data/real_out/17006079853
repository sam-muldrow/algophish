function updateTCString() {
    if (typeof __uspapi !== "undefined")
    {
        __uspapi("getUSPData", 1, (uspData, success) =>{
            if(success)
            {
                window.ccpa_cmp_string = uspData.uspString;
                // bellow event reserved for future use
                //let customCCPAEvent = new CustomEvent("iris:ccpa_cmp_string_ready");
                //window.dispatchEvent(customCCPAEvent);
            }
        });
    }
}
