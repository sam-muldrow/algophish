var yoRefreshResources =  function () {
  console.log('clearing cache');
  Yo.serviceWorker.cache.removeResources({
    name: 'html-cache',
    resources: [/.*/],
    revalidateOn: "request"
  });
}

var yoRemoveResources =  function () {
  console.log('clearing cache');
  Yo.serviceWorker.cache.removeResources({
    name: 'html-cache',
    resources: [/.*/],
    revalidateOn: "request"
  });
}



Yo.sequence.xhr('Cart-AddProduct').defer(0).afterLoaded(function(){
  Yo.serviceWorker.get("https://www.ralphlauren.com/ralph.service.worker.js").then(function (worker) {
    console.log('clearing cache via afterLoaded on Cart-AddProduct');
    Yo.serviceWorker.cache.removeResources({
      name: 'html-cache',
      resources: [/.*/]
    });
  }, function (error) {
    Yo.utils.log('Attempted to clear service worker cache but the service worker is not installed.  Error: ', error);
  });
})
Yo.sequence.xhr('Account-HandleLogin').defer(0).afterLoaded(function(){
  Yo.serviceWorker.get("https://www.ralphlauren.com/ralph.service.worker.js").then(function (worker) {
    console.log('clearing cache via afterLoaded on Account-HandleLogin');
    Yo.serviceWorker.cache.removeResources({
      name: 'html-cache',
      resources: [/.*/]
    });
  }, function (error) {
    Yo.utils.log('Attempted to clear service worker cache but the service worker is not installed.  Error: ', error);
  });
})
</script><!-- INSERT BY TRANSFORMER. SELECTOR=head,LOCATION=prepend --><script>
Yo.tec = {

    ready: false,

    initializeCore: function(cacheNames, serviceWorkerScriptUrl) {
        const state = Yo.tec.state();
        state.cacheNames = cacheNames;
        state.cacheUpdates = [];
        state.serviceWorkerScriptUrl = serviceWorkerScriptUrl;
        Yo.tec.state(state);
        Yo.tec._serviceWorkerReady();
    },

    state: function() {
        if (arguments.length == 0){
            try {
                return JSON.parse(localStorage.getItem('Yo.tec.state')) || {};
            }catch (e) {
                Yo.utils.log("Yo.tec: Error getting state", e);
            }
            return {};
        }else{
            try {
                arguments[0].updatedAt = new Date();
                localStorage.setItem('Yo.tec.state', JSON.stringify(arguments[0]));
                return arguments[0];
            }catch (e) {
                Yo.utils.log("Yo.tec: Error updating state", e);
            }
        }
    },

    createCache: function(cacheObject, force) {
        Yo.pubsub.subscribe({ topic: "tec/ready", callback: function (message, topic) {
            const state = Yo.tec.state();
            if (state.cacheNames.indexOf(cacheObject.name) === -1) {
                Yo.utils.log("Yo.tec: Adding cache object that was NOT already created", cacheObject);
                state.cacheNames.push(cacheObject.name);
                Yo.serviceWorker.cache.create(cacheObject);
            }else if (force === true) {
                Yo.utils.log("Yo.tec: Forcing cache object to be created that WAS already created", cacheObject);
                Yo.serviceWorker.cache.create(cacheObject);
            }
            Yo.tec.state(state);
        }});
    },

    refreshResources: function (resources, cacheNames) {
        const cacheNamesToUpdate = cacheNames || Yo.tec.state().cacheNames;
        Yo.utils.log('Yo.tec: refreshResources for ', cacheNamesToUpdate, resources || [/.*/]);
        cacheNamesToUpdate.forEach(function(name) {
            const state = Yo.tec.state();
            if (state.cacheUpdates.indexOf(name) == -1) {
                state.cacheUpdates.push(name);
                Yo.tec.state(state);
                Yo.serviceWorker.cache.refreshResources({
                    name: name,
                    resources: resources || [/.*/]
                });
            }
        });
    },

    removeResources: function (resources, cacheNames) {
        const cacheNamesToUpdate = cacheNames || Yo.tec.state().cacheNames;
        Yo.utils.log('Yo.tec: removeResources for ', cacheNamesToUpdate, resources || [/.*/]);
        cacheNamesToUpdate.forEach(function(name) {
            const state = Yo.tec.state();
            if (state.cacheUpdates.indexOf(name) == -1) {
                state.cacheUpdates.push(name);
                //we need to remove the urls from thinggs we put in cache
                state.objectsAlreadyInCache = [];
                Yo.tec.state(state);
                Yo.serviceWorker.cache.removeResources({
                    name: name,
                    resources: resources || [/.*/]
                });
            }
        });
    },

    sequencingInteraction: function(sequenceObjects, refresh, resources) {
        sequenceObjects.forEach(function(sequenceObject) {
            sequenceObject.afterLoaded(function() {
                if (refresh !== false) {
                    Yo.tec.refreshResources(resources);
                }else {
                    Yo.tec.removeResources(resources);
                }
            });
        });
    },

    navigationInteraction: function(pathNames, refresh, resources) {
        Yo.pubsub.subscribe({ topic: "core/load", callback: function() {
            pathNames.forEach(function(pathName) {
                if (window.location.pathname === pathName) {
                    if (refresh !== false) {
                        Yo.tec.refreshResources(resources);
                    } else {
                        Yo.tec.removeResources(resources);
                    }
                }
            });
        }});
    },

    smartPrefetch: function(cacheName, highTraffickedPageviews, maxUrlsToPrefetch) {
        Yo.pubsub.subscribe({ topic: "tec/ready", callback: function (message, topic) {
            const urls = Yo.tec.findOnPageUrls(highTraffickedPageviews, maxUrlsToPrefetch || 3);
            Yo.utils.log("Yo.tec: smartPrefetch being setup", cacheName, highTraffickedPageviews, maxUrlsToPrefetch, urls);
            Yo.tec.fillCache(cacheName, urls);
        }});
    },

    findOnPageUrls: function (highTraffickedPageviews, maxUrlsToPrefetch) {
        const aTagHrefs = Array.from(document.querySelectorAll('a:not([href*="#"])')).map(function (aTag) {
            return aTag.href;
        });
        const state = Yo.tec.state();
        let objectsAlreadyInCache = state.objectsAlreadyInCache || [];
        return highTraffickedPageviews.reduce(function (urlsToPutIntoCache, highTrafficUrl) {
            //On the page and not in the alread prefetched.
            if (urlsToPutIntoCache.length <= maxUrlsToPrefetch
                && aTagHrefs.indexOf(highTrafficUrl) != -1
                && objectsAlreadyInCache.indexOf(highTrafficUrl) == -1) {
                urlsToPutIntoCache.push(highTrafficUrl);
            }
            return urlsToPutIntoCache;
        }, []);
    },

    fillCache: function (cacheName, urlsToPutIntoCache) {
        if (urlsToPutIntoCache.length > 0) {
            const state = Yo.tec.state();
            state.objectsAlreadyInCache = (state.objectsAlreadyInCache || []).concat(urlsToPutIntoCache);
            Yo.tec.state(state);
            let created = false;
            Yo.pubsub.subscribe({
                topic: "tec/ready", callback: function (message, topic) {
                    Yo.utils.log("Yo.tec: fill cache for smartPrefetch with new", urlsToPutIntoCache)
                    if (created == false) {
                        created = true;
                        Yo.serviceWorker.cache.putResources({
                            name: cacheName,
                            refreshExisting: false,
                            resources: urlsToPutIntoCache
                        });
                    }
                }
            });
        }
    },

    cartReadiness: function(cartUrl, timeout) {
        Yo.pubsub.subscribe({ topic: "tec/ready", callback: function (message, topic) {
            const state = Yo.tec.state();
            if (state.cacheNames.indexOf(cartUrl) == -1) {
                Yo.utils.log("Yo.tec: create cart-readiness cache", cartUrl, timeout);
                state.cacheNames.push("cart-readiness");
                //Create Cache.
                Yo.serviceWorker.cache.create({
                    name: "cart-readiness",
                    prefetch: true,
                    resources: [cartUrl],
                    expireAfter: timeout || (30 * 60 * 1000),
                    htmlDocuments: true
                });
                Yo.tec.state(state);
            }else {
                Yo.utils.log("Yo.tec: cart-readiness cache should already be created", cartUrl, timeout);
            }
        }});
    },

    _serviceWorkerReady: function() {
        if (Yo.tec.ready === false) {
            navigator.serviceWorker.ready.then((registration) => {
                Yo.utils.log("Yo.tec: Service worker ready notification, configure message bridge", registration);
                Yo.serviceWorker.configure({messageBridge: true});
                const serviceWorkerSub = Yo.pubsub.subscribe({
                    topic: "service-worker/*", callback: function (message, topic) {
                        if (Yo.tec.ready === false) {
                            Yo.utils.log("Yo.tec: Service worker has started sending message consider it activated", topic);
                            Yo.tec.ready = true;
                            Yo.pubsub.unsubscribe(serviceWorkerSub);
                            Yo.pubsub.publish({topic: "tec/ready", persistent: true, message: {}});
                            Yo.serviceWorker.configure({messageBridge: false});
                        }
                    }
                });
            });
        }
    }
};
</script><!-- INSERT BY TRANSFORMER. SELECTOR=head,LOCATION=prepend --><script>
Yo.tec.initializeCore(["html-cache"], "https://www.ralphlauren.com/ralph.service.worker.js");
</script><!-- INSERT BY TRANSFORMER. SELECTOR=head,LOCATION=prepend --><script>
Yo.sequence.event.loadEvents([{
                    eventName: 'DOMContentLoaded',
                    eventObject: document
                }, {
                    eventName: 'load',
                    eventObject: window
                }, {
                    eventName: 'readystatechange',
                    eventObject: document
                }], true);

  //Yo.sequence.resource('/').event('pubsub sequence/chain/1/finished').alwaysBefore(2500);
  Yo.pubsub.subscribe({topic: "sequence/chain/1/finished", callback: function(message) {
     Yo.sequence.event.firejQueryEvents();
     Yo.sequence.event.fireLoadEvents();
 }});

