correctPath = function (existingPath) {
	if (existingPath.substring(0,1) == '/'){
		return 'https://' + document.location.hostname + existingPath;
	}
	else if (existingPath.string(0,8) == 'https://') {
		return existingPath
	}
	else {
		return existingPath
	}
}
rewritePictureJSON = function (tag) {
    var data = $(tag).attr('data-images');
    if (data && data != ''){
        json = JSON.parse(data.replace(/'/g, '"'));
        if (json.desktop && json.desktop.indexOf('yottaa.net') == -1 && json.desktop.indexOf('rlmedia.io') == -1){
        	json.desktop = json.desktop.replace(/ /g, '');
            json.desktop = Yo.utils.rewriteImageAPI(json.desktop);
        }
        if (json.mobile && json.mobile.indexOf('yottaa.net') == -1 && json.mobile.indexOf('rlmedia.io') == -1){
       	 	json.mobile = json.mobile.replace(/ /g, '');
            json.mobile = Yo.utils.rewriteImageAPI(json.mobile);
        }
    	$(tag).attr('data-images', JSON.stringify(json));
    }
}

var scriptToChange = document.getElementById('rlc-app_js')
if (scriptToChange.hasAttribute('data-library') && scriptToChange.hasAttribute('data-asset_path')) {
	if (scriptToChange.getAttribute('data-library').indexOf('yottaa') == -1 && scriptToChange.getAttribute('data-asset_path').indexOf('yottaa') == -1) {
		scriptToChange.setAttribute('data-library', Yo.utils.rewriteImageAPI(correctPath(scriptToChange.getAttribute('data-library'))));
		scriptToChange.setAttribute('data-asset_path', Yo.utils.rewriteImageAPI(correctPath(scriptToChange.getAttribute('data-asset_path'))));
	}
}

var pictureTags = document.getElementsByTagName('picture');

for (x = 0; x < pictureTags.length; x++){
	if (pictureTags[x].hasAttribute('data-images')){
		rewritePictureJSON(pictureTags[x]);
	}
}

var videoTag = document.getElementsByClassName('rlc-videocontainer');

for (x = 0; x < videoTag.length; x++){
	if (videoTag[x].hasAttribute('data-desktopposter') && videoTag[x].getAttribute('data-desktopposter').indexOf('yottaa') == -1 && videoTag[x].getAttribute('data-desktopposter').indexOf('yottaa') == -1) {
		videoTag[x].setAttribute('data-desktopposter',Yo.utils.rewriteImageAPI(videoTag[x].getAttribute('data-desktopposter')));
	}
	if (videoTag[x].hasAttribute('data-mobileposter') && videoTag[x].getAttribute('data-mobileposter').indexOf('yottaa') == -1 && videoTag[x].getAttribute('data-mobileposter').indexOf('rlmedia.io') == -1) {
		videoTag[x].setAttribute('data-mobileposter',Yo.utils.rewriteImageAPI(videoTag[x].getAttribute('data-mobileposter')));
	}
}
