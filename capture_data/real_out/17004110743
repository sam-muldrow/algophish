        }

        #rlc-buckets .rlc-copygroup {
          width: 100%;
          -webkit-transform: none;
          transform: none;
        }

        #rlc-buckets .rlc-slide .rlc-image {
          margin-bottom: 3.500em;
        }

        #rlc-buckets .rlc-carousel.rlc-three__m_carousel .rlc-carousel-pagination {
          top: 20.75em;
          width: 14.25em;
          height: 3.5em;
        }

        #rlc-buckets .rlc-carousel.rlc-three__m_carousel .rlc-slide .rlc-copygroup {
            left: 0;
        }

        #rlc-buckets .rlc-links .rlc-linecta {
            margin: 1.727em 1.455em 0;
        }

        #rlc-buckets .rlc-carousel .swiper-pagination-bullet:before,
        #rlc-buckets .rlc-carousel .swiper-pagination-bullet:after {
          background: #000000;
        }
      }
    </style>
  </noscript>
