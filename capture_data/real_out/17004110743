<script type="text/javascript">
(function () {
var hour = new Date().getHours();
if (hour >= 4 && hour < 12) {
document.getElementsByClassName('greeting-morning')[0].classList.remove('hide');
} else if (hour >= 12 && hour < 17) {
document.getElementsByClassName('greeting-afternoon')[0].classList.remove('hide');
} else {
document.getElementsByClassName('greeting-evening')[0].classList.remove('hide');
}
})();
</script>
