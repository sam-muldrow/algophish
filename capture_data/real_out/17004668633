      <div class="container">
  
  
  
  <div class="search-large-wrapper">
    <div class="logo"></div>
    <h1>
      PONS Online Dictionary
    </h1>
    <h2>
      Easily find the right translation
    </h2>
    <script type='text/javascript'>
  //<![CDATA[
    window.dictionarySearch = function(e) {
      e = e || window.event;

      var searchForm = $("#searchForm");

      if (searchForm.attr("method") == "post") {
        searchForm.submit();
        e.preventDefault();
        return false;
      }

      var searchButton = $("#search_button");
      var url = searchForm.attr("action");
      var params = searchForm.serializeArray();

      if (params != null) {
        var sep = '?';
        for (var i = 0; i < params.length; i++) {
          var param = params[i];
          url += sep + param.name + '=' + encodeURIComponent(param.value);
          sep = '&';
        }
      }

      searchButton.attr("href", url);
    };
  //]]>
</script>
