	.dot-text-before {
	    margin-right: 1px;
	}
  }
  
  @media (min-width: 390px) { 
	.mobile-liner {
		font-size: 10px;
	}
	.dot-text-before {
	    margin-right: 0;
	}
  }
  
  @media (min-width: 400px) { 
	.mobile-liner {
		font-size: 11px;
	}
	.dot {
	    top: 7px;
	}
  }
  
  @media (min-width: 920px) { 
    .header--sticky .desktop-liner {
      display: none;
    }
    .header--sticky #block-awardliner {
      display: none;
    }
    .header__secondary {
      margin-top: 0;
    }
  
	.desktop-liner {
	  font-style: normal;
      color: #262626;
      font-size: 12px;
      font-weight: 400;
      display: block;
      position: absolute;
      top: 15px;
	  left: 0;
	  z-index: -1;
	}
	
	.mobile-liner {
	  display: none;
	}

    #block-awardliner {
      left: 30px;
      top: 10px;
      margin-bottom: 0;
    }
  }

/*--><!]]>*/
</style></div>
      
          </div>
      </div>
  
</section>

