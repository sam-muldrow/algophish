<section id="block-plastergetcookiefunctionfixmyfeed" class="block block-block-content block-block-content21c9deba-d94f-4233-9443-2cc9b4b7ac4a block--type-interactive block--view-mode-full clearfix">
  
    
  <div  id="block-plastergetcookiefunctionfixmyfeed" class="markup markup--bottom-border block block-block-content block-block-content21c9deba-d94f-4233-9443-2cc9b4b7ac4a block--type-interactive block--view-mode-full clearfix">
        <div  class="markup__content">
              

            <div class="text-long"><script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script><script>
<!--//--><![CDATA[// ><!--

function getCookie(cname) {
          let name = cname + "=";
          let decodedCookie = decodeURIComponent(document.cookie);
          let ca = decodedCookie.split(';');
          for(let i = 0; i <ca.length; i++) {
            let c = ca[i];
            while (c.charAt(0) == ' ') {
              c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
              return c.substring(name.length, c.length);
            }
          }
          return "";
        }

//--><!]]>
</script><style type="text/css">
<!--/*--><![CDATA[/* ><!--*/
#block-plastergetcookiefunctionfixmyfeed {
    display: none;
  }

/*--><!]]>*/
</style></div>
      
          </div>
      </div>
  
</section>

