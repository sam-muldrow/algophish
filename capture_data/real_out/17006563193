<script type="text/javascript">
        (function() {
            var obj_left = $('#stickyleft');
            var fixAds_left = function() {
                var rate = screen.width / $(window).width();
                var x = Math.floor(((screen.width - (1064 * rate) - (2 * rate * obj_left.width())) / 2) / rate) - 30;
                obj_left.css({
                    left: x > 0 ? x : 0 - obj_left.width()
                });
            };
            fixAds_left();
            $(window).resize(function() {
                fixAds_left();
            });
            $(window).scroll(function() {
                obj_left.css({
                    top: $(document).scrollTop() > 120 ? '20%' : '20%'
                });
            });
        })();
        (function() {
            var obj_right = $('#stickyright');
            var fixAds_right = function() {
                var rate = screen.width / $(window).width();
                var x = Math.floor(((screen.width - (1064 * rate) - (2 * rate * obj_right.width())) / 2) / rate) - 30;
                obj_right.css({
                    right: x > 0 ? x : 0 - obj_right.width()
                });
            };
            fixAds_right();
            $(window).resize(function() {
                fixAds_right();
            });
            $(window).scroll(function() {
                obj_right.css({
                    top: $(document).scrollTop() > 120 ? '20%' : '20%'
                });
            });
        })();
        </script>
