        this.errorBlock.innerHTML = text;
        if (!mr.hasClass(this.block, this.authFailClassName)) {
            mr.addClass(this.block, this.authFailClassName);
        }
        mr.animate(this.errorBlock, 'animated_flash', 500);
    },
