<script>

    function showTooltip() {
        var x = document.getElementById("tooltip-desc");
        if (x.style.display === "flex") {
            x.style.display = "none";
        } else {
            x.style.display = "flex";
        }
    }

    document.addEventListener("click", function handleClickOutside(event) {
        var tooltipDesc = document.getElementById("tooltip-desc");
        var tooltipBtn = document.getElementById("phone-title-mobile");
        if (!tooltipDesc.contains(event.target) && !tooltipBtn.contains(event.target)) {
            tooltipDesc.style.display = "none";
        }
    });

    $(document).ready(function () {

        var formGenerator = $('#hx-form_generator');
        if (formGenerator.length != 0) {
            $('#message_framework').insertBefore(formGenerator);
        }
    })

    activeIndex = 1;
    const slides = document.getElementsByClassName("mySlides");
    const organizationalSub = document.querySelector(
        ".organizational-subscribes"
    );

    function fillCookie(cookie_value) {
        document.cookie = `theme_config=${cookie_value}; path=/`;
    }


    if ("base_1" !== "")
        fillCookie("base_1")

    if (slides.length) {
        showSlides(activeIndex);

        function showSlides(n) {
            let i;
            if (n > slides.length) {
                activeIndex = 1;
            }
            if (n < 1) {
                activeIndex = slides.length;
            }
            for (i = 0; i < slides.length; i++) {
                slides[i].style.display = "none";
            }
            slides[activeIndex - 1].style.display = "flex";
        }

        const realSub = document.querySelector(".real-subscribes");

    }

    function openHambMenu() {
        var x = document.getElementById("topNav");
        if (x.className.includes("top-nav")) {
            x.className += " responsive";
        } else {
            x.className = "top-nav";
        }
    }

    function closeHambMenu() {
        var x = document.getElementById("topNav");
        if (x.className.includes("responsive"))
            x.className = x.className.replace("responsive", "");
    }

    var body =
        `<div class="alert alert-{0}" role="alert" style="box-shadow: 0px 0px 6px 0px #0000003b; padding: 24px;
      width: 100%; height: 100%; position: fixed; top: 0; left: 0; z-index: 1000; background-color: rgb(0,0,0); background-color: rgba(0,0,0,0.27);" >
        <div class="modal-size">
        <svg class="bi flex-shrink-0 me-2 " width="24" height="24" role="img" aria-label="Danger:"><use xlink:href="#exclamation-triangle-fill"/></svg>
        <div class="lds-ring"><div></div><div></div><div></div><div></div></div>
          <div>{1}</div>
          <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        </div>
      </div>`

    document.addEventListener('django.contrib.messages', function (event) {
        var mesgTag = $('#message_framework');
        if (event.detail.message_list.length > 0) {
            for (const msg of event.detail.message_list) {

                if (msg.message != undefined || msg.extra_tags == null || msg.extra_tags == '') {
                    var tag = msg.extra_tags != undefined || msg.extra_tags == null || msg.extra_tags == '' ? msg.extra_tags : 'primary';
                    var newBody = body.replace('{0}', tag)
                    if (msg.message instanceof Object) {
                        let messageList = [];
                        for (const property in msg.message) {
                            messageList.push(`${property}:${msg.message[property]}`);
                        }
                        newBody = newBody.replace('{1}', `${messageList.join('\n')}`);
                    } else {

                        newBody = newBody.replace('{1}', msg.message);
                    }

                    $('#message_framework').append(newBody);
                    $('#message_framework').removeAttr('hidden');
                    setTimeout(
                        function () {
                            $('#message_framework').html('')
                        }, 4000);
                }
            }
        }
    })

    $(document).ready(function () {
        ""
        setTimeout(
            function () {
                $('#message_framework').html('')
            }, 4000);
    });

    let menuIcon = document.querySelector(".icon")
    let newMenu = document.querySelector(".newMenu")
    let menuBack = document.querySelector(".menuBack")

    menuIcon.addEventListener('click', function () {
        let isActiveMenu = newMenu.className.includes("active")
        if (isActiveMenu) {
            newMenu.classList.remove('active')
            newMenu.style.display = 'none';
            menuBack.style.display = 'none';
        } else {
            newMenu.classList.add('active')
            newMenu.style.display = 'flex';
            menuBack.style.display = 'flex';
        }
    })

    menuBack.addEventListener('click', function () {
        newMenu.classList.remove('active')
        newMenu.style.display = 'none';
        menuBack.style.display = 'none';
    })

    const checkSearchValue = (value) => {
        if (value.length > 0) return true
        else return false
    }

</script>





