<form action="/search/results" method="get" id="searchForm" class="search-form">
    <div class="search-container" style="position: relative;">
      <input  id="content_id" name="content" placeholder="جستجو"
        class="search" type="search" />
      <div style="display: flex; position: absolute; left: 15px; top: 2px;">
        <input class="search-icon" type="submit" value=""
          style="background-color: transparent; border:none; outline: none; z-index: 1;" />
        <img class="search-icon" src="/static/images/search.svg" />
      </div>
    </div>
</form>


<script>
    let searchIcon = document.querySelector(".search-icon")
    let searchInput = document.querySelector(".search")
    let searchForm = document.querySelector(".search-form")

    searchIcon.addEventListener('mouseover', function () {
        searchInput.classList.add("expand-input")
    })
    searchIcon.addEventListener('mouseleave', function () {
        searchInput.classList.remove("expand-input")
    })

    searchForm.addEventListener('submit', function (e) {
        let content = e.target.content
        if(content.value){
          return;
        }
        e.preventDefault();
    })
</script>


                            
