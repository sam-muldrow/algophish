    <script type="application/javascript" id="uc_fallback">
    window.UC_UI_AVAILABLE = false;
    (function () {
        if (!window.MutationObserver) {
            return;
        }

        window.ucInitialize = function (id) {
            const e = document.getElementById(id);
            if(e) {
                loadIFrameOrImg(e);
            }
        };

        function loadScript(s) {
            if (s.type === "text/plain" && s.dataset.usercentrics) {
                var script = document.createElement('script');
                for (var data in s.dataset) {
                    script.dataset[data] = s.dataset[data];
                }
                for (var cls in s.classList) {
                    script.classList[cls] = s.classList[cls];
                }
                s.src && (script.src = s.src);
                s.innerHTML && (script.innerHTML = s.innerHTML);

                s.parentNode.insertBefore(script, s);
                s.parentNode.removeChild(s);
            }
        }

        function loadIFrameOrImg(i){
            if (i.dataset.hdpSrc) {
                var element = i.cloneNode(true);
                element.src = i.dataset.hdpSrc;
                delete element.dataset.hdpSrc;
                i.parentNode.insertBefore(element, i);
                i.parentNode.removeChild(i);
            }
        }

        var observer = new MutationObserver(function (mutations) {
            for (var i = 0; i < mutations.length; i++) {
                var addedNodes = mutations[i].addedNodes;
                for (var j = 0; j < addedNodes.length; j++) {
                    var node = addedNodes[j];
                    if (node instanceof HTMLElement) {
                        if (node.tagName === 'SCRIPT') {
                            loadScript(node);
                        } else if ( (node.tagName === 'IFRAME' || node.tagName === 'IMG') && 'false' !== node.dataset.ucInitialize) {
                            loadIFrameOrImg(node);
                        }
                    }
                }
            }
        });
        observer.observe(document.documentElement, {childList: true, subtree: true});
    }());
</script>



