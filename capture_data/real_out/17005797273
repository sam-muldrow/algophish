<script>
var owl = jQuery('.owl-carousel');

// Go to the next item
jQuery('.ta_carouselNav__action--next').click(function() {
    owl.trigger('next.owl.carousel');
})

// Go to the previous item
jQuery('.ta_carouselNav__action--prev').click(function() {
    // With optional speed parameter
    // Parameters has to be in square bracket '[]'
    owl.trigger('prev.owl.carousel', [300]);
})

jQuery('.owl-carousel').owlCarousel({
    loop:true,
	autoplay:true,
	autoplaySpeed:500,
    margin: 15,
	autoWidth: true,
	nav: true,
})

var featuredArticles = jQuery ('.th_postList--featured .post-excerpt');

jQuery(document).ready(function() {
	jQuery(featuredArticles).click(function() {
		var href = jQuery(this).find('.entry-title a').attr('href');
		window.location.href = href;
	});
});
</script>
