<script>

/***** sticky ad footer closer ******/


/******** sidebar navigation trigger ***********/ 
	

$(document).ready(function() {
	$('#ta_sidebarNav__trigger').click(function() {
		var ele = $('html');
		if (ele.hasClass('ta_canvasPanel--active')) {
			ele.removeClass('ta_canvasPanel--active');
			$.setCookie('ta_sidebarCollapse', 1);
		} else {
			ele.addClass('ta_canvasPanel--active');
			$.setCookie('ta_sidebarCollapse', 0);
		}
	});
});

</script>
