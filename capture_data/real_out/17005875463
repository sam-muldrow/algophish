 <script>
//自动翻页类
//tableid:数据table的id
//pageid:页码div的id
//pagerows：每页显示条数
//rollInterval:滚动间隔毫秒，0代表不自动滚动，大于0代表自动滚动的时间间隔
function PageAction(tableid, pageid, pagerows,rollInterval) {
    //是否自动滚动
    this.AutoRoll = true;
    if (rollInterval == 0) {
        this.AutoRoll = false;
    }

    var delay = rollInterval;//翻页间隔毫秒
 
    var stepdelay = 40;
    var startopacity = 0.05; //透明度
    var stepopacity = 0.05;//透明度递增

    var ie4 = document.all && !document.getElementById;
    var DOM2 = document.getElementById;
  
    var index = 1;
    var pageSum = -1; //总页数
    var pageTimer = null;

    var that = this;

    function changecontent() {
        if (DOM2) {
            document.getElementById(tableid).style.opacity = startopacity;
            //翻到下一页
            that.turnpage(index);
            colorfade(startopacity + stepopacity)
        }
        index++;
        if (index > pageSum) { index = 1 }
    } 
    var fadecounter;
    function colorfade(A) {
        if (A < 1) {
            document.getElementById(tableid).style.opacity = A;

            A = A + stepopacity;
            fadecounter = setTimeout(function(){colorfade(A);}, stepdelay)
        } else {
            clearTimeout(fadecounter);
            document.getElementById(tableid).style.opacity = 1;           
            if (pageTimer != null) {
                clearTimeout(pageTimer);              
                pageTimer = setTimeout(function () { changecontent(); }, delay);
            }
        }
    }

    if (that.AutoRoll) {
        if (window.addEventListener) { window.addEventListener('load', changecontent, false) } else { if (window.attachEvent) { window.attachEvent('onload', changecontent) } else { if (document.getElementById) { window.onload = changecontent } } };
    } 
    //tableid:数据table的id
    //pageid:页码div的id
    //pagerows：每页显示条数
    //pagen：显示第几页
    this.turnpage = function (pagen) {

        //找到数据table
        var pageTab = document.getElementById(tableid);
        //获取table的所有行
        var trList = pageTab.getElementsByTagName("tr");

        //判断是否是当前页，如果是当前页，就不再重复翻页     
        var startShowIndex = -1;
        var aList = document.getElementById(pageid).getElementsByTagName("a");
        for (var i = 0; i < trList.length; i++) {
            if (trList[i].style.display == "block") {
                startShowIndex = i;
                break;
            }
        }
        if (startShowIndex > -1 && (startShowIndex + pagerows) / pagerows == pagen) {
            return;
        }

        pageSum = trList.length / pagerows;
        if (trList.length % pagerows != 0) {
            pageSum = pageSum + 1;
        }
        if (pagen < 1) {
            pagen = 1;
        }
        if (pagen > pageSum) {
            pagen = pageSum;
        }

        var year = new Date().getFullYear().toString();
        var month = (new Date().getMonth() + 1).toString();
        var day = new Date().getDate().toString();
        var today = year + (month.length == 1 ? "0" + month : month) + (day.length == 1 ? "0" + day : day);

        //循环行，控制显示和隐藏
        for (var i = 0; i < trList.length; i++) {
            if (i >= (pagen - 1) * pagerows && i < pagen * pagerows) {
                trList[i].style.display = "block";
            }
            else {
                trList[i].style.display = "none";
            }
            if (pagen > 1) {

                //检测td里的a标签
                var al = trList[i].getElementsByTagName("a");
               
                if (al.length > 0) {                 
                    var ahref = al[0].href;
                    var patt = new RegExp(".*?" + today + ".*?");
                    if (!patt.test(ahref)) {
                        al[0].style.color = "Gray";
                    }
                }
            }
        }
        //修改页码的超链接样式
        for (var i = 0; i < aList.length; i++) {
            //判断是否是当前页码
            if (i + 1 == pagen.toString()) {
                //设置当前页码超链接背景为灰色
                aList[i].style.backgroundColor = "#CC9999";
            }
            else {
                //设置其他页码超链接背景为白色
                aList[i].style.backgroundColor = "White";
            }
            if (aList[i].target != "_blank") {
                aList[i].target = "_self";
            }
        }
    }
    
    function mouseFunc(index) {
        this.mouseOverFunc = function () {
            if (index >= 0) {
                that.turnpage(index + 1);
            }
            clearTimeout(pageTimer);
            pageTimer = null;
        }
        this.mouseOutFunc = function () {
            pageTimer = setTimeout(function () { changecontent(); }, delay);
        }
        this.clickFunc = function () {
            that.turnpage(index+1);
        }
    }


    //初始化翻页事件
    this.initialFunc = function () {
        if (that.AutoRoll) {
            //找到数据table,设置table鼠标移上移出事件
            var pageTab = document.getElementById(tableid);
            pageTab.onmouseover = function () {
                clearTimeout(pageTimer);
                pageTimer = null;
            };
            pageTab.onmouseout = function () { pageTimer = setTimeout(function () { changecontent(); }, delay); };
        }

        //初始化翻页a的鼠标移上移出事件
        var pagelinks = document.getElementById(pageid).getElementsByTagName("a");
        for (var i = 0; i < pagelinks.length; i++) {
            if (pagelinks[i].target != "_blank") {
                var col = new mouseFunc(i);
                if (that.AutoRoll) {
                    pagelinks[i].onmouseover = col.mouseOverFunc;
                    pagelinks[i].onmouseout = col.mouseOutFunc;
                }
                else {
                    pagelinks[i].onclick = col.clickFunc;
                }
            }
        }
        //初始化显示第一页
        that.turnpage(1);
        if (that.AutoRoll) {
            pageTimer = setTimeout(function () { changecontent(); }, delay);
        }
    }
}


 //设置自动翻页
    
            var rp_pagejs = new PageAction('rp_table', 'rp_page', 7, 0);
            rp_pagejs.initialFunc();
            var mj_pagejs = new PageAction('mj_table', 'mj_page', 7, 0);
            mj_pagejs.initialFunc();
            var yw_pagejs = new PageAction('yw_table', 'yw_page', 7, 0);
            yw_pagejs.initialFunc();
            var gxkd_pagejs = new PageAction('gxkd_table', 'gxkd_page', 7, 0);
            gxkd_pagejs.initialFunc();
            var gsdj_pagejs = new PageAction('gsdj_table', 'gsdj_page', 7, 0);
            gsdj_pagejs.initialFunc();
            var ssgs_pagejs = new PageAction('ssgs_table', 'ssgs_page', 7, 0);
            ssgs_pagejs.initialFunc();
            var gssj_pagejs = new PageAction('gssj_table', 'gssj_page', 7, 0);
            gssj_pagejs.initialFunc();
            //var realnews_pagejs = new PageAction('realnews_table', 'realnews_page', 3, 2000);
            //realnews_pagejs.initialFunc();




//图片滚动
        var isIE = !(navigator.userAgent.indexOf('MSIE') == -1);
        var news;
        var curNew = 0;
        var timer;
        init();
        function init(pic, txt) {
            if (!pic) pic = "NewsPic";
            if (!txt) txt = "NewsPicTxt";
            var div = document.getElementById(pic);
            if (!div) return;
            var nav = document.createElement("DIV");
            nav.className = "Nav";
            var nodes;
            //if(isIE)
            //{
            //    nodes=div.childNodes;
            //}
            //else
            //{
            //    nodes=childrenNodes(div.childNodes);
            //}
            nodes = childrenNodes(div.childNodes);
            news = new Array(nodes.length);
            for (var i = nodes.length - 1; i >= 0; i--) {
                var element = nodes[i];
                news[i] = {};
                news[i].Element = element;
                news[i].Text = element.getAttribute("ptitle");
                news[i].Url = element.getAttribute("phref");
                var n = document.createElement("span");
                n.innerHTML = "<a herf=\"javascript:;\" onclick=\"javascript:curNew=" + (i - 1) + ";change('" + pic + "','" + txt + "');\">" + (i + 1) + "</a>";
                if (i == curNew) n.className = "Cur";
                nav.appendChild(n);
                news[i].LinkElement = n;
            }
            div.appendChild(nav);
            curNew--;
            window.setTimeout("change('" + pic + "','" + txt + "')", 3000);
        }
        function childrenNodes(node) {
            var c = new Array();
            for (var i = 0; i < node.length; i++) {
                if (node[i].nodeName.toLowerCase() == "u")
                    c.push(node[i]);
            }
            return c;
        }
        function change(pic, txt) {
            var div = document.getElementById(pic);
            var text = document.getElementById(txt);
            if (!div) return;
            curNew = curNew + 1;
            if (curNew >= news.length) curNew = 0;
            for (var i = 0; i < news.length; i++) {
                if (i == curNew) {
                    news[i].Element.style.display = "block";
                    news[i].Element.style.visibility = "visible";
                    news[i].LinkElement.className = "Cur";
                    text.innerHTML = "<u onclick=\"window.open('" + news[i].Url + "');\" title=\"" + news[i].Text + "\" ptitle=\"" + news[i].Text + "\" phref=\"" + news[i].Url + "\" target=\"_blank\">" + news[i].Text + "</u>";
                }
                else {
                    news[i].Element.style.visibility = "hidden";
                    news[i].Element.style.display = "none";
                    news[i].LinkElement.className = "Normal";
                }
            }
            if (timer) window.clearTimeout(timer);
            timer = window.setTimeout("change('" + pic + "','" + txt + "')", 5000);
        }



    </script>
