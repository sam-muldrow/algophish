document.getElementById('top_body_more_box_2').innerHTML ='<ul class="sbtn-list top_body_more_link"><li><a class="button line" href="https://www.sofmap.com/contents/?id=new_product&sid=' + month + '" onclick="ga(\'send\',\'event\',\'banner\',\'click\',\'pctop_item_2txt\'1);">もっと見る</a></li></ul>';
