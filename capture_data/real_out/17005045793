<script>
$(document).ready( function(){
	if ($('#button_close').is(':checked')) {
	} else {
		$("#voc_ec_button").addClass("button_on");
		$(".btn-pagetop").addClass("button_on");
		$(".product-filter-menu").addClass("button_on");
	}
});
$(function() {
	$('#button_close').change(function(){
	if ($(this).is(':checked')) {
		$("#voc_ec_button").removeClass("button_on");
		$(".btn-pagetop").removeClass("button_on");
		$(".product-filter-menu").removeClass("button_on");
	} else {
		$("#voc_ec_button").addClass("button_on");
		$(".btn-pagetop").addClass("button_on");
		$(".product-filter-menu").addClass("button_on");
	}
	});
});
</script>

<script>
var targets = document.querySelectorAll('#button_close');
var checkboxClick = function(){
    var self = this;
    sessionStorage.setItem('input[data-button="' + self.getAttribute('data-button') + '"]', self.checked);
};
Array.prototype.forEach.call(targets, function(e){
    e.addEventListener('click', checkboxClick)
})
</script>
<script>
var addSessionStorage = function() {
    var session = sessionStorage;
    Object.keys(session).forEach(function(key){
        if(key.indexOf('data-button') !== -1 && this[key] === 'true' && document.querySelector(key) !== null) {
            document.querySelector(key).checked = true;
        }
    }, session)
}
</script>
<script>window.addEventListener('load', addSessionStorage);</script>

