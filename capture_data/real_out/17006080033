    else if(myday == 2) day = "火"; 
    else if(myday == 3) day = "水"; 
    else if(myday == 4) day = "木"; 
    else if(myday == 5) day = "金"; 
    else if(myday == 6) day = "土"; 
    
    month = mymonth +1 ;
