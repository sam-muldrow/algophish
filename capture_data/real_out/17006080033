        var fluctStickyAdDivId = 'fluct-pc-sticky-ad';
        var closeButtionId = 'fluct-pc-sticky-ad-close-button';
        var slot = e.slot;

        if (slot.getSlotElementId() === stickyGptDivId && !e.isEmpty) {
