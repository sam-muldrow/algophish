        // スマホ用ブランドカルーセルの初期化処理
        function initBrandCarousel() {
            var showNum = $('#mobile-device').val() == 0 ? 7 : 3;
            var $this = $(".brand-carousel"),
                arrowsplace = $this.prev('.title');
            $this.slick({
                lazyLoad: 'progressive',
                rows: 3,
                appendArrows: arrowsplace,
                slidesToShow: showNum,
                slidesToScroll: 2,
                speed: 500,
                infinite: false,
                responsive: [{
                    breakpoint: 1401,
                    settings: {
                        slidesToShow: 4,
                        slidesToScroll: 2
                    }
                }, {
                    breakpoint: 1201,
                    settings: {
                        slidesToShow: 4,
                        slidesToScroll: 2
                    }
                }, {
                    breakpoint: 993,
                    settings: {
                        slidesToShow: 4,
                        slidesToScroll: 2
                    }
                }, {
                    breakpoint: 769,
                    settings: {
                        slidesToShow: 4,
                        slidesToScroll: 2
                    }
                }, {
                    breakpoint: 481,
                    settings: {
                        slidesToShow: 2,
                        slidesToScroll: 2
                    }
                }]
            });
        }
