<script id="metaTagJs" src="/vendor/ads/stroeer/au/js/metaTag.min.js" defer></script>

<script>

        function loadVideos() {

        if (typeof SDG === "undefined") {
            return;
        }

        document.querySelectorAll('.skystream-wrapper').forEach(el => {
            let finalSrc = null;
            if(window.matchMedia('(max-width: 1019px)').matches){
                finalSrc = el.getAttribute('data-src-mobil');
            } else {
                finalSrc = el.getAttribute('data-src-desktop');
            }

            try {
                let json = {
                    "preroll":true,
                    "videozone":"rest"
                };

                SDG.cmd.push(function(){
                    let output = SDG.Publisher.generateVastUrls(json);
                    if (output.hasOwnProperty('preroll')) {
                        finalSrc = finalSrc.concat("&dynamic_adtag=".concat(encodeURIComponent(output.preroll)));
                    }

                    let newIFrame = document.createElement('iframe');
                    newIFrame.classList.add('skystream');
                    newIFrame.classList.add('replacedFrameIdentifier');
                    newIFrame.setAttribute('allowFullScreen', '');
                    newIFrame.setAttribute('frameborder', '0');
                    newIFrame.setAttribute('referrerpolicy', 'no-referrer-when-downgrade');
                    newIFrame.setAttribute('src', finalSrc);

                    el.appendChild(newIFrame);
                });

            } catch(error) {
                console.error(error);
            }
        });
    }

    const init_stroeer = () => {
        const prefix = 'ad--stroeer--';
        const addSlotKeysDesktop = {
            'banner': {
                reserveSpace: true,
                fixedHeight: 198,
                showAdvertLabel: true,
                centerAds: true,
                showLoadingAnimation: false,
                lazyLoad: false
            },
        }
        const addSlotKeysMobile = {
            'topmobile': {
                reserveSpace: true,
                fixedHeight: 150,
                showAdvertLabel: true,
                centerAds: true,
                showLoadingAnimation: false,
                lazyLoad: false
            },
        }

        const mobil = window.matchMedia('(max-width: 1019px)').matches;
        let slotConfigs = addSlotKeysDesktop
        if(mobil){
            slotConfigs = addSlotKeysMobile
        }

        const keywords = ['apotheken_umschau'];
        let preventAds = false;
        keywords.forEach(function (value, index) {
            if (value.match(/^(noad)s?$/i)) {
                preventAds = true;
            }
        });



        const tagManager = document.querySelector('#metaTagJs')
        if (typeof SDG === "undefined") {
            tagManager.addEventListener("load", onSDGLoad); // @todo MSC Hook
        } else {
            onSDGLoad();
        }

        function onSDGLoad() {
            registerSlots()
            doTagManger()
        }

        function registerSlots() {
            if (!preventAds) {

                window.SDG = window.SDG || {};
                window.SDG.cmd = window.SDG.cmd || [];
                window.SDG.cmd.push(function() {
                    SDG.Publisher.setZone('startseite')
                    SDG.Publisher.addKeywords(keywords)

                    for (let configKey in slotConfigs) {

                        const slotConfig = slotConfigs[configKey];
                        const myAdSlot = SDG.Publisher.registerSlot(configKey, prefix + configKey);

                        myAdSlot.nativeBackfill(function() {
                            const selector = `#ad--stroeer--${configKey}`;
                            const el = document.querySelector(selector);
                            const slot = el.querySelector('[data-template-id]')
                            const templateId = slot.dataset.templateId;
                            const template = document.getElementById(templateId);

                            let ad = document.createElement('div');
                            ad.id = "default_" + templateId;
                            ad.innerHTML = template.innerHTML;
                            return ad;
                        });

                        myAdSlot.configure(slotConfig).load();
                    }

                    SDG.Publisher.finalizeSlots();

                })
            }
        }

        function doTagManger(){
            if (typeof SDG !== "undefined") {
                if (typeof __tcfapi !== 'undefined') {
                    __tcfapi('addEventListener', 2, function(tcData, success) {
                        if (success && ['tcloaded', 'useractioncomplete'].includes(tcData.eventStatus)) {

                            loadVideos();

                            // remove the ourself to not get called more than once
                            __tcfapi('removeEventListener', 2, function(success) {
                            }, tcData.listenerId);
                        }
                    });
                }

                const adSlotsService = new document.classes.adSlotsService();
                adSlotsService.action();
            }
        }

                document.addEventListener("DOMContentLoaded", function(event) {
            let cssSelector = ".inline-element:not(.ad--dynamic)";
            let adSlotCssSelector = 'ad--dynamic'
            let inlineElement = document.querySelector(cssSelector);
            if (inlineElement) {
                let prevElement = inlineElement.previousElementSibling;

                if (prevElement) {
                    if (prevElement.classList.contains(adSlotCssSelector)) {
                                                if (inlineElement.classList.contains('textbox')){
                            if (!prevElement.classList.contains('full-width'))
                            {
                                inlineElement.style.clear = 'none';
                                console.log("Reorganize css classes for adSlot: ".concat(adSlotCssSelector).concat(" for elem: textbox"));
                                prevElement.classList.add('inline-element','werbung','float-end', 'mt-0', 'pt-1');                                prevElement.classList.remove('d-flex', 'justify-content-center');                            }
                        } else {
                                                        console.log("Rearrange adSlot: ".concat(adSlotCssSelector).concat(" for elem: ").concat(cssSelector));
                            inlineElement.after(prevElement);
                        }
                    }
                }
            }
        });

        window.addEventListener('metaTagSystemSlotContainerAvailable', function (event) { // @todo MSC Hook
            const currentSlot = event['detail']['placement'];

            if (currentSlot.getName() === 'stickyfooter') {
                sdgStickyFooter.activate(currentSlot);
            }
        }); 

        const sdgStickyFooter = {
            stickyFooterCloseButton: document.createElement('button'),
            stickyFooterButtonCss: '.close-sticky-ad {position: absolute; top: -28px; visibility: visible; width: 28px; height: 28px;right: 0;background-image: url(\'data:image/svg+xml;charset=utf-8,%3Csvg width="13" height="13" viewBox="341 8 13 13" xmlns="http://www.w3.org/2000/svg"%3E%3Cpath fill="%234F4F4F" d="M354 9.31L352.69 8l-5.19 5.19L342.31 8 341 9.31l5.19 5.19-5.19 5.19 1.31 1.31 5.19-5.19 5.19 5.19 1.31-1.31-5.19-5.19z" fill-rule="evenodd"/%3E%3C/svg%3E\');background-size: 13px 13px;background-position: 9px;background-color: #fff;background-repeat: no-repeat;box-shadow: 0 -1px 1px 0 rgba(0,0,0,0.2); border: none;border-radius: 12px 0 0 0;}.sdgStickyFooter{position:fixed !important;z-index:5000;bottom:0px !important;left: 0px !important;background-color: white; box-shadow: 10px 10px 5px 10px rgba(0,0,0,0.2);padding-top:4px;width:100% !important;}',

            activate: function (currentSlot) {
                if (currentSlot.getFlag('stickyFooterActive') !== true) {
                    sdgStickyFooter.setUpStickyFooter(currentSlot);
                    currentSlot.getContainer().classList.add('sdgCloaked')
                }
            },
            setUpStickyFooter: function (currentSlot) {
                const instance = sdgStickyFooter;

                instance.constructStickyFooterElements(currentSlot);
                if (currentSlot.getStatus('loaded')) {
                    instance.connectStickyFooterToSlot(currentSlot);
                } else {
                    currentSlot.getContainer().addEventListener('metaTagSlotDone', function() {
                        instance.connectStickyFooterToSlot(currentSlot);
                    });
                }
            },
            constructStickyFooterElements: function (currentSlot) {
                const
                    instance = sdgStickyFooter,
                    adServerAnchorNode = currentSlot.getAdServerElements().getAdServerAnchor(),
                    utilities = SDG.getUtil();

                currentSlot.setFlag('stickyFooterActive', true);
                instance.stickyFooterCloseButton.className = 'close-sticky-ad';
				instance.stickyFooterCloseButton.setAttribute('name', 'Anzeige ausblenden');
                instance.stickyFooterCloseButton.setAttribute('type', 'button');
                instance.stickyFooterCloseButton.onclick = instance.removeStickyFooter;
                utilities.addCssToElement(adServerAnchorNode, instance.stickyFooterButtonCss);
            },
            connectStickyFooterToSlot: function (currentSlot) {
                const
                    adServerNode = currentSlot.getAdServerNode(),
                    isEmpty = currentSlot.getStatus('isEmpty'),
                    slotContainer = currentSlot.getContainer();

                if (isEmpty !== true) {
                    adServerNode.appendChild(sdgStickyFooter.stickyFooterCloseButton);
                    slotContainer.classList.add('sdgStickyFooter');
                    slotContainer.classList.remove('sdgCloaked')

                }
            },
            removeStickyFooter: function () {
                SDG.Publisher.unregisterSlot('stickyfooter', true);
            }
        };
    }

    const metaTagScriptTag = document.querySelector('#metaTagJs')
    if(metaTagScriptTag) {
        metaTagScriptTag.addEventListener('load', () => {
            ir.register_callback('showAds', init_stroeer , false, 'Stroeer')
        })
    }



</script>


