<script>
    const cookie = {
        set: (name, value, options = {}) => {
            options = {
                path: "/",
                ...options
            };

            // Fest definiertes Enddatum
            if (options.expires instanceof Date) {
                options.expires = options.expires.toUTCString();
            }

            // Lebenszeit des Cookies in Tagen. Überschreibt 'expires'
            if (Number.isInteger(options.lifetime)) {
                const date = new Date();
                date.setTime(date.getTime() + (options.lifetime * 24 * 60 * 60 * 1000));
                options.expires = date.toUTCString();
            }

            let updatedCookie = encodeURIComponent(name) + "=" + encodeURIComponent(value);

            for (const optionKey in options) {
                updatedCookie += "; " + optionKey;
                const optionValue = options[optionKey];
                if (optionValue !== true) {
                    updatedCookie += "=" + optionValue;
                }
            }

            document.cookie = updatedCookie;
        },
        get: name => {
            const matches = document.cookie.match(new RegExp(
                "(?:^|; )" + name.replace(/([.$?*|{}()[\]\\/+^])/g, "\\$1") + "=([^;]*)"
            ));
            return matches ? decodeURIComponent(matches[1]) : undefined;
        },
        delete: name => {
            cookie.set(name, "", {
                "max-age": -1
            });
        }
    };
</script>

<script>

                const groupVendorId = '64f0445439c2aa05f5c27886';     
    /*
     * Kommunikation mit dem CMP von Sourcepoint.
     */
    const cmpClientTool = {
        // https://sourcepoint-public-api.readme.io/reference/postcustomconsent
        postCustomConsent: () => {
            __tcfapi('postCustomConsent', 2, function (data, success) {
                let options = ir_abonnement_config.cookieOptions;
                let ttlCookie = cookie.get('consent_ttl');
                if (ttlCookie) {
                    options.lifetime = ir_abonnement_config.cookieTTL;
                    cookie.delete('consent_ttl');                }
                                let cookieSession = cookie.get(ir_abonnement_config.cookiePlenigoSession);
                if (cookieSession)
                {
                    cookie.set(ir_abonnement_config.cookiePlenigoSession, cookieSession, options);
                    localStorage.setItem('custom_consent', "1");
                }
                location.reload();
            }, ['5f8818b38480a02c02b69837', '63657be8bcb5be04a169758e', '5e717ad49a0b5040d5750815', '64895ef45ec2db05332fffc9'], [groupVendorId], []); // the category-ID is important.
        },
        // Information: Derzeit nicht in Nutzung
        // https://sourcepoint-public-api.readme.io/reference/getcustomvendorconsents
        hasCustomConsentForVendor: () => {
            return new Promise((resolve, reject) => {
                __tcfapi('getCustomVendorConsents', 2, (data, success) => {
                    let grants = Object.values(data.grants);
                    for (let a in grants) {
                        if (groupVendorId in grants[a].purposeGrants) {
                            resolve(!!grants[a].vendorGrant);                         }
                    }
                    resolve(false);
                });
            });
        },
        // https://sourcepoint-public-api.readme.io/reference/postrejectall-1
        rejectAll: () => {
            __tcfapi('postRejectAll', 2, function (postRejectAllResponse, success) {
                if (getRefUrl()) {
                    callBackHasUserBought(true);
                } else {
                    location.reload();
                }
            });
        }
    };

</script>

<script>

        ir_abonnement_config = {
                cookiePlenigoSession: 'plenigoSession',
                cookiePlenigoCheckout: 'plenigoCheckout',
                cookieOfferId: 'offerId',
        cookieRefUrl: 'refUrl',
                cookieOptions: {
            secure: false,
            sameSite: 'Lax',
        },
        cookieTTL : 14,
                accessAjaxUrl: '/services/ajax/checkAccessOfCustomer/',
                purchaseAjaxUrl: '/services/ajax/purchase/',
                validateTransferTokenUrl: '/services/ajax/validate/transferToken/',
        version: 'version',     };

    /**
     * Methode, die aus dem Cookie die redirect-URL ausliest oder false zurückliefert.
     * @returns {string|boolean}
     */
    function getRefUrl() {
        // RedirectUrl
        let refUrl = cookie.get(ir_abonnement_config.cookieRefUrl);
        if (refUrl) {
            return refUrl;
        }
        return false;
    }

    // Innerhalb vom iframe die event
    document.addEventListener("plenigo.WebAnalyticsLoad", function (e) {

        // Auf den Plenigo Seiten gibt es einen Spinner, dieser wird hier entfernt, da der Content geladen wurde!
        if (document.getElementById('pl_spinner')) {
            document.getElementById('pl_spinner').remove();
        }

        // Wir setzen die Höhe
                if (document.getElementById('plenigo-login')) {
            document.getElementById('plenigo-login').style.height = '100dvh';
        }
                if (document.getElementById('plenigo-snippets')) {
            let cls = "is-mobile";
            let height = '130dvh';
            if (document.getElementsByTagName("body")[0].className.match(new RegExp('(\\s|^)'+cls+'(\\s|$)')))
            {
                height = '145dvh';
            }
            document.getElementById('plenigo-snippets').style.height = height;

                        if (e.detail.page !== 'Overview - overview')
            {
                document.getElementById('btn-back-mein-konto').classList.remove('disabled');
            }
        }
    });

    // Checkout finishes with a javascript-Event one have to listen to
    document.addEventListener("plenigo.LoginSuccess", function (e) {
        // Plenigo liefert in e, den sessionToken.
        let cookieSession = e.detail.customerSession;
        let options = ir_abonnement_config.cookieOptions;

        if (e.detail.rememberMe) {
            options.lifetime = ir_abonnement_config.cookieTTL;
            cookie.set('consent_ttl', ir_abonnement_config.cookieTTL);
        }
                cookie.set(ir_abonnement_config.cookiePlenigoSession, cookieSession, options);
                localStorage.setItem(ir_abonnement_config.cookiePlenigoSession, cookieSession);
        hasUserBought();
    });

    /**
     *
     * @param hasBought {boolean}
     */
    const callBackHasUserBought = function(hasBought){
        let url = getRefUrl();
        if (url) {
            cookie.delete(ir_abonnement_config.cookieRefUrl);
            window.location.href = url;
        }
    };

    function getOfferIdToCheckAboStateInLoginProcess()
    {
        let openOfferId = cookie.get('plOffId');         if (!openOfferId) {
            openOfferId = cookie.get(ir_abonnement_config.cookieOfferId);        }

                if (!openOfferId) {
                            openOfferId = 'O_DE2I9GC9NU2EMHPZK9';
                    }
        return openOfferId;

    }

    /**
     * Prüft eine Plenigo-Offer-ID ob ein Kauf besteht.
     */
    function hasUserBought() {
        let openOfferId = getOfferIdToCheckAboStateInLoginProcess();
        if (!openOfferId) {
            callBackHasUserBought(false);
        }
        let purchaseAjaxUrl = ir_abonnement_config.purchaseAjaxUrl + openOfferId
                cookie.delete('plOffId');
        cookie.delete(ir_abonnement_config.cookieOfferId);
        if (ir_abonnement_config.accessAjaxUrl && purchaseAjaxUrl) {
            // Prüfen ob der User das "Produkt" bereits gekauft hat!
            ir.getAjax(ir_abonnement_config.accessAjaxUrl + '?token=' + cookie.get(ir_abonnement_config.cookiePlenigoSession), function (response) {
                let data = JSON.parse(response);
                if (data.hasOwnProperty('accessGranted') && data.accessGranted === true) {
                    localStorage.setItem(ir_abonnement_config.version, '2.0.1');

                    if (localStorage.getItem('custom_consent')) {                         if (getRefUrl()) {
                            callBackHasUserBought(true);
                        } else {
                            location.reload();
                        }
                    } else {
                        cmpClientTool.rejectAll();                     }
                } else {
                                        showLoginNoticeContainer();
                                        document.querySelectorAll('a[data-login-identifier]').forEach((aTag) => {
                                                let url = aTag.dataset.loginUrl;
                        let value = aTag.dataset.loginValue;
                        aTag.href=url;
                        aTag.textContent = value;
                    });
                }
            })
        }
    }

    /**
     * Zeige die Login-Notice Box an, und entferne das iFrame von Plenigo.
     *      *      */
    function showLoginNoticeContainer() {
        let loginNotifyContainer = document.getElementById('login-notify');
        if (loginNotifyContainer) {
            loginNotifyContainer.classList.remove('d-none');
            document.getElementById('plenigo-login').remove();
            document.getElementById('login-notify-btn').href = getRefUrl();
        }
    }

</script>

<script>

        if (location.href.includes('transferToken'))
    {
        const params = new Proxy(new URLSearchParams(window.location.search), {
            get: (searchParams, prop) => searchParams.get(prop),
        });
                let token = params.transferToken;         if (token) {
            ir.getAjax(ir_abonnement_config.validateTransferTokenUrl + token , function (response) {
                try {
                    let data = JSON.parse(response);
                    if (data.hasOwnProperty('success') && data.success) {
                        cookie.set(ir_abonnement_config.cookiePlenigoSession, data.token);
                        localStorage.setItem(ir_abonnement_config.cookiePlenigoSession, data.token);
                        // Force Reload without query-params
                        let options = ir_abonnement_config.cookieOptions;
                        let refUrl = window.location.href.split('?')[0];
                        console.info('set refUrl', refUrl);
                        cookie.set(ir_abonnement_config.cookieRefUrl, refUrl, options);
                        hasUserBought();
                    }
                } catch (e) {
                    console.error(e);
                }
                checkLogin();
            });
        } else {
            checkLogin();
        }
    }
    else
    {
        checkLogin();
    }

    /**
     * Prüft via Ajax den Login Status und erstellt li Elemente in den service.ul listen
     */
    function checkLogin()
    {
                const jwt = localStorage.getItem(ir_abonnement_config.cookiePlenigoSession);
        const cookieValue = cookie.get(ir_abonnement_config.cookiePlenigoSession);
        if ((jwt && cookieValue) && (jwt !== cookieValue)) {
            cookie.delete(ir_abonnement_config.cookiePlenigoSession);
            localStorage.removeItem(ir_abonnement_config.cookiePlenigoSession);
            location.reload();
            return;
        }
                document.addEventListener("DOMContentLoaded", function(event) {
                        if (cookieValue) {
                let isLoggedInUrl = '/services/ajax/isLoggedIn/' + '?token=' + cookieValue;
                ir.getAjax(isLoggedInUrl,
                    function (response) {
                        try {
                            let data = JSON.parse(response);
                            // Den Service UL-Listen das liElement zufügen, nicht aber der mobilen service nav
                            if (data.hasOwnProperty('isLoggedIn') && data.isLoggedIn) {
                                appendUserLoginLink(true);
                            } else {
                                appendUserLoginLink(false);
                                cookie.delete(ir_abonnement_config.cookiePlenigoSession);                                localStorage.removeItem(ir_abonnement_config.cookiePlenigoSession);
                            }
                        } catch (e) {
                            console.error(e);
                        }
                    }, function (response) {
                        console.error(response);
                    }
                );
            } else {
                cookie.delete(ir_abonnement_config.cookiePlenigoSession);
                localStorage.removeItem(ir_abonnement_config.cookiePlenigoSession);
                appendUserLoginLink(false);
            }
        });
    }

    /**
     * Fügt den Login Link oder Mein Konto Link den Navigations-Elementen hinzu.
     * @param isLoggedIn true|false
     */
    function appendUserLoginLink(isLoggedIn)
    {
        // Den Service UL-Listen das liElement zufügen, nicht aber der mobilen service nav
        document.querySelectorAll('nav.service:not(.d-lg-none) ul').forEach((ul) => {
            let newLiElement = document.createElement('li');
            newLiElement.classList.add('textLink');
            let linkElement = document.createElement('a');

            // isLoggedIn gibt an, ob user angemeldet ist oder nicht!
            if (isLoggedIn) {
                linkElement.setAttribute('href', '/services/mein-konto/');
                linkElement.appendChild(document.createTextNode('Mein Konto'));
            } else {
                linkElement.dataset.loginIdentifier = '1';
                linkElement.dataset.loginUrl = '/services/mein-konto/';
                linkElement.dataset.loginValue = 'Mein Konto';
                let loginDiv = document.getElementById('login-notify');
                if (loginDiv)
                {
                    let options = ir_abonnement_config.cookieOptions;
                    let refUrl = loginDiv.dataset.refUrl;
                    if (refUrl) {
                        console.info('set refUrl', refUrl);
                        cookie.set(ir_abonnement_config.cookieRefUrl, refUrl, options);
                    }
                }

                linkElement.setAttribute('href', '/services/login/');
                linkElement.appendChild(document.createTextNode('Login'));
            }
            // liElement das linkElement zufügen
            newLiElement.appendChild(linkElement);

            ul.prepend(newLiElement);
        });
    }


</script>        
