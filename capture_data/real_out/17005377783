        <script>
                        // closest polyfill
this.Element && function(ElementPrototype) {
    ElementPrototype.closest = ElementPrototype.closest ||
        function(selector) {
            var el = this;
            while (el.matches && !el.matches(selector)) el = el.parentNode;
            return el.matches ? el : null;
        }
}(Element.prototype);


const ir = {};


ir.fired_callbacks = [];
ir.callbacks = [];
ir.debugmode = true;

/**
 * Registriert Funktionen, die bei diversen Events gefeuert werden.
 * @param  {string} event_name
 * @param  {requestCallback} func
 * @param {bool} is_permanent Flag, ob die Callback in der Que nach feuert bestehen bleiben soll
 * @param  {string} comment
 * @return {void}
 */
ir.register_callback = function(event_name, func, is_permanent, comment) {
    comment = comment || "  ";

    if (ir.fired_callbacks.indexOf(event_name) > -1) { // Event bereits gefeuert?
        func.call();
        if (ir.debugmode) {
            console.log(`${event_name} bereits gefeuert. ${comment} wird direkt ausgeführt`)
        }
        if (!is_permanent) {
            return;
        }
    }

    let callback = {};
    callback.func = func;
    callback.comment = comment;
    callback.is_permanent = is_permanent || false;
    if (ir.debugmode) {
        console.log("Register Callback on " + event_name + ": " + comment);
    }
    ir.callbacks[event_name] = ir.callbacks[event_name] || [];
    ir.callbacks[event_name].push(callback);
};


/**
 * Feuert alle Callbacks des übegebenen Events
 * @param  {string} event
 */
ir.fire_all_callbacks = function(event, ...params) {

    ir.fired_callbacks.push(event);

    if (event && ir.callbacks.hasOwnProperty(event)) {
        let tmp_callbacks = ir.callbacks[event];
        ir.callbacks[event] = [];

        // Feuert ALLE registrierten Callbacks
        if (tmp_callbacks && tmp_callbacks.length > 0) {
            while (tmp_callbacks.length) {
                let obj = tmp_callbacks.shift();
                if (obj.is_permanent) {
                    ir.callbacks[event].push(obj); // Zurückschreiben
                }
                obj.func.call(this, ...params);
                if (ir.debugmode) {
                    console.log("Fired Callback on " + event + ": " + obj.comment);
                }
            }
        }
        return true;
    } else {
        return false;
    }
};

/**
 * @deprecated Sehr unzuverlässig. Erste Node wird verschluckt.
 * @param html
 * @return {NodeListOf<ChildNode>}
 */
ir.toNodeList = html => new DOMParser().parseFromString(html, 'text/html').body.childNodes;
ir.toNode = html => new DOMParser().parseFromString(html, 'text/html').body.childNodes[0];



ir.getAjax = function(url, success, fail){
    fail = fail || function() {};
    let xhr = new XMLHttpRequest();
    xhr.addEventListener('load', (ev) => {
        if (xhr.readyState>3 && xhr.status==200) success(xhr.response);
        else{
            console.error(xhr.status, xhr.responseText);
            fail(xhr);
        }
    });
    xhr.addEventListener('error', fail);

    xhr.open('GET', url);

    xhr.setRequestHeader('X-Requested-With', 'XMLHttpRequest');
    xhr.send();
    return xhr;
};

ir.postAjax = function(url, data, success, fail) {

    fail = fail || function() {};
    let params = typeof data == 'string' ? data : Object.keys(data).map(
        function(k){ return encodeURIComponent(k) + '=' + encodeURIComponent(data[k]) }
    ).join('&');

    let xhr = new XMLHttpRequest();
    xhr.open('POST', url);
    xhr.onload = function() {
        if (xhr.readyState>3 && xhr.status==200) { success(xhr.response); }
        else{
            console.error(xhr.status, xhr.responseText);
            fail(xhr);
        }
    };
    xhr.setRequestHeader('X-Requested-With', 'XMLHttpRequest');
    xhr.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
    xhr.send(params);
    return xhr;
};

ir.copyAttrs = (target, source) => {
    [...source.attributes].forEach(attr => {
        target.setAttribute(attr.nodeName, attr.nodeValue)
    })
};

// Diese Methode wird von google Maps nach Init aufgerufen
window.initMap = function() {
    ir.fire_all_callbacks('initGoogleMaps')
}

/**
 * Bindet die Google-Maps-Api ein, sofern noch nicht geschehen. Feuert Event `initGoogleMaps`
 * @fires initGoogleMaps
 * @param {function} cb Callback-Function
 */
ir.initGoogleMaps = function() {



    let apiKey = 'AIzaSyAJW2Kyoppz2PHMS5zvviZSI8vQ3qYOUIw'
    let url = `https://maps.googleapis.com/maps/api/js?key=${apiKey}&callback=initMap`

    let script = document.querySelector(`[src="${url}"]`)

    if(script) {
       console.log('Scripttag bereits geladen:', script)
       return;
    }

    script = document.createElement('script');
    script.src = url;
    script.defer = true;


    document.head.appendChild(script);
}

/**
 * Setzt abh. von der Viewportbreite eine entsprechende Klasse, die zur Kennung im JS benötigt wird
 * @returns {boolean}
 */
document.addEventListener('DOMContentLoaded', function(){
    (function(){
        const CLASS_MOBILE = 'is-mobile';
        const CLASS_DESKTOP = 'is-desktop';

        function checkForChanges()
        {
            if(window.matchMedia('(max-width: 1019px)').matches)
            {
                document.body.classList.add(CLASS_MOBILE);
                document.body.classList.remove(CLASS_DESKTOP);
                document.isScreenFormatMobile = true;
                return;
            }
            document.body.classList.add(CLASS_DESKTOP);
            document.body.classList.remove(CLASS_MOBILE);
            document.isScreenFormatMobile = false;
        }

        window.onresize = checkForChanges;
        checkForChanges();
    })();
});                    </script>

                                        
