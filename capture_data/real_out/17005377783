    <script>
                
                const divs = document.querySelectorAll('.ajax-element');
                            const promiseA = cmpClientTool.hasCustomConsentForVendor();
                        promiseA.then((val) => handleAjaxElements(val));
        
                function handleAjaxElements(hasCustomConsent)
        {
                        const paywall = document.getElementById('paywall-element');


            [].forEach.call(divs, function(div) {
                                if (hasCustomConsent) {
                                        let ajaxUrl = div.dataset.ajaxUrl;
                    if (ajaxUrl) {
                        ir.getAjax(ajaxUrl, function (response) {
                            try {
                                let responseData = JSON.parse(response);
                                if (responseData.hasOwnProperty('markup')) {

                                                                        const regex = /^ratio/;



                                    // Eine Schleife durch alle Klassen durchlaufen und diejenigen entfernen, die mit "ratio" beginnen
                                    div.classList.forEach(className => {
                                        if (regex.test(className)) {
                                            // Klasse entfernen, wenn sie mit "ratio" beginnt
                                            div.classList.remove(className);
                                        }
                                    });

                                    const html = "<div>" + responseData.markup + "</div>"
                                    let newDom = ir.toNode(html)

                                    // newDom.querySelectorAll('script').forEach(oldScript => {
                                    //     let newScript = document.createElement('script');
                                    //     newScript.innerText = oldScript.innerText;
                                    //
                                    //     ((newScript, oldScript) => {
                                    //         [...source.attributes].forEach(attr => {
                                    //             target.setAttribute(attr.nodeName, attr.nodeValue)
                                    //         })
                                    //     })();
                                    //
                                    //     oldScript.remove()
                                    //     div.appendChild(newScript)
                                    // })

                                                                        div.appendChild(newDom)

                                                                        if(div.classList.contains('inline-video') || div.classList.contains('video') || div.classList.contains('main-media-element')) {
                                        loadVideos()
                                    }

                                                                        if(div.classList.contains('inline-podcast') || div.classList.contains('podcast-serie')) {
                                        let scriptTag = div.querySelector('script.podigee-podcast-player')
                                        let newScriptTag = document.createElement('script')
                                        ir.copyAttrs(newScriptTag, scriptTag)
                                        scriptTag.replaceWith(newScriptTag)
                                    }


                                } else {
                                    div.innerHTML = '<!-- kein inhalt geliefert -->';
                                }
                            } catch (e) {
                                console.error(e);
                                div.innerHTML = '';                             }
                        } , function (response) {
                            console.error(response);
                            div.innerHTML = '';                         });
                    }
                } else {
                                        div.innerHTML = paywall.innerHTML;
                }
            });
        }
    </script>

        <script>
        document.querySelectorAll('[data-href="/services/logout/"]').forEach(el => {
            el.addEventListener('click', (ev) => {
                ev.preventDefault();
                let element = ev.currentTarget
                let url = element.getAttribute('data-href')
                localStorage.removeItem(ir_abonnement_config.version)
                localStorage.removeItem('custom_consent')
                localStorage.removeItem(ir_abonnement_config.cookiePlenigoSession)
                window.location = url;
            });
        });
    </script>

    </body>
</html>
