		<div class="w1200 mt30">
			<div class="h30 lh30 clf">
				<h2 class="f22 l">学说</h2>
				<a href="javascript:;" class="r f14" id="dicumRefresh" onclick="GetRndDictum();"  style="line-height: 30px;">换一换<img src="//img1.qianzhan.com/2017/images/refresh.png" alt="" class="ml10"></a>
			</div>
			<div class="c3_1200_65 clf xueshuo" id="dictumslist">
			</div>
			<script type="text/javascript">
				var myPix = new Array("//img1.qianzhan.com/2017/images/xueshuo1.jpg", "//img1.qianzhan.com/2017/images/xueshuo2.jpg", "//img1.qianzhan.com/2017/images/xueshuo3.jpg", "//img1.qianzhan.com/2017/images/xueshuo4.jpg", "//img1.qianzhan.com/2017/images/xueshuo5.jpg", "//img1.qianzhan.com/2017/images/xueshuo6.jpg", "//img1.qianzhan.com/2017/images/xueshuo7.jpg", "//img1.qianzhan.com/2017/images/xueshuo8.jpg", "//img1.qianzhan.com/2017/images/xueshuo9.jpg", "//img1.qianzhan.com/2017/images/xueshuo10.jpg");
				
                $(function () {
				    GetRndDictum();
				});

				var oRandomNum = -1;
				var oRandomNum1 = -1;
				function GetDifRndNum(i) {
				    var randomNum = Math.floor((Math.random() * myPix.length));
				    if (i == 0) {
				        oRandomNum = randomNum;
				        return randomNum
				    }
				    else if (i == 1) {
				        if (randomNum == oRandomNum) {
				            return GetDifRndNum(1);
				        }
				        oRandomNum1 = randomNum;
				        return randomNum
				    }
				    else if (i == 2) {
				        if (randomNum == oRandomNum || randomNum == oRandomNum1) {
				            return GetDifRndNum(2);
				        }
				        return randomNum
				    }
				    return randomNum
                }

                function GetRndDictum() {
                    var ajax = {
                        url: '/home/GetRndDictum', type: 'POST', dataType: 'json', cache: false,
                        data: {},
                        success: function (json, statusText) {
                            if (json.code == 1) {
                                var dictums = json.data;
                                var html = "<ul>";
                                for (var i = 0; i < dictums.length; i++) {
                                    var dictum = dictums[i];
                                    var imgNum = GetDifRndNum(i);
                                    var imgsrc = myPix[imgNum]

                                    html += "<li>";
                                    html += "<div class=\"xueshuo-content\">";
                                    html += "<div class=\"pic\"><img src=\"" + imgsrc + "\" alt=\"\" id=\"myPicture" + i.toString() + "\" /></div>";
                                    html += "<div class=\"text\">";
                                    html += "<h3>" + dictum.d_context + "</h3>";
                                    html += "</div>";
                                    html += "<div class=\"beizhu\">";
                                    html += "<p class=\"people\"><img src=\"" + dictum.dc_poto + "\" alt=\"\" />" + dictum.dc_name + "</p>";
                                    html += "<p class=\"tips\">" + dictum.d_desc + "</p>";
                                    html += "</div>";
                                    html += "</div>";
                                    html += "</li>";
                                }

                                html += "</ul>";
                                $("#dictumslist").html(html);
                            }
                        }
                    };
                    $.ajax(ajax);
                    return false;
                }
		    </script>
		</div>
