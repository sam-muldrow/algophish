        $(function () {
            Highcharts.SparkLine = function (a, b, c) {
                var hasRenderToArg = typeof a === 'string' || a.nodeName,
                        options = arguments[hasRenderToArg ? 1 : 0],
                        defaultOptions = {
                            chart: {
                                renderTo: (options.chart && options.chart.renderTo) || this,
                                backgroundColor: null,
                                borderWidth: 0,
                                type: 'area',
                                margin: [2, 0, 2, 0],
                                width: 98,
                                height: 64,
                                style: {
                                    overflow: 'visible',
                                    "z-index": 49
                                },
                                skipClone: true
                            },
                            title: {
                                text: ''
                            },
                            credits: {
                                enabled: false
                            },
                            xAxis: {
                                labels: {
                                    enabled: true
                                },
                                title: {
                                    text: null
                                },
                                startOnTick: false,
                                endOnTick: false,
                                tickPositions: []
                            },
                            yAxis: {
                                endOnTick: false,
                                startOnTick: false,
                                labels: {
                                    enabled: false
                                },
                                title: {
                                    text: null
                                },
                                tickPositions: [0]
                            },
                            legend: {
                                enabled: false
                            },
                            tooltip: {
                                backgroundColor: null,
                                borderWidth: 0,
                                shadow: false,
                                useHTML: true,
                                hideDelay: 0,
                                shared: true,
                                padding: 0,
                                positioner: function (w, h, point) {
                                    return { x: point.plotX - w / 2, y: point.plotY - h - 35 };
                                }
                            },
                            plotOptions: {
                                series: {
                                    animation: false,
                                    lineWidth: 1,
                                    shadow: false,
                                    states: {
                                        hover: {
                                            lineWidth: 1
                                        }
                                    },
                                    marker: {
                                        radius: 1,
                                        states: {
                                            hover: {
                                                radius: 2
                                            }
                                        }
                                    },
                                    fillOpacity: 0.25
                                },
                                column: {
                                    negativeColor: '#910000',
                                    borderColor: 'silver'
                                }
                            }
                        };
                options = Highcharts.merge(defaultOptions, options);
                return hasRenderToArg ?
                        new Highcharts.Chart(a, options, c) :
                    new Highcharts.Chart(options, b);
            };

            //loadZSCharts();

            $('#ulStocks').find('li[scode]').bind("click", function () {
                QZFMSubmit("//stock.qianzhan.com/", "_blank");
            });
        });

        var ArrDate = new Array();
        function loadZSCharts() {
            var codes = '';
            $('#ulStocks').find('li[scode]').each(function () {
                if (codes != '')
                    codes += ",";
                codes += $(this).attr("scode");
            });

            $.getJSON('//re2.qianzhan.com/stock/data/today2/' + codes + '?callback=?', function (json) {
                for (var a = 0; a < json.length; a++) {
                    var li = $('#ulStocks').find("li[scode='" + json[a].symbol + "']");
                    if (li.attr("scode")) {
                        var nmObj = li.find(".qz-n"); //名称
                        var zdObj = li.find(".qz-zs"); //涨跌
                        var chartObj = li.find(".qz-shareImg"); //图表

                        var nameStr = json[a].name;
                        var dataLen = json[a].data.length;
                        if (dataLen == 0)
                            nameStr += "<span>&nbsp;" + json[a].yestclose + "</span>";
                        else {
                            nameStr += "<span>&nbsp;" + json[a].data[dataLen - 1][1].toFixed(2) + "</span>";
                            if (json[a].data[dataLen - 1][1] > json[a].yestclose) {
                                zdObj.addClass("red");
                                zdObj.html((json[a].data[dataLen - 1][1] - json[a].yestclose).toFixed(2) + "<span>" + calcPercent(json[a].data[dataLen - 1][1], json[a].yestclose) + "</span>");
                            } else if (json[a].data[dataLen - 1][1] < json[a].yestclose) {
                                zdObj.removeClass("red");
                                zdObj.html((json[a].data[dataLen - 1][1] - json[a].yestclose).toFixed(2) + "<span>" + calcPercent(json[a].data[dataLen - 1][1], json[a].yestclose) + "</span>");
                            } else {
                                zdObj.removeClass("red");
                                zdObj.html("0.00<span>0.00%</span>");
                            }
                        }
                        //nmObj.html(nameStr);

                        ArrDate[json[a].symbol] = json[a].date.substr(4, 2) + "-" + json[a].date.substr(6, 2);
                        if (json[a].date1)
                            ArrDate[json[a].symbol + '1'] = json[a].date1.substr(4, 2) + "-" + json[a].date1.substr(6, 2); 

                        var dataX = new Array();
                        var dataY = new Array();
                        var dataAvg = new Array();
                        var curMin = 1000000000;
                        var curMax = 0;
                        for (var n = 0; n < json[a].count; n++) {
                            if (n < dataLen) {
                                dataX.push(json[a].data[n][0].substr(0, 2) + ":" + json[a].data[n][0].substr(2, 2));
                                dataY.push(json[a].data[n][1]);
                                dataAvg.push(json[a].data[n][2]);
                                if (curMin > json[a].data[n][1])
                                    curMin = json[a].data[n][1];
                                if (curMax < json[a].data[n][1])
                                    curMax = json[a].data[n][1];
                            }
                            else {
                                dataX.push(null);
                                dataY.push(null);
                            }
                        }
                        if (curMax <= json[a].yestclose) {
                            curMax = json[a].yestclose + json[a].yestclose - curMin;
                        } else {
                            if ((json[a].yestclose - curMin) < (curMax - json[a].yestclose)) {
                                curMin = json[a].yestclose - (curMax - json[a].yestclose);
                            } else {
                                curMax = json[a].yestclose + (json[a].yestclose - curMin);
                            }
                        }

                        //生成缩略图
                        chartObj.highcharts('SparkLine', {
                            series: [/*{
                                type: 'spline',
                                data: dataAvg,
                                name: '平均值',
                                zIndex: 50,
                                color: '#f1ad22',
                                lineWidth: 1,
                                index: 5,
                                //enableMouseTracking: false
                                states: {
                                    hover: {
                                        enabled: false
                                    }
                                }
                            }, */{
                                data: dataY,
                                name: json[a].symbol,
                                zIndex: 99,
                                index: 1,
                                color: '#6cb5ef'//#7cb5ec
                            }],
                            xAxis: {
                                categories: dataX
                            },
                            yAxis: {
                                min: curMin,
                                max: curMax,
                                plotLines: [{
                                    color: '#dddddd',
                                    dashStyle: 'ShortDash',
                                    value: json[a].yestclose,
                                    width: 1,
                                    label: {
                                        //text:json[a].yestclose.toFixed(0),
                                        align: 'right',
                                        x: 2,
                                        style: {
                                            color: '#dddddd',
                                            fontWeight: 'normal'
                                        }
                                    }
                                }]
                            },
                            tooltip: {
                                shared: true,
                                //headerFormat: '<b>{point.x}</b><br/>',
                                //pointFormat: '<b>{point.x} = {point.y}</b>',
                                useHTML: true,
                                formatter: function () {
                                    var yestVal = this.points[0].series.yAxis.plotLinesAndBands["0"].options.value;
                                    var perVal = calcPercent(this.y, yestVal);
                                    var cdate = ArrDate[this.points[0].series.name];
                                    if (this.x < '05:00')
                                        cdate = ArrDate[this.points[0].series.name + '1'];

                                    if (this.y > yestVal) {
                                        return '<b>' + cdate + '　' + this.x + '</b><br/><b style="color:#666666">&nbsp;' + this.y.toFixed(2) + '<br/> <b style="color:red">&nbsp;' + (this.y - yestVal).toFixed(2) + '（' + calcPercent(this.y, yestVal) + '）</b>';
                                    } else if (this.y < yestVal) {
                                        return '<b>' + cdate + '　' + this.x + '</b><br/><b style="color:#666666">&nbsp;' + this.y.toFixed(2) + '<br/> <b style="color:green">&nbsp;' + (this.y - yestVal).toFixed(2) + '（' + calcPercent(this.y, yestVal) + '）</b>';
                                    } else
                                        return '<b>' + cdate + '　' + this.x + '</b><br/><b style="color:#666666">&nbsp;' + this.y.toFixed(2) + '<br/> <b style="color:#666666">&nbsp;' + (this.y - yestVal).toFixed(2) + '（' + calcPercent(this.y, yestVal) + '）</b>';
                                }
                            },
                            chart: {
                                events: {
                                    click: function (e) {
                                        //alert(e.xAxis[0].value+" "+e.yAxis[0].value);
                                    }
                                }
                            }
                        });
                    }
                }
            });

            setTimeout("loadZSCharts()", 15000);
        }

        function calcPercent(cur, yest) {
            var val = (cur - yest) / yest * 100;
            if (val > 0) {
                return "+" + val.toFixed(3) + "%"
            } else if (val < 0) {
                return val.toFixed(3) + "%"
            }
            return val.toFixed(3) + "%"
        }
    </script>    <script type="text/javascript">
        var _load_partner = false;
        var scrollTime = 0;
        $(document).ready(function () {
            $(window).bind("scroll", Scroll2Loadpartners);

            $("div.tabtit_friend").find("em").mouseenter(function () {
                msObj = this;
                var curObj = this;
                setTimeout(function () {
                    if (msObj == curObj) {
                        $(curObj).addClass("cur").siblings().removeClass("cur");
                        var tab = $(curObj).attr("tab");
                        $("#divLinks" + tab).show().siblings("div").hide();
                    }
                }, 250);
            }).mouseleave(function () {
                msObj = null;
            });

            InitFDMenus();
        });
        function Scroll2Loadpartners() {
            if (_load_partner) { return; }
            var _bottom = $(document).scrollTop() + $(window).height();

            if (!_load_partner && $('#div-partners').offset().top < (_bottom + 150)) {
                LoadPartner();
                _load_partner = true;
            }
        }
        function LoadPartner() {
            var ajax = {
                url: '/about/loadPartner1', type: 'GET', dataType: 'html', cache: true,
                data: {},
                success: function (html, statusText) {
                    $('#div-partners').html(html);
                }
            };
            $.ajax(ajax);
        };
        function InitFDMenus() {
            $('a[name]').each(function (i) {
                $('#nav_fixed').append('<li><i></i><a href="#' + $(this).attr("name") + '">' + $(this).html() + '</a></li>');
            });
        }    </script>    <!-- 右侧悬浮导航 -->
