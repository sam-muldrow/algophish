    <script type="text/javascript">
         var timeOut = 0;
         var imgIndex = 0;
         var imgCount = 0;
         var FoxImg = {
             imgIndex: 0,
             int: function () {
                 var lis = $("#focusUL").find("li");
                 imgCount = lis.length;
                 $("#focusUL").css("width", imgCount.toString() + "00%");
             },
             loadImg: function () {
                 $("#focusUL").find("img[original]").each(function () {
                     if ($(this).attr("original") != $(this).attr("src")) {
                         $(this).attr("src", $(this).attr("original"));
                     }
                 });
             },
             changeIdx: function () {
                 var em = $('#foxPages').find("em[idx='" + (imgIndex + 1).toString() + "']");
                 em.parent().children("em").removeClass("cur");
                 em.addClass("cur");
             },
             nextImg: function () {
                 if (imgIndex >= imgCount - 1) {
                     FoxImg.prevImg();
                     return;
                 }
                 var offsetNext = (parseInt(imgIndex) + 1) * 100;
                 $("#focusUL").animate({
                     left: "-" + offsetNext + "%"
                 }, 600, function () {
                     imgIndex++;
                     FoxImg.changeIdx();
                     clearTimeout(timeout);
                     timeout = setTimeout("FoxImg.nextImg()", 5000);
                 });
                 return false;
             },
             nextImgClick: function () {
                 if (imgIndex >= imgCount - 1) {
                     imgIndex = -1;
                 }
                 var offsetNext = (parseInt(imgIndex) + 1) * 100;
                 $("#focusUL").animate({
                     left: "-" + offsetNext + "%"
                 }, 600, function () {
                     imgIndex++;
                     FoxImg.changeIdx();
                     clearTimeout(timeout);
                     timeout = setTimeout("FoxImg.nextImg()", 5000);
                 });
                 return false;
             },
             prevImg: function () {
                 if (imgIndex == 0) {
                     FoxImg.nextImg();
                     return;
                 }
                 var offseNext = (parseInt(imgIndex) - 1) * 100;
                 $("#focusUL").animate({
                     left: "-" + offseNext + "%"
                 }, 600, function () {
                     imgIndex--;
                     FoxImg.changeIdx();
                     clearTimeout(timeout)
                     timeout = setTimeout("FoxImg.prevImg()", 5000);
                 });
                 return false;
             },
             prevImgClick: function () {
                 if (imgIndex == 0) {
                     imgIndex = imgCount;
                 }
                 var offseNext = (parseInt(imgIndex) - 1) * 100;
                 $("#focusUL").animate({
                     left: "-" + offseNext + "%"
                 }, 600, function () {
                     imgIndex--;
                     FoxImg.changeIdx();
                     clearTimeout(timeout)
                     timeout = setTimeout("FoxImg.prevImg()", 5000);
                 });
                 return false;
             }
         }

         var timejx = 0;
         function changejx(way) {
             $('#div_jinxuan').children("div").each(function () {
                 if ($(this).css("display") != "none") {
                     var cur = $(this);
                     if (way == 1) {
                         if (cur.next().attr("class")) {
                             cur.fadeOut("fast", function () {
                                 cur.next().fadeIn("fast");
                             });
                         } else {
                             cur.fadeOut("fast", function () {
                                 cur.prev().fadeIn("fast");
                             });
                         }
                     } else {
                         if (cur.prev().attr("class")) {
                             cur.fadeOut("fast", function () {
                                 cur.prev().fadeIn("fast");
                             });
                         } else {
                             cur.fadeOut("fast", function () {
                                 cur.next().fadeIn("fast");
                             });
                         }
                     }
                 }
             });
             //clearTimeout(timejx);
             //timejx = setTimeout("changejx(" + way.toString() + ");", 6000);
             return false;
         }

         $(document).ready(function () {
             FoxImg.int();
             timeout = setTimeout("FoxImg.nextImg();", 5000);
             setTimeout("FoxImg.loadImg();", 5000);
             //timejx = setTimeout("changejx(1);", 6000);
             $('#div_hottabs').children('span').bind("mouseenter", function () {
                 changHot($(this).attr("idx"));
                 $(this).parent().children().removeClass("cur");
                 $(this).addClass("cur");
             });
             $("#div_kuaixun").mCustomScrollbar({
                 autoHideScrollbar: true
             });
         });

         function changHot(idx) {
             var ul = $('#div_hotlist').find('ul[idx=' + idx + ']');
             ul.siblings().hide();             
             ul.show();
         }
    </script>  

