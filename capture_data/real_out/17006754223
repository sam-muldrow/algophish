				.stylesgoright,
				.stylesgoleft{
					background-color: rgba(0,0,0,0.5);
					width: 60px;
					height: 60px;
					border-radius: 60px;
					text-align: center;
					line-height: 60px;
					color: #fff;
					top: 50%;
					cursor: pointer;
					display: inline-block;
					position: absolute;
					z-index: 100;
					margin-top: -30px;
					font-family: \5B8B\4F53;
					font-size: 20px;
