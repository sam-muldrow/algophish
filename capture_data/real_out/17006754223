    <script type="text/javascript">
        var sizeW = window.screen.width, sizeH = window.screen.height;
        $().ready(function () {
            var nsize = sizeW + 'x' + sizeH;
            var size = sDialog.getCookie('screensize');
            if (nsize != size) {
                sDialog.setCookie('screensize', nsize, 30 * 24 * 60);
            }
        });

        $(function () {
