.card-container.card-horizontal.row:not(.expanded) {
    max-width: 100%;
}
.card-container.card-horizontal.row:not(.expanded) .card-content {
    max-height: 359px;
}
.card-container.card-horizontal.row:not(.expanded) .card-content .card-info-wrapper .card-info .card-title {
    font-size: 1.5rem;
    font-weight: 600;
}
}
.white {background-color: #ffffff;}

</style>
</div>


