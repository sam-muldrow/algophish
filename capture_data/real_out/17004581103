        $(window).on('load', function () {
            var obj = {
                apiRoot: 'https://www.icicidirect.com/marketapi/market',
                siteRoot: 'https://www.icicidirect.com',
                equityBuyUrl: '',
                equitySellUrl: '',
                mfInvestUrl: ''
            };
            icicid.common.init(obj);
