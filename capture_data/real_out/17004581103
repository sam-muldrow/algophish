    <script>
        $(window).on('load', function () {
            $('.collapseBtn').click(function () {
                $(this).parents('.side-sticky-wrapper').toggleClass('active');
            })
            $('.btn-next').click(function () {
                var target = $(this).parents('[data-gstarget]').data('gstarget');
                $('[data-gstarget]').hide();
                $('[data-gstarget=" ' + (target + 1) + ' "]').show();
            });
        });
    </script>
    <script>
        $(window).on('load', function () {
            obj = {
                apiRoot: 'https://www.icicidirect.com/marketapi/market',
                siteRoot: 'https://www.icicidirect.com',
                imgPath: 'https://www.icicidirect.com/images/',
                imageDescIdJS: '1424',
                image2_descriptionJS: '1481',
                imageJS: '1425',
                videoJS: '1501'
            };
            $(".full_page_loader").addClass("d-none");
            $(".home_banner").show();
            var jsonobject = {
                "Method": "GetEquityKeyindices",
                "param": []
            }
            $.ajax({
                url: obj.apiRoot,
                type: 'POST',
                datatype: "application/json",
                data: jsonobject,
                success: function (response) {
                    if (response.IsSuccess && response.Data != null && response.Data.Table.length > 0) {
                        var data = response.Data.Table;
                        //debugger;
                        var usdmarket = data[5];
                        var eurmarket = data[3];
                        var niftymarket = data[6];
                        var sensexmarket = data[9];
                        var bankniftymarket = data[7];
                        var midcapmarket = data[8];
                        var goldMarket = data[1];
                        var silverMarket = data[2];
                        if (sensexmarket != null) {
                            var price = sensexmarket.PRICE;
                            var change = sensexmarket.PCHANGE;
                            var isPositive = "";
                            if (change > 0) {
                                isPositive = "positive";
                            } else {
                                isPositive = "negative";
                            }
                            var sensexHtml = '<label class="label_heading">Sensex</label>' +
                                '<div class="share ' + isPositive + '">' +
                                '<i class="icon-up-arrow"></i>' +
                                '<b class="share_value">' + icicid.helper.numberWithCommas(price) + '</b>' +
                                '<span class="chg">' + change + '%</span>' +
                                '</div>';
                            $("#divSensex").html(sensexHtml);
                        }
                        if (niftymarket != null) {
                            var price = niftymarket.PRICE;
                            var change = niftymarket.PCHANGE;
                            var isPositive = "";
                            if (change > 0) {
                                isPositive = "positive";
                            } else {
                                isPositive = "negative";
                            }
                            var niftyHtml = '<label class="label_heading">Nifty</label>' +
                                '<div class="share ' + isPositive + '">' +
                                ' <i class="icon-up-arrow"></i>' +
                                '<b class="share_value">' + icicid.helper.numberWithCommas(price) + '</b>' +
                                '<span class="chg">' + change + '%</span>' +
                                '</div>';
                            $("#divNifty").html(niftyHtml);
                        }
                        if (bankniftymarket != null && midcapmarket != null) {
                            var bnkprice = bankniftymarket.PRICE;
                            var bnkchange = bankniftymarket.PCHANGE;
                            var isBnkPositive = "";
                            if (bnkchange > 0) {
                                isBnkPositive = "positive";
                            } else {
                                isBnkPositive = "negative";
                            }
                            var midprice = midcapmarket.PRICE;
                            var midchange = midcapmarket.PCHANGE;
                            var ismidPositive = "";
                            if (midchange > 0) {
                                ismidPositive = "positive";
                            } else {
                                ismidPositive = "negative";
                            }
                            var bankexHtml = '<label class="label_heading">NIFTY BANK</label>' +
                                '<div class="share ' + isBnkPositive + '">' +
                                ' <i class="icon-up-arrow"></i>' +
                                '<b class="share_value">' + icicid.helper.numberWithCommas(bnkprice) + '</b>' +
                                '<span class="chg">' + bnkchange + '%</span>' +
                                '</div>';
                            $("#divBankex").html(bankexHtml);
                            var midcapHtml = '<label class="label_heading">Midcap</label>' +
                                '<div class="share ' + isBnkPositive + '">' +
                                ' <i class="icon-up-arrow"></i>' +
                                '<b class="share_value">' + icicid.helper.numberWithCommas(midprice) + '</b>' +
                                '<span class="chg">' + midchange + '%</span>' +
                                '</div>';
                            $("#divMidcap").html(midcapHtml);
                        }
                        if (usdmarket != null) {
                            var price = usdmarket.PRICE;
                            var change = usdmarket.PCHANGE;
                            var isPositive = "";
                            if (change > 0) {
                                isPositive = "positive";
                            } else {
                                isPositive = "negative";
                            }
                            var usdMarketHtml = '<div class="country"><img width="38" height="23" src="' + obj.siteRoot + '/Content/images/flag5.png" alt="">USD/INR</div>' +
                                '<div class="share_value">' +
                                '<i class="icon-up-arrow ' + isPositive + '"></i> ' + price + '</div>' +
                                '<span class="chg ' + isPositive + '">' + change + '%</span>'
                            $("#divusdmarket").html(usdMarketHtml);
                        }
                        if (eurmarket != null) {
                            var price = eurmarket.PRICE;
                            var change = eurmarket.PCHANGE;
                            var isPositive = "";
                            if (change > 0) {
                                isPositive = "positive";
                            } else {
                                isPositive = "negative";
                            }
                            var eurMarketHtml = '<div class="country"><img width="38" height="23" src="' + obj.siteRoot + '/Content/images/flag6.png" alt="">EUR/INR</div>' +
                                '<div class="share_value">' +
                                '<i class="icon-up-arrow ' + isPositive + '"></i> ' + price + '</div>' +
                                '<span class="chg ' + isPositive + '">' + change + '%</span>';
                            $("#diveurmarket").html(eurMarketHtml);
                        }
                        if (goldMarket != null && silverMarket != null) {
                            var bnkprice = goldMarket.PRICE;
                            var bnkchange = goldMarket.PCHANGE;
                            var isBnkPositive = "";
                            if (bnkchange > 0) {
                                isBnkPositive = "positive";
                            } else {
                                isBnkPositive = "negative";
                            }
                            var midprice = silverMarket.PRICE;
                            var midchange = silverMarket.PCHANGE;
                            var ismidPositive = "";
                            if (midchange > 0) {
                                ismidPositive = "positive";
                            } else {
                                ismidPositive = "negative";
                            }
                            var goldHtml = '<label class="label_heading">Gold (Fut)</label>' +
                                '<div class="share ' + isBnkPositive + '">' +
                                ' <i class="icon-up-arrow"></i>' +
                                '<b class="share_value">' + icicid.helper.numberWithCommas(bnkprice) + '</b>' +
                                '<span class="chg">' + bnkchange + '%</span>' +
                                '</div>';
                            $("#divGold").html(goldHtml);
                            var silverHtml = '<label class="label_heading">Silver (Fut)</label>' +
                                '<div class="share ' + ismidPositive + '">' +
                                ' <i class="icon-up-arrow"></i>' +
                                '<b class="share_value">' + icicid.helper.numberWithCommas(midprice) + '</b>' +
                                '<span class="chg">' + midchange + '%</span>' +
                                '</div>';
                            $("#divSilver").html(silverHtml);
                        }
                    }
                    else {
                    }
                },
                error: function (error) {
                    console.log(error)
                }
            })
        });
    </script>
    <script>
        function onError() {
            this.onerror = null;
            this.parentNode.children[0].srcset = this.parentNode.children[1].srcset = this.src;
        }
    </script>
