			$("#leftSec"+docId ).addClass('cur');
		}
		if(length == 4){
			$("#left"+tempNavArr[3].replace(/\+/g,"")).addClass('active');
			$("#nav"+tempNavArr[3]).addClass('cur').find('ul').show();
			$("#leftSec"+docId ).addClass('cur');
		}if(length == 5){
			$("#left"+tempNavArr[3]).addClass('active');
			$("#left"+tempNavArr[4]).addClass('active');
			$("#nav"+tempNavArr[4]).addClass('cur').find('ul').show();
			$("#leftSec"+docId ).addClass('cur');
		}
	}
