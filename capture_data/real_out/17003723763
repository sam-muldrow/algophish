	for (let i in zlData) {
		let len = zlData[i].length;
		let index = parseInt(Math.random() * len);
		let style = zlData[i][index].style;
		let classTag = '';
		let styleTag = '';
		let title = "";
		try{
			title = decodeURIComponent(zlData[i][index].title);
			if (style.indexOf('isBold') > -1) {
				classTag += 'linkNewsTopBold';
			}
			if (style.indexOf('isRed') > -1) {
				styleTag += ''; // color:red;
			}
		}catch(e) {
			title = zlData[i][index].title;
		}

		$(".news_top")
			.children("li")
			.eq(+i - 1)
			.html(
			`<a target="_blank" href=${zlData[i][index].url} class="${classTag}" style="${styleTag}">${title}</a>`
		);
	}
</script>
