<script type="module">
(function trackSiteCatalyst(CR) {
  const { userInfo, pageProps, cookies, getLayout } = CR;

  try {
    // Initialize Site Catalyst's s.*
    initSiteCatalyst();
  } catch (e) {
    console.error('Site Catalyst init:', e.message);
  }

  function initSiteCatalyst() {
    window.s = s_gi(s_account);

    Object.assign(s, pageProps.siteCat.data);

    if (s.prop18) {
      s.events = s.apl(s.events, 'event104', ',', '1');
    }

    s.prop15 = userInfo.getErightsId();
    s.prop16 = userInfo.getSegment();

    const tier = userInfo.getTier();
    if (tier) {
      s.prop27 = s.eVar80 = tier.toLowerCase();
      s.linkTrackVars += ',prop27,eVar80';
    }

    const subscriptions = userInfo.getSubscriptions();
    if (subscriptions) {
      s.prop28 = s.eVar86 = subscriptions;
      s.linkTrackVars += ',prop28,eVar86';
    }

    s.prop47 = userInfo.getId();
    s.eVar58 = s.prop58 = getLayout();

    if (cookies.get('siteCatalystLoginSuccess')) {
      s.events = s.apl(s.events, 'event13', ',', '1');

      cookies.set('siteCatalystLoginSuccess', null, {
        expires: -1,
        path: '/',
        domain: document.domain.split('.').slice(-2).join('.'),
      });
    }

    s.eVar115 = s.prop55;
    s.eVar116 = s.prop56;
    s.eVar117 = s.prop57;
    s.eVar63 = s.prop63;

      
      
      if (pageProps.siteCat.trackOnLoad) {
        s.t();
      }
  }

})(window.CR || {});
</script>
<!-- End Site Catalyst -->

