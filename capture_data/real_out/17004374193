	jQuery(document).ready(function($) {
		$(window).scroll(function() {
			var scroll = $(window).scrollTop();

			if (scroll >= 60) {
				$(".mali_logo_container").addClass("mali_logo_enlarge");
			} else {
				$(".mali_logo_container").removeClass("mali_logo_enlarge");
			}
		});
	});
