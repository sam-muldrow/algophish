<div class="balloon01 stay"><p><span>気になるリストに保存しました</span>「 気になるリストへ」のボタンから、気になるリスト一覧へ移動できます</p><!-- /.baloon01 --></div>
<script>
$(window).on("load", function() {
  // 一覧へリンクの設定
  var NOLOGIN_URLS = {
      recommend00: '/kyujin/recommend/ranking2.html?usrclk=PC_logout_kyujinRecommendArea_recommendList',
      recommend01: '/DodaFront/View/JobHistoryList.action?usrclk=PC_logout_kyujinRecommendArea_recommendList',
      recommend02: '/dcfront/bookmark/bookmarkList/?usrclk=PC_logout_kyujinRecommendArea_recommendList',
      recommend03: '/kyujin/recommend/ranking3.html?usrclk=PC_logout_kyujinRecommendArea_recommendList',
      recommend_newarrival_nologin: '/DodaFront/View/NewJobList.action?usrclk=PC_logout_kyujinRecommendArea_newArrivalList',
  };

  // レコメンド表示
  recommend("recommend00", "AWS_Recommend_oubo,AWS_Recommend_etsuran", "normal", "event70,event70", "Rekome_pc,Rekome_pc", "DODATOP_NotLogin_Rekome_Oubo_ChikaiKyujin_w2v_pc,DODATOP_NotLogin_Rekome_etsuran_ChikaiKyujin_w2v_pc", "", "応募した求人に近い求人,最近見た求人に近い求人");
  recommend("recommend01", "view_list", "normal", "event70", "Rekome_pc", "DODATOP_NotLogin_Rekome_etsuran_Rireki_pc", "", "最近見た求人");
  recommend("recommend02", "bookmark_list", "normal", "event70", "Rekome_pc", "DODATOP_NotLogin_Rekome_bookmarklist_pc", "", "気になるリストに保存している求人");
  recommend("recommend_newarrival_nologin", "newjob_list", "normal", "event70", "Rekome_pc", "DODATOP_NotLogin_Rekome_newjob_list_pc", "", '新着求人 <span id="recommend_newarrival_count">');
  recommend("recommend03", "ranking_ocl_pf,ranking_all", "normal,normal", "event70,event70", "Rekome_pc,Rekome_pc", "DODATOP_NotLogin_Rekome_Kibou_AreaRank_pc,DODATOP_NotLogin_Rekome_Kibou_ChikaiKyujinRank_pc", "", "人気求人ランキング");

  var targets = document.querySelectorAll('.recommend');
  for (var i = 0; i < targets.length; i++) {
    var target = targets[i];

    var observer = new MutationObserver(function (mutations) {
      var observerTarget = mutations[0].target;
      var elementId = observerTarget.id;
      if (observerTarget.querySelectorAll('.slider01:not(.bx-clone)').length <= 1) {
        observerTarget.innerHTML = '';
      } else {
        var parameter_val;
        var url;
        //設定するユーザークリックパラメータの決定
        url = NOLOGIN_URLS[elementId]
        parameter_val = elementId === 'recommend_newarrival_nologin' ? "PC_logout_kyujinRecommendArea_newArrivalDetail" : "PC_logout_kyujinRecommendArea_recommendDetail";

        var link = document.createRange().createContextualFragment('<a href=' + url +' class="moreLink">一覧へ</a>');
        observerTarget.querySelector('.head02').appendChild(link);

        //各レコメンドのリンクにdata-reqparamの属性を設定
        var targetLinks =document.getElementById(elementId).querySelectorAll('a:not(.moreLink):not(.btnType02):not(.bx-pager-link):not(.bx-next):not(.bx-prev):not(.btnList03):not(.btnList06)');
        for (i = 0; i < targetLinks.length; i++) {
          var element = targetLinks[i];
          var reqparamValue = element.getAttribute('data-reqparam');
          try{
            reqparamValue = JSON.parse(reqparamValue);
            param_length = reqparamValue.parameter.length;
            var usrclk_param = { key : "usrclk", val : parameter_val};
            reqparamValue.parameter[param_length] = usrclk_param;
            element.setAttribute("data-reqparam", JSON.stringify(reqparamValue));
          }
          catch(e) {
            // 処理エラーを無視する
          }
        }

        var target = document.getElementById('recommend_newarrival_count');
        if (target != null) {
          var htmlString = '<span class="number">' + document.getElementById('STAGT056_WSSTFL1_1').textContent + '</span><span> 件</span><br><span class="annotation">毎週月・木曜に新着求人をお知らせ</span>';
          target.innerHTML = htmlString;
        }
      }
    });

    observer.observe(target, {childList: true});
  }
});

</script>
					
				
			
