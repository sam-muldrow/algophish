       <script>

       function darichechClick(ItemID) {

        $('.daricheh-main-images').hide();

        $('#daricheh-main-image'+ItemID).show();  

        $('.daricheh-title-images').removeClass('active');

        $('#daricheh-title-image'+ItemID).addClass('active');

       }

       $(function() 

         {darichechClick(0); 

       });

       counter = 0;

       setInterval(function() {

         counter = (counter == 2) ? 0 : counter+1;

         darichechClick(counter);

       },7000);

       </script></div><div class='col-lg-4 col-md-5 col-sm-12 col-xs-12 stick_box2'><div class='row'>

