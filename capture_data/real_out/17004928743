      <script>

      

      function selectedSectionHover(boxID,ItemID) {

        $('#section'+boxID+'-images div').hide();

        $('#section'+boxID+'-image'+ItemID).show();  

        $('#section'+boxID+'-titles div').removeClass('active');

        $('#section'+boxID+'-title'+ItemID).addClass('active');

      }

