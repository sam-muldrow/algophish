//禁止图片粘贴
function formatHtml(str){
    str=str.replace(/<img[^>]*>/g,'');
    str=str.replace(/<a[^>]*>/g,'');
    str=str.replace(/<\/a>/g,'');
    alert('由于安全原因，免费用户不提供图片粘贴功能');
    return str;
}
