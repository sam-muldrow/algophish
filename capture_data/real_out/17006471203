  function batchStaticLeadingTipControll(){
    var countStorage = sessionStorage.getItem(STORAGE_KEY)
    var count = Number(countStorage);

    if(countStorage === null || count < FIRST_SHOW_COUNT){
      count += 1
      sessionStorage.setItem(STORAGE_KEY, count);
    }else if(count >= MUTE_COUNT && count < SECOND_SHOW_COUNT_AFTER_MUTE) {
      count += 1
      sessionStorage.setItem(STORAGE_KEY, count);
    }

    // // 关闭按钮
