  (function() {
    window.$isOverflowLength = function(str) {
      var s = encodeURI(str).toString().replace(/\%[0-9a-fA-F]{2}/g, 'a').length;

      return s > 2950;
    };

    window.$replaceOverflowText = function(str) {
      var arr = [];
      var ttlen = 0;
      for (var i = 0; i < str.length; i++) {
        var charLen = 0;

        if (str.charCodeAt(i) > 255) {
          charLen = 3;
        } else {
          charLen = 1;
        }
        if ((ttlen + charLen) > 2950) {
          break;
        } else {
          arr.push(str[i]);
          ttlen += charLen;
        }
      }

      return arr.join('');
    };
  })();
</script><script type="text/javascript" src="//static.clewm.net/cli/js/comments-upload-v3.js?v=20190430_1"></script><script type="text/javascript" src="//static.clewm.net/cli/js/upload-commentsV3.js?v=20190430_1"></script><div class="wrapper new-light show-header-offset-el" id="generate-area"><div class="page-main-content p-b-md padding-target text-sub-wrapper" style="padding-top: 2px; min-height: 416px;"><div id="VerifyTip" class="alert alert-warning none"></div><div class="row noMagrin"><div class="col-md-8 noPadding left-layout" id="qrgenerate-box"><div class="static-content-wrap"><div class="qrgenerate-box" id="trigger-type"><style>
