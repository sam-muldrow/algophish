$(".cookieNoticeCloser").ready(function () {

    $(".cookieNoticeCloser").click(function(){
        $.ajax( {
            url: '/notice_cookie_switch_off.do;jsessionid=g39CDA43F90BA0DC130A99CCA173D8AF3',
            type: "get",

            beforeSend: function(){
                ajaxOpenWait();
            },
            complete: function(){
                ajaxCloseWait()
            },
            success: function () {
                $('.cookieNotice').hide();
            }
        });
    });

});

$(".cookieNoticeCloser_essential").ready(function () {

    $(".cookieNoticeCloser_essential").click(function(){
        $.ajax( {
            url: '/notice_cookie_switch_off.do;jsessionid=g39CDA43F90BA0DC130A99CCA173D8AF3?essential=yes',
            type: "get",

            beforeSend: function(){
                ajaxOpenWait();
            },
            complete: function(){
                ajaxCloseWait()
            },
            success: function () {
                $('.cookieNotice').hide();
            }
        });
    });

});


</script>


