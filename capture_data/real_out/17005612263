                     console.log("1.5:" + infos_rm.is(e.target));
                     console.log("1.7:" + infos_rm.has(e.target).length);
                     console.log("2:" + div.has(e.target).length );*/
                    if (!div.is(e.target) // если клик был не по нашему блоку
                        && div.has(e.target).length === 0  // и не по его дочерним элементам
                        && !infos_rm.is(e.target)
                        && infos_rm.has(e.target).length === 0
                    ) {
                        /*   e.preventDefault();
                           e.stopPropagation();
                           e.stopImmediatePropagation();*/
                        div.hide(); // скрываем его
                        $("#clickclickRmInfosId").removeClass("PearlTopTabLightBlue");
                        $("#clickclickRmInfosId").addClass("PearlTopTabYellow");

                        return false;
                    }
                }
            });
        });
$(".ContentArticleRight").ready(function() {
            $("#clickclickRmInfosId").click(function() {
                if ($(".ContentRight").is(":hidden")) {

                    var contentWidth = $("#main_content").width();
                    if (!contentWidth) {
                        contentWidth = $(".ContentCenter").width();
                    }

                    $(".ContentRight").attr("style", "width: 48%; display: block !important;");
                    $(".ContentRight").attr("style", "width: 48%; display: block !important; position: absolute; left:" + (contentWidth-$(".ContentRight").outerWidth())  + "px");
                    $(".ContentRight").css("z-index", "500");
                    $(".ContentRight").offset({top: $(".infos_rm").offset().top + $(".infos_rm").height() });
                   // $(".ContentRight").css("background", "#f3f7fa");
                   // $(".ContentArticleRight").css("background", "#f3f7fa");
                    $("#clickclickRmInfosId").removeClass("PearlTopTabYellow");
                    $("#clickclickRmInfosId").addClass("PearlTopTabLightBlue");
                } else {
                    $(".ContentRight").hide();
                    $("#clickclickRmInfosId").removeClass("PearlTopTabLightBlue");
                    $("#clickclickRmInfosId").addClass("PearlTopTabYellow");

                }
                return false;
            });
        });
