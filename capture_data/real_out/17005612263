        function basketOrderTimeout(cartLink) {
            if ($.detectBrowser.isIE() && 8>$.detectBrowser.ieVersion()) {
                return true;
            }
            var l = $(this);
            if (!l.hasClass("unclickable")) {
                l.addClass("unclickable");
                setTimeout(function(){l.removeClass("unclickable")},1500);
                return true;
            }
            return false;
        }

