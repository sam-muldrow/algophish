function ffTimer() {
        if (document.forms['search_1'].query.value.trim() != '') return true;
        Timer();
        return false;
    }

    var str = 'white';
    var ii = 0;
    var timerID = null;
    var timerRunning = false;

    function Timer() {
        if (ii % 2 == 1 )
            str = 'white';
        else
            str = 'red';
        ii++;
        document.forms['search_1'].query.style.backgroundColor = str;
        timerID =  setTimeout("Timer()", 100 );
        timerRunning = true;
        if (ii==6) {
            if(timerRunning) clearTimeout(timerID);
            timerRunning=false;
            ii=0;
        }

    }

    $(document).ready(function() {
        if (!("ontouchstart" in window || (window.DocumentTouch && document instanceof DocumentTouch))) {
            var topSearchInput = $('#topSearchInput');
            var printerSearchInput = $('#printerSearchInput');
            var tmp;
            if (printerSearchInput.length) {
                printerSearchInput.focus();
                tmp = printerSearchInput.val();
                printerSearchInput.val('').val(tmp);
            }
            else {
                topSearchInput.focus();
                tmp = topSearchInput.val();
                topSearchInput.val('').val(tmp);
            }
        }
    });

    function isKeyboardEvent(event) {
        return event.originalEvent &&
                event.originalEvent.originalEvent &&
                /^key/.test( event.originalEvent.originalEvent.type );
    }
var currentCartPopUp = null;
    var currentCartPopUpHasMouse = false;
    function showCartPopup(popup){
        if (currentCartPopUpHasMouse) {
            return
        }
        if (currentCartPopUp && currentCartPopUp!=popup) {
            currentCartPopUp.hide();
        }
        currentCartPopUp = popup;
        if (currentCartPopUp && !isMobileClient()) {
            currentCartPopUp.show();
        }
    }
    $('#cart_state_info_id').mouseover(function(){
        currentCartPopUpHasMouse=false;
        showCartPopup( $('#pearl-dropdown-basket_view'));
    }).mouseleave(function(){
        currentCartPopUpHasMouse=false;
        showCartPopup(null);
    });
    $('#pearl-dropdown-basket_view')
        .mouseenter(function(){
            currentCartPopUpHasMouse=true;
        }).mouseleave(function(){
        currentCartPopUpHasMouse=false;
        showCartPopup(null);
    });
    $(document).click(function(){showCartPopup(null)});
function onYoutubeClick() {
		var youtubeStub = document.getElementById("youtubeStub");
		if (youtubeStub.getAttribute("hidden")
				|| youtubeStub.getAttribute("hidden") != "true") {
			youtubeStub.setAttribute("hidden", "true");
			var youtubediv = document.getElementById("youtubeIFrame");
			youtubediv.innerHTML = '<iframe style="border: none;" '
					+ 'src="https://www.youtube-nocookie.com/embed/live_stream?channel=UCp0urejlDaYz4G8xpTxdIvw&iv_load_policy=3&showinfo=0&rel=0&vq=144&autoplay=1" '
					+ 'allow="autoplay; encrypted-media" '
					+ 'allowfullscreen="allowfullscreen" frameborder="0" height="105" '
					+ 'width="187"></iframe>';
			youtubediv.insertBefore()
		}
	}
function setCookie(name, value, days) {
            if (days) {
                var date = new Date();
                date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
                var expires = "; expires=" + date.toGMTString()
            } else var expires = "";
            document.cookie = name + "=" + value + expires + "; path=/; SameSite=None; Secure;"
        }
        function getCookie(name) {
            var nameEQ = name + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') c = c.substring(1, c.length);
                if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length)
            }
            return null
        }
        function delCookie(name) {
            setCookie(name, "", -1)
        }
(function ($) {
        	
            $.fn.waitUntilExists = function (handler, shouldRunHandlerOnce, isChild) {
                var found = 'found';
                var $this = $(this.selector);
                var $elements = $this.not(function () {
                    return $(this).data(found);
                }).each(handler).data(found, true);

                if (!isChild) {
                    (window.waitUntilExists_Intervals = window.waitUntilExists_Intervals || {})[this.selector] =
                            window.setInterval(function () {
                                $this.waitUntilExists(handler, shouldRunHandlerOnce, true);
                            }, 500)
                    ;
                }
                else if (shouldRunHandlerOnce && $elements.length) {
                    window.clearInterval(window.waitUntilExists_Intervals[this.selector]);
                }
                return $this;
            }
        }(jQuery));


        $('#gts-checkbox').waitUntilExists(function () {
            $('#gts-checkbox').click(function (e) {
                console.log("Clicked on YES button");
                $.ajax({
                    type: "POST",
                    method: "POST",
                    url: "/google/gts/switchmarker.do",
                    data: "gts=true",
                    success: function (msg) {
                        console.log("GTS send: " + msg);
                    }
                });
            });
        }, true);


        $('#gts-g-cl').waitUntilExists(function () {
            $('#gts-g-cl').click(function (e) {
                console.log("Clicked on NO button");
                $.ajax({
                    type: "POST",
                    method: "POST",
                    url: "/google/gts/switchmarker.do",
                    data: "gts=false",
                    success: function (msg) {
                        console.log("GTS send: " + msg);
                    }
                });
                e.preventDefault();
            });
        }, true);

