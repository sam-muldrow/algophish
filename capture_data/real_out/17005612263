            <script language="JavaScript">
                $(document).ready(function() {
                    //$( '#dl-menu' ).dlmenu();
                });
                function goToCategory(url, param) {
                    console.log('url=' + url);
                    console.log('param=' + param);
                    var param2 = 'open';
                    if (param === 'category-menu__item_selected') {
                        dlTriggerClick();
                        return false;
                    }
                    if (url.indexOf("/nc-") < 0) {
                        param2='close';
                    }
                    var url2;
                    if (url.indexOf("?") > -1) {
                        url2 = url + "&selectedClass=" + param2;
                    } else {
                        url2 = url + "?selectedClass=" + param2;
                    }
                    console.log('url2=' + url2);
                    document.location.href = url2;
                    return false;
                }

                function dlTriggerClick() {
                    $(document).ready(function() {
                        $("#parent_swipe").css("top", $(window).scrollTop()+43);
                        if ( $("#pparent_swipe").prop('innerHTML') === "" ) {
                            

                            $("#pparent_swipe").load("/category/tree.do" + getJsessionId()+"?catid=xxxx&mobile=yes", function () {
                                $.getScript("/js/category-menu.js?v=1");
                      //          $('#dl-menu').dlmenu();
                               // $("#parent_swipe").css("dispay", "block");
                                $("#parent_swipe").css("display", "block");
                                $("#parent_swipe").css("opacity", "1");
                                $("#parent_swipe").css("pointer-events", "all");
                               // $("#parent_swipe").css("width", "100%");
                            });
                        } else  {
                            if ($("#parent_swipe").is(":visible")) {
                                $("#parent_swipe").css("display", "none");
                            } else {
                                $("#parent_swipe").css("display", "block");
                                $("#parent_swipe").css("opacity", "1");
                                $("#parent_swipe").css("pointer-events", "all");
                               // $("#parent_swipe").css("width", "100%");
                            }
                        }
                        return true;
                    });
                }

                $(document).ready(function() {
                    $(document).mouseup(function (e) { // событие клика по веб-документу
                        if ($("#parent_swipe").is(":visible")) {
                            var div = $("#parent_swipe"); // тут указываем ID элемента
                            var infos_rm = $(".dl-menuwrapper");
                            if (!div.is(e.target) // если клик был не по нашему блоку
                                && div.has(e.target).length === 0  // и не по его дочерним элементам
                                && !infos_rm.is(e.target)
                                && infos_rm.has(e.target).length === 0
                            ) {

                                div.hide(); // скрываем его

                                return false;
                            }
                        }
                    });
                });

           </script>


