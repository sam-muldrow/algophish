                var links = document.getElementsByTagName('a');
                for (var i = 0; i < links.length; i++) {
                    var l=links[i];
                    if (l.name.search('modal')==-1 && l.href!=null && l.href.lastIndexOf(";jsessionid=")>-1) {
                        l.href = l.href.replace(/;jsessionid=[a-zA-Z0-9_-]+/g,'');
                    }
                }
                delCookie('pearlTestEnableCookie');
            }


