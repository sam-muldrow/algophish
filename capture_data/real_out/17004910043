<script>
  var lang = document.getElementsByTagName("html")[0].getAttribute("lang");
    document.addEventListener("DOMContentLoaded", function (event) {
    var mq = window.matchMedia("(max-width: 667px)");
    var menuElement = document.getElementById("menu");
    if (menuElement) {
      menuElement.className = menuElement.className.replace(/\hidden\b/g, "");
      var mmenuOptions = {
        navbar: { title: 'ArabiaWeather',},
        extensions: [],
        navbars: {
          content: [
            "close",
          ],
          
        }
      }
      if (lang != 'ar')
        mmenuOptions.extensions.push("position-right")
      if (mq.matches)
        mmenuOptions.extensions.push("position-bottom", "fullscreen")
      new Mmenu("#menu", mmenuOptions, {});
    }
  });
    function showShareIcons(that, shareText, shareUrl) {
    shareText = decodeURIComponent(shareText);
    console.log('showShareIcons::', that, shareText, shareUrl);
    if (navigator.share) {
      navigator.share({
        title: 'طقس العرب',
        text: shareText,
        url: shareUrl
      });
    } else {
      console.log('navigator.share not supported!');
    }
  }
  
  var debugGoogleAnalyticsEvents = true;
  function trackGaEvent(eventCategory, eventAction, eventLabel) {
    /*
        google analytics event tracking
        https://developers.google.com/analytics/devguides/collection/analyticsjs/events
        ga('send', 'event', [eventCategory], [eventAction], [eventLabel], [eventValue], [fieldsObject]);
    */
    if (debugGoogleAnalyticsEvents) {
      if (navigator.userAgent.toLowerCase().indexOf('chrome') > -1) {
        console.log("%cTrack Event Fired!", 'color:purble;text-shadow:0 1px 1px #f00;font-weight:bold;font-size:16px;', 'eventCategory::', eventCategory, 'eventAction::', eventAction, 'eventLabel::', eventLabel);
      } else {
        console.log("Track Event Fired!", 'eventCategory::', eventCategory, 'eventAction::', eventAction, 'eventLabel::', eventLabel);
      }
    }
    //old tracking
    ga('send', 'event', eventCategory, eventAction, eventLabel);
    // ga4 event tracking
    gtag('event', eventCategory, {
        'eventAction': eventAction,
        'eventLabel': eventLabel
    });
  }

  </script>
