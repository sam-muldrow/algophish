<script>
    document.querySelectorAll('[data-cs-time]').forEach(function(box) {
        var localTime = new Date(box.getAttribute("data-cs-time")).toLocaleTimeString(undefined,{hour:  "2-digit",
          minute: "2-digit",
          second: "2-digit",
          hour12: true
        });
        box.querySelector("[data-cs-hours]").innerText = localTime.slice(0,2)
        box.querySelector("[data-cs-mins]").innerText = localTime.slice(3,5)
        box.querySelector("[data-cs-dorn]").innerText = localTime.slice(9,11)
    })
</script>

