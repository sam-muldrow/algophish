<script>
    function ready(fn) {
        if (document.readyState != 'loading') {
            fn();
        } else if (document.addEventListener) {
            document.addEventListener('DOMContentLoaded', fn);
        } else {
            document.attachEvent('onreadystatechange', function () {
                if (document.readyState != 'loading')
                    fn();
            });
        }
    }
    window.ready(function () {
        if (!localStorage.getItem('aw_dust_modal')) {
            openModal('meteo-modal');
            if (currentLocationId) {
                document.querySelectorAll('[dust-page-link]').forEach(function(link){
                    link.setAttribute('href','/dust?location_id='+currentLocationId)
                })
            }
            localStorage.setItem('aw_dust_modal','true');
        }
    });
</script>



