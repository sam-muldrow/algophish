    function createScriptElement(src,options) {
        var elm = document.createElement("script");
        if(options && options.defer == false){}else{elm.defer = !0;}
        if(options && options.async == true){elm.async = !0;}
        elm.type = "text/javascript";
        var b2 = "https:" == document.location.protocol;
        elm.src = src.toString();
        b2 = document.getElementsByTagName("script")[0];
        b2.parentNode.insertBefore(elm, b2);
    }
    cdDiv.classList.add('JC-WIDGET-DMROOT');
    cdDiv.setAttribute('data-widget-id','cbcc96663a573720bf1fcce836316192');
    cdParentDiv.appendChild(cdDiv);
    createScriptElement('https://static.jubnaadserve.com/api/widget.js');

