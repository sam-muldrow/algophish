<!-- The core Firebase JS SDK is always required and must be listed first -->
<script src="https://www.gstatic.com/firebasejs/8.4.2/firebase-app.js"></script>
<script src="https://www.gstatic.com/firebasejs/7.16.1/firebase-messaging.js"></script>
<script async defer>
var firebaseConfig = {
    apiKey: "AIzaSyBmAnZGewRz7uYiVX7CFSVesTjKGnIlp3E",
    projectId: "arabiaweather-main-app-4",
    messagingSenderId: "236488068453",
    appId: "1:236488068453:web:2b064d592c563d84ee258c"
};

 function SubscribeToTopics(token) {
        if (getLocal('newTopicToSubscribe') && getLocal('newTopicToSubscribe') == 'true') {
            if (get_cookie('aw_locationId') && get_cookie('aw_locationDomain')) {
                var xhttp = new XMLHttpRequest();
                var topics = ''
                topics += 'ar-location-' + getCookie('aw_locationId') + ','
                topics += 'ar-country-' + getCookie('aw_cid')+ ',';
                topics += 'ar-admin-' + getCookie('aw_aid');
                
        
                xhttp.open("POST", '/subscribe-to-topic/'+ topics , true);
                xhttp.setRequestHeader('Content-type', 'application/x-www-form-urlencoded');
                var data = 'token=' + token
                xhttp.send(data);
        
                new Promise(function (resolve, reject) {
                    xhttp.onreadystatechange = function () {
                        if (this.readyState == 4 && this.status == 200) {
                            var res = xhttp.responseText;
                        }
                    };
                })
            }
            setLocal('newTopicToSubscribe','');
        }
        
    }
var functions = 'all'
if (firebase) {
    firebase.initializeApp(firebaseConfig);
    const messaging = firebase.messaging();
    messaging
        .requestPermission()
        .then(function () {
            return messaging.getToken();
        })
        .then(function (token) {
            console.log("Token: " + token);
            if (functions == 'all')
                SubscribeToTopics(token)
        })
        .catch(function (err) {
            console.log('Unable to get permission to notify.', err);
        });
    let enableForegroundNotification = true;
    messaging.onMessage(function (payload) {
        console.log('Message received. ', payload);
        if (enableForegroundNotification) {
            var notification = payload.notification;
            navigator.serviceWorker
                .getRegistrations()
                .then((registration) => {
                    registration[0].showNotification(payload.data.title);
                    appendMessage(payload)
                });
        }
    });
}
function removeNotification(){
setTimeout(function() {
    if (document.querySelector("#subscribtion-alert"))
        document.querySelector("#subscribtion-alert").remove()
},30000)
}

function appendMessage(payload) {
    if (document.querySelector("#subscribtion-alert"))
            document.querySelector("#subscribtion-alert").remove();

    // in notification generated from cms
    var isCustom = payload.hasOwnProperty('isCustom');

    var data = payload.data;
    payload.data.id = payload.data.nid;
    var messagesElement = document.createElement('div');
    messagesElement.setAttribute('id','subscribtion-alert');
    messagesElement.classList.add('subscribtion-alert-box','p-side-p','w-100-p');

    var notificationBox =  document.createElement('div');
    notificationBox.classList.add('subscribtion-alert','bg-white','card', 'relative');


    var closeAlert =  document.createElement('img');
    closeAlert.classList.add('close-alert','pointer');
    closeAlert.setAttribute('src','https://assets.devops.arabiaweather.com/images/close-modal-revamp.svg')
    var fnOnClose = 'document.querySelector("#subscribtion-alert").remove();'

    closeAlert.setAttribute('onclick',fnOnClose)
    closeAlert.setAttribute('width','23')
    closeAlert.setAttribute('height','23')



    if (payload.data && payload.data.link && payload.data.link != '' ) {
        const link = payload.data.link;
        var anchor = document.createElement('a');
        anchor.setAttribute('href',link);
        anchor.setAttribute('onclick','trackGaEvent("notification open","notification","click");markNotificationAsRead("'+payload.data.id+'");');
        messagesElement.appendChild(anchor);
        anchor.appendChild(notificationBox);
        messagesElement.appendChild(closeAlert)
    } else {
        messagesElement.appendChild(notificationBox);
        notificationBox.setAttribute('onclick','markNotificationAsRead("'+payload.data.id+'");');
        notificationBox.appendChild(closeAlert)
    }

    var notificationBody = document.createElement('div');
    notificationBody.classList.add('body','d-flex');
    notificationBox.appendChild(notificationBody);

    var figureBox =  document.createElement('figure');
    figureBox.classList.add('relative','ml-10', 'noti-fig-img');
    notificationBody.appendChild(figureBox)
    var notificationLogo =  document.createElement('img');
    var showLogo = data.image ? 'notification-logo-img' : 'hide'
    notificationLogo.classList.add('notification-logo',showLogo)
    notificationLogo.setAttribute('src','https://assets.devops.arabiaweather.com/images/aw-alert-icon.svg')
    notificationLogo.setAttribute('width','33')
    notificationLogo.setAttribute('height','33')
    figureBox.appendChild(notificationLogo)

    var notificationImg =  document.createElement('img');
    notificationImg.classList.add('notification-img','radius-8','object-fit');
    notificationImg.setAttribute('src',data.image || '/assets/images/no-img.svg');
    notificationImg.setAttribute('width','80');
    notificationImg.setAttribute('height','80');

    figureBox.appendChild(notificationImg)

    var notificationTextBody = document.createElement('div');

    // تنبيه جديد!
    var notificationAlert = document.createElement('p');
    notificationAlert.classList.add('black','font-14','noti-title-custom');
    notificationAlert.textContent = 'تنبيه جديد!'
    notificationTextBody.appendChild(notificationAlert)
    // title
    var notificationTitle = document.createElement('p');
    notificationTitle.classList.add('bold','font-16', 'black','pb-10','font-14-p','title', 'two-line')
    notificationTitle.textContent = data.body
    notificationTextBody.appendChild(notificationTitle)

    // date & time
    data.sent_at = new Date().toISOString();
    var notificationDate = document.createElement('p');
    notificationDate.classList.add('font-11','noti-date');
    if (isCustom) {
        var dateOnly = payload.data.date;
        notificationDate.textContent = payload.data.time + " | "
        var notificationTime = document.createElement('span');
        notificationTime.textContent = dateOnly
        notificationDate.appendChild(notificationTime)
        var motificationDate = new Date(dateOnly + ' ' +payload.data.time)
        
        var FIVE_HOURS = 300 * 60 * 1000;
        
        if ((new Date() - motificationDate) > FIVE_HOURS) {
            hideCustomNotification(payload.data.id)
            return false;
        }
        
    } else {
        notificationDate.textContent = data.sent_at.slice(0,10) + " | "
        var notificationTime = document.createElement('span');
        notificationTime.textContent = new Date(data.sent_at).toLocaleTimeString('en-us',{
            hour: 'numeric',
            minute: '2-digit'
        }); 
        notificationDate.appendChild(notificationTime)

    }

    notificationTextBody.appendChild(notificationDate)
    notificationBody.appendChild(notificationTextBody)
    data.message = data.body;
    if (!isCustom)
        document.querySelector('.header-notifications-append').prepend(drawNotification(data))
    if (body.classList.contains('pushengine-page') && !isCustom)
        document.querySelector('.pushengine-append').prepend(drawNotification(data))
    // in notification generated from cms
    if (!isCustom)
        unreadNotificationCount(true)
    body.appendChild(messagesElement)
    setTimeout(function() {
        messagesElement.classList.add('active');
        removeNotification()
    },400)
}

function hideCustomNotification(nid) {
    var hideNotification = localStorage.getItem('custom_notification_read');
    if (hideNotification) {
        if (!hideNotification.includes(nid)) {
            hideNotification = hideNotification.split(',');
            hideNotification.push(nid)
            localStorage.setItem('custom_notification_read',hideNotification);
        }
    } else {
        localStorage.setItem('custom_notification_read',nid);
    }
}

// get last unread notification
function getLastNothfication(){
    var box = document.querySelectorAll('.header-notifications-append .notification-item:not(.read)')[0]
    if (box && !body.classList.contains('pushengine-page')) {
        var payload = {
            data: {
                nid: box.getAttribute('data-id'),
                date: box.querySelector('.notifi-date').textContent,
                time: box.querySelector('.notifi-time').textContent,
                link: box.querySelector('a.trans') && box.querySelector('a.trans').getAttribute('href') || '',
                image: box.querySelector('figure img').getAttribute('src').includes('no-img') ? '' : box.querySelector('figure img').getAttribute('src'),
                body: box.querySelector('.title').innerHTML || ''
            },
            isCustom: true
        }
        userCloseItAlready = false;
        if (localStorage.getItem('custom_notification_read') && localStorage.getItem('custom_notification_read').includes(payload.data.nid)) 
            userCloseItAlready = true;
        
        if (!userCloseItAlready) {
            setTimeout(function() {
                appendMessage(payload);
                hideCustomNotification(payload.data.nid)
            },1000);
        }
    }
}
</script>
<script async defer src="https://assets.devops.arabiaweather.com/js/pushengine.js?v=2.7.9"></script>
</body>

