    window.addEventListener ? 
    window.addEventListener("load",initCouponsSlider,false) : 
    window.attachEvent && window.attachEvent("onload",initCouponsSlider);

    function initCouponsSlider(){
        console.log('initCouponsSlider');
        window.couponsSlider = new Glide('#coupon-cards', {
            type: 'carousel',
            direction: 'rtl',
            startAt: 0,
            perView: 1,
            autoplay: 6000
        }).mount();
    }

    function copyCouponText(that,text){
        navigator.clipboard.writeText(text);
        that.style.backgroundColor = '#4caf50';
        that.innerText = 'تم النسخ';
        setTimeout(function(){
            that.style.backgroundColor = '#476782';
            that.innerText = 'نسخ الكوبون';
        },2000);
    }

