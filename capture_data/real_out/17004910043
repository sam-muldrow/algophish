<script>
    function getVideoSlide(arrow){
        var selectorNum = !body.classList.contains('livepage') ? 1 : 0;
        if (arrow == 'next') {
            document.querySelectorAll('.videos-slider-do-body')[selectorNum].classList.add('active');
            document.querySelectorAll('.v-arrow-prev')[selectorNum].setAttribute('src','/assets/images/arrow-video-active.svg');
            document.querySelectorAll('.v-arrow-next')[selectorNum].setAttribute('src','/assets/images/arrow-video.svg');
        } else {
            document.querySelectorAll('.videos-slider-do-body')[selectorNum].classList.remove('active')
            document.querySelectorAll('.v-arrow-next')[selectorNum].setAttribute('src','/assets/images/arrow-video-active.svg');
            document.querySelectorAll('.v-arrow-prev')[selectorNum].setAttribute('src','/assets/images/arrow-video.svg');
        }
    }
</script>    </div>
        


