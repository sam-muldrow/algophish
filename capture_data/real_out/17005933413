    <script type="text/javascript">
    window.dctk = window.dctk || {};

    (function (window, logging) {
        logging.clientProductionDomain = ['https://www.expediapartnercentral.com'];
        logging.pageStartTime = new Date().getTime();
        logging.pageName = "epc-login-web-dw.Partner Central Login";
        logging.applicationName = "epc-login-web-dw";
        logging.guid = "84eeeacf-e2d8-442f-846b-6710e65b6cac";
        logging.serverEnvironment = "us-west-2";
        logging.requestId = "84eeeacf-e2d8-442f-846b-6710e65b6cac";

        logging.data = logging.data ? (logging.data + "|city=" + "BURLINGTON") : ("city=" + "BURLINGTON");
        logging.data = logging.data + "|country=" + "US";
        logging.data = logging.data + "|throughput=" + "vhigh";
        logging.data = logging.data + "|bandwidth=" + "5000";

        // add htid in client logging
        try {
            var htid;
            if (location.search) {
                var regex = new RegExp("[\\?&]htid=([^&#]*)"), results = regex.exec(location.search);
                if (results) {
                    htid = decodeURIComponent(results[1].replace(/\+/g, " "));
                }
            }
            if (htid) {
                logging.data = logging.data ? (logging.data + "|htid=" + htid) : ("htid=" + htid);
            }
        } catch(ex) {
            // do nothing, so it doesn't impact client logging features.
        }
    })(window, window.dctk.logging = window.dctk.logging || {});
</script>




    
