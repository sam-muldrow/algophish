  window['klz'].debug_async_step.push('PAGE_LOAD-INCLUDE_STATIC_META');

  window['klz'].epaper = {
    firstIssueDate: new Date(2005, 0, 1),
  };

  window['klz'].notification = {
    list: [],
    notify: (msg, props = null) => {
      if(window['klz'].notification.hasOwnProperty('list')) {
        window['klz'].notification.list.push({ msg, props });
      } else {
        window.dispatchEvent(new CustomEvent('EVENT:NOTIFICATION', { "detail": { msg, props } }));
      }
    }
  }

  const _klzfe = localStorage.getItem('klz-fe');

  if (_klzfe) {
    const klzfe = JSON.parse(_klzfe);

    if (klzfe) {
      window['klz'].user = klzfe.user;
      window['klz'].region = klzfe.region;
    }
  }
</script>
