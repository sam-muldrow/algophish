  <script>
    function allowPianoCookie() {
      if (window.__tcfapi) {
        window.__tcfapi('accept', 2, (data, result) => {
        }, {
          // 10115 Piano
          vendorIds: [10115],
          purposeIds: [1,2,7,8,9,10]
        });

        document.getElementById("modal--consent").classList.add("hidden");

        window.dataLayer.push({
          event: 'gaEvent',
          eventCategory: 'Piano Banner::Login not possible',
          eventAction: 'Click to accept Cookie',
          eventLabel: 'Rejected Cookie Piano Software Inc.'
        });

        window['tp']['pianoId'].show('login');
      }
    }
  </script>
