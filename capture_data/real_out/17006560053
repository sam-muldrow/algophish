	<script>
  function sendHit(eventName) {
    hitEvent = '[{"name":"gdpr_liveramp","data":{"action":"'+eventName+'"}}]';
    hitUrl = encodeURI('https://logs128.xiti.com/event?s=627035&idclient='+Math.random()*1000000000000000000+'&events='+hitEvent);
    fetch(hitUrl)
      .then((response) => response.blob());
  }
  liverampEvents = ['isLoaded','commandQueued','consentToolShouldBeShown','consentChanged','auditIdChanged','acceptAllButtonClicked','denyAllButtonClicked','saveAndExitButtonClicked','exitButtonClicked','consentManagerDisplayed','consentManagerClosed','consentManagerAlreadyShown','consentNoticeClosed','consentNoticeAlreadyShown','noGdprApplies','dauLogSent'];
  for (let i = 0; i < liverampEvents.length; i++) {
    __tcfapi('addEventListener', 2, (tcData, success) => {sendHit(tcData.event)}, liverampEvents[i]);
  }
</script>

