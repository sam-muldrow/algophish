    <script>
  const cookieDomain = '.kleinezeitung.at';

  const setCookie = (name, value, expires) => {
    if (expires === undefined) {
      document.cookie = `${name}=${value}; path=/;`;
    } else {
      let date = new Date(expires * 1000);
      document.cookie = `${name}=${value}; expires=${date.toString()}; path=/;`;
    }
  }

  const setCookieLax = (name, value, expires) => {
    let cookieString = `${name}=${value};SameSite=None;Secure;domain=${cookieDomain};path=/;`; //  domain=.stage.cue.kleinezeitung.at; HostOnly=false;

    if (expires === undefined) {
      document.cookie = cookieString;
    } else {
      let date = new Date(expires * 1000);

      cookieString = `${cookieString} expires=${date.toString()};`;
      document.cookie = cookieString;
    }
  }

  const deleteCookie = (name, withDomain = false) => {
    const domainString = withDomain ? `domain=${cookieDomain};Secure;SameSite=None;` :'';

    document.cookie = `${name}=;${domainString}expires=Thu, 01 Jan 1970 00:00:00 UTC;path=/;`;
  }

  const deleteCookieLax = (name) => {
    document.cookie = `${name}=;domain=${cookieDomain};Secure;SameSite=None;expires=Thu, 01 Jan 1970 00:00:00 UTC;path=/;`;
  }

  const getCookie = (name) => {
    return document.cookie.match("(^|;)\\s*" + name + "\\s*=\\s*([^;]+)")?.pop() || null;
  };

  var UTIL = {
    env: "prod",

    loaded: {
      chartbeat: false,
    },

    data: {
      pv: "",
      isBot: null,
    },

    api: {
      middleware: "https://api.kleinezeitung.at/gateway",
      gateway: " https://api.kleinezeitung.at/gateway",
    },

    comments: {
      instance_key: "klz",
      coralRootUrl: "https://comments.klz.getoctopus.com",
      coralScriptSource: "https://comments.klz.getoctopus.com/assets/js/embed.js",
      countLimit: "3",
    },

    selfservice: {
      url: "https://service.kleinezeitung.at",
    },
  };

  UTIL.data.pv = parseInt(getCookie("klz.pv") || "1");
  UTIL.data.isBot =
    /Chrome\-Lighthouse|Google Page Speed Insights|googlebot|crawler|spider|robot|crawling|Taboolabot/i.test(
      navigator.userAgent
    );

  UTIL.loadScript = function (
    arg = {
      src: "",
      async: true,
      location: "",
      onLoad: undefined,
      onError: undefined,
      defer: false,
      fetchPriority: "low",
      attributes: null,
    }
  ) {
    const script = document.createElement("script");
    script.onload = arg.onLoad;
    script.onerror = arg.onError;
    script.src = arg.src;
    script.async = arg.async || true;
    script.defer = arg.defer || false;
    script.fetchPriority = arg.fetchPriority || "auto";

    if(typeof arg.attributes === 'object') {
      for(let key in arg.attributes) {
        if(typeof arg.attributes[key] === 'string') {
          script.setAttribute(key, arg.attributes[key]);
        }
      }
    }

    if (arg.location) {
      arg.location.parentNode.replaceChild(script, arg.location);
    } else {
      document.head.appendChild(script);
    }
  };

  UTIL.loadScriptPromise = function(args) {
    return new Promise((resolve, reject) => {
      args.onLoad = resolve;
      args.onError = reject;
      UTIL.loadScript(args);
    })
  }

  UTIL.waitForDomElement = function (selector) {
    return new Promise((resolve) => {
      if (document.querySelector(selector)) {
        return resolve(document.querySelector(selector));
      }

      const observer = new MutationObserver((mutations) => {
        if (document.querySelector(selector)) {
          resolve(document.querySelector(selector));
          observer.disconnect();
        }
      });

      observer.observe(document.body, {
        childList: true,
        subtree: true,
      });
    });
  };
</script>

