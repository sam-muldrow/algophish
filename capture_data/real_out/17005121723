<script type="text/javascript">
	
	function packetSwitch(elem1,elem2){
		$(elem1).hide();
		$(elem2).show();
	}
	
	var initInterval = setInterval(function(){
        if(window.jQuery) {
            clearInterval(initInterval);
            init(); // where init is the entry point for your code execution
        }
    }, 20);

	function init(){
		if($(window).width() > 991){
			$("#domain-text").focus();
		}
		
		setInterval(function(){
			$("#carousel-tldlist").carousel("next");
		},14000);
		
		$(".product-list-container .product").hover(function () {
			$(this).addClass("white");
			$(this).find("button").addClass("btn-order").removeClass("btn-review")
			$(this).find(".discount").removeClass("green").addClass("orange");
		});

		$(".product-list-container .product").mouseleave(function () {
			$(this).removeClass("white");
			$(this).find("button").addClass("btn-review").removeClass("btn-order")
			$(this).find(".discount").removeClass("orange").addClass("green");
		});


		$("[data-tagmanager='true']").on("click",function(){
		let formId = $(this).attr("data-formid")	
		var data = {
			'event': $(this).attr("data-event"),
			'ecommerce': {},
			'eventCallback': function(event) {
				document.forms[formId].submit();
			}
		};

		var commerceData = {
			'actionField': {'list': $(this).attr("data-list")},
			'products': [{
				'name': $(this).attr("data-name") != "" ? $(this).attr("data-name") : "",
				'id': $(this).attr("data-id") != "" ? $(this).attr("data-id") : "",
				'price': $(this).attr("data-price") != "" ? $(this).attr("data-price") : "",
				'brand': 'Natro',
				'category': $(this).attr("data-category") != "" ? $(this).attr("data-category") : "",
				'list': $(this).attr("data-list") != "" ? $(this).attr("data-list") : "",
				'position': $(this).attr("data-position") !="" ? $(this).attr("data-position") : ""
			}]
		};

		var dataKey = $(this).attr("data-type");
		data.ecommerce[dataKey] = commerceData;
		dataLayer.push(data);
		}); 
	}

</script> 
