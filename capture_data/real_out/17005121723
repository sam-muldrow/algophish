<script>
	$(function(){
		$(window).resize(function() {
			if(this.resizeTO) clearTimeout(this.resizeTO);
			this.resizeTO = setTimeout(function() {
				$(this).trigger('resizeEnd');
				var win = $(this);
				if($(window).width() > 768){
					$(".footer-link-wrapper .fa.hd").removeClass("fa-plus").addClass("fa-angle-down");
					$(".footer-link-wrapper .fa.hd").removeClass("fa-minus").addClass("fa-angle-down");
					$(".footer-link-wrapper .footer-link>ul>li").show();	
				}else{
					//$(".footer-link-wrapper .footer-link>ul>li").hide();
					
						$("span[data-footer-content]").on("click",function(){
							if($(window).width() < 768){
								$(".footer-link-wrapper .footer-link:not(.about)>ul>li:not(.footer-sub-header)").hide();
								$("[data-footer-ul-content='"+ $(this).attr("data-footer-content") +"'] li").show();
							}
						});
					
				}
			}, 100);
		});
		
		if($(window).width() < 768){
			$(".footer-link-wrapper .fa.hd").removeClass("fa-angle-down").addClass("fa-plus");
			$("[data-footer-content]").on("click",function(){
				if($(this).find(".fa.hd").hasClass("fa-plus")){
					$(".footer-link-wrapper .fa").removeClass("fa-minus").addClass("fa-plus");
					$(this).find(".fa.hd").removeClass("fa-plus").addClass("fa-minus");
					$(".footer-link-wrapper .footer-link:not(.about)>ul>li:not(.footer-sub-header)").hide();
					$("[data-footer-ul-content='"+ $(this).attr("data-footer-content") +"'] li").show();
				}else{
					$(".footer-link-wrapper .fa.hd").removeClass("fa-minus").addClass("fa-plus");
					$("[data-footer-ul-content='"+ $(this).attr("data-footer-content") +"'] li").hide();
				}
				
			});
		}else{
			$(".footer-link-wrapper .fa.hd").removeClass("fa-plus").addClass("fa-angle-down");
			$(".footer-link-wrapper .fa.hd").removeClass("fa-minus").addClass("fa-angle-down");
			$(".footer-link-wrapper .footer-link>ul>li").show();
		}
		
		
		const mouseEvent = e => {
			if (!e.toElement && !e.relatedTarget) {
				document.removeEventListener('mouseout', mouseEvent);
				if (typeof $.cookie("isHeraldOfOpportunity") === 'undefined') {
					$('#bsaModal').modal('show');
					var date = new Date();
					var minutes = 2160;
					date.setTime(date.getTime() + (minutes * 60 * 1000));
					$.cookie("isHeraldOfOpportunity", "1", {expires: date})
				}
			}
		};
		
		document.addEventListener('mouseout', mouseEvent);
		
		let popUpStatus = true;
		document.addEventListener("scroll", function(){
			popUpStatus = false
		}, false);	
		
		document.addEventListener("click", function(){
			popUpStatus = false
		}, false);
		
		document.addEventListener("keydown", function(){
			popUpStatus = false
		}, false);
		
		setInterval(function(){
			if(popUpStatus){
				if (typeof $.cookie("isHeraldOfOpportunity") === 'undefined') {
					$('#bsaModal').modal('show');
					var date = new Date();
					var minutes = 2160;
					date.setTime(date.getTime() + (minutes * 60 * 1000));
					$.cookie("isHeraldOfOpportunity", "1", {expires: date})
				}
			}
		},60000);
		
		
		
	});
	
	$(function(){
		var lastScrollTop = 0;
		$(window).scroll(function (event) {
			var st = $(this).scrollTop();
			if (st > lastScrollTop) {
			
				if($(window).width() < 767){
					$('.navbar-default').css("top", "-40px");
					$('body').css("padding-top", "-40px");
				}else{
					$('.navbar-default').css("top", "-40px");
					$('body').css("padding-top", "-50px");
				}
				
			} else {
				$('.navbar-default').css("top", "0px !important");
			}
			if (lastScrollTop < 0) {
				$('.navbar-default').css("top", "");
			}
			lastScrollTop = st;
		});
	});
</script>


