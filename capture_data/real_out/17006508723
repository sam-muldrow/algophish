<a href="javascript:;" class="btn show-results">Резултати</a>
</div>
</form>
</div>
<div class="results"></div>
</div>
<script>
$( document ).ready(function() {
    $('#homePollForm').submit(function() {
		var pollid = $(this).data('id'),
			answerValue = $("#homePollForm input[name='answer']:checked").val();
		
		if ( ! $("#homePollForm input.answerInput").is(':checked') ) {
			alert('Моля, изберете вашия отговор!')
		} else {
			$.get( "/polls/vote", { poll: pollid, answer: answerValue  }, function( data ) {
				$("#home-poll .form").slideUp(200, function() {
					$('#home-poll .results').html(data);
					$("#home-poll .results").slideDown(200);
					
				});
			});
		}
		
		return false;
		
	});
	
	$(document).delegate("#home-poll .show-results", 'click', function() {
		var pollid = $('#homePollForm').data('id');
		
		if ($('#home-poll .results').html() == '') {
			$.get( "/polls/results", { pid: pollid }, function( data ) {
				$("#home-poll .form").slideUp(200, function() {
					$('#home-poll .results').html(data);
					$("#home-poll .results").slideDown(200);
				});
			});
		} else {
			$("#home-poll .form").slideUp(200, function() {
				$("#home-poll .results").slideDown(200);
			});
		} 
	});
	
	$(document).delegate("#home-poll .poll-back", 'click', function() {
		$("#home-poll .results").slideUp(200, function() {
				$("#home-poll .form").slideDown(200);
			});
	});
});
</script>
