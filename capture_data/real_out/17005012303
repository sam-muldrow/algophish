                    var mapTosync = new Map();
                    var didomiLoaded = false;



                    const getGtmVendorStatus = function() {
                        return {
                            'googleAnalytics': Didomi.getUserConsentStatusForVendor('google') , 
                            'Facebook': Didomi.getUserConsentStatusForVendor('facebook') , 
