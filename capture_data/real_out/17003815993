        function replace_special_symbol(str) {
          str=str.replace(/&#039;/g, "'");
          str = str.replace(/['|,|&|@|%|;|+|=|#|!|^|*|(|)|-]/gi, "").replace(/  +/g, ' ');
          str = str.substring(0,40);
          return str;
        }
