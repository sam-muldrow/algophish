  @media(max-width: 1000px){.spar-w .swiper-button-prev, .spar-w .swiper-button-next{display:none;}.spar-w{padding: 0 0px;}.spar-w .swiper-pagination{display:none} .spar-w .custom-nav{display:none}} .swiper-spar a{border-radius: 10px;overflow:hidden;}
