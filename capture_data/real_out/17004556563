<script type="text/javascript">let hkSlideIndex=0;$(document).ready(function(){});function showProduct(productName,elmnt){let tabContent,tabLinks;tabContent=document.getElementsByClassName("tab-content");for(let i=0;i<tabContent.length;i++){tabContent[i].style.display="none";}tabLinks=document.getElementsByClassName("tab-link");for(i=0;i<tabLinks.length;i++){tabLinks[i].style.borderBottom="none";tabLinks[i].style.color="#757575";}document.getElementById(productName).style.display="block";elmnt.style.borderBottom="3px solid #5490eb";elmnt.style.color="#5490eb";stopAnimation=true;}function callHkSlider(){$(".phoneScreenHK").attr("src",$(".hkAppFeature")[hkSlideIndex].dataset.img);hkSlideIndex++;hkSlideIndex=hkSlideIndex%4;}let productIndex=1;showProdDiv(productIndex);$(document).ready(function(){setInterval(function(){if(!stopAnimation){showProdDiv(productIndex+=1);}},6000);});function currentProdDiv(n){showProdDiv(productIndex=n);stopAnimation=true;}function showProdDiv(n){let x=document.getElementsByClassName("tab-content");let dots=document.getElementsByClassName("demo-product");let tabLinks=document.getElementsByClassName("tab-link");for(i=0;i<tabLinks.length;i++){tabLinks[i].style.borderBottom="none";tabLinks[i].style.color="#757575";}if(n>x.length){productIndex=1}if(n<1){productIndex=x.length}for(let i=0;i<x.length;i++){x[i].style.display="none";}for(i=0;i<dots.length;i++){dots[i].className=dots[i].className.replace(" badge-primary","");}x[productIndex-1].style.display="block";dots[productIndex-1].className+=" badge-primary";tabLinks[productIndex-1].style.borderBottom="3px solid #5490eb";tabLinks[productIndex-1].style.color="#5490eb";}</script>

