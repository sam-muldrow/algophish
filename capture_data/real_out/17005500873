   <script>
      const mCrawl = document.getElementById('m-mCrawl-line');
      const mCrowlArrea = ['повысить трафик основного сайта', 'создать новый канал продаж', 'забрать покупателей у конкурентов'];
      mWriteText(mCrowlArrea);
      function mWriteText(mCrowlArrea) {
         let mSpeedWrite = 60;
         let mSpeedRemove = 20;
         let mCrowlArreaIndex = 0;
         var mPos = 0;
         var mWrite;
         var mRemove;

         mWrite = function() {
            mCrawl.innerHTML += mCrowlArrea[mCrowlArreaIndex][mPos++];
            if (mPos < mCrowlArrea[mCrowlArreaIndex].length) {
               setTimeout(mWrite, mSpeedWrite + Math.random() * 20);
            } else {
               setTimeout(mRemove, 1000);
            }
         };

         mRemove = function() {
            mCrawl.innerHTML = mCrawl.innerHTML.substring(0, --mPos);
            if(mPos > 0) {
               setTimeout(mRemove, mSpeedRemove);
            } else if(mCrowlArrea.length - 1 > mCrowlArreaIndex) {
               mCrowlArreaIndex++;
               setTimeout(mWrite, mSpeedWrite + Math.random() * 20);
            } else {
               mCrowlArreaIndex = 0;
               setTimeout(mWrite, mSpeedWrite + Math.random() * 20);
            }
         }
         setTimeout(mWrite, mSpeedWrite + Math.random() * 20);
      }
   </script>
