    <script>
        (function () {
            window.addEventListener('load', function () {
                document.querySelectorAll('script[defer-fetch], link[defer-fetch]').forEach(function ($elem) {
                    if ($elem.getAttribute('data-src')) {
                        $elem.setAttribute('src', $elem.getAttribute('data-src'));
                    }
                    if ($elem.getAttribute('data-href')) {
                        $elem.setAttribute('href', $elem.getAttribute('data-href'));
                    }
                });
            });
        })();

        function getOffsetTop($elem) {
            if (!$elem.offsetTop) {
                return getOffsetTop($elem.parentElement);
            } else {
                return $elem.offsetTop;
            }
        }

        function loadEmbeds($embeds, fn) {
            $embeds.forEach(function ($embed) {
                const observer = new IntersectionObserver((entries) => {
                    entries.forEach((entry) => {
                        if (entry.intersectionRatio > 0) {
                            if (fn) {
                                fn($embed);
                            }
                            $embed.fetched = true;
                            $embed.src = $embed.getAttribute('data-src') || '';

                            observer.unobserve(entry.target);
                        }
                    });
                }, {
                    root: document.body,
                    rootMargin: `50%`,
                    threshold: 0
                });
                observer.observe($embed);
            });
        }
    </script>
            
