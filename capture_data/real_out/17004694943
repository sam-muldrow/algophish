<script>

    document.addEventListener("touchstart", function(e) {
        console.log(e.defaultPrevented);  // will be false
        e.preventDefault();   // does nothing since the listener is passive
        console.log(e.defaultPrevented);  // still false
    }, {passive: true});
    // Passive event listeners
    jQuery.event.special.touchstart = {
        setup: function( _, ns, handle ) {
            this.addEventListener("touchstart", handle, { passive: !ns.includes("noPreventDefault") });
        }
    };
    jQuery.event.special.touchmove = {
        setup: function( _, ns, handle ) {
            this.addEventListener("touchmove", handle, { passive: !ns.includes("noPreventDefault") });
        }
    };
</script>

<script>

    loadEmbeds(document.querySelectorAll('[observable-embed]'));

</script>

<script>
    (function () {

        let related = [];
        document.querySelectorAll('ul li a[data-type="post"]').forEach(function ($post) {

            related.push($post.getAttribute('data-id'));

        });


        fetch('/wp-json/wp/v2/posts/?_embed&include=' + related.join(',')).then((response) => {

            return response.json();

        }).then((data) => {

            data.forEach(function (post) {

                console.log({
                    title: post.title.rendered,
                    link: post.link,
                    image: post._embedded['wp:featuredmedia'][0].source_url
                });

            });

        });

    });
</script>
<script>
window.addEventListener("load", () => {
    if( 'function' == typeof Adjacent ){
        gtag('event', 'whitelisted', { 'event_category': 'NTG adblock', 'event_label': document.location.href, 'send_to': 'G-ESXBCCCS5Y'});
    } else {
        gtag('event', 'detected', { 'event_category': 'NTG adblock', 'event_label': document.location.href, 'send_to': 'G-ESXBCCCS5Y'});
    }
});

( function($){
    $('.dropdown-menu .btn').click( (evt) => {
        let datoBoton = $(evt.target).data(), redSocial = false;

        if( 'undefined' != typeof datoBoton['href'] ){
            let url = datoBoton['href'];

            if( url.match(/facebook\.com/) ){ redSocial = 'facebook'; }
            if( url.match(/twitter\.com/) ){ redSocial = 'twitter'; }
        }

        if( false != redSocial ){
            gtag('event', 'social share', { 'event_category': 'NTG social', 'event_label': redSocial, 'non_interaction': false, 'send_to': 'G-ESXBCCCS5Y'});
        }
        
        evt.preventDefault();
    } );
})(jQuery);
</script>

