  <div class="product-group-slider slider home-teaser-items"  data-equalizer data-slider-page="home">

    

                      <div class="masonry-box grid-sizer object-group-overview  ">
          <div class="masonry-box-container">
            <a href="https://www.architonic.com/en/products/sound-absorption/0/3221466/1"
               title="Selected products of Sound absorption"
               class="prediction-item"
                data-item-id="3221466" data-log-type="log_click_group"             >
              <div class="group-box-teaser row">
                <div class="group-box-teaser-container">
                  <div class="group-box-teaser-overlay"></div>
                  <div class="object-group-box-teaser-content">
                    <div class="object-group-box-pictures">
                      <img data-pin-no-hover='true' class="lazyload"
