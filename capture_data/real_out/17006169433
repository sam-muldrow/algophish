    <script>
        function loginform() {
            $("#loginbox").removeClass("displayno");
            $("#loginbox").addClass("displayyes");
            $('#frmpopupregion').load('/LoginFromMember')
        }
        function createform() {
            $("#loginbox").removeClass("displayno");
            $("#loginbox").addClass("displayyes");
            $('#frmpopupregion').load('/CreateFromMember')
        }
        function commentlogin() {
            $("#loginbox").addClass("displayno");
            $("#loginbox").removeClass("displayyes");
            $.get('/CheckLogin/', function (data) {
                var nameid = data.split(',');
                $('#txtFullname').val(nameid[0]);
                $('#txtUserid').val(nameid[1]);
                $('#txtEmail').val(nameid[2]);
                $('#idFullNameCmt').html("<div style=\"color:#163a0c\"><b>" + nameid[0] + "</b>");
                $('#anonymousid').addClass("displayno");
                $('#loginmember').addClass("displayno");
            });
            $.get('/LoginPartialLoad/', function (data) {
                $('#LoginPartialBox').html(data);
            });
            comtnotlogin = 0;
        }
        function commentnologin() {
            $("#loginbox").addClass("displayno");
            $("#loginbox").removeClass("displayyes");
            $("#anonymousid").removeClass();
            $("#anonymousid").addClass("row displayyes");
            $("#loginmember").removeClass();
            $("#loginmember").addClass("displayno");
            var cmtid = oldcmtid;
            htmlreplybox = "<div class=\"comm-msg\" id =\"reply" + cmtid + "\">" +
                "<textarea id=\"txtcomment" + cmtid + "\" class=\"itxt\" maxlength=\"500\" placeholder=\"Nhập nội dung trả lời bằng tiếng Việt có dấu\" ></textarea >" +
                "</div>" +
                "<div class=\"displayyes\" id=\"anonymousid\">" +
                "<div class=\"comm-reg\">" +
                "<input id=\"inputFullname" + cmtid + "\" class=\"comm-txt\" type=\"text\" placeholder=\"Họ và tên\" />" +
                "<input id=\"inputEmail" + cmtid + "\" class=\"comm-txt\" type=\"email\" placeholder=\"Địa chỉ email\" />" +
                "<div class=\"clx\"></div>" +
                    "</div>" +
                    "</div>" +
                "<button class=\"comm-btn\" onclick=\"combtn_click(" + cmtid + "," + parentid +  ")\">Gửi trả lời</button>";
            $('#reply' + cmtid).html(htmlreplybox);
            comtnotlogin = 1;
        }

        if (window.addEventListener) {
            window.addEventListener("message", onMessage, false);
        }
        else if (window.attachEvent) {
            window.attachEvent("onmessage", onMessage, false);
        }
        function onMessage(event) {
            if (event.origin !== "https://member.bongdaplus.vn") return;
            var data = event.data;
            if (typeof (window[data.func]) == "function") {
                window[data.func].call(null, data.message);
            }
        }
        $.get('/LoginPartialLoad/', function (data) {
            $('#LoginPartialBox').html(data);
        });
        console.log("end");
    </script>

    
