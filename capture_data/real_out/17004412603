vigator.permissions.query({\r\n      name: \"clipboard-write\",\r\n    });\r\n    if (result.state === \"granted\" || result.state === \"prompt\") {\r\n      // Copy to the clipboard\r\n      navigator.clipboard.writeText(discountCode);\r\n    } else {\r\n      // force catch any unexpected responses\r\n      throw new Error(\"Result.state is:\", result.state);\r\n    }\r\n  } catch {\r\n    // Fallbacks for Firefox, Safari and older browsers\r\n    // Create and add a read only, offscreen, textarea element with code as value\r\n    const element = document.createElement(\"textarea\");\r\n    element.value = discountCode;\r\n    element.setAttribute(\"readonly\", \"\");\r\n    element.style.position = \"absolute\";\r\n    element.style.left = \"-9999px\";\r\n    document.body.append(element);\r\n    // Check if there is any content selected previously\r\n    const selected =\r\n      document.getSelection().rangeCount > 0\r\n        ? document.getSelection().getRangeAt(0) // Store selection if found\r\n        : false; // Mark as false to know no selection existed before\r\n    element.select(); // Select the <textarea> content\r\n    document.execCommand(\"copy\"); // Copy on user action (i.e. click events)\r\n    element.remove(); // Remove the <textarea> element\r\n    if (selected) {\r\n      // If a selection existed before copying restor selection\r\n      document.getSelection().removeAllRanges();\r\n      document.getSelection().addRange(selected);\r\n    }\r\n  }\r\n  // Update button element styles and content\r\n  event.target.classList.add(\"active\");\r\n  event.target.textContent = \"copied!\";\r\n}\r\n\r\n/*******************\r\n * Event Handlers\r\n *******************/\r\nfunction handleCopyClick(event) {\r\n  if (!Object.hasOwn(event.target.dataset, \"discountButton\")) return;\r\n  event.preventDefault();\r\n  const discountCode = getCode(event);\r\n  copyCode(event, discountCode);\r\n}\r\n\r\n/************\r\n * INITS\r\n ************/\r\ndocument.addEventListener(\"click\", handleCopyClick);\r\n","id":"7b7a844a-37b2-427a-b1f6-e87f954638b6"}]}]}]},{"type":"boots/components/content/experiencefragment","order":["root"],"fragmentVariationPath":"/content/experience-fragments/boots/homepage/_2023_2024/_3_november/wc_0611/7--wc0611_additional25s/master","rowCount":1,"editorError":false,"editorMessage":"","id":"7e49b1fd-6ef8-46f3-83be-ec122183dd9e","fragments":[{"type":"boots/components/content/grid","order":["section_1355781824_c"],"additionalMargins":false,"fullWidth":false,"gridClassNames":"aem-Grid aem-Grid--12 aem-Grid--default--12","columnClassNames":{"section_1355781824_c":"aem-GridColumn aem-GridColumn--default--12"},"columnCount":12,"id":"fb863b3a-0006-4cd5-86a2-08fd755849e8","fragments":[{"type":"boots/components/content/grid/section","order":["text_copy","teaser_copy_copy","teaser_copy_copy_498773493","teaser_copy_copy_306599422","teaser_copy_copy_721_765140603"],"sectionInfo":{"sideSpacing":true,"verticalSpacing":false,"extraSpacing":false,"border":false,"borderColor":null,"backgroundColor":null,"backgroundImage":""},"fullWidth":true,"gridClassNames":"aem-Grid aem-Grid--12 aem-Grid--default--12 aem-Grid--phone--12","columnClassNames":{"teaser_copy_copy_306599422":"aem-GridColumn aem-GridColumn--default--3 aem-GridColumn--offset--default--0 aem-GridColumn--default--none aem-GridColumn--phone--6 aem-GridColumn--offset--phone--0 aem-GridColumn--phone--none","teaser_copy_copy":"aem-GridColumn aem-GridColumn--default--3 aem-GridColumn--offset--default--0 aem-GridColumn--default--none aem-GridColumn--phone--6 aem-GridColumn--offset--phone--0 aem-GridColumn--phone--none","text_copy":"aem-GridColumn aem-GridColumn--default--12","teaser_copy_copy_721_765140603":"aem-GridColumn aem-GridColumn--default--3 aem-GridColumn--offset--default--0 aem-GridColumn--default--none aem-GridColumn--phone--6 aem-GridColumn--offset--phone--0 aem-GridColumn--phone--none","teaser_copy_copy_498773493":"aem-GridColumn aem-GridColumn