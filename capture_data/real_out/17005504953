<script>
    $(function() {
        /*=-login交互------=*/
        $(document).on('click', '.btn_showBigL', function(event){
            event.preventDefault();
            $(".loginStart .loginSmall").hide();
            $(".loginStart .loginBig").show()
        });
        $(document).on('click', '.btn_openBigL', function(event) {
            event.preventDefault();
            $(".loginAfter .loginSmall").hide();
            $(".loginAfter .loginBig").show();
        });
        $(document).on('click', '.btn_hideBigL', function(event) {
            event.preventDefault();
            $(".loginSmall").show();
            $(".loginBig").hide()
        });
        //记住用户名下拉
        $(".lable_reord").dropdown(".tips_warn_auto","100");
        //更多玩过的下拉
        $(".btn_morePlayed").dropdown(".morePlaygame","600");
        /*=------login交互*/

        //开服和新服列表
        $(".u-serverNav li").tab(".u-serverLst .lst",'mouseenter');

    });

    var min = 1;

    // 换一批
    $('.m-boxLeft').on('click', '.more', function(){
        var len = $('.ulist').children('ul').length;
        if (min >= len) {
            min = 0;
        }
        $('.ulist').children('ul').eq(min).show().siblings().hide();
        min++;
    });

    $(document).ready(function () {
        $('.searchTopBtn').click(function () {
            var value = $('.i-sch').val();
            window.location = 'https://www.youzu.com/tag/' + value;
        });
    });
    $('.CG_addFav').live('click', function () {
        homepage = location.href;
        sitename = location.host;
        try {
            window.external.addFavorite(homepage, sitename);
        } catch (e) {
            try {
                window.sidebar.addPanel(sitename, homepage, '');
            }
            catch (e) {
                alert('加入收藏失败,请手动添加。');
            }
        }
    });

</script>
