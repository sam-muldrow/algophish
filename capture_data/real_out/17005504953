        function browserRedirect(mburl, pcurl) {
            var current_url = window.location.href;
            var sUserAgent = navigator.userAgent.toLowerCase();
            //百度谷歌爬虫不做跳转
            // if(sUserAgent.indexOf(/googlebot/i) > -1 || sUserAgent.indexOf(/baiduspider/i) > -1){
            //     return false;
            // }
            // //该页面不做跳转
            // if(current_url.indexOf('/about/services.html') > -1 || current_url.indexOf('/about/privacy.html') > -1){
            //     return false;
            // }


            var bIsIpad = sUserAgent.match(/ipad/i) == "ipad";
            var bIsIphoneOs = sUserAgent.match(/iphone os/i) == "iphone os";
            var bIsMidp = sUserAgent.match(/midp/i) == "midp";
            var bIsUc7 = sUserAgent.match(/rv:1.2.3.4/i) == "rv:1.2.3.4";
            var bIsUc = sUserAgent.match(/ucweb/i) == "ucweb";
            var bIsAndroid = sUserAgent.match(/android/i) == "android";
            var bIsCE = sUserAgent.match(/windows ce/i) == "windows ce";
            var bIsWM = sUserAgent.match(/windows mobile/i) == "windows mobile";
            if (bIsIpad || bIsIphoneOs || bIsMidp || bIsUc7 || bIsUc || bIsAndroid || bIsCE || bIsWM) {
                if (current_url.indexOf('m.youzu') > -1){
                    return false;
                }
                window.location.href = mburl;

                if(current_url.indexOf('/about/services.html') > -1){
                    window.location.href = '//m.youzu.com/about/services.html';
                }

                if(current_url.indexOf('/about/privacy.html') > -1){
                    window.location.href = '//m.youzu.com/about/privacy.html';
                }

                if(current_url.indexOf('/about/child.html') > -1){
                    window.location.href = '//m.youzu.com/about/child.html';
                }

                if(current_url.indexOf('/about/sdk.html') > -1){
                    window.location.href = '//m.youzu.com/about/sdk.html';
                }

                if(current_url.indexOf('/about/ceshi.html') > -1){
                    window.location.href = '//m.youzu.com/about/ceshi.html';
                }

            } else {
                if (current_url.indexOf('www.youzu') > -1){
                    return false;
                }
                window.location.href = pcurl;
            }
        }

        browserRedirect('https://m.youzu.com','https://www.youzu.com');
