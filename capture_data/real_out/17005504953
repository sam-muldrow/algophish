<script>    $(function() {
        // 顶部登陆框显示隐藏
        var setTimeout1 = null;
        var elDiv = '<a href="javascript:;" class="close-loggin"></a>';
        $('.loginStart .header_photo_ha, .loginAfter .header_photo_ha').prepend(elDiv);
        // 鼠标移入后侧图标
        $('.loggin-btn').hover(function (e) {
            e.preventDefault();
            $('.DaLunGroup').fadeIn(300);
            $(this).fadeOut(300);
        });
        $('.close-loggin').click(function(e) {
            e.preventDefault();
            $('.DaLunGroup').fadeOut(300);
            $('.loggin-btn').fadeIn(300);
        });
        // 悠闲游戏显示二维码
        $('.Gm_x_zu .Gm_x_xiao .gm-wrap a').hover(function (e) {
                e.preventDefault();
                $(this).children('.big-qr-code').fadeIn(300);
            }, function (e) {
                e.preventDefault();
                $(this).children('.big-qr-code').fadeOut(300);
            }
        );
        //页游动画
        $(".webgame_list li").hover(function (e) {
            $(this).stop(true).animate({'top':'-67px'},200);
            $(this).find('.webgame_item_line,.item_img').stop(true).animate({'opacity':'1'},200);
            $(this).find('.item_detail_txt').stop(true).animate({'opacity':'0'},200);
            $(this).find('.item_num').css({'color':'#ff9a49'},200);
        },function (e) {
            $(this).stop(true).animate({'top':'0'},200);
            $(this).find('.webgame_item_line,.item_img').stop(true).animate({'opacity':'0'},200);
            $(this).find('.item_detail_txt').stop(true).animate({'opacity':'1'},200);
            $(this).find('.item_num').css({'color':'#ffffff'},200)
        });
        //手游二维码动画
        $('.mgame_list li').hover(function (e) {
            $(this).find('.mitem_share,.mitem_share img').stop().animate({'opacity':'1'},1000);
            $(this).find('.mitem_bg,.mitem_bg img').stop().animate({'opacity':'0'},1000);
        },function (e) {
            $(this).find('.mitem_share,.mitem_share img').stop().animate({'opacity':'0'},200);
            $(this).find('.mitem_bg,.mitem_bg img').stop().animate({'opacity':'1'},200);
        });
        $('.type_list dl').hover(function (e) {
            $(this).find('.type_img').stop().animate({'opacity':'0'},10);
            $(this).find('.type_hov').stop().animate({'opacity':'1'},200);
        },function (e) {
            $(this).find('.type_img').stop().animate({'opacity':'1'},200);
            $(this).find('.type_hov').stop().animate({'opacity':'0'},10);
        });
        //左右人物动画效果
        oWindow = $(window).width();
        var posArr = [];
        if(oWindow<1300){
            posArr = [-366,-545,-407,-680]
        }else if(oWindow>1301&&oWindow<1400){
            posArr = [0,-545,-407,-680]
        }else if(oWindow>1401&&oWindow<1500){
            posArr = [0,-545,-50,-680]
        }else if(oWindow>1501&&oWindow<1600){
            posArr = [0,-150,-64,-230]
        }else if(oWindow>1601&&oWindow<1700){
            posArr = [0,-155,-50,-265]
        }else{
            posArr = [0,-230,-38,-430]
        }
        $(window).scroll(function (dom,pos) {
            var windowScroll = $(window).scrollTop();
            var  curDom = {
                'posArr':posArr,
                'domArr':['.per_1','.per_2','.per_3','.per_4']
            },type;
            for(var i=0;i<4;i++){
                type = (i%2 ==0)?'right':'left';
                animateDom(windowScroll,curDom.domArr[i],curDom.posArr[i],type)
            }
            // console.log(posArr)
        });
        if($.cookie('isChecked')){
            $('#userLoginWidget_userNameInput').val($.cookie('isChecked'))
        }
    });var doc = window.document

    function animateDom(windowScroll,dom,pos,i) {
        var offsetTop = $(dom).offset().top;
        if(windowScroll>offsetTop)return false;
        if(windowScroll>offsetTop-200&&windowScroll<offsetTop+100){
            //IE8，IE7的动画
            if(!doc.addEventListener){
                if(i=='right'){
                    $(dom).animate({'right':pos+'px',
                        'width':$(dom).width()+'px'
                    },1000)
                }else{
                    $(dom).animate({'left':pos+'px',
                        'width':$(dom).width()+'px'
                    },1000)
                }
            }else{
                $(dom).addClass('is-animated')
            }
        }
    }

    //热门活动
    $('.slider3').bxSlider({
        slideWidth: 660,
        minSlides: 2,
        maxSlides: 2,
        moveSlides: 1,
        startSlides:0,
        auto: true,
        slideMargin:0,
        autoStart: true
    });

    //打点
    var loc_i = 2;

    window.onload = function () {
        var loginPanel = new LoginPanel({
            el: '#loginPanelWrap',// 面板容器，最好是id
            style: {// 扩展样式//最小宽度308px
                height: '462px',
                width: '330px'
            },
            locId: '000600001',// 打点用：data_loc_id（不传默认为000700001）
            onClose: function () {// 关闭后回调函数
                $('.loggin-btn').fadeIn(300)
            }
        })
        $('.loggin-btn').mouseover(function () {
            loginPanel.show();
            $(this).fadeOut(300);
        })
    }

    </script></body>
