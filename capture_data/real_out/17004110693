<script>
    window.onload = function () {
        closeModal();
        function closeModal() {
            var btn = document.getElementById('closeBtn');
            var modal = document.getElementById('noticeModalWrap');
            btn.addEventListener('click', function () {
                modal.setAttribute('style', 'display: none')
            })
        }
    }
