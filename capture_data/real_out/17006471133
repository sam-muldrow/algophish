<script >
    function myForm() {
        var email = $('#email').val();
        var honeypot = $('#honeypot').val();
        
        if (honeypot !== '') 
        {
            swal(
                 'Error',
                 'Spam detected, Your submission has been blocked.',
                 'error'
                ) 
            return false; 
        }
        
        if (email == '') {
            $('#email').addClass('error');
            $('#email').focus();
            return false;
        } else {
            if (send_email_valid($('#email').val()) == false) {
                $('#email').attr('placeholder', 'Please enter valid email!');
                $("#email").addClass('error');
                return false;
            } else {
                $('#email').removeClass('error');
            }
        }
        $("#submit").html("Submitting...");
        $("#submit").prop('disabled', true);
        $.ajax({
            type: 'post',
            url: "https://www.spectranet.com.ng/Welcome/Newsletter",
            data: {
                'email': email
            },
            success: function(data) {
                if (data == 0) {
                    var url = 'https://www.spectranet.com.ng/';
                    $('#email').val('');
                    swal("Internal Error", "Email Id Already Exists!", "error");
                    window.setTimeout(function() {
                        window.location.replace(url);
                    }, 5000);
                } else {
                    $('#email').val('');
                    var url = 'https://www.spectranet.com.ng/';
                    swal("Success", "Thanks For Subscribing Our Newsletter. Activation Link Has Been Sent On Your Email Id.", "success");
                    window.setTimeout(function() {
                        window.location.replace(url);
                    }, 5000);
                }
                $("#submit").html("Submit");
                $("#submit").prop('disabled', false);
            }
        });
    }

function send_email_valid(email) {
    if (/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(email)) {
        return true;
    } else {
        return false;
    }
}
</script>
<script type="text/javascript" >
    function check(e) {
                document.cookie = "name= " + e + '; expires=Fri, 22 Nov 2024 09:58:33 UTC; path=/';
        // document.cookie= "name= "+e+ '; expires=Sun, 25 Dec 2021 00:00:00 UTC; path=/';
        $('#exampleModal').modal('hide');
        var uid = "64.222.201.246";
        $.ajax({
            type: 'post',
            url: 'https://www.spectranet.com.ng/welcome/save',
            data: "text=" + e + "&uid=" + uid,
            success: function(result) {
                // alert(result);
            }
        })
    } 
</script>
