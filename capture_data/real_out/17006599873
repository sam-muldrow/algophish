<script type='text/javascript' src='/app/public/js/pc2sp.js?57bb624513ba5dd43811cf1cc2f1a6ef0d9bc59b'></script>
<script type="text/javascript">
(function ($) {
  $(window).load(function(){
    if (checkSlider()) {
      startSlider();
    }

    var timer = false;
    $(window).resize(function() {
      if (timer !== false) {
        clearTimeout(timer);
      }
      timer = setTimeout(function() {
        if (checkSlider()) {
          startSlider();
        }
      }, 200);
    })

    // スライダー処理
    function startSlider() {
      $('.hotTopicsSlideBox .flexslider').flexslider({
        animation: "slide",
        animationLoop: false,
        itemWidth: 210,
        itemMargin: 12,
        minItem: 4,
        controlNav: false,
        slideshow: false,
        start: function() {
          $('.hotTopicsSlideBox .flexslider').css('padding', '10px 0 40px 10px');
        }
      });
    }

    // スライダーの要素の幅をチェックしてスライダー発動が必要な場合はtrueを返す 
    function checkSlider() {
      return true;
    }

  });
})(jQuery);
</script>

