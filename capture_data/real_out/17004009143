           const showConsent=true;
                window.sysClientObject = window.sysClientObject || [];
                function sozcuClient(){sysClientObject.push(arguments);}
                function initGoogleFc() {
        //--> user consent trigger
        if(showConsent===true) {
            if (window.googlefc !== undefined && googlefc !==undefined && googlefc.getAllowAdsStatus!==undefined) {
                let consentStatus = googlefc.getAllowAdsStatus();
                /*if (consentStatus === googlefc.ConsentStatusEnum.UNKNOWN || consentStatus === googlefc.ConsentStatusEnum.NO_CONSENT) {
                    googlefc.showRevocationMessage();
                }*/
            } 
        }
        //--<

    }
                function scheduleGoogleApiReady () {
                        let self = this;
                        if (!(window.googletag && googletag.apiReady)) {
                            setTimeout(function () {
                                scheduleGoogleApiReady();
                            }, 10);
                        } else {
                            setTimeout(function () {
                                sozcuClient("initAt", new Date());
                                console?.log("sozcuClientInit");
                            }, 1);
                            
                        }
                }
                function scheduleGoogleFc(){
                if (window.googlefc === undefined || window.googlefc?.getAdBlockerStatus === undefined) {
                    setTimeout(function () {
                        scheduleGoogleFc();
                    }, 100);
                } else {
                    setTimeout(function () {
                        initGoogleFc();
                    }, 1);
                }
                }
                scheduleGoogleFc();
                scheduleGoogleApiReady();
                
             </script>
