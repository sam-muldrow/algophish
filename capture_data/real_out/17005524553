<script type="text/javascript">

    function loadMore(size, page){
    var page = page != undefined ? page : 2;
    var size = size != undefined ? size : 20;
    $.post('ajax?act=loadmore',
    {size, page: page},
        function(response){
            if(response.error == 0){
                $('#_latest_articles').append(response.latest_articles);
                $('#page_ajax').html(response.pagination);
                $("#_latest_articles .lazyload").each(function() {
                    $(this).attr("src", $(this).data("src"));
                    this.style.opacity = 1
                })
            }else{
                console.log(response.message);
            }
        },
        'json'
    );
}
    
</script>
