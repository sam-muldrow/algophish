:hidden"><a href="https://allwomenstalk.com/stories" class="text-sm font-semibold sm:block">All stories <span aria-hidden="true">&rarr;</span></a></div></div></div><script>function popularstories() {
  return {
    title: "Stories Trending Now ",
    list: [],
    init(){
      fetch('https://allwomenstalk.com/stories/list.json')
      .then(response => response.json())
      .then(data => {
        data.items.sort(() => Math.random() - 0.5);
        this.list = data.items.map (el=> {
          el.title = el.title.replace(/&#39;|[\uD800-\uDBFF][\uDC00-\uDFFF]|[\uD800-\uDFFF]/g, '');
          return el;
        });
        console.log(this.list);
      });
    }
  }
