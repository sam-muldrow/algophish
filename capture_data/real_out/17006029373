	jQuery(document).ready(function () {
		sneaker_landscape();
	});
	jQuery(window).resize(function () {
		sneaker_landscape();
	});
	function sneaker_landscape() {
		if (420 > window.screen.height) {
			jQuery('body').addClass("min-landscape");
		} else {
			jQuery('body').removeClass("min-landscape");
		}
	}
