			<div class="notify_mailchimp" id="notify_mailchimp_header"></div>
		</div><!-- subscribe-form -->
					</div>
		
        <script type="text/javascript" defer>
            jQuery( document ).ready( function () {
                /**
                 * MailChimp jQuery.
                 *
                 * @todo need to show the response text on div.
                 */

                var is_processing = false;

                jQuery( '#mailchimp_newsletter_header' ).on( 'submit', function ( e ) {
                    e.preventDefault();

                    if ( !is_processing ) {
                        is_processing = true;
                    } else {
                        return false;
                    }

                    var newsletter_data = {
                        _wpnonce: jQuery( this ).find( 'input[name="_wpnonce"]' ).val(),
                        _wp_http_referer: jQuery( this ).find( 'input[name="_wp_http_referer"]' ).val(),
                        action: 'mailchimp_newsletter',
                        is_mailchimp_request: jQuery( this ).find( 'input[name="is_mailchimp_request"]' ).val(),
                        mailchimp_user_email: jQuery( this ).find( 'input[name="mailchimp_user_email"]' ).val()
                    };

                    jQuery.ajax( {
                        type: "POST",
                        url: sneaker_global.ajax_url,
                        data: newsletter_data,
                        success: function ( data ) {
                            jQuery( "#notify_mailchimp_header" ).text( data );
                            jQuery( "#notify_mailchimp_header" ).show();
                            setTimeout( function () {
                                jQuery( "#notify_mailchimp_header" ).hide();
                            }, 3000 );
                            is_processing = false;
                        }
                    } );
                } );
            } );
        </script>
				</div>
