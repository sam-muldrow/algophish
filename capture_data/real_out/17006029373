			<form action="" method="POST" id="mailchimp_newsletter_header">
				<input type="hidden" id="_wpnonce" name="_wpnonce" value="fe4cd70bde" /><input type="hidden" name="_wp_http_referer" value="/" />				<input type="hidden" name="action" value="mailchimp_newsletter" />
				<div class="subscribe-input">
					<input type="hidden" name="is_mailchimp_request" value="1" />
					<label for="mailchimp_user" class="screen-reader-text">Enter Email</label>
					<input type="text" placeholder="Enter Your Email" name="mailchimp_user_email" required="required" id="mailchimp_user">
				</div>
				<div class="subscribe-button">
					<button type="submit" class="signupbtn">Subscribe</button>
