            <a id="DeepLAppPromo-link" href="#"></a>
            
        </div>
        <div id="DeepLAppPromo-illustration-container"></div>
    </div>
</div>

<script>
    const baseLink = "https://www.deepl.com"

    const getQueryParamsString = (utm_content) => {
        let host = window.location.host.replace(/\./g, "")
        return `?utm_source=${host}&utm_medium=linguee&utm_content=${utm_content}`
    }

    const getDownloadBadgeUrl = (language = 'en', os = 'iOS') => {
        const languageSuffix = language.toLowerCase()
        const availableBadgeLanguages = ['de', 'en', 'es', 'fr', 'it', 'ja', 'nl', 'pl', 'pt-br', 'pt-pt', 'ru', 'zh']
        const chosenLanguage = availableBadgeLanguages.includes(languageSuffix) ? languageSuffix : 'en'

        if (os === 'iOS') {
            return `/img5/DeepLAppPromoStartPage/iosBadge/download_on_the_app_store_black_${chosenLanguage}.svg`
        }

        return `/img5/DeepLAppPromoStartPage/androidBadge/get_it_on_google_play_${chosenLanguage}.png`
    }

    let headlineText = `Traduce cuando y donde quieras con la app m&#243;vil de DeepL`
    let descriptionText = `No te pierdas las funciones exclusivas de la app como traducir texto a partir de una imagen o un documento e incluso utilizando la c&#225;mara.`
    let buttonHTML = `Descargar gratis` 
    let buttonLink = `${baseLink}/mobile-apps${getQueryParamsString('mobile-apps-lp')}` 

    if(pageState.isMac) {
        headlineText = 'Traduce a&#250;n m&#225;s r&#225;pido con DeepL para Mac'
        descriptionText = 'Traduce todo lo que leas o escribas en cualquier aplicaci&#243;n y ahorra tiempo de muchas otras formas.'
        buttonLink = `${baseLink}/macos-app${getQueryParamsString('macos-app-lp')}`
        
    } else if (pageState.isWindows) {
        headlineText = 'Traduce a&#250;n m&#225;s r&#225;pido con DeepL para Windows'
        descriptionText = 'Traduce todo lo que leas o escribas en cualquier aplicaci&#243;n y ahorra tiempo de muchas otras formas.'
        buttonLink = `${baseLink}/windows-app${getQueryParamsString('windows-app-lp')}`
        
    } else if(pageState.isAndroid || pageState.isChromeOS) {
        headlineText = 'Traduce donde y cuando quieras con DeepL'

        let androidBadgeUrl = getDownloadBadgeUrl(interfaceLang, 'Android')
        buttonHTML = `<img id="DeepLAppPromoMobile-android-img" alt="Play-Store-Badge" src="${androidBadgeUrl}" />`
        buttonLink = 'https://play.google.com/store/apps/details?id=com.deepl.mobiletranslator&referrer=utm_source%3Dlinguee%26utm_campaign%3Dlinguee-web-button%26anid%3Dadmob'
    } else if (pageState.isIPad || pageState.isIPhone) {
        headlineText = 'Traduce donde y cuando quieras con DeepL'

        let iosBadgeBadgeUrl = getDownloadBadgeUrl(interfaceLang, 'iOS')
        buttonHTML = `<img id="DeepLAppPromoMobile-ios-img" alt="App-Store-Badge" src="${iosBadgeBadgeUrl}" />`
        buttonLink = 'https://apps.apple.com/app/apple-store/id1552407475?pt=309298&ct=linguee-web-button&mt=8'
    } 

    $('#DeepLAppPromo-title').html(headlineText)
    $('#DeepLAppPromo-description').html(descriptionText)

    $('#DeepLAppPromo-link').attr('href', buttonLink)
    $('#DeepLAppPromo-link').html(buttonHTML)
    
    if (!pageState.isIPad && !pageState.isIPhone && !pageState.isAndroid && !pageState.isChromeOS) {
        $('#DeepLAppPromo-link').addClass('DeepLAppPromo-mainButton')
    }

</script></div><!--lingueecontent-->
