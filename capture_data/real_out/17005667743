        //IF not mobile init only ads that are not mobile-only
        if('multiplex-native-native'.indexOf('mobile-only') <= 0){
          Wasp.Dfp.GoogleTag.initAd($("#multiplex-native-native"));
        }
      }
    });
  </script>
            </div>
        </div>
    </div>
