        <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/2.0.2/TweenMax.min.js"></script>
        <script> 
            jQuery(document).ready(function(e){
                //---------------------------------------------------------------------
                // HOME PAGE PERCENTAGE ANIMATION
                //---------------------------------------------------------------------

                var jsonUrl = 'https://indicedebonheur.com/surveys/mood_en.json';
                var prcTwoEnd;
                var prcLanguage;

                $.ajax({
                    url: jsonUrl,
                    dataType: 'json',
                    success: function( data ) {
                        // console.log(data);
                        prcTwoEnd = parseFloat( data.average);
                        prcLanguage = data.average;
                        emojiCodeWrap();
                    },
                    error: function( data ) {
                        prcTwoEnd = 75.5;
                        prcLanguage = 'fr';
                        emojiCodeWrap();
                    }
                });

                function emojiCodeWrap(){
                    // TWO ----------------------------------------------
                    var prcTwo = $('[data-prc-two]');

                    var prcTwoStart = prcTwoEnd - 15;
                    if(prcTwoStart < 0) prcTwoStart = 0;

                    var prcSTwo = {val:prcTwoStart};

                    TweenMax.to(prcSTwo,2 ,{val:prcTwoEnd,ease: Power3.easeOut, onUpdate:onUpdatePrcTwo, onComplete: onCompleteTwo} );

                    var diffPrevTwo = prcSTwo.val;
                    var incTwo = 0;

                    function onUpdatePrcTwo(){
                        if(prcLanguage === 'fr'){
                            prcTwo.html(prcSTwo.val.toFixed(1).toString().replace("." , ","));
                        }
                        else{
                            prcTwo.html(prcSTwo.val.toFixed(1).toString());
                        }

                        var diff = prcSTwo.val - diffPrevTwo;
                        diffPrevTwo = prcSTwo.val;
                        incTwo += diff;

                        if(incTwo > 1.25){
                            incTwo = 0;

                            var r = Math.round(Math.random()*7);

                            TweenMax.set($('[data-emoji-two]'), {autoAlpha:0});
                            TweenMax.set($('[data-emoji-two]').eq(r), {autoAlpha:1});
                        }
                    }

                    var emojiTwo;
                    switch (true){
                        case (prcTwoEnd <= 55):
                            emojiTwo = 1;
                            break;
                        case (prcTwoEnd > 55 && prcTwoEnd <= 61):
                            emojiTwo = 2;
                            break;
                        case (prcTwoEnd > 61 && prcTwoEnd <= 66):
                            emojiTwo = 3;
                            break;
                        case (prcTwoEnd > 66 && prcTwoEnd <= 71):
                            emojiTwo = 4;
                            break;
                        case (prcTwoEnd > 71 && prcTwoEnd <= 76):
                            emojiTwo = 5;
                            break;
                        case (prcTwoEnd > 76 && prcTwoEnd <= 81):
                            emojiTwo = 6;
                            break;
                        case (prcTwoEnd > 81 && prcTwoEnd <= 86):
                            emojiTwo = 7;
                            break;
                        case (prcTwoEnd > 86 && prcTwoEnd <= 100):
                            emojiTwo = 8;
                            break;
                    }

                    function onCompleteTwo(){
                        TweenMax.set($('[data-emoji-two]'), {autoAlpha:0});
                        TweenMax.set($('[data-emoji-two]').eq(emojiTwo), {autoAlpha:1});
                    }
                }
            });	
        </script>
    </article>
</div>
          <!-- dfpMultiflexMobileOnly -->
    <div class="item item-ad check-height neutral w1 h1">
        <div class="ad-multiplex-mobile-only">
    <div class="dfpLabel">
        <div class="ligne"></div>
        <div class="label">Publicité</div>
        <div class="ligne"></div>
    </div>
            <div id='multiplex-mobile-only-mobile4'
                 class='ad-to-load dfp-ad dfp-multiflex-mobile-only'
                 data-size="dfpMultiflexMobileOnly"
                 data-pos="mobile4"
                 data-tile=""
                 data-ajax-interval=""
                 data-is-empty="true"
                 data-hidden-if-empty="false">
            </div>
  <script>
    $(document).ready(function () {
      //If mobile init all ads
      if(isMobile()){
        Wasp.Dfp.GoogleTag.initAd($("#multiplex-mobile-only-mobile4"));
      }else{
        //IF not mobile init only ads that are not mobile-only
        if('multiplex-mobile-only-mobile4'.indexOf('mobile-only') <= 0){
          Wasp.Dfp.GoogleTag.initAd($("#multiplex-mobile-only-mobile4"));
        }
      }
    });
  </script>
        </div>
