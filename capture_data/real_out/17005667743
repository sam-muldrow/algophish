  <div id='promo-pkp' class="item w3 h1 " style="display: none;">
<article class="item-inner">
<a target="_blank" id="tva-promo-link">
<div class="tuilePromoTVA" id="tuilePromoTVA">
</div>
</a>
</article>
</div>



<script>

initPromo();

function loadJSON(callback) {   
  var xobj = new XMLHttpRequest();
  xobj.overrideMimeType("application/json");
  xobj.open('GET', 'https://s1.quebecormedia.com/infojdem/banieresPromo/promo.json', true); // Replace 'my_data' with the path to your file
  xobj.onreadystatechange = function () {
        if (xobj.readyState == 4 && xobj.status == "200") {
          // Required use of an anonymous callback as .open will NOT return a value but simply returns undefined in asynchronous mode
          callback(xobj.responseText);
        }
  };
  xobj.send(null);  
}

function initPromo() {
   loadJSON(function(response) {
    // Parse JSON string into object
    var promoJson = JSON.parse(response);

    // start promo logic here in the callback
    let defaultPromo =    {
      "titre": "TVAPlus",
      "analyticTag": "TVAPlus-FiletDeSecurite",
      "date": "2023-06-27",
      "link": "https://www.qub.ca/tvaplus/films",
      "small": "https://m1.quebecormedia.com/emp/tvanouvelles_prod/photo/065b1bff-c95f-4005-a672-2ef6506455ff_TVAP_Films_Ete_2300x636_2305.jpeg",
      "medium": "https://m1.quebecormedia.com/emp/tvanouvelles_prod/photo/d0b97964-3908-4f98-9e45-ddbb94d72a9e_TVAP_Films_Ete_1528x636_2305.jpeg",
      "large": "https://m1.quebecormedia.com/emp/tvanouvelles_prod/photo/abac3c27-a169-4242-b194-bb3fcdbbe852_TVAP_Films_Ete_756x626_2305.jpg"
      }

    window.dataLayer = window.dataLayer || [];
    var promoDate = new Date();
    // var promoDate = new Date(2020,9,2,21,0,2);
    // console.log('Date: '+ promoDate.getDay() +' '+ promoDate.getHours());
    var promoActive;
    var el = document.getElementById('tuilePromoTVA');
    
    var today;
    today = formatDate(promoDate, 0);
    
    /*
    if ( promoDate.getHours() >= 23 ){
    today = formatDate(promoDate, 1)
    } else {
    today = formatDate(promoDate, 0)
    }
    */


    var newArray = promoJson.filter(function(dataDate) {
      return dataDate.date == today;
    });

    // console.log('Nombre de promo aujourdhui: '+ newArray.length)

    // run the default promo if none are available
    if (newArray.length >= 1){
      const randomItemNumber = getRandomInt(newArray.length);
      runPromo(newArray[randomItemNumber]);
      document.addEventListener("scroll",elementInViewport);
    } else {
      runPromo(defaultPromo);
      document.addEventListener("scroll",elementInViewport);
    }

    function getRandomInt(max) {
      return Math.floor(Math.random() * Math.floor(max));
    }

    function formatDate(date, adjustment) {
      var d = new Date(date),
          month = '' + (d.getMonth() + 1),
          day = '' + (d.getDate() + adjustment),
          year = d.getFullYear();
      
      if (month.length < 2) 
          month = '0' + month;
      if (day.length < 2) 
          day = '0' + day;
      return [year, month, day].join('-');
    }

    function elementInViewport() {
      let bounding = el.getBoundingClientRect();
      let vh = Math.max(document.documentElement.clientHeight || 0, window.innerHeight || 0)
      let boundingDiffTop = bounding.top - vh
      let boundingDiffBottom = Math.round(bounding.bottom)
      if (boundingDiffTop <= 0 && boundingDiffBottom >= 0){
        // 		console.log('-: '+boundingDiffTop+':'+boundingDiffBottom );
        document.removeEventListener("scroll",elementInViewport);
        window.dataLayer.push({
          event: 'masterGAEvent',
          'non-interaction hit': 'true',
          eventCategory: 'bannieres autopromo',
          eventAction: 'impression banniere',
          eventLabel: promoActive.analyticTag 
        })
      } else {
      // 		console.log('OUT')
      }
    }

    function runPromo(promoData){
      // 	console.log('promoData: '+ JSON.stringify(promoData))
      promoActive = promoData;
      injectPromoImg(promoData);
      injectPromoInHTML(promoData);
    }

    function injectPromoInHTML(data){
      document.getElementById("tva-promo-link").href = data.link;
      document.getElementById("tva-promo-link").addEventListener("click", function(){
        window.dataLayer.push({
          event: 'masterGAEvent',
          'non-interaction hit': 'true',
          eventCategory: 'bannieres autopromo',
          eventAction: 'clic banniere',
          eventLabel: data.analyticTag 
        })
      });
    }

function injectPromoImg(imagePromoUrl){
var newStyle = document.createElement('style');
newStyle.innerHTML = `
#promo-pkp{
  display: block !important;
}
.tuilePromoTVA{
  position: absolute;
  top: -3px; left:0;
  width: 100%;
  height: 100%;
  padding: 0px 0px;
  background-repeat: no-repeat;
  background-size: cover;
  background-image: url('${imagePromoUrl.large}?impolicy=resize&width=1150&height=318');
  background-image: image-set( url('${imagePromoUrl.large}?impolicy=resize&width=1150&height=318') 1x, url('${imagePromoUrl.large}?impolicy=resize&width=2300&height=636') 2x );
  background-image: -webkit-image-set( url('${imagePromoUrl.large}?impolicy=resize&width=1150&height=318') 1x, url('${imagePromoUrl.large}?impolicy=resize&width=2300&height=636') 2x );
 }	

[data-column="3"] .w3.h1 .item-inner, [data-column="4"] .w3.h1 .item-inner, [data-column="5"] .w3.h1 .item-inner, [data-column="6"] .w3.h1 .item-inner, [data-column="7"] .w3.h1 .item-inner, [data-column="8"] .w3.h1 .item-inner, [data-column="9"] .w3.h1 .item-inner, [data-column="10"] .w3.h1 .item-inner{
  padding-top: 27.5%;
}
[data-column="2"] .tuilePromoTVA {
  background-image: url('${imagePromoUrl.medium}?impolicy=resize&width=764&height=318');
  background-image: image-set( url('${imagePromoUrl.medium}?impolicy=resize&width=764&height=318') 1x, url('${imagePromoUrl.medium}?impolicy=resize&width=1528&height=636') 2x );
  background-image: -webkit-image-set( url('${imagePromoUrl.medium}?impolicy=resize&width=764&height=318') 1x, url('${imagePromoUrl.medium}?impolicy=resize&width=1528&height=636') 2x );

}
[data-column="1"] .tuilePromoTVA {
  background-image: url('${imagePromoUrl.small}?impolicy=resize&width=378&height=313');
  background-image: image-set( url('${imagePromoUrl.small}?impolicy=resize&width=378&height=313') 1x, url('${imagePromoUrl.small}?impolicy=resize&width=756&height=626') 2x );
  background-image: -webkit-image-set( url('${imagePromoUrl.small}?impolicy=resize&width=378&height=313') 1x, url('${imagePromoUrl.small}?impolicy=resize&width=756&height=626') 2x );
}
`;
document.head.appendChild(newStyle);
}


  });
}



</script>
