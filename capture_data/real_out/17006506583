XCP_Ad.loadAd({
mark : "xcphome"
});
var layerIndex = null;
function openSecurityPage(_url){
if(typeof(_url) == "undefined" || _url.length < 0){
return ;
}
layerIndex = layer.open({
title: '友情链接',
type: 2, //iframe
area: ['772px', '444px'],
content: XCP_UC.urlPrefix+ "/index/popup-friendshipLink.html?openUrl="+_url,
scrollbar: false,//屏蔽浏览器滚动条
shade:0.6 //遮罩背景透明度
});
}
function closeSecurityPage(){
layer.close(layerIndex);
}
</script>
