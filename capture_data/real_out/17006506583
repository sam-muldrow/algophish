<!-- 短视频弹窗 js -->
<script type="text/javascript">
var player;
var list = new Array();
var pos = 0;
$(function () {
// 点击关闭按钮关闭短视频弹窗
$(".close-btn").click(function(){
destroyPlayer();
resetPos(0);
$(".shortVideo").hide();
$(".masks").hide();
document.getElementsByTagName('body')[0].style.overflow="auto";
$("#playbox").html("");
// 新增
$(".videobtn-left").hide();
$(".videobtn-right").hide();
});
//点击遮罩层关闭短视频弹窗
$(".masks").click(function(){
destroyPlayer();
resetPos(0);
$(".shortVideo").hide();
$(".masks").hide();
document.getElementsByTagName('body')[0].style.overflow="auto";
// 新增
$(".videobtn-left").hide();
$(".videobtn-right").hide();
});
//点赞
$('.pics').click(function(){
if ($(this).attr("praise") == 0){
var url = XCP_UC.urlPrefix + "/data/post.do";
var data = {};
var num = Number($(".video-wrap .num").text());
data.submitType = "praise";
data.targetId =  $(".video-wrap .zan").attr("data-id");
data.targetType =  $(".video-wrap .zan").attr("data-type");
$.post(url, data, function (res) {
if(res.code == 1){
layer.msg("点赞成功", {icon: 1, time: 1500});
$(".video-wrap .num").text(Number(num + 1));
$('.pics').attr("src","https://res.xincheping.com/images/2020/pc/v1/video_praise_red.png");
$('.pics').attr("praise","1");
}else if (res.code == -99){
layer.msg(res.msg, {icon:2,time: 1500});
$('.pics').attr("src","https://res.xincheping.com/images/2020/pc/v1/video_praise_red.png");
$('.pics').attr("praise","1");
}else if (res.code == -4){
XCP_UC.popupLoginWin('onHeaderLogin', window.location.href);
}else {
layer.msg(res.msg, {icon:2,time: 1500});
}
}, "json");
}
});
//切换短视频
$("body").on("click", ".rmd-tt", function() {
var qid = $(this).attr("qid");
var title = $(this).find(".min-text").text();
var targetId = $(this).attr("data-id");
var targetType = $(this).attr("data-type");
pos = parseInt($(this).attr("data-index"));
resetPos(pos);
openPlayBox(qid, title, targetId, targetType, 0);
});
//播放
$(".play-icon").click(function () {
player.play();
$(".play-icon").hide();
});
//暂停
$("#playbox").click(function () {
if (!player.playing()){
$(".play-icon").show();
}else {
$(".play-icon").hide();
}
})
});
function playVideo(state) {
if(state == 0){
//播放上一个视频
pos = pos - 1 > 0 ? pos - 1 : 0;
}else {
//播放下一个视频
pos = pos + 1 < list.length - 1 ? pos + 1 : list.length - 1;
}
resetPos(pos);
var obj = getVideoObj(pos);
if(obj != ""){
openPlayBox(obj.qCloudId, obj.title, obj.id, obj.targetType, 0);
}
}
function resetPos(index) {
pos = index;
if (pos > 0){
$(".videobtn-left").show();
}else {
$(".videobtn-left").hide();
}
if (pos == list.length - 1){
$(".videobtn-right").hide();
}else {
$(".videobtn-right").show();
}
}
//获取短视频对象
function getVideoObj(pos) {
for (var i = 0; i < list.length; i++){
var obj = list[i];
if(pos == i){
return obj;
}
}
return "";
}
//加载短视频推荐列表
function loadShortVideoCommend(targetId) {
$(".rmd-tt").remove();
var url = XCP_UC.urlPrefix+ "/shortVideo/commendList.do";
var data = {};
data.targetId = targetId;
data.pageSize = 9;
$.get(url, data, function (res) {
if (res.code == 1){
list = res.result;
$.each(res.result, function (i, v) {
$(".rmd-videobox").append('<a><div class="rmd-tt" data-index='+i+' data-id='+v.id+' data-type='+v.targetType+' praiseCount='+v.praiseCount+' qid='+v.qCloudId+' >' +
'<div class="pic-box"><img class="pic" src="'+v.picUrl+'"/></div><div class="min-text">'+v.title+'</div></div></a>');
});
}
},"json");
}
//加载短视频
var isClickQCloud = 0;
function openPlayBox(qid, title, targetId, targetType, load) {
destroyPlayer();
if (load == 1){
//加载短视频推荐列表
loadShortVideoCommend(targetId);
$(".videobtn-right").show();
}
if ( $(".shortVideo").css("display") != 'block'){
// 显示弹窗
$(".shortVideo").show();
$(".masks").show();
document.getElementsByTagName('body')[0].style.overflow = "hidden";
}
$("#playbox").html("");
$(".play-icon").hide();
//检查是否点赞
checkPraise(title, targetId, targetType);
var width = $(".video-wrap").width();
var height = $(".video-wrap").height();
var url = XCP_UC.urlPrefix + "/third/qcloud/get_playurl.do";
var data = {};
data.qid = qid;
if(isClickQCloud != 1){
isClickQCloud = 1;
$.getScript("https://imgcache.qq.com/open/qcloud/video/vcplayer/TcPlayer-2.3.2.js", function () {
$.get(url, data, function (res) {
if (res.code == 1) {
var src_sd = '';
var src_hd = '';
var src_shd = '';
if(typeof(res.result.sd) != 'undefined'){
src_sd = res.result.sd.playerUrl;
}
if(typeof(res.result.hd) != 'undefined'){
src_hd = res.result.hd.playerUrl;
}
if(typeof(res.result.shd) != 'undefined'){
src_shd = res.result.shd.playerUrl;
}
player = new TcPlayer("playbox", {
// sd 标清  hd高清   无后缀 超清
//                                "m3u8_hd": src_hd,
//                                "m3u8_sd": src_sd,
"m3u8": src_shd, //默认为超清
"autoplay": true,
"width": width,//视频的显示宽度，请尽量使用视频分辨率宽度
"height": height,//视频的显示高度，请尽量使用视频分辨率高度
"controls": "none",
"flash": false,
"listener": function (msg) {
if(msg.type == "ended"){
if($(".play-icon").css("display") == "none"){
$(".play-icon").show();
}
}
}
});
}else {
$("#playbox").html('<div style="text-align: center; line-height: '+height+'px" >该视频暂时不能播放</div>')
}
isClickQCloud = 0;
}, "json");
});
}
}
//先销毁player，避免出现之前的player进行加载视频流操作
function destroyPlayer() {
if (player != null){
player.destroy();
player = null;
}
}
//检查是否点赞
function checkPraise(title, targetId, targetType){
$(".text-wrap").text(title);
$(".video-wrap .zan").attr("data-id", targetId);
$(".video-wrap .zan").attr("data-type", targetType);
var url =  XCP_UC.urlPrefix + "/shortVideo/checkPraise.do";
var data = {};
data.targetId = targetId;
data.targetType = targetType;
$.get(url, data, function (res) {
if(res.code == 1){
if(res.result.isPraised == 1){
$('.pics').attr("src","https://res.xincheping.com/images/2020/pc/v1/video_praise_red.png");
$('.pics').attr("praise", "1");
}else {
$('.pics').attr("src","https://res.xincheping.com/images/2017/xcx/video_praise.png");
$('.pics').attr("praise", "0");
}
$(".video-wrap .num").text(res.result.praiseCount);
}else {
$('.pics').attr("src","https://res.xincheping.com/images/2017/xcx/video_praise.png");
$('.pics').attr("praise", "0");
$(".video-wrap .num").text(0);
}
}, "json");
}
</script>
<!-- 短视频 end -->
