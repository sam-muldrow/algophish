//<[!CDATA[
$(function() {
$(".search_form2").submit(function() {
var keyword = $(".search_form2 input[type='text']").val();
keyword = keyword.replace(/^\s+|\s+$/, "");
if (keyword.length != 0) {
return true;
} else {
return false;
}
window.location.href = '/search/zonghe.do?q='+keyword;
return false;
});
