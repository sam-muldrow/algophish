<script>
    if(location.hash != "" && location.hash.indexOf("fullscreen") != -1){
        $(".popup-notice").removeClass("popup-notice-float");
        $(".popup-notice").addClass("popup-notice-fullscreen");
    }
$(".close").click(function (){
        if($(".popup-notice.popup-notice-fullscreen").length > 0){
            $("body").css("overflow","auto");
        }
        $(".popup-notice").removeClass("popup-notice-show");
    });
var notice_now = new Date();
    var notice_end = new Date('2022/08/14 23:50:00');
    if(notice_now.getTime() < notice_end.getTime()){
        setTimeout(function () {
            if($(".popup-notice.popup-notice-fullscreen").length > 0){
                $("body").css("overflow","hidden");
            }
            $(".popup-notice").addClass("popup-notice-show");
        },1000);
    }
</script>
