    <script type="text/javascript">if(typeof window._takvimToken=='undefined')window._takvimToken = 'Basic /wGqlTJGUTH/lUQUzv4Ufabp1b23Pfw6yzPrGeMOoXiscByf4jz8tnGgKbyR7Q/V';</script>
<script type="text/javascript">
    if (typeof window.tService == 'undefined')
        window.tService = new JService('../JTakvimService', '');
    if (window.tService._authHeader == null || window.tService._authHeader ==='') {
        window.tService.setAuthHeader(window._takvimToken);
    }    function visualization_downloader() {
        var holder = jQuery('#visualization');
        var para = { sinavAnah : -1, yil : '2023', tur : -1 };        
        tService.promise('Events/GetAll', para).done(function (p) {
            visualization_loadData(p);
            window.sessionStorage.setItem("_wiyTakvimEvents", JSON.stringify(p));
        }).fail(function () {
            holder.html('Takvim güncelleme sırasında bir hata oluştu.');
        });
    }
    function visualization_loadData(p) {
        if (typeof vis != 'undefined') {
            if  (typeof window.sinav_anah != 'undefined'){
                if (!isNaN(window.sinav_anah)) {
                    p = p.filter(function (k) { return k.SinavAnah == window.sinav_anah; });
                }
            }
            var s1 =parseInt(15);if(s1<0) s1=15;
            var e1 =parseInt(15);if(e1<0) e1=345;
            //            var s1 = 15;
            //            var e1 = 345;
            var t = new OSYM.sinavTakvim({
                data:p,
                refId: '#visualization',
                onAddItem: function (a1, a2) {
                    if (a1.content) {               
                        a1.content = '';
                        switch(a1.tip){
                            case 1:a1.content  +='<strong>Sınav :</strong>';break;
                            case 2:a1.content  +='<strong>Başvuru:</strong>';break;
                            case 6:a1.content  +='<strong>Geç Başvuru:</strong>';break;
                            case 3:a1.content  +='<strong>Ödeme:</strong>';break;
                            case 4:a1.content  +='<strong>Sonuç:</strong>';break;
                            case 5:a1.content  +='<strong>Tercih:</strong>';break;
                        }
                        a1.content += (a1.content.length>0 ?' ':'' )+a2.SinavKisaAd;
                        if (a2.Aciklama != '')
                            switch(a1.tip){
                                case 1:a1.content  +=' | ' + '<a href="' + a2.UrlSin + '">';break;
                                case 2:a1.content  +=' | ' + '<a href="' + a2.UrlBas + '">';break;
                                case 4:a1.content  +=' | ' + '<a href="' + a2.UrlSon + '">';break;
                                case 5:a1.content  +=' | ' + '<a href="' + a2.UrlTer + '">';break;
                                case 6:a1.content  +=' | ' + '<a href="' + a2.UrlGecBas + '">';break;
                            }

                        switch(a1.tip){
                            case 1:a1.content  +='<span style="text-decoration: underline; color:#000;">' + a2.Aciklama + '</span></a>';break;
                            case 4:a1.content  +='<span style="text-decoration: underline; color:#000;">' + a2.Aciklama + '</span></a>';break;
                            default:a1.content  +='<span style="text-decoration: underline; color:#fff;">' + a2.Aciklama + '</span></a>';
                        }

                        switch(a1.tip){
                            case 1:a1.content  +='<br /><img src="/js/dist/img/timeline/pin.png" width="24" />';break;
                            case 4:a1.content  +='<br /><img src="/js/dist/img/timeline/pin.png" width="24" />';break;
                        }
                    }
                },
                visOptions: {                    
                    height: 210,
                    maxHeight: 210,
                    start: vis.moment().subtract(s1, 'days'),
                    end: vis.moment().add(e1, 'days'),
                    zoomable: false
                }
            });
        }
    }
    jQuery(function () {
        
        if (typeof tService._authHeader == 'undefined') throw ("takvim token undefined!");
        if (typeof tService._authHeader == null) throw ("takvim token null!");
        var dwn = true
        if (window.sessionStorage) {
            var item = window.sessionStorage.getItem('_wiyTakvimEvents');
            if (typeof item != 'undefined') {
                item = JSON.parse(item);
                if (jQuery.isArray(item)) {
                    dwn = false;
                    visualization_loadData(item);
                }
            }
        }
        if (dwn) {
            visualization_downloader();
        }
    });

</script>
<script type="text/javascript">
    jQuery(function () {
        var ddl1 = jQuery('#selSnv');
        var ddl2 = jQuery('#selTip');
        var tk = null;
        var load_tk = function () {
            window.setTimeout(function () {
                if (!tk) {
                    tk = jQuery('#visualization');
                    if (tk) tk = tk.data('takvim');
                }
                if (tk) {
                    var d1 = ddl1.val();
                    var d2 = ddl2.val();
                    if (d1 == '-1' && d2 == '-1')
                        tk.filter(null);
                    else {
                        var p1, p2;
                        if (d1 != '-1')
                            p1 = { name: 'SinavAnah', value: d1 };
                        if (d2 != '-1') {
                            p2 = { name: 'tip', value: d2 };
                        }
                        tk.filter.call(tk, p1, p2);
                    }
                }
            }, 5);
        };
        jQuery.each([{ key: '1', value: 'Sınav' }, { key: '2', value: 'Başvuru' }, { key: '4', value: 'Sonuç' }, { key: '5', value: 'Tercih' }, { key: '6', value: 'Geç Başvuru'}], function (i, v) { ddl2.append(jQuery('<option value="' + v.key + '">' + v.value + '</option>')); });
        ddl2.prop('selectedIndex', 0);
        var ddl1_loader = function (d) {
            if (jQuery.isArray(d)) {
                d = d.sort(function (aa, bb) { return aa.SinavKisaAd.localeCompare(bb.SinavKisaAd); });
                jQuery.each(d, function (i, v) {
                    ddl1.append(jQuery('<option value="' + v.Anah + '"  data-subtext="' + v.SinavAd + '">' + v.SinavKisaAd + '</option>'));
                });
            }
        };
        ddl1.on('change', function () {
            var v = ddl1.val();
            ddl1.attr('title', '');
            if (v != '-1') {
                var item = ddl1.find('option').eq(ddl1.get(0).selectedIndex);
                if (item) ddl1.attr('title', item.attr('title'));
            }
            load_tk();
        });
        ddl2.on('change', function () {
            load_tk();
        });
        //        var ld = typeof window.sessionStorage != 'undefined' ? window.sessionStorage.getItem('_wiyTakvimExams') : null;
        //        if (ld) ld = JSON.parse(ld);
        //        if (!jQuery.isArray(ld)) {
        //            if (typeof window.tService != 'undefined') {
        //                tService.promise('/Exams/GetAll', {}).done(function (ff) {
        //                    window.sessionStorage.setItem('_wiyTakvimExams', JSON.stringify(ff));
        //                    ddl1_loader(ff);
        //                });
        //            }
        //        }
        //        else
        //            ddl1_loader(ld);
    });
</script>
