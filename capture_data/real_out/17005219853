        window.pbjs = window.pbjs || {};
        window.pbjs.que = window.pbjs.que || [];

        window.adUnits = function () {
            return {
                prebidAdUnits: window.prebidAdUnits,
                adUnitSlots: window.adUnitSlots,
                prebidAdUnitsVideo: window.prebidAdUnitsVideo,
                adUnitSlotsVideo: window.adUnitSlotsVideo
