<script>
    // Delete the Funding Choices cookie if consent is more than 12 months old
    try {
        const nm = "FCCDCF";        // Match name of Funding Choices cookie
        const dm = "ekskluziva.ba";  // Match domain of Funding Choices cookie
        const pa = "/";             // Match path of Funding Choices cookie
        let tc = ('; ' + document.cookie).split('; ' + nm + '=');
        if (tc.length === 2) {
            tc = decodeURIComponent(tc.pop().split(';').shift());
            tc = JSON.parse(tc)[3][0].substring(1, 9);
            tc = Uint8Array.from(window.atob(tc), (v) => v.charCodeAt(0));
            let dt = (tc[0] * 2 ** 28) + (tc[1] * 2 ** 20) + (tc[2] * 2 ** 12) +
                (tc[3] * 2 ** 4) + (tc[4] >> 4);
            if (Date.now() / 1000 - dt / 10 > 86400 * 365)
                document.cookie = nm + "=;path=" + pa + ";domain=" + dm +
                    ";expires=" + new Date(0).toUTCString();
        }
    } finally {
    }
</script>

<script>
    // adds inside post
    $(function () {

        window.googlefc = window.googlefc || {};
        window.googlefc.ccpa = window.googlefc.ccpa || {}
        window.googlefc.callbackQueue = window.googlefc.callbackQueue || [];

        googlefc.callbackQueue.push({
            'CONSENT_DATA_READY': () => {
                switch (googlefc.getConsentStatus()) {
                    case googlefc.ConsentStatusEnum.NO_CONSENT:
                        break;
                    default:
                        const $elements = $([...document.getElementById('postContent').querySelectorAll(':scope > p')]
                            .filter(x => x.innerText.replace(/\s/g, '').length > 0 && x.children.length === 0));
                        //const $debug = $([...document.getElementById('postContent').querySelectorAll(':scope > p')]
                        //    .filter(x => x.innerText.replace(/\s/g, '').length > 0 && x.children.length === 0)
                        //    .map(x => x.innerText));
                        const adds = [{
                            id: 'div-gpt-ad-1693155446310-0',
                            slot: '/267568916/exkluziva/ex_article_inside_1',
                            sizes: [[250, 250], [300, 250], [728, 90], [200, 200], [468, 60]]
                        }, {
                            id: 'div-gpt-ad-1693155712301-0',
                            slot: '/267568916/exkluziva/ex_article_inside_2',
                            sizes: [[250, 250], [300, 250], [728, 90], [200, 200], [468, 60]]
                        }, {
                            id: 'div-gpt-ad-1693155750534-0',
                            slot: '/267568916/exkluziva/ex_article_inside_3',
                            sizes: [[250, 250], [300, 250], [728, 90], [200, 200], [468, 60]]
                        }/*, {
                            id: 'div-gpt-ad-1552706421326-0',
                            slot: '/267568916/exkluziva/ex_post_bellow_images_4',
                            sizes: [[250, 250], [300, 250], [728, 90], [200, 200], [468, 60]]
                        }, {
                            id: 'div-gpt-ad-1552706206387-0',
                            slot: '/267568916/exkluziva/ex_post_bellow_images_5',
                            sizes: [[250, 250], [300, 250], [728, 90], [200, 200], [468, 60]]
                        }*/];

                        if ($elements.length > 2) {
                            const last = $elements.length;
                            let takenAdd = 0; // first from adds
                            //console.log('Last index is ', last);
                            $elements.each(function (index, item) {
                                //console.log('We have used all adds, exit');
                                if (takenAdd > (adds.length - 1)) {
                                    return false;
                                }

                                const counter = index + 1;
                                //console.log('counter', counter);
                                if (counter > 1) {
                                    //console.log('We have more than 2 images in post, ', last);
                                    if (counter % 2 === 0) {
                                        //console.log('Image is even, check if last one is even');
                                        if (last === counter && last % 2 === 0) {
                                            //console.log('We have equal number of images, do not show last add, breaks');
                                            return false;
                                        }
                                        //console.log('Not last image, show add');

                                        //console.log('Define slot for add');
                                        googletag.cmd.push(function () {
                                            googletag.defineSlot(adds[takenAdd].slot, adds[takenAdd].sizes, adds[takenAdd].id).setCollapseEmptyDiv(true).addService(googletag.pubads());
                                        });

                                        //console.log('Create html for add', $(item));
                                        $(item).after('<p style="text-align: justify;"><div class="my-4 text-center"><div id="' + adds[takenAdd].id + '"></div></div></p>');

                                        //console.log('Diplay add');
                                        googletag.cmd.push(function () {
                                            googletag.display(adds[takenAdd].id);
                                        });

                                        // but in case that resolution is not bigger, we'll take their ad spot anyway
                                        takenAdd = takenAdd + 1;
                                    }
                                }
                            });
                        } else {
                            //console.log('No elements for showing (less than 2 pictures)');
                        }
                        break;
                }
            }
        });
    });
</script>
