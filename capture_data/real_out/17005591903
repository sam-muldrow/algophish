        function getVersion() {
            var width = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
            if (width >= 1200) {
                return 'ExtraLarge';
            } else if (width >= 992) {
                return 'Desktop';
            } else if (width >= 768) {
                return 'Tablet';
            } else if (width >= 420) {
                return 'BigMobile';
            } else {
                return 'Mobile';
            }
        }

        function isWallpaperSafe() {
            var screenWidth = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
            if (screenWidth >= 1600) {
                return 'Yes';
            } else {
                return 'No';
            }
        }

        //
        function gptrenderEnded(e) {
            if (!e.isEmpty) {
                switch (e.slot.getSlotElementId()) {
                    case "div-gpt-ad-ex_richmedia_banner":
                        if (getVersion() == 'Mobile' || getVersion() == 'BigMobile')
                            document.getElementsByClassName("mobile-afixed-ad")[0].style.display = 'block';
                        break;
                    case "div-gpt-ad-ex_wallpaper":
                        document.getElementsByClassName("wallpaper_container")[0].style.display = 'block';
                        //document.getElementsByTagName("body")[0].className = "has_skin";
                        break;
                    // TODO: ukomentariši da upališ takeover/floater
                    //case "gad_ads_takeover":
                    //document.getElementsByClassName("takeover_container")[0].style.display = 'block';
                    //break;

                    case "div-gpt-ad-ex_native_similar":
                        document.getElementsByClassName("native_ad_sim_cont")[0].style.display = 'block';
                        document.getElementById("div-gpt-ad-ex_native_similar").style.display = 'block';
                        break;

                    case "div-gpt-ad-ex_native_gallery":
                        document.getElementsByClassName("gal-placeholder")[0].style.display = 'none';
                        document.getElementsByClassName("ads_gallery")[0].style.display = 'block';
                        document.getElementById("div-gpt-ad-ex_native_gallery").style.display = 'block';
                        break;
                    default:
                        var priv = document.getElementById(e.slot.getSlotElementId());
                        if (priv != null) {
                            priv.style.display = 'block';
                            priv.parentNode.style.display = 'block';
                        }
                        break;
                }
            }


        }

