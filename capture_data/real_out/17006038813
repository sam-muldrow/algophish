<script>
	zrequire(["lib/swiper/swiper.min"], function (Swiper) {
		function initSwiper(node) {
			new Swiper(node, {
				cssWidthAndHeight: true,
				slidesPerView: 'auto',
				slidesPerGroupAuto: true,
				spaceBetween: 20,
				visibilityFullFit: true,
				autoResize: false,
				slideActiveClass: 'active-slide',
				slideToClickedSlide: true,
				mousewheel: {
					forceToAxis: true,
					releaseOnEdges: true,
					thresholdDelta: 10,
				},
				navigation: {
					nextEl: node.querySelector('.index-section__control--next'),
					prevEl: node.querySelector('.index-section__control--prev'),
					disabledClass: '_disabled',
				},
			});
		}

		document.querySelectorAll('.index-section__swiper').forEach(function(node) {
			initSwiper(node);
		});
	});
</script>
