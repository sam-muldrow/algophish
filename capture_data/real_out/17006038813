					<div class="index-map">
	<div class="index-map__title z-text--montserrat z-text--32 z-mb--36">
		Карта Москвы
	</div>
	<div class="index-map-container" data-uitest="city-map">
		<div id="cityMap" class="main-map-view"></div>
		<div class="js-zoomControl map-control hide" data-uitest="map-zoom-control-panel">
			<div class="map-control-item map-control-item-in"></div>
			<div class="map-control-item map-control-item-out"></div>
		</div>
	</div>
</div>
<script>
zrequire(["build/map/js/catalog","build/map/js/driver_yandex"], function(ZMap) {
	const formatBounds = function(bounds) {
		if (!bounds) return null;
		return { bottom: bounds[0][0], left: bounds[0][1], top: bounds[1][0], right: bounds[1][1] };
	}

	const getPoints = function(bounds){
		return zrequest.api('City.GetPoints', {
			city: 'msk',
			bounds: formatBounds(bounds),
		}).then(function (result) {
			if (!result.points) return [];
			return result.points;
		});
	};

	const initMap = function(node) {
		const map = new ZMap(node);

		map.pushOnReady(function(){
			getPoints(null).then(function(points){
				const bounds = map.getBoundsByPoints(points);

				map.createMap(map.getCenterAndZoom(bounds));
				map.updateObjects(points);
				map.initMapEvents();
				map.enableScrollZoom();

				initZoomControls(map);
			});
		});

		map.on('z-boundschange', function(bounds) {
			getPoints(bounds).then(function(points) {
				map.updateObjects(points);
			});
		});
	};

	const initZoomControls = function(map) {
		const zoomControls = document.querySelector('.js-zoomControl');
		if (!zoomControls) return;

		zoomControls.classList.remove('hide');

		zoomControls.addEventListener('click', function(e) {
			if (!e.target.closest('.map-control-item')) return;

			const zoomIn = e.target.classList.contains('map-control-item-in');
			const newZoom = map.getZoom() + (zoomIn ? 1 : -1);

			map.setZoom(newZoom);
		});
	};

	new IntersectionObserver(function (entries, observer) {
		if (!entries[0].isIntersecting) return;
		initMap(entries[0].target);
		observer.unobserve(entries[0].target);
	}).observe(document.getElementById('cityMap'));
});
</script>

		
