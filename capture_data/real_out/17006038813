			<ul class="slider-pages list-reset">
				<li class="active">1</li>
				<li>2</li>
				<li>3</li>
				<li>4</li>
			</ul>
			<div class="controls">
				<span class="btn next">Дальше</span>
			</div>
		</div>
	</div>
</div>

<script>
	requirejs(['jquery', 'lib/sly'], function($, Sly) {
		var $node = $('.js-head-thesis');
		var $slider = $node.find('.js-slider-block');
		var $sliderPages = $node.find('.slider-pages');

		var options = {
			horizontal: 1,
			itemNav: 'basic',
			smart: 1,
			activateMiddle: 1,
			activatePageOn: 'click',
			mouseDragging: 0,
			touchDragging: 1,
			releaseSwing: 1,
			startAt: 0,
			scrollBy: 0,
			slidebyone: 1,
			pagesBar: $sliderPages,
			speed: 500,
			elasticBounds: 1,
			dragHandle: 1,
			dynamicHandle: 1,
			clickBar: 1,
			cycleBy: 'items',
			cycleInterval: 12000,
			pauseOnHover: 1,
			next: $node.find('.next'),
		};

		$node.on('click', '.toStart', function(e) {
			e.preventDefault();
			slyHead.activate(0);
		});

		$node.find('.next').on('click', function(e) {
			e.preventDefault();

			if ($(this).closest('.disabled').length) {
				slyHead.activate(slyHead.rel.activePage);
			}

			if ($sliderPages.find('li:eq(3)').closest('.active').length) {
				$(this).removeClass('next').addClass('toStart');
			} else {
				$(this).removeClass('toStart').addClass('next');
			}
		});

		var slyHead = new Sly($slider, options);

		slyHead.init();

		$(window).on('resize', function(e){
			slyHead.reload();
		});
	})
</script>


