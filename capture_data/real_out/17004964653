<script>
    const radioButtons = document.querySelectorAll('input[type="radio"][name="progGroup"]');
    let currentIndex = 0;

    // Function to click the next radio button
    function clickNextRadioButton() {
        radioButtons[currentIndex].click(); // Click the current radio button
        currentIndex = (currentIndex + 1) % radioButtons.length; // Move to the next radio button
    }

    // Set an interval to click the next radio button every 3 seconds
    const intervalId = setInterval(clickNextRadioButton, 3000);

    // You can stop the interval using clearInterval if needed
    // clearInterval(intervalId);
</script>
