		<script type="text/javascript">
			
			var base64_util = (function(pub) {

				function noAtobDecode(str) {
					var keyStr = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/=";
					var retval = "", chr1, chr2, chr3 = "", enc1, enc2, enc3, enc4 = "", i = 0;
			
					// remove all characters that are not A-Z, a-z, 0-9, +, /, or =
					var base64test = /[^A-Za-z0-9\+\/\=]/g;
					if (base64test.exec(str)) {
						retval = false;
					} else {
						str = str.replace(/[^A-Za-z0-9\+\/\=]/g, "");
						do {
							enc1 = keyStr.indexOf(str.charAt(i++));
							enc2 = keyStr.indexOf(str.charAt(i++));
							enc3 = keyStr.indexOf(str.charAt(i++));
							enc4 = keyStr.indexOf(str.charAt(i++));
							chr1 = (enc1 << 2) | (enc2 >> 4);
							chr2 = ((enc2 & 15) << 4) | (enc3 >> 2);
							chr3 = ((enc3 & 3) << 6) | enc4;
			
							retval = retval + String.fromCharCode(chr1);
							if (enc3 != 64) {
								retval = retval + String.fromCharCode(chr2);
							}
			
							if (enc4 != 64) {
								retval = retval + String.fromCharCode(chr3);
							}
							chr1 = chr2 = chr3 = "", enc1 = enc2 = enc3 = enc4 = "";
			
						} while (i < str.length);
			
						retval = unescape(retval);
					}
					return retval;
				}
			
				pub.decode = function(str, URIDecode) {
					var retval = false;
					if (URIDecode == null) {
						URIDecode = false;
					}
			
					// try to use modern browsers base64 functionality
					if (window.atob) {
						retval = unescape(window.atob(str));
					} else if (!window.atob) // legacy decode for older browsers
					{
						retval = noAtobDecode(str);
					}
			
					if (URIDecode) {
						retval = decodeURIComponent(retval);
					}
					return retval;
				}
			
				return pub;
			}(base64_util || {}));
			
			var hashPrefix = '',
			    hash = window.location.hash,
			    subHash = '',
			    omxRedirUrl = 'omxRedirUrl=',
			    cmParam = 'cm_mmc=',
			    cmHashReferrer = '',
			    cmHashValue = '';
			
			if(hash.indexOf('omx') > -1) {
				hashPrefix = 'omx';
			}
			else if(hash.indexOf('reliable') > -1) {
				hashPrefix = 'reliable';
			}
			
			var decodeLegacyOmxHash = function () {
			   
			    if (hashPrefix !== '') {
			    	window.omxRequested = true;
			        subHash = base64_util.decode(hash.substring(hash.indexOf(hashPrefix) + hashPrefix.length));
			        
			        if(subHash.indexOf(omxRedirUrl) > -1){
			        	if(subHash.indexOf('&') > -1){
			        		cmHashReferrer = subHash.substring(subHash.lastIndexOf(omxRedirUrl) + omxRedirUrl.length, subHash.lastIndexOf('&'));
			        	} else {
			        		cmHashReferrer = subHash.substring(subHash.lastIndexOf(omxRedirUrl) + omxRedirUrl.length);
			        	}
			        }
			        
			        if(subHash.indexOf(cmParam) > -1){
			        	cmHashValue = subHash.substring(subHash.indexOf(cmParam) + cmParam.length);
			        }
					
					var hashObj = {
						mmc_value: cmHashValue,
						referrer: cmHashReferrer
					};
			        
			        return hashObj;
			    }
			    
			    return null;
			};

			var decodedHash = decodeLegacyOmxHash();
			
			if(decodedHash !== null){
				var	legacyOmxHash = decodedHash.mmc_value,
					legacyOmxReferrer = decodedHash.referrer;
	
