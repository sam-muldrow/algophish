			<script>
				function setReliableCookiesOnOD(data) {
				    "use strict";

				    var expDate = new Date();
					expDate.setFullYear(expDate.getFullYear() + 1);
				    var cookies = data.cookies;
				    var expectedRelCookieVal = 'Yes';
					for(var i = 0; i < cookies.length; i++) {
						if(cookies[i].success) {
							var setODcookieVal =cookies[i].value;
							if(setODcookieVal.toLowerCase() === expectedRelCookieVal.toLowerCase()){
								setODcookieVal ='true';
							}
							if ('reliable_visitor'== cookies[i].key){
								if(setODcookieVal===''){
									setODcookieVal='true';
								}
								document.cookie = cookies[i].key + '=' + setODcookieVal + '; expires=' + expDate.toGMTString() + '; path=/';
							}else if(setODcookieVal==='true'){
								document.cookie = cookies[i].key + '=' + setODcookieVal + '; expires=' + expDate.toGMTString() + '; path=/';
							}
						}
					}
				}
				
				function callReliableCookieService(relCookieKey) {
			       var script = document.createElement('script');
			       script.src = '//www.reliable.com/json/getCookie.do?key=' + relCookieKey + '&callback=setReliableCookiesOnOD';
			       document.getElementsByTagName('head')[0].appendChild(script);
				}
				
				if((document.cookie.indexOf('reliable_visitor')=== -1 || document.cookie.indexOf('REL_LOGIN')=== -1) && (window.location.hash==='#reliable' || window.location.href.indexOf('/reliable/')>-1)) {
					callReliableCookieService('reliable_visitor,REL_LOGIN');
				}
			</script>
		

		

















	
	
		
            
            
			
				
				
				
					
					
						
					
					
					
			
		
	




