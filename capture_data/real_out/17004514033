			<script type="text/javascript">
				// Ensure the legacy APIs are still available.
				window.tenantGetterQueue = window.tenantGetterQueue || [];
				window.tenantSetterQueue = window.tenantSetterQueue || [];

				// Register the Venatus task queue.
				window.ipcVenatusQueue = window.ipcVenatusQueue || [];
				window.ipcPlacementsMapping = window.ipcPlacementsMapping || {};

				( function( venatusQueue ) {
					var getDisplayWidth = function(){var e=0;if(typeof window.innerWidth!="undefined"){e=window.innerWidth}else if(typeof document.documentElement!="undefined"&&typeof document.documentElement.clientWidth!="undefined"&&document.documentElement.clientWidth!=0){e=document.documentElement.clientWidth}else{e=document.getElementsByTagName("body")[0].clientWidth}return Number(e)};
					var displayWidth = getDisplayWidth();

					var displayWidthToPlacementIds = function( contextPlacementMap ) {
						var placementIds = [];

						if ( contextPlacementMap.mobile && displayWidth < 1000 ) {
							placementIds.push( contextPlacementMap.mobile );
						}
						if ( contextPlacementMap.desktop && displayWidth >= 1000 ) {
							placementIds.push( contextPlacementMap.desktop );
						}
						if ( contextPlacementMap.all ) {
							placementIds.push( contextPlacementMap.all );
						}

						return placementIds;
					}

					var renderPlacement = function( placement ) {
						if ( 'object' === typeof placement.placement_ids && placement.div_id ) {
							displayWidthToPlacementIds( placement.placement_ids ).map( function( data ) {
								var placeholder = document.getElementById( placement.div_id );
								var element = document.createElement( 'div' );
								if ( placeholder ) {
									element.setAttribute( 'class', 'vm-placement' );
									if ( data.id ) {
										element.setAttribute( 'data-id', data.id );
									}
									if ( data.css ) {
										Object.keys( data.css ).forEach( function( prop ) {
											element.style[prop] = data.css[prop];
										} );
									}
									if( data.displaytype ) {
										element.setAttribute( 'data-display-type', data.displaytype );
									}
									placeholder.appendChild( element );
								}
							} );
						}
					}

					venatusQueue.push = function( placementConfig ) {
						if ( placementConfig ) {
							renderPlacement( placementConfig );
						}
					}

					venatusQueue.map( renderPlacement );
				} )( window.ipcVenatusQueue );
			</script>

