    $(function(){
        $('.js_timeSaleCountDown').each(function() {
            var timeSale = $(this);
            var finalDate = timeSale.data('ctcountdown');
            timeSale.countdown(finalDate, function(event) {
                let days = event.strftime('%D');
                let hours = event.strftime('%H');
                let minutes = event.strftime('%M');
                let seconds = event.strftime('%S');
                timeSale.html(days + '日' + hours + '時間' + minutes + '分' + seconds + '秒');

                // 12時間以内
                if (event.strftime('%T') < (3600 * 12)) {
                    var limitTextObj = timeSale.parents('.js_top-TimeSaleLimit_Box').children('.js_timeSaleLimit');
                    limitTextObj.text('終了間近');
                    limitTextObj.addClass('top-TimeSaleLimit_Label-end');
                }

                if (event.elapsed) {
                    setTimeout(function() {
                        limitTextObj.remove();
                        timeSale.closest('.js_timeSaleCountDown').text('終了しました');
                    }, 1000);
                }
            });
        });
    });
