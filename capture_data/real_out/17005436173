      <h2 class="r-mini-panel--title" data-label="Sport" data-url="/2020/sections/sport"><a href="/2020/sections/sport">Sport</a></h2>
        
  
  
            <r-viewmode class="r-viewmode--panel-left r-viewmode--o-panel-left">
          <i class="r-m" id="748441" data-extref="rossel_extref_Cue-8332112"></i>
    <article  class="r-viewmode--article">
<i-article><a href="/id748441/article/2023-11-20/objectif-garder-domenico-tedesco-apres-leuro" class="r-article--img">	<!-- scald=55209087:ena_manchette_optimise {"entity":{"type":"node","bundle":"package","id":"748441","vid":"748442"},"class":""} -->
