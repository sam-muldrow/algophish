<script>
    window.Zeit.renderFooterbar = function( type ) {
        window.Zeit.dispatchEvent('disableOverscrolling');

        import( 'https://wally.zeit.de/footerbar.js' ).then( ({ FooterBar }) => {
            new FooterBar({
                target: document.body,
                intro: true,
                props: {
                    type,
                    settings: {
                        contentUrl: "https://www.zeit.de/index",
                        apiUrl: "https://wally.zeit.de/api",
                        ssoUrl: "https://meine.zeit.de",
                        productId: "zede",
                        premiumHost: "https://premium.zeit.de",
                        assetHost: "https://static.zeit.de/p/zeit.web",
                    }
                }
            });
        });
    };
</script>

<script>
    function loadPiano() {
        function log() {
            location.hash === '#debug-piano' && console.debug.apply( console, arguments );
        }

        function mark(name) {
            try {
                window.performance.mark(name);
            } catch(e) {}
        }

        if ( !window.Zeit.user.adFree ) {
            tp = window.tp || [];
                        tp.push(["setUseTinypassAccounts", false]);
            tp.push(["setUsePianoIdUserProvider", false ]);
            tp.push(["setUsePianoIdLiteUserProvider", true ]);
            tp.push(["setPianoIdUrl", "https://id-eu.piano.io/"]);
            var token = window.Zeit.cookieRead("zeit_sso_piano_201501");
            if ( token ) {
                tp.push(["setExternalJWT", token]);
            }
                        tp.push(["setCustomVariable", "title", "ZEIT ONLINE Startseite"]);
            tp.push(["setCustomVariable", "uuid", "d995ba5a-a7fb-401a-bdc8-86cf2cbf0cda"]);
            tp.push(["setCustomVariable", "url", "https://www.zeit.de/index"]);
                        tp.push(["setContentSection", "homepage"]);
            tp.push(["setTags", ["open", "free"]]);
                        tp.push(["setContentCreated", "2019-06-12T10:41:20+02:00"]);
            tp.push(["setCustomVariable", "last_updated", "2023-11-19T15:07:41+01:00"]);
            tp.push(["setCustomVariable", "page_type", "centerpage"]);
            tp.push(["setCustomVariable", "device_site_version", window.Zeit.getSiteParam()]);
            tp.push(["setCustomVariable", "user_status", window.Zeit.user && window.Zeit.user.state || "none"]);
            tp.push(["setCustomVariable", "has_abo", window.Zeit.user && window.Zeit.user.hasAbo || false]);
            if ( window.Zeit.user && window.Zeit.user.ssoid ) {
                tp.push(["setCustomVariable", "sso_id", window.Zeit.user.ssoid]);
            }
            tp.push(["setCustomVariable", "paywall_status", "open"]);
            tp.push(["setCustomVariable", "access_right", "free"]);
            tp.push(["setContentIsNative", false]);

            var gateAccess;
            var gateLayout;
            var gateRendered;
            tp.push(["addHandler", "setResponseVariable", function( eventParams ) {
                var data = eventParams.responseVariables || {};
                log('Script handler fired!', data);

                if ( data.footerBar ) {
                    if (data.footerBar.indexOf( 'footer_bar' ) > -1) {
                        window.Zeit.readyWhenYouAre( function(p) {
                            window.Zeit.renderFooterbar( p );
                            })(data.footerBar);
                    }
                }
                if ( data.gateAccess ) {
                    var states = data.gateAccess.split('_').map(function(s) {
                        return s.replace('registered', 'register');
                    });

                    var state = states[0];
                    var access = states[1];

                    if (state === "free") {
                        if (['register', 'paid'].indexOf(access) > -1) {
                            gateAccess = access;
                        }
                    }
                    // hardcode type for dynamic pages
                    // if (state === 'dynamic') {
                    //     gateAccess = access = 'paid';
                    // }
                    if (state === 'dynamic' && access === 'paid') {
                                log('Show Z+ badges (pre rebrush)');
                                document.querySelectorAll('svg.zplus-logo.invisible').forEach(node => {
                                    node.classList.remove('invisible');
                                });
                            }
                        } else if (data.gateLayout) {
                    gateLayout = data.gateLayout;
                }

                }]);
            window.cX = window.cX || {};
                cX.callQueue = cX.callQueue || [];
                cX.callQueue.push(['setSiteId', "3604369443445173116"]);
                cX.callQueue.push(['invoke', function() {
                    var externalId = cX.getCookie("creid");
                    if (typeof externalId !== "undefined") {
                        cX.addExternalId({'id': externalId, 'type': "ngo"});
                    }
                }]);
                cX.callQueue.push(['sendPageViewEvent']);
            if ( typeof window.__tcfapi === 'function' ) {
                window.__tcfapi( 'addEventListener', 2, function( tcData, success ) {
                    if (success) {
                        if ( tcData.eventStatus == 'cmpuishown') {
                            tp.push(["setCustomVariable", "pur_layer", true]);
                        }
                        window.Zeit.loadJS('//experience-eu.piano.io/xbuilder/experience/load?aid=u6vAlvTEpe', function() {
                            log('Piano loaded', window.tp);
                            mark('load-piano');
                        }, false, function() {
                            log('Piano loading error.');
                            });
                    }
                    window.__tcfapi('removeEventListener', 2, (success) => {
                        if ( success ) {
                            log('Piano consent listener removed.');
                        }
                    }, tcData.listenerId);
                });
            }
            }
    }

    loadPiano();
    </script>


    <script>
    (function() {
        var ioDomain = 'data-af9f3dfb33.zeit.de',
            ioSt = 'zeitonl',
            isMobile = window.Zeit.isMobileView() || window.Zeit.isMobileApp,
            fraboTagName = isMobile ? 'mo' : 'in';

        if ( isMobile ) {
            ioDomain = 'data-614d3891ff.zeit.de';
            ioSt = 'mobzeit';
        }

        window.Zeit.loadJS('https://' + ioDomain + '/iomm/latest/manager/base/es6/bundle.js', function(){
            window.iommData = {
                st: ioSt,
                dn: ioDomain,
                cp : "homepage/bild-text",
                co: "URL: /index"};

            window.IOMm('configure', {
                st: window.iommData.st,
                dn: window.iommData.dn,
                mh: 5
            });
            window.IOMm('pageview', {
                cp: iommData.cp,
                co: iommData.co
            });
            });
    }());
</script>
<div class="navigation-backdrop" hidden></div>

<script src="https://interactive.zeit.de/g/2023/energiekostenmonitor/static/assets/dashboard.32d4e102.mjs" type="module"></script>
  <script>
  (function(){
    const webEndpoint = 'https://radio.zon.zeit.de/zeit-radio/radio-speechbert-format.json',
        appEndpoint = 'https://radio.zon.zeit.de/zeit-radio/radio-cp-format.json',
        radioCoverSource = 'https://img.zeit.de/static/assets/radio-cover',
        container = document.querySelector( '.zoner-radio-audio' ),
        infoContainer = document.querySelector( '.zoner-radio-info' ),
        dropdown = infoContainer.querySelector( '.zoner-radio-dropdown-content' ),
        dropdownButton = infoContainer.querySelector( '.zoner-radio-dropdown-button' ),
        dropdownArticleLink = dropdown.querySelector( '.zoner-radio-weblink-article > a' ),
        dropdownPodcastLink = dropdown.querySelector( '.zoner-radio-weblink-podcast > a' ),
        dropdownAppPromotionLink = dropdown.querySelector( '.zoner-radio-app-promotion-link' ),
        dropdownBookmarkButton = dropdown.querySelector( '.zoner-radio-bookmark-button' ),
        metaContainer = document.querySelector( '.zoner-radio-meta' ),
        audioElem = container.querySelector( '#zoner-radio-audioelem' ),
        playerSection = container.querySelector( '.zoner-radio-player' ),
        playButton = container.querySelector( '.zoner-radio-play' ),
        playIcon = playButton.querySelector( '.zoner-radio-playicon' ),
        pauseIcon = playButton.querySelector( '.zoner-radio-pauseicon' ),
        appPlayingIcon = container.querySelector( '.zoner-radio-app-playing' ),
        skipFwdButton = container.querySelector( '.zoner-radio-skipfwd' ),
        controls = container.querySelector( '.zoner-radio-controls' ),
        cta = container.querySelector( '.zoner-radio-cta' ),
        scrubbarElem = container.querySelector( '.zoner-radio-scrubbar' ),
        progressElem = container.querySelector( '.zoner-radio-scrubbar-played' ),
        kickerElem = metaContainer.querySelector( '.zoner-radio-kicker' ),
        titleElem = metaContainer.querySelector( '.zoner-radio-title' ),
        remainingTimeElem = container.querySelector( '.zoner-radio-timestamp span' );

    const podcastList = {
        'Was jetzt?': {
            'url': 'https://www.zeit.de/serie/was-jetzt',
            'description': 'Der Nachrichtenpodcast (Mo-Sa 2x t√§glich, So 1x)'
        },
        'Alles gesagt?': {
            'url': 'https://www.zeit.de/serie/alles-gesagt',
            'description': 'Der unendliche Interviewpodcast (monatlich)'
        },
        'Das Politikteil': {
            'url': 'https://www.zeit.de/serie/das-politikteil',
            'description': 'Der Politikpodcast (freitags)'
        },
        'Ehrlich jetzt?': {
            'url': 'https://www.zeit.de/serie/ehrlich-jetzt',
            'description': 'Der Politik-Talk mit Yasmine M\'Barek (Di, alle zwei Wochen)'
        },
        'Pegasus ‚Äì der Spion in unseren Handys': {
            'url': 'https://www.zeit.de/serie/pegasus-der-spion-in-unseren-handys',
            'description': 'Investigativ-Podcast'
        },
        'Auch das noch?': {
            'url': 'https://www.zeit.de/serie/auch-das-noch-krisenpodcast',
            'description': 'Der freundliche Krisenpodcast (alle zwei Wochen, Mi)'
        },
        'Geld Macht Katar': {
            'url': 'https://www.zeit.de/serie/geld-macht-katar',
            'description': 'Eine Podcastserie in acht Teilen (Di)'
        },
        'Verbrechen': {
            'url': 'https://www.zeit.de/serie/verbrechen',
            'description': 'Der Kriminalpodcast (alle zwei Wochen, Di)'
        },
        'Die sogenannte Gegenwart': {
            'url': 'https://www.zeit.de/serie/die-sogenannte-gegenwart',
            'description': 'Der Feuilleton-Podcast (alle zwei Wochen, Mo)'
        },
        'Ist das eine Blase?': {
            'url': 'https://www.zeit.de/serie/ist-das-eine-blase',
            'description': 'Der Podcast f√ºr alle, die Wirtschaft verstehen wollen (alle zwei Wochen, Mo)'
        },
        'Kicken kann er': {
            'url': 'https://www.zeit.de/serie/kicken-kann-er',
            'description': 'Der Fu√üball-Podcast (alle zwei Wochen, Mi)'
        },
        'Geht da noch was?': {
            'url': 'https://www.zeit.de/serie/geht-da-noch-was',
            'description': 'Ein Podcast, der das Leben leichter macht. (alle zwei Wochen, Mo)'
        },
        'Wie war das noch mal?': {
            'url': 'https://www.zeit.de/serie/wie-war-das-noch-mal',
            'description': 'Der Podcast von ZEIT Geschichte (monatlich, Sa)'
        },
        'Augen zu': {
            'url': 'https://www.zeit.de/serie/augen-zu',
            'description': 'Der Kunstpodcast (monatlich)'
        },
        'OK, America?': {
            'url': 'https://www.zeit.de/serie/ok-america',
            'description': 'Der US-Podcast (alle zwei Wochen, Do)'
        },
        'Und was machst du am Wochenende?': {
            'url': 'https://www.zeit.de/serie/und-was-machst-du-am-wochenende',
            'description': 'Der Podcast f√ºr die k√ºrzesten Tage der Woche (Do)'
        },
        'Ist das normal?': {
            'url': 'https://www.zeit.de/serie/ist-das-normal',
            'description': 'Der Sexpodcast (alle zwei Wochen, Mo)'
        },
        'Unter Pfarrerst√∂chtern': {
            'url': 'https://www.zeit.de/serie/unter-pfarrerstoechtern',
            'description': 'Der Bibelpodcast (alle zwei Wochen, Fr)'
        },
        'Servus.Gr√ºezi.Hallo.': {
            'url': 'https://www.zeit.de/serie/servus-gruezi-hallo',
            'description': 'Der transalpine Politikpodcast (mittwochs)'
        },
        'Was Chefinnen wirklich denken': {
            'url': 'https://www.zeit.de/serie/was-chefinnen-wirklich-denken-podcast',
            'description': 'Der Arbeitspodcast (alle zwei Wochen, Di)'
        },
        'Frisch an die Arbeit': {
            'url': 'https://www.zeit.de/serie/frisch-an-die-arbeit',
            'description': 'Der Arbeitspodcast (alle zwei Wochen, Di)'
        },
        'Und was macht die Uni?': {
            'url': 'https://www.zeit.de/serie/und-was-macht-die-uni',
            'description': 'Der Podcast von ZEIT Campus (monatlich)'
        },
        'Woher wei√üt Du das?': {
            'url': 'https://www.zeit.de/serie/woher-weisst-du-das',
            'description': 'Der ZEIT-WISSEN-Podcast (alle zwei Wochen, So)'
        },
        'ZEIT B√ºhne': {
            'url': 'https://www.zeit.de/serie/zeitbuehne',
            'description': 'Der Live-Interviewpodcast (monatlich)'
        },
        'Warum denken Sie das?': {
            'url': 'https://www.zeit.de/serie/warum-denken-sie-das',
            'description': 'Zwei Menschen. Zwei Meinungen. Und der Versuch, einander zu verstehen (monatlich)'
        },
        'Die Patrioten': {
            'url': 'https://www.zeit.de/politik/ausland/2021-12/die-patrioten-podcast',
            'description': 'Geschichten aus einem zersplitterten Amerika (7 Folgen, f√ºr Abonnenten)'
        },
        'Die Schaulustigen': {
            'url': 'https://www.zeit.de/serie/die-schaulustigen',
            'description': 'Der Fernsehpodcast'
        },
        'Rice and Shine': {
            'url': 'https://www.zeit.de/serie/rice-and-shine',
            'description': 'Der Podcast f√ºr vietdeutsche Perspektiven (monatlich)'
        },
        'Wird das was?': {
            'url': 'https://www.zeit.de/serie/wird-das-was',
            'description': 'Der Digitalpodcast'
        },
        'Wie war das im Osten?': {
            'url': 'https://www.zeit.de/serie/wie-war-das-im-osten',
            'description': 'Der DDR-Podcast'
        }
    };

    let playlist,
        blockIndex = 0,
        itemIndex = 0,
        isPlaying = false,
        dropdownIsOpen = false,
        progressAnim,
        currentAudioWebUrl;

    function createTimestring( seconds ) {
        if ( !isNaN( seconds ) ) {
            let hours = Math.floor( seconds / 3600 );
            let remainingSecs = seconds - hours * 3600;
            let minutes = Math.floor( remainingSecs / 60 );
            let minutesString = minutes.toString();
            if ( minutesString.length <= 1 ) {
                minutesString = '0' + minutesString;
            }
            remainingSecs = remainingSecs % 60;
            let secondsString = remainingSecs.toString();
            if ( secondsString.length <= 1 ) {
                secondsString = '0' + secondsString;
            }
            if ( hours > 0 ) {
                return hours + ':' + minutesString + ':' + secondsString;
            } else {
                return minutesString + ':' + secondsString;
            }
        } else {
            return '00:00';
        }
    }

    function isCompatibleApp() {
        if ( window.Zeit.isMobileApp ) {
            // Starting in ZONApp/iOS/2.2.3 and ZONApp/Android/2.2.2 the ZON App provides the complete radio playlist functionality.
            var patchVersion;
            if ( window.Zeit.wrapped?.client === 'ios' ) {
                patchVersion = 3;
            } else if ( window.Zeit.wrapped.client === 'android' ) {
                patchVersion = 2;
            } else {
                return true;
            }

            var version = window.Zeit.wrapped.version;
            if ( Number( version.major ) > 2 ) {
                return true;
            } else if ( Number( version.major ) < 2 ) {
                return false;
            }
            if ( Number( version.minor ) < 2 ) {
                return false;
            } else if ( Number( version.minor ) > 2 ) {
                return true;
            }

            if ( Number( version.patch ) < patchVersion ) {
                return false;
            }
            return true;
        } else {
            return false;
        }

    }

    let dispatch = ( type, item )=>{
        let event = {
            type: type,
            url: item.url,
            link: item.webUrl ? item.webUrl : null,
            seriesUrl: currentAudioWebUrl,
            uuid: item.uuid,
            paused: isPlaying ? false : true,
            currentTime: null,
            duration: null,
            playbackRate: null,
            supertitle: item.supertitle,
            title: item.title,
            poster: radioCoverSource,
            color: 'E6EBFF',
            playlist: {
                type: 'remote-cp',
                url: appEndpoint
            }
        };
        console.log( event );
        // iOS
        window.webkit?.messageHandlers?.audioPlayerHandler?.postMessage( event );
        // Andoid
        window.appInterface?.handleAudioPlayerEvent?.( JSON.stringify( event ) );
    };

    async function fetchPlaylist() {
        let response = await fetch( webEndpoint );
        let feed = await response.json();
        return feed;
    }

    const normalizeString = ( str ) => {
        // Remove leading/trailing special characters and whitespace
        return str.replace( /^[^a-zA-Z0-9√§√∂√º√ü,.;?!]+|[^a-zA-Z0-9√§√∂√º√ü,.;?!]+$/g, '' ).trim();
    };

    const findMatchingPodcast = ( itemType, str ) => {
        if ( itemType !== 'PODCAST_EPISODE' ) {
            return null;
        }
        str = normalizeString( str );

        for ( const podcastName in podcastList ) {
            const normalizedPodcastName = normalizeString( podcastName );
            if ( normalizedPodcastName.toLowerCase() === str.toLowerCase() ) {
                return podcastList[ podcastName ];
            }
        }

        return null; // No match found
    };

    let initWebRadio = ()=>{
        cta.innerHTML = 'Audio l√§dt';
        fetchPlaylist().then( ( feed )=>{
            playlist = feed;
            track( 'init_web_radio' );
            setCurrentAudio( playlist.blocks[ 0 ], 0 );
            if ( !container.classList.contains( 'init' ) ) {
                container.classList.add( 'init' );
            }
        });
    };

    let initNativeRadio = ()=>{
        fetchPlaylist().then( ( feed )=>{
            playlist = feed;
            track( 'init_native_radio' );
            let matchingPodcast = findMatchingPodcast( playlist.blocks[ 0 ].items[ 0 ].type, playlist.blocks[ 0 ].title );
            if ( matchingPodcast ) {
                currentAudioWebUrl = matchingPodcast.url;
            }
            dispatch( 'play', playlist.blocks[ 0 ].items[ 0 ]);
            showAppPlaying();
        });
    };

    let showAppPlaying = ()=>{
        controls.classList.add( 'native-player-active' );
        togglePlayPauseIcons( 'app-play' );
        cta.innerHTML = 'Wird abgespielt';
    };

    let stopPlayingAnimation = ()=>{
        document.querySelectorAll( '.zoner-playing-animation-bar' ).forEach( function( elem ) {
            elem.classList.remove( 'zoner-animation-active' );
        });
        cta.innerHTML = 'Pausiert';
    };

    let restartPlayingAnimation = ()=>{
        document.querySelectorAll( '.zoner-playing-animation-bar' ).forEach( function( elem ) {
            elem.classList.add( 'zoner-animation-active' );
        });
        cta.innerHTML = 'Wird abgespielt';
    };

    // sniff user agent to determine if we are on iOS or Android
    let isIOS = ()=>{
        return /iPad|iPhone|iPod/.test( navigator.userAgent ) && !window.MSStream;
    };

    let isAndroid = ()=>{
        return /Android/.test( navigator.userAgent );
    };

    let track = ( name )=>{
        let data = {
            linkId: appEndpoint,
            customClickParameter: {
                9: name
            }
        };

        if ( playlist && playlist.blocks ) {
            data.customClickParameter[ 6 ] = 'block_' + ( blockIndex + 1 );
            data.customClickParameter[ 7 ]  = 'item_' + ( itemIndex + 1 );
            data.customClickParameter[ 8 ] = audioElem.currentTime.toString();
        }

        if ( window.Zeit ) {
            window.Zeit.dispatchEvent( 'track', playButton, data );
        }
    };

    // listen for native app events
    // if time update is received and playlist.url === appEndpoint then we know that the native app is playing the radio
    // if pause event is received we should at least stop the playing animation
    window.addEventListener( 'audioplayer', function( e ) {
        if ( e.detail.type === 'timeupdate' && e.detail?.playlist?.url === appEndpoint ) {
            showAppPlaying();
        } else if ( e.detail.type === 'pause' && controls.classList.contains( 'native-player-active' ) ) {
            stopPlayingAnimation();
        } else if ( e.detail.type === 'play' && e.detail?.playlist?.url === appEndpoint ) {
            restartPlayingAnimation();
        }
    });

    let handlePlayButtonClick = ()=>{
        if ( isCompatibleApp() ) {
            initNativeRadio();
            // remove event listener from play button since the app player takes over from here
            playerSection.removeEventListener( 'click', handlePlayButtonClick );
            return;
        }
        if ( !container.classList.contains( 'init' ) ) {
            playerSection.removeEventListener( 'click', handlePlayButtonClick );
            playButton.addEventListener( 'click', handlePlayButtonClick );
            initWebRadio();
        } else {
            //toggle play functionality
            if ( audioElem.paused ) {
                playAudio();
            } else {
                pauseAudio( 'pause' );
            }
        }
    };

    playerSection.addEventListener( 'click', handlePlayButtonClick );


    if ( isCompatibleApp() ) {
        dropdownButton.style.display = 'none';
    } else {
        dropdownButton.addEventListener( 'click', function() {
            dropdownIsOpen = !dropdownIsOpen;
            dropdown.classList.toggle( 'show' );
        });
        // event listener to close dropdown if clicked outside of it
        document.addEventListener( 'click', function( event ) {
            if ( dropdownIsOpen && !dropdown.contains( event.target ) && !dropdownButton.contains( event.target ) ) {
                dropdownIsOpen = false;
                dropdown.classList.remove( 'show' );
            }
        });
    }

    skipFwdButton.addEventListener( 'click', ()=>{
        skipFwd( 'skip' );
    });

    audioElem.addEventListener( 'ended', ()=>{
        skipFwd( 'auto_skip' );
    });

    let togglePlayPauseIcons = ( action )=>{
        if ( action === 'play' ) {
            playIcon.style.display = 'none';
            pauseIcon.style.display = 'flex';
        } else if ( action === 'pause' ) {
            playIcon.style.display = 'flex';
            pauseIcon.style.display = 'none';
        } else if ( action === 'app-play' ) {
            playIcon.style.display = 'none';
            pauseIcon.style.display = 'none';
            appPlayingIcon.style.display = 'flex';
        }
    };


    // if the the given url includes the GET parameter source=webplayer we replace the value with "zeitradio_web"
    let replacePodigeeSource = ( url )=>{
        let urlObj = new URL( url );
        urlObj.searchParams.set( 'source', 'zeitradio_web' );
        return urlObj.toString();
    };


    let setCurrentAudio = ( block, itemIndex )=>{
        let item = block.items[ itemIndex ];
        let podcast = findMatchingPodcast( item.type, block.title );
        pauseAudio( 'auto_pause' );
        if ( podcast ) {
            dropdownPodcastLink.setAttribute( 'href', podcast.url );
            dropdownPodcastLink.parentNode.style.display = 'inline-flex';
        } else {
            dropdownPodcastLink.parentNode.style.display = 'none';
        }

        if ( item.type === 'FULL_TTS' || item.type === 'PREVIEW_TTS' ) {
            dropdownBookmarkButton.setAttribute( 'value', item.uuid );
            dropdownBookmarkButton.parentNode.style.display = 'inline-flex';
            dropdownArticleLink.setAttribute( 'href', item.webUrl );
            dropdownArticleLink.parentNode.style.display = 'inline-flex';
        } else {
            dropdownBookmarkButton.parentNode.style.display = 'none';
            dropdownArticleLink.parentNode.style.display = 'none';
        }

        if ( item.type === 'PODCAST_EPISODE' ) {
            item.url = replacePodigeeSource( item.url );
        }

        audioElem.src = item.url;
        kickerElem.innerText = item.supertitle ? item.supertitle : 'ZEIT Radio';
        titleElem.innerText = item.title ? item.title : 'Moderation';
        audioElem.load();
        playAudio();
    };

    let playAudio = ()=>{
        audioElem.play().catch( ( e )=>{
            console.log( e );
        });
        togglePlayPauseIcons( 'play' );
        isPlaying = true;
        progressAnim = requestAnimationFrame( animateProgressBar );
        track( 'play' );
    };

    let pauseAudio = ( mode )=>{
        audioElem.pause();
        togglePlayPauseIcons( 'pause' );
        isPlaying = false;
        cancelAnimationFrame( progressAnim );
        track( mode ? mode : 'pause' );
    };

    let skipFwd = ( skipMode )=>{
        //if there are still items in the current block, play the next item
        if ( itemIndex < playlist.blocks[ blockIndex ].items.length - 1 ) {
            itemIndex++;
        } else {
            //else if there are no more items left, skip to the next block
            if ( blockIndex < playlist.blocks.length - 1 ) {
            //if there are still blocks left, skip to the next block and go to the first item in the block
                blockIndex++;
                itemIndex = 0;
            } else {
                pauseAudio( 'playlist_ended' );
                return;
            }
        }
        setCurrentAudio( playlist.blocks[ blockIndex ], itemIndex );
        if ( skipMode ) {
            track( skipMode );
        }
    };

    let animateProgressBar = () => {
        var progress = Math.floor( audioElem.currentTime );
        var percent = progress / audioElem.duration;
        progressElem.style.width = percent * 100 + '%';
        remainingTimeElem.innerText = createTimestring( Math.floor( audioElem.duration - progress ) );

        progressAnim = requestAnimationFrame( animateProgressBar );
    };

    scrubbarElem.addEventListener( 'click', ( e ) => {
        var rect = scrubbarElem.getBoundingClientRect();
        var seekTime = Math.floor(
            ( ( e.clientX - rect.left ) / rect.width ) * audioElem.duration
        );
        audioElem.currentTime = seekTime;
        animateProgressBar();
    });

    if ( isIOS() ) {
        dropdownAppPromotionLink.setAttribute( 'href', 'https://apps.apple.com/de/app/zeit-online/id828889166' );
    } else if ( isAndroid() ) {
        dropdownAppPromotionLink
            .setAttribute( 'href', 'https://play.google.com/store/apps/details?id=de.zeit.online&referrer=zonappclid%3Dzeitradio' );
    }
  })();
  </script>

<script>
window.onload = function (){
    var regionElem = document.querySelector('.zoner-replace-parkett-header').parentNode;
    var headerElem = regionElem.querySelector('.headed-meta');
    headerElem.style.backgroundImage = "url('https://img.zeit.de/geld/wip/visuals/header-parkett/original')";
}();
</script>

</body>
</html>