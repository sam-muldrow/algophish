<section class="cp-area cp-area--headed-wirtschaftswoche" data-ct-context="headed-wirtschaftswoche"     data-vt-parameter="cp_area_impression.headed-wirtschaftswoche.cp_area_headed-wirtschaftswoche.." data-vt-link-id="#cp_area">
