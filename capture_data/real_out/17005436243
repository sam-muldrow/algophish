<script defer type="text/javascript">
    const entrySelector = '.swiper-wrapper.hero-main-entry';
    slideOptions.forEach(slideOption => {
        const slideWrapperElement = $('<div></div>');
        slideWrapperElement.addClass('swiper-slide')
        slideWrapperElement.css({
            backgroundImage: `url('${slideOption.slideImage}')`,
            overflow: 'hidden',
            backgroundRepeat: 'no-repeat',
            backgroundPosition: 'center center',
            backgroundSize: 'cover',
        })
        $(entrySelector).append(slideWrapperElement)

        const slideLinkElement = slideOption.linkUrl ? $(`<a href="${slideOption.linkUrl}" style="text-decoration: none;"> </a>`) : $('<div style="height: 100%;"></div>');
        slideWrapperElement.append(slideLinkElement)

        slideLinkElement.append(`
          <div class="hero-container">
            <div class="slide-content-wrapper">
              <div class="slide-content-inner">
                <h2 class="slide-header" style="color: ${slideOption.contentHeaderColor};"> ${slideOption.contentHeaderText} </h2>
                <p class="slide-content-text" style="color: ${slideOption.contentTextColor}"> ${slideOption.contentText} </p>
                <div
                    class="slide-button"
                    style="background-color: ${slideOption.buttonBackgroundColor};
                    color: ${slideOption.buttonTextColor};
                    border: ${(slideOption.buttonOutlineWidth ? slideOption.buttonOutlineWidth : 0)}px solid ${(slideOption.buttonOutlineColor ? slideOption.buttonOutlineColor : 'white')};">
                      ${slideOption.buttonText}
                </div>
              </div>
            </div>
          </div>
        `)
    })

    e14.func.queueScripts.add(function () {
        const script = document.createElement('script');
        script.onload = function () {
            const swiper = new Swiper('.swiper', {
                // Optional parameters
                loop: true,
                centeredSlides: true,
                spaceBetween: 0,
                slidesPerView: "auto",
                // If we need pagination
                pagination: {
                    el: '.swiper-pagination',
                    clickable: true,
                    renderBullet: function (index, className) {
                        return '<span class="' + className + '"></span>'
                    }
                },
                // Navigation arrows
                navigation: {
                    nextEl: '.swiper-button-next',
                    prevEl: '.swiper-button-prev',
                },
                // And if we need scrollbar
                scrollbar: {
                    el: '.swiper-scrollbar',
                },
                autoplay: {
                    delay: 5000,
                },
            });
        };
        script.src = "https://community.element14.com/e14/cfs/e14core/scripts/swiper/swiper-bundle.min.js"

        document.head.appendChild(script);
    })
</script><div style="clear:both;"></div>    </div></div>
