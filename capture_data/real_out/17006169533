    $(function(){
        let _width_facebook=$(".player-facebook").width();
        let _height_facebook=(_width_facebook/16)*9;

        console.log(_width_facebook);
        console.log(_height_facebook);
        $(".player-facebook iframe").attr("height",_height_facebook);
        $(".player-facebook iframe").attr("width","100%");

        let _width_patrocinado=$(".player-contenido-patrocinado").width();
        let _height_patrocinado=(_width_patrocinado/16)*9;

        $(".player-contenido-patrocinado iframe").attr("height",_height_patrocinado);
        $(".player-contenido-patrocinado iframe").attr("width","100%");

    })
</script>