<script language="JavaScript" type="text/javascript">
/*<![CDATA[*/
function news (radioValue) {
	document.getElementById('news_tab').className='active';
	document.getElementById('updates_tab').className='';
	document.getElementById('news').style.display='block';
	document.getElementById('updates').style.display='none';
}
function updates () {
	//document.getElementById('news_tab').className='';
	document.getElementById('updates_tab').className='active';
	//document.getElementById('news').style.display='none';
	document.getElementById('updates').style.display='block';
}
updates ();

