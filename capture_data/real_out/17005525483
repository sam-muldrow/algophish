    $(document).ready(function() {
    		$('img').each(function(key,ele) {
					src = $(ele).attr('src');
					$(this).removeAttr('src');
					$(this).attr('data-src',src);
					$(this).addClass('lazyload');
				});
    		lazyload();
		});
</script> 
