    <script type="text/javascript">
        //è·åcookie
        var getCookie = function(name){
            var cookieValue = '';
            if (document.cookie && document.cookie != '') {
                var cookies = document.cookie.split(';');
                for (var i = 0; i < cookies.length; i++) {
                    var cookie = cookies[i].replace(/(^\s*)|(\s*$)/g, "");

                    if (cookie.substring(0, name.length + 1) == (name + '=')) {
                        cookieValue = decodeURIComponent(cookie.substring(name.length + 1));
                        break;
                    }
                }
            }
            return cookieValue;
        };
        var unsetCookie = function (key, options) {
            if (key && getCookie(key)) {
                var value = null;
                options.expires = -1;
                var expires_date = new Date();
                expires_date.setMinutes(new Date().getMinutes() + options.expires);

                return (document.cookie = [
                    encodeURIComponent(key), '=', value,
                    expires_date ? '; expires=' + (expires_date.toUTCString()) : '', // use expires attribute, max-age is not supported by IE
                    options.path ? '; path=' + options.path : '',
                    options.domain ? '; domain=' + options.domain : '',
                    options.secure ? '; secure' : ''
                ].join(''));
            }
        }

        var _data = {};

        function login_succ() {
            //QQç»å½æååï¼éè¦æå¾®ä¿¡çcookieæ¸é¤ï¼å¹¶æufè®¾ç½®ä¸º0ï¼é²æ­¢åæ¶å­å¨QQåå¾®ä¿¡ä¸¤ç§ç»å½æ
            var cookie_options = {
                domain: 'weiyun.com',
                path: '/'
            };
            $.each(['wx_login_ticket', 'FTN5K', 'indep', 'openid', 'key_type', 'access_token', 'wy_uf', 'wy_appid'], function (i, key) {
                unsetCookie(key, cookie_options);
            });

            location.href = '//www.weiyun.com/disk';
        }

        window.addEventListener("message", function(event){
            try {
                var data = event.data;
                var origin = event.origin || event.originalEvent.origin;
                //éªè¯æ¥æº
                if(origin !== 'https://www.weiyun.com' && origin !== 'http://www.weiyun.com') {
                    return;
                }
                //å¶å®çmessageä¸å¤ç
                if(data && data.action === 'qq_login_ok' || data === 'qq_login_ok') { //IE9ä¼ éçæ¯å­ç¬¦ä¸²
                    location.href = '//www.weiyun.com/disk';
                } else if(data && data.action === 'wx_login_ok' || data === 'wx_login_ok') {
                    location.href = '//www.weiyun.com/disk';
                }
            }catch(e) {

            }
        }, false);

        function set_login_iframe() {
            var login_container = document.getElementById('login'),
                ptlogin_url = location.protocol === 'https:' ? 'https://ssl.xui.ptlogin2.weiyun.com/cgi-bin/xlogin' : 'https://ssl.xui.ptlogin2.weiyun.com/cgi-bin/xlogin',
                wxlogin_url = location.protocol === 'https:' ? 'https://user.weiyun.com/newcgi/web_wx_login.fcg': 'http://web2.cgi.weiyun.com/web_wx_login.fcg',
                s_url = encodeURIComponent(location.protocol) + '%2F%2Fwww.weiyun.com%2Fweb%2Fcallback%2Fcommon_qq_login_ok.html%3Flogin_succ',
                qq_login_querystr = '?appid=527020901&daid=372&low_login=0&qlogin_auto_login=1&s_url=' + s_url + '&style=20&hide_title=1&target=self&link_target=blank&hide_close_icon=1&pt_no_auth=1',
                wx_login_querystr = '?cmd=web_login&no_referer=1';

            var qq_iframe = create_iframe('qq_login_iframe', ptlogin_url + qq_login_querystr),
                wx_iframe =  create_iframe('wx_login_iframe', wxlogin_url + wx_login_querystr);
            wx_iframe.style.display = 'none';
            login_container.appendChild(qq_iframe);
            login_container.appendChild(wx_iframe);
        }

        function create_iframe(id, src_url) {
            var iframe = document.createElement('iframe');
            iframe.src = src_url;
            iframe.frameBorder = 'no';
            iframe.scrolling = 'no';
            iframe.height = '100%';
            iframe.width = '100%';
            iframe.id = id;
            return iframe;
        }

        set_login_iframe();

        function set_full_year() {
            var year = 2015,
                    $this_year = $('#this_year');
            if(new Date().getFullYear() > year) {
                year = new Date().getFullYear();
                $this_year.text(year);
            }
        }

        function autoLogin(auto) {
            if(!auto) {
                var qqsrc = $('#qq_login_iframe').attr('src');
                qqsrc = qqsrc.replace('&low_login=1&low_login_foreced=1', '');
                $('#qq_login_iframe').attr('src', qqsrc);

                var wxsrc = $('#wx_login_iframe').attr('src');
                wxsrc = wxsrc.replace('&low_login=1', '');
                $('#wx_login_iframe').attr('src', wxsrc);
            } else {
                var qqsrc = $('#qq_login_iframe').attr('src');
                if(qqsrc.indexOf('low_login=1') < 0) {
                    qqsrc = qqsrc + '&low_login=1&low_login_foreced=1';
                    $('#qq_login_iframe').attr('src', qqsrc);
                }
                var wxsrc = $('#wx_login_iframe').attr('src');
                if(wxsrc.indexOf('low_login=1') < 0) {
                    wxsrc = wxsrc + '&low_login=1';
                    $('#wx_login_iframe').attr('src', wxsrc);
                }
            }
        }

        set_full_year();

        // è´¦å·ç­å°è·³è½¬ç»å½é¡µï¼ç»å½é¡µç»æç¤º
        function getQueryString(name) {
            var reg = new RegExp('(^|&)' + name + '=([^&]*)(&|$)', 'i');
            var r = window.location.search.substr(1).match(reg);
            if (r != null) {
                return unescape(r[2]);
            }
            return null;
        }
        function detectErrorToast() {
            var code = +getQueryString('code')
            if (code === 190074) {
                $('.mod-msg .msg-inner').addClass('show')
                $('.mod-msg span').text('(190074)åå®¹å¯è½è¿åç¸å³æ³è§')
                setTimeout(function (){
                    $('.mod-msg .msg-inner').removeClass('show').addClass('hide')
                }, 10000)
            }
        }
        detectErrorToast()

        window.minWidth = 1194;
        var clientWidth = document.body.clientWidth;
        var realWidth = clientWidth > window.minWidth ? clientWidth : window.minWidth;
        $(".wrapper").width(realWidth);

        var itemNum = $(".wy-content-ul").children().length;
        var ulWidth = realWidth * itemNum;
        $(".wy-content-item").width(realWidth);
        $(".wy-content-ul").width(ulWidth);

        var curMod = 'qq',
            loginSwitch = $('.tab-wrapper'),
            agreeBtn = $('.j-agree-switch'),
            autoLoginBtn = $('.j-autologin-btn'),
            agreeMask = $('.j-agree-mask'),
            qqIframe = $('#qq_login_iframe'),
            wxIframe = $('#wx_login_iframe');
        loginSwitch.on('click', '[data-mod]', function(e) {
            e.preventDefault();
            var $target = $(e.target).closest('[data-mod]');
            var mod = $target.attr('data-mod');
            curMod = mod;
            if(mod == 'qq') {
                $target.addClass('checked');
                $('.choose-wx-wrap').removeClass('checked');
                qqIframe.show();
                wxIframe.hide();
                agreeMask.hide();
                agreeBtn.find(".mod-check").addClass('act');
                //autoLoginBtn.find(".mod-check").removeClass('act');
                //autoLogin(false);
            } else {
                $target.addClass('checked');
                $('.choose-qq-wrap').removeClass('checked');
                qqIframe.hide();
                wxIframe.show();
                agreeMask.hide();
                agreeBtn.find(".mod-check").addClass('act');
                //autoLoginBtn.find(".mod-check").removeClass('act');
                //autoLogin(false);
            }
        });

        loginSwitch.on('mouseover', '[data-mod]', function(e) {
            e.preventDefault();
            var $target = $(e.target).closest('[data-mod]');
            var mod = $target.attr('data-mod');
            if(mod == 'qq') {
                $target.addClass('checked');
                $('.choose-wx-wrap').removeClass('checked');
            } else {
                $target.addClass('checked');
                $('.choose-qq-wrap').removeClass('checked');
            }
        });

        loginSwitch.on('mouseout', '[data-mod]', function(e) {
            e.preventDefault();
            if(curMod == 'qq') {
                $('.choose-qq-wrap').addClass('checked');
                $('.choose-wx-wrap').removeClass('checked');
            } else {
                $('.choose-wx-wrap').addClass('checked');
                $('.choose-qq-wrap').removeClass('checked');
            }
        });

        $(document.body).on('click', '[data-global-action=agreement]', function(e){
            var $dom = $(this),
                action = $dom.attr('data-global-action'), type, url;

            var is_agree = $dom.parent().hasClass('act'),
                $mask = $('.j-agree-mask');

            if(is_agree) {
                $mask.show();
            } else {
                $mask.hide();
            }
            $dom.parent().toggleClass('act');
        });

        $(document.body).on('click', '[data-global-action=autoLogin]', function(e){
            var $dom = $(this),
                action = $dom.attr('data-global-action'), type, url;

            var has_auto = $dom.hasClass('act'),
                $mask = $('.j-agree-mask');

            if(has_auto) {
                autoLogin(false);
            } else {
                autoLogin(true);
            }
            $dom.toggleClass('act');
        });

        var  hasShowHeader = false;
        $(window).on('scroll', function(e) {
            var scrollTop = document.documentElement.scrollTop || document.body.scrollTop;
            if(scrollTop > 850 && !hasShowHeader) {
                $(document.body).addClass('show-header');
                hasShowHeader = true;
            } else if(scrollTop <= 850) {
                $(document.body).removeClass('show-header');
                hasShowHeader = false;
            }
        });

        $('#loginBtn').on('click', function(e) {
            window.scrollTo(0, 0);
        });

        if($.browser.webkit) {
            var videoSrc = '//qzonestyle.gtimg.cn/qz-proj/wy-pc-v3/static/img/web/' + ($.browser.chrome ? 'top.webm' : 'top.MP4');
            $('#video').attr('src', videoSrc);
        }

    </script>

<script type="text/javascript">
    if($.browser.msie) {
        $('.part-stable').addClass('part-stable-move');
        $('.part-save').addClass('part-save-move');
        $('.part-online').addClass('part-online-move');
        $('.part-secrect').addClass('part-secrect-move');
    } else {
        $(document).on('scroll',function(){
            if(($('.part-stable').offset().top - $(document).scrollTop()) < 700){
                $('.part-stable').addClass('part-stable-move');
            }

            if(($('.part-save').offset().top - $(document).scrollTop()) < 700){
                $('.part-save').addClass('part-save-move');
            }
            if(($('.part-online').offset().top - $(document).scrollTop()) < 700){
                $('.part-online').addClass('part-online-move');
            }

            if(($('.part-secrect').offset().top - $(document).scrollTop()) <700){
                $('.part-secrect').addClass('part-secrect-move');
            }
        });
    }

    //éèè¯·æ±ä¸ä¸è¿ä¸ªååï¼è§£å³æµéä½è§¦åstgw çå¼æ­¥ L5å¯¼è´ç504é®é¢
    var img = new Image();
    img.src = 'https://jump.weiyun.qq.com/';

</script>

<script>
    function set_full_year() {
        var year = 2015,
            $this_year = $('#this_year');
        if(new Date().getFullYear() > year) {
            year = new Date().getFullYear();
            $this_year.text(year);
        }
    }
    set_full_year();
</script>

