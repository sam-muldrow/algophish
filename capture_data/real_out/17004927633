  var pageUrl = '';
  var breadcrumbString = '{"@context": "http://schema.org", "@type": "BreadcrumbList", "itemListElement" : [';
  for (i = 1; i <= bcLength; i++) {
    pageUrl = 'https://www.express.com' + $( ".breadcrumb li:nth-child(" + i + ") a" ).attr("href"); 
    pageName = $( ".breadcrumb li:nth-child(" + i + ") a" ).text();
