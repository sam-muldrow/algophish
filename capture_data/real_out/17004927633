            <script type="text/javascript">
                // paddles
var leftPaddle = document.getElementById("lpw");
var rightPaddle = document.getElementById("rpw");

var leftPaddle2 = document.getElementById("lpm");
var rightPaddle2 = document.getElementById("rpm");

var leftPaddleDG = document.getElementById("lpdg");
var rightPaddleDG = document.getElementById("rpdg");

var womensMenu = document.getElementById("womenslide");
var mensMenu = document.getElementById("menslide");
var dgMenu = document.getElementById("dgslide");

// get items dimensions
var itemsLength = $('.item').length;
var itemSize = $('.item').outerWidth(true);
// get some relevant size for the paddle triggering point

var paddleMargin = 0;

// get wrapper width
var getMenuWrapperSize = function() {
  return $('.menu-wrapper').outerWidth();
}
var menuWrapperSize = getMenuWrapperSize();
// the wrapper is responsive
$(window).on('resize', function() {
  menuWrapperSize = getMenuWrapperSize();
});
// size of the visible part of the menu is equal as the wrapper size 
var menuVisibleSize = menuWrapperSize;

// get total width of all menu items
var getMenuSize = function() {
  return itemsLength * itemSize;
};
var menuSize = getMenuSize();
// get how much of menu is invisible
var menuInvisibleSizeW = menuSize - menuWrapperSize;
var menuInvisibleSizeM = menuSize - menuWrapperSize;
var menuInvisibleSizeDG = menuSize - menuWrapperSize;

// get how much have we scrolled to the left
var getMenuPosition = function() {
  return $(womensMenu).scrollLeft();
};

var getMenuPositionM = function() {
  return $(mensMenu).scrollLeft();
};

var getMenuPositionDG = function() {
  return $(dgMenu).scrollLeft();
};



// define the key items

// duration of scroll animation
var scrollDuration = 600;


// finally, what happens when we are actually scrolling the menu
$(womensMenu).on('scroll', function() {

  // get how much of menu is invisible
  menuInvisibleSize = menuSize - menuWrapperSize;

  // get how much have we scrolled so far
  var menuPosition = getMenuPosition();
  var menuEndOffset = menuInvisibleSizeW - paddleMargin;

  // show & hide the paddles 
  // depending on scroll position
  if (menuPosition <= paddleMargin) {
    $(leftPaddle).addClass('hidden');
    $(rightPaddle).removeClass('hidden');
  } else if (menuPosition < menuEndOffset) {
    // show both paddles in the middle
    $(leftPaddle).removeClass('hidden');
    $(rightPaddle).removeClass('hidden');
  } else if (menuPosition >= menuEndOffset) {
    $(leftPaddle).removeClass('hidden');
    $(rightPaddle).addClass('hidden');
  }

});

$(mensMenu).on('scroll', function() {

  // get how much of menu is invisible
  menuInvisibleSizeM = menuSize - menuWrapperSize;

  // get how much have we scrolled so far
  var menuPositionM = getMenuPositionM();
  var menuEndOffsetM = menuInvisibleSizeM - paddleMargin;

  // show & hide the paddles 
  // depending on scroll position
  if (menuPositionM <= paddleMargin) {
    $(leftPaddle2).addClass('hidden');
    $(rightPaddle2).removeClass('hidden');
  } else if (menuPositionM < menuEndOffsetM) {
    // show both paddles in the middle
    $(leftPaddle2).removeClass('hidden');
    $(rightPaddle2).removeClass('hidden');
  } else if (menuPositionM >= menuEndOffsetM) {
    $(leftPaddle2).removeClass('hidden');
    $(rightPaddle2).addClass('hidden');
  }

});

$(mensMenu).on('scroll', function() {

  // get how much of menu is invisible
  menuInvisibleSizeDG = menuSize - menuWrapperSize;

  // get how much have we scrolled so far
  var menuPositionDG = getMenuPositionDG();
  var menuEndOffsetDG = menuInvisibleSizeDG - paddleMargin;

  // show & hide the paddles 
  // depending on scroll position
  if (menuPositionM <= paddleMargin) {
    $(leftPaddleDG).addClass('hidden');
    $(rightPaddleDG).removeClass('hidden');
  } else if (menuPositionM < menuEndOffsetDG) {
    // show both paddles in the middle
    $(leftPaddleDG).removeClass('hidden');
    $(rightPaddleDG).removeClass('hidden');
  } else if (menuPositionM >= menuEndOffsetDG) {
    $(leftPaddleDG).removeClass('hidden');
    $(rightPaddleDG).addClass('hidden');
  }

});


// scroll to left
$(leftPaddle).on('click', function() {
	$(womensMenu).animate( { scrollLeft: "-=210px"}, scrollDuration);
});

// scroll to right
$(rightPaddle).on('click', function() {
	$(womensMenu).animate( { scrollLeft: "+=210px"}, scrollDuration);
});

// scroll to left
$(leftPaddle2).on('click', function() {
	$(mensMenu).animate( { scrollLeft: "-=210px"}, scrollDuration);
});

// scroll to right
$(rightPaddle2).on('click', function() {
	$(mensMenu).animate( { scrollLeft: "+=210px"}, scrollDuration);
});


// scroll to left
$(leftPaddleDG).on('click', function() {
	$(dgMenu).animate( { scrollLeft: "-=245px"}, scrollDuration);
});

// scroll to right
$(rightPaddleDG).on('click', function() {
	$(dgMenu).animate( { scrollLeft: "+=245px"}, scrollDuration);
});

            </script>
        
    
