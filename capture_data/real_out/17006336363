<script type="text/javascript">
function setList(param){
	if(param=="1"){
		document.getElementById("themelist").style.display="block";
		setcookie("themelist","1",30);
		document.getElementById("theme_bt").innerHTML='<strong>表示する</strong> ｜ <a href="#" onclick="setList(\'0\');return false;" >表示しない</a>';
		
	}else if(param=="0"){
		document.getElementById("themelist").style.display="none";
		setcookie("themelist","0",30);
		document.getElementById("theme_bt").innerHTML='<a href="#" onclick="setList(\'1\');return false;" >表示する</a> ｜ <strong>表示しない</strong>';
	}
}

function setList1(){
		document.getElementById("themelist").style.display="block";
		setcookie("themelist","1",30);
		document.getElementById("theme_bt").innerHTML='<strong>表示する</strong> ｜ <a href="#" onclick="setList0();return false;" >表示しない</a>';
}

function setList0(){
		document.getElementById("themelist").style.display="none";
		setcookie("themelist","0",30);
		document.getElementById("theme_bt").innerHTML='<a href="#" onclick="setList1();return false;" >表示する</a> ｜ <strong>表示しない</strong>';
}

if(getcookie("themelist") == "0"){
		document.getElementById("themelist").style.display="none";
		document.getElementById("theme_bt").innerHTML='<a href="#" onclick="setList1();return false;" >表示する</a> ｜ <strong>表示しない</strong>';
}else{
		document.getElementById("theme_bt").innerHTML='<strong>表示する</strong> ｜ <a href="#" onclick="setList0();return false;" >表示しない</a>';
}

function setcookie(name,value,expiredays){
	var todayDate = new Date();
//	todayDate.setDate(todayDate.getDate() +expiredays);
	todayDate.setDate(todayDate.getDate() +(60*60*24*1000*expiredays) );
	document.cookie = name + "=" + escape(value) + "; path=/; expires=" + todayDate.toGMTString() + ";"
	
}
function getcookie(cookieName){
	var search = cookieName + "=";
	var cookie = document.cookie;
	if(cookie.length > 0){
		startIndex = cookie.indexOf(cookieName);
		if(startIndex != -1){
			startIndex += cookieName.length;
			endIndex = cookie.indexOf(";",startIndex);
			if(endIndex == -1) endIndex = cookie.length;
			return unescape(cookie.substring(startIndex+1,endIndex));
		}else{
			return "-1";
		}
	}else{
		return "-1";
	}
}
</script>



