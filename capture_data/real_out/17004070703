    btnTrack = function(location, event) {
    var eventLabel = "";
    var context = {};
    context.campaign = {};
    if(event){
    context.campaign.name = "criteocom";
    context.campaign.source = "criteocom";
     context.campaign.medium = eventPageName;
    }else{
    context = null;
    }
    if(location == "" || location == undefined){location = null;}
    if(event == "" || event == undefined || event == "undefined" ){event = null}
    if(event == "CLICKED_CRITEO_COM_GET_STARTED"){
    eventLabel = "Criteo.com Clicked Get Started"
    }else if(event == "CLICKED_CRITEO_COM_JUMP_PAGE"){
    eventLabel = "Criteo.com Clicked Jump Page"
    }
    var page = document.location.href;
        analytics.track(eventLabel, {
            application_name: 'ADVERTISER_PLATFORM',
            event_type_id: event,
            section_id: 'CRITEO_COM',
            page_url: page,
            location:'FRONT',
            proterties:{
            button_location: location
            },
            context: context
        });

    }

