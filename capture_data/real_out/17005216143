<!-- case studies scrolltop -->
<script type="rocketlazyloadscript" defer>window.addEventListener('DOMContentLoaded', function() {
	jQuery(function ($) {
		$(document).ready(function(){
			$('.vertical-tabs-case-study li').addClass('case_study_integration');
			$(".case_study_integration > a").attr('href','javascript:void(0)');
		});
		
		$(document).ready(function(){
			$(".case_study_integration").click(function(){
				$('html,body').animate({
					scrollTop: $(".vertical-tabs-case-study").offset().top -100 }, 200);
			});
		});
	});
});</script>

