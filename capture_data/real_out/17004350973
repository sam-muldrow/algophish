    <script type="text/javascript">
        $(window).load(function () {
            textAutoHeight();
        });
        function textAutoHeight() {
            var $elmLth = $('.mt_idx_body4 .bundle.ty2');
            $elmLth.each(function(idx,item){
                var $thum = $elmLth.eq(idx).find('.thum');
                var $thumH = $thum.height();
                var $subject = $elmLth.eq(idx).find('.subject');

                if(265 > $thumH && $thumH > 240) {
                    $thum.css('max-height',264+'px');
                    $subject.css('max-height',48+'px');
                } else if(241 > $thumH && $thumH > 216) {
                    $thum.css('max-height',240+'px');
                    $subject.css('max-height',72+'px');
                } else if(217 > $thumH && $thumH > 192) {
                    $thum.css('max-height',216+'px');
                    $subject.css('max-height',96+'px');
                } else if(193 > $thumH) {
                    $thum.css('max-height',192+'px');
                    $subject.css('max-height',120+'px');
                }
            });
        }
    </script>
