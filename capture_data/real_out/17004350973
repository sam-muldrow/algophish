		<script type="text/javascript">
			$(document).ready(function() {
				//console.log('cookie = '+$.cookie("nobanner"));
				if ($.cookie("nobanner") != "Y") {
					$("#sbsb_link").show();              
				}
			});
			function sbsbClose(){
				var todayDate = new Date();
				expiresDate = new Date(parseInt(todayDate.getTime() / 86400000) * 86400000 + 54000000);
				if (todayDate > expiresDate){    
					expiresDate = new Date(parseInt(todayDate.getTime() / 86400000) * 86400000 + 54000000 + 86400000);
					$.cookie("nobanner", "Y", {  
						expires : expiresDate       
					});                                                            
					$("#sbsb_link").hide();           
				} else {                                                                                           
					$.cookie("nobanner", "Y", {                  
						expires : expiresDate
					});
					$("#sbsb_link").hide();                
				}
			}
		</script>
