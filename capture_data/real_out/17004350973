<button type="button" id="aside2017_open" onclick="javascript:asdShow();"><span>우측영역 열기</span></button>
<script type="text/javascript">
    function on_showArticle(strHashtag, num) {
        $.ajax({
            type: 'POST',
            url: '/contain/hashtagProc.php',
            data: 'hashtag=' + strHashtag,
            async: false,
            dataType: 'html',
            error: function (request, status, error) {
                returnStr = '';
            },				// 통신 에러 발생 시
            success: function (response, status, request) {
                returnStr = response;
            },	// 통신 성공 시
            complete: function () {
            }													// 통신이 완료 후
        });

        $("a[name='hashtag']").removeClass('on');

        $('#hashtag' + num).addClass('on');

        var arrArticleList = returnStr.split('||');
        var articleList = '';
        for (var i = 0; i < arrArticleList.length - 1; i++) {
            var arrAritcle = arrArticleList[i].split('|');
            var articleId = arrAritcle[0];
            var articleTitle = arrAritcle[1];
            var articleLink = arrAritcle[2];
            articleList = articleList + '<li class="bundle"><a href="' + articleLink + '&MRHT_T' + '">' + articleTitle + '</a></li>';
        }
        $('#hashtagArticle').html(articleList);
    }

    function setCookie(cookieName, cookieValue, expireDate) {
        var today = new Date();
        today.setDate(today.getDate() + parseInt(expireDate));
        document.cookie = cookieName + "=" + escape(cookieValue) + "; path=/; expires=" + today.toGMTString() + ";"
    }

    function asdHide() {
        //var winW = screen.availWidth;
        var winW = $('#wrap').width();			//브라우저의 가로길이
        $('#aside2017').hide();
        $('#aside2017_open').show();
        $(".leftbn_wrap").show(); /* 수정 181228 세종 */ //#leftbn_wrap
        //$("#aside_q_bn").show(); //선관위배너관련 추가 180511 세종
        containerHeightSetter(); /* 추가 200331 세종 */
        $(".rightbn_wrap").show(); /* 추가 200331 세종 */

        var boolAsd = true;
        if ($("#aside2017").css("display") == "block") {
            boolAsd = true;
        } else {
            boolAsd = false;
        }
        var boolLfb = true;
        if ($(".leftbn_wrap").css("display") == "block") /* 수정 181228 세종 */ //#leftbn_wrap
        {
            boolLfb = true;
        } else {
            boolLfb = false;
        }
        /* 수정 200331 세종 */
        if (!boolAsd && boolLfb) {
            $('#wrap').css('min-width', '1404px');
            $('#mt_header').css('padding-left', '190px').css('padding-right', '190px');
            $('#container').css('padding-left', '190px').css('padding-right', '190px');
            $('#mt_footer').css('padding-left', '190px').css('padding-right', '190px');
            //    $('.wrap_banner').css('padding-left', '190px').css('padding-right', '190px'); /* 추가 190102 동현 */
        } else if (!boolAsd && !boolLfb) {
            $('#wrap').css('min-width', '1114px');
            $('#mt_header').css('padding-left', '0').css('padding-right', '0');
            $('#container').css('padding-left', '0').css('padding-right', '0');
            $('#mt_footer').css('padding-left', '0').css('padding-right', '0');
            //    $('.wrap_banner').css('padding-left', '0').css('padding-right', '0'); /* 추가 190102 동현 */
        }
        /* //수정 200331 세종 */

        setCookie("mtmainright", "off", 365);
    }

    function asdShow() {
        //var winW = screen.availWidth;
        var winW = $('#wrap').width();			//브라우저의 가로길이
        $('#aside2017_open').hide();
        $(".rightbn_wrap").hide(); /* 추가 200331 세종 */
        $('#aside2017').show();
        //$("#aside_q_bn").hide(); //선관위배너관련 추가 180511 세종
        /*	수정 200331 세종
        if (winW < 1368) {
            $('.leftbn_wrap').hide();
        } else {
            $(".leftbn_wrap").show();
        }
        */

        var boolAsd = true;
        if ($("#aside2017").css("display") == "block") {
            boolAsd = true;
        } else {
            boolAsd = false;
        }
        var boolLfb = true;
        if ($(".leftbn_wrap").css("display") == "block") /* 수정 181228 세종 */ //#leftbn_wrap
        {
            boolLfb = true;
        } else {
            boolLfb = false;
        }

        /* 수정 200331 세종 */
        if (boolAsd && boolLfb) {
            $('#wrap').css('min-width', '1464px');
            $('#mt_header').css('padding-right', '220px').css('padding-left', '220px');
            $('#container').css('padding-right', '220px').css('padding-left', '220px');
            $('#mt_footer').css('padding-right', '220px').css('padding-left', '220px');
            //   $('.wrap_banner').css('padding-right', '220px').css('padding-left', '220px'); /* 추가 190102 동현 */
        } else if (boolAsd && !boolLfb) {
            $('#wrap').css('min-width', '1464px');
            $('#mt_header').css('padding-right', '220px').css('padding-left', '0');
            $('#container').css('padding-right', '220px').css('padding-left', '0');
            $('#mt_footer').css('padding-right', '220px').css('padding-left', '0');
            //   $('.wrap_banner').css('padding-right', '220px').css('padding-left', '0'); /* 추가 190102 동현 */
        }
        /* //수정 200331 세종 */

        setCookie("mtmainright", "on", 365);
    }
</script>
