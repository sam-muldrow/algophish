            <div class="col-md-4 border-left mb-7" id="homepage_rankings">
                <label class="text-uppercase text-muted small">
                    <strong>
                        Rankings
                    </strong>
                </label>
                

<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<script type="text/javascript">
    document.addEventListener("DOMContentLoaded",function(event) {
        // Load the Visualization API and the corechart package.
        google.charts.load('43', {'packages': ['table', 'corechart']});
        google.charts.setOnLoadCallback(drawRanking);
        var rankings = [];

        function drawRanking() {
            if ("" == 'movies') {
                rankings.push(["worldwide_box_office", "B1:M4", 1, "Table", "Worldwide Box Office", 11, {columns: [0]}, 'tab=7&item=1']);
                rankings.push(["domestic_box_office", "B1:J4", 1, "Table", "Domestic Box Office", 8, {columns: [0]}, 'tab=7&item=2']);
            } else if ("" == 'ctv') {
                rankings.push(["ott_service", "B1:F4", 2, "Table", "Top CTV Services", 4, {columns: [0]}, 'tab=6']);
                rankings.push(["ctv_1", "B1:E4", 1, "Table", "Most Popular CTV Segments", 3, {columns: [0]}, '']);
            } else if ("" == 'political') {
                rankings.push(["political_1", "B1:E4", 1, "Table", "Top brand safety segments", 3, {columns: [0]}, '']);
                rankings.push(["political_2", "B1:E4", 1, "Table", "Top misinformation segments", 3, {columns: [0]}, '']);
            } else if ("" == 'sports') {
                rankings.push(["social_sports", "B1:I4", 1, "Table", "Top Sports Media Companies", 7, {columns: [0]}, 'tab=2&item=33']);
            } else if ("" == 'instant_insights') {
                rankings.push(["video_top_properties", "B1:E4", 1, "Table", "U.S. Online Video Properties", 3, {columns: [0,1]}, 'tab=4&item=1']);
                rankings.push(["digital_media_top50_mp", "B1:F4", 1, "Table", "Multi-Platform Properties", 4, {columns: [0,1]}, 'tab=1']);
                rankings.push(["ott_service", "B1:F4", 1, "Table", "Top CTV Services", 4, {columns: [0,1]}, 'tab=6']);
                rankings.push(["mobile_apps", "B1:F4", 1, "Table", "Smartphone Apps", 4, {columns: [0,1]}, 'tab=3&item=3']);
                rankings.push(["telecast_broadcast", "C1:K4", 1, "Table","TV Engagement Broadcast", 8, {columns:[0,2]},'tab=5&item=3']);
                rankings.push(["worldwide_box_office", "B1:M4", 1, "Table", "Worldwide Box Office", 11, {columns:[0,1]},'tab=7&item=1']);
                rankings.push(["domestic_box_office", "B1:J4", 1, "Table", "Domestic Box Office", 8, {columns:[0,1]},'tab=7&item=2']);
            } else if ("" == 'world_cup_2022') {
                rankings.push(["LATAM_world_cup", "B1:E4",1,"Table","Selección con más engagement", 3, {columns:[0,1]},'']);
            } else {
                rankings.push(["worldwide_box_office", "B1:M4", 1, "Table", "Worldwide Box Office", 11, {columns:[0]},'tab=7&item=1']);
                rankings.push(["domestic_box_office", "B1:J4", 1, "Table", "Domestic Box Office", 8, {columns:[0]},'tab=7&item=2']);
                rankings.push(["telecast_broadcast", "C1:K4", 1, "Table","TV Engagement Broadcast", 8, {columns:[0]},'tab=7&item=3']);
                rankings.push(["digital_media_top50_mp", "B1:F4", 1, "Table", "Multi-Platform Properties", 4, {columns:[0]},'tab=1']);
                rankings.push(["video_top_properties", "B1:E4", 1, "Table", "U.S. Online Video Properties", 3, {columns:[0]},'tab=4']);
                rankings.push(["mobile_apps", "B1:F4", 1, "Table", "Smartphone Apps", 4, {columns:[0]},'tab=3&item=3']);
                rankings.push(["ott_service", "B1:F4", 1, "Table", "Top CTV Services", 4, {columns:[0]},'tab=6']);
            }

            for (var i = 0; i < rankings.length; i++) {
                $('.carouseling').slick('slickAdd','<div class="col px-0"><div><span id="ranking_' + i + '" class="h4 d-block mb-1"><a href="/Insights/Rankings?' + rankings[i][7] + '" title="Rankings: ' + rankings[i][4] + '">' + rankings[i][4] + '</a></span><div class="small text-muted semibold text-uppercase mb-2 reporting_period" id="reporting_period_' + i + '"></div><div id="ranking-' + i + '"></div><div id="link-' + i + '" class="chart-link"></div></div></div>');
                var query = new google.visualization.Query('https://docs.google.com/spreadsheets/d/1l-yq6MX8Egp5ITuKxmVl0LXUc2TeMYaGW6urqXTKSQ8/edit?usp=sharing?&sheet=' + rankings[i][0] + '&range=' + rankings[i][1] + '');
                runQuery(query, i, rankings[i][5], rankings[i][6]);
            }
        }

        function runQuery(q, iteration, column_count, columns_to_display, anchor){
            q.send(function (response) {
                var cssClassNames = {'selectedTableRow':'active-row','table':'','hoverTableRow':'hover-row'};
                var data = response.getDataTable();
                var reporting_period = data.getValue(0,column_count);
                var chart = new google.visualization.ChartWrapper({
                    chartType: rankings[iteration][3],
                    containerId: 'ranking-' + iteration,
                    options: {
                        'title': rankings[iteration][4],
                        width: '100%',
                        height: '100%',
                        showRowNumber: true,
                        alternatingRowStyle: false,
                        'cssClassNames': cssClassNames,
                        'allowHtml': true
                    },
                    dataTable: data
                });
                // ONLY SHOW THE FIRST COLUMN, REST IS NOT DISPLAYED IN THIS VIEW
                chart.setView(columns_to_display);

                // USE EVENTLISTENER TO REMOVE GOOGLE VISUALIZATION CLASSES, ADD BOOTSTRAP
                google.visualization.events.addListener(chart, 'ready', function(){
                    $(".google-visualization-table-table").removeAttr('cellspacing').attr('aria-describedby', 'ranking_' + iteration).removeClass('google-visualization-table-table').addClass('table google-viz mb-0 table-condensed light');
                    // SHOW COLUMN TITLES IF MORE THAN 1 COLUMN IS SHOWN
                    if(columns_to_display['columns'].length>1) {
                        $(".google-viz thead").attr('style','display:table-header-group;')
                    }
                    $("#reporting_period_"+iteration).html(reporting_period);
                    $(".carouseling div, .carouseling tr, .carouseling td").removeClass(function (index, css) {
                        return (css.match(/\bgoogle-visualization-\S+/g) || []).join(' ');
                    });
                    $('.carouseling').slick('slickGoTo',0);
                });

                chart.draw();
            });
        }
    });
</script>

<div class="carouseling mb-0"></div>

