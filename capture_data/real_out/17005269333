            <script type="text/javascript">
        document.addEventListener("DOMContentLoaded", function(event) {
            /*
             * COOKIE ALERT CHECK:
             * DISPLAY THE ALERT IF
             * THE NUMBER OF TIMES IT WAS SHOWN IS LESS THAN maxCount
             * UNSET THE COUNT VALUE FIRST IF IT ALREADY EXISTS -
             * THE USE THE CUSTOM FUNCTION TO FETCH THE LATEST COUNT
             */
            var alertSelector = "#prod_promo";
            var cookieName = "cspprom62207";
            var showedCount = 0;
            var cookieValue = getCookie(cookieName);
            if (cookieValue !== false) {
                showedCount = cookieValue;
            }
            if (showedCount < 3) {
                if (showedCount > 0) {
                    var updateCount = parseInt(showedCount, 10) + 1;
                }
                else {
                    var updateCount = 1;
                }
                setCookie(cookieName, updateCount, 7);
                $(alertSelector).show();
            }
            $('#prod_promo button.close').click(function() {
                $(alertSelector).hide();
                setCookie(cookieName, 99, 7);
            });
        });
    </script>


