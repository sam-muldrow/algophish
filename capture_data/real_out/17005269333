<script>
    document.addEventListener("DOMContentLoaded", function(event){
        $('#insights_slider').slick({
            infinite:false,
            slidesToShow:3.3,
            slidesToScroll:3,
            respondTo:'min',
            responsive:[
                {breakpoint:1140,
                    settings:{slidesToShow:3.3, slidesToScroll:3}},
                {breakpoint:960,
                    settings:{slidesToShow:2.2, slidesToScroll:2}},
                {breakpoint:720,
                    settings:{slidesToShow:1.1, slidesToScroll:1}}
            ]
        });

        $('.slick-slider .card').hover(
            function() {
                $(this).stop(true,true).toggleClass('bg-light bg-primary bgb-skew',1000);
                $(this).find('h4').stop(true,true).toggleClass('text-dark text-white',1000);
                $(this).find('button').stop(true,true).toggleClass('btn-outline-primary btn-outline-light',1000);
                $(this).find('label').stop(true,true).toggleClass('text-muted text-white',1000);
            }
        );
    });
</script>

