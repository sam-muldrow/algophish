    document.addEventListener("DOMContentLoaded",function(event){
        // ADD BOOTSTRAP TABLE CLASS TO GOOGLE VISUALIZATION TABLES
        $('.carouseling').slick({
            arrows:false,
            dots: true,
            infinite: true,
            speed: 300,
            slidesToShow: 1,
            slidesToScroll: 1,
            /*adaptiveHeight: true,*/
            autoplay: false
        });
    });
</script>

