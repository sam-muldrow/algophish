    var serveur = window.location.hostname.slice(0, 4);
    //console.log("@serveur "+serveur);/* serveur => tst-, sit-, rqt-, www. */
    if (serveur == "tst-" || serveur == "sit-" || serveur == "rqt-") {
      var eltLienRecrutement = document.getElementById("aRecrutement");
      eltLienRecrutement.setAttribute(
        "href",
        eltLienRecrutement
          .getAttribute("href")
          .replace("//recrutement", "//" + serveur + "recrutement")
      );
      eltLienRecrutement.setAttribute(
        "title",
        eltLienRecrutement
          .getAttribute("title")
          .replace("recrutement.", serveur + "recrutement.")
      );
    }
  </script>
