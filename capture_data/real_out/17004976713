<script type="text/javascript">
    //return the value of a cookie
function dclic_getCookie(nomCookie) {
    deb = document.cookie.indexOf(nomCookie + "=");
    if (deb >= 0) {
        deb += nomCookie.length + 1;
        fin = document.cookie.indexOf(";", deb);
        if (fin < 0) {
            fin = document.cookie.length;
        }
        return unescape(document.cookie.substring(deb, fin));
    } else {
        return "";
    }
}

//create dclic cookie in local path
function dclic_setLocalCookie(value) {
    if (value) {
        var date = new Date();
        date.setTime(date.getTime() + (13 * 30 * 24 * 60 * 60 * 1000));
        document.cookie = "dclic_tid_local=" + value + "; expires=" + date.toUTCString() + "; path=/; secure";
    }
}

//return current origin
function dclic_getOrigin() {
    return window.location.origin;;
}

//return true if tracking is allowed in browser
function dclic_isTrackingAllowed() {
    return typeof(navigator) != "undefined" &&
        navigator.doNotTrack != "1" &&
        navigator.msDoNotTrack != "1" &&
        typeof(window) != "undefined" &&
        window.doNotTrack != "1";
}

// check for accepted solution
function dclic_isSolutionAccepted(solution) {
    var euConsent = "";
    // Recuperation du cookie
    var cookieName = "eu-consent=";
    var ca = document.cookie.split(";");
    for (var i = 0; i < ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) === " ") {
            c = c.substring(1, c.length);
        }
        if (c.indexOf(cookieName) === 0) {
            euConsent = c.substring(cookieName.length, c.length);
        }
    }

    if (euConsent !== undefined && euConsent !== "") {
        euConsent = JSON.parse(decodeURIComponent(euConsent));

        var path = document.location.pathname;

        // Selection du chemin dans le cookie correspondant le mieux a l'url de la page courante
        var matchs = [];
        Object.keys(euConsent).forEach(function(acceptancePath) {
            if (path.substring(0, acceptancePath.length) === acceptancePath) {
                matchs.push(acceptancePath);
            }
        });
        matchs = matchs.sort(function(a, b) {
            return b.length - a.length;
        });

        // Retour
        if (matchs.length > 0 && euConsent[matchs[0]]["solutions"].hasOwnProperty(solution)) {
            return euConsent[matchs[0]]["solutions"][solution];
        }
    }
    return false;
}


//check if user allowed DCLIC to collect data
function dclic_checkUserConsent() {
    return dclic_isSolutionAccepted("DCLIC") === true ||
    ((typeof(dclic_getCookie) != "undefined" && dclic_getCookie('cookies_accepted') != null && dclic_getCookie('cookies_accepted') != "")
        && (dclic_getCookie('cookies_accepted') == "1" || dclic_getCookie('cookies_accepted').match("cookie_marketing=true") ) );
}

//read value in dataElement depending on multiple indices
function dclic_readDatalayer(datalayer, indices) {
    if (!indices instanceof Array) {
        return undefined;
    }
    if (indices.length == 0) {
        return datalayer;
    }

    if (datalayer === undefined) {
        return undefined;
    }

    id = indices.shift();

    if ((datalayer instanceof Array && id < datalayer.length) ||
        (datalayer instanceof Map && datalayer.has(id)) ||
        (datalayer instanceof Object && id in datalayer)) {
        return dclic_readDatalayer(datalayer[id], indices);
    }
    return undefined;
}

var dclic_UrlSearchParams = {
    getSearchObject: function(query) {
        return query ?
            (/^[?#]/.test(query) ? query.slice(1) : query)
            .split("&")
            .reduce(function(params, param) {
                var splitted = param.split("=");
                var key = splitted[0];
                var value = splitted[1];
                params[key] = value ?
                    value :
                    "";
                return params;
            }, {}) :
            {};
    },
    getAll: function(query) {
        return dclic_UrlSearchParams.getSearchObject(query);
    },
    get: function(query, param) {
        var searchParams = dclic_UrlSearchParams.getSearchObject(query);
        return searchParams[param];
    }
};


//return a specific value of a parameter in querystring
function dclic_getUrlParameter(name) {
    return dclic_UrlSearchParams.get(window.location.search, name);
    /*name = name.replace([[], '[').replace([]], ']');
    var regex = new RegExp('[&]' + name + '=([^&#])');
    var results = regex.exec(location.search);
    return results === null  ''  decodeURIComponent(results[1].replace(+g, ' '));*/
}

var dclicClient = {
    info: {
        v: "1.0.0"
    },
    watMap: {
        "user_niveau":"s21",
"user_space":"s17",
"dir_reg":"s13",
"cat_juridique":"s14",
"env_marque":"s24",
"user_frequence_bad":"s10",
"env_langue":"s12",
"user_formule_bad":"s4",
"user_federation":"s2",
"env_channel":"s22",
"typepage":"s16",
"statutuser":"s18",
"data_ga_dispositif":"s150",
"user_segment":"s8",
"user_type_client":"s5",
"user_agence":"s3",
"page_cat1":"s26",
"page_cat3":"s28",
"page_cat2":"s27",
"page_cat5":"s30",
"page_cat4":"s29",
"env_espace":"s15",
"env_country":"s25",
"user_segment_commercial":"s9",
"substitution_mode":"s19",
"env_work":"s11",
"espace":"s1",
"user_type_personne":"s20",
"relation_group":"s23"
    },

    init: function() {
        if (typeof(dclic_setLocalCookie) != "undefined" && (dclic_getCookie('dclic_tid_local') == null || dclic_getCookie('dclic_tid_local') == "")) {
            dclic_setLocalCookie();
        }
        document.addEventListener("DOMContentLoaded", function() {
            dclicClient.pushEvent("Page Viewed");
        });
    },
    setEid: function(value) {
        dclicClient.info.eid = value;
    },
    setAid: function(value) {
        dclicClient.info.aid = value;
    },
    pushEvent: function(name, payload) {
        if (dclic_isTrackingAllowed() && dclic_checkUserConsent()) {
            var evt = {};
            payload = payload || {};

            $.extend(evt, payload);

            if (typeof watData !== 'undefined' && typeof tc_vars !== 'undefined') {
                Object.assign(watData, tc_vars);
            }

            Object.keys(dclicClient.watMap).forEach(function(key, index) {
                if (typeof watData !== 'undefined') {
                    if (watData.hasOwnProperty(key)) {
                        evt[dclicClient.watMap[key]] = watData[key];
                    }
                }
            });

            $.extend(evt, dclicClient.info);
            $.extend(evt, {
                origin:dclic_getOrigin(),
ps:(document.referrer.length > 0 && document.referrer.lastIndexOf(window.location.origin, 0) === 0 ? (function parseurl() { var parser = document.createElement('a'); parser.href = document.referrer; return parser.pathname.substring(0, 255); })() : ""),
tz:new Date().getTimezoneOffset(),
pt:document.title,
px:Math.round(window.pageXOffset),
py:Math.round(window.pageYOffset),
s7:(typeof alias !== "undefined" ? alias : ""),
s6:(typeof clientreconnu !== "undefined" && clientreconnu === "Y" ? true : false),
pc:window.location.pathname.substring(0, 255),
dl:document.documentElement.lang,
s11:(window.location.href.split('/')[2].charAt(3) === "-") ? window.location.href.split("/")[2].split("-")[0] : "prod",
bs:window.innerWidth + "x" + window.innerHeight,
e:name,
ss:window.screen.width + "x" + window.screen.height,
oc:new Date().toISOString(),
s44:dclic_getUrlParameter("utm_source"),
s45:dclic_getUrlParameter("utm_medium"),
s46:dclic_getUrlParameter("utm_campaign"),
s47:dclic_getUrlParameter("utm_term"),
s43:(typeof url_page !== "undefined") ? url_page : "",
sc:window.screen.colorDepth,
s48:dclic_getUrlParameter("utm_content"),
s49:dclic_getUrlParameter("codepromo")
            });

            $.ajax({
                url: "https://dclic-events.e-i.com/",
                type: "POST",
                headers: {
                    "Access-Control-Allow-Origin": "*"
                },
                crossDomain: true,
                contentType: "application/json",
                data: JSON.stringify({
                    dclic_tid_local: dclic_getCookie("dclic_tid_local"),
                    data: evt
                }),
                dataType: "json",
                xhrFields: {
                    withCredentials: true
                },
                success: function(res) {
                    var tid = null;
                    if (res) {
                        tid = res.dclic_tid;
                        dclic_setLocalCookie(res.dclic_tid);
                    }
                    setTimeout(function() {
                        dclicClient.resolve(tid);
                    }, 5000);
                }
            });
        }
    },
    resolve: function(dclic_tid) {
        var url = "https://dclic-resolver.e-i.com/";
        if (dclic_tid) {
            url += dclic_tid;
        }
        $.ajax({
            url: url,
            type: "GET",
            headers: {
                "Access-Control-Allow-Origin": "*"
            },
            crossDomain: true,
            contentType: "application/json",
            data: {
                origin: dclic_getOrigin()
            },
            dataType: "json",
            xhrFields: {
                withCredentials: true
            },
            success: function(res, status) {
                if (res !== null && 'dclicseg' in res) {
                    var date = new Date();
                    date.setTime(date.getTime() + (13 * 30 * 24 * 60 * 60 * 1000));
                    document.cookie = "dclic_seg=" + res.dclicseg + "; expires=" + date.toUTCString() + "; path=/; secure";
                }
            }
        });
    }
};

dclicClient.init();


function eia(action, data) {
    if (action === "set") {
        dclicClient.setEid(data.eid);
        dclicClient.setAid(data.aid);
    }
}


//allow dclic to be called on consent update
document.addEventListener("DOMContentLoaded", function() {
  if (typeof(euConsentOnUpdate) == "function") {
      if (typeof(DISABLE_EI_EU_CONSENT_MODAL) == "undefined" || DISABLE_EI_EU_CONSENT_MODAL == false) {
          euConsentOnUpdate(function() { dclicClient.pushEvent("Page Viewed"); });
      }
  }
});
    
