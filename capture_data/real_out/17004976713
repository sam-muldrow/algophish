            var mapping = {"d1":"espace","d12":"env_langue","d15":"env_espace","d16":"typepage","d18":"statutuser","d19":"substitution_mode","d197":"product_code_promo","d194":"page_cat1","d193":"page_cat2","d192":"page_cat3","d191":"page_cat4","d190":"page_cat5"};
            for (var key in mapping) {
