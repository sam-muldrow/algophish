	<div id="searchbox">


		<script src="https://kit.fontawesome.com/5a9a5c50a3.js" crossorigin="anonymous"></script>

		<input type="text" id="urlInput" placeholder="SEARCH" onkeydown="handleKeyDown(event)">
		<button onclick="final_search()"><i class="fa-solid fa-magnifying-glass" style="color: #deddda; margin-top: 3px;" onmouseover="changeColor(this)" onmouseout="restoreColor(this)"></i></button>
		<div id="result-box"></div>
	</div>

	

	<style>
		/* Style for the container div */
		#searchbox{
			position: relative;
			display: inline-block;
		}

		/* Style for the input field */
		#urlInput{
			padding-right: 30px; /* Create space for the button */
		}

		.result{
			display: flex;
			font-size: 18px;
			border-top: 2px solid;
			flex-direction: column;
			flex-grow: 1;
			width: 100%;
		}

		a.show-more-link{
			background-color: #1d1f21;
			display: flex;
			justify-content: center;
			flex-grow: 1;
			width: 100%;
			font-size: 20px;
			color: white;
		}

		.show-more-link:hover{
			color: red; /* Change icon color on hover */
		}

		/* Style for the button */
		#searchbox button {
			position: absolute;
			right: 0;
			top: 0;
			padding: 5px 10px;
			background-color: #ccc;
			border: none;
			cursor: pointer;
			background-color: rgba(255, 255, 255, 0);
		}

		#searchbox button:hover i {
			color: red; /* Change icon color on hover */
		}

		/* styles.css */

		/* Style for search results container */
		#result-box{
			display: flex;
			flex-direction: column;
			align-items: center;
			position: absolute;
			background-color: white;
			z-index: 1000;
			width: 893px;
			height: auto;
			right: -316px;
			margin-top: 12px;
		}

		.result-top {
			flex: 1;
			margin-left: 10px;
			margin-left: 10px;
			display: flex;
			flex-direction: column;
			justify-content: space-evenly;
		}

		.result-bot{
			display: flex;
			margin-left: 10px;
		}

		.date {
			font-weight: bold;
			color: #1e2022;
			margin-left: 5px;
		}

		.result-img{
			height: 124px;
			width: 215px;
		}

		/* Hover effect for search result items */
		.result:hover {
			background-color: #ddd;
		}



	</style>
		<script>

			let query_var = null;
			var timer;

			const searchBox = document.getElementById('searchbox');

			//make GET request when user types on search bar
			searchBox.addEventListener('keyup', (e) => {
				if(checkPlaceholder()) {

					//window.console.log("Search String: ",e.target.value);
					clearTimeout(timer);

					// Set a new timer for a 2-second delay
					timer = setTimeout(() => {if(e.target.value != query_var){makeGETRequest(e.target.value);};
						console.log("Start Timer")}, "750");
					//console.log(timer)
				}
				clearResults();
			});


			document.addEventListener('click', function(event) {
				// Check if the clicked element has the 'delete-me' class
				if (!event.target.classList.contains('result') && event.target.id !== 'urlInput' && !event.target.classList.contains('fa-magnifying-glass')){
					// If not, remove all elements with the 'result' class
					clearResults();
				}
			});

			function changeColor(element) {
				element.style.color = 'red'; // Change color on hover
			}

			function restoreColor(element) {
				element.style.color = '#deddda'; // Restore the default color on mouseout
			}

			function checkPlaceholder() {
				let input = document.getElementById('urlInput').value;
				if (input.trim() === '' || query_var === input.trim()) {
					return false;
				} else {
					return true;
				}
			}

			function clearResults(){
				// Select all elements with a certain class (replace '.myClass' with your desired class)
				let elementsToRemove = document.querySelectorAll('.result');

				// Iterate through the selected elements and remove each one
				elementsToRemove.forEach(element => {
					element.remove();
				});

				let showmore = document.querySelectorAll('.show-more-link');

				// Iterate through the selected elements and remove each one
				showmore.forEach(element => {
					element.remove();
				});

			}

			// Custom comparator function to sort articles by date
			function compareByDate(a, b) {
				let dateA = new Date(a.article_date.raw.replace(/(\d{2})\/(\d{2})\/(\d{4}), (\d{2}):(\d{2})/, '$2/$1/$3 $4:$5'));
				let dateB = new Date(b.article_date.raw.replace(/(\d{2})\/(\d{2})\/(\d{4}), (\d{2}):(\d{2})/, '$2/$1/$3 $4:$5'));

				return dateB - dateA;
			}


			function makeGETRequest(query) {
				let defaultPage = 1;
				let urlInput = "http://localhost:3000/search?query="+query.trim()+"&page="+defaultPage;

				query_var = query.trim();
				// Make a GET request using fetch
				fetch(urlInput)
						.then(response => {
							if (!response.ok) {
								throw new Error('Network response was not ok');
							}
							return response.json();
						})
						.then(data => {
							// Now 'data' contains the JSON response
							console.log(data);

							//filters by date
							data.sort(compareByDate);

							const searchResultsContainer = document.getElementById('result-box');
							if (data.length == 0){
								const no_result = document.createElement('div');
								no_result.className = 'result';
								no_result.textContent = 'No results';
								searchResultsContainer.appendChild(no_result);

							}
							else {

								//Iterate through top 5 results
								for (let i = 0; i < 15 && i < data.length; i++) {
									let obj = data[i];
									const resultDiv = document.createElement('a');
									resultDiv.className = 'result';
									resultDiv.href = obj.article_url.raw;
									//resultDiv.textContent = obj.title.raw;
									searchResultsContainer.appendChild(resultDiv);

									let resultTopDiv = document.createElement('div');
									resultTopDiv.className = 'result-top';

									// //Geting the image
									// let imgElement = document.createElement('img');
									// imgElement.classList.add('result-img');
									// imgElement.src = obj.article_photo.raw || null;
									// imgElement.alt = 'article photo';
									//
									// resultLeftDiv.appendChild(imgElement);

									// Create and add a title element on the top
									let titleElement = document.createElement('div');
									titleElement.textContent = obj.article_title.raw ; // Set the title text
									resultTopDiv.appendChild(titleElement); // Add the title element to the div

									resultDiv.appendChild(resultTopDiv);



									let resultBotDiv = document.createElement('div');
									resultBotDiv.className = 'result-bot';

									let category = document.createElement('span');
									category.classList.add("post-category", "lightred-c", "zonanormal", "uppercase");
									category.textContent = obj.article_category.raw+" | ";
									resultBotDiv.appendChild(category);

									// Create and add a date element on the bottom
									let dateElement = document.createElement('div');
									dateElement.classList.add('date'); // Apply styling to the date
									dateElement.textContent = obj.article_date.raw; // Set the date text
									resultBotDiv.appendChild(dateElement); // Add the date element to the div

									resultDiv.appendChild(resultBotDiv);



								}

								let showMoreLink = document.createElement('a');
								showMoreLink.className = 'show-more-link';
								showMoreLink.href = '/search?query='+query.trim()+"&page=1"; // Link to another page
								showMoreLink.textContent = 'Show more';
								searchResultsContainer.appendChild(showMoreLink);

								// // Iterate through the JSON data and create divs for each search result
								// filteredData.forEach(obj => {
								// 	const resultDiv = document.createElement('a');
								// 	resultDiv.className = 'result';
								// 	resultDiv.href = obj.article_url.raw;
								// 	//resultDiv.textContent = obj.title.raw;
								// 	searchResultsContainer.appendChild(resultDiv);
								//
								// 	let resultLeftDiv = document.createElement('div');
								// 	resultLeftDiv.className = 'result-left'
								//
								// 	//Geting the image
								// 	let imgElement = document.createElement('img');
								// 	imgElement.classList.add('result-img');
								// 	imgElement.src = obj.article_photo.raw || null;
								// 	imgElement.alt = 'article photo';
								//
								// 	resultLeftDiv.appendChild(imgElement);
								//
								// 	// Create and add a date element on the left
								// 	let dateElement = document.createElement('div');
								// 	dateElement.classList.add('date'); // Apply styling to the date
								// 	dateElement.textContent = obj.article_date.raw || null; // Set the date text
								// 	resultLeftDiv.appendChild(dateElement); // Add the date element to the div
								//
								// 	resultDiv.appendChild(resultLeftDiv);
								//
								//
								//
								// 	let resultRightDiv = document.createElement('div');
								// 	resultRightDiv.className = 'result-right';
								//
								// 	// Create and add a title element on the right
								// 	let titleElement = document.createElement('div');
								// 	titleElement.textContent = obj.article_title.raw || null; // Set the title text
								// 	resultRightDiv.appendChild(titleElement); // Add the title element to the div
								//
								//
								// 	let descElement = document.createElement('div');
								// 	descElement.textContent = obj.meta_description.raw; // Set the title text
								// 	resultRightDiv.appendChild(descElement); // Add the title element to the div
								//
								// 	// Append the new div to the parent container
								// 	resultDiv.appendChild(resultRightDiv);
								//
								// });

						 }
						})
						.catch(error => {
							console.log('Error: ' + error.message);
						});
			}


			function final_search() {
				if (checkPlaceholder()) {
					clearResults();
					makeGETRequest(document.getElementById('urlInput').value);
					console.log("Entered Final Search");
				}
			}

			function handleKeyDown(event) {
				if (event.key === 'Enter') {
					final_search();
				}
			}


		</script>

	</div>
</div>

