<script>
    (runinit = window.runinit || []).push(function () {
        $(function () {
            $('#btn-login-popup').click(function (e) {
                e.stopPropagation();
                if ($('#btn-login').length > 0) {
                    $('#btn-login').click();
                }
                else {
                    startSigninMainWindow();
                }
            });
        });
    });
</script>

            
