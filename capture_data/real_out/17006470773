<script>
    (runinit = window.runinit || []).push(function () {
        var swppaycate = new Swiper(".listpay", {
            slidesPerView: 4,
            slidePerGroup: 4,
            spaceBetween: 20,
            navigation: {
                nextEl: ".boxcatepay .swiper-button-next",
                prevEl: ".boxcatepay .swiper-button-prev"
            },
            pagination: {
                el: '.boxcatepay .swiper-pagination',
                type: 'bullets',
            },
            on: {
                init: function () {
                    var maxheight = 0;
                    $('.boxcatepay .listpay li').each(function () {
                        if (maxheight < $(this).height())
                            maxheight = $(this).height();
                    });
                    $('.boxcatepay .listpay li').height(maxheight).css('background', '#fff');
                }
            }
        })
    });
</script>

