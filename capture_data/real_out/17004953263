<script>
    var pageSpinner = document.querySelector('#pageSpinner');
    function activateProgressSpinner(s) {
        pageSpinner.classList.add('spinner--active');
        if (s != undefined) {
            pageSpinner.querySelector('.spinner__text > p').textContent = s;
            pageSpinner.querySelector('.spinner__text').style = 'display: block;';
            animateIt();
        }
    }

    function animateIt() {
        pageSpinner.querySelector('.fader1').classList.add('moveUp1');
    }

    function deAnimateIt() {
        pageSpinner.querySelector('.fader1').removeClass('moveUp1');
    }

    function killSpinner() {
        pageSpinner.removeClass('spinner--active');
        pageSpinner.find('.spinner__text').hide();
    }

    function addAnimation(x) {
        setTimeout(function() {
            if (x < 5) {
                $D('.spinner__text > p').removeClass('moveUp moveUp1 moveUp2');
                $D('.fader' + x).addClass('moveUp');
                x++;
            } else {
                if (x == 5) {
                    $D('.spinner__text > p').removeClass('moveUp moveUp1 moveUp2');
                    $D('.fader' + x).addClass('moveUp');
                    x = 1;
                }
            }
            addAnimation(x);
        }, 5000);
    }
</script>
</form>
