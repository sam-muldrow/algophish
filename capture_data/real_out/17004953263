  function write_packui(oPack) {
    let packid = oPack.id;
    let name = oPack.name;
    let channelNo = oPack.ChannelNo;
    let arrPackRegularPrice = oPack.regularprice.toString().split(".");
    let arrPack2yPLPrice = oPack.promoprice.toString().split(".");
    let arrPackStrikePrice = oPack.strikeprice.toString().split(".");
    let thisName = document.querySelectorAll("." + packid + "-name");
    let thisChannelNo = document.querySelectorAll("." + packid + "-chno");
    let query2YOD = document.querySelectorAll("." + packid + "-2yod");
    let query2YOC = document.querySelectorAll("." + packid + "-2yoc");
    let queryRPD = document.querySelectorAll("." + packid + "-rpd");
    let queryRPC = document.querySelectorAll("." + packid + "-rpc");
    let query2YOCS = document.querySelectorAll("." + packid + "-2yocs");
    let query2YODS = document.querySelectorAll("." + packid + "-2yods");

    const priceWiseInit = () => {
      for (const n of thisName) {
        n.textContent = name;
      }
      for (const c of thisChannelNo) {
        c.textContent = channelNo;
      }
      for (const a of query2YOD) {
        a.textContent = arrPack2yPLPrice[0]; //2yod - 2 year offer price dollar
      }
      for (const b of query2YOC) {
        b.textContent = arrPack2yPLPrice[1]; //2yoc - 2 year offer price cents
      }
      for (const d of queryRPD) {
        d.textContent = arrPackRegularPrice[0]; //rpd - regular price dollar
      }
      for (const f of queryRPC) {
        f.textContent = arrPackRegularPrice[1]; //rpc - regular price cents
      }
      for (const g of query2YODS) {
        g.textContent = arrPackStrikePrice[0]; //2yods - 2 year strikeout price dollar
      }
      for (const h of query2YOCS) {
        h.textContent = arrPackStrikePrice[1]; //2yocs - 2 year strikeout price cents
      }
    };

    if (arrPack2yPLPrice.length > 0) {
      priceWiseInit();

      console.log("Pricelock Flag: " + gpList.priceLock +" Pack Name :" + name + " |  Channel no: " + channelNo + " Class Names : ." + packid + "-name | Channel No : " + "." + packid + "-chno");
      console.log("price data: $" + arrPack2yPLPrice[0] + "." + arrPack2yPLPrice[1] + " | CSS Dollar : " + packid + "-2yod | CSS Cent : " + "." + packid + "-2yoc");
      console.log("2. Regular price data: $" + arrPackRegularPrice[0] + "." + arrPackRegularPrice[1] + " | CSS Dollar : " + packid + "-rpd | CSS Cent: " + "." + packid + "-rpc");
    }

    // if (arrPackStrikePrice.length > 0) {
    //   let sp_dollar = arrPackStrikePrice[0];
    //   let sp_cents = arrPackStrikePrice[1];

    //   if (sp_dollar == 0) {
    //     for (const d of query2YODS) {
    //       d.style = "display: none";
    //     }
    //     for (const c of query2YOCS) {
    //       c.style = "display: none";
    //       c.parentElement.style = "display: none"; //hide the entire strike price div
    //     }
    //   } else {
    //     //priceWiseInit();
    //   }

    //   //console.log("3. Strike Price data : $" + arrPackStrikePrice[0] + "." + arrPackStrikePrice[1] + " | CSS Dollar : " + packid + '-2yods | CSS Cent: ' + '.' + packid + '-2yocs');
    // }
    //console.log(".......................");
  }
  function ManageOffers() {
    let topWindow = window.location === window.parent.location ? window : window.parent,
      checkEs = getCookie("lang");
    if (checkEs != null) {
      if (checkEs == "ES" && topWindow.location.href.indexOf("/es/") > -1) {
        //console.error('DL CART IN PRICEWISE....');
      } else {
        //removeLangCookie();
      }
    }
    //if the user visits dish.com, remove lang cookie.
    function removeLangCookie() {
      //console.error('DISH.COM - LANG COOKIE REMOVED');
      createCookie("lang", "", -1);
    }
    /* US48258 remove default FreeEchodot Offer*/
    var partnerOffer = readCookie("PARTNER_PCC");
    var directOffer = readCookie("OFFER");

    if (partnerOffer != null && partnerOffer == "CJFREEECHODOT") createCookie("PARTNER_PCC", "CJ", -1);
    else {
      if (directOffer == "FREEECHODOT" || directOffer == "HOLIDAYSPECIAL" || directOffer == "EBATESSPECIAL" || directOffer == "HOLIDAYDEAL" || directOffer == "WINTEREXCLUSIVE") createCookie("OFFER", "", -1);
    }
    var IsCJFlow = readCookie("PARTNER_NAME");
    if (IsCJFlow != null && IsCJFlow == "CJ" && partnerOffer == null) {
      createCookie("PARTNER_PCC", "CJ", 30);
    }

    //Remove ticket cash offer for affiliate flow.
    //partnerOffer = readCookie("PARTNER_PCC");
    //if (partnerOffer != null && partnerOffer != "CJ")
    //    createCookie("ticketcashoffer", "", -1);

    //US84359 Remove Cash Offer site-wide 10/16
    //US91709 Hometown 1/15 Promo - Activate
    //createCookie("ticketcashoffer", "", -1);

    if (directOffer && directOffer == "FREEDOT") {
      //if it is free echo dot, remove offer cookie
      createCookie("OFFER", "", -1);
    }
    //if (directOffer == null && partnerOffer == null) {
    //    createCookie("OFFER", "FREEDOT", 30);
    //}
    if (partnerOffer != null) {
      // hide & remove FREEDOT offer stickynav for partners/affiliates 8/9.
      createCookie("OFFER", "", -1);
    }

    $D("#SecondaryNav #offer-wrapper").show();

    /*Whitelist to show only red nav on specific pages.*/
    if (document.location.toString().indexOf("/international-packages/") > -1 || document.location.toString().indexOf("/lp/own-football/") > -1) {
      console.log("*sticky-nav* show red*");
      try {
        $D("#SecondaryNav2").remove();
      } catch (e) {}
    }
    if (directOffer != null) {
      if (directOffer.indexOf("SEC100") > -1 || directOffer.indexOf("PAC12100") > -1 || directOffer.indexOf("LONGHORN100") > -1 || directOffer.indexOf("COLLEGE100") > -1 || directOffer.indexOf("BIGTEN100") > -1) $D("#SecondaryNav2").remove();
    }
  }
</script>

