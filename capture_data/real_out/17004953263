    <script>
      //vtRedirect
      if (readCookie("DCFID") !== null) {
        var dcfidStr = readCookie("DCFID");
        var vtURLStart = dcfidStr.indexOf("url") + 4;
        var vtURLEnd = dcfidStr.indexOf("|", vtURLStart);
        var vtURL = dcfidStr.substring(vtURLStart, vtURLEnd);
        if (!readCookie("vtRedirected") && vtURLStart - 4 > -1) {
          createCookieInDays("vtRedirected", 1, 30);
          window.location.replace(vtURL);
        }
      }
    </script>

