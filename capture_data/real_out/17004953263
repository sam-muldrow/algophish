    const covid = $D('#covid');
    let covidModal;
    function loadCovid() {
        if (window.Modal != undefined) {
            covidDynamite = new Modal({
                content: covid,
                addColor: 'red',
                overlay: true,
                autoOpen: false,
                className: 'fadeIt',
                width: 300,
            });
            covid.hide();
            window.anyPopup = true;
        } else {
            setTimeout(loadCovid, 100);
        }
    }
    if (!window.anyPopup) {
        loadCovid();
    }
