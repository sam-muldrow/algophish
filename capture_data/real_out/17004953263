<script>
    
    AddToCart = true;
    
    if(AddToCart){
        $("#ddlCorePack").click(function () {
            var $this = $(this);
            $this.toggleClass("active");

            if ($this.hasClass("active"))
                $this.children(".dropdown").show();
            else
                $this.children(".dropdown").hide();
        });

        $("#showCartSummary").on("click", function () {
            if ($("body").hasClass("menu-open"))
                return false;

            var Core = getCookie("Build_Core"),
               Addons = getCookie("Build_Addons"),
               cartSummaryElem = $("#cartSummary"),
               IntlPrimaryAddons = getCookie("Primary_Addons"),
               packageName = [];

            //RESET CART SUMMARY
            cartSummaryElem.find(".corePackageList").children().remove();
            cartSummaryElem.find(".addOnsList").children().remove();
            cartSummaryElem.find(".international-addons").removeClass("show");
            cartSummaryElem.find(".viewCartButton").show();

            //GET PRODUCT NAME FROM PRODUCT ID
            if (Addons != null) {
                $.ajax({
                    type: "POST",
                    url: "/cart/includes/products.asmx/getproducts?productids=" + Addons,
                    dataType: "json",
                    contentType: "application/json; charset=utf-8",
                    success: function (data) {
                        var returnedstring = data.d;
                        var jsondata = $.parseJSON(data.d); //if you want your data in json
                        $.each(jsondata, function (index, value) {
                            packageName.push(jsondata[index]);
                        });

                        $.each(packageName, function (index, value) {
                            if (packageName[index] != null)
                                cartSummaryElem.find(".addOnsList").append($("<li>" + packageName[index] + "</li>"));
                        });
                    }
                });
            }

            if (Core != null) {
                cartSummaryElem.find(".corePackageList").append($("<li>" + Core + "</li>"));
                cartSummaryElem.find(".corePackageDropDown").hide();
            }
            else {
                cartSummaryElem.find(".corePackageDropDown").show();
                cartSummaryElem.find(".viewCartButton").hide();
                cartSummaryElem.find(".corePackageList").hide();
            }
            if (checkCore(getCookie("Build_Core"), getCookie("Primary_Addons"))) { CartRedirectPlan(); return false;}

            objOverlay.update(function (thisElem) {
                $("#main-nav").css("z-index", "999999");
                if ($("#dishOverlayWrapper").find(".dishOverlayContent").length > 0) {
                    var winWidth = $(window).width();

                    if (winWidth > 1024) {
                        if ($("#main-nav").hasClass("fixed")) {
                            $(".dishOverlayContent.cartSummary").css("top", "72px");
                        }
                        else {
                            $(".dishOverlayContent.cartSummary").css("top", "155px");
                        }
                    }
                    else if (winWidth > 767 && winWidth < 1025) {
                        $(".dishOverlayContent.cartSummary").css("top", "88px");
                    }
                    else {
                        $(".dishOverlayContent.cartSummary").css("top", "72px");
                    }
                }

                $(document).scroll(function () {
                    var winWidth = $(window).width();

                    if (winWidth > 1024) {
                        if ($("#main-nav").hasClass("fixed")) {
                            $(".dishOverlayContent.cartSummary").css("top", "72px");
                        }
                        else {
                            $(".dishOverlayContent.cartSummary").css("top", "155px");
                        }
                    }
                    else if (winWidth > 767 && winWidth < 1025) {
                        $(".dishOverlayContent.cartSummary").css("top", "88px");
                    }
                    else {
                        $(".dishOverlayContent.cartSummary").css("top", "72px");
                    }
                });
            });

        });

        $("#cartSummary").on("click", ".viewCartBtn", function () {
            $(this).hide();
            $("#cartSummary").find(".loadingAnimationButton").show();
            $("#dishOverlay").css({ 'background': '#000', 'opacity': '.8' });
            $("#main-nav").css({ 'z-index': '99999'});
        });
        $(function () {
         /*   objOverlay.update(function () {*/
                if ($("#main-nav").hasClass("fixed"))
                    $("#dishOverlayWrapper .dishOverlayContent.cartSummary").css("top", "70px !important");
                else
                    $("#dishOverlayWrapper .dishOverlayContent.cartSummary").css("top", "150px !important");
         /*   });*/
        });
            $(function(){
                $(".byd-order").hide();
                OrderNowButtonSwap(true);
                if (getCookie("Build_Core")!=null||getCookie("Build_Addons")!=null){
                    $(".icon-cart").addClass("full");
                }

            });
        }else{
            $("li.icon-cart").hide();
            $(".dish-order").hide();
            OrderNowButtonSwap(false);
        }

        function checkCore(Core, IntlPrimaryAddons){

            if (Core != null) {
                if (Core.indexOf("International") > -1 || Core.indexOf("Chinese") > -1) {
                    if (IntlPrimaryAddons != "true") {
                        $("#cartSummary").find(".international-addons").addClass("show");
                        $("#cartSummary").find(".viewCartButton").hide();
                    }
                    else{return true;}
                }
                else{return true;}
            }
            return false;
        }
        function CartRedirectPlan(){
            if (getCookie("oplot")!=null && getCookie("Montel") === null){
                Montel="1";
                if(getCookie("oplot")=="2"){
                    Montel="2";
                }createCookieInDays("Montel",Montel,30);
            }else{createCookieInDays("oplot",getCookie("Montel"),30);}
            CartPlan="https://www.dish.com/secure/dish-order/";

            var ourLoc=getCookie("location");
            if (ourLoc != null) {
                ourLoc = ourLoc.toUpperCase();
                if (ourLoc.indexOf(" PR ")>-1){    //WCR-29670
                    CartPlan+="dpr/";
                }
            }
            window.location=CartPlan;
        }
        function OrderNowButtonSwap(addOrder){
            if(!addOrder){
                orderButtons=$("#package-panels .package-panel a:contains('Add '),.all-wrapped-up a:contains('Add ')");nu=1;
                $(orderButtons).each(function(){
                    $(this).text("Order Now");
                    $(this).attr(href,"/redirects/csa_links/?csa=byd");
                });
            }
            else{
                var CoreSelectors="#package-panels .package-panel a:contains('Order ')";
                    CoreSelectors+=",.all-wrapped-up a:contains('Order N')"
                    CoreSelectors+=",#channels-grid a:contains('Order ')"
                $(CoreSelectors).each(function(){
                    //$(this).text("Order Now");
                    //$(this).attr("href","#");
                    try{
                    if($(this).attr("href").indexOf("pack=I")>-1){
                        if($(this).attr("href").indexOf("pack=IBASIC")>-1){
                            //createCookieInDays("Build_Core", "International Basic", 30);
                        }
                        else if ($(this).attr("href").indexOf("pack=ICHIN") > -1) {
                            {
                              //  createCookieInDays("Build_Core", "Chinese Basic", 30);
                            }
                        }
                        if (getCookie("Build_Addons")==null){
                            $(this).attr("href","/international-packages/");
                        }
                    }
                    }catch(e){}
                });
            }
            return addOrder;
        }
</script>
