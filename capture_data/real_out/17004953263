<script>
    //if (partnerCookie) // hide DTV2DISH sticky nav for only partners.  5/12/2023
        //$D('#DTVdisclaimer').hide();

  const disclaimerOne = document.querySelector(".disclaimerOne");
  const disclaimerTwo = document.querySelector(".disclaimerTwo");
  const awardDisclaimer = document.querySelector(".award-disclaimer");
  let packageCols = document.querySelectorAll("#legal-disclaimers .package-col");

  disclaimerOne.addEventListener("click", function (e) {
    e.preventDefault();
    this.classList.add("hide");
    disclaimerTwo.classList.add("show");
    awardDisclaimer.classList.add("show");
    awardDisclaimer.style = "display: inline;";
  });

  disclaimerTwo.addEventListener("click", function (e) {
    e.preventDefault();
    this.classList.remove("show");
    disclaimerOne.classList.remove("hide");
    awardDisclaimer.classList.remove("show");
    awardDisclaimer.style = "";
  });
</script>

            </div>
          </div>
        </div>
        <script src="/global/configurator/configurator.js"></script>
