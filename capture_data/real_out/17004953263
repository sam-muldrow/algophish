    var securityBtn = document.querySelectorAll(".security-dish");

    function securityConstr() {
      //console.log('CLICKED');
      var bodyEl = document.querySelector("body"),
        sheet = document.createElement("DIV"),
        foundation = document.createElement("DIV"),
        textCont = document.createElement("P"),
        closeMsg = document.createElement("BUTTON");
      message = 'For any security concerns please contact: <a href="mailto:Security@dish.com" style="text-decoration: underline; color: #fff;"  >Security@dish.com</a>';
      textCont.style.cssText = "color: #fff; text-align: center; ";
      closeMsg.setAttribute("value", "Close");
      closeMsg.textContent = "Close X";
      closeMsg.style.cssText = "display: block; position: absolute; top: 0; right: 0; padding: 8px; color: white; background: none; border: none;";
      sheet.id = "securityBed";
      sheet.style.cssText = "display: flex; flex-direction: column; flex-wrap: nowrap; justify-content: center; position: fixed; top: 0; bottom: 0; right: 0; left: 0; background: rgba(0,0,0,.9); z-index: 99999999999; transition: opacity .12s ease-out;";
      foundation.style.cssText = "display: flex; align-self: center; position: relative; padding: 3vw 6vw; width: 90%; max-width: 600px;";
      textCont.innerHTML = message;
      foundation.appendChild(textCont);
      foundation.appendChild(closeMsg);
      sheet.appendChild(foundation);
      bodyEl.appendChild(sheet);
      bodyEl.style.overflow = "hidden";

      function securityClose() {
        var sheetEl = document.getElementById("securityBed");
        sheetEl.parentNode.removeChild(sheetEl);
        bodyEl.style.overflow = "";
      }
      closeMsg.addEventListener("click", securityClose, false);
    }

    for (const s of securityBtn) {
      s.addEventListener("click", function (event) {
        event.preventDefault();
        securityConstr();
      });
    }
  </script>
</footer>

