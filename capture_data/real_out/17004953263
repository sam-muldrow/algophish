<script>
    const gcDisclaimerOpen = document.querySelector("#sb-threehundogiftcard .gcDisclaimer");
    const gcDisclaimerClose = document.querySelector("#sb-threehundogiftcard .gcDisclaimer-close");
    const disclaimerPlus = document.querySelector("#sb-threehundogiftcard .disclaimerHidden");

    gcDisclaimerOpen.addEventListener("click", function (e) {
      e.preventDefault();
      this.classList.add("hide");
      disclaimerPlus.classList.add("show");
      disclaimerPlus.style = "display: inline;";
    });

    gcDisclaimerClose.addEventListener("click", function (e) {
        e.preventDefault();
        gcDisclaimerOpen.classList.remove("hide");
        disclaimerPlus.classList.remove("show");
        disclaimerPlus.style = "";
    });
</script>
