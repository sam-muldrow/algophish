    (function() {
        $D('#ddlCorePackAlert, #ddlAddonPackAlert').on('click', function() {
            this.toggleClass('active');

            if (this.hasClass('active')) this.getChildren('.dropdown').show();
            else this.getChildren('.dropdown').hide();
        });

        $D('#cartAlert .viewCartBtn').on('click', function() {
            this.hide();
            $D('#cartAlert')
                .find('.loadingAnimationButton')
                .show();
