  //US62134 - check agent availablity time.. 5am - 11pm.
  function isAgentAvailable() {
    //+ ":" +DateTime.Now.Minute
    let status = false;
    const currtime = "8";
    const iCurrtime = 0;
    iCurrtime = parseInt(currtime);
    if (iCurrtime >= 5 && iCurrtime <= 23) status = true;
    return status;
  }
