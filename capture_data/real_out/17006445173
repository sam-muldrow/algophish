                spaceBetween: 16,
                autoplay: {
                    delay: 3000,
                },
                loop: true,
                autoHeight:true,
                navigation: {
                    nextEl: ".swiper-button-next",
                    prevEl: ".swiper-button-prev",
                },
                pagination: {
                    el: ".swiper-pagination",
                },
                thumbs: {
                    swiper: swiper,
                },
            });

        $(".swiper-pagination-num").text(swiper2.slides[1].ariaLabel);

        swiper2.on('slideChange', function () {
            $(".swiper-pagination-num").text(swiper2.slides[swiper2.realIndex+1].ariaLabel);
        });
        

