    $(document).ready(function() {
         
        var swiper = new Swiper(".homeSwiper", {
                    spaceBetween: 16,
                    loop: true,
                    slidesPerView: 4,
                    freeMode: true,
                    //autoplay: {delay: 3000,},
                    watchSlidesProgress: true,
