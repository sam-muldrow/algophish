  var load = function load(url) {
    var h = document.getElementsByTagName('head')[0];
    var l = document.createElement('link');
    l.rel = 'stylesheet';
    l.href = url;
    h.appendChild(l);
  };

  var initRegularCSSChunk = function (url) {
    if (url) {
      var raf = requestAnimationFrame || mozRequestAnimationFrame || webkitRequestAnimationFrame || msRequestAnimationFrame;

      if (raf) {
        load(url);
      } else {
        window.addEventListener('domContentLoaded', function () {
          load(url);
        });
      }
    }
  };

    initRegularCSSChunk('/_chunks/content/privacyPopup/default-1fdbb4103dd662da915a.css')
    initRegularCSSChunk('/_chunks/page/front/default/default-d2c0a881e75a6ab8fb5f.css')
    initRegularCSSChunk('/_chunks/page/global/site/dagbladet/default-8c5b220bf6f482881a90.css')
