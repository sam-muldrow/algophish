<script>
function hideNajnowsze() {
 var sidebar= jQuery(".sidebar").height();     //  console.log(sidebar);
 var maincont= jQuery(".maincont").height(); //console.log(maincont);
 var count = jQuery('.listNew .detailsNew').length; //console.log(count);
 if(count>5){
  var diff=    sidebar -  maincont;   
      if(sidebar > maincont) {
          jQuery('.listNew .detailsNew:last-child').remove();
          hideNajnowsze();
      }
 }
// console.log(count);
}
function hideNajnowszeSmall() {
 var sidebarS= jQuery(".firstPart").height();       
 var maincontS= jQuery(".secondPart").height();
 var count = jQuery('.listNew .detailsNew').length;
 if(count>15){
  var diffS=    sidebarS -  maincontS;
          // console.log(diffS) ;
      if(diffS > 35) {
          jQuery('.listNew .detailsNew:last-child').remove();
          hideNajnowszeSmall();
      }
 }
}
jQuery( document ).ready(function() {  
    if(jQuery('iframe').hasClass('iframeRmf24')  ){
      jQuery.ajaxSetup({cache:true});
      jQuery.getScript( "/j/iframeSizer.min.js" ) 
        .done(function( script) {   
        
         jQuery(".iframeRmf24").iFrameResize({  
				      checkOrigin: false, 
              log:false,
              enablePublicMethods     : true,                  
              heightCalculationMethod : 'lowestElement', 
             
           });
         })
     };
  
    
    var minWidth=jQuery( document ).width() ;
    if(minWidth>992){
      
     
      if(jQuery('.isBig')){
           jQuery('.najpopHidden').show();
        }
        
     hideNajnowsze();
     setTimeout(function(){ hideNajnowsze();  }, );
   //  myVar = setTimeout(hideNajnowsze, 300);
    jQuery(window).on('resize scroll', function() {
      hideNajnowsze();
    });
    } else if(minWidth>480 && minWidth<992){
         //   jQuery('.mainContent_blog').show();
           hideNajnowszeSmall() ; 
       }
});
</script>




<script>
  function ll_rmf24() {
       if ("loading" in HTMLImageElement.prototype && document.querySelectorAll('img.lazyload:not([loading="lazy"])').length == 0) { console.log('lazy');
            var images = document.querySelectorAll('img[loading="lazy"]');
           // var scripts = document.querySelectorAll('div[data-script]');
            var sources = document.querySelectorAll("source[data-srcset]");
            sources.forEach(function (source) {
                source.srcset = source.dataset.srcset;
            });
            images.forEach(function (img) {
                img.src = (img.dataset && img.dataset.src) ? img.dataset.src : '/s/classic/p.gif';
            });
           /* scripts.forEach(function (el) {
                var script = document.createElement("script");
                script.src = el.getAttribute('data-script');
                script.setAttribute('loading', 'lazy');
                el.append(script);
            });*/
        } else {
          window.lazySizesConfig = window.lazySizesConfig || {};
          window.lazySizesConfig.lazyClass = 'lazy';
          window.lazySizesConfig.throttleDelay=50;
            var script = document.createElement("script");
            script.src = "/j/lazySizes/lazysizes.min.js";
            document.body.appendChild(script); 
        }
  };
  ll_rmf24();
</script>
