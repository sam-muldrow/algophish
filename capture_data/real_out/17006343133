<script>
jQuery(function(){ 
  jQuery("#readMore").click(function(event){  
        event.preventDefault();
       var loader=jQuery("#moreArticles  .loader");
       var minWidth=jQuery( document ).width() ;
       jQuery(".wiecej").hide();  
       loader.show();                                           
       jQuery("#readMore").hide(); 
       setTimeout(function(){
          jQuery(".hiddenNews").slideDown( "slow" ); 
          loader.hide(); 
          jQuery("#readMore2").slideDown( "slow" );  
        if(minWidth>992){
         // jQuery('.mainContent_blog').show();    
          jQuery('.mainContent_blog').attr('style','display: block !important'); 
          jQuery('.najpopHidden').attr('style','display: block !important'); 
          jQuery('#podcastymain').attr('style','display: block !important'); 
        //  jQuery('.podcastHidden').slideDown( "slow" ); 
           hideNajnowsze(); 
          }
         
       }, 1500); 
                                              
  })
})                                               
</script>      
