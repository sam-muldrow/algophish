    <script>
    jQuery(document).ready(function() {
        var minWidth=jQuery( document ).width() ;
        if(minWidth<1290){
            //jQuery('.line').remove('.line');
            jQuery('.line').contents().unwrap();
            var $set = jQuery('#najwazniejsze ul').children();    
            for(var i=0, len = $set.length; i < len; i+=2){
                $set.slice(i, i+2).wrapAll('<div class="smallLine"/>');
            } 
       
        }else{
            
             jQuery('.isBig').each(function() {
           
              var position = jQuery(this).data( "position" ) ;
              var side = jQuery(this).data( "side" ) ;
              var linia = jQuery(this).data( "linia" ) ;
              if(side == 'left' && position== 'z2'){
                  jQuery('.'+linia+'.z1').addClass('pull-right');
                  jQuery('.'+linia+'.z0').addClass('pull-right');
              }else if(side === 'left' && position === 'z0'){
                  var $wrapper = jQuery(this).detach();
                  $wrapper.insertBefore('.'+linia+'.z1');
              }else if(side === 'right' && position === 'z0'){
                  var $wrapper = jQuery(this).detach();
                  $wrapper.insertBefore('.'+linia+'.z2');
              }
            });
            
        }
    });

    </script>
