        <script language="javascript" type="text/javascript">
function getClientHeight(){
return typeof( window.innerHeight) !== "undefined" ?
window.innerHeight :
document.documentElement.clientHeight;
}

function getClientWidth(){
return typeof( window.innerWidth) !== "undefined" ?
window.innerWidth :
document.documentElement.clientWidth;
}

function fixFloatElement() {
// get client height
var offset = getClientHeight();
// get client width
var offsetWidth = getClientWidth();
// get the element you want to change position to
var footer = document.getElementById('liveBar');
var footer2 = document.getElementById('liveButton');
// get the element right above the previous element
var main = document.getElementById('container');
// set styles to the element to emulate position:fixed
footer.style.position = 'absolute';
footer.style.top = (document.documentElement.scrollTop + offset - footer.offsetHeight) + 'px';
footer.style.width = (offsetWidth) + 'px';

footer2.style.position = 'absolute';
footer2.style.top = (document.documentElement.scrollTop + offset - footer2.offsetHeight) + 'px';
footer2.style.width = '26px';

}

// If browser is IE6
if (navigator.userAgent.toLowerCase().indexOf('msie 6')!=-1) {
			window.onload = fixFloatElement;
			window.onresize = fixFloatElement;
			window.onscroll = fixFloatElement;
		}




</script>



