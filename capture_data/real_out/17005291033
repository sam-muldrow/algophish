<script async>
  require(['lib/jQuery', 'tnc/gallery/View', 'tnc/gallery/Source'], function(jQuery, Gallery, Source){
    var $slider = jQuery('.advantage-slider');
    var $items = $slider.children().remove();
    var source = new Source($items.get());
    var gallery = new Gallery(source, $slider.html('<div class="tnc-gallery-view"><div class="tnc-gallery-payload"></div></div>').get(0), {
      swipeable: true, capacity: 1
    });
    var $bullets = $slider.append('<div class="circling-buttons">' + source.map(function(node){
      return '<a href="#" data-uuid="' + node.uuid + '"><i aria-hidden="true" class="fa fa-circle"></i></a>';
    }).toArray().join('') + '</div>').find('.circling-buttons').on('click', 'a', function(){
      gallery.moveToNode(source.hash[this.getAttribute('data-uuid')]);
    }).children();
    gallery.generate = function(node, element){
      $bullets.eq(source.indexOf(node)).addClass('active').siblings().removeClass('active');
      element.appendChild(node.data);
    };
    gallery.init();
  });
</script>
