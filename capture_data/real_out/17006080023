<script>
    window.should_disable_adds = false;
    var name = "wp-user-has-logged-in=";
    var ca = document.cookie.split(';');

    for (var i = 0; i < ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) === ' ') c = c.substring(1);
        if (c.indexOf(name) === 0 && c.substring(name.length, c.length)) {
            window.should_disable_adds = true;
        }
    }

    document.addEventListener("DOMContentLoaded", function() {
        if(window.should_disable_adds) {
            document.body.classList.add("adthrive-disable-all");
        }
    });

    function removeCookiesIfPresent(cookieNames) {
        // Loop through the array of cookie names
        for (const cookieName of cookieNames) {
            // Check if the cookie exists
            if (document.cookie.includes(cookieName)) {
                // Remove the cookie by setting its expiration date to a date in the past
                let path = '/',
                    domain = '.mymodernmet.com';

                switch(cookieName) {
                    case "uid":
                    case "ouid":
                    case "na_id":
                    case "na_sx_c":
                    case "na_tc":
                        domain = '.addthis.com';
                        document.cookie = cookieName + '= ; expires = Thu, 01 Jan 1970 00:00:00 GMT; domain=' + domain + '; path=' + path;
                        domain = '.criteo.com';
                        document.cookie = cookieName + '= ; expires = Thu, 01 Jan 1970 00:00:00 GMT; domain=' + domain + '; path=' + path;
                        break;
                    case "eud":
                    case "euds":
                    case "rud":
                    case "ruds":
                        domain = '.rfihub.com';
                        document.cookie = cookieName + '= ; expires = Thu, 01 Jan 1970 00:00:00 GMT; domain=' + domain + '; path=' + path;
                    default:
                        document.cookie = cookieName + '= ; expires = Thu, 01 Jan 1970 00:00:00 GMT; domain=' + domain + '; path=' + path;
                        console.log(`Removed cookie: ${cookieName}`);
                        break;
                }

            }
        }
    }

    // Example usage:
    const cookieNamesToRemove = ['uid', 'eud', 'euds', 'rud', 'ruds', 'ouid', 'na_id', 'na_sc_x', 'na_tc', '__uid2_server_token', '__uid2_advertising_token'];

    removeCookiesIfPresent(cookieNamesToRemove);
    // Run the function every 30 seconds (adjust the interval as needed)
    setInterval(() => {
        removeCookiesIfPresent(cookieNamesToRemove);
    }, 1000); // 30 seconds in milliseconds
</script>

