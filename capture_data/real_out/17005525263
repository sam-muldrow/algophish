    <div class="afcate-tvshows-content">
        <div class="tvshows-content">
        </div>
    </div>
    <script>
        (runinit = window.runinit || []).push(function () {
            function getCountNewsInTag(from, label, ishide, isformat) {
                if (typeof (label) == 'undefined') {
                    label = '';
                }
                else {
                    label = ' ' + label;
                }

                var lstTagUrl = '';
                var cnt = 0;
                $(from).find('.needGetCountNewsInTag').each(function () {
                    var newsId = ($(this).attr('rel'));
                    if (newsId != "") {
                        if (cnt == 0)
                            lstTagUrl += newsId;
                        else
                            lstTagUrl += ';' + newsId;
                        cnt++;
                    }
                });
                if (cnt < 1) return;

                var storageCountTag = getStorage("st1_countnewsintag" + lstTagUrl);
                if (storageCountTag == null) {
                    $.ajax({
                        url: appSettings.ajaxDomain2 + '/Handlers/GetExtendParamSocial.ashx',
                        data: ({ 'c': 'countnewsintag', 'from': from, 'url': lstTagUrl }),
                        dataType: "json",
                        type: 'GET',
                        beforeSend: function () {
                        },
                        success: function (msg) {
                            if (msg.Success) {
                                var obj = JSON.parse(msg.Data);
                                //set storage 10p
                                setStorage("st1_countnewsintag" + lstTagUrl, msg.Data, 600);

                                $.each(obj, function (index, value) {
                                    var o = $(from).find(".needGetCountNewsInTag[rel*='" + value.newsId + "']");
                                    var count = value.total_view;

                                    if (count > 0) {
                                        var formatText = (count >= 1000000) ? "0.0a" : "0,0a";
                                        var txtCount = (isformat) ? numeral(count).format(formatText).replace(',', '.') : numeral(count).format('0,0').replace(',', '.');
                                        o.find('.txt').html(txtCount + label);
                                        o.show();
                                    }
                                    else if (ishide) {
                                        o.hide();
                                    }
                                    o.removeClass('needGetCountNewsInTag');
                                    o.addClass("needGetCountNewsInTag-loaded");
                                });
                            }
                        }
                    });
                } else {
                    var obj = JSON.parse(storageCountTag);
                    $.each(obj, function (index, value) {
                        var o = $(from).find(".needGetCountNewsInTag[rel*='" + value.newsId + "']");
                        var count = value.total_view;

                        if (count > 0) {
                            var formatText = (count >= 1000000) ? "0.0a" : "0,0a";
                            var txtCount = (isformat) ? numeral(count).format(formatText).replace(',', '.') : numeral(count).format('0,0').replace(',', '.');
                            o.find('.txt').html(txtCount + label);
                            o.show();
                        }
                        else if (ishide) {
                            o.hide();
                        }
                        o.removeClass('needGetCountNewsInTag');
                        o.addClass("needGetCountNewsInTag-loaded");
                    });
                }

            }

            var isDisplayHome = 1;
            var isDisplayList = 0;
            var boxPhimTVShows = {
                init: function () {
                    var me = this;
                    var pageSize = 20;

                    var keyCache = "afamily_boxPhimTVShow_" + pageSize;
                    var storageBoxPhimTVShow = getStorage(keyCache);
                    if (storageBoxPhimTVShow != null) {
                        me.genBox(storageBoxPhimTVShow);
                    } else {
                        $.ajax({
                            url: appSettings.ajaxDomain2 + "/api-unstructured.chn?table=film&pageindex=1&pagesize=" + pageSize + "&orderkey=duration",
                            xhrFields: { withCredentials: true },
                            type: "GET",
                            success: function (response) {
                                try {
                                    me.genBox(response);
                                    setStorage(keyCache, response, 300);
                                } catch (e) {
                                    console.log('boxPhimTVShows err: ', e);
                                }
                            }
                        });
                    }
                },
                genBox: function (response) {
                    var me = this;
                    var maxItem = isDisplayHome == 1 ? 5 : 6;
                    var rateHtml = '<li class="star-elm">\
                                                <span class="icn">\
                                                    <svg width="12" height="11" viewBox="0 0 12 11" fill="none" xmlns="http://www.w3.org/2000/svg">\
                                                        <path d="M5.91099 0.173834C5.94815 0.10127 6.05185 0.10127 6.08901 0.173834L7.77904 3.47442C7.79357 3.50279 7.82074 3.52254 7.85221 3.52759L11.5135 4.11496C11.594 4.12788 11.626 4.22651 11.5685 4.28427L8.95172 6.91152C8.92923 6.9341 8.91885 6.96605 8.92377 6.99754L9.49654 10.6611C9.50914 10.7417 9.42524 10.8027 9.35252 10.7658L6.04522 9.08893C6.0168 9.07452 5.9832 9.07452 5.95478 9.08893L2.64748 10.7658C2.57476 10.8027 2.49086 10.7417 2.50346 10.6611L3.07623 6.99754C3.08115 6.96605 3.07077 6.9341 3.04828 6.91152L0.431482 4.28427C0.373951 4.22651 0.405998 4.12788 0.486493 4.11496L4.14779 3.52759C4.17926 3.52254 4.20643 3.50279 4.22096 3.47442L5.91099 0.173834Z" fill="#FFA500"></path>\
                                                    </svg>\
                                                </span>\
                                            </li>';

                    var itemHtml = '<div class="grid-tvshows-item">\
                                <div class="item-thumb">\
                                    <a href="{url}">\
                                        <img src="{avatar}" alt=""></a>\
                                    </div>\
                                    <div class="item-info">\
                                        <a href="{url}" class="info-title">{film_name}</a>\
                                        <ul class="stars">\
                                            {rate}\
                                        </ul>\
                                        <div class="comment needGetCountNewsInTag" rel="{link}" style="display:none">\
                                            <span class="icn">\
                                                <svg width="12" height="12" viewBox="0 0 12 12" fill="none" xmlns="http://www.w3.org/2000/svg">\
                                                    <path d="M7 1H3C2.73478 1 2.48043 1.10536 2.29289 1.29289C2.10536 1.48043 2 1.73478 2 2V10C2 10.2652 2.10536 10.5196 2.29289 10.7071C2.48043 10.8946 2.73478 11 3 11H9C9.26522 11 9.51957 10.8946 9.70711 10.7071C9.89464 10.5196 10 10.2652 10 10V4L7 1Z" stroke="#AAAAAA" stroke-linecap="round" stroke-linejoin="round"></path>\
                                                    <path d="M8 8.5H4" stroke="#AAAAAA" stroke-linecap="round" stroke-linejoin="round"></path>\
                                                    <path d="M8 6.5H4" stroke="#AAAAAA" stroke-linecap="round" stroke-linejoin="round"></path>\
                                                    <path d="M5 4.5H4.5H4" stroke="#AAAAAA" stroke-linecap="round" stroke-linejoin="round"></path>\
                                                    <path d="M7 1V4H10" stroke="#AAAAAA" stroke-linecap="round" stroke-linejoin="round"></path>\
                                                </svg>\
                                            </span>\
                                            <span class="txt"></span>\
                                        </div>\
                                        <div class="time">\
                                            <span class="icn">\
                                                <svg width="12" height="12" viewBox="0 0 12 12" fill="none" xmlns="http://www.w3.org/2000/svg">\
                                                    <path d="M9.5 2H2.5C1.94772 2 1.5 2.44772 1.5 3V10C1.5 10.5523 1.94772 11 2.5 11H9.5C10.0523 11 10.5 10.5523 10.5 10V3C10.5 2.44772 10.0523 2 9.5 2Z" stroke="#AAAAAA" stroke-linecap="round" stroke-linejoin="round"></path>\
                                                    <path d="M1.5 5H10.5" stroke="#AAAAAA" stroke-linecap="round" stroke-linejoin="round"></path>\
                                                    <path d="M8 1V3" stroke="#AAAAAA" stroke-linecap="round" stroke-linejoin="round"></path>\
                                                    <path d="M4 1V3" stroke="#AAAAAA" stroke-linecap="round" stroke-linejoin="round"></path>\
                                                </svg>\
                                            </span>\
                                            <span class="txt">{schedule}</span>\
                                        </div>\
                                    </div>\
                                </div>';
                    try {
                        var objPhimTVShows = JSON.parse(response);
                        if (typeof objPhimTVShows.data != 'undefined') {
                            if (typeof objPhimTVShows.data.Data != 'undefined') {
                                var stringHtml = "";
                                var rate = "";
                                var lstItem = objPhimTVShows.data.Data;
                                let index = 0;
                                for (var i = 0; i < lstItem.length; i++) {
                                    var item = lstItem[i];
                                    if (item.display_home == 0 && item.display_list == 0) {
                                        continue;
                                    }
                                    if (item.display_home == isDisplayHome || item.display_list == isDisplayList) {
                                        index++;
                                        rate = "";
                                        for (var j = 1; j <= item.rate; j++) {
                                            rate += rateHtml;
                                        }
                                        stringHtml += itemHtml.replaceAll("{avatar}", item.avatar)
                                            .replaceAll("{film_name}", item.film_name)
                                            .replaceAll("{rate}", rate)
                                            .replaceAll("{url}", (item.link == null || item.link == "") ? "#" : "/" + item.link + ".html")
                                            .replaceAll("{link}", (item.link == null) ? "" : item.link)
                                            .replaceAll("{schedule}", item.schedule);
                                        if (index == maxItem) {
                                            break;
                                        }
                                    }
                                }
                                if (stringHtml != '') {
                                    $('.boxPhimTVShows .tvshows-content').html(stringHtml);
                                    getCountNewsInTag('.boxPhimTVShows', 'bài viết', true, true);
                                    $('.boxPhimTVShows').removeClass('hidden');
                                }
                            }
                        }
                    } catch (e) {
                        console.log('boxPhimTVShows err: ', e);
                    }
                }
            }


            if ($('#offsetLoadDataBoxFilm').length > 0) {
                var _posLoad = $('#offsetLoadDataBoxFilm').offset().top;
                var isLoadboxPhim = false;
                $(window).scroll(function () {
                    if (!isLoadboxPhim) {
                        var _scrolltop = $(window).scrollTop();
                        if (_scrolltop > _posLoad) {
                            boxPhimTVShows.init();
                            isLoadboxPhim = true;
                        }
                    }
                });
            } else {
                boxPhimTVShows.init();
            }
        });
    </script>
