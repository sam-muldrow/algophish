    <script>
        (runinit = window.runinit || []).push(function () {
            //Tiêu dùng thông thái
            getBoxprById('525310', bindDataTieuDungThongThai);
            //Mẹ và bé
            getBoxprById('525311', bindDataMeVaBe);
            //deal hot mỗi ngày
            getBoxprById('525312', bindHotDeal);
        });

        (runinit = window.runinit || []).push(function () {
            window.addEventListener('scroll', function () {
                stickyScroll();
            });
            if ($(".sticky_hv2").length > 0) {
                var pinTop = 0;
                var scroll = 0;
                var bottomstream = 0;
                var oldItemHeight = $(".sticky_hv2").outerHeight();
                $(".sticky_hv2").before('<div id="topPinSticky" style="display: block; height: 1px;"></div>');
                var startSwitch = 0
            }
            function stickyScroll() {
                bottomstream = $("#bottomstream").offset().top - oldItemHeight;
                var $this = $(".sticky_hv2");
                if ($("#topPinSticky").length > 0) {
                    pinTop = $("#topPinSticky").offset().top;

                    if (startSwitch == 0) {
                        if (bottomstream > pinTop) {
                            startSwitch = 1;
                        }
                        return;
                    }
                    scroll = $(window).scrollTop();

                    if (scroll > (pinTop - 50)) {
                        if (scroll < (bottomstream)) {
                            $this.css({ "position": "fixed", "top": "50px", "bottom": "auto", "width": "200px" });
                        } else {
                            $this.css({ "position": "absolute", "top": "auto", "bottom": "25px", "width": "200px" });
                        }
                    } else {
                        $this.css({ "position": "unset", "top": "unset" });
                    }
                }
            }

        });
    </script>

    


