		/** BANNER STICKY **/
		$(function(){
			const initialBannerSticky = () => {
				const offsetHeader = 15;
				const $bannerSticky = $('.js--sidebar-banner-sticky');
				if (!$bannerSticky.length) return;
				const bannerPaddingTop = $('.p-navSticky--primary').height() + offsetHeader
				const bannerY = $bannerSticky.offset().top - bannerPaddingTop;

				const handleBannerSticky = () => {
					const $bodyMain = $('.p-body-main--withSidebar');
					const $bodySidebar = $('.p-body-sidebar');
					const stickyLimitY = $bodyMain.height() + $bodyMain.offset().top - $bannerSticky.height()
					if ($(this).scrollTop() >= stickyLimitY) {
						$bodySidebar.css('position', 'relative');
						$bannerSticky.css({
							position: 'absolute',
							top: '',
							bottom: 0
						})
						return;
					}
					if ($(this).scrollTop() > bannerY) {

						$bannerSticky.css({
							position: 'fixed',
							top: bannerPaddingTop + 'px',
							bottom: ''
						})
						return
					}
					$bannerSticky.css({
						position: '',
						top: '',
						bottom: '',
					})
				}
				handleBannerSticky();
				$(window).scroll(function() {
					handleBannerSticky();
				})
			}
			setTimeout(() => {
				initialBannerSticky() 
			}, 1000);
		});
	</script>
	
