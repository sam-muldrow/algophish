<!-- Script START -->
<script src="https://www.google.com/recaptcha/api.js?render=6LcGcL8UAAAAANDI_Lx-DYrWbMac-7ggSUxPQ3xX"></script>
<script>
    function submitFMP( event ) {
        event.preventDefault ? event.preventDefault() : (event.returnValue = false);
        formId = event.target.id;
        var receiverError = $("#" + formId).find("#receiver_error");
        receiverError.hide();

        grecaptcha.ready(function() {
            grecaptcha.execute('6LcGcL8UAAAAANDI_Lx-DYrWbMac-7ggSUxPQ3xX', {action: formId}).then(function(token) {

                var receiverReference = $("#" + formId).find("#receiver_reference").val().split(' ').join('');
                var receiverPostcode  = $("#receiver_postcode").length ? $("#receiver_postcode").val().split(' ').join('') : "";

                if ( receiverReference ) {
                    $("#receiver_reference").val( receiverReference );
                    $("#receiver_postcode").val( receiverPostcode );

                    $('<input>').attr({
                        type: 'hidden',
                        name: 'captchaResponse',
                        value:   token
                    }).appendTo('#' + formId);

                    var qaEnvUrl = "";

                    if (window.location.href.indexOf("qa5") > -1) {
                      qaEnvUrl = "q2-";
                    }

                    var trackUrl = 'https://apis.' + qaEnvUrl + 'track.dpd.co.uk/v1/track?postcode=' + receiverPostcode + '&parcel=' + receiverReference;

                    console.log("trackUrl ==== ", trackUrl);

                    window.location.href = trackUrl;
                    //document.getElementById(formId).submit();

                } else {
                    receiverError.html( "Please enter a reference" ).show();
                    return false;
                }
        }) });
    }
</script>
<!-- Script End -->

