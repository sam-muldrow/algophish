    <script defer src="https://use.fontawesome.com/releases/v5.0.9/js/all.js" integrity="sha384-8iPTk2s/jMVj81dnzb/iFR2sdA7u06vHJyyLlAd4snFpCl/SnyUjRrbdJsw1pGIl" crossorigin="anonymous"></script>
    <script>
        function submitQBForm() {
            // get form data
            var _from = document.getElementById("qqfrom").value || document.getElementById("qqfromMobile").value;
            var _to = document.getElementById("qqto").value || document.getElementById("qqtoMobile").value;
            var _weight = document.getElementById("qqweight").value || document.getElementById("qqweightMobile").value;

            if (_from && _to && (_weight > 0)) {

                var qqPostcodes = "";

                var _colP = document.getElementById("qqCP").value || document.getElementById("qqCPMobile").value;
                if (_colP) {
                    qqPostcodes += "&cp=" + _colP;
                }

                var _delP = document.getElementById("qqDP").value || document.getElementById("qqDPMobile").value;
                if (_delP) {
                    qqPostcodes += "&dp=" + _delP;
                }


                var location = 'https://www.dpdlocal-online.co.uk/quotes?col=' + _from + '&dest=' + _to + qqPostcodes + '&p=1~' + _weight;

                var _length = document.getElementById("qqlength").value || document.getElementById("qqlengthMobile").value;
                var _width = document.getElementById("qqwidth").value || document.getElementById("qqwidthMobile").value;
                var _height = document.getElementById("qqheight").value || document.getElementById("qqheightMobile").value;

                if (_length && _width && _height) {
                    location += "|" + _length + "|" + _width + "|" + _height;
                }

                var win = window.open(location, '_blank');
                if (win) {
                    //Browser has allowed it to be opened
                    win.focus();
                } else {
                    //Browser has blocked it
                    window.location = location;
                }
            }
        }

        function getQuote() {
            document.getElementById("qqDimensions").classList.add("hidden");
            document.getElementById("qqGetQuote").classList.add("selected");
            document.getElementById("qqLongerOneMetre").classList.remove("selected");
            document.getElementById("qqSubmitButtonGetQuote").classList.remove("hidden");
            document.getElementById("weightContainerButton").classList.remove("small-12");
            document.getElementById("weightContainerButton").classList.add("small-6");
        }

        function longerOneMetre() {
               document.getElementById("qqDimensions").classList.remove("hidden");
            document.getElementById("qqGetQuote").classList.remove("selected");
            document.getElementById("qqLongerOneMetre").classList.add("selected");
            document.getElementById("qqSubmitButtonGetQuote").classList.add("hidden");
            document.getElementById("weightContainerButton").classList.add("small-12");
            document.getElementById("weightContainerButton").classList.remove("small-6");
        }

        /*mobile view */
        function getQuoteMobile() {
            document.getElementById("qqDimensionsMobile").classList.add("hidden");
            document.getElementById("qqGetQuoteMobile").classList.add("selected");
            document.getElementById("qqLongerOneMetreMobile").classList.remove("selected");
            document.getElementById("qqSubmitButtonGetQuoteMobile").classList.remove("hidden");
            document.getElementById("weightContainerButtonMobile").classList.remove("small-12");
            document.getElementById("weightContainerButtonMobile").classList.add("small-6");
        }

        function longerOneMetreMobile() {
            document.getElementById("qqDimensionsMobile").classList.remove("hidden");
            document.getElementById("qqGetQuoteMobile").classList.remove("selected");
            document.getElementById("qqLongerOneMetreMobile").classList.add("selected");
            document.getElementById("qqSubmitButtonGetQuoteMobile").classList.add("hidden");
            document.getElementById("weightContainerButtonMobile").classList.add("small-12");
            document.getElementById("weightContainerButtonMobile").classList.remove("small-6");
        }
        /*mobile view */
    </script>
