barJsLibraryLoad('/customize/pics/js/typed.min.js?v=eea798');

barCssLoad('/global/pics/css/material-icons.css?v=e6d856');
barCssLoad('/elearning/pics/css/etest.css?v=8a0d1a');
barCssLoad('/elearning/pics/css/etestEditor.css?v=eb30aa');
barCssLoad('/customize/pics/css/helpus.css?v=22a7b0');

	
barCssLoad('/global/pics/js/jquery/royalSlider/skins/edupage/rs-edupage-info.css?v=69aa65');
barCssLoad('/skin/pics/css/layoutDefault.css?v=a5ba54');
barCssLoad('/quickintro/pics/css/quickIntro.css?v=72f910');	
barCssLoad('/customize/pics/css/wizards.css?v=8a4a58');
barJsLibraryLoad('/global/pics/js/jquery/jquery.royalslider.min.js?v=3');
		
barJsLibraryLoad('/skin/pics/js/skinResponsiveScripts.js?v=ff88fd');
barJsLibraryLoad('/skin/pics/js/utilsEdit.js?v=8f01fa');
barJsLibraryLoad('/elearning/pics/js/etest/utilsETest.js?v=1e4196');
barLoadGoogleFont('Open Sans');

$j(document).ready(function($) {	
	var elem = $('#benefitsallbenefits_main');
	var openCard = "";
	var gotoCard = null;
	var openCardQi = null;
	var fullScreen = false;
	if (fullScreen) {
		barToggleEdubarLogo(false, true);
		barToggleSideBar(false);	
		barToggleEdubarBorder(false);
	}
	
	var leftLtr = ertl('left','right');

	var edubar = $('#edubar');
		
	var heroElem = elem.find('.helpus-hero');	
	
	var arrowBox = null;
	function handleScroll() {
		
		var posY = $(window).scrollTop();
		if (fullScreen) {
			barToggleEdubarBorder(posY >= heroElem.outerHeight()*0.5);
		}	
		
		sessionStorage.setItem('helpusMainSpqintro_all',posY);	
	}
	
	function initBackBtn() {
		var s = '<a href="/user/" class="edubarSmartLink">';
		s += '<i class="fa fa-fw fa-arrow-left"></i></a>';
		
		var btn = $(s).appendTo(edubar.find('.edubarHeader'));
		
	}
	

	elem.find('.helpus-hero-btn').click(function() {
		var scrollTop = elem.find('.helpus-intro-question').offset().top-25;
		$('html, body').animate({
			'scrollTop': scrollTop
		},500,function() {			
			elem.find('.helpus-intro-question b').typed({
		        strings: ["What do you want to do?"],
		        typeSpeed: 0,
		        callback: function() {		        	
		        	elem.find('.helpus-intro-question').find('.typed-cursor').remove();	
		        }
		    });
			elem.find('.helpus-intro-question .typed').each(function() {
				var ohtml = $(this).html();
				if ($(this).find('.inlineText').length == 0) {
					$(this).html('<span class="inlineText">'+ohtml+'</span>');
				}
				
				var that = this;
				$(this).find('.inlineText').typed({
					strings: [$(this).find('.inlineText').text()],
					typeSpeed: 0,
					callback: function() {		        	
						$(that).html(ohtml);
						$(that).find('.typed-cursor').remove();	
						
					}
				});
		    });			
		});		
	});
		$(document).off('keydown.helpusscripts').on('keydown.helpusscripts', function(e) {
		if (elem.closest('body').length == 0) {
			$(document).off('keydown.helpusscripts');
		}
		if (e.which == 32 || e.which == 39 || e.which == 37) {
			var nextCard = null;
			var prevCard = null;
			var bolaExpanded = false;
			var allCards = elem.find('.helpus-card');
			allCards.each(function() {					
				if (bolaExpanded && !nextCard) {
					nextCard = $(this);
				}
				if ($(this).hasClass('expanded')) {
					bolaExpanded = true;
				}
				if (!bolaExpanded) {
					prevCard = $(this);					
				}
			});
			if (!nextCard && !bolaExpanded) {
				nextCard = allCards.first();
			}			
		}
		if (e.which == 32 || e.which == 39) { //pagedown
			if (nextCard) {
				nextCard.find('.helpus-learnmore-btn').click();
			}
			
		} else if ( e.which == 37) {//pageup
			if (prevCard) {
				prevCard.find('.helpus-learnmore-btn').click();
			}
		}
	});
		
	elem.find(".helpus-learnmore-btn").click(function() {		
		var $btn = $(this);
		var $card = $(this).closest('.helpus-card');
		var cardid = $card.attr('data-id');
		
		
		var bolaAktKarta = false;
		var remScrollTop = 0;
		elem.find('.helpus-card').each(function() {	
			
			if ($(this).attr('data-id') == cardid) {
				bolaAktKarta = true;
				return;
			}
			if (!$(this).hasClass('expanded')) {
				return;
			}
						
			
			var h = $(this).find('.helpus-more,.helpus-more0').outerHeight();
			sessionStorage.setItem('helpusExp'+$(this).attr('data-id'),'0');
										
			if (!bolaAktKarta) {
				remScrollTop += h;							
			}
									
			
			$(this).toggleClass('expanded',false);
			$(this).find('.helpus-more,.helpus-more0').slideToggle(350);
			$(this).find('.helpus-learnmore-btn').each(function() {										
				var props = {};
				props[leftLtr] = '0';
				$(this).animate(props,350);

				$(this).find('i').html($(this).data('origIcon'));
				$(this).find('span').html($(this).data('origText'));
			});
					
		});
		
		$card.toggleClass('expanded');
		
		$card.find('.helpus-more,.helpus-more0').slideToggle(350);
		var origIcon = $(this).data('origIcon');
		if (!origIcon) {
			 origIcon = $(this).find('i').html();
			 $(this).data('origIcon', origIcon);			 
		}

		var origText = $(this).data('origText');
		if (!origText) {
			origText = $(this).find('span').html();
			$(this).data('origText', origText);	
		}

		$(this).find('i').html($card.hasClass('expanded') ? '&#xE5CD;' : origIcon);
		$(this).find('span').html($card.hasClass('expanded') ? 'Close' : $(this).data('origText'));
		if ($card.hasClass('expanded')) {	
			var scrollTop = $card.offset().top-65-remScrollTop; 
			if ($(window).width() <= 600) {				
				scrollTop = $btn.offset().top-80-remScrollTop;
			}	

			
			$('html, body').animate({
				'scrollTop': scrollTop
			},{
				duration: 350, 
				done: function() {	
					/*setTimeout(function() {			
						
						
						$('html, body').scrollTop($(window).scrollTop()-remScrollTop);	
					},1);*/
				}				
			});
				
			var offset = $btn.parent().offset();	
			var targetLeft = $(window).width()/2;
			var props = {};
			if (ertl('left','right') == 'right') {
				offset.left = $(window).width() - (offset.left+$btn.parent().outerWidth());
			}
			props[leftLtr] = targetLeft - offset.left - 25;
			if (!$(this).hasClass('helpus-learnmore-btn-nm')) {
				$btn.animate(props,350);
			}
			
			sessionStorage.setItem('helpusExp'+cardid,'1');
			
			
		} else {
			var props = {};
			props[leftLtr] = 0;
			$btn.animate(props, 350);
			
			sessionStorage.setItem('helpusExp'+cardid,'0');			
		}			
	});
	
	elem.find('.helpus-card').each(function() {
		var cardid = $(this).attr('data-id');
		var $card = $(this);
		if (sessionStorage.getItem('helpusExp'+cardid) == '1') {
			$card.toggleClass('expanded', true);
			$card.find('.helpus-more,.helpus-more0').toggle(true);
			var $btn = $card.find('.helpus-learnmore-btn');
			
			var origIcon = $btn.data('origIcon');
			if (!origIcon) {
				 origIcon = $btn.find('i').html();
				$btn.data('origIcon', origIcon);
			}
			var origText = $(this).data('origText');
			if (!origText) {
				origText = $(this).find('span').html();
				$btn.data('origText', origText);	
			}
			
			$btn.find('i').html('&#xE5CD;');
			$btn.find('span').html('Close');
			
			var offset = $btn.parent().offset();			
			var targetLeft = $(window).width()/2;
			if (ertl('left','right') == 'right') {
				offset.left = $(window).width() - (offset.left+$btn.parent().outerWidth());
			}
			if (!$btn.hasClass('helpus-learnmore-btn-nm')) {
				$btn.css(leftLtr,(targetLeft - offset.left - 25)+'px');
			}
		}
	});
	
	
	elem.find('.helpus-goto-btn').click(function() {
		var id = $(this).attr('data-goto');
		var gotoDiv = elem.find('div[data-id="'+et(id)+'"]');
		var scrollTop = gotoDiv.offset().top-55;
		$('html, body').animate({
			'scrollTop': scrollTop
		},500, function() {
			gotoDiv.find('.helpus-learnmore-btn i, .helpus-card-mainBtn').css('transform','scale(1.4)').css('background-color','#0D47A1');
			setTimeout(function() {
				gotoDiv.find('.helpus-learnmore-btn i, .helpus-card-mainBtn').css('transform','').css('background-color','');
			}, 500);			
		});		
	});
	
	elem.find('.helpus-dialog-btn').click(function() {
		var src = $(this).attr('data-dialogSource');
		
		barNewDialog({			
			'source': src
		});		
	});
	
	elem.find('.helpus-dialog-script-btn').click(function() {
		var src = $(this).attr('data-dialogSource');
		
		$.post(src, {}, function(data) {
		}, 'script');		
	});
	
	elem.find('.helpus-help-link').click(function() {
		var id = $(this).attr('data-id') || '';
		var hid = $(this).attr('data-hid') || '';
		var p = $(this).attr('data-p') || '';

		barNewDialog({			
			'source': '/skin/?cmd=HelpusHelpDialog&helpid='+id+'&hashid='+hid+'&helpp='+p
		});	
	});
	
	elem.find('.helpus-video-link').click(function() {

		var id = $(this).attr('data-id');
		//var hid = $(this).attr('data-hid');
		barNewDialog({			
			'source': '/skin/?cmd=HelpusVideoDialog&videoid='+id
		});	
	});
	
	elem.find('.helpus-uploaddata-btn').click(function() {
		
		barNewDialog({			
			'source': '/customize/helpus.php?cmd=DataUpload'
		});	
	});
	
	elem.find('.helpus-qintro-btn').click(function() {
		$.post('/quickintro/?cmd=QIntroSlider&forceShow=1&template='+$(this).attr('data-introid'), {}, function(data) {
			$j('body').append(data);	
		});		
	});
	
	elem.find('.helpus-ttonline-commingsoon').click(function() {
		barShowMessageBox('Thank you for your interest in creating timetables online. This feature will be available soon.', 'fa-info',{
			title: 'Create timetable online'});
	});
		
	if (openCardQi) {
		var qibtn = elem.find('.helpus-qintro-btn[data-introid="'+et(openCardQi)+'"]');	
		openCard = qibtn.closest('.helpus-card').attr('data-id');
	}
	if (openCard) {		
		var id = openCard;
		var gotoDiv = elem.find('div[data-id="'+et(id)+'"]');
		var scrollTop = gotoDiv.offset().top-55;
		$('html, body').animate({
			'scrollTop': scrollTop
		},500, function() {
			if (!gotoDiv.hasClass('expanded')) {
				gotoDiv.find('.helpus-learnmore-btn').click();
			}
					
		});
	} else if (gotoCard) {		
		var id = gotoCard;
		var gotoDiv = elem.find('div[data-id="'+et(id)+'"]');
		var scrollTop = gotoDiv.offset().top-55;
		$('html, body').animate({
			'scrollTop': scrollTop
		},500, function() {
			gotoDiv.find('.helpus-learnmore-btn i, .helpus-card-mainBtn').css('transform','scale(1.4)').css('background-color','#0D47A1');
			setTimeout(function() {
				gotoDiv.find('.helpus-learnmore-btn i, .helpus-card-mainBtn').css('transform','').css('background-color','');
			}, 500);			
		});			
	} else {		
		var posY =  sessionStorage.getItem('helpusMainSpqintro_all');
		if (!posY) posY = 0;
		
				
		$(window).scrollTop(posY);
		setTimeout(function() {
			$(window).scrollTop(posY);
			$(window).scroll();
		}, 300);
		
	}
	
	$(window).off('scroll.helpusMain');
	$(window).on('scroll.helpusMain',function() {
		handleScroll();
	});
	
	$(window).one('load.helpusMain',function() {
		$(window).scroll();
	});
	
	$(window).off('beforeunload.helpusmain');
	$(window).on('beforeunload.helpusmain', function(event) {		
		$('a,input[type="button"]').off('click.helpusmain');
	});
	
	
	$('a,input[type="button"],.helpus-learnmore-btn').on('click.helpusmain',function() {
		var infoData = {
			
		};
		var attrs = ['data-id','data-introid','data-hid','data-goto','href'];
		var pom = $(this).attr('class').split(' ');
		
		var lbl = pom.sort().join(' ');
		for (var i=0;i<attrs.length;i++) {
			var attr = $(this).attr(attrs[i]);
			if (attr) {
				lbl += ' '+attr;
				//infoData[attrs[i]] = attr;
			}
		}  
		
		barTrackEvent({
			eventCategory: 'helpus',
	  		eventAction: 'click',
	  		eventLabel: lbl 	
		});
		
		
	});

	elem.find('.timeline-photosRenderer').each(function() {
		var photos = [];
		var ind = 0;
		$(this).attr('data-photos').split(';').map(function(r) {			
			
			photos.push({
				id: ind,
				albumid: '',
				dirid: '',
				sdsrc: r,				
				hdsrc: r
			});
			ind++;
		});
		
		$(this).timelinePhotosRenderer({
			photos: photos,
			maxCount: 100,
			cols: 2,
			noGalPreview: true
		});
	})

});
