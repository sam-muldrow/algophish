  function toggleDarkLight() {

    if ($('body').attr('class') == 'light-mode') {
      $('body').attr('class', 'dark-mode');
      thm = 'dark-mode';

    } else {
      $('body').attr('class', 'light-mode')
      thm = 'light-mode';

    }
    $.ajax({
      type: "GET",
      url: "/ajax/theme/" + thm
    });


  }

  const currentTheme = '';
    </script>
 

 
