    // Determine browser mode: desktop or mobile
 
    // Widget Icon Rendering
    if (!!document.getElementById('live_room')) {
      lottie.loadAnimation({
        container: document.getElementById('live_room'),
        renderer: 'svg',
        loop: true,
        autoplay: true,
        animationData: jsonData
      })
      // Add click event to open widget window upon tapping on widget icon
      document.getElementById("live_room").onclick = function showIframe() {
        fun()
      }
    }

