	<!-- <script src="https://lstatic.shangxueba.com/comm/plugins/swiper/swiper.min.js"></script> -->
	<script src="https://unpkg.com/swiper@7/swiper-bundle.min.js"></script>
	<script>
		$(function () {
			$(".container_box").each(function () {
				if ($(this).find("a").length > 1) {
					// new Swiper('.container_box_'+$(this).data("id"), {
					// 	direction: 'horizontal',
					// 	pagination: '.exam_pagination',
					// 	paginationClickable: true,
					// 	speed: 2000,
					// 	loop: true,
					// 	autoplay: 1000,
					// 	observer: true,
					// 	observeParents: true, 
					// 	observeSlideChildren:true,
					// });

					var mySwiper = new Swiper('.container_box_' + $(this).data("id"), {
						slidesPerView: 1,
						spaceBetween: 30,
						loop: true,
						pagination: {
							el: ".swiper-pagination",
							clickable: true,
						},
						navigation: {
							nextEl: ".swiper-button-next",
							prevEl: ".swiper-button-prev",
						},
						autoplay: {
							delay: 2000,
							disableOnInteraction: false,
						},
						observer: true,
						observeParents: true,
						observeSlideChildren: true
					});
				}
			})

			// var swiperLb = new Swiper('.container_lb', {
			// 	direction: 'horizontal',
			// 	pagination: '.pagination_lb',
			// 	paginationClickable: true,
			// 	speed: 2000,
			// 	paginationClickable: true,
			// 	loop: true,
			// 	autoplay: 2000
			// });

			var mySwiper = new Swiper('.container_lb', {
				slidesPerView: 1,
				spaceBetween: 30,
				loop: true,
				pagination: {
					el: ".swiper-pagination",
					clickable: true,
				},
				navigation: {
					nextEl: ".swiper-button-next",
					prevEl: ".swiper-button-prev",
				},
				autoplay: {
					delay: 2000,
					disableOnInteraction: false,
				},
				observer: true,
				observeParents: true,
				observeSlideChildren: true
			});

			$('.hot_tab_link a').hover(function () {
				$(this).addClass('hot_cur').siblings().removeClass('hot_cur');
				$(this).parents('.hot_flex_l').find('.hot_infor').eq($(this).index()).addClass('hot_show').siblings().removeClass('hot_show');
			});
			$('.test_tab_title li').on('click', function () {
				$(this).addClass('test_cur').siblings().removeClass('test_cur');
				$(this).parent().parent().find('.test_item').eq($(this).index()).addClass('hot_show').siblings().removeClass('hot_show');
			});

			$('.tab_link a').hover(function () {
				$(this).addClass('tab_active').siblings().removeClass('tab_active');
				$(this).parent().parent().parent().next().find('.box_item').eq($(this).index()).addClass('hot_show').siblings().removeClass('hot_show');
			});
			$('.paper_tab_title li').on('click', function () {
				$(this).addClass('paper_active').siblings().removeClass('paper_active');
				$(this).parent().next().find('.paper_item').eq($(this).index()).addClass('show').siblings().removeClass('show');
			});

			// 滚动监听 锚点定位
			var boxCont_scro = $('.sxb_scroll').offset().top;
			var boxArr = $('.sxb_scroll .sxb_scroll_box');
			var toolsArr = $('.zda_tools .tools_ul li');
			var offsetArr = {};
			var isScroll = true; //是否执行滚动监听逻辑

			for (var m = 0; m < boxArr.length; m++) {
				offsetArr[m] = $(boxArr[m]).offset().top;
			}
			$(window).scroll(function () {
				if (isScroll) {
					var boxMonitor = document.documentElement.scrollTop || document.body.scrollTop;
					if (boxMonitor > parseInt(boxCont_scro) - 100) {
						$('.zda_tools,.sxb_slide_suspen').css('display', 'block');
					} else {
						$('.zda_tools,.sxb_slide_suspen').css('display', 'none');
					}

					for (var n = 0; n < boxArr.length; n++) {
						if (boxMonitor > parseInt(offsetArr[n] - 300)) {
							$('.zda_tools .tools_ul li').removeClass('tools_cur');
							$(toolsArr[n]).addClass('tools_cur');
						}
					}
				}
			});

			$('.zda_tools .tools_ul li').on('click', function () {
				if (!isScroll) {
					return;
				}
				isScroll = false;
				var thisIndex = $(this).index();
				$('.zda_tools .tools_ul li').removeClass('tools_cur');
				$(this).addClass('tools_cur');
				$('html,body').animate({
					scrollTop: offsetArr[thisIndex]
				}, 300);
				setTimeout(function () {
					isScroll = true;
				}, 350);
			});

			//div切换
			var imgTab = 0;
			$('.guide_turn img').on({
				click: function () {
					var div_parent = $(this).parents(".test_guide"),
						$div = div_parent.find(".guide_turn_box .guide_turn_item"),
						idx = $(this).index(),
						div_len = $div.length;
					if (idx == 1) {
						++imgTab;
						if (imgTab > div_len - 1) {
							imgTab = 0;
						}
					} else {
						--imgTab;
						if (imgTab < 0) {
							imgTab = div_len - 1;
						}
					}
					$div.eq(imgTab).removeClass("hide").siblings().addClass("hide");
				}
			})
		});
	</script>
