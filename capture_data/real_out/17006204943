<script class="_iub_cs_activate-inline" type="text/plain" >
  function facebookConnect(form){
    function handleResponse(response){
      if (response.authResponse) {
        FB.api(`/${response.authResponse.userID}/`, {fields: ['name', 'email', 'picture', 'first_name', 'last_name']}, function(res) {
          if (res && !res.error) {
            
              
                form.querySelector('[name="fb_id"]').value = res.id
              
            
              
                form.querySelector('[name="fb_name"]').value = res.name
              
            
              
                form.querySelector('[name="fb_email"]').value = res.email
              
            
              
                form.querySelector('[name="fb_first_name"]').value = res.first_name
              
            
              
                form.querySelector('[name="fb_last_name"]').value = res.last_name
              
            
              
                if ("picture" in res)
                  form.querySelector('[name="fb_picture"]').value = res.picture.data.url
              
            
            formAddCsrfProtection(form).then(data => {
              form.submit();
            })
          }       
        });
      }
    }
    FB.login(handleResponse, {scope:'email,public_profile'});
  }

  
  </script>

