<script>document.addEventListener("DOMContentLoaded",function(){let xOffset=0;let xOffset1=0;let isMouseIn=false;let isMouseIn2=false;const slides=document.getElementById("slides");const slides1=document.getElementById("slides1");const slideElements=slides.querySelectorAll(".slide");const slideCount=slideElements.length;const slideElements1=slides1.querySelectorAll(".slide");const slideCount1=slideElements1.length;function translate(){let offsetIncrementor=isMouseIn?0.1:0.2;if(xOffset>=200*slideCount)xOffset=0;else xOffset=xOffset+offsetIncrementor;slides.style.transform="translateX(-"+xOffset+"px)";}function translate2(){let offsetIncrementor=isMouseIn2?0.1:0.2;if(xOffset1<=0)xOffset1=200*slideCount1;else xOffset1=xOffset1-offsetIncrementor;slides1.style.transform="translateX(-"+xOffset1+"px)";}slides.addEventListener("mouseover",function(event){isMouseIn=true;});slides1.addEventListener("mouseover",function(event){isMouseIn2=true;});slides1.addEventListener("mouseout",function(event){isMouseIn2=false;});slides.addEventListener("mouseout",function(event){isMouseIn=false;});function checkScreenSize(){if(window.innerWidth<768){clearInterval(translate2);clearInterval(translate);}else{setInterval(translate2,0);setInterval(translate,0);}}checkScreenSize();window.addEventListener('resize',checkScreenSize);});</script>

