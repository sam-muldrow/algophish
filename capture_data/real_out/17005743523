    <script data-cfasync="false" type='text/javascript' src='https://www.criteo.com/wp-content/plugins/motionpage/assets/js/gsap/gsap.min.js?ver=3.11.4' id='mp-gsap-js'></script>
    <script data-cfasync="false" type='text/javascript' id='mp-SplitText-js-before'>
    function _mp_GENSEL(e) {
        const n = [];
        for (; e.tagName;) {
            let t = 0;
            if (e.parentNode) {
                const n = e.parentNode.children;
                for (; t < n.length && n[t] !== e;) t++
            }
            n.unshift(e.nodeName + (t > 0 ? `:nth-child(${t + 1})` : "")), e = e.parentNode
        }
        return n.join(" > ")
    }
    </script>
    <script data-cfasync="false" type='text/javascript' src='https://www.criteo.com/wp-content/plugins/motionpage/assets/js/gsap/SplitText.min.js?ver=3.11.4' id='mp-SplitText-js'></script>
    <script data-cfasync="false" type='text/javascript' src='https://www.criteo.com/wp-content/plugins/motionpage/assets/js/gsap/ScrollTrigger.min.js?ver=3.11.4' id='mp-ScrollTrigger-js'></script>
    <script data-cfasync="false" type='text/javascript' id='mp-ScrollTrigger-js-after'>
    window._$W = window;
    addEventListener("DOMContentLoaded", () => {
        document.body.style.overflowX = "hidden";
        gsap.registerPlugin({
            name: "transition",
            init(t, e, n) {
                return this.target = t, this.tween = n, 1
            },
            render(t, {
                target: e,
                tween: n
            }) {
                void 0 !== (null == e ? void 0 : e.style) && n && (e.style.transition = "unset")
            }
        });
        gsap.defaults({
            duration: 5,
            transition: "unset"
        });
        gsap.config({
            nullTargetWarn: false
        });
        if (!matchMedia("(prefers-reduced-motion: reduce)").matches) {
            window._mp_1685519873_ST0 = new SplitText(".use_character_highlighter .animated-text", {
                    type: "chars,words"
                }), window._mp_1685519873 = [], gsap.utils.toArray(".use_character_highlighter .animated-text")
                .forEach(item => {
                    let _n = "_mp_1685519873_" + (_mp_1685519873.length + 1);
                    _mp_1685519873.push(_n);
                    let _t = gsap.timeline({
                        scrollTrigger: {
                            scrub: 1,
                            trigger: item,
                            start: "top 60%",
                            end: "bottom 60%"
                        }
                    }).from(_mp_1685519873_ST0.chars.filter(s => s.closest(item.id ? `#${item.id}` :
                        _mp_GENSEL(item))), {
                        stagger: {
                            from: "start",
                            each: 1
                        },
                        autoAlpha: .25
                    }, 0);
                    window[_n] = _t
                });
            if (matchMedia("screen and (min-width: 768px)").matches) {
                window._mp_1685519874 = gsap.timeline({
                    scrollTrigger: {
                        scrub: 1,
                        trigger: ".criteo-dashboard-image",
                        start: "top 100%",
                        end: "bottom 80%"
                    }
                }), _mp_1685519874.from(".criteo-dashboard-image", {
                    scale: 1.4
                }, 0);
            }
            if (matchMedia("screen and (min-width: 768px)").matches) {
                window._mp_1685519876 = gsap.timeline({
                    scrollTrigger: {
                        scrub: 1,
                        trigger: ".criteo_dashboard_popups",
                        start: "top 100%",
                        end: "bottom 80%"
                    }
                }), _mp_1685519876.from(".criteo_dashboard_popups .popup", {
                    autoAlpha: 0
                }, 0);
            }
            if (matchMedia("screen and (min-width: 992px)").matches) {
                window._mp_1685519878 = gsap.timeline({
                    scrollTrigger: {
                        trigger: ".fixed_cont",
                        start: "top 0%",
                        end: "bottom 0%",
                        pin: ".fixed_cont",
                        pinSpacing: "margin"
                    }
                });
            }
            if (matchMedia("screen and (min-width: 992px)").matches) {
                window._mp_1685519879 = gsap.timeline({
                    scrollTrigger: {
                        trigger: ".platform_accordion",
                        start: "top 10%",
                        end: "bottom 50%",
                        pin: ".platform_accordion .image-column img",
                        pinSpacing: "margin"
                    }
                });
            }
        }
        _$W._mp_refresher = (t = 0) => {
            ScrollTrigger && setTimeout((() => {
                ScrollTrigger.sort(), ScrollTrigger.getAll().forEach((r => r.refresh()))
            }), t)
        }, addEventListener("load", (() => _mp_refresher(92)));
    });
    </script>
    <script>const video=document.getElementById('accordionImage1');function isElementInViewport(el){const rect=el.getBoundingClientRect();return(rect.top<=(window.innerHeight||document.documentElement.clientHeight)&&rect.left>=0&&rect.bottom>=0&&rect.right>=0);}function handleVideoPlay(){if(isElementInViewport(video)){video.play();}else{video.pause();}}window.addEventListener('scroll',handleVideoPlay);handleVideoPlay();</script>
    <script>const box=document.getElementById('four_section_homepage');const textTitle=document.getElementById('home-title');function isElementInViewport(el){const rect=el.getBoundingClientRect();return(rect.top<=(window.innerHeight||document.documentElement.clientHeight)&&rect.left>=0&&rect.bottom>=0&&rect.right>=0);}function handleBoxHighlight(){if(isElementInViewport(textTitle)){textTitle.classList.add('active');}else{textTitle.classList.remove('active');}}window.addEventListener('scroll',handleBoxHighlight);handleBoxHighlight();</script>
