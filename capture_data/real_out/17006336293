            $(function () { $(".become-flex-wrap").addClass("clearfix"); });
        </script>
    <script src="https://extsearch.yahoo.co.jp/js/rusk.min.js" charset="utf-8"></script>
<script type="text/javascript">

YAHOO.JP.srch.rusk.onLoad(function(rusk) {
    // console.log('YAHOO.JP.srch.rusk.onLoad');

    var setUrlValue = jQuery("#yahoo_setUrl").val();
    var verifyUrlValue = jQuery("#yahoo_verifyUrl").val();
    var params = {
        cookieName: "yahoo_cookie_jv",
        setUrl:  setUrlValue,
        verifyUrl: verifyUrlValue
    };
    // console.log(params);
    rusk.setParams(params);
});

</script>
<!-- Global site tag (gtag.js) - AdWords: 957883620 -->
<script async src="https://www.googletagmanager.com/gtag/js?id=AW-957883620"></script>
<script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());
    gtag('config', 'AW-957883620');
</script>
<script>
    var root = window.location.pathname;
    var pathVal = window.location.href;
    var pid = '';
    var ptype = '';
    var tot = 0.0;

    if (root == "/") {
        ptype = 'home';
    }
    else if(jQuery('div').hasClass('goodsproductdetail_')){
        pid = jQuery('.goodsproductdetail_ .code').text().split('商品コード：')[1];
        tot = jQuery('.goodsproductdetail_ .price').text().replace(/[^0-9.]/g,'');
        ptype = 'offerdetail';
    }
    else if(root.match('/pid/')){
        pid = root.split('/pid/')[1].split('/')[0];
        tot = jQuery('[itemprop="price"]').attr('content');
        ptype = 'offerdetail';
    }
    else if(jQuery('div').hasClass('become-flex-wrap')){
        ptype = 'searchresults';
    }
    else {
        ptype = 'other';
    }

    gtag('event', 'page_view', {
        'send_to': 'AW-957883620',
        'dynx_itemid': pid,
        'dynx_pagetype': ptype,
        'dynx_totalvalue': parseFloat(tot)
    });
</script>
