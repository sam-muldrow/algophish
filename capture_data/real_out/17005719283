	window.onload = function() {
		var dc = document.cookie;

		if (dc.indexOf('aceitoCookies') == -1) {
			document.querySelector('.banner-cookies').style.display = 'flex';
		} else {
			document.querySelector('.banner-cookies').style.display = 'none';
