        function showgoodsimage(obj, img) {
            show_img(img);
        }
        function show_qrcode_img(img) {
            if (img != "") {
                var arrimg = img.split(";");
                var html = "";
                var picjson = "{id:123,start:0,data: [";
                for (var i = 0; i < arrimg.length; i++) {
                    if (arrimg[i] != "")
                        html += "<div style='text-align:center;padding:10px;'><img  src='" + arrimg[i] + "' class='img-responsive'  style='margin:auto;'/></div>";
                }

                if (html != "") {
                    layer.open({
                        type: 1,
                        title: "扫描二维码，联系微信客服",
                        area: ['400px', '450px'], //宽高
                        shadeClose: true,
                        content: html
                    });

                }
            }
        }
        function show_img(img) {
            if (img != "") {
                var arrimg = img.split(";");
                var html = "";
                var picjson = "{id:123,start:0,data: [";
                for (var i = 0; i < arrimg.length; i++) {
                    if (arrimg[i] != "")
                        html += "<div style='text-align:center;padding:10px;'><img  src='" + arrimg[i] + "' class='img-responsive'  style='margin:auto;'/></div>";
                }

                if (html != "") {
                    layer.open({
                        type: 1,
                        title: "扫描二维码，关注天翼公众号",
                        skin: 'layui-layer-rim', //加上边框
                        area: ['700px', '280px'], //宽高
                        shadeClose: true,
                        content: html
                    });

                }
            }
        }
    </script> 

