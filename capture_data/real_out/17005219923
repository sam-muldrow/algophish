	<script>
		
		//å£°æå¨å±åé
		var shareId = "",
			title = '';
		//å¨å±åéèµå¼ï¼ä¾¿äºåè¾¹æ´æ¹éç½®
		$('.index_news_content').on('mouseover', '.bdsharebuttonbox a', function() {
			shareId = $(this).attr("data-id");
			title = $(this).attr("data-title");
			console.log(shareId + '-----' + title);
		})

		function SetConf(cmd, config) {
			config.bdUrl = shareId;
			config.bdText = title;
			console.log('!' + config);
			return config;
		}
		window._bd_share_config = {
			common: {
				onBeforeClick: SetConf
			},
			share: [{
				"bdSize": 12
			}],
			//slide : [{bdImg : 0,bdPos : "right",bdTop : 100}],//è®¾ç½®äºè¿ä¸é¡¹ï¼é¡µé¢å³è¾¹ä¼åºç°ä¸ä¸ªåäº«çæµ®çªï¼è¯¥æµ®çªåªè½åäº«å½åè®¾ç½®çbdUrl
			image: [{
				viewType: 'list',
				viewPos: 'top',
				viewColor: 'black',
				viewSize: '12',
				viewList: ['qzone', 'tsina', 'huaban', 'tqq', 'renren']
			}],
			selectShare: [{
				"bdselectMiniList": ['qzone', 'tqq', 'kaixin001', 'bdxc', 'tqf']
			}]
		}
		//ä»¥ä¸ä»£ç æ¯å¿é¡»
		with(document) 0[(getElementsByTagName('head')[0] || body).appendChild(createElement('script')).src =
			'https://www.21-sun.com/static/api/js/share.js?cdnversion=' + ~(-new Date() / 36e5)];
	</script>
	<!-- new_year_around -->
	<script type="text/javascript">
	$(function(){
		
		function lar_to_thu(){
			var lar_p = $('.new_year_lar');
			var thu_p = $('.new_year_thu');
			
			lar_p.animate({
				opacity:0
			},1000,function(){
				lar_p.addClass('hide');
				thu_p.removeClass('hide');
			})
		}
		
		$('.lar_close').on('click',function(){
			lar_to_thu();
		})
		
		$('.thu_close').on('click',function(){
			$('.new_year_thu').addClass('hide');
		})
		
		var three_s_count = setTimeout(function(){
			lar_to_thu();
			clearTimeout(three_s_count);
		},3000);
		
	})
</script>
</html>
