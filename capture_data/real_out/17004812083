<script type="text/javascript">
</script>  <script>
function isUserAuthenticated(isUserLoggedIn, authenticatedUrl) {
var isUserLoggedIn = isUserLoggedIn;
if (!isUserLoggedIn) {
window.location.href = authenticatedUrl;
return false;
}
return true;
}
function updateButtonAfterApply(button, isAllApply = false) {
setTimeout(function () {
button.text(isAllApply ? "HEPSİNE BAŞVURDUN" : "BAŞVURDUN")
.removeClass(isAllApply ? "btn-warning" : "btn-default")
.addClass("btn-success disabled")
.css("opacity", "1")
.parent().parent().css("background", "#fbfbfb");
}, 500);
}
function sendApplicationData(advertisementID, userID, cvID, isUserLoggedIn, authenticatedUrl) {
if (!isUserAuthenticated(isUserLoggedIn, authenticatedUrl)) return;
var formData = {
AdvertisementID: advertisementID,
UserID: userID,
CvID: cvID
};
$.ajax({
type: "POST",
url: "/Application/QuickApply",
data: formData,
beforeSend: function () {
$("#" + advertisementID + "_qapply").html("<i class='fa fa-spinner fa-spin'></i>")
},
success: function () {
updateButtonAfterApply($("#" + advertisementID + "_qapply"));
},
error: function (error) {
//console.error(error);
}
});
}
function sendAllApply(isUserLoggedIn, authenticatedUrl) {
if (!isUserAuthenticated(isUserLoggedIn, authenticatedUrl)) return;
$.ajax({
type: "POST",
url: "/Application/QuickApplyAll",
beforeSend: function () {
$(".btnQuickApply").each(function () {
if ($(this).text() == "BAŞVUR") {
$(this).html("<i class='fa fa-spinner fa-spin'></i>")
}
})
},
success: function () {
updateButtonAfterApply($(".btnQuickApply"));
updateButtonAfterApply($("#allApply"), true);
},
error: function (error) {
//console.error(error);
}
});
}
