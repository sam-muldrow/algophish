0438\u0442\u044c \u0432\u0441\u0435 \u0441\u0432\u043e\u0435 \u0441\u0435\u043c\u0435\u0439\u0441\u0442\u0432\u043e \"\u043f\u043e \u0443\u043f\u0440\u043e\u0449\u0435\u043d\u043a\u0435\" \u0438 \u0442\u0443\u0442 \u0436\u0435 \u0441\u0430\u0436\u0430\u0442\u044c \u043e\u043d\u043e\u0435 \u043d\u0430 \u0432\u0441\u0435 \u0433\u043e\u0441\u0433\u0430\u0440\u0430\u043d\u0442\u0438\u0438 \u0438 \u043f\u043e\u0441\u043e\u0431\u0438\u044f.<br><br>3) \u041e\u0421\u041d\u041e\u0412\u0410\u0422\u0415\u041b\u042c\u041d\u041e \u043f\u0440\u043e\u0432\u0435\u0440\u044f\u0442\u044c \u0438 \"\u043b\u0438\u0447\u043d\u044b\u0435 \u0437\u0430\u0441\u043b\u0443\u0433\u0438\" \u043f\u0440\u0435\u0442\u0435\u043d\u0434\u0435\u043d\u0442\u0430 \u043d\u0430 \u0432\u044a\u0435\u0437\u0434 \u0418 \u0422\u0415\u0425, \u041a\u0422\u041e \u0418\u0425 \u041f\u0420\u041e\u0412\u0415\u0420\u042f\u0415\u0422.( \u043d\u0430 \u043f\u0440\u0435\u0434\u043c\u0435\u0442 \u043a\u043e\u0440\u0440\u0443\u043f\u0446\u0438\u043e\u043d\u043d\u043e\u0439 \u0441\u043e\u0441\u0442\u0430\u0432\u043b\u044f\u044e\u0449\u0435\u0439). \u041f\u0440\u0438\u0440\u0430\u0432\u043d\u0438\u0432\u0430\u0442\u044c \u043f\u043e\u043f\u044b\u0442\u043a\u0443 \u043f\u043e\u0434\u043a\u0443\u043f\u0430 \u043c\u0438\u0433\u0440\u0430\u043d\u0442\u043e\u043c \u0440\u0430\u0431\u043e\u0442\u043d\u0438\u043a\u0430 \u0433\u043e\u0441\u043e\u0440\u0433\u0430\u043d\u043e\u0432 \u043a \u0442\u044f\u0433\u0447\u0430\u0439\u0448\u0435\u043c\u0443 \u043f\u0440\u0435\u0441\u0442\u0443\u043f\u043b\u0435\u043d\u0438\u044e \u0438 \u0441\u0430\u0436\u0430\u0442\u044c \u043c\u0438\u0433\u0440\u0430\u043d\u0442\u0430 \u0432 \u0442\u044e\u0440\u044c\u043c\u0443 \u0441 \u043f\u0440\u0438\u043d\u0443\u0434\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0439 \u043e\u0442\u0440\u0430\u0431\u043e\u0442\u043a\u043e\u0439 \u0441\u0432\u043e\u0435\u0433\u043e \u0441\u043e\u0434\u0435\u0440\u0436\u0430\u043d\u0438\u044f \u0438 \u0441\u0443\u043c\u043c\u044b \u043f\u0440\u0435\u0434\u043b\u043e\u0436\u0435\u043d\u043d\u043e\u0439 \u0432\u0437\u044f\u0442\u043a\u0438. \u043f\u043e \u043e\u043a\u043e\u043d\u0447\u0430\u043d\u0438\u0438 \u0441\u0440\u043e\u043a\u0430\u00a0\u2014 \u0434\u0435\u043f\u043e\u0440\u0442\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u043d\u0430 \u0440\u043e\u0434\u0438\u043d\u0443.<br>( \u0430 \u043d\u0435 \u043f\u0440\u043e\u0441\u0442\u043e \u0432\u044b\u0441\u044b\u043b\u0430\u0442\u044c \u0438 \u043f\u043e\u043b\u0443\u0447\u0430\u0442\u044c \u0435\u0433\u043e \u0443\u0436\u0435 \u0437\u0430\u0432\u0442\u0440\u0430, \u0441 \u0434\u0440\u0443\u0433\u0438\u043c \u043f\u0430\u0441\u043f\u043e\u0440\u0442\u043e\u043c).<br><br>4) \u0412\u0432\u0435\u0441\u0442\u0438 \u0414\u041b\u0418\u0422\u0415\u041b\u042c\u041d\u042b\u0419 \u0446\u0435\u043d\u0437 \u043d\u0430 \u043e\u0442\u0437\u044b\u0432 \u0433\u0440\u0430\u0436\u0434\u0430\u043d\u0441\u0442\u0432\u0430 \u0434\u043b\u044f \u043c\u0438\u0433\u0440\u0430\u043d\u0442\u043e\u0432, \u0432 \u0441\u043b\u0443\u0447\u0430\u0435 \u0441\u043e\u0432\u0435\u0440\u0448\u0435\u043d\u0438\u044f \u0438\u043c, \u0438\u043b\u0438 \u0447\u043b\u0435\u043d\u0430\u043c\u0438 \u0432\u0432\u0435\u0437\u0435\u043d\u043d\u044b\u0445 \u0438\u043c\u0438 \u0441\u0435\u043c\u0435\u0439, \u043f\u0440\u0435\u0441\u0442\u0443\u043f\u043b\u0435\u043d\u0438\u0439 \u0438 \u0441\u043e\u0446\u0438\u0430\u043b\u044c\u043d\u043e \u0437\u043d\u0430\u0447\u0438\u043c\u044b\u0445 \u043f\u0440\u0430\u0432\u043e\u043d\u0430\u0440\u0443\u0448\u0435\u043d\u0438\u0439. <br><br>5) \u041f\u0440\u0438\u0440\u0430\u0432\u043d\u0438\u0432\u0430\u0442\u044c \u043f\u043e\u043f\u044b\u0442\u043a\u0438 \u0438\u0437\u043c\u0435\u043d\u0438\u0442\u044c \u0436\u0438\u0437\u043d\u0435\u043d\u043d\u044b\u0439 \u0443\u043a\u043b\u0430\u0434 \u0440\u0435\u0433\u0438\u043e\u043d\u0430 \u043f\u0440\u0435\u0431\u044b\u0432\u0430\u043d\u0438\u044f \u043c\u0438\u0433\u0440\u0430\u043d\u0442\u0430, \u043f\u043e\u043f\u044b\u0442\u043a\u0438 \u043d\u0430\u0432\u044f\u0437\u044b\u0432\u0430\u0442\u044c \u0438\u043d\u043e\u0439 \u043e\u0431\u0440\u0430\u0437 \u0436\u0438\u0437\u043d\u0438 \u043c\u0435\u0441\u0442\u043d\u043e\u043c\u0443 \u043d\u0430\u0441\u0435\u043b\u0435\u043d\u0438\u044e\u00a0\u2014 \u043a \u0434\u0438\u0432\u0435\u0440\u0441\u0438\u043e\u043d\u043d\u043e\u0439 \u0434\u0435\u044f\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u0438  \u0441 \u043d\u0435\u0437\u0430\u043c\u0435\u0434\u043b\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0439 \u0434\u0435\u043f\u043e\u0440\u0442\u0430\u0446\u0438\u0435\u0439 \u0437\u0430 \u0442\u0430\u043a\u0443\u044e \u0434\u0435\u044f\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u044c.<br><br>6) \u0412\u044b\u044f\u0432\u043b\u044f\u0442\u044c \u041b\u042e\u0411\u042b\u0415 \u043f\u043e\u043f\u044b\u0442\u043a\u0438 \u0434\u0435\u0442\u0435\u0439 \u043c\u0438\u0433\u0440\u0430\u043d\u0442\u043e\u0432 \u0438 \u043d\u0435\u0434\u0430\u0432\u043d\u043e \u043f\u043e\u043b\u0443\u0447\u0438\u0432\u0448\u0438\u0445 \u0433\u0440\u0430\u0436\u0434\u0430\u043d\u0441\u0442\u0432\u043e \u0420\u0424 \u043f\u0440\u0438\u0432\u043d\u043e\u0441\u0438\u0442\u044c \u043d\u043e\u0440\u043c\u044b \u0428\u0430\u0440\u0438\u0430\u0442\u0430, \u043d\u0430\u0446\u0438\u043e\u043d\u0430\u043b\u0438\u0437\u043c\u0430, \u0432 \u0443\u0447\u0435\u0431\u043d\u0443\u044e \u0441\u0440\u0435\u0434\u0443.<br> \u041f\u0440\u0438\u0440\u0430\u0432\u043d\u0438\u0432\u0430\u0442\u044c \u043f\u043e\u0434\u043e\u0431\u043d\u044b\u0435 \u0441\u043b\u0443\u0447\u0430\u0438 \u043a \"\u043d\u0435\u043d\u0430\u0434\u043b\u0435\u0436\u0430\u0449\u0435\u043c\u0443 \u0432\u043e\u0441\u043f\u0438\u0442\u0430\u043d\u0438\u044e\" \u0441 \u043f\u043e\u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0438\u043c \u043d\u0430\u0447\u0430\u043b\u043e\u043c \u0437\u0430\u043f\u0443\u0441\u043a\u0430 \u043c\u0435\u0445\u0430\u043d\u0438\u0437\u043c\u0430 \u0438\u0437 \u043f.5<br><b>7) \u041d\u0443 \u0438 \u0433\u043b\u0430\u0432\u043d\u043e\u0435\u00a0\u2014 \u043f\u0440\u0438\u0440\u0430\u0432\u043d\u0438\u0432\u0430\u0442\u044c \u043f\u0440\u0435\u0441\u0442\u0443\u043f\u043d\u0443\u044e \u0434\u0435\u044f\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u044c  \u0432\u0437\u044f\u0442\u043e\u0447\u043d\u0438\u043a\u043e\u0432 \u0438\u0437 \u041c\u0412\u0414, \u043e\u0442\u0432\u0435\u0447\u0430\u044e\u0449\u0438\u0445 \u0437\u0430 \u043c\u0438\u0433\u0440\u0430\u043d\u0442\u0441\u043a\u0443\u044e \u043f\u043e\u043b\u0438\u0442\u0438\u043a\u0443 \u043a \u0433\u043e\u0441\u0438\u0437\u043c\u0435\u043d\u0435.<br> \u041e\u043d\u0438 \u043d\u0435 \u043f\u0440\u043e\u0441\u0442\u043e \u0432\u0437\u044f\u0442\u043a\u0438 \u0431\u0435\u0440\u0443\u0442\u00a0\u2014 \u043e\u043d\u0438 \u0438\u043d\u043e\u0441\u0442\u0440\u0430\u043d\u043d\u044b\u0445 \u043f\u0440\u0435\u0441\u0442\u0443\u043f\u043d\u0438\u043a\u043e\u0432 \u0432 \u0420\u043e\u0441\u0441\u0438\u044e \u0437\u0430 \u0434\u0435\u043d\u044c\u0433\u0438 \u043f\u0440\u043e\u043f\u0443\u0441\u043a\u0430\u044e\u0442 \u0438 \u0433\u0440\u0430\u0436\u0434\u0430\u043d\u0441\u0442\u0432\u043e  \u0440\u0430\u0437\u0434\u0430\u044e\u0442.<br> \u041d\u0435 \"\u0443\u0432\u043e\u043b\u044c\u043d\u044f\u0442\u044c \u0438\u0437 \u043e\u0440\u0433\u0430\u043d\u043e\u0432\" \u0430 \u0441\u0430\u0436\u0430\u0442\u044c \u0437\u0430 \u0433\u043e\u0441\u0438\u0437\u043c\u0435\u043d\u0443.<br> \u0422\u043e\u0433\u0434\u0430 \u0438 \u0431\u0440\u0430\u0442\u044c \u043f\u0435\u0440\u0435\u0441\u0442\u0430\u043d\u0443\u0442 ( \u043b\u0438\u0431\u043e \u0431\u0443\u0434\u0443\u0442 \u0431\u0440\u0430\u0442\u044c \u0441\u0442\u043e\u043b\u044c\u043a\u043e, \u0447\u0442\u043e  \"\u043e\u043f\u0446\u0438\u044f\" \u043f\u0435\u0440\u0435\u0441\u0442\u0430\u043d\u0435\u0442 \u0431\u044b\u0442\u044c \u043c\u0430\u0441\u0441\u043e\u0432\u043e \u0434\u043e\u0441\u0442\u0443\u043f\u043d\u043e\u0439)</b><br><br> \u0422\u043e