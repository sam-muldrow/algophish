  <div class="ev-paragraph ev-paragraphbullets stockofthedaycontainer" data-error-message="Diese Information steht in Kürze wieder zur Verfügung.">
	<div class="loading-info ev-paragraph loading-stockoftheday">
    	<a class="ev-ajax-placeholder" style="pointer-events: none;" href="#"></a>
	</div>
	<div class="ev-pagecontentrowsitemap444-headline-mobile-shadow hide ">
	  <div class="ev-pagecontentrowsitemap444-headline-mobile-shadow-left"></div>
	  <div class="ev-pagecontentrowsitemap444-headline-mobile-shadow-center"></div>
	  <div class="ev-pagecontentrowsitemap444-headline-mobile-shadow-right"></div>
	</div>
</div>

<script type="text/javascript">
	(function($, doc) {
		var loadingindicator = $('.loading-stockoftheday');
	
		function getStockOfTheDayData() {
	         var req = {
	                 url : location.protocol + "//" + location.hostname + (location.port ? ":" + location.port: "") + "/web-sec-service/api/marketdata/stocks/stockofthedayv1?frontendType=web", 
	                 type : "GET"
	         };
	          
	         $.ajax(req).done(function(data) {        	
	             if (typeof data !== 'undefined' && data !== null && typeof data.StockOfTheDayListV1 !== 'undefined' && data.StockOfTheDayListV1 !== null && typeof data.StockOfTheDayListV1.LIST !== 'undefined' && data.StockOfTheDayListV1.LIST !== null && data.StockOfTheDayListV1.LIST.length > 0) {
	            	 data.StockOfTheDayListV1.LIST.forEach(function(entry, index) {
	            		 if (index > 3 || entry.ISIN === null || entry.ISIN === "") {
	            			 return;
	            		 }
	            		 var container = document.createElement("div");
	            		 container.classList.add("ev-bullet");
	            		 container.classList.add("ev-bulletlink");
	            		 var anchor = document.createElement("a");
	            		 anchor.setAttribute("href", "/web/Wertpapier/"+entry.ISIN);
	            		 anchor.setAttribute("title", entry.NAME_SHORT);
	            		 anchor.innerText = entry.NAME_SHORT;
	            		 container.appendChild(anchor);
	            		 loadingindicator.hide();
	            		 var stockOftheDayContainer = jQuery(document).find(".stockofthedaycontainer");
	            		 if(stockOftheDayContainer && stockOftheDayContainer.length > 0) {
	            			 stockOftheDayContainer[0].appendChild(container);
	            		 }
	            			 		            		 
	            	 });
	             } else {
	            	 displayErrorMessage();
	             }
	         }).fail(function() {
	          		displayErrorMessage();	
	         });
		}
		
		function displayErrorMessage() {
			loadingindicator.hide();
         		
         		var stockOftheDayContainer = jQuery(document).find(".stockofthedaycontainer");
       		if(stockOftheDayContainer && stockOftheDayContainer.length > 0) {
       			var errorMessage = stockOftheDayContainer.attr("data-error-message");
       			var errorContainer = document.createElement("div");
       			errorContainer.innerText = errorMessage;
       			stockOftheDayContainer[0].appendChild(errorContainer);
       		}		   
		}
		getStockOfTheDayData();
		
	}(jQuery, document));
</script>
  


