<script type="text/javascript">
  $(function() {
    var hdr_trigger = $("#section_bazaar").offset();
    $(window).scroll(function(){
      var trigger_position = hdr_trigger.top - $(window).height();
      if ($(window).scrollTop() > trigger_position) {
        var distanceTOP = $(window).height() - 120;
        $(".footer_jack").animate({'top':distanceTOP},1500);
        $(".footer_jack").addClass("jack_open");
      }
    });
  });
  $('.footer_jack #close_btn').click(function(){
    $(".footer_jack").remove();
  });
</script>

