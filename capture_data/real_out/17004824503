<script type="text/javascript">
    var InterValObj;
    $(document).ready(function() {
        InterValObj = window.setInterval(SetRemainTimePC, 10);
    });
    function SetRemainTimePC() {
        var now = new Date();
        var num = 2;
        for (var i = 0; i < num; i ++) {
          var end_bazaar = document.getElementById("end_bazaar"+i);
            if (end_bazaar === null) {
              continue;
            }
            var SysSecond = new Date(end_bazaar.innerHTML.replace(/\s/, 'T') + '+09:00');
            var passtime = SysSecond.getTime() - now.getTime();
            if (passtime > 0) {
                var d = Math.floor(passtime / (1000 * 60 * 60 * 24));
                passtime = passtime - (d * (1000 * 60 * 60 * 24));
                var h = Math.floor(passtime / (1000 * 60 * 60));
                passtime = passtime - (h * (1000 * 60 * 60));
                var m = Math.floor(passtime / (1000 * 60));
                passtime = passtime - (m * (1000 * 60));
                var s = Math.floor(passtime / 1000);
                passtime = passtime - (s * (1000));
                var ms = Math.floor(passtime / 10);
                if (m < 10) { m = '0' + m; }
                if (s < 10) { s = '0' + s; }
                if (ms < 10) { ms = '0' + ms; }
                if (d > 0) {
                    h = h + d*24;
                }
                $("#remain_time" + i).html("cè<span>" + h + "</span>Ô<span>" + m + "</span>ª<span>" + s + "</span>b" + "<span>" + ms + "</span>");
            } else {
                window.clearInterval(InterValObj);
            }
        }
    }
</script>

  
