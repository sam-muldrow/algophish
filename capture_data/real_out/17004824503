        function update_page_view() {
            var current_uri = '/welcome';
            var rid = "200000";
            var tdate = "";
            $.ajax({
                type : "GET",
                url : "/sp/update_page_view/index",
                data: {'page_uri': current_uri, 'rid': rid, 'tdate': tdate},
                dataType: "json",
                success:function(x, textStatus, xhr) {
                    if (x.is_updated == true) {
                        return;
                    }
                }
            });
        }
