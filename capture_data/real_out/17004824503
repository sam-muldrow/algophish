<script>
$(function(){
    $('.nav').hover(function(){
        var nav = $(this).find('li').length;
        $(this).not(':animated').animate({'height':140 * nav + 'px'},'fast');
    },function(){
        $(this).animate({'height':'40px'},'fast');
    });
});
$(function(){
    // uid="jQueryBox"vðñ\¦
    $("#jQueryBox","#jQueryPush").css("display", "none");
        // uid="jQueryPush"vªNbN³ê½ê
    $("#jQueryPush").click(function(){
        var is_checked = $("#example").is(':checked');
        var close_var = 0;
        if (is_checked) {
          close_var = 1;
        }
        var run_os = 'pc';
        $.ajax({
          type : "GET",
          url : "/smartphone/set_close",
          data: {close: close_var, os:run_os},
          dataType: "json",
          success:function(x, textStatus, xhr) {
          },
          error:function(xhr, textStatus, errorThrown) {
          }
        });
        // uid="jQueryBox"vÌ\¦Añ\¦ðØèÖ¦é
        $("#jQueryBox").toggle();
    });
});
</script>

