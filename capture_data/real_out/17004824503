  // |bvAbv
  $("a[alt=popnewserviceLink]").click(function(){
    var pop_name = "#"+this.id+"up";
    $.modal('<div id="alert_window" width="550" height="490"></div>',{
      closeHTML: "<a href='#' title='Close' class='modalCloseImg' ></a>",
       overlayId: 'confirm-overlay',
       containerId: 'confirm-container',
       minWidth:'550',
       minHeight:'490',
       position:[60, 0],
       overlayClose:true,
      onShow: function() {
        var innerHTML = $(pop_name).get(0).firstChild.nodeValue;
        $("div[id=alert_window]").html(innerHTML);
        $('html, body').addClass('lock');
      },
      onClose: function() {
        $.modal.close();
        $('html, body').removeClass('lock');
      },
      containerCss:{
        Left: 0,
        paddingLeft:80,
        backgroundColor: '#fff'
      },
      overlayCss: {
        opacity: 0.7,
        backgroundColor: "#000",
        Left: 394
      }
    });
  });
</script>

