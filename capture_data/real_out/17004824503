  <script type="text/javascript">
    var InterValObj;
    $(document).ready(function() {
      InterValObj = window.setInterval(SetRemainTime, 10);
    });
    function SetRemainTime() {
      var now = new Date();
      var SysSecond = new Date(document.getElementById("countdown_end").innerHTML);
      var passtime = SysSecond.getTime() - now.getTime();
      if (passtime > 0) {
        var d = Math.floor(passtime / (1000 * 60 * 60 * 24));
        passtime = passtime - (d * (1000 * 60 * 60 * 24));
        var h = Math.floor(passtime / (1000 * 60 * 60));
        passtime = passtime - (h * (1000 * 60 * 60));
        var m = Math.floor(passtime / (1000 * 60));
        passtime = passtime - (m * (1000 * 60));
        var s = Math.floor(passtime / 1000);
        passtime = passtime - (s * (1000));
        var ms = Math.floor(passtime / 10);
        if (m < 10) { m = '0' + m; }
        if (s < 10) { s = '0' + s; }
        if (ms < 10) { ms = '0' + ms; }
        if (d > 0) { h = h + d*24; }
        $("#countdown").html("cè<span id='hour'class='countNumber'>"
          + h + "</span>Ô<span id='min' class='countNumber'>"
          + m + "</span>ª<span id='sec' class='countNumber'>"
          + s + "</span>b" + "<span class='countNumber'>"
          + ms + "</span>"
        );
      } else {
        window.clearInterval(InterValObj);
      }
    }
  </script>

