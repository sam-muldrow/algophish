        function showUnableActionModal() {

          $('html, body').addClass('lock');
          $('body').append('<div class="modal-overlay"></div>');
          $('.modal-overlay').fadeIn('slow');

          var modal = '#guest_modal_content';
          $(modal).wrap("<div class='modal-wrap'></div>");
          $('.modal-wrap').show();
          modalResize();

          $(modal).fadeIn('slow');
          $(modal).click(function(e){
            e.stopPropagation();
          });

          $('.modal-wrap, .modal-close, #guest_continue').off().click(function() {
            $(modal).fadeOut('slow');
            $('.modal-overlay').fadeOut('slow',function(){
              $('html, body').removeClass('lock');
              $('.modal-overlay').remove();
              $(modal).unwrap("<div class='modal-wrap'></div>");
              $('#guest_restrict_content').show();
              $('#guest-logout-alert').hide();
             });
          });
          // TCYµ½ç\¦ÊuðÄæ¾
          $(window).on('resize', function(){
            modalResize();
          });
          // [_RecÌ\¦ÊuðÝè·éÖ
          function modalResize() {
            var w = $(window).width();
            var h = $(window).height();

            var mw = $(modal).outerWidth(true);
            var mh = $(modal).outerHeight(true);
            var scrollsize = $(window).width() - $('body').prop('clientWidth');

            if ((mh > h) && (mw > w)) {
              $(modal).css({'left': 0 + 'px','top': 0 + 'px'});
            } else if ((mh > h) && (mw < w)) {
              var x = (w - scrollsize - mw) / 2;
              $(modal).css({'left': x + 'px','top': 0 + 'px'});
            } else if ((mh < h) && (mw > w)) {
              var y = (h - scrollsize - mh) / 2;
              $(modal).css({'left': 0 + 'px','top': y + 'px'});
            } else {
              var x = (w - mw) / 2;
              var y = (h - mh) / 2;
              $(modal).css({'left': x + 'px','top': y + 'px'});
            }
          }
        }

        $('.gray_color, #user_exchange, #navi_invitation, #navi_mypage, #list_merit_overlay, #shopping_guaranntee, #logic, #pc_shopping_point, #immediate_ec').click(function(e) {
            var is_guest = 0;
            e.preventDefault();
            if (is_guest) {
              showUnableActionModal();
            } else {
              var redirectUrl = $(this).find('a').attr('href');
              location.href = redirectUrl;
            }
          });

        $('#guest_leave').click(function() {
          var is_guest = 0;
          if (is_guest) {
            $('#guest_restrict_content').hide();
            $('#guest-logout-alert').show();
            showUnableActionModal();
          }
        });

          // QXgpÌNbN|CgãÀð\¦
          
            $('a').click(function(e) {
              var isGuestClickModalDisplayable = 0;
              var today = '20231120';
              var ck = '557e3c3b93a1d26b5819107324eaaf8a';
              if (! isGuestClickModalDisplayable || getCookie(ck) === today) {
                return true;
              }
              e.preventDefault();
              var redirectUrl = $(this).attr('href');

              if (typeof redirectUrl === 'undefined') {
                return;
              }

              var needRedirectUrl = true;
              $('#user_exchange, #navi_invitation, #navi_mypage, #list_merit_overlay, #shopping_guaranntee, #logic, #pc_shopping_point, #immediate_ec')
                .find('a').each(function() {
                  if ($(this).attr('href') === redirectUrl) {
                    needRedirectUrl = false;
                  }
              });

              if (redirectUrl.indexOf('/cl/?id=') >= 0) {
                $.ajax({
                url: '/guest/update_display_click_modal_flag',
                cache: false,
                type: 'GET',
                success: function(res) {
                  if (res.display === true) {
                    $('#guest-click-alert').show();
                    showUnableActionModal();
                    $('.modal-wrap, .modal-close').off().click(function() {
                      $('#guest_modal_content').fadeOut('slow');
                      $('.modal-overlay').fadeOut('slow',function() {
                        $('html, body').removeClass('lock');
                        $('.modal-overlay').remove();
                        $('#guest_modal_content').unwrap("<div class='modal-wrap'></div>");
                        $('#guest-click-alert').hide();
                      });
                    });
                  }
                }
                });
              } else {
                if (needRedirectUrl) {
                  location.href = redirectUrl;
                }
              }
            });
      });

  </script>

