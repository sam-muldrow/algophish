        $(function(){
            $("a[id=daily_gacha]").on('click', function(){
                $.modal('<iframe width="740" height="650" src="" frameborder="0" id="alert_window" allowfullscreen></iframe>',{
                    closeHTML: "<a href='#' title='Close' class='modalCloseImg' ></a>",
                    overlayId: 'confirm-overlay',
                    containerId: 'confirm-container_gacha',
                    minHeight:'650',
                    minWidth:'740',
                    position:[55, 0],
                    //autoPosition:true,
                    overlayClose:true,
                    onShow: function() {
                        $("iframe[id=alert_window]").attr("src", $("a[id=daily_gacha]").attr('href'));
                    },
                    overlayCss: {
                    opacity: 0.9,
                    backgroundColor: "#000",
                    Left: 394
                    }
                });
                return false;
            });

            $("a[class=attention]").on('click', function(){
