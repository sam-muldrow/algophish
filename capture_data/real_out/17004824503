var tpl = "top";
var view = "index";
if (tpl == 'top' && view == 'index') {
  var iframes = document.getElementsByTagName("iframe");
  for(var i = 0; i < iframes.length; i++){
    var ids = iframes[i].id;
    var str = ids.split("_");
    if (str[2] == '562231') {
     $("#"+ids).hide();
     $("#third_tag").hide();
    }
  }
}
</script>




