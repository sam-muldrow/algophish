<!--Cookies popup starts-->
<div id="acceptall" class="cookies-popup open" style="display: none;">
    <div class="white-box">
        <div class="all-data">
            <div class="text">
            <p>By clicking “Accept All Cookies”, you agree to the storing of cookies on your device to enhance site navigation, analyze site usage, and assist in our marketing efforts. <a href="https://www.bollywoodlife.com/cookie-policy/">Cookie Policy</a></p>
            </div>
            <!--div class="popup-buttons"> <a href="#CookiesSettings" class="btn btn-settings">Cookies Settings</a-->
            <div class="btnall">
            <button id="rejectcookies" type="button" class="btn btn-reject">Reject all</button>
            <button id="allcookies" type="button" class="btn btn-accept">Accept all cookies</button>
            </div>
            </div>
            <i class="cookies-close"></i>
      </div>
       </div>
</div>
<!--Cookies popup ends--> 
<script langauge="javascript">
function setBLCookie(cName, cValue, expDays) {
      let date = new Date();
      if(expDays==365){
         date.setTime(+ date + (expDays * 86400000)); //24 * 60 * 60 * 1000
      }else{
          date.setTime(+ date + (expDays * 86400000));
      }
      
      const expires = "expires=" + date.toUTCString();
      document.cookie = cName + "=" + cValue + "; " + expires + "; path=/";
}
function getBLCookie(cName) {
      const name = cName + "=";
      const cDecoded = decodeURIComponent(document.cookie); //to be careful
      const cArr = cDecoded .split('; ');
      let res;
      cArr.forEach(val => {
          if (val.indexOf(name) === 0) res = val.substring(name.length);
      })
      return res;
}
function checkCookie() {
  var cname;
  cname = getBLCookie('cpAcc');
  if(cname=='1'){
    return 1;
  }
  cname = getBLCookie('cpRej');
  if(cname=='1'){
    return 1;
  }
  return 0;  
}
 jQuery(document).ready(function ($) {
        //cookies settings popup
         var flag = checkCookie();
         if(flag==0){
            $('#acceptall').show();
         }else{
            $('#acceptall').hide();
         }
        $('#allcookies').on("click",function(e){
            $('#acceptall').hide();
            setBLCookie('cpAcc', 1, 365);
        });

        $('#rejectcookies').on("click",function(e){
            $('#acceptall').hide();
            setBLCookie('cpRej', 1, 1);
        });
        $(".cookies-close").on("click",function(e){
            $('#acceptall').hide();
        });

    });

</script>
<!--Cookie settings ends--><script langauge="javascript">
$('.whatshot').click(function(){
	 var url   = $(this).find('a').attr("href")
	 dataLayer.push({"event":"trending_strip",
                            "event_label":url});
})
$(document).on("click",".homewidget",function() {
  var widgetname = $(this).attr('class').split(' ')[2];
  dataLayer.push({"event":"homepage",
                            "event_label":widgetname});
});
$(document).on("click",".social_share",function() {
        var platformname = $(this).attr('class').split(' ')[1];
        var event_name = 'social_share' 
        if(platformname=='chat'){
          event_name = 'chatbot';
          platformname = $(location).attr('href');
        }
        dataLayer.push({"event":event_name,
                            "event_label":platformname});
     });
$(document).on("click",".li_main_menu",function() {
  var lang = $(this).attr('class').split(' ')[1];
  dataLayer.push({"event":"lang_select",
                            "event_label":lang});
});

$(document).on("click","a.relatedarticlelink",function() {
				if($(this).hasClass('pgalsoread')){
                return "";
        }else{
        dataLayer.push({"event":"also_read",
        "event_action":"click ",
				"event_label":"Article details",
				"publish_date":$(this).attr('postdate'),
				"author_name":$(this).attr('author'),
				"section":$(this).attr('category'),
				"story_url":$(this).attr('href')
        });
      }
 });

$(document).on("click","a.pgalsoread",function() {
        dataLayer.push({"event":"also_read",
                "event_action":"click ",
                "event_label":"Photo Galleries",
                "publish_date":$(this).attr('postdate'),
                "author_name":$(this).attr('author'),
                "section":$(this).attr('category'),
                "story_url":$(this).attr('href')
                });
        });
$(document).on("click","a.bottomnav",function() {
     dataLayer.push({"event":"bottom_nav",
                "event_action":"click ",
                "event_label":$(this).attr('title'),              
                "custom_page_url":window.location.href
      });
});
$(document).on("click","a.watchnext",function() {
     dataLayer.push({"event":"videos",
                "event_action":"click ",
                "event_label":'watch next',     
                "publish_date":$(this).attr('postdate'),
                "author_name":$(this).attr('author'),
                "section":$(this).attr('category'),         
                "story_url":$(this).attr('href')
      });
});
$(document).on("click","a.alsowatch",function() {
     dataLayer.push({"event":"also_watch",
                "event_action":"click",
                "event_label":$(this).attr('title'),     
                "publish_date":$(this).attr('postdate'),
                "author_name":$(this).attr('author'),
                "section":$(this).attr('category'),         
                "story_url":$(this).attr('href')
      });
});
</script>
<script>    
// Subscribe Now popup
$( document ).ready(function() {
jQuery(document).on("click touchstart",function(e){var a=jQuery(".subscribe-btn, .subscribe-popup .white-box, .subscribe-close");a.is(e.target)||0!==a.has(e.target).length||jQuery(".subscribe-popup").fadeOut();}),jQuery(".subscribe-close, .subscribe-btn").click(function(){jQuery(".subscribe-popup").fadeToggle();});
  $(".subscribe-btn").on("click", function() {
    $('#fullname').val("");
    $('#email').val("")
    $('.showhide').show();
    $('.thanks-u').hide();    
  });
  $("#submitnow").on("click", function() {
    var pattern = /^[a-zA-Z ]+$/;
    var name = $.trim($('#fullname').val());
    if(!pattern.test(name)){
        $('#fullname').focus();
        alert("Please enter valid name");
        return false;
    }
    var email = $('#email').val();
    var pattern = /^\b[A-Z0-9._%-]+@[A-Z0-9.-]+\.[A-Z]{2,4}\b$/i

    if(!pattern.test(email))
    {
      $('#email').focus();
      alert("Please enter valid email");
      return false;
    }
    $("#submitnow").prop('disabled', true);
    // Send the data to the controller using Ajax
    $.ajax({
        url: 'https://www.bollywoodlife.com/subscribe/',
        method: 'post',
        data: {
            name: name,
            email: email
        },
        success: function(response) {
            $("#submitnow").prop('disabled', false);
            $('.showhide').hide();
            $('.thanks-u').show();
        }
    });
  })
})
</script>
</body>
</html>
