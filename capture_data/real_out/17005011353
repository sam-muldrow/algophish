<script>
if (typeof playerInstance !== 'undefined') {
 var url = window.location.href;
 var videotype = "Own Video"; 
 var isVideoPlayed = false;
 var isAdSkippedCompleted = false;

var media_id;
var final = ":";
url = url.concat(final).concat(media_id); 
url = media_id;

  playerInstance.on('adRequest', function(e) {
  gtag('event', 'adRequest', { 'event_category': videotype, 'event_label': url});
  gtag('event', 'play', { 'event_category': videotype, 'event_label': url});
  isVideoPlayed = true;
  isAdSkippedCompleted = true;
 });
 
  playerInstance.on('adComplete', function(e) {
  gtag('event', 'adComplete', { 'event_category': videotype, 'event_label': url});
  isAdSkippedCompleted = true;
 });
 
 playerInstance.on('adSkipped', function(e) {
  gtag('event', 'adSkipped', { 'event_category': videotype, 'event_label': url});
  isAdSkippedCompleted = true;
 });
 
 playerInstance.on('adClick', function(e) {
  gtag('event', 'adClick', { 'event_category': videotype, 'event_label': url});
 });
 
 playerInstance.on('pause', function(e) {
  gtag('event', 'pause', { 'event_category': videotype, 'event_label': url});
 });
 
 playerInstance.on('firstFrame', function(e) {
 });
 
 playerInstance.on('complete', function(e) {
  gtag('event', 'complete', { 'event_category': videotype, 'event_label': url});
 });
 
 playerInstance.on('play', function(e) {
  if((isVideoPlayed) && (isAdSkippedCompleted)){
  isAdSkippedCompleted = false;
  }else if((isVideoPlayed)){
  gtag('event', 'resume', { 'event_category': videotype, 'event_label': url});
  }else{
  gtag('event', 'play', { 'event_category': videotype, 'event_label': url});
  isVideoPlayed = true;
  }
 });
 
}
</script>




<script type="text/javascript">
	function getCookie(name){
     var re = new RegExp(name + "=([^;]+)");
     var value = re.exec(document.cookie);
     return (value != null) ? unescape(value[1]) : null;
	}
     
	var getUrlParameter = function getUrlParameter(sParam) {
    var sPageURL = window.location.search.substring(1),
        sURLVariables = sPageURL.split('&'),
        sParameterName,
        i;
    for (i = 0; i < sURLVariables.length; i++) {
        sParameterName = sURLVariables[i].split('=');
        if (sParameterName[0] === sParam) {
            return sParameterName[1] === undefined ? true : decodeURIComponent(sParameterName[1]);
        }
    }
	};

	var akamai_source = "";
	var akamai_medium = "";
  
	akamai_source = getUrlParameter('utm_source');
	akamai_medium = getUrlParameter('utm_medium');
  
	if(akamai_source == '' || akamai_source == undefined ){ akamai_source = getCookie("traffic_source"); }
	if(akamai_medium == '' || akamai_medium == undefined ){ akamai_medium = getCookie("traffic_medium"); }
window.addEventListener("load", (event) => {
	$(document).ready(function(){
     localStorage.setItem('SSO-UTM',JSON.stringify({"utm_source":akamai_source,"utm_medium":akamai_medium}));
	});
})
</script>


<script type="text/javascript">

function doRegisterEvents(kalturaPlayer) {
 var mymediaid = kalturaPlayer.config.sources.dash[0].id;

 /* Playback Related */
 kalturaPlayer.addEventListener(kalturaPlayer.Event.Core.PLAY, function() { playEvent(mymediaid); });
 kalturaPlayer.addEventListener(kalturaPlayer.Event.Core.PAUSE, function() { pauseEvent(mymediaid); });
 //kalturaPlayer.addEventListener(kalturaPlayer.Event.Core.PLAYING, function() { playingEvent(mymediaid); });
 kalturaPlayer.addEventListener(kalturaPlayer.Event.Core.PLAYBACK_ENDED, function() { playbackEndedEvent(mymediaid); });
 kalturaPlayer.addEventListener(kalturaPlayer.Event.Core.MUTE_CHANGE, function() { muteChangeEvent(mymediaid); });
// kalturaPlayer.addEventListener(kalturaPlayer.Event.Core.SEEKED, function() { seekedEvent(mymediaid); });

 /* AD Related */
 kalturaPlayer.addEventListener(kalturaPlayer.Event.AD_LOADED, function() { adLoadedEvent(mymediaid); });
 kalturaPlayer.addEventListener(kalturaPlayer.Event.AD_PAUSED, function() { adPausedEvent(mymediaid); });
 //kalturaPlayer.addEventListener(kalturaPlayer.Event.AD_PROGRESS, function() { adProgressEvent(mymediaid); });
 kalturaPlayer.addEventListener(kalturaPlayer.Event.AD_STARTED, function() { adStartedEvent(mymediaid); });
 kalturaPlayer.addEventListener(kalturaPlayer.Event.AD_COMPLETED, function() { adCompletedEvent(mymediaid); });
 kalturaPlayer.addEventListener(kalturaPlayer.Event.AD_SKIPPED, function() { adSkippedEvent(mymediaid); });
 kalturaPlayer.addEventListener(kalturaPlayer.Event.AD_CLICKED,function() { adClicked(mymediaid); });

 /* Fullscreen & PIP Related */
 kalturaPlayer.addEventListener(kalturaPlayer.Event.Core.ENTER_FULLSCREEN, function() { enterFullscreenEvent(mymediaid); });
 kalturaPlayer.addEventListener(kalturaPlayer.Event.Core.EXIT_FULLSCREEN, function() { exitFullscreenEvent(mymediaid); });
 kalturaPlayer.addEventListener(kalturaPlayer.Event.Core.ENTER_PICTURE_IN_PICTURE, function() { enterPipEvent(mymediaid); });
 kalturaPlayer.addEventListener(kalturaPlayer.Event.Core.LEAVE_PICTURE_IN_PICTURE, function() { leavePipEvent(mymediaid); });

}

  var isVideoPlayed = false;
  var videotype = "Own Video";


  function adLoadedEvent(mymediaid) {
    gtag('event', 'adRequest', { 'event_category': videotype, 'event_label': mymediaid});
  }

  function adStartedEvent(mymediaid) {
    gtag('event', 'adStarted', { 'event_category': videotype, 'event_label': mymediaid});
  }

  function adPausedEvent(mymediaid) {
    gtag('event', 'adPaused', { 'event_category': videotype, 'event_label': mymediaid});
  }

  function adProgressEvent(mymediaid) {
    gtag('event', 'adProgress', { 'event_category': videotype, 'event_label': mymediaid});
  }

  function adCompletedEvent(mymediaid) {
    gtag('event', 'adCompleted', { 'event_category': videotype, 'event_label': mymediaid});
  }

  function adSkippedEvent(mymediaid) {
    gtag('event', 'adSkipped', { 'event_category': videotype, 'event_label': mymediaid});
  }

  function adClicked(mymediaid) {
    gtag('event', 'adClicked', { 'event_category': videotype, 'event_label': mymediaid});
  }

  function enterFullscreenEvent(mymediaid) {
    gtag('event', 'enterFullscreen', { 'event_category': videotype, 'event_label': mymediaid});
  }

  function exitFullscreenEvent(mymediaid) {
    gtag('event', 'exitFullscreen', { 'event_category': videotype, 'event_label': mymediaid});
  }

  function enterPipEvent(mymediaid) {
    gtag('event', 'enterPip', { 'event_category': videotype, 'event_label': mymediaid});
  }

  function leavePipEvent(mymediaid) {
    gtag('event', 'leavePip', { 'event_category': videotype, 'event_label': mymediaid});
  }

  function playEvent(mymediaid) {
    if((isVideoPlayed)){
      gtag('event', 'play', { 'event_category': videotype, 'event_label': mymediaid});
    }else{
      gtag('event', 'play', { 'event_category': videotype, 'event_label': mymediaid});
      isVideoPlayed = true;
    }	
  }

  function playingEvent(mymediaid) {
    gtag('event', 'playing', { 'event_category': videotype, 'event_label': mymediaid});
  }

  function pauseEvent(mymediaid) {
    gtag('event', 'pause', { 'event_category': videotype, 'event_label': mymediaid});
  }

  function playbackEndedEvent(mymediaid){
    gtag('event', 'complete', { 'event_category': videotype, 'event_label': mymediaid});
  }

  function muteChangeEvent(mymediaid){
    gtag('event', 'mute', { 'event_category': videotype, 'event_label': mymediaid});
  }

  function seekedEvent(mymediaid){
    gtag('event', 'seeked', { 'event_category': videotype, 'event_label': mymediaid});
  }

  
  $(".sub_nav_ul li a").on('click',function(){
        var thisurl = window.location.href.split(/[?#]/)[0];
        gtag('event', 'click', { 'event_category': 'Trending Strip', 'event_label': thisurl});
  });


</script>

