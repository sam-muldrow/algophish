<script>
(function() {
    window.wbq = window.wbq || [];
    function hack() {
        Array.from(document.querySelectorAll("[href='https://www.linkedin.com/careersite/tmz']")).forEach(function(link) {
            link.href = 'https://www.linkedin.com/company/tmz/';
        });
    }
    window.wbq.push(function() {
        if (window.TMZ.select(window.TMZ.selectors.getPage).rendered) {
            hack();
        } else {
            document.addEventListener(window.TMZ.constants.actionTypes.PAGE_RENDERED, hack);
        }
    });
})();
</script>
<!-- END https://github.com/tmz-apps/tmz-web/issues/2617 -->

