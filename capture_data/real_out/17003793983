  <script>
    wbq.push(function renderYoutubeVideoBlock() {
      var $target = $('#cb-f5b5aef76aad1235422a670f916e634c-1150437648-target');
      var targetWidth = $target.width();

      function initPlayer(onReady) {
          new YT.Player('cb-f5b5aef76aad1235422a670f916e634c-1150437648-target', {
            height: targetWidth / (16 / 9),
