<script>
wbq.push(function renderInstagramMediaBlock() {
  var response = 'unknown';
  var target = document.getElementById('cb-d279e94febc77823021c4af10622e7c0-1861931937-target');

  function accept() {
    if (response === 'accepted') {
      return;
    }

    target.innerHTML = document.getElementById('cb-d279e94febc77823021c4af10622e7c0-1861931937-accepted').innerHTML;
    window.TMZ.utils.loadInstagramJs().then(function (instgrm) {
      instgrm.Embeds.process();
      setTimeout(function () {
        // only apply to the target, not the entire document
        $('#cb-d279e94febc77823021c4af10622e7c0-1861931937-target .instagram-media-rendered').addClass('ratio');
      }, 1000);
    }).catch(reject);

    response = 'accepted';
  }

  function reject() {
    if (response === 'rejected') {
      return;
    }

    target.innerHTML = document.getElementById('cb-d279e94febc77823021c4af10622e7c0-1861931937-rejected').innerHTML;
    response = 'rejected';
  }

  watchConsent(function (canUse) {
    canUse('instagram') ? accept() : reject();
  });
});
</script>
