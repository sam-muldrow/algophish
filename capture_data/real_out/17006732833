  /*
    Make Webflow lightbox configuration script tags "visible" (without taking
    up any visual space) so we can have CSS animations fire on them.
  */
  script[class='w-json'] {
    display: block;
    max-height: 0;
    overflow: hidden;
    pointer-events: none;
    position: absolute;
    visibility: hidden;
  }

  /* Trigger animation events for scripts and iframes that need updating */
  script[class="w-json"], /* Webflow lightbox configuration scripts */
  iframe[src^="https://www.youtube.com/"] /* Youtube embeds using the wrong domain */
  {
    animation-name: nodeInserted;
    animation-duration: 0.001s;
  }

  @keyframes nodeInserted {
    from {
      opacity: 0.99;
    }
    to {
      opacity: 1;
    }
  }
</style>

<script type="text/javascript">
  ;(function () {
    'use strict'

    // Rewrite Webflow lightbox and iframe youtube embeds to use the
    // www.youtube-nocookie.com domain.
    //
    // We're using the animationstart event because it fires as soon as these
    // elements are added to the DOM and (most importantly) _before_ Webflow's
    // "ready" logic executes. Ideally this will also update any iframes not
    // already set to use www.youtube-nocookie.com before the iframe has a chance
    // to load anything from www.youtube.com.
    document.addEventListener(
      'animationstart',
      function (e) {
        if (e.animationName == 'nodeInserted') {
          var node = e.target

          // Webflow's lightbox component stores its configuration in
          // <script type="application/json" class="w-json"> tags
          if (node.nodeName === 'SCRIPT' && node.className === 'w-json') {
            console.log('updating Webflow lightbox configuration')
            node.textContent = node.textContent
              .replace(/www\.youtube\.com/g, 'www.youtube-nocookie.com')
              .replace(/schema=youtube\\/g, 'schema=youtube-nocookie\\')
          } else if (node.nodeName === 'IFRAME') {
            console.log('updating iframe youtube embed')
            node.src = node.src.replace(
              /www\.youtube\.com/g,
              'www.youtube-nocookie.com'
            )
          }
        }
      },
      false
    )

    // Looks for specific query parameters in the URL and adds a class to the
    // <html> element if they are present.
    ;(function addTemplateModifierClassNames() {
      const urlParamToClassNameMap = {
        isolated: 'js-template-isolated',
        'mobile-help': 'js-template-mobile_help',
      }

      const urlParams = new URLSearchParams(window.location.search)
      Object.keys(urlParamToClassNameMap).forEach(function (param) {
        if (urlParams.has(param)) {
          const className = urlParamToClassNameMap[param] || ''
          document.documentElement.classList.add(className)
        }
      })
    })() // self-executing function
    
    function appendStylesheet(url) {
      const linkElement = document.createElement('link')
      linkElement.setAttribute('rel', 'stylesheet')
      linkElement.setAttribute('href', url)
      document.head.appendChild(linkElement)
    }

    function appendHeadScript(url, type = 'module', id = '', defer = false) {
      const scriptElement = document.createElement('script')
      scriptElement.setAttribute('src', url)
      scriptElement.setAttribute('type', type)
      scriptElement.setAttribute('id', id)
      scriptElement.defer = defer
      document.head.appendChild(scriptElement)
    }

    function appendBodyScript(url, type = 'module') {
      const scriptElement = document.createElement('script')
      scriptElement.setAttribute('src', url)
      scriptElement.setAttribute('type', type)
      document.addEventListener('DOMContentLoaded', () => {
        document.body.appendChild(scriptElement)
      })
    }

    function startGrowthbook() {
      console.log('[GROWTHBOOK] SDK loaded');

        if (!window.growthbook) return;

        var gb = new growthbook.GrowthBook({
          apiHost: 'https://cdn.growthbook.io',
          clientKey: 'sdk-14kZvGs73TZdHUMG',
          enableDevMode: true,
          subscribeToChanges: true,
          trackingCallback: function(experiment, result) {
            var experimentId = experiment.key;
            var variationId = result.key;

            console.log("[GROWTHBOOK] Experiment Viewed", {
              experimentId: experimentId,
              variationId: variationId,
            });
            if (!!window._amplitude) {
              console.log("[GROWTHBOOK] reporting experiment to Amplitude", {
                experimentId: experimentId,
                variationId: variationId,
              });
              window._amplitude(undefined, 'track', 'Received Experiment Variant', {
                'Experiment ID': experimentId,
                'Variant ID': variationId,
              });
            }
          }
        });

        gb.loadFeatures().then(function() {
          console.log("[GROWTHBOOK] Features loaded");
        });
    }
    
    function loadGrowthBookSDK() {
      console.log('[GROWTHBOOK] loading GrowthBook SDK')

      appendHeadScript('https://cdn.jsdelivr.net/npm/@growthbook/growthbook/dist/bundles/index.min.js', 'text/javascript', 'growthbook-sdk', true)
      
      if (window.growthbook) {
        startGrowthbook()
      } else {
        document.querySelector('#growthbook-sdk').addEventListener('load', startGrowthbook)
      }
    }
    
    const isProduction = window.location.hostname.endsWith('ynab.com')
    const cdnDomain = isProduction ? 'www.ynabassets.com' : 'www.banyassets.dev'

    appendStylesheet(`https://${cdnDomain}/assets/style.css`)
    appendHeadScript(`https://${cdnDomain}/assets/head.js`)
    appendBodyScript(`https://${cdnDomain}/assets/body.js`)
    
    if (!isProduction) {
      loadGrowthBookSDK()
    }
  })()
</script><meta property="og:url" content="https://www.ynab.com"/></head><body data-track-page-view="true" data-track-page-name="Home" class="body"><div class="page-wrapper"><div class="global-styles w-embed"><style>

