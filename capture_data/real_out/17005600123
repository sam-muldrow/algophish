<script type="text/javascript">
  window.xcelAdblockDetect = window.xcelAdblockDetect || {}

  const adblock = document.querySelector('.js-adblock')
  const closeButton = adblock.querySelector('.js-adblock-close-button')

  const toggleWarning = (setCookie = false) => {
    adblock.classList.toggle('adblock--show')

    if (setCookie && !xcelAdblockDetect.getCookie()) {
      xcelAdblockDetect.setCookie()
    }
  }

  closeButton.addEventListener('click', () => toggleWarning(true));

  window.xcelAdblockDetect.toggleWarning = toggleWarning
</script>

