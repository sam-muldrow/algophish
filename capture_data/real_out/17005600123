// Ad Friendly Style checker
var afs = function (checkAfs, onRefMatch, onParMatch) {
	var md = new MobileDetect(window.navigator.userAgent);

	// Attention: the afsActive cookie can be reset by mobileModal.js to switch true/false user-side decision
	var createCookie = function (cookieName) {
		if(cookieName == undefined) {
			cookieName = 'afsActive'
		}
		// we are usinf max-age instead of expire - it changed in the iso standard some times ago,
		// and we'll set no value to activate session time cookies!
		document.cookie = cookieName + '=true; max-age=' + (30 * 60) + '; path=/';
	};

	var readCookie = function (name) {
		var name = name + "=";
		var decodedCookie = decodeURIComponent(document.cookie);
		var ca = decodedCookie.split(';');
		for (var i = 0; i < ca.length; i++) {
			var c = ca[i];
			while (c.charAt(0) == ' ') {
				c = c.substring(1);
			}
			if (c.indexOf(name) == 0) {
				return c.substring(name.length, c.length);
			}
		}
		return null;
	};

	this.enabled = function () {
		if (!checkAfs) {
			// if checkAfs is disabled, will quit at this point
			return false;
		} else if (readCookie('afsActive')=='true') {
			// if we already have a true decision saved in cookie we'll return at this point
			return true;
		} else if (readCookie('afsActive')=='false') {
			// if we already have a false decision saved in cookie we'll return at this point
			return false;
		} else if (md.tablet() !== null) {
			// save true decision for tablets and return the same at this point
			createCookie();
			return true;
		} else if (onRefMatch && document.referrer.indexOf(onRefMatch) !== -1) {
			// save true decision on matching referrer to this page and return the same at the end of the block
			createCookie();
			// and show modal for mobile devices to enable user decision
			if (md.phone()) {
				showModal();
			}
			return true;
		} else if ((onParMatch && window.location.href.indexOf(onParMatch) !== -1)) {
			// save true decision on matching query parameter on this page and return the same at the end of the block
			// but only for mobile devices and show modal to enable user decision
			if (md.phone()) {
				createCookie();
				showModal();
				return true;
			}
			// for other devices we'll return false decision
			return false;
		} else {
			// nothing matched until here, so we will return false decision
			return false;
		}
	};

	function showModal() {
		window.onload = function () {
			if (document.getElementById('js-mobile-modal') !== null) {
				document.getElementById('js-mobile-modal').style.display = 'block';
			}
		};
	}
};

var afsMode = new afs(true, false);

    if (afsMode.enabled() && true) {
      var ratio = window.width < window.screen.width ? window.width / 1280 : window.screen.width / 1280;
      document.write('<meta name="viewport" content="width=device-width, user-scalable=yes, initial-scale=' + ratio + '" />');
      document.write('<link rel="stylesheet" type="text/css" href="/sites/all/themes/tvmovie/css/ad-friendly.css" />');
    }
    else {
      document.write('<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no"/>');
      document.write('<link rel="stylesheet" href="/sites/all/themes/tvmovie/css/main.css" media="screen, tv, projection, print" />');
    }
//--><!]]>
</script>
