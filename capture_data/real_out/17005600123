<script type="text/javascript">
  window.openPrivacyManager = function() {
    window._sp_.loadPrivacyManagerModal(685132)
  }

  window.addEventListener("load", async () => {
    if (window.location.href.includes('datenschutz')) {
      var anchorText = 'Datenschutzmanager'
      var placeholder = `[${anchorText}]`
      var link = `<a href="javascript:void(0)" onclick="window.openPrivacyManager()">${anchorText}</a>`

      var paragraphs = document.querySelector('.typo--editor, .text')?.querySelectorAll('p')
      Array.from(paragraphs)
        ?.filter((paragraph) => paragraph.innerText?.includes(placeholder))
        ?.forEach((paragraph) => { paragraph.innerHTML = paragraph.innerText.replace(placeholder, link) })
    }

	  var queryString = window.location.search;
	  var urlParams = new URLSearchParams(queryString);
    if (urlParams.get('showprivacymanager')) {
      // sometimes sourcepoint is not loaded or the window.openPrivacyManager is not defined, so we retry here
      // waiting for document.readyState === 'complete' is not enough, because the sourcepoint script is loaded async and there's no event when it's loaded that we can listen to
      let retryCount = 0
      while (retryCount < 25) {
        // apparently waiting for window._sp_ is not enough, the function gets attached to the _sp_ object during runtime. big fuck
        if (typeof window._sp_?.loadPrivacyManagerModal === 'function' && typeof window.openPrivacyManager === 'function') {
          window.openPrivacyManager()
          break
        } else {
          retryCount++
          // wait for 200ms before next attempt
          await new Promise(resolve => setTimeout(resolve, 200))
        }
      }
    }
  })
</script>
