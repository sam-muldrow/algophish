	<!--SKY GAAT MEE MET SCROLL-->
	<style>
		#skyCrapp.vast{
			position:fixed !important;
			top:90px !important;
			z-index:99999999999;
		}
	</style>

	<script type="text/javascript">
		$(function() {
			var offset = $("#skyCrapp").offset();
			var topPadding = 0;
			$(window).scroll(function() {

				//$("#getInfoo").html($(window).scrollTop() + " - " + offset.top);

				if ($(window).scrollTop() > (offset.top)) {
					$("#skyCrapp").addClass("vast");
					//alert(1);
					//$("#skyCrapp").stop().animate({
					//	marginTop: $(window).scrollTop() - offset.top + topPadding
					//});
				} else {
					$("#skyCrapp").removeClass("vast");
					//$("#skyCrapp").stop().animate({
					//	marginTop: 0
					//});
				};
			});
		});
	</script>


	

