				<script>
				$(function() {
					var startPos = $(window).scrollTop();
					var fixedStatus = false;
					var subShown = false;
					var searchBar = false;
					$(window).scroll(function() {
						checkForFixedHeader();
						if (searchBar == true) {
							$("#openZoek").hide();
							searchBar = false;
							//console.log("SLUITENNN");
						}
					});

					$(document).ready(function() {
						checkForFixedHeader();
					});

					//SEARCH
					$("#nav .search").click(function(){
						if (searchBar == true) {
							$("#openZoek form").submit();
						} else {
							$("#openZoek").fadeIn( "slow", function() {
								$("#openZoek input").focus();
								searchBar = true;
							});
						}
					});

					$(".header-frame .searchHead").click(function(){
						if (searchBar == true) {
							$(".header-frame .searchBox form").submit();
						} else {
							$(".header-frame .searchBox").fadeIn( "slow", function() {
								$(".header-frame .searchBox input").focus();
								searchBar = true;
							});
						}
					});

					//ZOEK SLUITEN!
					var lockSearch = false;
					$("div, body").click(function() {
						if (searchBar == true) {

							//if ( $(this).attr('class') == "inlogVenster" ) {
							if (lockSearch == false) {

								if ( ($(this).hasClass("searchBox") == true) || ($(this).hasClass("searchHead") == true) || ($(this).attr('id') == "openZoek")) {

									//console.log("niet disablen!");

									lockSearch = true;
									setTimeout(function(){
										lockSearch = false;
									}, 100);

									//

								} else {
									//console.log("disablen!!");
									searchBar = false;
									lockSearch = false;
									$(".header-frame .searchBox").hide();
									$("#openZoek").hide();
								}
								////CLICKEN BUITEN ZOEK
								//if (searchBar == true) {
								//	//console.log(2);
								//	searchBar = false;
								//	$(".header-frame .searchBox").hide();
								//	$("#openZoek").hide();
								//}
							}

							//console.log("niks doen!");
						}

					})



					//INLOGVENSTER
					$("#wrapper").addClass('wrapper');
					
					//SLUITEN
					$(document).keyup(function(event) {
						//ESC
						if ( $('html').hasClass('login') ) {
							//console.log(event.which);
							if ((event.which) == 27) {
								hideInlog();
							};
						};
						if ((event.which) == 27) {
							searchBar = false;
							lockSearch = false;
							$(".header-frame .searchBox").hide();
							$("#openZoek").hide();
						};


					});
					$("#escVenster").click(function() {
						if ( $('html').hasClass('login') ) {
							hideInlog();
						};
					});
					$("div:not(.inlogVenster *,.wrapper)").click(function() {
						//console.log($(this));
						if ( $('html').hasClass('login') ) {
							//console.log($(this));
							if ( $(this).attr('class') == "inlogVenster" ) {

							} else {
								hideInlog();
							}
						}
					});


					function hideInlog(){
						$("#inlogHide").fadeOut("fast");
						$("#inlogVenster").hide();
						$('html').removeClass('login');
					}
					//FACEBOOKLOGIN
					$("#facebookLogin, #postFBlogin").click(function() {
						if ($(this).hasClass("reageerFB")) {
							registreerMetFacebook("reageer");
							jumper("#reageer");
						} else {
							$(this).hide();
							registreerMetFacebook("");
						}
					});




					function checkForFixedHeader(){
						if ( $('body').hasClass('fixed') ) {
							//SMALLER HEADER
							//SEARCH
							//console.log("searchBar: " + searchBar);
							if (searchBar == true) { //GEKLIKT!
								$(".header-frame .searchBox").hide();
								$("#openZoek").show();
							}
						} else {
							if (searchBar == true) { //GEKLIKT!
								$(".header-frame .searchBox").show();
								$("#openZoek").hide();
							}
							offset = $("#content").offset();
						}
						//console.log($(window).scrollTop() + " -- " + (offset.top + 80));
						if ($(window).scrollTop() > (offset.top + 10)) { //KLEINE HEADER
							if ( $('body').hasClass('fixed') ) {
								//=FIXED
								//console.log("FIXED!");
							} else {

								$('body').addClass('fixed');
								$('.main-menu').prepend('<li class="fixedLogo"><img height="40" src="https://static.voetbalzone.nl/reskin/images/small-logo.png?v=2" /></li>');
								$('.fixedLogo').show("fast");
								$('.showInlogTx .tx').fadeIn("");
								$('.fixedLogo').click(function(){
									parent.location.href = "/";
								});

								fixedStatus = true;
							}

							if ( startPos < $(window).scrollTop() ) {
								//console.log("down");

								if (fixedStatus == true){
									if (subShown == true) {
										//console.log("SLIDE UP!");
										subShown = false;
										$('body.fixed .add-nav').slideUp( "fast", function() {
											$('body').addClass('down');
											$('body').removeClass('up');
										});
									}
								}


							}
							if ( startPos > $(window).scrollTop() ) {
								//console.log("up");

								if (fixedStatus == true){
									if (subShown == false) {
										//console.log("SLIDE DOWN!");
										subShown = true;
										$('body.fixed .add-nav').slideDown( "fast", function() {
											$('body').addClass('up');
											$('body').removeClass('down');
										});
									}
								}
							}

						} else { //NORMALE HEADER
							if ( $('body').hasClass('fixed') ) {
								//console.log("NORMAAL");
								$('body').removeClass('fixed');
								$('.main-menu .fixedLogo').remove();
								$('.add-nav').show();
							}
						}

						startPos = $(window).scrollTop();
					}
				});
				</script>
				
				

