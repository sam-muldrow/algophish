<script type="text/javascript">
    let docId = 0;
    let catId = 0;
    let ac = "";
    const ageInfo = document.getElementById('age_info');
    const cmpInfo = document.getElementById('age_info_sp');
    
    
     

    function SaveAgeInfo(cond) {
        localStorage.setItem("bettingAge", cond);
    }

    function setCookie(cname, cvalue, exdays) {
        const d = new Date();
        d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
        let expires = "expires=" + d.toUTCString();
        document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
    }

    function getCookie(cname) {
        let name = cname + "=";
        let decodedCookie = decodeURIComponent(document.cookie);
        let ca = decodedCookie.split(';');
        for (let i = 0; i < ca.length; i++) {
            let c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    }

    function addToLayer(cond) {
        dataLayer = window.dataLayer || [];
        dataLayer.push({
            'user24andolder': cond
        });
    }

    function hideBlur() {
        document.getElementById('bg_blur').style.visibility = 'hidden';
    }

    document.getElementById('age_modal_confirmation').addEventListener('click', () => {
        hideBlur();
        ageInfo.close();
        location.href = "https://www.voetbalzone.nl/";
    });  

    document.getElementById('age_modal_redirect').addEventListener('click', () => {
        hideBlur();
        ageInfo.close();
        location.href = "https://www.voetbalzone.nl/";
    });

    document.getElementById('age_modal_CMP').addEventListener('click', () => {
        hideBlur();
        cmpInfo.close();
        window._sp_.gdpr.loadPrivacyManagerModal(824217);
    });
</script>

 

            <div id="bg_blur"></div>
                <dialog id="age_modal" class="age_modal_form">
                    <div class="top_bar"></div>
                    <br />
                    <div class="age_modal_question">Om content over online kansspelen volgens wet- en regelgeving te tonen, willen we zeker weten tot welke leeftijdsgroep u behoort.
                    </div>
                    <div>
                        <button id="age_modal_above24" class="btnFont">Ik ben 24 jaar of ouder</button>
                        <button id="age_modal_below24" class="btnFont">Ik ben jonger dan 24 jaar</button>
                    </div>
                    <br/>
                    <div style="font-size: 10px;">Door je keuze te maken bevestig je dat je je bewust bent van de risicoÔøΩs van online kansspelen en dat je momenteel niet bent uitgesloten van deelname aan kansspelen bij online kansspelaanbieders.</div>
                </dialog>

                <script>
                    const ageModal = document.getElementById('age_modal');
                    document.getElementById('bg_blur').style.visibility = 'visible';
                    $('body').css("overflow", "hidden");
                    ageModal.showModal();

                    document.getElementById('age_modal_below24').addEventListener('click', () => {
                        setCookie('bettingAge', 'false', 30);
                        SaveAgeInfo('false');
                        addToLayer('false')
                        ageModal.close();
                        if (docId >0 && catId===23){
                           $('body').css("overflow", "hidden");
                           document.getElementById('bg_blur').style.visibility = 'visible';
                           age_info.showModal();
                        }
                        else {
                            hideBlur();
                            $('body').css("overflow", "auto");
                        }
                    });

                    document.getElementById('age_modal_above24').addEventListener('click', () => {
                        setCookie('bettingAge', 'true', 90);
                        SaveAgeInfo('true');
                        addToLayer('true')
                        hideBlur();
                        ageModal.close();
                        $('body').css("overflow", "auto");
                        
                    });

                
                </script>

