<script>
	$(".competities-box .hold .tabsA .nTab").click(function(){
		$(".competities-box .hold .tabsA > .nTab").removeClass('selected');
		ntabNr = $('.competities-box .hold .tabsA > .nTab').index(this) + 2;

		$('.competities-box .hold .tabsA > .nTab:nth-child('+ntabNr+')').addClass('selected');
		$(".competities-box .hold .inhoudA > .inhoud").removeClass('show');
		$('.competities-box .hold .inhoudA > .inhoud:nth-child('+(ntabNr-1)+')').addClass('show');
	});
