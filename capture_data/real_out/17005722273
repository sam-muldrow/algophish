$(document).ready(function(){
	var koppelTabs 		= $(".main-tabs > li a"); //ALLE TABS INDEXEREN
	var alleTabs		= $("#compTabsOverzicht > div.tab");

	$('.main-tabs li a').click(function(){
		koppelTabs.removeClass('active');
		$(this).addClass('active');
		indexNr = $('.main-tabs > li a').index(this);

		//alert(indexNr);
		nObj = alleTabs[indexNr];
		alleTabs.css("display", "none");
		$(nObj).css("display", "block");
	});

	$("#compSelector_23327").change(function () {
		//alert("include_bakken/index_actueel_"+this.value+".htm");
