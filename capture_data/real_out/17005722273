			$(window).scroll(function(event) {
				if(lazyloadheadlinestonen == 0){
					if($(".allestonenheadlines").offset().top < $(window).scrollTop() + $(window).outerHeight()) {
						lazyloadheadlinestonen = 1;
						$('.lazyloadverberg:lt(20)').removeClass("lazyloadverberg");
						console.log("Advertentie weergegeven Lazyloade!");
						lazyloadheadlinestonen = 0;
					}
				}	
			});
