    <!-- =analytics -->
    
<script async src="https://www.googletagmanager.com/gtag/js?id=UA-2225206-1"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());
  var gtag_config = {
    'link_attribution': true,
    'transport_type': 'beacon'
  };
  if (typeof vurl !== 'undefined') {
      //仮想ページビュー
      gtag_config['page_path'] = vurl;
  }

  gtag('config', 'UA-2225206-1', gtag_config);
</script>
<script>
    /*
    * aタグ自動取得用イベントトラッキング
    * カテゴリ：クリックしたページのパス
    * アクション：リンク先URL
    * ラベル：クリックしたリンクのIDまたはクラス（なければリンク先URL）
    */
    function sendEventAuto() {
        $('a').on('click',function(){
            var ga_action = $(this).attr('href');
            if (typeof ga_action !== 'undefined' && ga_action !== false) {
                var ga_category = document.location.pathname;
                var ga_label = ga_action;
                var id_name = $(this).attr('id');
                var class_name = $(this).attr('class');
                if (typeof id_name !== 'undefined') {
                    ga_label = id_name;
                } else if (typeof class_name !== 'undefined') {
                    ga_label = class_name;
                }
                gtag('event', ga_action, {
                    'event_category':ga_category,
                    'event_label': ga_label
                });
            }
        });
    }

    // イベントトラッキング読み込み
    if (typeof jQuery !== "undefined") {
        $(function(){
            sendEventAuto();
        });
    } else {
        // jQueryが読み込まれていない場合、読み込む処理追加
        var script = document.createElement('script');
        script.src = '/js/jquery-3.6.0.min.js';
        document.head.appendChild(script);
        script.onload = function() {
            sendEventAuto();
        }
    }
</script>

    <!-- /analytics -->


