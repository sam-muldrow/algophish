	}
	function newPaginate_490927(){
		var interval_pag_fn_490927 = setInterval(setPagina_490927, 100);
		function setPagina_490927() {
			if(  typeof(jQuery) != "undefined" && jQuery != null){
				if( jQuery('.ts_vgallery_main_490927').attr("data-pagination")=='pagination'){
					paginationGenerate_490927("490927");
				}
				if( jQuery('.ts_vgallery_main_490927').attr("data-pagination")=='load-more'){
					paginationGenerate_490927("490927");
				}
				clearInterval(interval_pag_fn_490927);
			}
		}
	}
	newPaginate_490927();
	function paginationGenerate(){
		paginationGenerate_490927();
	}
