            function getNumImpressions(objThis, strThatID) {
                var spend = objThis.value;
                var numImps = spend * 1000; 
                
                if (spend >= 100) {
                    numImps = numImps * 3;
                }
                
                strVal = formatNumber(numImps, '');
                document.getElementById(strThatID).innerHTML = strVal;
                getTotalCost();
                return true;
            }

            // Formats numbers by adding commas (and prefix could be '$')
            function formatNumber(nStr, prefix) {
                var prefix = prefix || '';
                nStr += '';
                x = nStr.split('.');
                x1 = x[0];
                x2 = x.length > 1 ? '.' + x[1] : '';
                var rgx = /(\d+)(\d{3})/;
                while (rgx.test(x1))
                    x1 = x1.replace(rgx, '$1' + ',' + '$2');
                return prefix + x1 + x2;
            }
            
            // Following 3 functions used to display field limits
            function objLimit(objThis, objMax) {
                if (objThis.value.length == objMax) return false;
	            return true;
            }
            function objCount(objThis, objID, objMax) {
                objObj = createObject(objID);
                objVal = objThis.value;
                //if (objVal) objVal = objVal.replace(/(\r\n)/g, '\n');   // USE objVal.replace(/(\r\n)/g, '\n') OR objVal.replace('\r\n', '\n')
	            if (objVal.length > objMax) objVal = objVal.substring(0, objMax);
	            //if (objObj) objObj.innerText = objMax - objVal.length;
	            if (objObj) objObj.innerHTML = objMax - objVal.length;
	            return true;
            }
            function createObject(objId) {
	            if (document.getElementById) return document.getElementById(objId);
	            else if (document.layers) return eval("document." + objId);
	            else if (document.all) return eval("document.all." + objId);
	            else return eval("document." + objId);
            }
            
            function insertAtCaret(areaId, text) {
                var txtarea = document.getElementById('ctl00_cphMain_' + areaId);
                if (!txtarea) return;
            
                var scrollPos = txtarea.scrollTop;
                var strPos = 0;
                var br = ((txtarea.selectionStart || txtarea.selectionStart == '0') ? "ff" : (document.selection ? "ie" : false));
                if (br == "ie") {
                    txtarea.focus();
                    var range = document.selection.createRange();
                    range.moveStart('character', -txtarea.value.length);
                    strPos = range.text.length;
                } else if (br == "ff") {
                    strPos = txtarea.selectionStart;
                }
            
                var front = (txtarea.value).substring(0, strPos);
                var back = (txtarea.value).substring(strPos, txtarea.value.length);
                txtarea.value = front + text + back;
                strPos = strPos + text.length;
                if (br == "ie") {
                    txtarea.focus();
                    var ieRange = document.selection.createRange();
                    ieRange.moveStart('character', -txtarea.value.length);
                    ieRange.moveStart('character', strPos);
                    ieRange.moveEnd('character', 0);
                    ieRange.select();
                } else if (br == "ff") {
                    txtarea.selectionStart = strPos;
                    txtarea.selectionEnd = strPos;
                    txtarea.focus();
                }
            
                txtarea.scrollTop = scrollPos;
            }



