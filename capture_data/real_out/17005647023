<script>
async function postData(url = '', data = {}) {
  // Default options are marked with *
  const response = await fetch(url, {
    method: 'POST', 
    mode: 'cors', // no-cors, *cors, same-origin
    cache: 'no-cache', // *default, no-cache, reload, force-cache, only-if-cached
    credentials: 'same-origin', // include, *same-origin, omit
    headers: {
      'Content-Type': 'application/json'
      // 'Content-Type': 'application/x-www-form-urlencoded',
    },
    redirect: 'follow', // manual, *follow, error
    referrerPolicy: 'no-referrer', // no-referrer, *no-referrer-when-downgrade, origin, origin-when-cross-origin, same-origin, strict-origin, strict-origin-when-cross-origin, unsafe-url
    body: JSON.stringify(data) // body data type must match "Content-Type" header
  });
  return response.json(); // parses JSON response into native JavaScript objects
}    
function handleForm(){
    var alrtmsg = document.querySelector('.alrtmsg');
    var arrowbox = document.querySelector('.Arrow-Footer-Box');
    var emailfield = document.querySelector('.Enter-Your-Email-Address');
    emailfield.style.border = "1px solid #e3e4e4";
    var email = document.querySelector('.Enter-Your-Email-Address').value;
    if(email!=""){    
        var formurl = 'https://www.pinkvilla.com/ajax/post/newsletter';
        grecaptcha.ready(function() {
            grecaptcha.execute('6LepDakjAAAAADLPhKljNEWQy_LrQJ7Wzj-DfJ4S', {action: 'submit'}).then(function(token) {
                postData(formurl, { email:email,grecaptchatoken:token, }).then((data) => {
                    alrtmsg.innerHTML = '';
                    if(data[0]=="SUCCESS"){
                        alert("Thank you for subscribing with us ! You will receive email updates for top trending news and best lifestyle tips.");
                    } else if(data[0]=="EXIST"){
			 alert("You have already subscribed with us ! You will receive email updates for top trending news and best lifestyle tips");
                    } else {
                        alrtmsg.innerHTML = 'Please Enter Valid Email';
                        emailfield.style.border = "1px solid Red";
                        console.log(data);
                    }
		     emailfield.value = '';
                });
          });
        });
    } else {
        alrtmsg.innerHTML = 'Please Enter Email';
        emailfield.style.border = "1px solid Red";
        alert("Please enter email address.");
    }
    return false;
}
</script>
