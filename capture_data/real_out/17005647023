  <script>
    document.addEventListener("DOMContentLoaded", function() {
      if (document.getElementsByClassName('darkmode_alink')[0]) {
        var darkmode_alink = document.getElementsByClassName('darkmode_alink')[0];
        darkmode_alink.addEventListener('click', () => {
          darkmode_alink.classList.toggle('active');
          alert("Do u want dark mode ? that logic code will here...");
        });
      }

      if (document.getElementsByClassName('loggedin_user_alink')[0]) {
        var loggedin_user_alink = document.getElementsByClassName('loggedin_user_alink')[0];
        var loggedin_users_popup = document.getElementsByClassName('loggedin_users_popup')[0];
        loggedin_user_alink.addEventListener('click', () => {
          if (!loggedin_users_popup.style.display || loggedin_users_popup.style.display == "none") {
            loggedin_users_popup.style.display = "block";
          } else {
            loggedin_users_popup.style.display = "none";
          }
          loggedin_user_alink.classList.toggle('active');
        });
        document.addEventListener('click', function(event) {
          var isClickInsideParent = loggedin_user_alink.contains(event.target);
          var isClickInside = loggedin_users_popup.contains(event.target);
          if (!isClickInsideParent && !isClickInside && loggedin_user_alink.classList.contains('active')) {
            loggedin_users_popup.style.display = "none";
            loggedin_user_alink.classList.toggle('active');
          }
        });
      }
    });
  </script>

