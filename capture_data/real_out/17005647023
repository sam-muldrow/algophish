  'main_category': 'NA'       
});


function triggerL1(eventName,pageType,pageUrl,clickText,clickURL,mainCategory,subCategory){
   dataLayer.push({
   'event':eventName,
   'page_type':pageType,
   'page_url':pageUrl,
   'click_text':clickText,
   'click_url':clickURL,
   'main_category':mainCategory,
   'sub_category':subCategory,
   }); 
}

