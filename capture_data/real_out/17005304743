window.isAuthedUser = localStorage.getItem('authedUser');
if ((window.isAuthedUser == true) || (window.isAuthedUser == 'true')) {
window.isAuthedUser = true;
document.getElementById('root').classList.add('shark');
} else {
window.isAuthedUser = false;
}
window.runDarkMode = function() {
window.useDarkMode = localStorage.getItem('useDarkMode');
if (window.useDarkMode === "off") {
window.useDarkMode = false;
window.labelDarkMode = 'off';
} else if (window.useDarkMode === "on") {
window.useDarkMode = true;
window.labelDarkMode = 'on';
} else {
window.labelDarkMode = 'auto';
if (window.matchMedia && window.matchMedia('(prefers-color-scheme: dark)').matches) {
window.useDarkMode = true;
} else {
window.useDarkMode = false;
}
}
var oRoot = document.getElementById('root');
oRoot.classList.remove('dark-mode-on', 'dark-mode-off', 'dark-mode-auto');
oRoot.classList.add('dark-mode-' + window.labelDarkMode);
if (window.useDarkMode) {
oRoot.classList.add('dark-mode');
} else {
oRoot.classList.remove('dark-mode');
}
};
window.runDarkMode();
window.nopagelevelad = true;
