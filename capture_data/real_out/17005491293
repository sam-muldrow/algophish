	<script type="snippy/no-run" id="snippy-init-cas-newsletter">
		var newsletter = new Newsletter({
			userIsAuthenticated: false,
			loginUrl: '/czytelnicy/logowanie',
			casDomain: 'https://konto.wpolityce.pl',
			formWithEmailCallback: function() {
				if (dataLayer) {
					dataLayer.push({
						'event': 'gaEvent',
						'hitType': 'event',
						'eventCategory': 'Zapis na newsletter',
						'eventAction': 'Niezalogowany, klik do rejestracji'
					})
					// don't remove it, because it makes default behavior working
					return true
				}
			},
			formWithoutEmailCallback: function() {
				if (dataLayer) {
					dataLayer.push({
						'event': 'gaEvent',
						'hitType': 'event',
						'eventCategory': 'Zapis na newsletter',
						'eventAction': 'Zalogowany, klik do zapisu'
					})
					// don't remove it, because it makes default behavior working
					return true
				}
			},
			utms: {
				utm_source: 'wpolityce',
				utm_medium: 'homepage_form',
				utm_campaign: 'newsletter_encourage'
			},
		});
		newsletter.init();
		if (true) {
			window.loginConfig.refreshedContentType = 'newsletterContent';
			window.loginConfig.newsletter = newsletter;
		}
	</script>

