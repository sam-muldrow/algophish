<script type="text/javascript" >
require ([
		'common/cjos/util/configUtil'
	], function (
			configUtil
		) {
		
		function initView() {
			var protocol = location.protocol;
			var infolwInfoCookieStr = Cookies.get('INFLOW_INFO') || '';
			
			if ( infolwInfoCookieStr === '' ) {
				return;
			}
			var partnerCd = infolwInfoCookieStr.split('|,^')[1];
			var iframeHeight = '0px';
			
			// https 지원되지 않는 외부 제휴 헤더 ssl 페이지에서 노출되지 않도록 처리
			if ( ( partnerCd == '2541' || partnerCd == '2429' || partnerCd == '2430' || partnerCd == '0934') && protocol === 'https:' ) {
				return;
			}
			
			try {
				switch ( partnerCd ) {
					case '2034':
						// 01. 신한카드
						iframeHeight = '110px';
						break;
					case '1993':
					case '2045':
					case '2677':
						// 02. 현대카드
						iframeHeight = '80px';
						break;
					case '1989':
					case '2044':
						// 03. 삼성카드
						iframeHeight = '115px';
						break;
					case '2541':
						// 04. NH카드
						iframeHeight = '95px';
						break;
					case '2429':
						// 05. KB국민카드
						iframeHeight = '130px';
						break;
					case '2430':
						// 06. 하나 SK카드
						iframeHeight = '148px';
						break;
					case '0934':
						// 07. 드림엑스
						iframeHeight = '84px';
						break;
					case '2056':
						// 08. 오케이캐쉬백몰
						iframeHeight = '92px';
						break;
					case '2038':
					case '2046':
						// 09. 아시아나
						iframeHeight = '55px';
						break;
					default:
						break;
				}
			}  catch ( e ) {
				console.log ( e );
			}
			
			if (iframeHeight === '0px') {
				return;	
			}
			
			var iframe = window.document.createElement ('iframe');
			iframe.width = '100%';
			iframe.height=iframeHeight;
			iframe.id = 'affiliateHeaderIframe';
			iframe.src = configUtil.get('ecBaseUrlNoProtocol') + '/p/advertise/inflow/affiliate/iframe';
			iframe.style = 'display:block; frameBorder="0"';
			iframe.scrolling = 'no';
			
			$("#affiliateHeader").append (iframe);
		}
		
		initView ();
	});
</script>
<script type="text/javascript" >
	requirejs ([
		'common/cjos/util/pageEntryUtil',
		'common/cjmall/ui/p/header/views/headerView',
		'common/cjmall/ui/p/footer/views/footerView'
	], function (
		pageEntryUtil,
		HeaderView,
		FooterView
	) {
		// 공통UI 생성
		var mainHeaderView = new HeaderView ({isActivatedTab: true, hmtabMenuId: '003147'}); // 홈탭에서만 GNB배너 노출하기 위해 hmtabMenuId 값 필요
		var footerView = new FooterView (); // 푸터

		// 등록된 엔트리모듈이 있는지 체크
		if (pageEntryUtil.has()) {
			// 엔트리 모듈용 데이터세팅
			var data = {
				initData: {
					hmtabMenuId : '003147',
					baseMenuYn: 'Y',
					preview: 'N',
					searchDay: '',
					mnpp: 'Y'
				}
			};
			// 엔트리모듈 호출
			pageEntryUtil.execute (data);
		}
	});


	var hometabId= "003147";

	if (GtmLib) {
		if (hometabId == "002409") {
 			GtmLib.prepare(["dimension56","dimension84"]);
 		} else {
 			GtmLib.prepare(["dimension56","dimension88", "dimension89"]);
 		}
	}
</script>
