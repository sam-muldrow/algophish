<script type="text/javascript">
	GtmLib.prepare();
	require ([
		'common/cjos/util/configUtil'
	], function (configUtil) {
			function setDefault() {
				GtmLib.setData("dimension3", null);
				GtmLib.setData("dimension6", null);
				GtmLib.setData("dimension7", null); // 성별
				GtmLib.setData("dimension8", null);
				GtmLib.setData("dimension9", null);
				GtmLib.setData("dimension10", "N");
				GtmLib.setData("dimension11", null);
				GtmLib.setData("dimension13", "N");
				GtmLib.setData("dimension35", "N");
			}
			$.ajax({ 
				url: configUtil.get ( 'ecUserAPIUrl' ) + '/v1/login/information',
				xhrFields: {withCredentials: true}
			}).done(function(data) {
				if(!!data.result) {
					GtmLib.setData("dimension3", data.result['accountCreationDate']);
					GtmLib.setData("dimension6", data.result['custNo']);
					GtmLib.setData("dimension7", (data.result['gender'] == null)?null:(data.result['gender'] == "MALE")?"M":"F"); // 성별
					GtmLib.setData("dimension8", data.result['age']);
					GtmLib.setData("dimension9", data.result['segCd']);
					GtmLib.setData("dimension10", (data.result['isAutoLogin'])?"Y":"N");
					GtmLib.setData("dimension11", (data.result['isCJOneMember'] == null)?null: (data.result['isCJOneMember'])?"Y":"N");
					GtmLib.setData("dimension13", (data.result['isEmployee'])?"Y":"N");
					GtmLib.setData("dimension35", (data.result['isLogin'])?"Y":"N");
				} else {
					setDefault();
				}		
			}).fail(function() {
				setDefault();
			});
		});
</script>
