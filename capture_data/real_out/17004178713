<script>
document.addEventListener('DOMContentLoaded', function () {
  var $catalogPurchases = $('#onliner-catalog-purchases'),
      activeTabClass = 'b-main-page-tabs__item_active';

  $catalogPurchases.on('click', '.b-main-page-tabs__item', function (event) {
    event.preventDefault();

    var item = event.target,
        tabId = item.getAttribute('data-tab');

    $('.b-best-layer', $catalogPurchases).load('catalogpurchases/tabs/' + tabId, {method: 'POST'}, function () {
      $('.b-main-page-tabs__item', $catalogPurchases).removeClass(activeTabClass);
      $(item).addClass(activeTabClass);
    });
  });
});
</script>
