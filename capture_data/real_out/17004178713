<script>
    document.addEventListener('DOMContentLoaded', function () {
        var $belarusLayer = $('#onliner-belarus-layer'),
            activeTabClass = 'b-main-page-tabs__item_active';

        $belarusLayer.on('click', '.b-main-page-tabs__item', function (event) {
            event.preventDefault();

            var item = event.target,
                tabId = item.getAttribute('data-tab');

            $('.b-best-layer', $belarusLayer).load('belaruslayer/tabs/' + tabId, {method: 'POST'}, function () {
                $('.b-main-page-tabs__item', $belarusLayer).removeClass(activeTabClass);
                $(item).addClass(activeTabClass);
            });
        });
    });
</script>
