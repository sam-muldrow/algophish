<script>
document.addEventListener('DOMContentLoaded', function () {
  var $bestLayer = $('#onliner-best-layer'),
      activeTabClass = 'b-main-page-tabs__item_active';

  $bestLayer.on('click', '.b-main-page-tabs__item', function (event) {
    event.preventDefault();

    var item = event.target,
        tabId = item.getAttribute('data-tab');

    $('.b-arena-layer', $bestLayer).load('bestlayer/tabs/' + tabId, {method: 'POST'}, function () {
      $('.b-main-page-tabs__item', $bestLayer).removeClass(activeTabClass);
      $(item).addClass(activeTabClass);
    });
  });
});
</script>
