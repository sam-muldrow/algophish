<script>
            // Trendy Slider
            let trandyList = document.getElementById("trendyList");
            function tListToggle(pList) {
                var tListLength = pList.children.length;
                var i = 0;
                var prev = tListLength - 1;

                (function move() {

                    pList.children[prev].classList.remove('active');
                    pList.children[i].classList.add('active');

                    prev = i;
                    i++;
                    if (i === tListLength) { i = 0; }
                    setTimeout(move, 4000);
                })();
            }
            tListToggle(trandyList);

            // Search Open & Close
            let searchOpen = document.getElementById("searchOpen");
            let searchClose = document.getElementById("searchClose");
            let searchBox = document.getElementById("PnlSearch");
            searchOpen.onclick = function () {
                searchBox.classList.add('open');
            };
            searchClose.onclick = function () {
                searchBox.classList.remove('open');
            };


            const toggleMode = document.getElementById('toggleMode');
            const body = document.getElementById("PageBody");

            //// alternative to DOMContentLoaded
            //document.onreadystatechange = function () {
            //    if (document.readyState == "interactive") {
            //        CheckCookie();
            //    }
            //}

            function CheckCookie() {
               
                let Nmode = getCookie("nightmode");
                if (Nmode != "") {
                    if (Nmode == "ON") {
                       //body.classList.add('night-mode');
                        toggleMode.checked = true;
                    } else {
                       // body.classList.remove('night-mode');
                        toggleMode.checked = false;
                    }
                   
                }
            }


            function setCookie(cname, cvalue, exdays) {
                const d = new Date();
                d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
                let expires = "expires=" + d.toUTCString();
                document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
            }

            function getCookie(cname) {
                let name = cname + "=";
                let ca = document.cookie.split(';');
                for (let i = 0; i < ca.length; i++) {
                    let c = ca[i];
                    while (c.charAt(0) == ' ') {
                        c = c.substring(1);
                    }
                    if (c.indexOf(name) == 0) {
                        return c.substring(name.length, c.length);
                    }
                }
                return "";
            }


            // Night mode toggle

            function modeBtnToggle(modeBox) {
                
                if (modeBox.checked) {
                    body.classList.add('night-mode');
                    document.getElementById("logo24").setAttribute('xlink:href', '/assets/imgs/sprite.svg#24studies-dsection');
                 
                    setCookie("nightmode", "ON", 365);
                } else {
                    body.classList.remove('night-mode');
                    document.getElementById("logo24").setAttribute('xlink:href', '/assets/imgs/sprite.svg#24studies-section');
                  
                    setCookie("nightmode", "Off", 365);
                }
            }
            CheckCookie();
            modeBtnToggle(toggleMode);
            toggleMode.addEventListener('change', (event) => {
                modeBtnToggle(event.currentTarget);
            });

            
        </script>
