        //}
        //else {
        //    return true;
        //}
    }

    function istrackconsignmentPostback() {
        document.getElementById("ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_hdnIsTrackConsignmentPostback").value == 'true';
        var keyid = "/_layouts/15/DOP.Portal.Tracking/TrackConsignment.aspx";
        document.getElementById("ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_key_id").value = keyid


    }
    function hdnIsTrackConsignmentId() {
        return document.getElementById("ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_hdnIsTrackConsignmentPostback").value;
    }
   



</script>

