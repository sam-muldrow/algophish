<script>

    function TrackConsign() {

        var consignNum = document.getElementById('ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_txtTrackConsign').value;

        if (consignNum == "Enter consignment/Ref number" || consignNum == "Enter consignment/Ref number") alert("Please " + consignNum);
        if (consignNum != '') {
            if (consignNum !== "Consignment Number") {
                var trackConsignPage = "/VAS/Pages/trackconsignment.aspx" + "?t=" + consignNum;
                window.location.href = trackConsignPage;
            }
        }
    }

    function txtCaptchaKeyPress(evnt) {
        if (evnt.keyCode == 13) {
            document.getElementById('ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_imgSearch1').click();
            return false;
        }
    }
