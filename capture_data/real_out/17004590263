$(document).ready(function () {

    $("div[id*='divMathCaptcha']").attr("tabindex", "0");
    $("div[id*='divimgcaptcha']").attr("tabindex", "0");
    $("input[id*='imgbtnCaptcha']").attr("tabindex", "0");
    $("input[id*='txtCaptcha']").attr("tabindex", "0");

    if ($("#ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_hdnIsPostBack").val() == 'true') {
        $("#ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_lblTrackConsignment").focus();
        $("#ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_hdnIsPostBack").val('false')
    }
    //  document.getElementById('SearchTextBoxPeople').focus();
    // document.getElementById('SkipIcon').focus();
    var ua = window.navigator.userAgent;

    var msie = ua.indexOf('MSIE ');
    if (msie > 0) {
        // IE 10 or older => return version number
        //alert(parseInt(ua.substring(msie + 5, ua.indexOf('.', msie)), 10))
        var ieversion = parseInt(ua.substring(msie + 5, ua.indexOf('.', msie)), 10);
        if (ieversion >= 8) {
            var consignNum = document.getElementById('ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_txtTrackConsign');
            //consignNum.style.width = "80%";
            var concomplaintNum = document.getElementById('ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_txtTrackComplaint');
            //concomplaintNum.style.width = "80%";

        }

        if (ieversion == 10) {
            try {
                $("#ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_txtTrackComplaint").removeAttr('maxlength');
                $("#ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_txtTrackComplaint").removeAttr("keypress");
                $("#ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_txtTrackComplaint").keypress(function (e) {
                    var MaxLength = 12;
                    var flag = false;
                    if ($(this).val().length >= MaxLength) {
                        flag = false;
                    }
                    else {
                        flag = IsValidComplaintNumber(e);
                    }
                    return flag;
                });
            }
            catch (err)
            { }
        }
    }


    var edge = ua.indexOf('Edge/');
    if (edge > 0) {
        // Edge (IE 12+) => return version number
        //alert(parseInt(ua.substring(edge + 5, ua.indexOf('.', edge)), 10))
        // return parseInt(ua.substring(edge + 5, ua.indexOf('.', edge)), 10);
    }
});

function TrackingMessageFocusComplaint(x) {
    if (x.value == 'Enter complaint number' || x.value == 'शिकायत नंबर दर्ज करें') { x.value = ''; x.style.color = 'black'; }
    //  document.getElementById('ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_rbTrackConsignment').checked = false;
    //  document.getElementById('ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_rbTrackComplaint').checked = true;
}



    function TrackingMessageBlurComplaint(x) {
        var value; if (lcidValuePortal != undefined) { value = lcidValuePortal; } else { value = _spPageContextInfo.currentLanguage; }
        if (x.value == '') {
            if (value == 1033) {
                x.value = 'Enter complaint number'; x.style.color = 'grey';
            }
            if (value == 1081) {
                x.value = 'शिकायत नंबर दर्ज करें'; x.style.color = 'grey';
            }
        }
        if (x.value != '' && x.value != 'Enter complaint number' && x.value != 'शिकायत नंबर दर्ज करें') {
            $("#ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_txtTrackComplaint").val($("#ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_txtTrackComplaint").val().replace(new RegExp(" ", "g"), ""));
        }
    }

    function TrackingMessageFocus(x) {
        if (x.value == 'Enter consignment number' || x.value == 'Enter Ref number' || x.value == 'Enter Ref number' | x.value == 'रेफ नंबर दर्ज करें' || x.value == 'कन्साइनमेंट संख्या दर्ज करें' || x.value == 'रेफ नंबर दर्ज करें') {
            x.value = ''; x.style.color = 'black';
        }
        //     document.getElementById('ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_rbTrackConsignment').checked = true;
        //      document.getElementById('ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_rbTrackComplaint').checked = false;
    }

    

    function TrackingMessageBlur(x) {
        var rbTrackConsignment = document.getElementById('ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_rbTrackConsignment');
        var value; if (lcidValuePortal != undefined) { value = lcidValuePortal; } else { value = _spPageContextInfo.currentLanguage; }
        if (x.value == '') {
            if (value == 1033) {
                if (rbTrackConsignment.checked) {
                x.value = 'Enter consignment number'; x.style.color = 'grey';
            }
                else {

                    x.value = 'Enter' + '\xa0' + 'Ref' + '\xa0' + 'number'; x.style.color = 'grey';
                }
            }
            if (value == 1081) {
                if (rbTrackConsignment.checked) {
                    x.value = 'कन्साइनमेंट संख्या दर्ज करें'; x.style.color = 'grey';
                }
                else {

                    x.value = 'रेफ' + '\xa0' + 'नंबर' + '\xa0' + 'दर्ज' + '\xa0' + 'करें'; x.style.color = 'grey';
                }

            }
        }
        if (x.value != '' && x.value != 'Enter Ref number' && x.value != 'Enter Ref number' && x.value != 'रेफ नंबर दर्ज करें' && x.value != 'रेफ नंबर दर्ज करें' && x.value != 'Enter consignment number' && x.value != 'कन्साइनमेंट संख्या दर्ज करें') {
            $("#ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_txtTrackConsign").val($("#ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_txtTrackConsign").val().replace(new RegExp(" ", "g"), ""));
        }
    }






