    Sys.WebForms.PageRequestManager.getInstance().add_pageLoaded(function (evt, args)
    {
   
    var rbTrackConsignment = document.getElementById('ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_rbTrackConsignment');
    var rbTrackComplaint = document.getElementById('ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_rbTrackComplaint');
    var rbTrackAWB = document.getElementById('ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_rbTrackAWB');

    if (rbTrackConsignment.checked || rbTrackAWB.checked) {

        document.getElementById('consignment').style.display = 'block';
        document.getElementById('complaint').style.display = 'none';
        document.getElementById('ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_txtTrackConsign').focus();
    }
    else if (rbTrackComplaint.checked) {

        document.getElementById('consignment').style.display = 'none';
        document.getElementById('complaint').style.display = 'block';
        document.getElementById('ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_txtTrackComplaint').focus();

    }

