          <script>
            var lcidValuePortal = null;
            try {
                $(document).ready(function () {
                    $('.gotop').hide();
                    // Show or hide the sticky footer button
                    $(window).scroll(function () {
                        if ($(this).scrollTop() > 50) {
                            //$('.gotop').fadeIn(500);
                            $('.gotop').show();
                        }
                        else {
                            //$('.gotop').fadeOut(300);
                            $('.gotop').hide();
                        }
                    });

                    // Animate the scroll to top
                    $('.gotop').click(function (event) {
                        event.preventDefault();

                        $('html, body').animate({ scrollTop: 0 }, 300);
                    });

                    if (_spPageContextInfo != null || _spPageContextInfo != 'undefined') {
                        lcidValuePortal = _spPageContextInfo.currentLanguage;
                    }
                });
            } catch (err) {
            }
        </script>

        <script>

            $(document).ready(function () {
                $(".dropdown").hover(
                    function () {
                        $('.dropdown-menu', this).fadeIn("fast");
                    },
                    function () {
                        $('.dropdown-menu', this).fadeOut("fast");
                    });
            });

            Sys.WebForms.PageRequestManager.getInstance().add_pageLoaded(function (evt, args) {
                var app = Sys.Application;
                var origTitle = "";
                app.add_init(SPCustomAppnInit);

                function SPCustomAppnInit(sender) {
                    origTitle = document.title; // grab the original title.
                    var prm = Sys.WebForms.PageRequestManager.getInstance();
                    if (!prm.get_isInAsyncPostBack()) {
                        prm.add_pageLoaded(SPCustomPageLoaded); // wire up loaded handler.
                    }
                }

                function SPCustomPageLoaded(sender, args) {
                    document.title = origTitle; // put the original title back on the document.
                }
            });
        </script>

        <script>
            function SearchResults() {
                var searchText = document.getElementById("SearchTextBoxPeople").value;
                document.location.href = "/sites/Search/Pages/results.aspx?k=" + searchText;
            }

            function myFunctionKeySearch(event) {
                var x = event.which || event.keyCode;
                if (x == 13) {
                    var searchText = document.getElementById("SearchTextBoxPeople").value;
                    document.location.href = "/sites/Search/Pages/results.aspx?k=" + searchText;
                    window.event.returnValue = false;
                }
            }
        </script>
