<script>
   
    function Category_Select(x) {
        try {
            if (x.id.match('Consign') || x.id.match('AWB')) {
                if (x.checked) {

                    TrackingMessageFocus(x);
                    document.getElementById('consignment').style.display = 'block';
                    document.getElementById('complaint').style.display = 'none';
                    if (x.id.match('Consign')) {

                        document.getElementById('ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_txtTrackConsign').value = document.getElementById('ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_lblConsignmentNumber').innerText;
                        document.getElementById('ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_txtTrackConsign').title = document.getElementById('ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_lblConsignmentNumber').innerText;
                    }
                    else {

                        document.getElementById('ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_txtTrackConsign').value = document.getElementById('ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_lblRefNumber').innerText;
                        document.getElementById('ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_txtTrackConsign').title = document.getElementById('ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_lblRefNumber').innerText;

                    }
                    document.getElementById('ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_txtTrackConsign').focus();

                }
                else {
                    TrackingMessageBlur(x);
                }
            }
            else {
                if (x.id.match('Complaint')) {
                if (x.checked) {
                    TrackingMessageFocusComplaint(x);


                    document.getElementById('consignment').style.display = 'none';
                    document.getElementById('complaint').style.display = 'block';
                    document.getElementById('ctl00_SPWebPartManager1_g_aa197fec_b38c_41a8_b14e_a11722636b37_ctl00_txtTrackComplaint').focus();

                }
                else {
                    TrackingMessageBlurComplaint(x);
                }
            }
            }







    } catch (err) {
    }
}




