      function removePictureTag(imgElt) {
        try {
          if (imgElt.parentNode.tagName === 'PICTURE' && imgElt.currentSrc.indexOf('base64') === -1) {
            imgElt.setAttribute('src', imgElt.currentSrc);
            imgElt.parentNode.replaceWith(imgElt);
            if (imgElt.getAttribute('onload') === 'removePictureTag(this)') {
              imgElt.removeAttribute('onload');
            }
          }
        } catch(err) {}
      }
      document.addEventListener('lazyloaded', function(e){
        if (e.target.getAttribute('onload') === 'removePictureTag(this)') {
          removePictureTag(e.target);
        }
      });
