				var next = $(this).next();
				if (next.length == 0){
				next = $(obj).children(':first');
							}
                                $(obj).rotaterator({child : next, fadeSpeed : o.fadeSpeed, pauseSpeed : o.pauseSpeed});
						})
					});
            });
        }
    });
})(jQuery);


jQuery( document ).ready(function( $ ) {


$(window).scroll(function(){
    if (($(window).scrollTop() >= 200 ) && ($(window).innerWidth() >= 768)) {
        $('.banner ').addClass('fixed-header');
    }
    else {
        $('.banner').removeClass('fixed-header');
    }
});


$('.navigation  a[href*=#]').click(function(){
  $('html, body').animate({
    scrollTop: $( $.attr(this, 'href') ).offset().top
  }, 500);
  return false;
});

    $('#testimonial-rotator .fixed-width .one-column').rotaterator({fadeSpeed:1500, pauseSpeed:6100});

   	
    $( ".instant-access" ).wrapAll( "<div class='instant-access-wrap' ></div>");
  
    $(".step-nav ul li:first-child").addClass("current");
    $(".step-nav ul li a").click(function(event) {
        event.preventDefault();
        $(this).parent().addClass("current");
        $(this).parent().siblings().removeClass("current");
        var tab = $(this).attr("href");
        $(".step-content").not(tab).css("display", "none");
        $(tab).fadeIn();
    });
});




