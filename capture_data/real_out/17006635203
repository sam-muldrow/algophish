    <script type="text/javascript" async>
        var COOKIE_POLICY = true;
        document.addEventListener("DOMContentLoaded", function() {             var el = document.getElementById('cookie-policy');
            COOKIE_POLICY = el.offsetHeight;
            document.getElementsByTagName('html')[0].classList.add('has-cookie-policy');
            document.getElementById('cookie-policy-button').addEventListener('click', function (e) {
                e.preventDefault();
                e.stopPropagation();
                el.remove();
                document.getElementsByTagName('html')[0].classList.remove('has-cookie-policy');
                var xhr = new XMLHttpRequest();
                xhr.open('GET', '/cookiePolicy');
                xhr.send();
            });
        });
    </script>
