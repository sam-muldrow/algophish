    </div>
    <script type="text/javascript">
                window.addEventListener('DOMContentLoaded', function () {
          const f = function () {
            let h = 0
            if (window.visualViewport) {
              h = window.visualViewport.height
            } else {
              h = window.innerHeight
            }
            document.documentElement.style.setProperty('--viewport-height', `${h}px`)
          }
          f()
          window.visualViewport.addEventListener('resize', f, {passive: true});

          let loginForm = {
            modal: $('#login-modal'),
            modalCode: $('#phone-code'),
            tabs: $('#login-modal .tabs'),
            tab: $('#login-modal .tabs li'),
            tabContent: $('#login-modal .tab-content'),
            phone: {
              fields: {
                phone: $('#login-modal #phone_comp'), // composite
                phone_country: $('#login-modal #countryCode'),
                phone_number: $('#login-modal #phone'),
                code: $('#phone-code #phone_codes'), // composite
              },
              buttons: {
                phoneCode: $('#login-modal #phone-get-code'),
                phoneLogin: $('#phone-code #phone-login'),
                resendCode: $('#phone-code .resendCode')
              },
              resendTimeout: 60, // seconds
              value: '',
            }
          }
          loginForm.init = function () {
            loginForm.initTabs()
            loginForm.getPhoneCode()
            loginForm.loginWithPhone()
            loginForm.resendPhoneCode()
          }
          loginForm.initTabs = function () {
            loginForm.tab.click(function (e) {
              loginForm.tab.removeClass('active')
              loginForm.tabContent.addClass('hide')
              $(e.currentTarget).addClass('active')
              loginForm.modal.find(`#${$(e.currentTarget).data('tab')}`).removeClass('hide')
            })
          }
          loginForm.req = function ($btn, url, data, success, error) {
            $.ajax({
              type: 'POST',
              url: url,
              data: JSON.stringify(data),
              dataType: 'json',
              contentType: 'application/json',
              beforeSend: function () {
                $btn.prop('disabled', true)
              },
              complete: function () {
                $btn.prop('disabled', false)
              },
              error: error,
              success: success
            })
          }
          loginForm.getPhoneCode = function () {
            loginForm.phone.buttons.phoneCode.click(function (e) {
              loginForm.phone.fields.phone.removeClass('error')
              loginForm.phone.fields.phone.next('.error_container').remove()
              const phone = loginForm.phone.fields.phone.find('input').map(function (i, el) {
                return $(el).val()
              }).get().join('')
              loginForm.req(
                $(e.currentTarget),
                '/api/smsLogin/init',
                {phone: phone},
                function (data) {
                  loginForm.phone.value = data.items.phone
                  loginForm.modalCode.find('[data-role=description]').html(loginForm.modalCode.find('[data-role=description]').html().replace('{phone}', loginForm.phone.value))
                  MicroModal.show('phone-code', {disableScroll: true})
                  loginForm.resendTimer()
                },
                function () {
                  loginForm.phone.fields.phone.addClass('error').after('<div class="error_container">Nieznany telefon</div>')
                }
              )
            })
            loginForm.phone.fields.phone_country.focus(function () {
              loginForm.phone.fields.phone.addClass('focus')
            })
            loginForm.phone.fields.phone_number.focus(function () {
              loginForm.phone.fields.phone.addClass('focus')
            })
            loginForm.phone.fields.phone_country.blur(function () {
              loginForm.phone.fields.phone.removeClass('focus')
            })
            loginForm.phone.fields.phone_number.blur(function () {
              loginForm.phone.fields.phone.removeClass('focus')
            })
            loginForm.phone.fields.phone_country.keyup(function (e) {
              e.target.value = e.target.value.replace(/[^\d\+]/g, '')
              if (e.target.value.replace('+', '').length >= 3) {
                e.target.value = e.target.value.substring(0, 1) == '+' ? e.target.value.substring(0, 4) : e.target.value.substring(0, 3)
              }
              if (['370', '371', '380', '386', '421', '40', '44', '48'].indexOf(e.target.value.replace('+', '')) !== -1) {
                loginForm.phone.fields.phone_number.focus()
              }
            })
            loginForm.phone.fields.phone_number.keyup(function (e) {
              e.target.value = e.target.value.replace(/[^\d]/g, '')
              if (e.target.value.length > 13) {
                e.target.value = e.target.value.substring(0, 13)
              }
              if (e.target.value == '' && [8, 46].indexOf(e.keyCode) !== -1) {
                loginForm.phone.fields.phone_country.focus()

                const el = loginForm.phone.fields.phone_country.get(0)
                if (typeof el.selectionStart == "number") {
                  el.selectionStart = el.selectionEnd = el.value.length
                } else if (typeof el.createTextRange != "undefined") {
                  var range = el.createTextRange()
                  range.collapse(false)
                  range.select()
                }
              }
              loginForm.phone.buttons.phoneCode.prop('disabled', false)
            })
          }
          loginForm.loginWithPhone = function () {
            loginForm.phone.buttons.phoneLogin.click(function (e) {
              loginForm.phone.fields.code.removeClass('.error')
              loginForm.phone.fields.code.next('.error_container').remove()
              const code = loginForm.phone.fields.code.find('input').map(function (i, el) {
                return $(el).val()
              }).get().join('')
              const phone = loginForm.phone.value
              loginForm.req(
                $(e.currentTarget),
                '/api/smsLogin/login',
                {phone: phone, code: code},
                function () {
                  window.location.href = '/home'
                },
                function () {
                  loginForm.phone.fields.code.addClass('error').after('<div class="error_container">B≈Çƒôdny kod</div>')
                }
              )
            })
            loginForm.phone.fields.code.find('input').each(function (i, el) {
              $(el).on('input', function (e) {
                const value = e.target.value
                if (isNaN(parseInt(value)) === false && value.length == 5) {
                  const parts = value.split('')
                  loginForm.phone.fields.code.find('input').each(function (i, el) {
                    $(el).val('').val(parts[i]).blur()
                  })
                  loginForm.phone.buttons.phoneLogin.prop('disabled', false)
                }
              })
              $(el).keyup(function (e) {
                if (isNaN(parseInt(e.target.value))) {
                  e.target.value = ''
                }
                if (e.target.value.length >= 1) {
                  if ($(el).next('input').length) {
                    $(el).next('input').focus()
                  }

                  e.target.value = e.target.value.substring(0, 1)
                }
                if (e.target.value == '' && [8, 46].indexOf(e.keyCode) !== -1) {
                  if ($(el).prev('input').length) {
                    $(el).prev('input').focus()
                  }
                }
                let fullCode = loginForm.phone.fields.code.find('input').map(function (i, el) {
                  return $(el).val()
                }).get().join('')
                if (fullCode.length == 5) {
                  loginForm.phone.buttons.phoneLogin.prop('disabled', false)
                } else {
                  loginForm.phone.buttons.phoneLogin.prop('disabled', true)
                }
              })
            })
          }
          loginForm.resendPhoneCode = function () {
            let canResend = true
            loginForm.phone.buttons.resendCode.click(function (e) {
              if (!canResend) return
              const phone = loginForm.phone.value
              loginForm.req(
                $(e.currentTarget),
                '/api/smsLogin/init',
                {phone: phone},
                function () {
                  loginForm.resendTimer()
                },
                function (err) {
                  console.log('error', err)
                }
              )
            })
          }
          loginForm.resendTimer = function () {
            let timer = loginForm.phone.resendTimeout
            canResend = false
            loginForm.phone.buttons.resendCode.html('Popro≈õ o inny kod po {time}'.replace('{time}', `${timer}s`))
            loginForm.phone.buttons.resendCode.attr('disabled', true)
            const resendInverval = setInterval(function () {
              if (timer == 0) {
                loginForm.phone.buttons.resendCode.html('<a>Wy≈õlij ponownie </a>')
                loginForm.phone.buttons.resendCode.attr('disabled', false)
                clearInterval(resendInverval)
                canResend = true
              } else {
                timer -= 1
                loginForm.phone.buttons.resendCode.html('Popro≈õ o inny kod po {time}'.replace('{time}', `${timer}s`))
              }
            }, 1000)
          }
          loginForm.init()
          $('.button.is-login').click(function () { MicroModal.show('login-modal', {disableScroll: true}) });
          $('#login-modal form').submit(function () {
            var u = $('#username'), p = $('#password');
            u.removeClass('error'); p.removeClass('error');
            if ($.trim(u.val()) == '') u.addClass('error');
            if ($.trim(p.val()) == '') p.addClass('error');
            return !(u.hasClass('error') || p.hasClass('error'));
          });
          if (window.location.hash == '#login') MicroModal.show('login-modal', {disableScroll: true});
        })            </script>
    
