iers articles de ParisMatch.be","source":"rss-feed","rssUrl":"https://www.parismatch.be/arc/outboundfeeds/rss/?outputType=xml","pbInternal_cloneId":"f0fQHiy3NMII1Yn","plusText":"Vers ParisMatch.be","plusURL":"https://parismatch.be/","hideImage":true},"displayProperties":{},"localEdits":{},"variants":{}}},{"collection":"features","type":"breaking-news/default","props":{"collection":"features","type":"breaking-news/default","id":"f0fmp6XoqCR53mw","contentConfig":{"contentService":"","contentConfigValues":{},"inherit":true},"customFields":{"visibility":"Visible","containerDecoration":"title + bord","title":"Dernières dépêches","count":4,"plusURL":"/dernieres-depeches/","plusText":"Vers les dernières dépêches","hideOnWebApp":false,"hideOnMobileApp":false},"displayProperties":{},"localEdits":{},"variants":{}}}]},{"collection":"features","type":"jobat/default","props":{"collection":"features","type":"jobat/default","id":"f0f4YBIXrCR53CB","contentConfig":{"contentService":"","contentConfigValues":{},"inherit":true},"customFields":{"visibility":"Visible","hideOnMobileApp":true},"displayProperties":{},"localEdits":{},"variants":{}}},{"collection":"features","type":"adContainer/default","props":{"collection":"features","type":"adContainer/default","id":"f0f8Wq30sCR53Bw","contentConfig":{"contentService":"","contentConfigValues":{},"inherit":true},"customFields":{"adFormat":"leaderboard","pbInternal_cloneId":"f0fCWoFliIIf5Hp"},"displayProperties":{},"localEdits":{},"variants":{}}},{"collection":"features","type":"proxistore-widget/default","props":{"collection":"features","type":"proxistore-widget/default","id":"f0fkxKr2sCR53Ux","contentConfig":{"contentService":"","contentConfigValues":{},"inherit":true},"customFields":{},"displayProperties":{},"localEdits":{},"variants":{}}}]},{"collection":"sections","props":{"collection":"sections","id":2},"children":[{"collection":"features","type":"footer/default","props":{"collection":"features","type":"footer/default","id":"f0fQsc2msCR