    var PREBID_USD_RATE = "0.65067";
    var PREBID_EUR_RATE = "0.59848";


        window.prebidAdSlots = adSlots;

        function calculatePfFromBidResponse(bidResponse) {
            var cpm = bidResponse.cpm * PREBID_EUR_RATE;
            var maxCpm = 80.00;

            if (cpm < 5.00) {
                return (Math.floor(cpm * 5) / 5).toFixed(2);
            } else if (cpm < 10.00) {
                return (Math.floor(cpm * 2) / 2).toFixed(2);
            } else if (cpm < 20.00) {
                return (Math.floor(cpm * 1) / 1).toFixed(2);
            } else if (cpm < maxCpm) {
                return (Math.floor(cpm * 0.2) / 0.2).toFixed(2);
            } else {
                return (maxCpm).toFixed(2);
            }
            return cpm;
        };

        function calculateCpmFromBidResponse(bidResponse) {
            var cpm = bidResponse.cpm;
            var maxCpm = 80.00;

            if (cpm < 10.00) {
                return (Math.floor(cpm * 100) / 100).toFixed(2);
            } else if (cpm < 40.00) {
                return (Math.floor(cpm * 5) / 5).toFixed(2);
            } else if (cpm < maxCpm) {
                return (Math.floor(cpm * 2) / 2).toFixed(2);
            } else {
                return (maxCpm).toFixed(2);
            }
            return cpm;
        };

        function getDemandFromBidResponse(bidResponse) {
            var demandSource = '';
            if (bidResponse && bidResponse.adId) {
                var demandPattern = /(adsrvr\.org)|(turn\.com)|(quantcast)|(mathtag)|(pr\.ybp\.yahoo\.com)|(doubleclick\.net)|(gwallet\.com)|(criteo)|(adnxs\.com)/i;
                try {
                    var demandFrom = demandPattern.exec(bidResponse.ad);
                    if (demandFrom) {
                        demandSource = demandFrom[0];
                    } else {
                        demandSource = 'other';
                    }
                } catch (e) {
                    demandSource = 'error';
                }
            }
            return demandSource;
        };

        function getBidTimeFromBidResponse(bidResponse) {

            if (!bidResponse) return '';

            var bidTime = bidResponse.timeToRespond;
            var timeIncrement = 100;

            if (bidTime > 0)   {
                bidTime = Math.ceil(bidTime / timeIncrement) * timeIncrement;
            }

            return bidTime;
        };

        window.prebidResponses = {};

        var prebidPartnersConfig = JSON.parse('{"criteo":{"[728,90]":{"networkId":"3120"},"[300,250]":{"networkId":"3120"},"[810,140]":{"networkId":"3120"},"[300,600]":{"networkId":"3120"},"[955,70]":{"networkId":"3120"},"[320,50]":{"networkId":"3120"},"[160,600]":{"networkId":"3120"},"[320,60]":{"networkId":"3120"},"[970,250]":{"networkId":"3120"}},"appnexus":{"[728,90]":{"placementId":"16487773"},"[300,250]":{"placementId":"16487772"},"[300,600]":{"placementId":"16487775"},"[320,50]":{"placementId":"16487774"},"[160,600]":{"placementId":"16487776"},"[970,250]":{"placementId":"16487771"}},"pubmatic":{"[728,90]":{"publisherId":"42651","adSlot":"1476612@728x90"},"[300,250]":{"publisherId":"42651","adSlot":"1476610@300x250"},"[300,600]":{"publisherId":"42651","adSlot":"1476611@300x600"},"[320,50]":{"publisherId":"42651","adSlot":"1476619@320x50"},"[160,600]":{"publisherId":"42651","adSlot":"1476609@160x600"},"[970,250]":{"publisherId":"42651","adSlot":"1476613@970x250"}},"rubicon":{"[728,90]":{"accountId":"11340","siteId":"114598","zoneId":"539304"},"[300,250]":{"accountId":"11340","siteId":"114598","zoneId":"539304"},"[300,600]":{"accountId":"11340","zoneId":"539304","siteId":"114598"},"[320,50]":{"accountId":"11340","zoneId":"539308","siteId":"114600"},"[160,600]":{"accountId":"11340","zoneId":"539304","siteId":"114598"},"[970,250]":{"accountId":"11340","zoneId":"542288","siteId":"114598"}},"ix":{"[728,90]":{"siteId":"210275","sizeAsText":"[728,90]"},"[300,250]":{"siteId":"210274","sizeAsText":"[300,250]"},"[300,600]":{"siteId":"213720","sizeAsText":"[300,600]"},"[160,600]":{"siteId":"210277","sizeAsText":"[160,600]"},"[320,50]":{"siteId":"210276","sizeAsText":"[320,50]"},"[970,250]":{"siteId":"209592","sizeAsText":"[970,250]"}}}');

