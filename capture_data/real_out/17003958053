    <script type="text/javascript">
        (function () {
            var img = document.getElementById('random-hero-img');
            if (img.complete) {
                // onload cannot detect the image load from memory cache
                img.classList.remove('header__home-hero-hidden');
            } else {
                img.onload = function () {
                    img.classList.remove('header__home-hero-hidden');
                };
            }
        }());
    </script>

