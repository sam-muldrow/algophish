            <script>
                    function sendIDLEnvelope() {
                        if (window.ats) {
                            window.ats.start({
                                placementID: 13354,
                                storageType: "localStorage",
                                detectionType: "scrape",
                                cssSelectors: [],
                                detectionSubject: "all",
                                logging: "error",
                                emailHashes: [window.dataLayer[0].u.ehue]
                            });
                            console.log('create an IDL envelope in LocalStorage');
                        }
                    }
                    if (
                        !localStorage.getItem('idl_env') &&
                        window.dataLayer[0].u &&
                        window.dataLayer[0].u.ehue
                    ) {
                        try {
                            var atsScript = document.createElement('script');
                            atsScript.onload = function() {
                                sendIDLEnvelope();
                            };
                            atsScript.async = true;
                            atsScript.src = 'https://ats.rlcdn.com/ats.js';
                            document.body.appendChild(atsScript);
                        } catch {
                            console.log('The ATS script has not been loaded');
                        }
                    }
                </script>
            <link rel="prefetch" href="https://www.google.com/adsense/search/ads.js">
