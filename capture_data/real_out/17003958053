                        }
                    },v_appnexus: {
                        bidCpmAdjustment : function(bidCpm, bid){
                            var adjustBidCpm = bidCpm * (1/PREBID_USD_RATE) * (1);
                            var mediaType = bid.mediaType;
                            if (mediaType === 'video')  {
                                adjustBidCpm *= 0.33;
                            }
                            return adjustBidCpm;
                        }
                    },rubicon: {
