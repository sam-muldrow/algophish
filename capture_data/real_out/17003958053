<script type="text/html" id='tab-content'>
    <div class="tabbed__tab-items c-clearfix">
    <@
    var i, item,
        galleryUpsell = false,
        hasMrecAd = true;
    /*
     * Homepage Gallery upsell - make it as first block
     */
    if (tab.name === 'gumtreegallery' && galleryUpsell) {
    @>

        <div class="homepage-gallery__block homepage-gallery__block--initial-state homepage-gallery__upsell gallery-listing">
