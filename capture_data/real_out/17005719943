      <script type="text/javascript">
    "use strict";
(() => {
  // apps/wiwo-text-length/src/lib/piano_adjust_pw.ts
  function getServiceUri() {
    const shortUri = location.href.match(/\/\d*.html.*$/);
    return null === shortUri ? location.href : location.protocol + "//" + location.host + shortUri[0];
  }
  var isStaging = document.location.host.match(/e2e-(.*)\.wiwo\.de/);
  function adjustDomainIfNeeded(checkoutLink) {
    if (!isStaging) {
      return checkoutLink;
    } else {
      return checkoutLink.replace(/https:\/\/assign\.wiwo\.de/, "https://e2e-assign.wiwo.de");
    }
  }
  function addServiceParam(checkoutLink) {
    try {
      const u = new URL(checkoutLink);
      u.searchParams.set("service", getServiceUri());
      return u.toString();
    } catch (e) {
      console.error("NOT A URL:", checkoutLink, e);
      return void 0;
    }
  }
  function piano_adjust_pw(checkoutLink, buttonText) {
    var _a;
    checkoutLink = adjustDomainIfNeeded(checkoutLink);
    const weiterBtn = document.querySelector(
      ".o-paywall .c-reco__button-wrapper a.c-button.c-button--red"
    );
    if (weiterBtn) {
      const duplicateBtn = weiterBtn.cloneNode();
      const href = addServiceParam(checkoutLink);
      if (href) {
        duplicateBtn.href = href;
      }
      if (buttonText) {
        duplicateBtn.innerHTML = buttonText;
      }
      (_a = weiterBtn.parentElement) == null ? void 0 : _a.replaceChild(duplicateBtn, weiterBtn);
    }
  }

  // apps/wiwo-text-length/src/lib/piano_paywall_text.ts
  function addCSS(s) {
    document.head.appendChild(document.createElement("style")).innerHTML = s;
  }
  function showSlider(maxLength, setParagraphVisiblity) {
    var _a;
    const leadTextElement = document.querySelector(".o-article__element .c-leadtext");
    if (leadTextElement === null)
      return;
    const slider = document.createElement("div");
    slider.innerHTML = `
       <input type="range" id="text_length" name="text_length" min="0" max="${maxLength}" />
       <label for="text_length">Text length: ${maxLength}</label>
  `;
    (_a = leadTextElement.parentElement) == null ? void 0 : _a.insertBefore(slider, leadTextElement);
    const sliderInput = document.getElementById("text_length");
    const setLabeL = (text) => {
      var _a2;
      if ((_a2 = sliderInput.labels) == null ? void 0 : _a2.length) {
        sliderInput.labels[0].textContent = text;
      }
    };
    sliderInput.value = maxLength.toString();
    sliderInput.oninput = (e) => {
      const sliderValue = parseInt(e.target.value);
      setLabeL("Text length: " + sliderValue);
      setParagraphVisiblity(sliderValue);
    };
  }
  function piano_paywall_text(text_length, is_dev) {
    if (typeof text_length !== "number")
      return;
    if (typeof is_dev !== "boolean")
      return;
    if (document.getElementsByClassName("o-paywall").length === 0)
      return;
    addCSS(".o-paywall::before {     z-index: 100; }\n.o-article__element {z-index: 110}");
    const paragraphs = Array.from(
      document.querySelectorAll(".o-article__content .o-article__content-element")
    );
    const textparas = paragraphs.map((p) => {
      const els = p.innerText.match(/\S+/g);
      return els ? els.length : 0;
    }).reduce((prev, curr, index) => {
      console.log(curr);
      prev.push((prev[index - 1] || 0) + curr);
      return prev;
    }, []);
    const maxLength = textparas[textparas.length - 1];
    function setParagraphVisiblity(length) {
      console.log(`[PAYWALL TEXT] adjust to length: ${length}`);
      let index = -1;
      if (length < maxLength) {
        index = textparas.findIndex((v) => v >= length);
        if (index == -1) {
          index = textparas.length - 1;
        }
      }
      console.log(`[PAYWALL TEXT] hide paras: ${index}`);
      paragraphs.forEach((p, i) => {
        p.style.display = index > -1 && i >= index ? "none" : "block";
      });
    }
    if (is_dev) {
      showSlider(maxLength, setParagraphVisiblity);
    } else {
      setParagraphVisiblity(text_length);
    }
  }

  // apps/wiwo-text-length/src/index.ts
  window.piano_adjust_pw = piano_adjust_pw;
  window.piano_paywall_text = piano_paywall_text;
})();

      </script>
