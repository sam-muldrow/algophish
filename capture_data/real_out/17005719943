      <script type="javascript/blocked" src="//static.chartbeat.com/js/chartbeat_mab.js" async></script>
      <script type="text/javascript">
    (function () {
  'use strict';

  function hasOwnProperty(obj, prop) {
    return Object.prototype.hasOwnProperty.call(obj, prop);
  }
  function isEnvelope(message, obj) {
    return hasOwnProperty(obj, "ftMessage") && obj["ftMessage"] === message;
  }
  function addTemplateMessageListener(iframeOrigin, message, callback) {
    window.addEventListener("message", (event) => {
      if (event.origin !== iframeOrigin || event.data === null || typeof event.data !== "object" || !isEnvelope(message, event.data)) {
        return;
      }
      callback(event.data.payload);
    });
  }

  class Piano {
    constructor(settings) {
      this.settings = settings;
      window.tp = window.tp || [];
      window.cX = window.cX || {};
      window.cX.callQueue = window.cX.callQueue || [];
    }
    addDmpExternalId(dmpExternalId) {
      this.cXCallQueue.push(["addExternalId", { id: dmpExternalId, type: this.settings.customerPrefix }]);
    }
    useSandbox() {
      return !this.settings.isProd && !!this.settings.applicationIdSandbox;
    }
    getExperienceOrigin() {
      return this.useSandbox() ? "https://sandbox.tinypass.com" : "https://experience-eu.piano.io";
    }
    getTemplateOrigin() {
      return this.useSandbox() ? "https://sandbox.tinypass.com" : "https://buy-eu.piano.io";
    }
    getExperienceUrl() {
      const experienceOrigin = this.getExperienceOrigin();
      const applicationId = this.useSandbox() ? this.settings.applicationIdSandbox : this.settings.applicationIdProduction;
      return `${experienceOrigin}/xbuilder/experience/load?aid=${applicationId}`;
    }
    initPiano() {
      if (!this.settings.isSPA) {
        const a = document.createElement("script");
        a.type = "text/javascript";
        a.async = true;
        a.src = this.getExperienceUrl();
        const b = document.getElementsByTagName("script")[0];
        b.parentNode?.insertBefore(a, b);
      } else {
        this.initCustomPiano();
      }
    }
    get tp() {
      return window.tp;
    }
    get cXCallQueue() {
      return window.cX.callQueue;
    }
    initCustomPiano() {
      this.tp.push([
        "setAid",
        this.useSandbox() ? this.settings.applicationIdSandbox : this.settings.applicationIdProduction
      ]);
      this.tp.push(["setSandbox", this.useSandbox()]);
      this.tp.push(["setUseTinypassAccounts", false]);
      this.tp.push(["setEndpoint", `https://${this.useSandbox() ? "sandbox.tinypass.com" : "buy-eu.piano.io"}/api/v3`]);
      this.tp.push([
        "init",
        () => {
          this.tp.experience.init();
        }
      ]);
    }
    initAdblockDetection() {
      if (!this.settings.isSPA) {
        document.cookie = "__adblocker=; expires=Thu, 01 Jan 1970 00:00:00 GMT; path=/";
        window.setNptTechAdblockerCookie = (adblocker) => {
          const d = /* @__PURE__ */ new Date();
          d.setTime(d.getTime() + 60 * 5 * 1e3);
          document.cookie = "__adblocker=" + (adblocker ? "true" : "false") + "; expires=" + d.toUTCString() + "; path=/";
        };
        const script = document.createElement("script");
        script.setAttribute("async", "true");
        script.setAttribute("src", "//www.npttech.com/advertising.js");
        script.onerror = (e) => {
          window.setNptTechAdblockerCookie(true);
        };
        document.getElementsByTagName("head")[0].appendChild(script);
      }
    }
    initialize({ dmpExternalId, cxenseSiteId }) {
      if (dmpExternalId) {
        this.addDmpExternalId(dmpExternalId);
      }
      if (cxenseSiteId) {
        this.setCxenseSiteId(cxenseSiteId);
      }
      this.initPiano();
      this.initAdblockDetection();
    }
    setCxenseSiteId(cxenseSiteId) {
      this.tp.push(["setCxenseSiteId", cxenseSiteId]);
    }
    setTags(...setTags) {
      const tags = setTags.filter((t) => t && t.length > 0);
      if (tags.length > 0) {
        this.tp.push(["setTags", tags]);
      }
    }
    setCustomVariables(variables) {
      for (const variableName of Object.keys(variables)) {
        this.setCustomVariable(variableName, variables[variableName]);
      }
    }
    setCustomVariable(variableName, variableValue) {
      if (variableValue !== void 0) {
        this.tp.push(["setCustomVariable", variableName, variableValue]);
      }
    }
    onTemplateMessage(message, callback) {
      addTemplateMessageListener(this.getTemplateOrigin(), message, callback);
    }
    executeExeperience() {
      this.tp.experience.execute();
    }
    setSection(section) {
      this.tp.push(["setContentSection", section]);
    }
  }

  var PianoClient = /* @__PURE__ */ ((PianoClient2) => {
    PianoClient2["hb"] = "hb";
    PianoClient2["wiwo"] = "wiwo";
    PianoClient2["relaunch"] = "relaunch";
    PianoClient2["checkout_hb"] = "checkout_hb";
    PianoClient2["checkout_wiwo"] = "checkout_wiwo";
    return PianoClient2;
  })(PianoClient || {});

  const config = {
    pianoComposerApplicationIdProd: "ihsWfIzUpe",
    customerPrefix: "uyz",
    client: PianoClient.wiwo
  };

  function getAllExceptPremium() {
    let all_except_premium = true;
    if (window.hcf_userconfig?.cgi_user_premium_products)
      all_except_premium = false;
    return all_except_premium;
  }

  function getDev(isProd) {
    const pd = new URLSearchParams(window.location.search).get("pianodev");
    const isPianoDev = pd && pd.toLowerCase() === "true";
    return isPianoDev ? isPianoDev : !isProd;
  }

  function getGuest() {
    let guest = true;
    if (window.hcf_userconfig?.logged)
      guest = false;
    return guest;
  }

  function localStorageItems(key) {
    return {
      getItem: () => localStorage.getItem(key),
      removeItem: () => localStorage.removeItem(key),
      setItem: (value) => localStorage.setItem(key, value)
    };
  }
  const lsiRecurringUser = localStorageItems("recurringUser");
  const lsiBottomClose = localStorageItems("bottom_close");

  function getHasClosedBottom() {
    return !!lsiBottomClose.getItem();
  }

  function getIsLoggedIn() {
    let loggedIn = false;
    if (window._context?.user?.isPremium === true || window._context?.user?.isFree === true) {
      loggedIn = true;
    }
    return loggedIn;
  }

  function getIsMEW() {
    return window._context?.client?.isMEW;
  }

  function getIsSubscriber() {
    let subscriber = false;
    if (window.hcf_userconfig?.cgi_user_premium_products)
      subscriber = true;
    return subscriber;
  }

  function getIsWWW() {
    return window._context?.client?.isHandelsblatt;
  }

  function getLogged() {
    let logged = false;
    if (window.hcf_userconfig?.logged)
      logged = true;
    if (window.hcf_userconfig?.cgi_user_premium_products)
      logged = false;
    return logged;
  }

  function getPremium() {
    let premium = false;
    if (window.hcf_userconfig?.cgi_user_premium_products)
      premium = true;
    return premium;
  }

  function getRecurringUser() {
    let recurringUser = true;
    if (!lsiRecurringUser.getItem()) {
      lsiRecurringUser.setItem("true");
      recurringUser = false;
    }
    return recurringUser;
  }

  function getReferrer() {
    let referrer;
    if (document.referrer) {
      referrer = document.referrer.replace("www.", "").replace(/(^\w+:|^)\/\//, "").split("/").shift();
    }
    return referrer;
  }

  function getShortUri() {
    const s = window.location.href.match(/\/\d*\.html.*$/);
    return s ? s[0] : null;
  }
  function getServiceUrl() {
    const sp = new URLSearchParams(window.location.search);
    if (sp.get("service")) {
      return sp.get("service");
    }
    const shortUri = getShortUri();
    const regex = /^(preview-)?(app\.)(handelsblatt\.com|[a-zA-Z0-9-]+--hb.hmg.systems)/gim;
    const host = window.location.host.replace(regex, `$1www.$3`);
    return null === shortUri ? window.location.href : window.location.protocol + "//" + host + shortUri;
  }

  function getPaywallVisible() {
    return !!document.querySelector(".temp-paywall2") || !!document.querySelector(".o-paywall");
  }

  function readMetaTag(identifier, name) {
    return document.head.querySelector(`meta[${identifier}="${name}"]`)?.content;
  }

  function getArticleCategory(isArticle) {
    const articleCategory = readMetaTag("name", "vr:category");
    if (isArticle && articleCategory) {
      return articleCategory;
    }
    return "";
  }

  function getArticleClassification() {
    const articleContentTier = readMetaTag("property", "article:content_tier");
    if (articleContentTier) {
      return articleContentTier === "locked" ? "premium" : articleContentTier;
    }
    return "";
  }

  function initFooterListener(piano, isHb) {
    if (lsiBottomClose.getItem() && isHb) {
      const bottomCloseDate = new Date(lsiBottomClose.getItem());
      if (dateDiffInDays(bottomCloseDate, /* @__PURE__ */ new Date()) === 2)
        lsiBottomClose.removeItem();
    }
    piano.onTemplateMessage("closedFooter", () => {
      isHb && lsiBottomClose.setItem(String(/* @__PURE__ */ new Date()));
    });
    piano.onTemplateMessage("wtSendInfo", (payload) => {
      window.wt?.sendinfo(payload);
    });
    piano.onTemplateMessage("banner_close", (payload) => {
      isHb && lsiBottomClose.setItem(String(/* @__PURE__ */ new Date()));
      window.wt?.sendinfo({
        linkId: "Schliessen_Button",
        customClickParameter: { 5: "BottomLayer-schliessen" }
      });
    });
    piano.onTemplateMessage("banner_click", (payload) => {
      window.wt?.sendinfo({
        linkId: "Aktionstracking",
        customClickParameter: { 12: "BottomLayer-composer" }
      });
    });
    piano.onTemplateMessage("banner_show", (payload) => {
      window.wt?.sendinfo({
        linkId: "Digitalpasslayer",
        customClickParameter: { 4: "BottomLayer" }
      });
    });
  }
  function dateDiffInDays(closeDate, currentDate) {
    const t2 = currentDate.getTime();
    const t1 = closeDate.getTime();
    const diffTime = Math.abs(t2 - t1);
    let diffDays = Math.ceil(diffTime / (1e3 * 60 * 60 * 24));
    if (diffDays === 0) {
      diffDays = 1;
    }
    return diffDays;
  }

  function initHeaderContainer(selector, containerId) {
    const element = document.querySelector(selector);
    if (!element) {
      return;
    }
    element.setAttribute("id", containerId);
    element.removeAttribute("href");
    element.outerHTML = element.outerHTML.replace(/a /g, "div ");
  }

  function pianoSetup() {
    const isProd = !~window.location.host.indexOf("preview");
    const isArticle = !!readMetaTag("property", "article:content_tier");
    const piano = new Piano({
      isSPA: !!config.isSPA,
      isProd,
      applicationIdProduction: config.pianoComposerApplicationIdProd,
      applicationIdSandbox: config.pianoComposerApplicationIdSandbox,
      customerPrefix: config.customerPrefix
    });
    const dmpExternalId = window.hcf_userconfig?.logged && window.hcf_userconfig?.cgi_customerid ? window.hcf_userconfig.cgi_customerid : void 0;
    piano.initialize({ dmpExternalId });
    piano.setCustomVariable("dev", getDev(isProd));
    piano.setCustomVariable("logged", getLogged());
    piano.setCustomVariable("all-exept-premium", getAllExceptPremium());
    piano.setCustomVariable("premium", getPremium());
    piano.setCustomVariable("isSubscriber", getIsSubscriber());
    piano.setCustomVariable("guest", getGuest());
    piano.setCustomVariable("referrer", getReferrer());
    piano.setCustomVariable("service_url", getServiceUrl());
    piano.setCustomVariable("paywallVisible", getPaywallVisible());
    piano.setTags(getArticleClassification(), getArticleCategory(isArticle));
    initFooterListener(piano, config.client === PianoClient.hb);
    if (config.client === PianoClient.hb) {
      const headerSelector = "#ref_header > div > div.vhb-c-nav-logos > a", headerId = "composer-header-wrapper", stickyHeaderSelector = "#ref_header > div > div.vhb-c-action-link.vhb-u-sticky__show > a", stickyHeaderId = "composer-sticky-header-wrapper";
      initHeaderContainer(headerSelector, headerId);
      initHeaderContainer(stickyHeaderSelector, stickyHeaderId);
      piano.setCustomVariable("isMEW", getIsMEW());
      piano.setCustomVariable("isWWW", getIsWWW());
      piano.setCustomVariable("isLoggedIn", getIsLoggedIn());
      piano.setCustomVariable("recurring_user", getRecurringUser());
      piano.setCustomVariable("hasClosedBottom", getHasClosedBottom());
    }
  }

  window._hmgCmp.addEventFn("hasConsent", pianoSetup, { vendor: "5eec8924b8e05c699567f398" });

})();

