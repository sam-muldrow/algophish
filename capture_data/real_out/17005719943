    margin-bottom: 15px !important;
    text-shadow: 0 0 7px rgb(0 0 0 / 50%); 
  }
  `;
    document.head.appendChild(styleSheet);
  }
  function ftc729_blurry_text() {
    const random = () => Math.floor(Math.random() * (4 - 3 + 1) + 3);
    let i = 0, number = random();
    addCSS();
    while (availableChars != 0) {
      if (i > 0 && i % number === 0) {
        randomHeadline();
        number = random();
      }
      randomTextBlock();
      i++;
    }
  }

