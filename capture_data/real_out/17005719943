  var availableChars = articleLength;
  function randomizedText(textLength) {
    let text = "", i = 0;
    while (i < textLength) {
      text += `${randomWord()} `;
      i = text.length;
    }
    return text;
  }
  function randomWord() {
    const wordLength = Math.floor(Math.random() * 5);
    let word = "";
    for (let i = 0; i < wordLength; i++) {
      word += fragments[Math.floor(Math.random() * fragments.length)];
    }
    return word;
  }
  function insertAfter(newNode, referenceNode) {
    var _a3;
    (_a3 = referenceNode == null ? void 0 : referenceNode.parentNode) == null ? void 0 : _a3.insertBefore(newNode, referenceNode.nextSibling);
  }
  function randomTextBlock() {
    let characters = availableChars;
    if (availableChars > 1e3) {
      characters = Math.floor(Math.random() * (1e3 - 200 + 1) + 200);
    }
    availableChars = availableChars - characters;
    const tag = document.createElement("p"), string = randomizedText(characters), text = document.createTextNode(string.charAt(0).toUpperCase() + string.slice(1));
    tag.className = "blurry-text";
    tag.appendChild(text);
    insertAfter(tag, paywallEl);
  }
  function randomHeadline() {
    const characters = Math.floor(Math.random() * (60 - 40 + 1) + 40), tag = document.createElement("h3"), string = randomizedText(characters), text = document.createTextNode(string.charAt(0).toUpperCase() + string.slice(1));
    tag.className = "blurry-text";
    tag.appendChild(text);
    insertAfter(tag, paywallEl);
  }
  function addCSS() {
    const styleSheet = document.createElement("style");
    styleSheet.innerText = `.blurry-text { 
    color: transparent; 
    text-shadow: 0 0 6px rgb(0 0 0 / 50%); 
