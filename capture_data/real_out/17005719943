      <script type="text/javascript">
    /* Chartbeat Custom Fields Setup */
let ArtJsonLD = '';
let ArtJson = '';

function jsonldparse(){
    if(ArtJsonLD.length === 0){
        ArtJsonLD = document.querySelectorAll('script[type="application/ld+json"]');
        if(ArtJsonLD.length){
            ArtJson = JSON.parse(ArtJsonLD[0].text);
        }
    }
}

(function(window){

    var _cbq = window._cbq = (window._cbq || []);
    var user_type = 'anon';

    jsonldparse();

    var category = '';
    if(ArtJsonLD.length > 0){
        if(typeof ArtJson.creator !== 'undefined'){
            var authors = ArtJson.creator.toString();
            if (authors !== null){
                window.chartbeatAuthors = authors;
            }
        }
        if(typeof ArtJson.articleSection !== 'undefined'){
            category = ArtJson.articleSection;
        }
    }

    //sections parameter can be field by multiple souces
    var sections = ['channel-online'];
    //source: category meta info
    if (category){
        sections.push(category);
    }else{
        var category =  document.querySelector('[name="vr:category"]');
        if (category){
            sections.push(category.content);
        }
    }
    //tech: empty.js not available
    if (typeof hcf_userconfig !== 'undefined'){

        //tech: signed in user
        if (hcf_userconfig.logged){
            user_type = 'lgdin';
            if(hcf_userconfig.cgi_adb_granting_product){
                user_type = 'paid';
            }
        } else {
            user_type = 'anon';
        }
    }

    _cbq.push(['_acct', user_type]);

    //build category string
    if (sections.length > 0){
        window.chartbeatSections = sections.join(',');
    }
})(window);


(function() {

    jsonldparse();

    var title = document.getElementsByTagName('title')[0].text;
    if(ArtJsonLD.length > 0){
        if(typeof ArtJson.headline !== 'undefined'){
            title = ArtJson.headline;
        }
        if(typeof ArtJson['@type'] !== 'undefined'){
            title = ArtJson['@type'] + " | " + title;
        }

    }

    if (typeof hcf_userconfig !== 'undefined'){

        //title = title + " | " + hcf_userconfig.cgi_contentclassification;

        if (location.href.indexOf('.de/my/') > 0){
            title = title + " | " + 'Premium';
        }
    }

    var currenUrl = window.location.href;
    currenUrl = currenUrl.substring(0, currenUrl.indexOf('?'))

    var artUrl = ArtJson.url;

    /** CONFIGURATION START **/
    var _sf_async_config                = window._sf_async_config = (window._sf_async_config || {});
    _sf_async_config.uid                = 65705;
    _sf_async_config.domain             = window.location.hostname.replace(/^www\.(?=[^\.]+)/, '');
    _sf_async_config.flickerControl     = false;
    _sf_async_config.useCanonical       = true;
    _sf_async_config.useCanonicalDomain = true;
    _sf_async_config.title              = title;
    if(currenUrl !== artUrl){
        console.log('_sf_async_config.path');
        _sf_async_config.path = currenUrl;
    }
    /*_sf_async_config.lazyLoadImageAttributes = 'data-src';*/
    _sf_async_config.articleBlockSelector = 'div.c-teaser, .c-teaser__image';

    if (window.chartbeatSections) {_sf_async_config.sections = window.chartbeatSections;}
    if (window.chartbeatAuthors) {_sf_async_config.authors = window.chartbeatAuthors;}

    /** CONFIGURATION END **/
    function loadChartbeat() {

        if(self.origin !== top.origin && window.parent.origin === 'https://www.google.com'  || self.origin !== top.origin && window.parent.origin === 'https://amp2.wiwo.de'){
            return false;
        }

        var e = document.createElement('script');
        var n = document.getElementsByTagName('script')[0];
        e.type = 'text/javascript';
        e.async = true;
        e.src = '//static.chartbeat.com/js/chartbeat.js';;
        n.parentNode.insertBefore(e, n);
    }

    _hmgCmp.addEventFn('hasConsent', function () {
        loadChartbeat();
    }, {vendor: '5ea172e36ede87504f7b4590'});
})();

