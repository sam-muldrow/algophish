      <script type="text/javascript">
    /**
 * Remove JS Script without consent check
 */
(function () {
    const allowlist = [
        "jquery-1.12.4.min.js",
        "cmp-sp.handelsblatt.com",
        "meine-news",
        "iomm",
        "tx.io",
        "11002860",
        "22976510",
        "21189236",
        "embed-v1.handelsblatt.com",
        "assets/wiwo/js/core.js",
        "26194908",
        "25269278",
        "11002856",
        "25452436",
        "19292274",
        "19346040",
        "outbrain.com"
    ];

    const observer = new MutationObserver(function (mutations) {
        mutations.forEach(function (_ref) {
            const addedNodes = _ref.addedNodes;

            addedNodes.forEach(function (node) {
                if (!node.tagName) {
                    return false;
                }

                const tagName = node.tagName.toLowerCase();

                if (!["iframe", "script"].includes(tagName)) {
                    return false;
                }

                const hasSource = node.src !== undefined;

                if (hasSource && allowlist.some((v) => node.src.includes(v))) {
                    return false;
                }

                const code = node.innerHTML;
                if (
                    (code && !code.includes(".src")) ||
                    (code.includes(".src") && code.includes("_hmgCmp"))
                ) {
                    return false;
                }

                if (node.src || node.innerHTML) {
                    console.warn(
                        `CMP removed <${node.tagName}> with src: `,
                        node.src || node.innerHTML
                    );
                    node.remove();
                }
            });
        });
    });

    __tcfapi("getCustomVendorConsents", 2, function (vendorConsents) {
        if (vendorConsents.consentedPurposes.length === 0) {
            if(typeof window.wt === 'undefined'){
                window.wt = {};
                window.wt.sendinfo = function (click){};
            }
            if(typeof window.ee === 'undefined' && !!~location.href.indexOf('wiwo.de')) {
                window.ee = {}
                window.ee.emitEvent = function () {}
            }
            observer.observe(document.documentElement, {
                childList: true,
                subtree: true,
            });
        }
    });
})();

      </script>
