  function addOrUpdateCordialContact(email, ukey, email_64, invite_link, source) {
    var auth_data = {
      email: email,
    };
    var contact_data = {
      'channels': {
        'email': {
          'subscribeStatus': "subscribed",
        },
      },
    'subs_nps_survey': true,
    'subs_billing_method_expiring': true,
    'subs_birthday': true,
    'subs_favorite_notification': true,
    'subs_invite_acceptance': true,
    'subs_credit_expiring': true,
    'subs_credit_added': true,
    'subs_abandoned_cart': true,
    'subs_at_most_one_per_day': false,
    'subs_weekly': true,
    'subs_daily': true,
    'subs_item_ending': true,
    'welcome': true,
    'subscription_status': 1,
    'preference_center_link' : `https://www.touchofmodern.com/invites/subscriptions?u=${email_64}&ukey=${ukey}`, 
    'invite_link' : invite_link,
    'source' : source
    };    

    if (crdl) {
      crdl("contact", auth_data, contact_data);
      console.log(`cordial contact ran for ${email}`);
    } else {
      console.log(`crdl unable to be reached for ${email}`)
    }
  }
  
  startCordial()
