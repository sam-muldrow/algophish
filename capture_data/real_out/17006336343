<script defer>
jQuery(document).ready(function(){
	var $window=jQuery(window);
	jQuery(document).on('click','#sk-learmore-expand:not(.expand) .elementor-button',function(e){
		e.preventDefault();
		if ($window.width() < 767) {
      		jQuery('body:not(.elementor-editor-active) .sk-learmore-expand-content.expand-sec-1').slideDown(300);
    	}
    	else if ($window.width() > 767 && $window.width() < 1023) {
      		jQuery('body:not(.elementor-editor-active) .sk-learmore-expand-content.expand-sec-2').slideDown(300);
    	}
    	else {
     		jQuery('body:not(.elementor-editor-active) .sk-learmore-expand-content.expand-sec-1').slideDown(300);
    	}
		jQuery('#sk-learmore-expand').addClass('expand');
		jQuery('#sk-learmore-expand .elementor-button-text').html('Close');
	})
	jQuery(document).on('click','#sk-learmore-expand.expand .elementor-button',function(e){
		e.preventDefault();
		if ($window.width() < 767) {
      		jQuery('body:not(.elementor-editor-active) .sk-learmore-expand-content.expand-sec-1').slideUp(300);
    	}
    	else if ($window.width() > 767 && $window.width() < 1023) {
      		jQuery('body:not(.elementor-editor-active) .sk-learmore-expand-content.expand-sec-2').slideUp(300);
    	}
    	else {
     		jQuery('body:not(.elementor-editor-active) .sk-learmore-expand-content.expand-sec-1').slideUp(300);
    	}
		jQuery('#sk-learmore-expand').removeClass('expand');
		jQuery('#sk-learmore-expand .elementor-button-text').html('Learn More ...');
	})
});
</script>
