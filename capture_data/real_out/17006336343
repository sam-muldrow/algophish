<script id="cookie-js">
//<![CDATA[
/*!
 * jQuery Cookie Plugin v1.2
 * https://github.com/carhartl/jquery-cookie
 *
 * Copyright 2011, Klaus Hartl
 * Dual licensed under the MIT or GPL Version 2 licenses.
 * http://www.opensource.org/licenses/mit-license.php
 * http://www.opensource.org/licenses/GPL-2.0
 */
(function ($, document, undefined) {

    var pluses = /\+/g;

    function raw(s) {
        return s;
    }

    function decoded(s) {
        return decodeURIComponent(s.replace(pluses, ' '));
    }

    $.cookie = function (key, value, options) {

        // key and at least value given, set cookie...
        if (value !== undefined && !/Object/.test(Object.prototype.toString.call(value))) {
            options = $.extend({}, $.cookie.defaults, options);

            if (value === null) {
                options.expires = -1;
            }

            if (typeof options.expires === 'number') {
                var days = options.expires, t = options.expires = new Date();
                t.setDate(t.getDate() + days);
            }

            value = String(value);

            return (document.cookie = [
                encodeURIComponent(key), '=', options.raw ? value : encodeURIComponent(value),
                options.expires ? '; expires=' + options.expires.toUTCString() : '', // use expires attribute, max-age is not supported by IE
                options.path    ? '; path=' + options.path : '',
                options.domain  ? '; domain=' + options.domain : '',
                options.secure  ? '; secure' : ''
            ].join(''));
        }

        // key and possibly options given, get cookie...
        options = value || $.cookie.defaults || {};
        var decode = options.raw ? raw : decoded;
        var cookies = document.cookie.split('; ');
        for (var i = 0, parts; (parts = cookies[i] && cookies[i].split('=')); i++) {
            if (decode(parts.shift()) === key) {
                return decode(parts.join('='));
            }
        }

        return null;
    };

    $.cookie.defaults = {};

    $.removeCookie = function (key, options) {
        if ($.cookie(key, options) !== null) {
            $.cookie(key, null, options);
            return true;
        }
        return false;
    };

})(jQuery, document);
//]]
</script>
<script id="main-js">
//<![CDATA[
// Main JS
jQuery( document ).ready( function( $ ) {

	function initCookies(){		
		if ( $.cookie( 'no-thanks' ) != 1 ) {
			$( '#block-down' ).show();
		}

		$( '#block-down .no-thanks' ).click( function( e ) {
			$.cookie( 'no-thanks', 1, { 'expires' : 100 } );
			e.stopPropagation();
			$( '#block-down' ).hide(); 
		});	
	}

	function initRedirect() {	
		$( '#block-down div.block-down-img' ).click( function( e ) {
			window.open( 'https://www.home-designing.com/book' );
		});
	}

	$( document ).ready( function() {
		initCookies();	
		initRedirect();
	});



	// Hipmill - handle ad placements on mobile display.
	function hd_maybe_adjust_ad_placements() {
		// Only run in case screen is mobile.
		if ( $( window ).width() <= 665 ) {
			if ( ! $( '#recent-posts .advertisements' ).hasClass( 'hidden' ) ) {
				$( '#recent-posts .advertisements' ).addClass( 'hidden' );
				$( '#recent-posts .advertisements .post.ad-block' ).each( function() {
					var $ad_block = $( this ).detach();
					$ad_block.insertAfter( '#recent-posts .post:not(.ad-block):nth-of-type(' + $ad_block.data( 'ad-index' ) * 6 + ')' );
				});
			}
		} else {
			$( '#recent-posts > .post.ad-block' ).each( function() {
				$( '#recent-posts .advertisements' ).append( $( this ).detach() );
			});
			$( '#recent-posts .advertisements' ).removeClass( 'hidden' );
		}
	}

	if ( $( '#recent-posts .advertisements' ).length === 1 ) {
		// Label the advertising blocks in the advertisement container.
		$( '#recent-posts .advertisements .post.ad-block' ).each( function( index, $element ) {
			$( this ).data( 'ad-index', index + 1 );
			$( this ).attr( 'data-ad-index', index + 1 );
		});

		hd_maybe_adjust_ad_placements();

		$( window ).resize( function() {
			hd_maybe_adjust_ad_placements();
		});
	}
});

//]]
</script>
<script id="wootabs-js">
//<![CDATA[
jQuery(document).ready(function($){

	// UL = .wooTabs
	// Tab contents = .inside
	
	var tag_cloud_class = '#tagcloud'; 
	
	//Fix for tag clouds - unexpected height before .hide() 
	var tag_cloud_height = $('#tagcloud').height();
	
	$('.inside ul li:last-child').css('border-bottom','0px'); // remove last border-bottom from list in tab conten
	$('.wooTabs').each(function(){
		$(this).children('li').children('a:first').addClass('selected'); // Add .selected class to first tab on load
	});
	$('.inside > *').hide();
	$('.inside > *:first-child').show();
	
	$('.wooTabs li a').click(function(evt){ // Init Click funtion on Tabs
	
		var clicked_tab_ref = $(this).attr('href'); // Store HREF value
		
		$(this).parent().parent().children('li').children('a').removeClass('selected'); //Remove selected from all tabs
		$(this).addClass('selected');
		$(this).parent().parent().parent().children('.inside').children('*').hide();
		
		$('.inside ' + clicked_tab_ref).fadeIn(500);
		 
		 evt.preventDefault();
	
	});
});


