jQuery( function( $ ) {
	$(document).ready(function() {
		$('.tooltip-link').mouseover(function(){
			$(this).parent().parent().parent().parent().find('.tooltip').css({'opacity':'0', 'display':'block'});
			$(this).parent().parent().parent().parent().find('.tooltip').animate({ opacity: 1 });
	});
	$('.tooltip').mouseleave(function(){
			$(this).fadeOut();
		});
	});
} );

