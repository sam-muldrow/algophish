        <script>
        jQuery(document).ready(function($) {
			if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
				let check = '';

				setTimeout(function(){
					show_fancybox();
				},parseInt('60000'));

				if(check == true || check == 'true'){
					show_fancybox();
				}
				$('body').on('click','.cs-fancybox-close',function(e){
					e.preventDefault();
					$('.snp-root.cs_mobile_device').removeClass('cs_mobile_device');
					$('.snp-pop-85659.cs_mobile_device').removeClass('cs_mobile_device');
				})
				function show_fancybox(){
					$.ajax({
						type: 'POST',
						dataType: 'json',
						url: 'https://www.home-designing.com/wp-admin/admin-ajax.php',
						data: {'action': 'check_fancybox'},
						success: function (data) {
							if (data.success == true) {
								$('.snp-pop-85659').addClass('cs_mobile_device');
								$('.snp-pop-85659').closest('.snp-root').addClass('cs_mobile_device');
								$('.snp-pop-85659.cs_mobile_device').prepend('<a title="Close" class="cs-fancybox-close" href="javascript:;"></a>');
								$('.snp-pop-85659.cs_mobile_device .heading-text h1').html('Enjoy architecture and design?');
								$('.snp-pop-85659.cs_mobile_device .heading-text > p').html('Get the best of it delivered directly to your inbox. Free.');
								$('.snp-pop-85659.cs_mobile_device .cs-popup-text').html('When you subscribe you get:');
							}
						},
					});
				}
			}
        });
    </script>
