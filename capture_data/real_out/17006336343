        <script type="text/javascript">
        //Columns
        function resizeWin() {
            var rightPostsWidth = jQuery( "#recent-posts .post" ).outerWidth( true );
            jQuery( ".right_posts" ).width( rightPostsWidth );
            var mainWidth = jQuery( "#main" ).width();
            var leftPostsQuantity = Math.floor( ( mainWidth - rightPostsWidth ) / rightPostsWidth );
            var leftPostsWidth;
            leftPostsWidth = leftPostsQuantity * rightPostsWidth;
            jQuery( ".left_posts" ).width( leftPostsWidth );
            if ( leftPostsQuantity == 0 ) {
              jQuery( ".left_posts" ).width( rightPostsWidth );
              jQuery( ".left_posts, .right_posts" ).css({ "margin":"0 auto", "float":"none" });  
            } else {
                jQuery( ".left_posts, .right_posts" ).css({ "margin":"0", "float":"left" });
            }
        }
        jQuery( window ).resize( function() {
            resizeWin(); 
        }).trigger( "resize" );
    </script>
