<script>
    function changeNums() {
        //下载量
        var webDown = document.getElementById("webDown").innerHTML;
        var webDownTmp = webDown.replace(eval("/,/g"), '');
        var webDown = parseInt(webDownTmp);
        //alert(webDown);
        var storage = window.localStorage;
        var val = parseInt(storage.getItem('webDown'));        //访问存储数据
        if (isNaN(val)) {
            storage.setItem('webDown', webDown);
        }
        changeNum('webDown');

        //注册量
        var bbsMember = document.getElementById("bbsMember").innerHTML;
        var bbsMemberTmp = bbsMember.replace(eval("/,/g"), '');
        var bbsMember = parseInt(bbsMemberTmp);
        var storage = window.localStorage;
        var val = parseInt(storage.getItem('bbsMember'));        //访问存储数据
        if (isNaN(val)) {
            storage.setItem('bbsMember', bbsMember);
        }
        changeNum('bbsMember');


        //在线量
        var onlineMember = document.getElementById("onlineMember").innerHTML;
        var onlineMemberTmp = onlineMember.replace(eval("/,/g"), '');
        var onlineMember = parseInt(onlineMemberTmp);
        var storage = window.localStorage;
        var val = parseInt(storage.getItem('onlineMember'));        //访问存储数据
        if (isNaN(val)) {
            storage.setItem('onlineMember', onlineMember);
        }
        changeNum('onlineMember');
    };

    function changeNum(k) {
        if (k == 'onlineMember') {
            var rand = getRand(-3, 3);
        } else {
            var rand = getRand(1, 3);
        }

        var storage = window.localStorage;
        var newVal = rand + parseInt(storage.getItem(k));
        storage.setItem(k, newVal);
        document.getElementById(k).innerHTML = formatting(newVal);
        if (k == 'bbsMember') {
            var time = 1000 * 60 * 10;
        } else if (k == 'onlineMember') {
            var time = 1000 * 2;
        } else {
            var time = 1000;
        }
        setTimeout("changeNum('" + k + "')", time);
    }

    function getRand(Min, Max) {
        var Range = Max - Min;
        var Rand = Math.random();
        return (Min + Math.round(Rand * Range));
    }

    /*function formatting(s)
    {
        var sTmp = s;
        var len = sTmp.toFixed().length;
        var count = Math.ceil(len/3);
        var rs='';
        for(var i=1; i<=count; i++)
        {
            if(i==1)
            {
                var rs = s%1000;
            }
            else
            {
                var rs = s%1000 + ',' + rs;
            }
            s = parseInt(s/1000);
        }
        return rs;
    }*/
    function formatting(n) {
        var b = parseInt(n).toString();
        var len = b.length;
        if (len <= 3) {
            return b;
        }
        var r = len % 3;
        return r > 0 ? b.slice(0, r) + "," + b.slice(r, len).match(/\d{3}/g).join(",") : b.slice(r, len).match(/\d{3}/g).join(",");
    }

    /*
    function formatting(amount) {
    var delimiter = ","; // replace comma if desired
    amount = new String(amount);
    var a = amount.split('.',2)
    var d = a[1];
    var i = parseInt(a[0]);
    if(isNaN(i)) { return ''; }
    var minus = '';
    if(i < 0) { minus = '-'; }
    i = Math.abs(i);
    var n = new String(i);
    var a = [];
    while(n.length > 3)
    {
    var nn = n.substr(n.length-3);
    a.unshift(nn);
    n = n.substr(0,n.length-3);
    }
    if(n.length > 0) { a.unshift(n); }
    n = a.join(delimiter);
    if(d.length < 1) { amount = n; }
    else { amount = n + '.' + d; }
    amount = minus + amount;
    return amount;
    }
    */
</script>
