<script>

const recommListObj = {
		mainApi:"/subscribe/api/pc_main_recommend_api.jsp?device=pc",
		row:100,
		lowestInx:0,
		total:0,
		data:new Array(),
		getLowestGoods(){
			let urlApi = this.mainApi;
			let param = {device:"pc" }
			// 속성 API 호출
			let subscribeListPromise = $.ajax({
				type : "GET",
				url : urlApi,
				data : param,
				dataType : "json"
			});
			
			subscribeListPromise.then(this.drawList, this.failAttr);
		
		},
	    drawList(json){
			if( !json.success  ){
				$("#lowestTab").hide();
			}else{
				recommListObj.total = json.listArr.length;
			
				if( recommListObj.total > 0 ){
					
					$("#inexpensive__list").empty();
					$("#timeText").text(json.timeText);
					
					recommListObj.data = json.listArr;
					lowestDraw();
					
				}else{
					$("#lowestTab").hide();
				}
				
			}
		}
}

function lowestLiMake(v){
	
	var template = 
        `<li class="inexpensive__prod" data-cate-no="${v.cate_cd}">    
            <a href="detail.jsp?modelno=${v.enr_model_no}" onclick="insertLog(30033);" class="inexpensive__prod--item" >       
                <div class="prod-item__thumb">          
                    ${(v.lowest_flag == "Y") ? "<div class='tag--lowest'>역대최저가</div>" : ""}    
                    <img src="${v.img_url}" alt="${v.enr_model_nm}">      
                </div>
                <div class="prod-item__info">
                    <div class="prod-item__tx--name">${v.enr_model_nm}</div>    
                    <div class="prod-item__price">           
                        <dl class="price__tx--mall">
                            <dt>N사 최저가</dt> 
                            <dd><del>`+commaNum(v.nv_min_prc)+`</del>원</dd>  
                        </dl>  
                        <dl class="price__tx--diff"> 
                            <dt><em>${v.prc_cmpr_rt}</em>%</dt>     
                            <dd><em>`+commaNum(v.prc_cmpr_amt)+`</em>원</dd>  
                        </dl>
                            <dl class="price__tx--enuri">             
                            <dt>에누리 최저가</dt>          
                            <dd><em>`+commaNum(v.enr_min_prc)+`</em>원</dd>        
                        </dl>        
                    </div>   
                </div>  
            </a>  
            <button data-modelno="${v.enr_model_no}" data-type="N" class="btn--subscription ${(v.zzimYN == 'Y') ? "is--on" : ""}">구독</button>
        </li>`
        return template;
	
}

function changeLowest (param) {
	//(default : 오른쪽으로 2칸)
	var moveSize = typeof param !== "undefined" ? param : 4; 
	recommListObj.lowestInx = (recommListObj.lowestInx + moveSize + recommListObj.total) % recommListObj.total;
	
	lowestDraw();
}

function lowestDraw() {
	var v = recommListObj.data[recommListObj.lowestInx];                                                                        
	var lowestHtml = "";
	
	try{
		lowestHtml = lowestLiMake(v);
		v = recommListObj.data[recommListObj.lowestInx+1];
		lowestHtml += lowestLiMake(v);
		v = recommListObj.data[recommListObj.lowestInx+2];
		lowestHtml += lowestLiMake(v);
		v = recommListObj.data[recommListObj.lowestInx+3];
		lowestHtml += lowestLiMake(v);
		
		$("#inexpensive__list").html(lowestHtml);
		
	}catch(e){
		console.log(e);
	}
}

try{
	recommListObj.getLowestGoods();
}catch(e){
	
}

$('body').on('click', "#lowestBtn > button", function(e) {
	changeLowest(($(this).index() == 0) ? -4 : 4);
});

$('body').on('click', ".btn--subscription", function(e) {
	
	var modelno = $(this).data("modelno");
	var plno = $(this).data("plno");
	var type = $(this).data("type");

	if(!islogin()){
		if( confirm("구독하시려면 로그인이 필요합니다.\n로그인 하시겠습니까?") ){
			location.href = "/member/login/login.jsp?rtnUrl="+encodeURIComponent(location.href);
			return false;
		}else{
			return false;
		}
	}

	if( type == "B" ){
		if( $(this).hasClass("is--on") ){
			$(this).removeClass("is--on");
			delZzim("P", plno);
		}else{
			addZzim("P",plno);
			$(this).addClass("is--on");
			insertLog(30501);
		}
	}else{
		if( $(this).hasClass("is--on") ){
			$(this).removeClass("is--on");
			delZzim("G", modelno);
		}else{
			addZzim("G",modelno);
			$(this).addClass("is--on");
			insertLog(30034);
		}
	}
});

function addZzim(gp, modelno){
	let data = "";
	if(gp == 'G')		data = "G:"+modelno;	
	else		data = "P:"+modelno;
	
	var $layZzim = $(".lay-zzim");
	
	$.ajax({
		url : "/m/ajax/resentZzim/insertSaveGoodsProc.jsp",
		type : "POST",
		data : { 
			"modelnos" : data 
		},
		dataType : "JSON",
		success : function(data){
			if(data && data.count === "1")
				$("#btn_zzim_G"+modelno).addClass("is--on");
			
			setTimeout(function(){
                $layZzim.removeClass("is--off").addClass("is--show").show();
            },1);
		},
		complete : function(){
	 		zzimTimer = setTimeout(function(){
			    $layZzim.removeClass('is--show');
			},2000);
		}
	});
}

function delZzim(gp, modelno){
	let data = "";
	if(gp == 'G')		data = "G:"+modelno;	
	else		data = "P:"+modelno;
	
	var $layZzim = $(".lay-zzim");
	
	if(confirm("이미 구독중인 상품입니다.\n취소하시겠습니까?")){
		$.ajax({
			url : "/view/deleteSaveGoodsProc.jsp",
			type : "POST",
			data : {
				"modelnos" : data,
				"tbln" : "save"
			},
			success : function(d) {
				$("#btn_zzim_G"+modelno).removeClass('is--on');
				setTimeout(function(){
                    $layZzim.addClass("is--off is--show").show();
                },1);
			},
			complete : function(){
				zzimTimer = setTimeout(function(){
				    $layZzim.removeClass('is--show');
				},2000);
				
	 			$("#goZzim").on("click",function(){
					window.open("https://"+location.host+"/my/my_enuri.jsp?name=zzim");
				});
			}
		});
    }
}
