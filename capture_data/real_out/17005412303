<script>
	function clearRest(){
		var p;
		for(p=currentPageLevel+1; p<20; p++){
			sessionStorage.removeItem(p+"");
		}
	}

	var currentPageLevel = 1;
	sessionStorage.setItem("1", "Home:::https://www.equifax.com/");
	clearRest();
</script>



