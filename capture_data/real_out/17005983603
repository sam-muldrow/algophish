  // PPID handling
  var _PPID_LS_KEY = "_adloader_uuid";
  var _PUBCID_LS_KEY = "_pubcid";

  var generateUUID = function () {
    // https://stackoverflow.com/questions/105034/how-to-create-a-guid-uuid/2117523#2117523
    // tslint:disable
    var u = '', m = 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx', i = 0, rb = Math.random() * 0xffffffff | 0;
    while (i++ < 36) {
      var c = m[i - 1], r = rb & 0xf, v = c == 'x' ? r : (r & 0x3 | 0x8);
      u += (c == '-' || c == '4') ? c : v.toString(16); rb = i % 8 == 0 ? Math.random() * 0xffffffff | 0 : rb >> 4
    }
    return u;
    // tslint:enable
  }

  var setCookie = function (cname, cvalue, exdays) {
    "use strict";
    var d = new Date();
    d.setTime(d.getTime() + exdays * 24 * 60 * 60 * 1000);

    var expires = "expires=" + d.toUTCString();

    cvalue = escape(cvalue);
    document.cookie = cname + "=" + cvalue + "; " + expires;
  };

  var getCookie = function (cname) {
    "use strict";
    var name = cname + "=";
    var ca = document.cookie.split(";");

    for (var i = 0; i < ca.length; i++) {
      var c = ca[i];

      while (c.charAt(0) === " ") {
        c = c.substring(1);
      }

      if (c.indexOf(name) !== -1) {
        return unescape(c.substring(name.length, c.length));
      }
    }

    return "";
  };

  var setPPID = function (uuid) {
    "use strict";
    console.info("Using uuid" + uuid + " as PPID.");
    googletag.pubads().setPublisherProvidedId(uuid);
  };

  var handlePPID = function () {
    "use strict";
    var uuidData = window.localStorage.getItem(_PUBCID_LS_KEY) || getCookie(_PUBCID_LS_KEY);
    if (uuidData && (/^([0-9a-zA-Z]{32,150})$|^([0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12})$/).test(uuidData)) {
      console.info("Found pubcid uuid data in proper format: " + uuidData);
      setPPID(uuidData);
      return;
    }
    var adloaderUuidData = window.localStorage.getItem(_PPID_LS_KEY) || getCookie(_PPID_LS_KEY);
    if (adloaderUuidData) {
      console.info("Found adloader uuid data: " + adloaderUuidData);
      setPPID(adloaderUuidData);
    } else {
      console.warn("No stored uuid data found for PPID. Generating and storing new uuid.");
      adloaderUuidData = generateUUID();
      window.localStorage.setItem(_PPID_LS_KEY, adloaderUuidData);
      setCookie(_PPID_LS_KEY, adloaderUuidData, 365);
      setPPID(adloaderUuidData);
    }
  };

  window.abcn.adloader.handlePPID = handlePPID;

}());

googletag.cmd.push(function() {
  var adloader = window.abcn.adloader;
  var slots = adloader.slots;
  var adUnit = adloader.adUnit;
  var targeting = adloader.targeting;
  var roadblock = adloader.roadblock;

  if (window.innerWidth < 1025) window.abcn.adloader.roadblock.ids = [];

  for (key in targeting) {
    if (targeting[key].length) {
      googletag.pubads().setTargeting(key, targeting[key]);
    }
  }

  googletag.pubads().setCentering(true);
  googletag.pubads().enableSingleRequest();
  googletag.pubads().disableInitialLoad();

  if (console.time) console.time('Audiencereport');
  window.apr('dfp-targeting-done', function() {
    if (console.time) console.timeEnd('Audiencereport');

    googletag.cmd.push(function() {
      var disablePubcid = localStorage.getItem('_pubcid_optout') || document.cookie.includes('_pubcid_optout');
      if (!disablePubcid && window.abcn.adloader.handlePPID) {
        window.abcn.adloader.handlePPID();
      }
      googletag.enableServices();
    });
    window.abcn.adloader.targetingDone = true;
    window.abcn.adloader.onTargetingDone.forEach(function(callback) {
      callback();
    });
  });
});
</script>

