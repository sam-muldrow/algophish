<script>
  (function(){
    window.addEventListener('K5A:GetArticleInfo', function (e) {
      var abcn = window.abcn || {};
      abcn.kilkayaToolbarActive = true;

      console.log('K5A:GetArticleInfo', e);
      if (typeof e.detail.ping !== "undefined"){
          //answer the ping call
          window.dispatchEvent(
            new CustomEvent('K5A:GetArticleInfoPing', 
                { detail: { ping:"ok" }}
            )
          );
          return;
      }
      var element = document.getElementById(e.detail.id);
      if (element){
          console.log('K5A:GetArticleInfo element', element);
          //fetch values we want to send to the AB test
          var headlines = element.getElementsByTagName("h3");
          var headline = "";
          if (headlines.length) {
              headline = headlines[0].textContent;
          }
          var image = "";
          var images = element.getElementsByTagName("img");
          if (images.length) {
              image = images[0].src;
          }
          var pretitle = "";
          var pretitleElement = element.querySelector('.df-style-siste');
          if (pretitleElement) {
            pretitle = pretitleElement.textContent;
          }
          
          window.dispatchEvent(new CustomEvent('K5A:ShowABTestView', { 
              detail: { 
                  pretitle: pretitle,
                  title: headline,
                  url: e.detail.url,
                  image: image,
                  placementId: e.detail.id,
                  mainUrl:"" //empty = use default value
              } 
          }));
      }
    }, false);

    var renderSlot = function(slotData) {
      var placementEl = document.getElementById(slotData.placementId);
      if (placementEl) {
        console.log('K5A executing abtest with data:', slotData, 'for:', slotData.placementId);
        var imgEl = placementEl.getElementsByTagName('img')[0];
        var headingEl = placementEl.getElementsByTagName('h3')[0];
        var pretitleEl = placementEl.querySelector('.df-style-siste');
        if (slotData.data.image && slotData.data.image !== '' && imgEl) {
            imgEl.src = slotData.data.image;
            imgEl.classList.remove('lazyload');
            imgEl.removeAttribute('data-src');
            imgEl.removeAttribute('data-srcset');
            imgEl.removeAttribute('srcset');
        }
        if (slotData.data.title && slotData.data.title !== '' && headingEl && headingEl.lastChild) {
            headingEl.lastChild.textContent = slotData.data.title;
        }
        if (slotData.data.color && slotData.data.color !== '') {
            placementEl.style.backgroundColor = slotData.data.color;
        }
        if (slotData.data.pretitle && pretitleEl) {
            pretitleEl.innerHTML = slotData.data.pretitle;
        }
        placementEl.setAttribute('data-k5a-nisa',slotData.testId);          
      }
    };

    document.addEventListener('K5A:NisaReady', function(evt) {
      console.log('K5A:NisaReady:', evt);
      if (evt.detail && evt.detail.nisa && evt.detail.nisa.getTests) {
        evt.detail.nisa.getTests().forEach(function(testData) {
          renderSlot(testData);
        });
        evt.detail.nisa.renderDone();
      }
    });
  })();
</script>



