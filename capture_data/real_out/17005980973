<script type="text/javascript">
	$(function() {
		$('#large-logo-link').show();
		$('#large-logo-image').css('display','inline-block');
		if ($.browser.msie){
			$('#large-logo-image').css('marginTop',0);
		}else{
			$('#large-logo-image').animate({
				width: 517,
				height: 312,
				marginTop: 0
			},
			{
				duration: 800,
				specialEasing: {
					width: 'easeInOutBounce',
					height: 'easeInOutBounce',
					marginTop: 'easeInOutBounce'
				}
			});
		}
		$(".click-to-front").click(function(){
			if ($(this).hasClass('bottom-screenshot')) {
				$(this).removeClass('bottom-screenshot');
				$(this).parent().find(".top-screenshot").removeClass('top-screenshot').addClass('bottom-screenshot');
				$(this).addClass('top-screenshot');
				
			}
			
			

		});
		$(".home-tool-link").click(function(){
			var groupClass = $(this).data('group');
			var targetId = $(this).data('target');
			$("."+groupClass).hide();
			$("#large-logo-link").hide();
			$("#"+targetId).show();
		});
		
		$(".info-close-btn").click(function(){
			var groupClass = $(this).data('group');
			$("."+groupClass).hide();
			$("#large-logo-link").show();
		});	
		
		$(window).on('scroll', function() {
			var offset = $(".paired-screenshot-wrapper").offset();
			var span_y = $(".paired-screenshot-wrapper").height();
			var starting_y = offset.top-span_y;
			span_y = span_y*2;
			var multiplier = 40;
			var scroll_val = $(window).scrollTop()-starting_y;
			var current_margin = $('.top-screenshot').css('margin-top');
			
			if (scroll_val < 0) {
				scroll_val = 0;
			}
			if (scroll_val > span_y) {
				scroll_val = span_y;
			}
			var new_margin = Math.floor((scroll_val / span_y) * multiplier) + "px";
			if (current_margin == new_margin) {
				var txt = '';
				if (scroll_val == 0) txt = " fixed low";
				if (scroll_val == span_y) txt = " fixed high";
		
				return;
			}
		    $('.top-screenshot').css('margin-top', new_margin);
		});
		
		
		
	});
</script>
</body>
</html>