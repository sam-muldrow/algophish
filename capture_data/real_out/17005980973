		<script>
		var selected_theme = null;
		
		
		var auto_theme_capable = false;
		var auto_theme_active = false;
		var auto_theme_prefer_dark = false;
		var auto_preference_changed = false;
		
		
	
		if (window.matchMedia) {
			auto_theme_capable = true;
		}
		if (window.matchMedia && window.matchMedia('(prefers-color-scheme: dark)').matches) {
			auto_theme_prefer_dark = true;
		}
		
		if (current_theme == 'auto') {
			var updated_theme = false;
			if ((current_theme_hint == 'light' && auto_theme_prefer_dark) || (current_theme_hint == 'dark' && auto_theme_prefer_dark == false)) {
				if (current_theme_hint == '') {
					if (auto_theme_prefer_dark) {
						current_theme_hint = 'dark';
					} else {
						current_theme_hint = 'light';
					}
					updated_theme = true;			
				} else if (current_theme_hint == 'light' && auto_theme_prefer_dark) {
					current_theme_hint = 'dark';
					updated_theme = true;
				} else if (current_theme_hint == 'dark' && auto_theme_prefer_dark == false) {
					current_theme_hint = 'light';
					updated_theme = true;
				}
				if (updated_theme) {
					var img = new Image();
					if (current_theme == 'auto') {
						img.src = './dash_dashboard.php?grip_theme='+current_theme+'&theme_hint='+current_theme_hint+'&img=1';
					} else {
						img.src = './dash_dashboard.php?grip_theme='+current_theme+'&img=1';
					}
				}
				//update_theme_selector();
			}
		}
		
			/* Test data: TODO: remove */
		//auto_theme_prefer_dark = true;
		//current_theme == 'auto'
		//current_theme_hint = '';
		/* end of test */
		
		
			
	
				
	</script>
		
	
	

	
	
	
		
	
