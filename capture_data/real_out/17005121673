n    font-weight: 500;\n    line-height: 12px;\n    margin-top: 8px;\n  }\n  .bsx-deal-section .hero-upper-text .hero-text .hero-heading .break-word{\n    display: none;\n  }\n}\n<\/style>","cloud_cta":"","download_cta":"","hide_cloud_btn":false,"hide_download_btn":false,"add_pkg_in_download":true,"download_cta_bg_color":"","download_cta_border_color":"","download_cta_bg_img":"","download_cta_text_alignment":"","download_cta_text_color":"","download_btn_shape":""}};
window.takeoverBannerData.show_hyper_link_UI = true;
if ((navigator.appVersion.indexOf("Mac") !== -1) && window.innerWidth > 767) {
  document.body.classList.add('mac-os');
  const bannerImage = document.querySelector('.hero-banner img');
  const gameLogo = document.querySelector('.game-logo img');
  const heroHeading = document.querySelector('.hero-heading');
  const cloudEnabledFlag = document.querySelector('.is-cloud-enabled');
  const redirectLink = document.querySelectorAll('.bsx-redirect-lnk');
  const heroVideoMac = document.querySelector('.bsx-video .hero-video.hide');
  const heroBanner = document.querySelector('.hero-banner');
  const heroVideo = document.querySelector('.bsx-video .hero-video');
  const downloadWrapper = document.querySelector('.download-wrapper-layer');
  const placeHolder = document.querySelector('.placeholder-for-extra-script');
  const hyperLinkElement = document.querySelector('.hyper-link');
  if (hyperLinkElement) {
    hyperLinkElement.style.display = 'none';
  }

  if (downloadWrapper && downloadWrapper.dataset['macApp']) {
    downloadWrapper.querySelector('.download-button').setAttribute('data-app',
      `{"package" : "${downloadWrapper.dataset['macApp']}"}`);
  }

  if (heroBanner && heroBanner.classList.contains('hide')) {
    heroBanner.classList.remove('hide');
    heroVideo.classList.add('hide');
  }

  if (heroVideo) {
    const macVideoSrc = heroVideo.querySelector('source').dataset['macSrc'];
    if (macVideoSrc && macVideoSrc !== '' && heroBanner) {
      heroVideo.classList.remove('hide');
      heroBanner.classList.add('hide');
      heroVideo.querySelector('source').dataset['src'] = macVideoSrc;
    }
  }

  if (bannerImage && bannerImage.src !== bannerImage.dataset['macBanner']) {
    bannerImage.src = bannerImage.dataset['macBanner'];
  }

  if (gameLogo && gameLogo.dataset['src'] !== gameLogo.dataset['macImgSrc']) {
    gameLogo.src = gameLogo.dataset['macImgSrc'];
    gameLogo.classList.remove('lazy');
  }

  if (heroHeading && heroHeading.innerText.trim() !== heroHeading.dataset['macHeading'].trim()) {
    heroHeading.innerHTML = `${heroHeading.dataset['macHeading']}`;
  }

  const cloudCTABtn = document.querySelector('.browser-cta');
  let macCTAString = '';
  if (cloudCTABtn) {
    macCTAString = cloudCTABtn.dataset['macCtaStr'];
  }

  if (cloudEnabledFlag) {
    cloudEnabledFlag.classList.add(cloudEnabledFlag.dataset['macCloudEnabled']);
    cloudEnabledFlag.querySelector('p').innerText = cloudEnabledFlag.querySelector('p').dataset['macStr'];
    if (macCTAString && cloudCTABtn) {
      cloudCTABtn.querySelector('.play-so-button').innerHTML = macCTAString;
    }
    if (cloudEnabledFlag.dataset['macCloudEnabled'] === 'not-cloud-enabled') {
      document.querySelectorAll('.green-tick use')[1].classList.remove('hide');
      document.querySelectorAll('.green-tick use')[0].classList.add('hide');
    } else {
      document.querySelectorAll('.green-tick use')[0].classList.remove('hide');
      document.querySelectorAll('.green-tick use')[1].classList.add('hide');
    }
  }

  redirectLink.forEach(item => {
    if (item.dataset['macHref'] && item.href !== item.dataset['macHref']) {
      item.href = item.dataset['macHref'];
    }
  });

  if (placeHolder && placeHolder.dataset['macScript']) {
    placeHolder.innerHTML = placeHolder.dataset['macScript'];
  }
}
