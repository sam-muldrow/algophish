<!-- Evento de analitycs 4-->
<script>
    function gtag_evento_select_item(pasaporte, price, currency) {
        gtag('event', 'select_item', {
            items: [{
                item_id: pasaporte,
                item_name: pasaporte,
                item_category: 'pasaportes',
                price: price,
                currency: currency,
                quantity: 1
            }],
            item_list_name: 'Pasaportes',
            item_list_id: 'pasaportes'
        });
        gtag('event', 'view_item', {
            items: [{
                item_id: pasaporte,
                item_name: pasaporte,
                item_category: 'pasaportes',
                price: price,
                currency: currency,
                quantity: 1
            }],
            value: price,
            currency: currency
        });
        return false;
    }

    function gtag_evento_add_payment_info(pasaporte, price, currency, payment_type) {
        gtag('event', 'add_payment_info', {
            currency: currency,
            items: [{
                item_id: pasaporte,
                item_name: pasaporte,
                item_category: 'pasaportes',
                price: price,
                currency: currency,
                quantity: 1
            }],
            payment_type: payment_type,
            value: price
        });
        return false;
    }

    function gtag_evento_begin_checkout(pasaporte, price, currency) {
        gtag('event', 'begin_checkout', {
            currency: currency,
            items: [{
                item_id: pasaporte,
                item_name: pasaporte,
                item_category: 'pasaportes',
                price: price,
                currency: currency,
                quantity: 1
            }],
            value: price
        });
        return false;
    }

    function gtag_evento_de_compra(pasaporte, price, currency, transaction_id) {
        gtag('event', 'purchase', {
            currency: currency,
            transaction_id: transaction_id,
            items: [{
                item_id: pasaporte,
                item_name: pasaporte,
                item_category: 'pasaportes',
                price: price,
                currency: currency,
                quantity: 1
            }],
            value: price,
        });
        return false;
    }
</script>    <!-- Event snippet for Page View conversion page -->
