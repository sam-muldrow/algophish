<div class="spacerInternasRR">
</div></div><div id="fusion-static-exit:html-block-f0f4nRJWVNFh4F4" style="display:none" data-fusion-component="html-block-f0f4nRJWVNFh4F4"></div><div id="fusion-static-enter:html-block-f0fseFESOrym70u" style="display:none" data-fusion-component="html-block-f0fseFESOrym70u"></div><div> <script type="text/javascript">
      // Aca agregamos el listener para los resize de los GNAFactories que pueden tener los
      // articulos importados desde Xalok o nuevos desarrollos que se hagan en ARC
      window.addEventListener("message", function (e) {
        var data = e.data;

        if (data.sentinel != 'amp') {
          return;
        }
        if (data.type == 'embed-size') {
          const iframes = document.querySelectorAll('iframe[src="' + data.location + '"]');
          iframes.forEach(function(iframe) {
            iframe.style.height = data.height + 'px';
          });
        }
        console.log('LCBA 5 Portada Listener', data);
      });
