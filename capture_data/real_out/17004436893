<script>
    function setActionGA(action) {
        sessionStorage.SessionName = "actionGA";
        sessionStorage.setItem("actionGA", action);
    }

    function handleClearInput(e) {
        inputWasCleared = !('data' in e)

        if (inputWasCleared) {
            e.target.blur();
            e.target.focus();
        }
    }
</script>

