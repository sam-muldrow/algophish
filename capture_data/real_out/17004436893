    let catalog_fiter_item_fit_control = 0;

    document.getElementById("catalog-fiter-item-fit-item").addEventListener('scroll', function(e) {
        if (catalog_fiter_item_fit_state.length > 0 && (e.target.scrollTop > (catalog_fiter_item_fit_control + e.target.clientHeight))) {
            catalog_fiter_item_fit_control = e.target.scrollTop;
            for (let i = 0; i < 20; i++) {
                let itemToAdd = document.createElement('div');
                itemToAdd.innerHTML = catalog_fiter_item_fit_state.shift();
                e.target.append(...itemToAdd.children);
                delete itemToAdd;
            }
        }
    })

    document.getElementById("catalog-fiter-item-fit-item").fetchAll = function() {
        while (catalog_fiter_item_fit_state.length > 0) {
            let itemToAdd = document.createElement('div');
            itemToAdd.innerHTML = catalog_fiter_item_fit_state.shift();
            this.append(...itemToAdd.children);
            delete itemToAdd;
        }
    }
