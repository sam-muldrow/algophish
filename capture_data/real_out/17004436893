        categList = [''],
        categList = categList[0].split(',');
        categList.length > 1 ?
            function(){
                for (var i = 1; i < categList.length; i++) {
                    apiOutput.push(categList[i])
                }
                return apiOutput
            }()
            : apiOutput = categList
        DY.recommendationContext = {type:'CATEGORY', data: apiOutput};
