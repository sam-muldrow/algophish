<accordion-item
    title="Modelagem"
    data-accordion-content="padding-top:0px;"
    data-facet="modelagem"
    open="">
    <div slot="content" class="">
                <ul class="catalog-filter-list" id="catalog-fiter-item-fit-item">
    <li class="catalog-filter-list-item " data-value="Acinturada">
        <a href='https://www.dafiti.com.br/outlet-ofertas-descontos-promocao/?discount=19.5-95&q=63efd8ddc4a31f5f49190474&sp=1&fit=Acinturada' class='catalog-filter-list-link  '>
        <i class="catalog-filter-list-link-icon icon-check-sharp show-mobile-inline"></i>
        <span class="catalog-filter-list-link-text">Acinturada</span>    </a>
    </li>
<li class="catalog-filter-list-item " data-value="Ampla">
        <a href='https://www.dafiti.com.br/outlet-ofertas-descontos-promocao/?discount=19.5-95&q=63efd8ddc4a31f5f49190474&sp=1&fit=Ampla' class='catalog-filter-list-link  '>
        <i class="catalog-filter-list-link-icon icon-check-sharp show-mobile-inline"></i>
        <span class="catalog-filter-list-link-text">Ampla</span>    </a>
    </li>
<li class="catalog-filter-list-item " data-value="Biquíni">
        <a href='https://www.dafiti.com.br/outlet-ofertas-descontos-promocao/?discount=19.5-95&q=63efd8ddc4a31f5f49190474&sp=1&fit=Biqu%C3%ADni' class='catalog-filter-list-link  '>
        <i class="catalog-filter-list-link-icon icon-check-sharp show-mobile-inline"></i>
