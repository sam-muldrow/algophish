        if (catalog_fiter_item_heel_height_state.length > 0 && (e.target.scrollTop > (catalog_fiter_item_heel_height_control + e.target.clientHeight))) {
            catalog_fiter_item_heel_height_control = e.target.scrollTop;
            for (let i = 0; i < 20; i++) {
                let itemToAdd = document.createElement('div');
                itemToAdd.innerHTML = catalog_fiter_item_heel_height_state.shift();
                e.target.append(...itemToAdd.children);
                delete itemToAdd;
            }
        }
    })

    document.getElementById("catalog-fiter-item-heel-height-item").fetchAll = function() {
        while (catalog_fiter_item_heel_height_state.length > 0) {
            let itemToAdd = document.createElement('div');
            itemToAdd.innerHTML = catalog_fiter_item_heel_height_state.shift();
            this.append(...itemToAdd.children);
            delete itemToAdd;
        }
    }
