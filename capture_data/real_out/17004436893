    <div id="div-scroll"></div>
</div>

<script type="text/cjs">
    const buttons_scroll = [...document.querySelectorAll(".scroll-products")];
    let links = [];

    buttons_scroll.map((button) => {
        button.addEventListener("click", clickHandler);
        links.push(button.getAttribute("href"));
    });

    function clickHandler(e) {
        // e.preventDefault();
        const attribute = this.getAttribute("data-section");
        const offsetTop = document.querySelector(`#${attribute}`).offsetTop;
        document.documentElement.scrollTop = offsetTop;
        scroll({
            top: offsetTop - 150,
            behavior: "smooth",
        });
        activeButton();
    }

    function activeButton() {
        setTimeout(() => {
            const url_pathname = `${window.location.pathname}${window.location.search}`;
            links.includes(url_pathname) &&
                document
                    .querySelector(`.filters-choice [href='${url_pathname}']`)
                    .classList.add("active");
        }, 500);
    }

    window.addEventListener("load", activeButton());
</script>            </div>
        </div>
    
    <div class="clearfix">
        <div class="row">
            <div class="col-md-7 col-xs-12 catalog-seo-h1 hide-mobile">
                            </div>
            <div class="col-md-5 col-xs-12">
                                    <div data-catalog="ajax" data-catalog-filter-top>
                            <div class="l-catalog-header selection-options-top clearfix">
                
        <div class="items-products select-options-item ">
    <span class="value">15383</span> Produtos</div>
                            <div class="items-per-row select-options-item item-per-row-desk hide-mobile" data-target=".main-list">
                <div class="popover-hover-top dark selection-options-popover">
