<script>
    let catalog_fiter_item_waist_state = [];
    let catalog_fiter_item_waist_control = 0;

    document.getElementById("catalog-fiter-item-waist-item").addEventListener('scroll', function(e) {
        if (catalog_fiter_item_waist_state.length > 0 && (e.target.scrollTop > (catalog_fiter_item_waist_control + e.target.clientHeight))) {
            catalog_fiter_item_waist_control = e.target.scrollTop;
            for (let i = 0; i < 20; i++) {
                let itemToAdd = document.createElement('div');
                itemToAdd.innerHTML = catalog_fiter_item_waist_state.shift();
                e.target.append(...itemToAdd.children);
                delete itemToAdd;
            }
        }
    })

    document.getElementById("catalog-fiter-item-waist-item").fetchAll = function() {
        while (catalog_fiter_item_waist_state.length > 0) {
            let itemToAdd = document.createElement('div');
            itemToAdd.innerHTML = catalog_fiter_item_waist_state.shift();
            this.append(...itemToAdd.children);
            delete itemToAdd;
        }
    }
