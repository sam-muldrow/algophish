    if("cartpage" == et){
        var oKeys = Object.keys(JSON.parse(''));
        for(x in oKeys) {
            apiOutput.push(oKeys[x].split('-')[0]);
        }
        DY.recommendationContext = {type:'CART', data: apiOutput};
    }
    if("landingpage" == et){
        DY.recommendationContext = {type:'CATEGORY'};
    }
</script>

