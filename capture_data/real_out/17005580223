function isNotCartPage() {
var mainBody = document.getElementById('mainBody');
if (window.location.href.indexOf('viewcart') == -1 && mainBody) {
mainBody.className += ' nonCartBody';
return true;
} else {
return false;
}
}
isNotCartPage();
window.addEventListener('DOMContentLoaded', (event) => {
try {
var searchIcon = document.querySelector("#hdr-search__icon svg"),
searchContainer = document.getElementById("header__SearchContainer"),
searchClose = document.getElementById("forSearch-close"),
searchBoxMobile = document.getElementById("txtSearch"),
searchBox = document.querySelector(".searchBar-desktop");
searchIcon.addEventListener("click", function () {
searchIcon.style.display = "block";
searchContainer.style.display = "none";
searchBox.style.display = "none";
if (searchBox.style.display == "none") {
searchContainer.style.display = "block";
searchBoxMobile.focus();
searchIcon.style.display = "none";
searchClose.style.display = "none";
} else {
searchContainer.style.display = "none";
}
});
} catch(e) {
console.log('Search UI Error', e)
}
});
