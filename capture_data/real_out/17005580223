<script type="text/javascript">
/**/
window.addEventListener('DOMContentLoaded', () => {
let dropMenu = document.querySelectorAll('.dropmenu-title');
let dropMenuList = document.querySelectorAll('.dropmenu');
const isMobile = () => window.outerWidth < 1008;
$('.dropmenu').on('click', function(e) {
if (e.target.nodeName !== 'A') {
if ($('.dropmenu-list', this).hasClass('over')) {
$('.dropmenu-list', this).removeClass('over');
} else {
$('.dropmenu-list', this).addClass('over');
}
}
})
dropMenuList.forEach(dropMenuContainer => {
['mouseleave'].forEach( evt =>
dropMenuContainer.addEventListener(evt, (e) => {
try {
const clickedDiv = e.target;
if (clickedDiv) clickedDiv.classList.remove('selected');
if (clickedDiv.children[1]) clickedDiv.children[1].classList.remove('over');
} catch(e) {
console.log('Menu mouseleave/click error')
}
}, false)
);
});
dropMenu.forEach(menuName => {
['mouseenter'].forEach( evt =>
menuName.addEventListener(evt, (e) => {
try {
const dropMenuClass = e.target.nextElementSibling;
e.target.parentNode.classList.add('selected');
if (!isMobile()) dropMenuClass.classList.add('over');
const selectedMenu = document.querySelector('.dropmenu.selected');
selectedMenu.onclick = e => {
if (e.target.classList.contains('selected')) {
e.target.classList.remove('selected');
if (e.target.children[1]) e.target.children[1].classList.remove('over');
}
};
} catch(e) {
console.log('Menu mouseenter error');
}
}, false)
);
});
});
/**/
</script>
