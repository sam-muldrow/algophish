var f = (evt.currentTarget.id == "hdr_logIn") ? com.art.core.components.LoginModal.LOGIN : com.art.core.components.LoginModal.REGISTER;
var g = new com.art.core.utils.Note(MyGalleriesCore.events.SHOW_GLOBAL_LOGINMODAL,{
