<script src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.1/moment.min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.1/moment-with-locales.min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/moment-timezone/0.5.39/moment-timezone-with-data-1970-2030.min.js"></script>
<script type="text/javascript">
/**/
function getCookie(name) {
const value = `; ${document.cookie}`;
const parts = value.split(`; ${name}=`);
if (parts.length === 2) return parts.pop().split(';').shift();
}
function getParameterByName(name, url = window.location.href.toLowerCase()) {
name = name.replace(/[\[\]]/g, '\\$&');
var regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)'),
results = regex.exec(url);
if (!results) return null;
if (!results[2]) return '';
return decodeURIComponent(results[2].replace(/\+/g, ' '));
}
const aid=[809692559,423816591,1799548303,1413672335,1027796367,641920399,256044431,2017652111,1631776143,1245900175,860024207,474148239,88272271,1578757519,35253647,1796861327,1410985359,1025109391,253357455,1583441169,1586128145,1580754193,1633772817,1636459793,1639146769,1801544977,1804231953,1806918929,92955921,809692559,423816591,1799548303,31120758,62522707,51997496,51040872,13284987,60122738,32454944,22832007,54248177,39082181,48567013];
var hideCDT=false;
var href= document.location.href;
//find cookie aid
var aidValue=getCookie('UTEPixelId');
//find queryString aid in lowercase
var aidToCheck=getParameterByName('aid') || aidValue
if(aidToCheck){
for(const index in aid) {
if(parseInt(aidToCheck)===aid[index]){
hideCDT=true;
}
}
}
function pad(number) {
return (number < 10 ? '0' : '') + number;
}
function getTimeRemaining(enddate) {
const timezone = 'America/Los_Angeles';
const expireDate = moment.tz(enddate, 'MMM DD YYYY HH:mm:ss', timezone);
const now = moment(moment.tz(moment(), timezone).format());
let days = 0;
let hours = 0;
let minutes = 0;
let seconds = 0;
let totalDays=0;
if (now.isBefore(expireDate)) {
totalDays = moment.duration(expireDate.diff(now))._data;
days = pad(totalDays.days);
hours = pad(totalDays.hours);
minutes = pad(totalDays.minutes);
seconds = pad(totalDays.seconds);
}
return {
totalDays,
days,
hours,
minutes,
seconds
};
}
function initializeClock(id, endtime) {
const banner = document.getElementById('cdt-banner');
const clock = document.getElementById(id);
const daysSpan = clock.querySelector(".days");
const hoursSpan = clock.querySelector(".hours");
const minutesSpan = clock.querySelector(".minutes");
const secondsSpan = clock.querySelector(".seconds");
function updateClock() {
const dateArray = getTimeRemaining(endtime);
daysSpan.innerHTML = dateArray.days;
hoursSpan.innerHTML = dateArray.hours;
minutesSpan.innerHTML = dateArray.minutes;
secondsSpan.innerHTML = dateArray.seconds;
if (dateArray.totalDays <= 0) {
clock.style.display = "none";
banner.style.display="none";
window.clearInterval(timeinterval);
} else {
banner.style.display="flex";
}
}
updateClock();
const timeinterval = window.setInterval(updateClock, 59000);
}
window.addEventListener('DOMContentLoaded', () => {
const timezone = 'America/Los_Angeles';
const runTimer = true; // Enable/Disable Timer
const expireDate = moment.tz('Dec 19 2022 14:00:00', 'MMM DD YYYY HH:mm:ss', timezone);
const now = moment(moment.tz(moment(), timezone).format());
if (runTimer && !hideCDT && expireDate.unix() >= now.unix()) {
console.log('Show Countdown Timer');
if (document.getElementById('cdt-banner').classList.contains('hide')) {
document.getElementById('cdt-banner').classList.remove('hide');
}
initializeClock('flashSaleTimer', expireDate);
const banner = document.getElementById('cdt-banner');
banner.classList.remove('sticky');
window.addEventListener('scroll', () => {
const mobileBreak = window.innerWidth < 768
if (!mobileBreak && window.scrollY > 48 || mobileBreak && window.scrollY > 82) {
banner.classList.add('sticky');
} else {
banner.classList.remove('sticky');
}
});
} else {
console.log('Show Promo Banner');
document.getElementById('cdt-banner').classList.add('hide');
if(document.getElementById('skinny-banner-container')) {
document.getElementById('skinny-banner-container').style.display = 'block';
}
}
});
/**/
</script>
<style>
/**/

/**/
.type-ahead-category,a.type-ahead-category:hover{text-decoration:none}#type-ahead-mount{width:100%;max-width:284.75px;position:absolute;top:-1.75px;border-radius:0 0 2px 2px;border-color:silver;border-style:solid;border-width:0 1px 1px;background-color:#fff;z-index:1501}#type-ahead-wrapper{width:670px;margin:0 auto;position:relative}.type-ahead-category{background-color:#fff;color:#000;display:-ms-flexbox;display:flex;-ms-flex-direction:column;flex-direction:column;cursor:pointer;padding:10px 7.5px;text-align:left}.type-ahead-category:not(:last-child){border-bottom:1px solid #f1f1f1}.type-ahead-category:focus{background-color:#e0dede}.type-ahead-category:hover{color:initial;background-color:#f3f3f3;outline:0}.type-ahead-category-url{display:none}.type-ahead-category-count{color:#6d6d6d;font-family:Apercu;font-size:13px;line-height:15px}.type-ahead-category-name{color:#181818;font-family:Apercu;font-size:15px;line-height:18px}input:focus{outline:0}@media only screen and (max-width:1008px){#type-ahead-wrapper{width:96%;top:33px}#type-ahead-mount{max-width:initial;max-height:196px;overflow-y:scroll}}@media screen and (max-width:845px){#type-ahead-wrapper{width:95%}}@media screen and (max-width:680px){#type-ahead-wrapper{width:94%}}@media screen and (max-width:570px){#type-ahead-wrapper{width:93%}}@media screen and (max-width:491px){#type-ahead-wrapper{width:92%}}@media screen and (max-width:432px){#type-ahead-wrapper{width:91%}}@media screen and (max-width:386px){#type-ahead-wrapper{width:89%}}
/*]]*/

/**/
</style>
<script>
/**/
function TypeAhead(
containerId,
searchInputId,
zoneId,
langId,
categoryMaxCount
) {
this.containerElement =
document.getElementById(containerId) ||
document.getElementsByClassName(containerId);
this.searchInputElement = document.getElementById(searchInputId);
this.apiEndpoint = "https://bizapi.art.com/v2/typeahead";
this.categoryMaxCount = categoryMaxCount || 10;
this.zoneId = zoneId || 1;
this.langId = langId || 1;
this.inputValue = "";
this.minCharacterLength = 0;
}
TypeAhead.prototype.getWrapperElement = function() {
return document.getElementById("type-ahead-wrapper");
};
TypeAhead.prototype.getMountElement = function() {
return document.getElementById("type-ahead-mount");
};
TypeAhead.prototype.getContainerElement = function() {
if (this.containerElement.length) {
return window.innerWidth > 530
? this.containerElement[0]
: this.containerElement[1];
}
return this.containerElement;
};
TypeAhead.prototype.getSearchInputElement = function() {
return this.searchInputElement;
};
TypeAhead.prototype.getZoneId = function() {
return this.zoneId;
};
TypeAhead.prototype.getLangId = function() {
return this.langId;
};
TypeAhead.prototype.setInputValue = function(value) {
return (this.inputValue = value);
};
TypeAhead.prototype.getInputValue = function() {
return this.inputValue;
};
TypeAhead.prototype.getAPIEndpoint = function() {
return this.apiEndpoint;
};
TypeAhead.prototype.getCategoryMaxCount = function() {
return this.categoryMaxCount;
};
TypeAhead.prototype.createTypeAheadElement = function() {
var wrapper = document.createElement("div");
wrapper.id = "type-ahead-wrapper";
var mount = document.createElement("div");
mount.id = "type-ahead-mount";
wrapper.appendChild(mount);
this.getContainerElement().appendChild(wrapper);
};
TypeAhead.prototype.removeTypeAheadElement = function() {
if (this.getMountElement()) {
this.getContainerElement().removeChild(this.getWrapperElement());
}
};
TypeAhead.prototype.getURL = function() {
return (
this.getAPIEndpoint() +
"?q=" +
encodeURIComponent(this.getInputValue()) +
"&categoryMaxCount=" +
this.getCategoryMaxCount() +
"&zoneId=" +
this.getZoneId() +
"&langId=" +
this.getLangId()
);
};
TypeAhead.prototype.fetchData = function(cb) {
var xhr = new XMLHttpRequest();
var parseData = this.parseData;
var apiUrl = "";
if (this.getInputValue().length > 2){
apiUrl = this.getURL();
}
xhr.onreadystatechange = function() {
if (xhr.readyState == XMLHttpRequest.DONE) {
cb(JSON.parse(xhr.responseText));
}
};
if(apiUrl != ''){
xhr.open("GET", apiUrl, true);
xhr.send();
}
};
TypeAhead.prototype.appendCategory = function(category) {
if (!category) {
return;
}
var child = document.createElement("a");
child.className = "type-ahead-category";
var categoryName = category.name.toLowerCase();
var inputValue = this.getInputValue().toLowerCase();
var destinationUrl = category.destinationUrl + (category.destinationUrl.indexOf('?') >= 0 ? '&' : '?') + 'SSK=' + encodeURIComponent(categoryName);
child.href = destinationUrl;
categoryName = categoryName.replace(inputValue, "<b>" + inputValue + "</b>");
child.innerHTML =
'<div class="type-ahead-category-name trendingsearch_text">' +
categoryName +
'</div><div class="type-ahead-category-count">' +
category.itemCount.toLocaleString() +
" items" +
'</div><div class="type-ahead-category-url">' +
category.destinationUrl +
"</div>";
if (window.superGal) {
const typeaheadElement = this;
child.addEventListener("click", function() {
typeaheadElement.removeTypeAheadElement();
event.preventDefault();
event.stopPropagation();
window.goToCategory($(this));
})
}
this.getMountElement().appendChild(child);
};
var categoryCount = 0;
TypeAhead.prototype.parseData = function(data) {
if (data) {
if (!this.getMountElement()) {
this.createTypeAheadElement();
} else {
this.getMountElement().innerHTML = "";
}
if (data.category) {
data.category.forEach(this.appendCategory.bind(this));
}
}
};
TypeAhead.prototype.handleSearchInput = function(event) {
try {
categoryCount = 0;
this.setInputValue(event.target.value);
var inputValue = this.getInputValue();
if (inputValue.length < this.minCharacterLength) {
this.removeTypeAheadElement();
}
if (inputValue.length >= this.minCharacterLength && (inputValue.length != 1 && inputValue.length != 2)) {
this.fetchData(this.parseData.bind(this));
}
if(inputValue.length == 1 || inputValue.length == 2){
this.removeTypeAheadElement();
}
} catch (error) {
console.log(error.message);
}
};
TypeAhead.prototype.handleResize = function() {
var wrapper = this.getWrapperElement();
var container =
window.innerWidth < 530
? this.containerElement[1]
: this.containerElement[0];
if (wrapper && !container.contains(wrapper)) {
container.appendChild(wrapper);
}
var headerSearchBar = document.getElementById("hdr-search-bar");
if (window.innerWidth > 530) {
headerSearchBar.style.display = "none";
}
if (window.innerWidth < 530) {
headerSearchBar.style.display = "";
document.getElementById("hdr-search-icon").style.display = "none";
}
};
TypeAhead.prototype.attachHandlersToSearchInput = function(element) {
element.addEventListener(
"keypress",
function(event) {
if (event.keyCode === 13) {
this.removeTypeAheadElement();
}
}.bind(this)
);
element.oninput = this.handleSearchInput.bind(this);
element.onclick = this.handleSearchInput.bind(this);
};
TypeAhead.prototype.initialize = function() {
window.callAjaxAutoSuggest = function() {};
window.AutoSuggest = function() {};
window.onclick = function() {
this.removeTypeAheadElement();
}.bind(this);
this.attachHandlersToSearchInput(this.getSearchInputElement());
if (window.location.hostname.indexOf("art") >= 0) {
this.attachHandlersToSearchInput(
document.getElementById("search_text_bar")
);
window.onresize = this.handleResize.bind(this);
}
};
window.setTimeout(function() {
var typeahead = new TypeAhead('header__SearchContainer', 'txtSearch');
typeahead.initialize();
},1000);
/**/
</script>
<script type="text/javascript">
/**/
/* Klaviyo on Account Create */
window.addEventListener('DOMContentLoaded', (event) => {
if (typeof window.MyGalleriesEnvironmentVariables.KlaviyoAcquisitionSource == 'undefined' && typeof window.MyGalleriesEnvironmentVariables.KlaviyoAcquisitionSourceSignup == 'undefined' && typeof window.MyGalleriesEnvironmentVariables.KlaviyoSubscribeURL == 'undefined') {
$('body').on('click', '.createAccountModal', function() {
try {
var email = $('#regEmail').val();
var domain = window.location.hostname.split('.').reverse()[1] + "." + window.location.hostname.split('.').reverse()[0];
var isProd = window.location.hostname.split('.')[0].toString() === 'www'
var host = (!isProd) ? 'qa-' : ''
$.ajax({
url: "https://"+host+"bizapi.art.com/v1/email/subscribe",
type: "POST",
contentType: "application/json",
data: JSON.stringify({
acquisitionSource: "00-000082",
domain: domain,
email: email
}),
dataType: "json",
success: function (response) {
console.log(response)
},
error: function (error) {
console.log(error)
}
});
} catch(e) {
console.log(e)
}
})
}
})
