<script>


$(function() {
// default nav file
$('#skinny-banner-container').prependTo('body');
setTimeout(function() {
$(window).scroll(function() {
var skinnyBanner = $('#skinny-banner-container');
$(skinnyBanner).ready(function() {
var pos = skinnyBanner.position();
var windowpos = $(window).scrollTop();
var skinnyBannerHeight = skinnyBanner.outerHeight();
function isMobileScreenSize() {
if ((screen.width < 768) || ($(window).width() < 768)) {
return true;
} else {
return false;
}
}
//when the window equals the
if (windowpos >= skinnyBannerHeight) {
if (!isMobileScreenSize()) {
skinnyBanner.addClass('fixedTopSkinny');
$('body').css('padding-top', skinnyBannerHeight + 'px');
}
} else {
skinnyBanner.removeClass('fixedTopSkinny');
$('body').css('padding-top', '0');
};
});
});
}, 1000);
});
setTimeout(function() {
try {
var sbObj = $('#skinnybanner_single');
var params = $('.sbID', sbObj).text().split('###');
var promoURL = params.map(function(url) {
// Filter by PromoURL (remove case sensitivity)
if (url.split(':')[0].toLowerCase() == 'promourl') return url.split(':')[1];
});
sbObj.click(function() {
// Reset all events on skinnybanner
return false;
});
sbObj.on('click', function(e) {
var isViewDetailsEvent = (e.target.className == 'viewDetails') ? true : false;
var isBannerSubTextEvent = (e.target.className == 'bannerSubText') ? true : false;
e.preventDefault();
if(promoURL[0] && (!isViewDetailsEvent && !isBannerSubTextEvent)) {
// Entire banner goes to url
window.location.href = encodeURI(promoURL[0]);
}
if(isViewDetailsEvent || isBannerSubTextEvent) {
// if view details, show modal but do not go to url
$('#banner-terms').slideDown('fast');
}
return false;
});
} catch(e) {
console.log(e);
}
}, 100);


</script>

