<script type="text/javascript">
/**/
window.addEventListener('DOMContentLoaded', function() {
var cookieObj = document.cookie.split('; ')
var isCanadianUser = cookieObj.filter(function(item) {
return item.split('=')[0] === 'ca_ip'
})
if (isCanadianUser.length !== 0) {
$('#exclusive-offers').hide();
} else {
var isProd = window.location.hostname === 'www.allposters.com'
var devPrefix = !isProd ? 'qa-' : ''
var subscribe = "/v1/email/subscribe"
$('.footer-form__submit').on('click', function(e) {
e.preventDefault()
sendForm()
})
$('#footer-form__input').on('click', function() {
$('#footer-form .copy.copy__error').hide();
})
$('.footer-form__close').on('click', function() {
$(this).parent().addClass('hide')
$('.footer-form__terms span').show();
$('.footer-form__input').val('');
})
$('.footer-form__popup').on('click', function(e) {
e.preventDefault();
var monitorWidth = window.screen.availWidth;
var centerWidth = parseInt(Number(monitorWidth) / 2) - 250 || 0
var linkURL = $(this).attr('href');
window.open(linkURL, '_new', `location=1,top=100,left=${centerWidth},width=500,height=450`)
})
function sendForm() {
var emailValue = $('.footer-form__input').val();
var pattern = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
var validTest = pattern.test(emailValue);
if (emailValue == "") {
$('#NLError02_I0').hide();
$('#NLError01_I0').show();
} else {
$('#NLError01_I0').hide();
if (validTest) {
var payload = {
"acquisitionSource": "04-000001",
"domain": "allposters.com",
"email": emailValue
}
$.ajax({
method: 'POST',
url: `https://${devPrefix}bizapi.art.com${subscribe}`,
data: JSON.stringify(payload),
contentType: "application/json",
dataType: 'json',
success: function(resp) {
$('#footer-form__sent').removeClass('hide');
$('.footer-form__terms span').hide();
document.cookie = `email_md5=${resp.emailMd5}`
},
error: function(error) {
$('.footer-form .copy__error').hide();
$('#NLError03_I0').show();
}
})
return true
} else {
$('#NLError02_I0').show();
}
}
return false
}
}
})
/**/
</script>
