    border-top: 1px solid rgba(255,255,255,.8);
    width: 100%;
    height: 50%;
    background: -webkit-linear-gradient(top,rgba(255,255,255,.7) 0,rgba(255,255,255,.2) 100%);
    border-radius: 10px 10px 12px 12px;
  }
</style>
<script type="text/javascript">
  jQuery(document).ready(function(){
    if(window.innerWidth <= 989)
    {
      if(jQuery.cookie("sb-closed")!="true"){
        //jQuery('#smartbanner').css("display","block");
        //jQuery('#smartbanner').addClass("show");
        
        Checkdevice();
        //jQuery('.mainDiv').css("top","76px");
      }
      else{
        jQuery('#smartbanner').css("visibility","hidden");
      	jQuery('#smartbanner').addClass('hide');
        jQuery('#iossmartbanner').css("visibility","hidden");
        jQuery('#iossmartbanner').addClass('hide');
      }
    }
    else{
      jQuery('#smartbanner').css("display","none");
      jQuery('.mainDiv').css("top","0px");
    }
    jQuery('.sb-close').click(function(){
      CreateCookie();
      jQuery('#smartbanner').removeClass('show');
	  jQuery('#smartbanner').addClass('hide');
      jQuery('#iossmartbanner').removeClass('show');
	  jQuery('#iossmartbanner').addClass('hide');
      jQuery('.mainDiv').css("top","0px");
    });
    //Hide banner in mobile app start
    var urlParams = new URLSearchParams(window.location.search);
    if (urlParams.has("from_app")) {
      if (urlParams.get("from_app") === "1") {
        jQuery('#smartbanner').css("visibility","hidden");
        jQuery('#smartbanner').addClass('hide');
        jQuery('#iossmartbanner').css("visibility","hidden");
        jQuery('#iossmartbanner').addClass('hide');
        jQuery.cookie("from_app", true);
      }
    }
    if(jQuery.cookie("from_app")){
      jQuery('#smartbanner').css("visibility","hidden");
      jQuery('#smartbanner').addClass('hide');
    }
    //Hide banner in mobile app end
  });
  function Checkdevice(){
    var isIOS = /iPad|iPhone|iPod|Macintosh/.test(navigator.userAgent);
    if(isIOS){
      jQuery('#smartbanner').css('display','none');
      jQuery('#iossmartbanner').css('display','block');
      jQuery('#iossmartbanner').addClass("show");
    }
    else{
      jQuery('#smartbanner').addClass("show");
      jQuery('#iossmartbanner').css('display','none');
      jQuery('#smartbanner').css('display','block');
    }
  }
  function CreateCookie() {
    var date = new Date();
    var m = 60*24;
    date.setTime(date.getTime() + (m * 60 * 1000));
    jQuery.cookie("sb-closed", "true", { path: '/',expires: date });
  }
</script>
