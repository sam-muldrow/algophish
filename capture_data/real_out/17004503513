	//今日关注选项卡
	$(".news-nav li").click(function(){
		$(this).addClass("active").siblings("li").removeClass("active");
		var index=$(this).index();
		$(".news_list").eq(index).show().siblings(".news_list").hide();
	})
	var newList=$(".news_list");
	newList.each(function(){
		var bigImg=$(this).find(".news_big_img img");//左侧大图
		var bigImgText=$(this).find(".news_big_img .news_line");//大图标题
		var tab=$(this).find(".news_num li")//右侧列表
		tab.mouseover(function(){
			//$(this).addClass("active").siblings("li").removeClass("active");	
			var imgSrc=$(this).find(".news_num_img img").attr("src");
			var text=$(this).find("a").text();
			bigImg.attr("src",imgSrc)
			bigImgText.text(text)
		})
	})
