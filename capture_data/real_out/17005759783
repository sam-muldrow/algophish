<script>
  var date = new Date();

  var dayOfMonth = date.getDate();
  var dayOfWeek = date.toLocaleDateString('default', { weekday: 'long' });
  var month = date.toLocaleDateString('default', { month: 'long' });
  var year = date.getFullYear();

  var diaSemanaInner = document.getElementById("diaSemana");
  var dia = document.getElementById("dia");
  var mes = document.getElementById("mes");
  var ano = document.getElementById("ano");

  diaSemanaInner.innerHTML = dayOfWeek;
  dia.innerHTML = dayOfMonth;
  mes.innerHTML = month;
  ano.innerHTML = year;
</script>
