<script id="navegg" type="text/javascript">
    (function(n, v, g) {
        o = "Navegg";
        if (!n[o]) {
            a = v.createElement('script');
            a.src = g;
            b = document.getElementsByTagName('script')[0];
            b.parentNode.insertBefore(a, b);
            n[o] = n[o] || function(parms) {
                n[o].q = n[o].q || [];
                n[o].q.push([this, parms])
            };
        }
    })
