<script>
  fetch("https://obuxixo.correiobraziliense.com.br/wp-json/wp/v2/posts?tags=1331&per_page=3&_orderby=date&_order=desc")
    .then((response) => response.json())
    .then((data) => {
      const standards = document.querySelector("#standards");

      data.forEach((item, i) => {
        const content = item.content.rendered;
        const tempElement = document.createElement('div');
        tempElement.innerHTML = content;

        const imgElements = tempElement.querySelectorAll('img');
        let imageUrl;

        if (i === 0 && imgElements.length >= 2) {
          // Para o primeiro item, pegue a segunda imagem (Ã­ndice 1)
          imageUrl = imgElements[1].getAttribute('src');
        } else if (imgElements.length >= 1) {
          // Para os outros itens, pegue a primeira imagem (Ã­ndice 0)
          imageUrl = imgElements[0].getAttribute('src');
        }

        if (imageUrl) {
          const imageElement = document.createElement("img");
          imageElement.src = imageUrl;
          imageElement.classList.add("newsImage");

          const title = item.title.rendered;
          const link = item.link;

          if (i === 0) {
            standards.insertAdjacentHTML(
              "beforeend",
              `
            <article class="cardStandard">
              <a class="news" href="${link}" target="_blank">
                <picture>
                  ${imageElement.outerHTML}
                </picture>
                <small>Obuxixo</small>
                <h2>${title}</h2>
              </a>
            </article>
          `
            );
          } else {
            standards.insertAdjacentHTML(
              "beforeend",
              `
            <article class="cardStandard">
              <a class="news" href="${link}" target="_blank">
                <picture>
                  ${imageElement.outerHTML}
                </picture>
                <small>Obuxixo</small>
                <h2>${title}</h2>
              </a>
            </article>
          `
            );
          }
        }
      });
    })
    .catch((error) => {
      console.error("Erro ao buscar e processar os dados:", error);
    });
</script>


