        <script>
          fetch(`https://www.correiobraziliense.com.br/_conteudo/jsons/menu-topo-2.json`).then((response) => {
            response.json().then((data) => {
              const ul = document.querySelector("#ulNav");
              for (i = 0; i < data.matia.length; i++) {
                let link = data.matia[i].ds_site == "Concursos" ? data.matia[i].link : "https://wwww.correiobraziliense.com.br${data.matia[i].link}"; 
                // console.log(data.matia[i].ds_site);
                if (data.matia[i].ds_site == "Concursos"){
                  ul.insertAdjacentHTML(
                    "beforeend",
                    `
                    <li><a href="${data.matia[i].link}" >${data.matia[i].ds_site}</a></li>
                    `
                  );
                }
                else {
                  ul.insertAdjacentHTML(
                    "beforeend",
                    `
                    <li><a href="https://www.correiobraziliense.com.br${data.matia[i].link}">${data.matia[i].ds_site}</a></li>
                    `
                  );
                }
              }
            });
          });
        </script>
