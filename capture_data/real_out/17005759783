  </section><script>
fetch("https://www.correiobraziliense.com.br/_conteudo/jsons/ultimas-vitrine.json").then((response) => {
  response.json().then((data) => {
    const standards = document.querySelector("#standards");
    const horizontais = document.querySelector("#horizontais");

    for (i = 0; i <= 3; i++) {
      if (i <= 1) {
        horizontais.insertAdjacentHTML(
          "beforebegin",
          `
            <article class="cardStandard">
              <a class="news" href="${data.matia[i].link}" target="_blank">
                <picture>
                  <img src="${data.matia[i].img}" class="newsImage" alt="${data.matia[i].title}" loading="lazy" width="444" height="280">
                </picture>
                <small>${data.matia[i].channel}</small>
                <h2>${data.matia[i].title}</h2>
              </a>
            </article>
          `
        );
      }
      if (i > 1) {
        horizontais.insertAdjacentHTML(
          "beforeend",
          `
            <article class="cardHorizontal">
              <a class="news" href="${data.matia[i].link}" target="_blank">
                <picture>
                  <img src="${data.matia[i].img}" class="newsImage" alt="${data.matia[i].title}" loading="lazy" width="208" height="130">
                </picture>
                <div class="words">
                  <small>${data.matia[i].channel}</small>
                  <h2>${data.matia[i].title}</h2>
                </div>
              </a>
            </article>
          `
        );
      }
    }
  });
});
</script>

