/**
 * Loads the Consent Management (CMP) script
 */
const oneTrustDomainId = '115b0808-1069-4da4-93ea-fa5fca85c11c';

function loadCmpScript(doc, win) {
	const search = window.location.search;
	const ua = navigator.userAgent;
	const env = /docker|hearstapps/i.test(doc.location.hostname) ? 'stage' : 'prod';
	const docBody = doc.body || doc.getElementsByTagName('body')[0];
	const siteCountry = 'GB'.toLowerCase();

	win.cmpUtils = {
		/**
		 * Config & Default consent state
		 */
		debug: [],
		bypassed: false,
		consentInfo: {},
		cmpQueue: [],
		hasWwwInHostName: win.location.hostname.startsWith('www'),
		haveConsent: false,
		timerStart: Date.now(),
		wrapperInit: false,

		/**
		 * Scroll fix for OneTrust consent & preference center modals
		 */
		checkOtTextScroll(sel) {
			setTimeout(function handleTimeOut() {
				var otp = doc.querySelector(sel);
				if (otp && otp.scrollTop > 0) {
					otp.scrollTop = 0;
				}
			}, 250);
		},

		/**
		 * CCPA Forms Request Types
		 */
		formRequestTypes: [
			'Global DNS',
			'Do Not Sell My Information',
			'Delete My Personal Information',
			'Access My Personal Information',
		],

		/**
		 * @desc Checks IAB TCF CMP API to verify vendor consent
		 *
		 * - Pings the __tcfapi and sets listener for response
		 * - Callback response collects vendor consents from tcData
		 * - Calls checkVendorConsent
		 *
		 * @param {integer} vendorId - IAB Vendor ID
		 * @param {function} callback - vendor tag callback function
		 * @callback {checkVendorConsent}
		 */
		checkTCFApi(vendorId, callback) {
			// Adding TCF CMP API event listener callback to when data is loaded
			function tcfCallback(tcData, success) {
				if (
					success &&
					(tcData.eventStatus === 'tcloaded' || tcData.eventStatus === 'useractioncomplete')
				) {
					const vendorConsents = tcData?.vendor?.consents || {};
					win.__tcfapi('removeEventListener', 2, function handleTcfListener() {}, tcData.listenerId);
					if (vendorConsents) win.cmpUtils.checkVendorConsent(vendorConsents, vendorId, callback);
				}
			};

			// Check TCF ping and run event listener or function depending on status
			win.__tcfapi('ping', 2, function handleTcfPing() {
				win.__tcfapi('addEventListener', 2, tcfCallback);
			});
		},

		/**
		 * @desc Checks Vendor Consents from TCF CMP API
		 *
		 * Will run callback function (tag) if vendor consent is true
		 * AND if user is not the following:
		 * - GPP US National Opt Out
		 * - CCPA Opt Out (deprecated soon)
		 * - GPC set
		 *
		 * Sets listener for possible user consent change
		 *
		 * @param {object} vendorConsents - from TCP API tcData response
		 * @param {integer} vendorId - IAB Vendor ID
		 * @param {function} callback - vendor tag callback function
		 * @param {boolean} init - used to set the change listener if initial check
		 * @callback callback supplied - self if consent change
		 */
		checkVendorConsent(vendorConsents, vendorId, callback, init = true) {
			// If there is vendor consent for the supplied vendor ID
			if (
				vendorConsents[vendorId] === true &&
				!win.CMP_GPP_USNAT_OPTOUT &&
				!win.CMP_CCPA_OPTOUT &&
				!win.CMP_GPC_SET
			) {
				win.cmpUtils.log(
					'cmpUtils - checkVendorConsent',
					'consent',
					`vendorId ${vendorId} = ${vendorConsents[vendorId]}`
				);
				return callback();
			}
			win.cmpUtils.log(
				'cmpUtils - checkVendorConsent',
				'consent DECLINED',
				`vendorId ${vendorId} = ${vendorConsents[vendorId]}`
			);

			// No consent intially so add listener for consent change to try again
			if (init) {
				win.addEventListener(
					'cmp.consent.change',
					function handleConsentChange() {
						win.cmpUtils.checkVendorConsent(vendorConsents, vendorId, callback, false);
						return false;
					},
					{ once: true }
				);
			}

			return false;
		},

		/**
		 * Display the OneTrust Modal's Decline Button
		 *
		 * The new requirement is to display a "Decline" button
		 * of the same size right next to the "Accept" button on the main consent modal.
		 *
		 * There are multiple scenarios we're handling.
		 *
		 * ES (Spain) sites/users - will NOT see a "Decline" button on the main consent modal UNTIL Jan. 11, 2024
		 * IT (Italy) sites/users - will see a "Decline" button at the top right, and NOT next to "Accept"
		 * NL (Netherlands) sites/users - will NOT see a "Decline" button (until further notice)
		 * UK (United Kingdom aka: GB) sites/users - WILL see a "Decline" button next to "Accept" on the main consent modal
		 * Every other EU sites/users - WILL see a "Decline" button next to "Accept" on the main consent modal
		 */
		displayDeclineButton() {
			const { site, user } = win.cmpUtils.locale;
			const otModal = doc.querySelector('#onetrust-banner-sdk');

			if (otModal) {
				// Spain shows button after Jan. 11, 2024
				const spainDisplayDecline = new Date() > new Date('2024-01-11T00:00:00');

				// Spain or Netherlands
				if (
					(site === 'es' || user === 'es') && !spainDisplayDecline
					|| (site === 'nl' || user === 'nl')
				) {
					otModal.classList.add('without-decline-option');
				}

				// Italy
				if (site === 'it' || user === 'it') {
					otModal.classList.add('with-close-option');
				}
			}
		},

		/**
		 * Event Dispatch Wrapper Method
		 *
		 * @param {string} name
		 * @param {object} options
		 */
		emitEvent(name, options = {}) {
			win.dispatchEvent(new CustomEvent(name, { detail: options }));
		},

		/**
		 * Event handler for OneTrust forms completion
		 * - uses cmpUtils.formRequestTypes
		 * - E.g. Do Not Sell My Personal Information
		 *
		 * @param {object} event
		 * @returns
		 */
		formCompletionHandler(event) {
			if (!event.origin.includes('onetrust')) return;

			win.cmpUtils.log('formCompletionHandler', 'event', event);

			const { data } = event;
			const parsedData = JSON.parse(data);

			win.scrollTo(0, 0);

			const matchedRequest = win.cmpUtils.formRequestTypes.filter(function matchReq(req) {
				return parsedData.requestTypes.includes(req);
			});

			if (matchedRequest.length > 0) {
				// On successful form submission, flips switch to off for
				// "Opt Out of Sale of Sharing/Targeted Advertising"
				// leaving only cookie categories C0001 (strictly necessary) & C0003 (functional) allowed
				// - GPP US National Opt Out
				// - CCPA Opt Out (deprecated soon)
				win.Optanon.RejectAll();

				win.removeEventListener('click', win.cmpUtils.formCompletionHandler);
			}
		},

		/**
		 * Returns the OneTrust 'OptanonAlertBoxClosed' cookie value
		 *
		 * @returns {array}
		 */
		getAlertBoxCookieValue() {
			// Get the OptanonAlertBoxClosed Cookie
			return win.cmpUtils.getCookieMatch('OptanonAlertBoxClosed');
		},

		/**
		 * Returns a regex match on the document.cookie object, capturing the value
		 * https://regex101.com/r/yaCpvJ/1
		 *
		 * @param {*} name
		 * @returns {*} - value passed if matched
		 */
		getCookieMatch: (name) => doc.cookie.match(`[; ]*${name}=([^;]+)`),

		/**
		 * Returns the CCPA Opt Out Value from the 'usprivacy' cookie if present
		 *
		 * @returns {boolean}
		 */
		getCCPAOptOut() {
			const usprivacy = win.cmpUtils.getCookieMatch('usprivacy');
			return !!(usprivacy && usprivacy[1].charAt(2) === 'Y');
		},

		/**
		 * Checks consent states from OneTrust Consent Groups & IAB TCF CMP API
		 * - If 'category' has consent (2 usage options):
		 *   - runs callback function
		 *   - USAGE: window.getCMPConsent('category', 'C0004', callBackFunc);
		 *
		 *   - returns boolean for condition to run or not
		 *   - USAGE: if (window.getCMPConsent('category', 'C0004') === true) { then do something... }
		 *
		 * - If 'vendor' has consent:
		 *   - runs callback function
		 *   - USAGE: window.getCMPConsent('vendor', [ vendor ID here ], callBackFunc);
		 *
		 * @param {string} type - Type of consent to check for: 'category' or 'vendor'
		 * @param {integer} id - IAB vendor ID to be checked against
		 * @param {function} tag - The tag/vendor callback function to run
		 * @param {boolean} init - used to set the change listener if initial check
		 * @returns {boolean}
		 */
		getCMPConsent(type, id, tag, init = true) {
			// Run tag if site/user is not GDPR eligible
			// AND check to see if user is not the following:
			// - GPP US National Opt Out
			// - CCPA Opt Out (deprecated soon)
			// - GPC set
			const notGDPROrOptedOut =
				!win.GDPR && !win.CMP_GPP_USNAT_OPTOUT && !win.CMP_CCPA_OPTOUT && !win.CMP_GPC_SET;

			// Check type of consent we are checking for
			switch (type) {
				case 'category':
					try {
						if (
							notGDPROrOptedOut ||
							// Check if user has consented to category
							win.CMP_CONSENT_GROUPS.includes(id)
						) {
							win.cmpUtils.log('cmpUtils - getCMPConsent', `category '${id}' consent`, true);
							if (tag) tag();
							return true;
						}
						// No consent intially so add listener for consent change to try again
						if (init) {
							win.addEventListener(
								'cmp.consent.change',
								function handleConsentChange() {
									win.cmpUtils.getCMPConsent(type, id, tag, false);
								},
								{ once: true }
							);
						}

						break;
					} catch (e) {
						return false;
					}
				case 'vendor':
					if (notGDPROrOptedOut) return tag();

					// If __tcfapi isn't defined, push command into queue
					if (typeof win.__tcfapi === 'undefined') {
						win.cmpUtils.cmpQueue.push([id, tag]);
					} else {
						win.cmpUtils.checkTCFApi(id, tag);
					}
					return true;

				default:
					return false;
			}

			// Return false if cases fail:
			return false;
		},

		/**
		 * Gets the Consent Groups via 'OptanonConsent' cookie value
		 *
		 * @returns {object} - consent groups and partial consent
		 */
		getConsentGroups() {
			let partialConsent = false;
			let cmpConsentGroups = '';

			// Check for consent via cookie value
			const onetrustCookieVal = win.cmpUtils.getCookieMatch('OptanonConsent');

			if (onetrustCookieVal) {
				let groups = onetrustCookieVal[1].match(/groups=(.*?)(&|$)/);
				const interactionCount = onetrustCookieVal[1].match(/interactionCount=(.*?)(&|$)/);

				win.cmpUtils.log('cmpUtils - getConsentGroups', 'groups', groups);
				win.cmpUtils.log('cmpUtils - getConsentGroups', 'interactionCount', interactionCount);

				if (groups) {
					groups = decodeURIComponent(groups[1]).split(',');
					groups.forEach(function handleGroup(group) {
						if (group.includes(':')) {
							const gs = group.split(':');
							if (gs[1] === '1') {
								cmpConsentGroups += (cmpConsentGroups ? ',' : '') + gs[0];
								// show partial consent only after initial interaction
							} else if (gs[1] === '0' && interactionCount[1] > 0) {
								partialConsent = true;
							}
						}
					});
				}
			}

			return { cmpConsentGroups, partialConsent };
		},

		/**
		 * Returns the Global Privacy Control (GPC) Value from the browser
		 *
		 * @returns {boolean}
		 */
		getGPCStatus() {
			return !!(navigator && navigator.globalPrivacyControl);
		},

		/**
		 * Gets the User's cookie Info from a HDM-set cookie
		 * - Fastly set
		 * - manually set from FRE or ad bundle
		 *
		 * Note: Value MUST be in the format of a JSON string
		 *
		 * @returns {object}
		 */
		getHdmCookie(name) {
			const hdmCookie = win.cmpUtils.getCookieMatch(name);
			return hdmCookie ? JSON.parse(hdmCookie[1]) : '';
		},

		/**
		 * Retrieves the user's country code from the Fastly 'location_data' cookie
		 *
		 * @returns {string}
		 */
		getUserCountry() {
			// Get location cookie set by Fastly
			const locationDataCookie = win.cmpUtils.getHdmCookie('location_data');
			// Get country code from location cookie
			const { country_code: countryCode } = locationDataCookie;

			return countryCode ? countryCode.toLowerCase() : false;
		},

		/**
		 * Global Privacy Protection (GPP)
		 */
		gpp: {
			listenerSet: false,

			/**
			 * Retrieves GPP US National Opt Out Signal from "hdm_cmp" cookie
			 *
			 * @returns {boolean}
			 */
			getUsNatOptOut() {
				// If 'hcm_cmp' cookie is found
				const hdmCmp = win.cmpUtils.getHdmCookie('hdm_cmp');

				// If GPP US National Opt Out is found
				const optOut = hdmCmp?.gpp?.usNatOptOut;
				if (optOut) {
					win.cmpUtils.log('cmpUtils - GPP', 'getUsNatOptOut - hdmCmp.gpp.usNatOptOut', !!optOut);
					return !!optOut;
				}

				return false;
			},

			/**
			 * Retrieves and Sets GPP US National Opt Out Signal from __gpp CMP API
			 *
			 * @returns {boolean}
			 */
			setUsNatOptOut(evt) {
				win.cmpUtils.log('cmpUtils - GPP', 'setUsNatOptOut - evt', evt);

				// Checking __gpp CMP API
				const { pingData } = evt;
				const OPTED_OUT_VALUE = 1;
				let optOut = false;

				function updateHdmCmpCookie() {
					const hdmCmpCookie = win.cmpUtils.getHdmCookie('hdm_cmp');

					// If no cookie or status has changed, set/update 'hdm_cmp' cookie
					if (!hdmCmpCookie || win.CMP_GPP_USNAT_OPTOUT !== optOut) {
						const cookieInfo = {
							gpp: {
								usNatOptOut: optOut,
								// TODO: remove after all scripts updated to new version
								uspNatOptOut: optOut,
							},
						};
						win.CMP_GPP_USNAT_OPTOUT = optOut;
						// TODO: remove after all scripts updated to new version
						win.CMP_GPP_USPNAT_OPTOUT = win.CMP_GPP_USNAT_OPTOUT;
						win.cmpUtils.setHdmCmpCookie(cookieInfo);
					}
				}

				function handleGetSection(resp) {
					win.cmpUtils.log('cmpUtils - GPP', 'setUsNatOptOut - handleGetSection - resp', resp);

					const { SaleOptOut, SharingOptOut, TargetedAdvertisingOptOut } = resp;

					// If any of these are opted out
					optOut =
						SaleOptOut === OPTED_OUT_VALUE ||
						SharingOptOut === OPTED_OUT_VALUE ||
						TargetedAdvertisingOptOut === OPTED_OUT_VALUE;

					win.cmpUtils.log('cmpUtils - GPP', 'setUsNatOptOut - handleGetSection - optOut', optOut);

					updateHdmCmpCookie();
				}

				function handleHasSection(resp) {
					win.cmpUtils.log('cmpUtils - GPP', 'setUsNatOptOut - handleHasSection - resp', resp);

					if (resp) {
						// Get GPP details from the section
						win.__gpp('getSection', handleGetSection, 'usnatv1');
					} else {
						updateHdmCmpCookie();
					}
				}

				// If GPP ready and loaded
				if (pingData && pingData.cmpStatus === 'loaded') {
					// Check to see if the 'usnatv1' section is available
					// - if configured, handle it
					win.__gpp('hasSection', handleHasSection, 'usnatv1');
				}
			},

			/**
			 * Initializes GPP Event Listener
			 * - Retrieves initial info
			 *
			 * @returns {boolean}
			 */
			init() {
				win.cmpUtils.log('cmpUtils - GPP', 'init', true);

				win.__gpp('addEventListener', win.cmpUtils.gpp.setUsNatOptOut);

				win.cmpUtils.gpp.listenerSet = true;
			},
		},

		/**
		 * Check if OneTrust Modal is present
		 */
		hasModal: () => !!doc.querySelector('#onetrust-banner-sdk'),

		/**
		 * Logs when OneTrust script is loaded
		 */
		handleOtLoad(e) {
			win.cmpUtils.log('loadCmpScript', 'handleOtLoad - e', e);
		},

		/**
		 * Check if US Site AND User
		 */
		isUSSiteAndUser: () => win.cmpUtils.locale.site === 'us' && win.cmpUtils.locale.user === 'us',

		/**
		 * Loads the IAB CCPA Script
		 */
		loadIabCcpaScript() {
			const docBod = doc.body || doc.getElementsByTagName('body')[0];

			const script = doc.createElement('script');
			script.id = 'ot-ccpa-iab-script';
			script.src = 'https://cdn.cookielaw.org/opt-out/otCCPAiab.js';
			script.setAttribute('type', 'text/javascript');
			script.setAttribute('charset', 'UTF-8');
			script.setAttribute('ccpa-opt-out-ids', ['C0004'].join(','));
			script.setAttribute('ccpa-opt-out-geo', 'us');
			script.setAttribute('ccpa-opt-out-lspa', 'false');

			docBod.appendChild(script);

			win.cmpUtils.log('loadCmpScript - loadIabCcpaScript', 'script', script);

			// After the script loads, fire the "consent.onetrust" event
			// until the uspData is available or timeout reached
			script.addEventListener(
				'load',
				function handleLoad() {
					let intv = 0;
					const t = setInterval(function checkIntv() {
						// eslint-disable-next-line no-undef,no-unused-vars
						__uspapi('getUSPData', 1, function checkUspData(uspData, success) {
							if (uspData.uspString !== null || intv === 50) {
								// Get Inital CCPA Opt Out
								win.CMP_CCPA_OPTOUT = win.cmpUtils.getCCPAOptOut();
								win.cmpUtils.log(
									'loadCmpScript - loadIabCcpaScript',
									'uspData.uspString',
									uspData.uspString
								);
								clearInterval(t);
							}
						});
						intv += 1;
					}, 200);
				},
				{ once: true }
			);
		},

		/**
		 * Keeps a log of the CMP events
		 *
		 * @param {*} name
		 * @param {*} evt
		 * @param {*} msg
		 */
		log(name, evt, msg) {
			const ts = Date.now() - win.cmpUtils.timerStart;
			win.cmpUtils.debug.push({
				name,
				evt,
				msg,
				ts,
			});
		},

		/**
		 * Cookie Setting Function
		 * - Production domains need to have cookie domain explicitly set
		 * - Adds path adjustment for international sites
		 *
		 * Reads an object that passes the following properties:
		 *
		 * @param {string} name
		 * @param {JSON} value
		 * @param {integer} minutes
		 */
		setCookie(info) {
			const { name, value, minutes } = info;

			// Set name/value
			let cookie = `${name}=${value}`;

			// Set Expiration (if applicable)
			if (minutes) {
				let date;

				if (minutes === 'delete') {
					date = new Date(0);
				} else {
					const expires = Date.now() + minutes * 60 * 1000;
					date = new Date(expires);
				}

				cookie += `; expires=${date.toUTCString()}`;
			}

			// Set Domain
			// - prod/stage domains need 'www' or 'www-stage' removed and use apex
			const domain = win.cmpUtils.hasWwwInHostName
				? win.location.hostname.replace(/(www-stage)|(www)/, '')
				: '';
			cookie += domain ? `; domain=${domain}` : '';

			// Set Path
			const localePath = win.__SITE?.locale_path;
			cookie += `; path=${localePath ? `/${localePath}/` : '/'}`;

			win.cmpUtils.log('cmpUtils - setCookie', 'cookie', `${cookie};`);

			// Set final cookie
			document.cookie = `${cookie};`;
		},

		/**
		 * Sets all the global values
		 *
		 * window object properties:
		 * - CMP_CONSENT
		 * - CMP_PARTIAL_CONSTENT - depends on if opted out of some categories
		 * - CMP_WITHDRAWN - only set when the user declines consent
		 * - CMP_CONSENT_GROUPS - all cookie category groups consented to
		 * - CMP_GPC_SET - true if global privacy control is on
		 * - CMP_GPP_USNAT_OPTOUT - true if global privacy policy (GPP) is on for 'usnatv1' (US National)
		 * - CMP_CCPA_OPT_OUT - true if user opted out of sale of data (deprecated soon)
		 */
		setGlobalValues() {
			// Global function used by video and tags to determine if a GDPR consent modal is showing
			// TODO: remove duplicate vars after migrate to CMP vocab
			win.GDPR_CONSENT_MODAL = win.cmpUtils.hasModal;
			win.CMP_CONSENT_MODAL = win.cmpUtils.hasModal;

			// Get Consent Groups
			const { cmpConsentGroups, partialConsent } = win.cmpUtils.getConsentGroups();
			win.cmpUtils.log('cmpUtils - setGlobalValues', 'cmpConsentGroups', cmpConsentGroups);
			win.cmpUtils.log('cmpUtils - setGlobalValues', 'partialConsent', partialConsent);

			// Set Consent Groups into Global
			// TODO: remove duplicate vars after migrate to CMP vocab
			win.GDPR_CONSENT_GROUPS = cmpConsentGroups;
			win.CMP_CONSENT_GROUPS = cmpConsentGroups;

			// US Sites & Users
			// - GPP US National Opt Out
			// - CCPA Opt Out (deprecated soon)
			if (win.cmpUtils.isUSSiteAndUser()) {
				// Set US National Opt Out
				win.CMP_GPP_USNAT_OPTOUT = win.cmpUtils.gpp.getUsNatOptOut();
				// TODO: remove after all scripts updated to new version
				win.CMP_GPP_USPNAT_OPTOUT = win.CMP_GPP_USNAT_OPTOUT;
				win.cmpUtils.log(
					'cmpUtils - setGlobalValues',
					'win.CMP_GPP_USNAT_OPTOUT',
					win.CMP_GPP_USNAT_OPTOUT
				);

				// Get CCPA Opt Out
				win.CMP_CCPA_OPTOUT = win.cmpUtils.getCCPAOptOut();
				win.cmpUtils.log('cmpUtils - setGlobalValues', 'win.CMP_CCPA_OPTOUT', win.CMP_CCPA_OPTOUT);
			}

			// Get GPC Status
			win.CMP_GPC_SET = win.cmpUtils.getGPCStatus();
			win.cmpUtils.log('cmpUtils - setGlobalValues', 'win.CMP_GPC_SET', win.CMP_GPC_SET);

			// If consent in any of categories 2 through 5 - check for YES consent
			win.cmpUtils.haveConsent = !!/C0002|C0003|C0004|C0005/.test(cmpConsentGroups);

			win.cmpUtils.log(
				'cmpUtils - setGlobalValues',
				'cmpUtils.haveConsent',
				win.cmpUtils.haveConsent
			);

			// Set consent state as a global variable for use by tags
			// - Caution: this is a generic consent value
			// - Consent should be checked by cookie category or vendor ID
			// - See details here:
			//   https://www.notion.so/mediaos/GDPR-Consent-Management-38127bd6b9904408a809a9946a358d9f
			// TODO: remove duplicate vars after migrate to CMP vocab
			win.GDPR_CONSENT = !!win.cmpUtils.haveConsent;
			win.CMP_CONSENT = !!win.cmpUtils.haveConsent;
			win.cmpUtils.log('cmpUtils - setGlobalValues', 'win.CMP_CONSENT', win.CMP_CONSENT);

			// Check for partial consent and set global if true
			if (partialConsent) {
				// TODO: remove duplicate vars after migrate to CMP vocab
				win.GDPR_PARTIAL_CONSENT = true;
				win.CMP_PARTIAL_CONSENT = true;
				win.cmpUtils.log(
					'cmpUtils - setGlobalValues',
					'win.CMP_PARTIAL_CONSENT',
					win.CMP_PARTIAL_CONSENT
				);
			}

			// Get the OptanonAlertBoxClosed Cookie
			const onetrustAlertBoxCookieVal = win.cmpUtils.getAlertBoxCookieValue();

			// If consent has been withdrawn set a global variable for use by tags
			if (!win.cmpUtils.haveConsent && onetrustAlertBoxCookieVal) {
				// TODO: remove duplicate vars after migrate to CMP vocab
				win.GDPR_WITHDRAWN = true;
				win.CMP_WITHDRAWN = true;
				win.cmpUtils.log('cmpUtils - setGlobalValues', 'win.CMP_WITHDRAWN', win.CMP_WITHDRAWN);
			}
		},

		/**
		 * Sets/Updates 'hdm_cmp' Cookie with current user consent
		 *
		 * Supports various settings including:
		 * - GPP US National Optout
		 * - CCPA Opt Out (deprecated soon)
		 * - OneTrust Cookie Categories
		 * - GPC Setting
		 *
		 * @param {object} info
		 * @calls {setCookie}
		 */
		setHdmCmpCookie(info) {
			if (!info) return;

			// get current parsed cookie value if any
			const hdmCmpCookie = win.cmpUtils.getHdmCookie('hdm_cmp');

			// combine info arg value with current value
			// and convert to JSON string
			const combinedInfo = JSON.stringify({ ...hdmCmpCookie, ...info });

			// set cookie info
			const cookieInfo = {
				name: 'hdm_cmp',
				value: combinedInfo,
				minutes: 525600,
			};

			win.cmpUtils.log('cmpUtils - setHdmCmpCookie', 'cookieInfo', cookieInfo);

			win.cmpUtils.setCookie(cookieInfo);
		},

		/**
		 * Updates Your Privacy Rights form iframe styles (heights)
		 */
		updateFormIframeStyles(formFrame) {
			if (formFrame) {
				const formContInner = formFrame.parentElement;
				const formContEmbed = formContInner.parentElement;
				const formContBody = formContEmbed.parentElement;
				const formContMain = formContBody.parentElement.parentElement;
				formContInner.classList.add('onetrust-do-not-sell-form-embed-inner');
				formContEmbed.classList.add('onetrust-do-not-sell-form-embed');
				formContBody.classList.add('onetrust-do-not-sell-form-body');
				formContMain.classList.add('onetrust-do-not-sell-form-content');
				formFrame.classList.add('onetrust-do-not-sell-form');
			}
		},

		/**
		 * Wait for OptanonAlertBoxClosed Cookie to be set
		 *
		 * @param {string} name
		 * @returns {promise}
		 */
		waitForOTCookie(name) {
			// time counter to avoid infinite loop when OptanonAlertBoxClosed does not exist
			let counter = 0;
			function assignOTCookiePromise(resolve, reject) {
				if (counter >= 1000) reject(`${name} does not exist`);
				if (win.cmpUtils.getCookieMatch(name)) {
					resolve(true);
				} else if (counter < 1000) {
					counter += 100;
					setTimeout(assignOTCookiePromise.bind(this, resolve, reject), 100);
				}
			}
			return new Promise(assignOTCookiePromise);
		},

		/**
		 * Waits for tcfapi to be defined
		 * - calls checkTCFApi to process cmpQueue
		 */
		waitForTCF() {
			if (typeof win.__tcfapi !== 'undefined') {
				// loop through queue
				if (win.cmpUtils.cmpQueue.length) {
					win.cmpUtils.cmpQueue.forEach(function handleTcfArgs(args) {
						if (Array.isArray(args)) {
							win.cmpUtils.checkTCFApi(...args);
						}
					});
					// reset the queue
					win.cmpUtils.cmpQueue = [];
				}

				return;
			}
			setTimeout(win.cmpUtils.waitForTCF, 100);
		},
	};

	/**
	 * Loads the Consent Management Logic - OneTrust
	 */
	function loadCmpScriptLogic() {
		win.cmpUtils.log('loadCmpScript - loaded', 'cmpUtils', win.cmpUtils);

		// Set User & Site Locales
		win.cmpUtils.locale = {
			site: siteCountry,
			user: win.cmpUtils.getUserCountry(),
		};

		// Set Global Values
		win.cmpUtils.setGlobalValues();

		// Set CMP Consent Check to global
		win.getCMPConsent = win.cmpUtils.getCMPConsent;

		// For GDPR, wait TCF to load and process cmpQueue
		if (window.GDPR) win.cmpUtils.waitForTCF();

		// Cookie Choices Button - used to trigger preference center modal event
		const cookieChoicesButton = doc.querySelector('.ot-sdk-show-settings');
		if (cookieChoicesButton) {
			cookieChoicesButton.addEventListener(
				'click',
				function emit() {
					win.cmpUtils.emitEvent('onetrust_pc_modal');
				},
				false
			);
		}

		/**
		 * OneTrust Callback
		 * Callback executed by OneTrust
		 * - immediately after their vendor script is loaded
		 * - if user changes consent via preferences center
		 */
		win.OptanonWrapper = function optanonWrapper() {
			win.cmpUtils.log(
				'loadCmpScript - OptanonWrapper',
				'started - cmpUtils.wrapperInit',
				win.cmpUtils.wrapperInit
			);
			win.cmpUtils.wrapperInit = true;
			win.cmpUtils.log(
				'loadCmpScript - OptanonWrapper',
				'started - cmpUtils.haveConsent',
				win.cmpUtils.haveConsent
			);

			// Wait for OneTrust cookie information to be set/present
			win.cmpUtils.waitForOTCookie('OptanonConsent').then(function handleWaitFor() {
				// Get the OptanonAlertBoxClosed Cookie
				const onetrustAlertBoxCookieVal = win.cmpUtils.getAlertBoxCookieValue();

				// Reset Global Values
				win.cmpUtils.setGlobalValues();

				// Get datestamp from the OptanonAlertBoxClosed cookie if it exists
				const otAbcCookieTimestamp = onetrustAlertBoxCookieVal
					? new Date(onetrustAlertBoxCookieVal[1]).getTime()
					: 0;
				win.cmpUtils.log(
					'loadCmpScript - OptanonWrapper',
					'otAbcCookieTimestamp',
					otAbcCookieTimestamp
				);

				// this check if approve or not
				// Maximum waiting time for the last user action in milliseconds
				const MAX_TIME_ACTION = 250;
				const recentActionTs = otAbcCookieTimestamp
					? Date.now() - otAbcCookieTimestamp < MAX_TIME_ACTION
					: false;

				// TODO: find all instances of binary consent
				//       and change to category/vendor using getCMPConsent method,
				//       then deprecate
				//
				// OptanonActiveGroups is a global variable that stores the current OneTrust consent status
				// If one of the categories C0002 through C0005 are present means we have targeting consent
				const haveCurrentConsent = /C0002|C0003|C0004|C0005/.test(win.OptanonActiveGroups);

				// Modal load tracking:
				const modalShowing = win.cmpUtils.hasModal();

				if (modalShowing) {
					// Display appropriate decline button on the main modal
					win.cmpUtils.displayDeclineButton();

					// Send event for GA tracking if it hasn't happened;
					if (!win.ga_modal_tracking) {
						win.ONETRUST_MODAL = true;
						win.cmpUtils.log('loadCmpScript - OptanonWrapper', 'modal event', 'onetrust_modal');
						win.cmpUtils.emitEvent('onetrust_modal');
					}

					// Learn More Button - used to trigger preference center modal event
					const gdprLearnMoreButton = doc.querySelector('#onetrust-pc-btn-handler');
					if (gdprLearnMoreButton) {
						gdprLearnMoreButton.addEventListener(
							'click',
							function emit() {
								win.cmpUtils.log(
									'loadCmpScript - OptanonWrapper',
									'modal event',
									'onetrust_pc_modal'
								);
								win.cmpUtils.emitEvent('onetrust_pc_modal');
							},
							{ once: true }
						);
					}
				}

				win.cmpUtils.log('loadCmpScript - OptanonWrapper', 'modalShowing', modalShowing);
				win.cmpUtils.log('loadCmpScript - OptanonWrapper', 'recentActionTs', recentActionTs);

				// Send consent events
				// - accept, decline, and notification of any changes
				if (modalShowing && recentActionTs) {
					// If we have consent or not
					if (haveCurrentConsent) {
						win.cmpUtils.log('loadCmpScript - OptanonWrapper', 'modal event', 'onetrust_accept');
						win.cmpUtils.emitEvent('onetrust_accept');
					} else {
						// If we previously had consent
						win.cmpUtils.log('loadCmpScript - OptanonWrapper', 'modal event', 'onetrust_reject');
						win.cmpUtils.emitEvent('onetrust_reject');
					}
					win.cmpUtils
						.waitForOTCookie('OptanonAlertBoxClosed')
						.then(() => win.cmpUtils.emitEvent('cmp.consent.change'));
				}

				// Send CMP Set event with consent object
				// TODO: remove duplicate vars after migrate to CMP vocab
				win.cmpUtils.consentInfo = {
					consentGroups: win.CMP_CONSENT_GROUPS,
					gpcSet: win.CMP_GPC_SET,
					haveConsent: !!win.cmpUtils.haveConsent,
				};

				// Add US Site/Users Info
				if (win.cmpUtils.isUSSiteAndUser()) {
					// GPP US National Opt Out
					win.cmpUtils.consentInfo.gpp = {
						usNatOptOut: win.CMP_GPP_USNAT_OPTOUT,
						// TODO: remove after all scripts updated to new version
						uspNatOptOut: win.CMP_GPP_USNAT_OPTOUT,
					};
					// CCPA Opt Out (deprecated soon)
					win.cmpUtils.consentInfo.ccpaOptOut = win.CMP_CCPA_OPTOUT;
				}

				// Set or update `hdm_cmp` cookie
				win.cmpUtils.setHdmCmpCookie(win.cmpUtils.consentInfo);

				// Send cmp consent set event
				win.cmpUtils.emitEvent('gdpr.consent.set', win.cmpUtils.consentInfo);
				win.cmpUtils.emitEvent('cmp.consent.set', win.cmpUtils.consentInfo);
				win.cmpUtils.log(
					'loadCmpScript - OptanonWrapper',
					'finished - cmpUtils.consentInfo',
					win.cmpUtils.consentInfo
				);
			});
		};

		// Initial Run if auto-run has happened before script loaded
		if (!win.cmpUtils.wrapperInit) win.OptanonWrapper();

		// Main Listener for OneTrust
		win.addEventListener(
			'consent.onetrust',
			function handleConsentOneTrustEvent(e) {
				win.cmpUtils.log('loadCmpScript', 'Event - consent.onetrust - e', e);
			},
			false
		);

		// Listen for OneTrust Groups assignment
		win.addEventListener(
			'OneTrustGroupsUpdated',
			function handleOneTrustGroupsUpdatedEvent(e) {
				// If user/site is in US
				if (win.cmpUtils.isUSSiteAndUser()) {
					// Load IAB CCPA Script (deprecated soon)
					win.cmpUtils.loadIabCcpaScript();

					// Load GPP
					if (win.__gpp && !win.cmpUtils.gpp.listenerSet) {
						win.cmpUtils.gpp.init();
						win.cmpUtils.log('loadCmpScript', 'Event - OneTrustGroupsUpdated - init GPP', true);
					}
				}

				// Adjust Your Privacy Rights Iframe Styles (heights) if present
				const oneTrustForm = doc.querySelector('iframe[data-src*="onetrust.com/webform"]');
				if (oneTrustForm) win.cmpUtils.updateFormIframeStyles(oneTrustForm);

				win.cmpUtils.log('loadCmpScript', 'Event - OneTrustGroupsUpdated - e', e);
			},
			false
		);

		// Listen for OneTrust Consent Modal
		win.addEventListener('onetrust_modal', function handleOneTrustModalEvent() {
			win.cmpUtils.checkOtTextScroll('#onetrust-policy');
		}, { once: true });

		// Listen for OneTrust Preferences Center Modal
		win.addEventListener('onetrust_pc_modal', function handleOneTrustPrefCenterModalEvent() {
			win.cmpUtils.checkOtTextScroll('#ot-pc-content');
		}, false);

		// A postMessage will be sent upon a successful OneTrust form submission
		win.addEventListener('message', win.cmpUtils.formCompletionHandler, false);
	}

	/**
	 * Load OneTrust SDK Script
	 */
	(function loadOneTrust() {
		/**
		 * Disable OneTrust CMP if:
		 * - query param "disableCMP" is present
		 * - the user agent is "Chrome-Lighthouse" or "PTST" from the automated build tests
		 */
		if (~search.indexOf('disableCMP') && (~ua.indexOf('Chrome-Lighthouse') || ~ua.indexOf('PTST')) ) return;

		// Load script with callback/onload
		const domainId = `${oneTrustDomainId}${(env === 'stage') ? '-test' : ''}`;
		const s = doc.createElement('script');
		s.type = 'text/javascript';
		s.async = true;
		s.onload = win.cmpUtils.handleOtLoad;
		s.charset = 'UTF-8';
		s.src = 'https://cdn.cookielaw.org/scripttemplates/otSDKStub.js';
		s.setAttribute('data-domain-script', domainId);
		s.setAttribute('data-document-language', 'true');
		docBody.appendChild(s);
	})();

	/**
	 * Load OneTrust SDK Script Logic
	 */
	loadCmpScriptLogic();

	win.cmpUtils.emitEvent('app.cmpScriptLoaded');
}

/**
 * Loads the CMP bypass script
 * - Sends event to notify CMP has been bypassed
 */
function loadCmpBypassScript(win) {
	win.cmpUtils = {
		/**
		 * Config
		 */
		bypassed: false,
		debug: [],
		timerStart: Date.now(),

		/**
		 * Event Dispatch Wrapper Method
		 *
		 * @param {string} name
		 * @param {object} options
		 */
		emitEvent(name, options = {}) {
			win.dispatchEvent(new CustomEvent(name, { detail: options }));
		},

		/**
		 * Keeps a log of the CMP events
		 *
		 * @param {*} name
		 * @param {*} evt
		 * @param {*} msg
		 */
		log(name, evt, msg) {
			const ts = Date.now() - win.cmpUtils.timerStart;
			win.cmpUtils.debug.push({
				name,
				evt,
				msg,
				ts,
			});
		},
	};

	/**
	 * Loads the Consent Management Bypass Event - OneTrust
	 */
	function loadCmpBypassEvent() {
		win.cmpUtils.bypassed = true;
		// TODO: remove duplicate vars after migrate to CMP vocab
		win.GDPR_CONSENT = true;
		win.CMP_CONSENT = true;
		win.cmpUtils.emitEvent('cmp.consent.bypassed', {});
		win.cmpUtils.log('loadCmpBypassScript', 'CMP bypassed', true);
	}

	loadCmpBypassEvent();
}

// If OneTrust Domain ID load the script and logic
// otherwise, run bypass
if (oneTrustDomainId) {
	loadCmpScript(document, window);
} else {
	loadCmpBypassScript(window);
}

/**
 * Loads the relevant tag manager scripts
 * considers:
 * - GDPR and consent status
 * - Ads Free status
 */
(function loadTagManagerScript(doc, win) {
	const search = window.location.search;
	const ua = navigator.userAgent;

	/**
	 * Disable Tag Manager if:
	 * - query param "disableTagManager" is present
	 * - the user agent is "Chrome-Lighthouse" or "PTST" from the automated build tests
	 */
	if (~search.indexOf('disableTagManager') && (~ua.indexOf('Chrome-Lighthouse') || ~ua.indexOf('PTST')) ) return;

	const docBody = doc.body || doc.getElementsByTagName('body')[0];

	/**
	 * Creates a script tag to append
	 *
	 * @param {object} d (document)
	 * @param {string} id
	 * @param {string} src
	 * @param {boolean} defer
	 * @returns {object} - script element
	 */
	function createScript(d, id, src, defer = false) {
		const script = d.createElement('script');
		script.id = id;
		script.src = src;
		// programmatic scripts are asynchronous by default
		script.defer = defer;
		return script;
	}

	/**
	 * Load Tag Manager script
	 */
	function loadTagManager() {
		const base = '//nexus.ensighten.com/hearst/mag-uk/Bootstrap.js';
		const dev = '//nexus.ensighten.com/hearst/mag-uk-dev/Bootstrap.js';

		if (!base) return;

		let tagManagerScript = base;

		const src = `${doc.cookie.includes('nsghtn=d') ? dev : tagManagerScript}`;
		const addTag = () => docBody.appendChild(createScript(doc, 'tag-manager-script', src, true));

		addTag();
	}

	/**
	 * Load Google Tag Manager script
	 */
	function loadGoogleTagManager() {
		const googleTagManagerId = '';

		if (!googleTagManagerId) return;

		win.dataLayer = win.dataLayer || [];
		win.dataLayer.push({
			'gtm.start': new Date().getTime(),
			event: 'gtm.js',
		});

		const src = `https://www.googletagmanager.com/gtm.js?id=${googleTagManagerId}&l=dataLayer`;
		const addTag = () => docBody.appendChild(createScript(doc, 'google-tag-manager-script', src, true));

		addTag();
	}

	if (
		(window.cmpUtils && win.cmpUtils?.consentInfo?.haveConsent !== 'undefined') ||
		(window.cmpUtils && window.cmpUtils.bypassed)
	) {
		loadTagManager();
		loadGoogleTagManager();
	} else {
		// Listen for consent event, then load the tag manager
		window.addEventListener('cmp.consent.set', () => {
			loadTagManager();
			loadGoogleTagManager();
		}, { once: true });
		window.addEventListener('cmp.consent.bypassed', () => {
			loadTagManager();
			loadGoogleTagManager();
		}, false);
	}
})(document, window);
</script>
		</body>
	</html>
	

