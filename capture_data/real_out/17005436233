    <script type="text/javascript">
        // ShopRunner related settings
        var endpoint = '';
        var protocol = document.location.protocol;

        if( protocol == 'http:' ) {
            endpoint = "https://www.katespade.com/on/demandware.store/Sites-ks_us_rt-Site/en_US/ShopRunner-PayrunnerAPI";
        } else {
            endpoint = "https://www.katespade.com/on/demandware.store/Sites-ks_us_rt-Site/en_US/ShopRunner-PayrunnerAPI";
        }

        var _shoprunner_com = new Object();
        _shoprunner_com.enabled = true;
        _shoprunner_com.retailerID = "KSPADE";
        _shoprunner_com.loginValidationURL = "https://www.katespade.com/on/demandware.store/Sites-ks_us_rt-Site/en_US/ShopRunner-ValidateToken";
        _shoprunner_com.environmentID = 2;

        /*
         * ----------------------------------------
         * PayRunner Configuration
         *  ---------------------------------------
         */
        _shoprunner_com.checkout = new Object();
        _shoprunner_com.checkout.enabled = false;
        _shoprunner_com.checkout.singleProductBuyNowEnabled = false;
        _shoprunner_com.checkout.cartBuyNowEnabled = true;
        _shoprunner_com.checkout.partnerAPIEndPoint = endpoint;

        _shoprunner_com.checkout.mixedCart = "null";

        /*
         * ----------------------------------------
         * MOV Configuration - enable if applicable
         *  ---------------------------------------
         */
        //_shoprunner_com.cart = new Object();
        //_shoprunner_com.cart.srSubTotal = “XX.XX";

        // Maintain shipping method
        var intervalId = window.setInterval(registerShippingMethodEvent, 100);

        function registerShippingMethodEvent() {
            window.sr_init_count = window.sr_init_count || 0;
            sr_init_count++;

            if (sr_init_count >= 30) {
                clearInterval(intervalId);
                return;
            }

            if (typeof $ === 'undefined') {
                return;
            }
            sr_init_count = 30;
        }

    </script>




