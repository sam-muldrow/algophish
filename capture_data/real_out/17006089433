			cvlturl = '/pages/banner_cv_sbankas.html';break;
		case (cvltrnd <= 14):
			cvlturl = '/pages/banner_cv_kpmg.html';break;
		case (cvltrnd <= 16):
			cvlturl = '/pages/banner_cv_luminor.html';break;
		case (cvltrnd <= 18):
			cvlturl = '/pages/banner_cv_ikilietuva.html';break;
	}	
	if(cvlturl!=''){
		$.ajax({
			url: cvlturl,
			success: function(result){
				$("#cvSidebar").parent().html(result);
				adsCnt = $('#cvSidebar ul.cvlt').length;
