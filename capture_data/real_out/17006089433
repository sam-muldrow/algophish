            		<script type="text/javascript">
            			$(document).ready(function(){
            				$.ajax({
            					type: "GET",
            					url: 'https://verslodirbtuvesvideo.vz.lt/?json=true',
            					success: function(data){
									for(var i = 0; i < Math.min(data.length,18); i++) {
										var obj = data[i];
										$('#videoreports ul').append('<li><a style="padding:0 !important;max-width:250px;" href="'+obj.url+'" target="_blank"><img  src="'+obj.image+'" alt="img"></a></li>');
									}
									var $slider;
									var nw = $('.vreports-slider').outerWidth();
									if($(window).width() > 974) { slides = 3; }
									if($(window).width() <= 974) { slides = 2; }
									if($(window).width() < 483) { slides = 1; }
									$slider = $('.vreports-slider').bxSlider({
										mode: 'horizontal',
										auto:true,
										pager:false,
										infiniteLoop: true,
										speed: 1000,
										pause: 4000,
										autoHover: true,
										touchEnabled: false,
										captions: true,
										minSlides: slides,
										maxSlides: slides,
										moveSlides: slides,
										slideWidth: 750,
										nextText: '<i class="fa fa-angle-right"></i>',
										prevText: '<i class="fa fa-angle-left"></i>',
									});
            					}
							});
            			});
