<script src="https://cdn.jwplayer.com/libraries/c6EQs7Yn.js"></script>
<script type="text/javascript">
	function onArticleMatch(callback){
		$('.main-article p, #marketing-wr p').each(function(index){
			var match = $(this).text().match(/podcast::(\S+)\s*.*$/m);
			if (match) {
				callback($(this), match[1]);
			}
		});
	}
	window.urlParams = function() {
		var match,
			pl = /\+/g,
			search = /([^&=]+)=?([^&]*)/g,
			decode = function(s) {
				return decodeURIComponent(s.replace(pl, " "));
			},
			query = [window.location.search.substring(1), window.location.hash.substring(2)];
		var o = {};
		for (var i = 0; i < query.length; i++)
		{
			while (match = search.exec(query[i])) {
				var v1 = decode(match[1]);
				var v2 = decode(match[2]);
				if (v2 === 'true' || v2 === 'false') {
					v2 = JSON.parse(v2);
				}
				o[v1] = v2;
			}
		}
		return o;
	}
	var options = urlParams();
	options.logged = SessionSingleSale.length > 0;
	options.standalone = window.location.pathname !== '/podkastai'; //-1 != 1872;
	options.premium = SessionSingleSale.toLowerCase().indexOf('premium') >= 0;
	options.threshold = options.premium ? -1 : 120;
	options.highlight = 'ial9qeN1';
	if (options.template && options.template === 'api_article')
	{
		onArticleMatch(function(element, id){
			element.remove();
			$('.main-article .img .podcast-overlay').on('click', function(){
				if (!$(this).hasClass('playing')) {
					if (window.hasOwnProperty('TRINITY_PLAYER')) {
						TRINITY_PLAYER.api.pauseAll();
					}
					$.ajax({
						url: window.location.href,
		                data: 'media_id=' + id
		            });
					let previous = $('.main-article .img .podcast-overlay.podcast-current');
					if (previous.size() > 0) {
						previous.find('.pause').hide();
						previous.find('.play').show();
						previous.removeClass('playing');
						previous.removeClass('podcast-current');
					}
				} else {
					console.log('podcast:pause');
				}
				$(this).addClass('podcast-current');
			}).show();
		});
		function setPlayerButtonState(state) {
			if (state === 'play') {
				$('.main-article .img .podcast-overlay .play').hide();
				$('.main-article .img .podcast-overlay .pause').show();
				$('.main-article .img .podcast-overlay').addClass('playing');
			} else {
				$('.main-article .img .podcast-overlay .pause').hide();
				$('.main-article .img .podcast-overlay .play').show();
				$('.main-article .img .podcast-overlay').removeClass('playing');
			}
		}
	}
	else
	{
		onArticleMatch(function(element, id){
			options.play = id;
			options.muted = false;
			if (element.text() === 'podcast::' + id)
			{
				element.remove();
			}
			else
			{
				element.text(element.text().replace(new RegExp('podcast::' + id + '\s*'), ''));
			}
			VZPlayer.ready(function(){
				$('.main-article .img .podcast-overlay').show();
				$('.main-article .img').on('click', function(){
					VZPlayer.toggle(options.play);
				});
				VZPlayer.on('play', function(){
					$('.main-article .img .play').hide();
					$('.main-article .img .pause').show();
				});
				VZPlayer.on('pause', function(){
					$('.main-article .img .play').show();
					$('.main-article .img .pause').hide();
				});
				VZPlayer.on('stop', function(){
					$('.main-article .img .play').show();
					$('.main-article .img .pause').hide();
				});
			});
		});
		options.events = {
			dtvAccount: {
				album: 'SXHj5a3m'
			}
		};
		VZPlayer.player.ui(options);
		new JWReader(['gcYnCI81', 'rCfhoIMy', 'aGU5dGEB', 'iauVB9O2', 'BAQgMy37', 'suHAj6Ac', 'a1m3ti66', 'ial9qeN1']).start();
	}
</script>

