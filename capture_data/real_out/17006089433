<script type="module">
	import { TOP } from "/js/es6/all.min.js?v2.478";
	TOP.host = 'https://mano.vz.lt';
	TOP.init();
</script>

		<div class="poll_sidebar_parent"></div>
<script>
    $(document).ready(function(){
        var poll_host = 'https://mano.vz.lt/';
        var p = $.ajax({
            url: poll_host + "api/public/surveys/homepage",
            datatype: 'json',
            type: 'GET',
            //cache: true,
            success: function(response){
                if(response != undefined){
                    var id = response.data.id;
                    var html = `<div class="vz_poll_wrapper">
                        <div class="vz_poll">
                            <div class="poll_branding">vž | apklausa
                                <button class="show-poll">
                                    <span>Atidaryti</span>
                                    <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                                        <path fill-rule="evenodd" clip-rule="evenodd" d="M7.29289 5.29289C6.90237 5.68342 6.90237 6.31658 7.29289 6.70711L10.5858 10L7.29289 13.2929C6.90237 13.6834 6.90237 14.3166 7.29289 14.7071C7.68342 15.0976 8.31658 15.0976 8.70711 14.7071L12.7071 10.7071C13.0976 10.3166 13.0976 9.68342 12.7071 9.29289L8.70711 5.29289C8.31658 4.90237 7.68342 4.90237 7.29289 5.29289Z" fill="#aeaeae"/>
                                    </svg>
                                </button>
                            </div>
                            <div class="poll_title">
                                <h4 class="shimmer"></h4>
                            </div>
                            <!-- Query -->
                            <div class="poll_options">
                                <form action="#" method="POST">
                                    <div class="options_container">
                                        <div class="option-placeholder shimmer"></div>
                                        <div class="option-placeholder shimmer"></div>
                                        <div class="option-placeholder shimmer"></div>
                                    </div>
                                    <input type="submit" value="Balsuoti" /> 
                                    <a href="javascript:;" class="close_poll">Uždaryti</a>
                                </form>
                            </div>
                            <!-- Results -->
                            <div class="poll_answers">
                                <form action="#" method="POST">
                                    <div class="answers_container">
                                        <div class="option-placeholder shimmer"></div>
                                        <div class="option-placeholder shimmer"></div>
                                        <div class="option-placeholder shimmer"></div>
                                    </div>
                                    <div class="poll_summary">
                                        <p>Ačiū už atsakymą!</p>
                                        <div>Iš viso atsakė: <span class="poll_total">0</span></div>
                                    </div>
                                    <button class="hide-poll" type="button">Uždaryti</button>
                                </form>
                            </div>
                        </div>
                    </div>`;
                    $('.poll_sidebar_parent').html(html);
                    var container = $('.vz_poll');
                        container.attr('poll-id', id);
                    //
                    if(response.data.related_story !== null){
                        var related_html = document.createElement('div');
                        related_html.classList.add('related-story');
                        (response.data.related_story.title != null) ? related_html.innerHTML = `<p>Plačiau: <a href="${response.data.related_story.url}">${response.data.related_story.title}</a></p>` : related_html.innerHTML = `<p class="text-center"><a href="${response.data.related_story.url}">Plačiau skaitykite čia</a></p>`;
                        if(container.find('.related-story').length == 0){
                            container.append(related_html);
                        }
                    }
                    //
                    var poll_id = container.attr('poll-id');
                    var poll_title = container.find('.poll_title h4');
                    if(localStorage.getItem('poll-'+id) === null){
                        var poll_questions_container = container.find('.options_container');
                        var poll_form = container.find('.poll_options form');
                        poll_title.text(response.data.question);
                        poll_title.removeClass('shimmer');
                        var questions = '';
                        for(var i = 0; i < response.data.answers.length; i++){
                            var question = `
                                <div class="input_group">
                                    <input id="poll-${response.data.answers[i].id}" data-id="${response.data.answers[i].id}" type="radio" name="poll_answer" data-val="true"/>
                                    <label for="poll-${response.data.answers[i].id}">${response.data.answers[i].answer}</label>
                                </div>
                            `;
                            questions += question;
                        }
                        poll_questions_container.html(questions);
                        poll_form.validate({
                            ignore: [],
                            rules: {
                                poll_answer: {
                                    required: true
                                }
                            },
                            messages: {
                                poll_answer: {
                                    required: 'Prašome pasirinkti atsakymą.'
                                }
                            },
                            errorPlacement: function(error, element) {
                                $('.options_container').after(error);
                            },
                            submitHandler: function(form){
                                var poll_id = container.attr('poll-id');
                                var checked = $(form).find('input:checked');
                                var checked_id = checked.attr('data-id');
                                var uid = uuidv4();
                                $.ajax({
                                    url: poll_host + "api/public/surveys/" + poll_id + "/vote/" + checked_id,
                                    type: 'POST',
                                    data: {user_hash: uid},
                                    cache: false,
                                    success: function(response){
                                        if(response.data.status !== false){
                                            localStorage.setItem('poll-' + poll_id, response.data.user_hash);
                                            render_poll_answers(poll_id, response.data.survey);
                                        }
                                    },
                                    error: function(err){
                                        var error = 'Įvyko serverio klaida.'
                                        $('.options_container').after(error);
                                    }
                                });
                            }
                        });
                    }else{
                        //render_poll_answers(id);
                        var poll_answers_container = container.find('.answers_container');
                        var answers = '';
                        var answers_sum = 0;
                        poll_title.text(response.data.question);
                        poll_title.removeClass('shimmer');
                        for(var i = 0; i < response.data.answers.length; i++){
                            var answer = `
                                <div class="poll_answer">
                                    <span class="answer_title">${response.data.answers[i].answer}</span>
                                    <div class="answer_progress">
                                        <div class="percentage">${response.data.answers[i].percentage}%</div>
                                        <div class="visual-bar" style="width:${response.data.answers[i].percentage}%">
                                            <span>(${response.data.answers[i].votes})</span>
                                        </div>
                                    </div>
                                </div>
                            `;
                            answers += answer;
                            answers_sum += response.data.answers[i].votes;
                        }
                        poll_answers_container.html(answers);
                        container.find('.poll_total').text(answers_sum);
                        container.addClass('show-answers');
                    }
                }
            },
            error: function(){
                $('.poll_parent').remove();
            }
        });
        p.done(function(data, textStatus, jqXHR) {
            if(jqXHR.status !== 200) {
                return;
            }
        });
    });
</script>
