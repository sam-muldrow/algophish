function showtable(shift,chartabledirection)
{
	row = 2*(chartabledirection?1:0) + (shift?1:0);
	for (var i=0;i<chartable[row].length;i++)  
	{
		if ($("#charbutton"+i).length) {
			if (chartable[row][i].length>0) 
			{
				$("#charbutton"+i).val(chartable[row][i]);
			}
			else
			{
				$("#charbutton"+i).val(" ");
			}
		}
	}
	setfoc();
}
