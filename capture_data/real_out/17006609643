<script>
  var installAppBanner = {};
  installAppBanner.isIos = /iPhone|iPad|iPod/i.test(navigator.userAgent);
  installAppBanner.isAndroid = /(android)/i.test(navigator.userAgent);

  installAppBanner.show = function () {
    
    if (!(installAppBanner.isIos || installAppBanner.isAndroid)) return false;
    if (installAppBanner.el != null) {
      var applicationType = document.getElementById('js-hook-for-application-type');
      if (installAppBanner.isIos) {
        applicationType.innerText = 'iPhone Uygulaması';
      } else {
        applicationType.innerText = 'Android Uygulaması';
      }
    }
  };
  installAppBanner.redirect = function () {
    window.location.href = "/mobil-uygulama"
  };

  window.addEventListener("DOMContentLoaded", function () {
    installAppBanner.el = document.getElementById("js-hook-for-install-app-banner");
    installAppBanner.show();
  });
</script>

