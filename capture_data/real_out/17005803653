                $j("#divAdRightTopBox2").show();
            */
            if (!IsIsrael) {
                $j(".one-article").each(function (i) {
                    $j(this).html($j(this).html()
                        .replace("צפו בתקצירים", "")
                        .replace("צפו בתקציר", "")
                        .replace("צפו באירועים", "")
                        .replace("צפו באירוע", ""));
                });
            }

            window.OneVote.Init(".home-page-vote");

            window.setTimeout("document.location=document.location", 7 * 60 * 1000);

            
            window.setTimeout(function () {

                
                if ($j("#adxONEElement_Winner").css("display") === "block")
                    $j("#adxONEElement_Winner").css("margin-bottom", "2rem");

                if ($j("#adxONEPromotion").css("display") === "block")
                    $j("#adxONEPromotion").css("margin-bottom", "2rem");
            }, 5000);
            
        })
    </script>



