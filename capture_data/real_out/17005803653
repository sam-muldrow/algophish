    <script type="text/javascript">

        function ToggleONEAccessibility() {

            $j("a,input,button,textarea").addClass("one-accessibility");
            StartAccessibility();
            $j('.btn_accessibility').hide();

            $j("#tblTopMenuItems a:first").focus();
        }

        function AddAccessibilityKeyboard() {
            var accessTag = '<a role="button" tabindex="2" id="accessibility_action0" href="javascript:void(0);" onclick="ToggleONEAccessibility();return false;" class="btn_accessibility_action accessibility_component" style="z-index: 2147483644; visibility: visible; top: 163px; width: 280px; height: 42px;" title="ניווט מקלדת"><img loading=lazy src="https://www.negishim.com/accessibility/contrast_18_black.png" alt="keyboard" class="accessibility_component" style="border:0" aria-hidden="true" tabindex="-1"><span class="accessibility_component" aria-hidden="true" tabindex="-1">ניווט מקלדת</span></a>';

            $j("#accessibility_action3").text("ניווט מקלדת").attr("title", "ניווט מקלדת").attr("onclick", "ToggleONEAccessibility();return false;");
        }

        $j(document).ready(function () {

            

            AddAccessibilityKeyboard();
            /* reset all tabindex */
            $j("*").attr("tabindex", -1);

            /* set default tabindex */
            $j("a,input,button,textarea").attr("tabindex", 10000);

            $j("#aAccessibility").addClass("one-accessibility").attr("tabindex", 1);
            $j(".accessibility_component a").addClass("one-accessibility").attr("tabindex", 2);

            /* top menu */
            $j("#tblTopMenuItems a").attr("tabindex", 10);

            // MAIN PAGE
            /* top article */
            $j("#TopArticle a").attr("tabindex", 20);

            /* secondary articles */
            $j(".one-article-secondary").attr("tabindex", 30);

            /* main strip articles */
            $j(".one-hp-left-container-column-articles a").attr("tabindex", 40);

            /* Videos */
            $j("#MainVideos a").attr("tabindex", 50);

            /* RIGHT MENU */
            $j(".one-navigation-right-container a").attr("tabindex", 100);

            /* Left extra column / glitches, oppinions, etc */
            $j(".one-hp-left-container-column-extras a").attr("tabindex", 200);

            /* ARTICLE PAGE */
            $j(".article-body-container p a").attr("tabindex", 20);

            /* glitches */
            $j("#glitches-items a, .glitch-body p a").attr("tabindex", 20);
            $j("#glitches-navigator a").attr("tabindex", 30);


            /* Some excludes */
            $j("a[href*='CommentsTitler']" ).attr( "tabindex", -1);
            
            
        })

    </script>

