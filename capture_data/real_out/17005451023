            <div id="cll-tab-1" class="tab__cnt cll">

                <div id="cll-1" class="cll__header d-lg-none " data-toggle="collapse" data-target="#tab-cnt-1" aria-controls="tab-cnt-1" aria-expanded="true" data-tabs-content-id="1" data-id="1">
                    <div class="cll__title">Filiali e ATM</div>
                </div>

                <div id="tab-cnt-1" class="collapse tab-pane fade show" aria-labelledby="cll-1" role="tabpanel" data-id="1">
    
                    <div id="panel-filiali">
                    
                        <form method="get" action="/contatti/filiali-atm.html" onsubmit="return validateFiliali()">

