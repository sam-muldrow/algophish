    var loadReviewsIoRatingSnippets = function () {
      if(typeof reviewsIoStore !== 'undefined') {
        ratingSnippet("ruk_rating_snippet",{
            store: reviewsIoStore,
            color: '#000',
            
              textClr: '#333',
            
            linebreak: false,
            
            usePolaris: true,
            
            
            
            
            
              text: 'Reviews',
            
