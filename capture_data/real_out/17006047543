        function SetIsSboTop(domains) {
            const url = window.location.href;
            var isSboTop = false;
            for (let i = 0; i < domains.length; i++) {
                if (url.indexOf(domains[i]) > -1) {
                    isSboTop = true;
                    break;
                }
            }
            return isSboTop;
        }
