      data-config="[&quot;.carousel-recently_returned&quot;, 6, 5, 4, 3, 2, 1, {&quot;url&quot;: &quot;/browse.json?q=&amp;subject=&amp;sorts=lending___last_browse desc&quot;, &quot;pageMode&quot;: &quot;page&quot;, &quot;limit&quot;: 18}]">
      

