        <script>
            // JavaScript
function updateRecTitle() {
    var whatsNew = document.querySelectorAll('.tab .tab-title:last-child')[0];
    var bestSellerRecTitle = document.querySelectorAll('.tab .tab-title:last-child')[1];
    var recTitle = document.querySelectorAll('.tab .tab-title:last-child')[2];
    // if(pangeaCore?.isMobileOrTablet()) {
    if(window.innerWidth <= 767) {

        recTitle.innerText = "Recommended";
        bestSellerRecTitle.innerText = "Best Sellers";
        whatsNew.innerText = "New";
    } else {
        recTitle.innerText = "RECOMMENDED"; 
        bestSellerRecTitle.innerText = "BEST SELLERS";
        whatsNew.innerText = "NEW";
    }
}

if(document.readyState =="complete" || document.readyState == "interactive"){
    updateRecTitle();
} else {
    document.addEventListener("DOMContentLoaded", updateRecTitle);
}

window.addEventListener("resize", updateRecTitle);

        </script>
    </div>

    
    



    

