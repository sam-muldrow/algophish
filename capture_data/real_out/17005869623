                        {bidder: 'adpone',params: {placementId: '122221132130535'}},
                        {bidder: 'sspBC'},
                        {bidder: 'rubicon',params: {accountId: '23572', siteId: '484512', zoneId: '2872694'}}
                    ],
                    mediaTypes: {
                        banner: {
                            sizes: [[300, 250], [300, 600]]
                        }
                    }
                }, {
                    "code": rectangle2,
                    "bids":[
                        {bidder: "criteo", params:{"networkId":8796}},
                        {bidder: 'teads',params: {pageId: '178116',placementId: '193121'}},
                        {bidder: 'adpone',params: {placementId: '12075105549222'}},
                        {bidder: 'adpone',params: {placementId: '1206811395453'}},
                        {bidder: 'adpone',params: {placementId: '122221918113'}},
                        {bidder: 'adpone',params: {placementId: '122221132130535'}},
                        {bidder: 'sspBC'},
                        {bidder: 'rubicon',params: {accountId: '23572', siteId: '484512', zoneId: '2872694'}}
                    ],
                    mediaTypes: {
                        banner: {
                            sizes: [[300, 250], [300, 600]]
                        }
                    }
                }
            ];

            pbjs.setConfig({
                consentManagement: {
                    gdpr: {
                        cmpApi: 'iab',
                        timeout: 8000,
                        defaultGdprScope: true
                    }
                },
                userSync: {
                    filterSettings: {
                        all: {
                            bidders: '*',   // '*' means all bidders
                            filter: 'include'
                        }
                    },
                    userIds: [{
                        name: 'teadsId',
                        params: {
                            pubId: 26540
                        }
                    }]
                },
                enableSendAllBids: true,
                priceGranularity: customPriceGranularity,
                currency: {
                    adServerCurrency: 'EUR',
                },
                cache: {url: true},
                bidderTimeout: BIDDER_TIMEOUT,
            });
            pbjs.addAdUnits(adUnits);

            pbjs.bidderSettings = {
                standard: {
                    adserverTargeting: [{
                        key: "hb_bidder",
                        val: function(bidResponse) {
                            return bidResponse.bidderCode;
                        }
                    }, {
                        key: "hb_adid",
                        val: function(bidResponse) {
                            return bidResponse.adId;
                        }
                    }, {
                        key: "hb_pb",
                        val: function(bidResponse) {
                            return bidResponse.pbCg;
                        }
                    }, {
                        key: 'hb_size',
                        val: function (bidResponse) {
                            return bidResponse.size;
                        }
                    }, {
                        key: 'hb_source',
                        val: function (bidResponse) {
                            return bidResponse.source;
                        }
                    }, {
                        key: 'hb_format',
                        val: function (bidResponse) {
                            return bidResponse.mediaType;
                        }
                    }],
                    storageAllowed: true
                }
            }

            //pbjs.requestBids({bidsBackHandler: sendAdserverRequest});

            pbjs.enableAnalytics([{
                provider: 'adagio'
            }]);

            pbjs.requestBids({
                timeout: PREBID_TIMEOUT,
                bidsBackHandler: function (bidResponses) {
                    initAdserver();
                }
            });
        });


        function sendAdserverRequest() {
            if (pbjs.adserverRequestSent) return;
            pbjs.adserverRequestSent = true;
            cX.callQueue.push(['invoke', function() {googletag.cmd.push(function() {
                pbjs.que.push(function() {
                    pbjs.setTargetingForGPTAsync();
                    googletag.pubads().refresh();
                });
            });}]);
        }

        setTimeout(function() {
            sendAdserverRequest();
        }, FAILSAFE_TIMEOUT);

    </script>
