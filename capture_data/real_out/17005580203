  // header pc
  const headerNavItems = document.querySelectorAll('.header-nav-item');
  const headerElement = document.querySelector('header');
  const sideMenuElement = document.getElementById('side-menu-container');
  const openHover = (event) => {
    const headerNavItem = event.target;
    if (headerNavItem.dataset.target == 3) {
      closeHover();
    } else {
      headerElement.dataset.target = headerNavItem.dataset.target;
      sideMenuElement.dataset.target = headerNavItem.dataset.target;
      headerElement.classList.add('is-hover');
      sideMenuElement.classList.add('is-open');
    }
  };
  const closeHover = (_event) => {
    headerElement.dataset.target = '';
    sideMenuElement.dataset.target = '';
    headerElement.classList.remove('is-hover');
    sideMenuElement.classList.remove('is-open');
  };
  headerNavItems.forEach(elm => {
    elm.addEventListener('mouseover', openHover);
    headerElement.addEventListener('mouseleave', closeHover);
  });

  // header sp
  const spMenuElement = document.getElementById('sp-menu-container');
  const headerNavMenu = document.querySelector('.header-nav-menu');
  const toggleSpMenu = (_e) => {
    headerNavMenu.classList.toggle('is-active');
    spMenuElement.classList.toggle('is-open');
  };
  headerNavMenu.addEventListener('click', toggleSpMenu);

  // firstView
  const slideParent = document.querySelector('.firstview-container');
  const slide = document.querySelector('.firstview');
  const meServiceNavigation = document.querySelector('.services-navigation[data-target="me"]');
  const bizServiceNavigation = document.querySelector('.services-navigation[data-target="biz"]');

  let slideIntervalId;

  // スライドショーの開始と再開を行う関数
  function startSlideshow() {
    slideIntervalId = setInterval(() => {
      toggleSlide()
    }, 6000);
  }

  function toggleSlide(nextDomain = null) {
    if (nextDomain != null && slide.classList.contains('biz') && nextDomain == 'biz') {
      return;
    } else if (nextDomain != null && !slide.classList.contains('biz') && nextDomain == 'me') {
      return;
    }
    slide.classList.toggle('biz');
    if (slide.classList.contains('biz')) {
      slideParent.classList.add('biz-appear');
      slideParent.classList.remove('me-appear');
      bizServiceNavigation.classList.add('is-active');
      meServiceNavigation.classList.remove('is-active');
    } else {
      slideParent.classList.remove('biz-appear');
      slideParent.classList.add('me-appear');
      bizServiceNavigation.classList.remove('is-active');
      meServiceNavigation.classList.add('is-active');
    }
  }

  function restartSlide(e) {
    let nextDomainTarget = e.target;
    if (e.target.tagName == 'IMG') {
      nextDomainTarget = e.target.parentNode;
    }
    clearInterval(slideIntervalId);
    toggleSlide(nextDomainTarget.dataset.target)
    startSlideshow();
  }

  // 初期のスライドショーの開始
  startSlideshow();
  const selectViews = document.querySelectorAll('.services-navigation-container .services-navigation');
  // スライドのクリックイベント
  selectViews.forEach(elm => {
    elm.addEventListener('click', restartSlide);
  });

  // リサイズ時のコンテンツの高さ変更
  let checkingResizeTimer = 0;
  function resetContentSize(e) {
    e.preventDefault();
    if (checkingResizeTimer > 0) {
      clearTimeout(checkingResizeTimer);
    }

    checkingResizeTimer = setTimeout(function () {
      document.querySelectorAll('.service-item').forEach(elm => {
        const detailMenuContainer = document.querySelector('.service-item-detail-menu-container[data-target="' + elm.dataset.target + '"]');
        let innerHeight = detailMenuContainer.getBoundingClientRect().height;
        elm.style.height = (innerHeight + 100) + 'px';
      });
    }, 300);
  }
  window.addEventListener("resize", resetContentSize);

  // services
  const toggleAccordion = (event) => {
    let targetElement = event.target;
    if (targetElement.tagName == 'IMG') {
      targetElement = targetElement.parentNode.parentNode;
    } else if (!targetElement.classList.contains('service-item-wrapper')) {
      targetElement = targetElement.parentNode;
    }
    const serviceItem = document.querySelector('.service-item[data-target="' + targetElement.dataset.target + '"]');
    const detailMenuContainer = document.querySelector('.service-item-detail-menu-container[data-target="' + targetElement.dataset.target + '"]');
    const itemAddElement = document.querySelector('.service-item-add[data-target="' + targetElement.dataset.target + '"]');
    serviceItem.classList.toggle('is-open');
    itemAddElement.classList.toggle('is-remove');

    if (serviceItem.classList.contains('is-open')) {
      let innerHeight = detailMenuContainer.getBoundingClientRect().height;
      serviceItem.style.height = (innerHeight + 100) + 'px';
    } else {
      serviceItem.style.height = '100px';
    }
  };

  const serviceItemWrapper = document.querySelectorAll('.service-item-wrapper');
  serviceItemWrapper.forEach(elm => {
    elm.addEventListener('click', toggleAccordion);
  });
  window.addEventListener('scroll', function () {
    let contentsDivs = document.querySelectorAll('.service-item');
    contentsDivs.forEach(function (divElement) {
      let boxNum = divElement.dataset.target;
      let scrollTop = window.pageYOffset || document.documentElement.scrollTop;
      let areaTop = divElement.getBoundingClientRect().top + window.pageYOffset;
      if (scrollTop > areaTop - 400) {
        let imagesElement = document.querySelector('.services-images .scroll-box0' + boxNum);
        if (imagesElement) {
          imagesElement.classList.add('active');
        }
      } else {
        let imagesElement = document.querySelector('.services-images .scroll-box0' + boxNum);
        if (imagesElement) {
          imagesElement.classList.remove('active');
        }
      }
    });
  });

  const headerContainer = document.querySelector('.header-container');
  const buffer = headerContainer.clientHeight;
  const anchorLinks = document.querySelectorAll('.service-anchor-link a');

  function handleSmoothScroll(e) {
    e.preventDefault();
    const bufferValue = e.target.dataset.buffer == 'default' ? buffer : buffer + 80;
    const href = e.target.getAttribute('href');
    const targetElement = document.querySelector(href);
    const rectTop = targetElement.getBoundingClientRect().top;
    const offsetTop = window.scrollY;
    const scrollTop = rectTop + offsetTop - bufferValue;
    window.scrollTo({
      left: 0,
      top: scrollTop,
      behavior: 'smooth'
    });
  }

  anchorLinks.forEach(elm => {
    elm.addEventListener('click', handleSmoothScroll);
  });
})();</script></div></div><!-- Modal -->
