<script>$(document).ready(function() {
  $('#js-form-change-group').on('change',function(){
    var this_val = $(this).find(':selected').val();
    if(this_val == 'create_group'){
      location.href = '/groups';
    }else{
      $(this).submit();
    }
  });
});</script></body></html>