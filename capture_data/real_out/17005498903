    if (!GetCookie('mobile')) {
        va = $('a[name=\'' + location.hash.replace('#', '') + '\']').offset().top;
        setTimeout(function () {
            $(window).scrollTop(va - 105);
        }, 100);
        setTimeout(function () {
            $(window).scrollTop(va - 105);
        }, 300);
    }
    hashbrown();
}

$('img.lazy').lazyload();
</script>
