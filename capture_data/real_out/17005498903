<script type="text/javascript">
var lastscroll=lastaction=last_sidead3_top=0;

$(window).scroll(function(){
    if(!last_sidead3_top) last_sidead3_top=$('#sidead3').css('top');

    if($(window).scrollTop()<50){ //올라갈때
        if(lastaction!=1){
            $('#menuall').stop().animate({'top':'0'});
            $('.sidead3').stop().animate({'top':last_sidead3_top});
        }
        lastaction=1;
    }else if(lastscroll-30>$(window).scrollTop()){ //올라갈때
        if(lastaction!=1){
            $('#menuall').stop().animate({'top':'0'});
        }
        lst=($(window).scrollTop()>135)?'87px':last_sidead3_top;
        $('.sidead3').stop().animate({'top':lst});
        lastaction=1;
    }else if(lastscroll+30<$(window).scrollTop()){ //내려갈때
        if(lastaction!=2){
            $('#menuall').stop().animate({'top':'-41px'});
            $('.sidead3').stop().animate({'top':'47px'});
        }
        lastaction=2;
    }
    lastscroll=$(window).scrollTop();
});

