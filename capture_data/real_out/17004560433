            puid5: ':',
            puid6: (mobIndicator?1:2)
        }
    });
});

window.yaContextCb.push(()=>{
        Ya.adfoxCode.create({
            ownerId: 323017,
            containerId: 'bottom-sticky',
            params: {
                pp: 'cxyy',
                ps: 'dntn',
                p2: 'hfkp',
                puid1: '/',
                puid2: '',
                puid3: '',
                puid4: 'index',
                puid5: ':',
                puid6: (mobIndicator?1:2)
            }
        })
});
}
</script>
