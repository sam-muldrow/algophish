<script>
 var showSignupPopupWithRedirectUrl = function () {
 fs.login.showSignupPopup({redirect: '/usercontact/'});
 };

 fs.signup.start.controllerUrl = '/signup/signup/';
 (function () {

 var counter = {
 init: function () {
 var el = document.querySelector('#user-count');
 var value = 6117601;
 var $cookie = $.cookie('seo_user_count');

 value = $cookie ? parseInt($cookie) : value;

 if (!$cookie) {
 $.cookie('seo_user_count', value, {path: '/' });
 }

 var od = new Odometer({
 el: el,
 value: value,
 duration: 200,
 format: '( ddd)'
 });

 var interval = setInterval(function(){
 value = value + Math.floor(Math.random() * 6) + 1;
 od.update(value);
 $.cookie('seo_user_count', value, {path: '/' })
 }, 3000);
 }
 };

 window.land = {
 lastPage: false,
 init: function () {
 var self = this;

 $('body').addClass('newmain--body');

 self.style();

 $('.signup-sex__button').on('click', function (e) {
 self.toggleToSignupForm();
 var sex = e.target.getAttribute('data-sex');
 $('input[value="' + sex + '"]').attr('checked', true);
 });

 $('.js_signup_bottom_button').on('click', function (e) {
 $("html, body").animate({ scrollTop: 0 }, "fast");
 self.toggleBack();
 });

 $('.js_signup_login_bottom_button').on('click', function (e) {
 $("html, body").animate({ scrollTop: 0 }, "fast");
 self.toggleToLogin();
 return false;
 });


 $('.signup-forms__back').on('click', function (e) {
 self.toggleBack();
 });

 $('.signup-top__button').on('click', function (e) {
 self.toggleToLogin();
 return false;
 });
 $('.newmain__top-link--signup').on('click', function (e) {
 self.toggleToLogin();
 $('.signup-top__button').toggleClass('h');
 return false;
 });
 $('.js-hide-logout-bye-screen').on('click', function (e) {
 self.toggleToLogin();
 return false;
 });
 $('.signup-forms__back').on('click', function () {
 if (!$('.newmain__top-link--signup-signup').hasClass('h')) {
 $('.signup-top__button').toggleClass('h');
 }
 });
 $('.newmain__top-link--signup-signup')
 .on('click', function (e) {
 self.toggleBack();
 $('.signup-top__button').toggleClass('h');
 return false;
 });
 },
 toggleToSignupForm: function () {
 this.lastPage = 1;

 // vanila js + jquery for fade animation
 $('.signup-body__main').fadeOut(0, function () {
 $('.signup-body__forms--reg').show();
 $('.signup-body__forms--login').hide();
 $('.signup__body').removeClass('signup__body--mirror');
 $('.signup__body').addClass('signup__body--short');
 $('.signup-body__forms').fadeIn(0);
 $('#userName').focus();
 });
 },
 toggleBack: function() {
 if (this.lastPage === 1) {
 $('.signup-body__forms').fadeOut(0, function () {
 $('.signup__body').removeClass('signup__body--short signup__body--mirror');
 $('.signup-body__main').fadeIn(0)
 });
 } else if (this.lastPage === 2) {
 $('#captchaField, .__recaptcha-link').fadeOut(0, function () {
 $('.signup-forms__common').fadeIn();
 $('#btnRegister').text('Зарегистрироваться').removeClass('__short-btn');
 land.lastPage = 1;
 });
 } else if (this.lastPage === 3) {
 $('.signup-body__forms').fadeOut(0, function () {
 $('.signup__body').removeClass('signup__body--short signup__body--mirror');
 $('.signup-body__forms--reg').show();
 $('.signup-body__forms--login').hide();
 $('.signup-body__main').fadeIn(0)
 });
 } else if (this.lastPage === 4) {
 $('#captchaFieldLogin, .__recaptcha-link').fadeOut(0, function () {
 $('#social-form-login').show();
 $('.signup-forms__common').fadeIn();
 $('#loginPopupSubmitButton').text('Войти на сайт').removeClass('__short-btn');
 land.lastPage = 3;
 });
 } else if (this.lastPage === 5) {
 iPopup.close();
 land.lastPage = 1;
 }
 },
 toggleToLogin: function() {
 $('.js-logout-bye-screen').addClass('h');
 if (this.lastPage === 5) {
 this.toggleBack();
 }
 this.lastPage = 3;
 $('.signup-body__main').fadeOut(0, function () {
 $('.signup__body').addClass('signup__body--short signup__body--mirror');
 $('.signup-body__forms--reg, .__recaptcha-link').hide();
 $('.signup-body__forms--login').show();
 $('.signup-body__forms').fadeIn();
 $('#user_email').focus();
 });
 },
 style: function () {
 // terms
 $('#register-form .terms-agree-container')
 .addClass('signup-forms__common');

 // signup username
 $('#userName')
 .attr('placeholder', '')
 .parent().parent()
 .addClass('signup-forms__common')
 .find('label').text('Ваше имя');

 // signup birthday
 $('.signup-user-birthday')
 .addClass('signup-forms__common')
 .find('label').text('Дата рождения');

 // signup email or phone
 $('#userEmail')
 .attr('placeholder', '')
 .parent().parent()
 .addClass('signup-forms__common')
 .find('label').text('Номер телефона или email');

 $('.signup-user-email')
 .append('<span class="phone-mail-help">Например, +7 (ХХХ) ХХХ-ХХ-ХХ или ivanov@mail.ru</span>');

 // signup captcha
 var $captha = $('#signup-form-captcha');
 var $elem = $('<div class="captcha__label"><label>Введите символы с картинки,<br> чтобы подтвердить, что вы не робот</label></div>');
 $captha.parent().attr('id', 'captchaField');
 $elem.insertBefore($captha);

 // signup recaptcha link
 var $recaptchaLink = $('<div class="__recaptcha-link" onclick="fs.signup.reloadCaptchaImg(); return false;">Показать другую картинку</div>');
 $('#label-user-btn').append($recaptchaLink);

 // login email or phone
 $('#user_email')
