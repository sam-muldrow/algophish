 var checkUrlInData = function(urls) {
 var urlsLength = urls.length;
 var path = location.pathname;
 var finded = false;

 if (urls.length) {
 for (var i = 0; i < urlsLength; i++) {
 if (path.search(urls[i]) >= 0) {
