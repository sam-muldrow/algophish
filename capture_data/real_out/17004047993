<script>
 //var $ticker = $('#activity-rating-counter');
 var $tasks = $('.rating-questline-wrapper');
 var $cookieAgreement = $('.footer-cookie-agreement');

 //$ticker.hide('slow');
 $tasks.hide('slow');

 function cookieAgreement(opts) {
 opts = opts || {};
 if (window.currentUser) {
 _ajax.get('/cookieCheck/ajax/check/', opts, function() {
 $cookieAgreement.hide('slow');
 //$ticker.show('slow');
 $tasks.show('slow');
 });
 } else {
 $cookieAgreement.hide('slow');
 }
 }
</script>

