      <r-viewmode class="r-viewmode--niveau-2 r-layout--video r-viewmode--o-niveau-2 -md-only -arche">
          <i class="r-m" id="1399138" data-extref="rossel_extref_B9735485544Z.1"></i>
    <article  class="r-viewmode--article r-layout--video">
