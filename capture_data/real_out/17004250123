                            $(".awards_footer .af_award .af_icon_award_9").css('background', 'url(/i/img/site/footer/awards_2.png) 10px -181px no-repeat');
                            $(".awards_footer .af_award .af_icon_award_8").css('background', 'url(/i/img/site/footer/awards_2.png) 10px -284px no-repeat');
                            $(".awards_footer .af_award .af_icon_award_7").css('background', 'url(/i/img/site/footer/awards_2.png) 10px -371px no-repeat');
                            $(".awards_footer .af_award .af_icon_award_6").css('background', 'url(/i/img/site/footer/awards_2.png) 10px -457px no-repeat');
                            $(".awards_footer .af_award .af_icon_award_5").css('background', 'url(/i/img/site/footer/awards_2.png) 10px -555px no-repeat');
                            $(".awards_footer .af_award .af_icon_award_4").css('background', 'url(/i/img/site/footer/awards_2.png) 10px -639px no-repeat');
                            $(".awards_footer .af_award .af_icon_award_3").css('background', 'url(/i/img/site/footer/awards_2.png) 10px -718px no-repeat');
                            $(".awards_footer .af_award .af_icon_award_2").css('background', 'url(/i/img/site/footer/awards_2.png) 10px -802px no-repeat');
                            $(".awards_footer .af_award .af_icon_award_1").css('background', 'url(/i/img/site/footer/awards_2.png) 10px -871px no-repeat');
                        });*/

                        // код для крутилки .awards_footer
                        var awards_width   = 795,
                            awardsEu_width = 1200,
                            awards_status   = false,
                            awardsEu_status = false;

                                                    UpdateAwardsBlock('awardsCom', awards_width, awards_status);
                                                $(window).resize(function() {
                                                            UpdateAwardsBlock('awardsCom', awards_width, awards_status);
                                                    });

                        function SlickStartAwards(){
                            awards_status = true;
                            $('.awards_footer').slick({
                                centerMode: true,
                                slidesToShow : 9,
                                slidesToScroll: 1,
                                arrows: false,
                                                            dots: false,
                                variableWidth: true,
                                responsive: [
                                    {
                                        breakpoint: 1172,
                                        settings: {
                                            slidesToShow : 7
                                        }
                                    },
                                    {
                                        breakpoint: 950,
                                        settings: {
                                            slidesToShow : 5
                                        }
                                    },
                                    {
                                        breakpoint: 750,
                                        settings: {
                                            slidesToShow : 3
                                        }
                                    },
                                    {
                                        breakpoint: 420,
                                        settings: {
                                            slidesToShow : 1
                                        }
                                    }
                                ]
                            });
                        }

                        function SlickStartAwardsEu(){
                            awardsEu_status = true;
                             $('.awards_sponsors_footer .asf_award, .awards_sponsors_footer .asf_sponsor').slick({
                                centerMode: true,
                                slidesToShow : 1,
                                slidesToScroll: 1,
                                arrows: false,
                                                            dots: false,
                                variableWidth: true
                            });
                        }

                        function SlickStopAwards(){
                            awards_status = false;
                            $('.awards_footer').slick('unslick');
                        }

                        function SlickStopAwardsEu(){
                            awardsEu_status = false;
                            $('.awards_sponsors_footer .asf_award, .awards_sponsors_footer .asf_sponsor').slick('unslick');
                        }

                        function viewport() {
                            var e = window, a = 'inner';
                            if (!('innerWidth' in window )) {
                                a = 'client';
                                e = document.documentElement || document.body;
                            }
                            return { width : e[ a+'Width' ] , height : e[ a+'Height' ] };
                        }

                        function UpdateAwardsBlock(awardsBlock, width, status){
                            let awardsWindowsWidth = viewport().width;

                            if(awardsWindowsWidth < width)
                            {
                                if(!status)
                                {
                                    if(awardsBlock == 'awardsCom') { SlickStartAwards(); }
                                    if(awardsBlock == 'awardsEu') { SlickStartAwardsEu(); }
                                }
                            }
                            else
                            {
                                if(status)
                                {
                                    if(awardsBlock == 'awardsCom') { SlickStopAwards(); }
                                    if(awardsBlock == 'awardsEu') { SlickStopAwardsEu(); }
                                }
                            }
                        }  // код для крутилки .awards_footer
                    });
                });
            </script>
