<script>
        $(document).ready(function(){
            $("#monitoring_widgets").intersectionObserver(function(element){

                var   block = $(element);
                var   url = 'https://www.instaforex.com/forexcopy_top5';

                $.ajax({
                    url: url,
                    type: 'get',
                    data: {action: 'get_top_traders_main'},
                    success: function(result) {
                        block.html(result);
                    },
                    error: function() {
                        console.log('error');
                    }
                });
            })
        });
    </script>
