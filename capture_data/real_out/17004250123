                                    if(parseInt(navbar.getBoundingClientRect().width) >= 878) {
                                        filtersMain.setAttribute('style', '');
                                    } else {
                                        filtersMain.setAttribute('style', 'width:100%!important;justify-content:flex-end!important;');
                                    }
                                }
