<script>
                $(function(){
                    $(document).mouseup(function (e){
                        let mcCurrentLang = $(".mc_current_lang"),
                            mcListLang = $(".mc_list_lang");
                        if((!mcListLang.is(e.target) && mcListLang.has(e.target).length === 0) && (!mcCurrentLang.is(e.target) && mcCurrentLang.has(e.target).length === 0)){
                            mcCurrentLang.removeClass('mc_current_hover');
                            mcListLang.removeClass('mc_current_click');
                        }
                    });
                });
                $(function() {
                    $(document).ready(function(){
                        //lazy load background
                        $(".mc_languages_socnetworks .mc_socnetworks").intersectionObserver(function (elem) {
                            $(".mc_socnetworks .mc_vk").css('background-image', 'url(/i/img/site/footer/vk.svg)');
                            $(".mc_socnetworks .mc_facebook").css('background-image', 'url(/i/img/site/footer/facebook.svg)');
                            $(".mc_socnetworks .mc_twitter").css('background-image', 'url(/i/img/support/social/twitter3.png)');
                            $(".mc_socnetworks .mc_youtube").css('background-image', 'url(/i/img/site/footer/youtube.svg)');
                            $(".mc_socnetworks .mc_rss").css('background-image', 'url(/i/img/site/footer/rss.svg)');
                            $(".mc_socnetworks .mc_linkedin").css('background-image', 'url(/i/img/site/footer/linkedin.svg)');
                        });
                        
                        let mcLanguages = $('.menu_footer .mc_languages');

                        $('.mc_current_lang').on('click', mcLanguages, function(){
                            if($('.mc_list_lang').hasClass('mc_current_click'))
                            {
                                $('.mc_list_lang').removeClass('mc_current_click');
                                $('.mc_current_lang').removeClass('mc_current_hover');
                            }
                            else
                            {
                                $('.mc_list_lang').addClass('mc_current_click');
                                $('.mc_current_lang').addClass('mc_current_hover');
                            }
                        });
                    });
                });
                
                
            </script>
