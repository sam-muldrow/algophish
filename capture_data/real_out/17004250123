	$(document).ready(function() {

        if ( $(window).width() < 769 ) {
            var is_opened = false;
            $('.drop-down').on('click', function() {
                var ul = $(this).find('ul');
                if (is_opened) {
                    ul.hide();
                    is_opened = false;
                } else {
                    ul.show();
                    is_opened = true;
                }
            });
        }

            });
</script>
