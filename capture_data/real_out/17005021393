		extend: 'cD0.3434490266',

		link_scan: function(){
			var rst = "|"+this.rest.join("|")+"|",
			r = new RegExp("^"+location.protocol+"//"+location.hostname+".*/a-(\\w+)/.*$"),
			l = new RegExp("login-(.*?)(\/|$)");
			go("a").each(function(ref, ext){
				var mt = this.href.match(r), lk = this.href.match(l);
				if (ref = (lk && lk[1]) || ref) ref = "/referer-"+ref;

				if (mt && rst.indexOf("|"+mt[1]+"|") != -1){
					this.setAttribute('data-recur', 'true');
					this.setAttribute('data-target', 'modal');
					this.setAttribute('data-type', 'window');
					this.setAttribute('data-href', '/a-logon/popup'+ref+'/extend-'+ext);
				}
			}, this.referer, this.extend);
		}
	}
