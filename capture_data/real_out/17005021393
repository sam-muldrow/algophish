		"–ò–≤–∞–Ω, 28", "–ê–ª–µ–∫—Å–∞–Ω–¥—Ä, 30", "–ò–≤–∞–Ω, 27", "–ê–ª–µ–∫—Å–µ–π, 29", "–ê–ª–µ–∫—Å–µ–π, 31", "–ï–≤–≥–µ–Ω–∏–π, 28",
		"Sergey, 29", "–ê–Ω–¥—Ä–µ–π, 26", "–ê–ª–µ–∫—Å–∞–Ω–¥—Ä, 29", "–†–æ–¥–∏–æ–Ω, 28", "–ê–ª–µ–∫—Å, 28"
	];

	var namesw = [
		"–ú–∞—Ä–∏–Ω–∞, 28", "–û–ª—å–≥–∞, 28", "–õ–µ–Ω–æ—á–∫–∞, 30", "–ê–ª–µ–Ω–∞, 22", "–ö—Ä–∏—Å—Ç–∏–Ω–∞, 27", "–û–ª—å–≥–∞, 28",
		"–ê–Ω—é—Ç–∫–∞, 22", "–ù–∞—Ç–∞–ª—å—è, 26", "–Ø–Ω–∞, 27", "–õ–∞–¥–∞, 26", "–ò–ª–æ–Ω–∞, 22", "Alias, 31",
		"–°–æ—Ñ—å—è, 23", "–ò–Ω–Ω–∞, 27", "–ú–∞—Ä–∏—è, 24", "–úelena, 33", "–ê–Ω–Ω–∞, 24", "–¢–∞—Ç—å—è–Ω–∞, 34",
		"–ê–ª–µ–∫—Å–∞–Ω–¥—Ä–∞, 23", "–ê–ª–∏–Ω–∞, 26", "–ö–∞—Ä–∏–Ω–∞, 26", "–ê–Ω–∞—Å—Ç–∞—Å–∏—è, 36", "–í–∞–ª–µ—Ä–∏—è, 27",
		"–ù–∞—Å—Ç–µ–Ω—å–∫–∞, 30", "Olya13, 26", "Tania, 27", "–°–≤–µ—Ç–ª–∞–Ω–∞, 35", "–û–ª—å–≥–∞, 29",
		"Irrrok, 23", "kamilla, 28", "Maya, 37", "–ê–Ω–∞—Å—Ç–∞—Å–∏—è, 27", "–ú–∞—Ä–∏–Ω–∞, 29",
		"Annnn, 27", "–ï–∫–∞—Ç–µ—Ä–∏–Ω–∞, 30", "–ö–∞—Ç—é—à–∫–∞, 28", "Black N., 25", "–ú–∞–π—è, 24",
		"–î–∞—Ä—å—è, 29", "–ï–ª–µ–Ω–∞, 36", "Julia_happy, 26", "–í–µ—Ä–∞, 25", "–ê–Ω–∞—Å—Ç–∞—Å–∏—è, 35",
		"–í–∏–∫—Ç–æ—Ä–∏—è, 29", "–ï–∫–∞—Ç–µ—Ä–∏–Ω–∞, 24", "life, 28", "–Æ–ª–∏—è, 28", "–ú–∞—Ä–≥–∞—Ä–∏—Ç–∞, 39",
		"–ï–ª–∏–∑–∞–≤–µ—Ç–∞, 27", "–ê–Ω–∞—Å—Ç–∞—Å–∏—è, 33"
	];

	if (!pol) pol = Math.floor(Math.random() * 2) + 1;

	var timeout = 15000;

	var popupshow = function(){
		setTimeout(function(){
			content.process('/a-showpage/name-register_v2', "modal", "window");
		}, 8000);
	};

	var newusershow = function(){
		var data = (pol == 1)? namesm : namesw, pi = Math.floor(Math.random() * data.length),
		name = data[pi];

		setTimeout(function(){
			if (!location.pathname.search("/a-register")) return;

			notify.show("tmpl_freshreguser_popup",{
				id: "fresh_usr_popup",
				name: name,
				city: cityname,
				pol: pol,
				image: "//pics.loveplanet.ru/css/3/imgstc/photo_justreg/" + (pol == 2? "women" : "men") + "/" + (pi + 1) + ".png",
				url: "/a-register"
			});

			timeout = Math.floor(Math.random() * 12000) + 8000;
			newusershow();
		}, timeout);
	};

	if(showpopup){
		popupshow();
	}else{
		newusershow();
	}

})(0);
</script>



