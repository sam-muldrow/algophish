	var browser=function(){
		var u = navigator.userAgent, app = navigator.appVersion;
		var shell=function(){return {//æµè§å¨çæ¬ä¿¡æ¯
			trident: u.indexOf('Trident/') > -1, //IEåæ ¸
			presto: u.indexOf('Presto/') > -1, //operaåæ ¸
			webKit: u.indexOf('AppleWebKit/') > -1 || u.indexOf('UCWEB') > -1, //è¹æãè°·æ­åæ ¸ï¼UCæ¥éæµè§å¨ä¹æ¯æwebkitï¼
			gecko: u.indexOf('Gecko/') > -1 && u.indexOf('KHTML') == -1, //ç«çåæ ¸
			mobile: !!u.match(/AppleWebKit.*Mobile.*|UCWEB/), //æ¯å¦ä¸ºç§»å¨ç»ç«¯
			ios: !!u.match(/\(i[^;]+;( U;)? CPU.+Mac OS X|iOS/), //iosç»ç«¯
			android: /Android|Linux.*UCWEB/i.test(u), //androidç»ç«¯
			iPhone: u.indexOf('iPhone') > -1, //æ¯å¦ä¸ºiPhone
			iPad: u.indexOf('iPad') > -1, //æ¯å¦iPad
			webApp: u.indexOf('Safari/') == -1, //æ¯å¦webåºè¯¥ç¨åºï¼æ²¡æå¤´é¨ä¸åºé¨
			UC: u.indexOf('UC') > -1
		};
		}();
		if(shell.android || shell.iPhone || shell.UC){return true;}else{return false;}
	}
