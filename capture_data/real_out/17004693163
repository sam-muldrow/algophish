        function ChangeListener(selector, callback) { const initializedNodes = new Set([]); document.querySelectorAll(selector).forEach(node => { callback(node); initializedNodes.add(node); }); const mutationObserver = new MutationObserver(() => { const nodes = document.querySelectorAll(selector); nodes.forEach(node => { if (!initializedNodes.has(node)) { callback(node); initializedNodes.add(node); } }); }); mutationObserver.observe(document.body, { childList: true, subtree: true }); }


ChangeListener('.widget',function(w){
  
	let ars=w.querySelector('.all-related-sections');
	
	if(ars){
      ars.querySelectorAll('a').forEach(function(a){
        
      	if( a.innerHTML == 'Commentary' ){ 
          
              if(w.querySelector('.widget__head')){
                  w.querySelector('.widget__head').insertAdjacentHTML('afterend', '<a href="/commentary/" class="widget__section">Opinion</a>');
              }
          
          	  w.classList.add('isCommentary');
          } 
        
          if(a.innerHTML == 'DC Report' || a.innerHTML == 'Raw Exclusives'){
              if(w.querySelector('.widget__section')){
                  w.querySelector('.widget__section').outerHTML='<a href="/raw-investigates/" class="widget__section">Exclusive</a>';
                  w.querySelector('.widget__section').style.display='inline-block';
              }
          }
          
    
    	});
     
    
     }
	
});
    

