	<div class="contents contents--pink">
		<div class="container">
			<div class="pc-content">
				<main class="pc-content__main">
					<script>
			// auto redirect to previous url
			$(function(){
				autoRedirect();

				$.ajax({
					type : "post",
					url : "ajaxGetVoteInfo",
					success : function(json){
						var voteHtml ="";
						voteHtml += '<div class="separator">';
						voteHtml += '<div class="vote-box">';
						voteHtml += '<div class="vote-box__head">';
						voteHtml += '<h2><img class="lazyload" data-src="https://cdn.3ple.jp/3ple-static/3ple/pro/img/pages/top/vote-box-head_pc.png" alt="投票BOX" />';
						voteHtml += '</h2>';
						voteHtml += '</div>';
						if (json.userId) {
							// ログイン済み
							if (json.voteFlag) {
								// 投票済み
								if (json.themeName == null) {
									voteHtml += '<br><br>';
								} else {
									voteHtml += '<div class="vote-box__question">';
									voteHtml += '<p>' + json.themeName + '</p>';
									voteHtml += '</div>';
									voteHtml += '<div class="vote-box__complete">';
									voteHtml += '<p class="vote-box-complete">ご投票ありがとうございました</p>';
									voteHtml += '</div>';
									voteHtml += '<div class="vote-box__bottom">';
									voteHtml += '<div class="vote-box__submit">';
									voteHtml += '<p class="vote-box-submit vote-box-submit--complete">投票</p>';
									voteHtml += '</div>';
									voteHtml += '<div class="vote-box__more"><a class="vote-box-more hover" href="/vote/voteHistory/"><span class="icon"><span class="fas fa-chevron-right"></span></span>過去の投票BOXを見る</a></div>';
									voteHtml += '</div>';
								}
							} else {
								// 未投票
								if(json.voteChoiceList.length != 0){
									voteHtml += '<div class="vote-box__question">';
									voteHtml += '<p>' + json.themeName + '</p>';
									voteHtml += '</div>';
									voteHtml += '<div class="vote-box__input">';
									voteHtml += '<div class="vote-box-radio-list">';
									voteHtml += "<form method='POST' action='/index/vote' name='frm_vote'>";
									voteHtml += "<input type ='hidden' name = 'userId' value = " + json.userId + ">";
									voteHtml += "<input type ='hidden' name = 'voteId' value = " + json.voteId + ">";
									for(var i = 0; i < json.voteChoiceList.length; i++){
										voteHtml += '<div class="vote-box-radio-list__item">';
										voteHtml += '<label class="radio-content hover"><span class="radio-content__input"><span class="radio-btn">';
										voteHtml += '<input class="radio-btn__input" type="radio" id="' + json.voteChoiceList[i].choiceNo + '" value="' + json.voteChoiceList[i].choiceNo +'" name="voteChoice">';
										voteHtml += '<span class="radio-btn__view"></span></span></span><span class="radio-content__body"><span class="radio-content__text">';
										voteHtml += json.voteChoiceList[i].choiceName;
										voteHtml += '</span></span></label>';
										voteHtml += '</div>';
									}
									voteHtml += '<div class="vote-box__bottom">';
									voteHtml += '<div class="vote-box__submit">';
									voteHtml += '<button class="vote-box-submit hover" id="vote_submit">投票</button>';
									voteHtml += '</div>';
									voteHtml += '</form>';
									voteHtml += '<div class="vote-box__more"><a class="vote-box-more hover" href="/vote/voteHistory/"><span class="icon"><span class="fas fa-chevron-right"></span></span>過去の投票BOXを見る</a></div>';
									voteHtml += '</div>';
								} else {
									voteHtml += '<br><br>';
								}
							}
						} else {
							// 未ログイン
							if ($.cookie("3ple_vote")) {
								// 投票済み
								if (json.themeName == null) {
									voteHtml += '<br><br>';
								} else {
									voteHtml += '<div class="vote-box__question">';
									voteHtml += '<p>' + json.themeName + '</p>';
									voteHtml += '</div>';
									voteHtml += '<div class="vote-box__complete">';
									voteHtml += '<p class="vote-box-complete">ご投票ありがとうございました</p>';
									voteHtml += '</div>';
									voteHtml += '<div class="vote-box__bottom">';
									voteHtml += '<div class="vote-box__submit">';
									voteHtml += '<p class="vote-box-submit vote-box-submit--complete">投票</p>';
									voteHtml += '</div>';
									voteHtml += '<div class="vote-box__more"><a class="vote-box-more hover" href="/vote/voteHistory/"><span class="icon"><span class="fas fa-chevron-right"></span></span>過去の投票BOXを見る</a></div>';
									voteHtml += '</div>';
								}
							} else {
								// 未投票
								if(json.voteChoiceList.length != 0){
									voteHtml += '<div class="vote-box__question">';
									voteHtml += '<p>' + json.themeName + '</p>';
									voteHtml += '</div>';
									voteHtml += '<div class="vote-box__input">';
									voteHtml += '<div class="vote-box-radio-list">';
									voteHtml += "<form method='POST' action='/index/vote' name='frm_vote'>";
									voteHtml += "<input type ='hidden' name = 'voteId' value = " + json.voteId + ">";
									for(var i = 0; i < json.voteChoiceList.length; i++){
										voteHtml += '<div class="vote-box-radio-list__item">';
										voteHtml += '<label class="radio-content hover"><span class="radio-content__input"><span class="radio-btn">';
										voteHtml += '<input class="radio-btn__input" type="radio" id="' + json.voteChoiceList[i].choiceNo + '" value="' + json.voteChoiceList[i].choiceNo +'" name="voteChoice">';
										voteHtml += '<span class="radio-btn__view"></span></span></span><span class="radio-content__body"><span class="radio-content__text">';
										voteHtml += json.voteChoiceList[i].choiceName;
										voteHtml += '</span></span></label>';
										voteHtml += '</div>';
									}
									voteHtml += '<div class="vote-box__bottom">';
									voteHtml += '<div class="vote-box__submit">';
									voteHtml += '<button class="vote-box-submit hover" id="vote_submit">投票</button>';
									voteHtml += '</div>';
									voteHtml += '</form>';
									voteHtml += '<div class="vote-box__more"><a class="vote-box-more hover" href="/vote/voteHistory/"><span class="icon"><span class="fas fa-chevron-right"></span></span>過去の投票BOXを見る</a></div>';
									voteHtml += '</div>';

									// 投票したらcookieを設定するイベントハンドラをセット
									$(".vote-box").on("click", "#vote_submit" , function(){
										console.log("ok");
										//check selected radio neither or not
										if ($('input[name=voteChoice]:checked').length > 0) {
											$.cookie("3ple_vote", true,  {path: '/'});
										}
									});
								} else {
									voteHtml += '<br><br>';
								}
							}
						}
						voteHtml += '</div>';
						voteHtml += '</div>';
						$("#vote").html(voteHtml);
					},
					error : function(){
					}
				});

			});
		</script>

