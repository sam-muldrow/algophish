		<script>
		var display_sp = function() {
			document.cookie = "forcepc=; max-age=0; path=/";
			location.replace(location.pathname);
		}
		var display_pc = function() {
			document.cookie = "forcepc=true; path=/";
			location.replace(location.pathname);
		}
		if ($.cookie("forcepc")) {
			$("#change_display_block").show();
		}
		</script>

