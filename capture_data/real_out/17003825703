                <script>
                    $(document).ready(function(){
                        var weekend = $('#weekend_news .news_item');

                        weekend.hide().first().show();
                        $('.weekend_wrap .btn_refresh').click(function() {
                            var nextdaily = weekend.filter(':visible').hide().next();

                            if (nextdaily.length > 0) {
                                nextdaily.show();
                                $("#weekend_page").text( weekend.filter(':visible').index()+1 );
                            } else {
                                weekend.first().show();
                                $("#weekend_page").text( 1 );
                            }
                        });
                    });
                </script>
                    <div class="mainnews_wrap">
