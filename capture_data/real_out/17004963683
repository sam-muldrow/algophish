<script>
    $(function () {
        var wallet = {
            ui_popMidClose: function (obj) {
                $('.ui-pop-mask').remove();
                return $(obj).hide();
            },
            ui_popMidObj: function (obj) {
                var closeBtn, objH, objW, winHeight, winScrollTop;
                winHeight = $(window).height();
                winScrollTop = $(window).scrollTop();
                objH = $(obj).height();
                objW = $(obj).width();
                closeBtn = '.j_pop-close';
                $('body').append("<div class=\"ui-pop-mask\"></div>");
                $(obj).css({
                    'top': winScrollTop + Math.max(0, (winHeight - objH) / 2.5),
                    'margin-left': -objW / 2 | 0
                }).show();
                $(document).on('click', closeBtn, (function (_this) {
                    return function () {
                        return _this.ui_popMidClose(obj);
                    };
                })(this));
            }
        };
        /**
         * 弹窗样例
         * 关闭按钮class：j_pop-close
         * 弹窗调用方法：wallet.ui_popMidObj('#弹窗id / .class')
         * 弹窗关闭方法：wallet.ui_popMidClose('#弹窗id / .class')
         **/
        $('.j_btn1').click(function () {
            wallet.ui_popMidObj('#J_pop1');
        });
        $('.j_btn2').click(function () {
            wallet.ui_popMidObj('#J_pop2');
        });
    });
</script>
