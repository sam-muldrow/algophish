	$(document).ready(function () {
		$(".txtbox2").focus(function () {
			$(this).parent(".txtContainer").addClass("active");
		});
		$(".txtbox2").blur(function () {
			$(this).parent(".txtContainer").removeClass("active");
		});


	});
	function EmailValidation(sender, args) {

	   

		if ($.trim(args.Value) == "") {
			args.IsValid = false;
			$(sender).parent().find(".txtContainer").addClass("error");
			$(sender).parent().find(".txtContainer").removeClass("success");

			$('#lblforgetemailerror').text('وارد کردن آدرس ایمیل ضروری است');
				$('#lblforgetemailerror').css("display", "block");


			} 
			else {
				args.IsValid = true;
				$('#lblforgetemailerror').css("display", "none");
				$('#lblforgetemailerror').text('');

				$(sender).parent().find(".txtContainer").removeClass("error");
				$(sender).parent().find(".txtContainer").addClass("success");
			}
	}

</script>
