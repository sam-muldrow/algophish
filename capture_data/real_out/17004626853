		function CheckNickNameLogin(sender, args) {
			var nickname = $('#txtmail')
			var empty = ($.trim(nickname.val()) == "");
			var regex = /^([\u0600-\u06FF]|[\u0410-\u044f]|[A-Za-z0-9])([\u0600-\u06FF]|[\u0410-\u044f]|\w|[-.@]){3,}$/;
			var regexResult = regex.test(nickname.val());
			if (empty) {

				args.IsValid = false;
				$(sender).parent().find(".txtContainer").addClass("error");
				$(sender).parent().find(".txtContainer").removeClass("success");

				$('#lblNickNameError').text('وارد کردن آدرس ایمیل ضروری است');
				$('#lblNickNameError').css("display", "block");

			} else if (!regexResult) {
				args.IsValid = false;
				$(sender).parent().find(".txtContainer").addClass("error");
				$(sender).parent().find(".txtContainer").removeClass("success");
				$('#lblNickNameError').text('آدرس ایمیل نامعتبر است');
				$('#lblNickNameError').css("display", "block");

			}
			else {
				args.IsValid = true;
				$(sender).parent().find(".txtContainer").removeClass("error");
				$(sender).parent().find(".txtContainer").addClass("success");
				$('#lblNickNameError').css("display", "none");
				$('#lblNickNameError').text("");
			}
	}
	function CheckPassLogin(sender, args) {
		var passControl = $('#txtpw')
		var empty = ($.trim(passControl.val()) == "");
		var regex = /^.{6,32}$/;
		var regexResult = regex.test(passControl.val());
		if (empty) {

			args.IsValid = false;
			$(sender).parent().find(".txtContainer").addClass("error");
			$(sender).parent().find(".txtContainer").removeClass("success");

			$('#LabelErrorPass').text('رمز عبور موردنیاز میباشد!');
			$('#LabelErrorPass').css("display", "block");

		} else if (!regexResult) {
			args.IsValid = false;
			$(sender).parent().find(".txtContainer").addClass("error");
			$(sender).parent().find(".txtContainer").removeClass("success");
			$('#LabelErrorPass').text('رمز عبور شما باید بین 6 تا 32 کاراکتر باشد!');
			$('#LabelErrorPass').css("display", "block");

		}
		else {
			args.IsValid = true;
			$(sender).parent().find(".txtContainer").removeClass("error");
			$(sender).parent().find(".txtContainer").addClass("success");
			$('#LabelErrorPass').css("display", "none");
			$('#LabelErrorPass').text("");
		}
}
