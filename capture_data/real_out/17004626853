<script type="text/javascript">
    $(document).ready(function () {
        $(".trailerLink").each(function () {
            $(this).fancybox({
            
                content: unescape($("#hiddenTrailer", $(this).parent()).val()).replace(/\+/g, " "),
                closeClick: true,
                scrolling: 'no',
                openEffect: 'elastic',
                closeEffect: 'elastic',
                wrapCSS: 'CustomBack'
        

            });
        });
        $(".screeshotsLink").click(function () {
            $.fancybox.showLoading();
            var ServiceUrl = "/Services/screenShots.svc/GetScreenShots";
            var productMarketId = $("#hiddenProductMarket", $(this).parent()).val();
            
            var listScreens = new Array();
            $.ajax(
              {
                  type: "POST",
                  url: ServiceUrl,
                  data: '{"productMarketId":' + productMarketId + '}',
                  contentType: "application/json; charset=utf-8",
                  dataType: "xml",
                  success: function (items) {
                      var vcount = items.getElementsByTagName("MarketScreenshots").length;
                     

                      for (var i = 0; i < vcount; i++) {
                          var resultDescription = items.getElementsByTagName("Description")[i];
                          var resultImageURL = items.getElementsByTagName("ImageURL")[i];
                          var _ScreenshotDescription = ((resultDescription != null && resultDescription.childNodes[0] != null) ? resultDescription.childNodes[0].nodeValue : '');
                          var _ScreenshotImageURL = ((resultImageURL != null && resultImageURL.childNodes[0] != null) ? resultImageURL.childNodes[0].nodeValue : '');
                          var screenShot = {};
                          screenShot["href"] = _ScreenshotImageURL;
                          screenShot["title"] = _ScreenshotDescription;
                          listScreens.push(screenShot);
                         
                      }
                  }
                  ,
                  error: function (err) {
                      
                  },
                  complete: function()
                  {
                      $.fancybox.hideLoading();
                  },
                  async: false
              }
              );
            $.fancybox.open(listScreens,
                
                    {
                        maxWidth: 900,
                        maxHeight: 600,
                        openEffect: 'elastic',
                        closeEffect: 'elastic',
                        wrapCSS: 'CustomBack'
                    }
                );

        });
            

    });
    
</script>
