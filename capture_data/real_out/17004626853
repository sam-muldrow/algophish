<script type="text/javascript">
    $(function () {

    InitFbLikeBox();
    });
          
    function InitFbLikeBox() {
        var $fbb = $('.fb_like_box');
        $fbb.hover(function () {
            $fbb.stop().animate({ left: 0 }, 500);
        }, function () {
            $fbb.stop().animate({ left: -237 }, 400);
        });
        $fbb.show();
    }
</script>

