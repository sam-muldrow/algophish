        <script type="text/javascript">
            function changeShowMoreButtonText(e, contentText){
                if (contentText === 'SVE VESTI') {
                    $(e).text('PRIKAŽI MANJE');
                } else {
                    $(e).text('SVE VESTI');
                }
            }
        </script>
        
