 	<div class="search-box" id="drsearchbox">
	<p class="search-head" style="font-size: 26px;">Find the Right Doctor for your Problem</p>
    <div class="display-item" id="searchbox">
      <form method="POST" name="frmdrsearch" action="https://www.medindia.net/patients/calculators/consult-the-right-doctor-result.asp">
	  
	  <input type="text" name="durl" id="durl" style="display: none; color: rgb(204, 204, 204);">
        <div class="parent">
          <div class="input-group">
            <input type="text" class="form-control" name="searchtxt" id="searchtxt" placeholder="Type Condition or disease to Know Which Medical Specialist to Consult" onkeypress="getdruglist('https://www.medindia.net','','No');" onchange="getdruglist('https://www.medindia.net','','No');" maxlength="50" autocomplete="off" >
			
            <span class="input-group-btn">
           <button class="clear-btn drsb" type="button" onclick="clearsearch()" style="background-image: linear-gradient(to bottom right, #65c0a4, #9ed1c1);color:#000000"> <span style="font-size: 28px; font-weight: 600;">&#215;</span> </button>
            </span>
            <div class="ajax-result disable_box" style="display: none;" id="drsearchdiv" autocomplete="off">
              <ul id="drsearchlist" data-inset="true" style="display: block;"></ul>
              <span class="ajax-bx-close icon" onclick="myFunction()" style="display: block;">X</span> </div>
          </div>
        </div>
		
		<div id="resulttext" style="background-color:#FFFFFF; padding:10px; display:none"></div>
      </form>
    </div>
	</div>

<script language="javascript">

function getdruglist(url,section,source)
{
var search_input = document.getElementById("searchtxt");
search_input.onkeyup = function() {
	$('#drsearchlist li').remove();
		 $('#drsearchlist').empty();
if (search_input.value.length>2 )  
{
document.getElementById('drsearchdiv').style.display ="inline";
$('#drsearchlist li').remove();
getdrugdetails(url,search_input.value,section,source);
}
if (search_input.value.length<=0 )  
{
$('#drsearchlist li').remove();
document.getElementById('drsearchdiv').style.display ="none";
}
}
 }
 

function clearsearch()
{ 
document.getElementById('drsearchdiv').style.display ="none";
document.getElementById("searchtxt").value="";
  document.getElementById("resulttext").innerHTML= "";
  document.getElementById('resulttext').style.display ="none";

}

 

function getdrugdetails(serviceurl,txt,section,source) {
//alert(serviceurl+'/patients/calculators/cal_includes/getcondition.asp?txt='+txt+'&section='+section+'&source='+source)
		$.getJSON(serviceurl+'/patients/calculators/cal_includes/getcondition.asp?txt='+txt+'&section='+section+'&source='+source, function(data) {
		$('#drsearchlist li').remove();
		 $('#drsearchlist').empty();
		dd =data.items;
		$.each(dd, function(index, result) {
	//alert(result.question1);
		$('#drsearchlist').append('<div class="search-list"><a onclick="setvalue(\''+result.id+'\',\''+result.Question1+'\')"  style="cursor:pointer;text-transform:capitalize">'+result.Question+'</a></div>');	


			//$('#drsearchlist').append('<li><a href="' + result.url + '">' + result.txt +  '</a></li>');
			
		});
		
		if($("#drsearchlist a").length < 1){
			$('#drsearchlist').css('display','none');	
			$('.ajax-bx-close').css("display","none");
		} else {
			$('#drsearchlist').css('display','block');
			$('.ajax-bx-close').css("display","block");
		}
		
		$('#drsearchlist').listview('refresh');
	});
	
}
$(function _drugdisable_box(){
		$(".ajax-bx-close").click(function(){
		document.getElementById('drsearchdiv').style.display ="none";
			$('#drsearchlist li').remove();
		 $('#drsearchlist').empty();
		});	
	});





function setvalue(durl,txt) {


drugtxt=txt;
drugtxt=drugtxt.replace("<font  color=#737373>","");
drugtxt=drugtxt.replace("</font>","");
	document.getElementById("searchtxt").value=drugtxt;
		document.getElementById("durl").value=durl;
//alert(document.getElementById("durl").value);
	document.getElementById('drsearchdiv').style.display ="none";
	
	
	
	
	showdrsresult(durl);




}



function showdrsresult(durl){

	  document.frmdrsearch.submit();
		  document.getElementById("drsearchbox").innerHTML="<h1>Searching....</h1>";
}

	

</script>
<script>
function myFunction() {
  var x = document.getElementById("drsearchdiv");
  if (x.style.display === "none") {
    x.style.display = "block";
  } else {
    x.style.display = "none";
  }
}
</script>
