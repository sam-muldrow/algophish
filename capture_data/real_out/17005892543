<script type="text/javascript">
function pollcheck()									// for Health Poll
{
	var fname = document.Poll;
	var chlen = fname.choice.length;
	var count=0;
	var isSelected=false;
	var selChoice;
	for (count = 0; count < chlen; count++)
	{
		if (fname.choice[count].checked == true)
		{
			selChoice=fname.choice[count].value;
			isSelected=true;
			break;
		}
	}
	if (isSelected == true)
	{
	 document.Poll.action='https://www.medindia.net/poll/index.asp?Choice='+selChoice
       document.Poll.submit()
	return true;
	}
	else
	{
		alert("Please Select A Choice");
		return false;
	}
}




function drsearch_fncheck()
{  
		var frm=document.drsearch_frm	
		if ((frm.dr_name.value=="")&&(frm.others_search.value=="")&&(frm.Specialist.value==""))
		{
			alert("Please select atleast one option!");
			return false;
		}
		if(document.drsearch_frm.dr_name.value!="")
		{
			if (!document.drsearch_frm.dr_name.value.match(/^([a-zA-Z.]+[ a-zA-Z.]*)$/))
			{
				alert("Please enter valid doctor name!");
				document.drsearch_frm.dr_name.focus();
				return false;
			}
			if ((document.drsearch_frm.dr_name.value).indexOf("  ")>0)
			{
				alert("Please enter valid doctor name!");
				document.drsearch_frm.dr_name.focus();
				return false;
			}
			if (document.drsearch_frm.dr_name.value.length<3 || document.drsearch_frm.dr_name.value.length>50)
			{
				alert("You can enter minimum 3 characters and maximum 50 characters length!");
				document.drsearch_frm.dr_name.focus();
				return false;
			}
		}
		
		if(document.drsearch_frm.others_search.value!="")
		{
		if (document.drsearch_frm.others_search.value.length<3 || document.drsearch_frm.others_search.value.length>50)
		{
			alert("You can enter minimum 3 characters and maximum 50 characters length!");
			document.drsearch_frm.others_search.focus();
			return false;
		}
		}
		else
		{
			return true;
		}
 }
 

function getdoctorlist(url1,fieldno,dirtype)
{
if (fieldno==1){var drname_input = document.getElementById('dr_name');}
if (fieldno==2){var drname_input = document.getElementById('others_search');}
var _list = document.getElementById('doctorsearchlist'+fieldno).childNodes.length;
document.getElementById('doctorsearchlist'+fieldno).style.display=_list < 1 ? 'none':'block';
drname_input.onkeyup = function() {
$( "#doctorsearchdiv"+fieldno+ " > ul").addClass( "hide" )

if (drname_input.value.length>2)  
{
$( "#doctorsearchdiv"+fieldno+ " > ul").removeClass( "hide" )
sp= document.getElementById('Specialist').value;
city= document.getElementById('others_search').value;
dr_name= document.getElementById('dr_name').value;
if (fieldno==2){fieldtype="city";}
if (fieldno==1){fieldtype="drname";}
getdoctorlist2(url1,dr_name,dirtype,fieldtype,fieldno,city,sp,this);
}
}
}

function checklist(name)
{
document.getElementById('others_search').value=name;
$( "#doctorsearchdiv2 > ul").addClass( "hide" )
//getsplist(name,'2',this);
}

function getdoctorlist2(serviceurl,txt,dirtype,fieldtype,fieldno,city,sp,element) {
//alert(serviceurl+'/includes-opt/components/home/get-doctor-details.asp?txt='+txt+'&fieldtype='+fieldtype+'&dirtype='+dirtype+'&city='+city+'&sp='+sp)
$.getJSON(serviceurl+'/includes-opt/components/home/get-doctor-details.asp?txt='+txt+'&fieldtype='+fieldtype+'&dirtype='+dirtype+'&city='+city+'&sp='+sp, function(data) {
		$('#doctorsearchlist'+fieldno+' li').remove();
		$.each(data.items, function(index, dr) {
			if (fieldno==2){$('#doctorsearchlist'+fieldno).append('<li><a href="javascript:checklist('+"'"+ dr.dirname + "'" +');" style="text-transform:capitalize">' + dr.dirname + '</a></li>');}
			if (fieldno==1){$('#doctorsearchlist'+fieldno).append('<li><a href="' + dr.dirurl + '" style="text-transform:capitalize">' + dr.dirname + '</a></li>');}
		});
		if($('#doctorsearchlist'+fieldno+' li').length < 1){
			$( "#doctorsearchdiv"+fieldno+ " > ul").addClass( "hide" )
		} else {
			$( "#doctorsearchdiv"+fieldno+ " > ul").removeClass( "hide" )
		}
		$('#doctorsearchlist'+fieldno).listview('refresh');
		
	});
}


function getsplist(city,fieldno,element) {
//alert('https://www.medindia.net/includes-opt/components/home/get-splist.asp?city='+city)
$.getJSON('https://www.medindia.net/includes-opt/components/home/get-splist.asp?city='+city, function(data) {
		$('#doctorsearchlist2 li').remove();
		$('#doctorsearchlist'+fieldno).append('<li><h4><strong>Specialities in <span style="text-transform:capitalize">'+city+'</span></strong></h4></li>')
		$.each(data.items, function(index, dr) {
			$('#doctorsearchlist'+fieldno).append('<li><a href="https://www.medindia.net/patients/doctor_search/' + dr.cityurl + '" style="text-transform:capitalize">' + dr.Specialization1 + '</a></li>');
		});
		if($('#doctorsearchlist'+fieldno+' li').length < 1){
			$( "#doctorsearchdiv"+fieldno+ " > ul").addClass( "hide" )
		} else {
			$( "#doctorsearchdiv"+fieldno+ " > ul").removeClass( "hide" )
		}
		
		$('#doctorsearchlist'+fieldno).listview('refresh');
		
	});
}


function hospsearch_fncheck()
{  
		var frm=document.hospsearch_frm	
		if ((frm.hos_name.value=="")&&(frm.others_searchhos.value==""))
		{
			alert("Please select atleast one option!");
			return false;
		}
		if(document.hospsearch_frm.hos_name.value!="")
		{
			if (!document.hospsearch_frm.hos_name.value.match(/^([a-zA-Z.]+[ a-zA-Z.]*)$/))
			{
				alert("Please enter valid hospital name!");
				document.hospsearch_frm.hos_name.focus();
				return false;
			}
			if ((document.hospsearch_frm.hos_name.value).indexOf("  ")>0)
			{
				alert("Please enter valid hospital name!");
				document.hospsearch_frm.hos_name.focus();
				return false;
			}
			if (document.hospsearch_frm.hos_name.value.length<3 || document.hospsearch_frm.hos_name.value.length>50)
			{
				alert("You can enter minimum 3 characters and maximum 50 characters length!");
				document.hospsearch_frm.hos_name.focus();
				return false;
			}
		}
		
		if(document.hospsearch_frm.others_searchhos.value!="")
		{
		if (document.hospsearch_frm.others_searchhos.value.length<3 || document.hospsearch_frm.others_searchhos.value.length>50)
		{
			alert("You can enter minimum 3 characters and maximum 50 characters length!");
			document.hospsearch_frm.others_searchhos.focus();
			return false;
		}
		}
		else
		{
			return true;
		}
 }
 
function changetexthos()
{
if (document.hospsearch_frm.othershos[0].checked){document.hospsearch_frm.others_searchhos.value='';$('#others_searchhos').attr('placeholder','Enter City');$("#hospitalsearchdiv2 > ul").addClass( "hide" );}
if (document.hospsearch_frm.othershos[1].checked){document.hospsearch_frm.others_searchhos.value='';$('#others_searchhos').attr('placeholder','Enter State');$( "#hospitalsearchdiv2 > ul").addClass( "hide" );}
if (document.hospsearch_frm.othershos[2].checked){document.hospsearch_frm.others_searchhos.value='';$('#others_searchhos').attr('placeholder','Enter Pincode');$( "#hospitalsearchdiv2 > ul").addClass( "hide" );}
}

 
function gethospitallist(url1,fieldno,dirtype)
{
if (fieldno==1){var drname_input = document.getElementById('hos_name');}
if (fieldno==2){var drname_input = document.getElementById('others_searchhos');}

var _list = document.getElementById('hospitalsearchlist'+fieldno).childNodes.length;
document.getElementById('hospitalsearchlist'+fieldno).style.display=_list < 1 ? 'none':'block';
drname_input.onkeyup = function() {

$( "#hospitalsearchdiv"+fieldno+ " > ul").addClass( "hide" )

if (drname_input.value.length>2)  
{
$( "#hospitalsearchdiv"+fieldno+ " > ul").removeClass( "hide" )
var fieldtype="hospname"
if (fieldno==2 && document.hospsearch_frm.othershos[0].checked){fieldtype="city";}
if (fieldno==2 && document.hospsearch_frm.othershos[1].checked){fieldtype="state";}
if (fieldno==2 && document.hospsearch_frm.othershos[2].checked){fieldtype="pincode";}
gethospitallist2(url1,drname_input.value,dirtype,fieldtype,fieldno,this);
}

}
}

function gethospitallist2(serviceurl,txt,dirtype,fieldtype,fieldno,element) {
	$.getJSON(serviceurl+'/includes-opt/components/home/get-hospital-details.asp?txt='+txt+'&fieldtype='+fieldtype+'&dirtype='+dirtype, function(data) {

		$('#hospitalsearchlist'+fieldno+' li').remove();
		$.each(data.items, function(index, dr) {
			$('#hospitalsearchlist'+fieldno).append('<li><a href="' + dr.dirurl + '" style="text-transform:capitalize">' + dr.dirname + '</a></li>');
		});
		$('#hospitalsearchlist'+fieldno).listview('refresh');
	
		
	});
}



function getsearchlist(url,section,source)
{
var search_input = document.getElementById("brand_name");
search_input.onkeyup = function() {
if (search_input.value.length>2 )  
{
$( "#searchdiv > ul").addClass( "hide" )
getsearchdetails(url,search_input.value,section,source);
}
if (search_input.value.length<=0 )  
{
$( "#searchdiv > ul").removeClass( "hide" )
}
}
 }
 

function search_valid()
{  
var _input=document.getElementById("brand_name").value.trim()
if (_input.length<3){alert("Enter atleast 3 characters");return false;}
return true;
document.frm_search.submit();
}

 

function getsearchdetails(serviceurl,txt,section,source) {
		$.getJSON(serviceurl+'/includes-opt/components/get-search-details.asp?txt='+txt+'&section='+section+'&source='+source, function(data) {
		//alert(serviceurl+'/includes-opt/components/get-search-details.asp?txt='+txt+'&section='+section+'&source='+source)
		$('#searchlist li').remove();
		dd =data.items;
		$.each(dd, function(index, result) {
			$('#searchlist').append('<li><a href="' + result.url + '">' + result.txt +  '</a></li>');
			
		});
		
		if($("#searchlist a").length < 1){
			$( "#searchdiv > ul").addClass( "hide" )
			//$('.ajax-bx-close').css("display","none");
		} else {
			$( "#searchdiv > ul").removeClass( "hide" )
			//$('.ajax-bx-close').css("display","block");
		}
		
		$('#searchlist').listview('refresh');
	});
	
}
</script>

