		$('head').append('<style>.empowering .google-auto-placed{display:none;}.google-auto-placed{position: relative;  border: 1px solid #ccc; margin-top: 38px!important; margin-bottom: 20px;}.google-auto-placed:before{display: block;content: "Advertisement"; text-align: center; margin-top: -19px; width: 100%;font-size: 10px; color: #383535; text-transform: uppercase;padding-bottom:5px;}.google-auto-placed ins {margin: 5px auto 0px 0px!important;}</style>');
    }, 500);
	
});
