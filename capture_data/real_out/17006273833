    <script>
    //Adding custom class for different languages
        $("body").addClass("custom_lang_"+themeDisplay.getLanguageId());
    
    $(document).ready(function(){
        $(".increaseFont,.decreaseFont").click(function(){
           var type= $(this).val();
           //alert($(this).val());
           var curFontSize = $('body,li,p,div,a').css('font-size');
           if(type=='increase'){
              $('body,li,p,div,a').css('font-size', parseInt(curFontSize)+1);
              $('.decreaseFont').removeClass('disablezoom');
              if(curFontSize>='17px'){
               $('.increaseFont').addClass('disablezoom');
              
              }
           } 
           
           if(type=='decrease'){
              $('body,li,p,div,a').css('font-size', parseInt(curFontSize)-1);
              $('.increaseFont').removeClass('disablezoom');
              if(curFontSize<='15px'){
               $('.decreaseFont').addClass('disablezoom');
               
           }
           }
           
        });
        
        $('.dropdown-submenu a.test').on("click", function(e){
                $(this).next('ul').toggle();
                e.stopPropagation();
                e.preventDefault();
              });
     });

$(".taglib-language-option").each(function () {
   $(this).text($(this).text().replace("[beta]","")); 
});

    </script>
