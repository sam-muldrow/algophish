<script type="text/javascript">
function getAutoSugestedKeywords(){
	
	var keyword = $("#desktopNavBar .custom-global-search-input").val();
	
	//var keyword = $("#custom-global-search-input").val();
	var parentDiv="#desktopNavBar";
	var requestContextPath="/o/boi.search";
	
	if($(".searchopenmobile").is(":visible")){
		keyword =$(".searchopenmobile .custom-global-search-input").val();
		parentDiv=".searchopenmobile";
	}
	
	
	keyword  = keyword.replace(/^\s+|\s+$/, '');

	
	//$('#desktopNavBar .custom-search-menu ul.custom-search-results-ul').empty();
	 var html = '';
	 var showMoreResultsHtml='';
	 var friendlyUrl="/guest";
	 $(parentDiv+' .custom-search-suggestions').empty();
	if(keyword.length >=3 ){
		$(parentDiv+" .Search-journalArticle").hide();
		$(parentDiv+' .custom-search-suggestions').show();
		
	 $.ajax({
	       url: 'https://bankofindia.co.in/home?p_p_id=boi_search_BoiSearchPortlet&p_p_lifecycle=2&p_p_state=normal&p_p_mode=view&p_p_resource_id=getSuggestedKeywords&p_p_cacheability=cacheLevelPage',
	       type: "POST",
		   dataType : "json",
	       data: {
	    	   "_boi_search_BoiSearchPortlet_keyword":keyword
	       },
	        success: function(data){
	        	$(parentDiv+' .custom-search-suggestions').empty();
	        	//$('#dataListTest').empty();
	        	//$('#dataListTest').show();
	        	 const autoSuggestData = JSON.parse(JSON.stringify(data));
	        	 
	        	  html += '<ul class="custom-search-results-ul">';
				var autoSuggestionLength=autoSuggestData.reverse().length; //latest added reverse() 
				if(autoSuggestionLength>8){
					autoSuggestionLength=8;
				}
	        	 for(var i =0; i<autoSuggestionLength; i++){
	        		
	        		if(i <=8 ){
	        			
	        			html += '<li class="custom-search-results-li" data-lid="'+autoSuggestData[i].layoutId+'">'+
	        			'<a class="search-list-a"  href="'+autoSuggestData[i].url+'">'
	        			+autoSuggestData[i].title_+
	        			'<img src="'+requestContextPath+'/images/active-arrow.svg" class="float-right" alt="right-arrow"/></a>'+
	        			'<span class="search-list-snippet-content">'+autoSuggestData[i].snippet_content+'</span>'
	        			+'</li>';
	        		}
	        	 }
	        	
	        	 if(autoSuggestData.length>8){
	        		 //showMoreResultsHtml= '<li class="custom-search-results-li custom-search-show-more-li"><a class="search-list-a" href="/web/guest/search?q='+keyword+'">Show More Results<img src="'+requestContextPath+'/images/active-arrow.svg" alt="right-arrow"/></a></li>';
	        		 html += '<li class="custom-search-results-li"><a class="search-list-a" href="/search?q='+keyword+'">Show More Results<img src="'+requestContextPath+'/images/active-arrow.svg" alt="right-arrow"/></a></li>';
	        	 }
	        	// $('#desktopNavBar .custom-search-menu ul.custom-search-results-ul').append(showMoreResultsHtml);
	        	
	        	 html +='<ul/>';
	        	 $(parentDiv+' .custom-search-suggestions').append(html);

	        	 //getChangedKeyword(keyword);
	        	 getSearchTermString(keyword);
	        },
			error: function (error) {
      			
          }
	    });
    }
	
	else
		{
		$(parentDiv+" .Search-journalArticle").show();
		$(parentDiv+" .custom-search-suggestions").hide();
		
		}
	
 }
function searchSuggestion() {
	//$(".custom-search-menu").addClass("d-block");
	getAutoSugestedKeywords();
	}
	
function redirectToSearchResults(){
	
	var parentDiv="#desktopNavBar";
	if($(".searchopenmobile").is(":visible")){
		parentDiv=".searchopenmobile";
	}
	
	
	var searchInput = $(parentDiv+" #custom-global-search-input");
	var searchInputVal = $(parentDiv+" .custom-global-search-input").val();
	var friendlyUrl="/guest";
	  if (event.key === "Enter") {
	    event.preventDefault();
	    window.location.href="/search?q="+searchInputVal;
	    		
	  }
}
function redirectToSearchResultsPage()
{
	var parentDiv="#desktopNavBar";
	if($(".searchopenmobile").is(":visible"))
	{
		parentDiv=".searchopenmobile";
	}
	var searchInput = $(parentDiv+" #custom-global-search-input");
	var searchInputVal = $(parentDiv+" .custom-global-search-input").val();
	var friendlyUrl="/guest";
	window.location.href="/search?q="+searchInputVal;
	    		
}



var SpeechRecognition = window.SpeechRecognition || window.webkitSpeechRecognition || window.mozSpeechRecognition || window.msSpeechRecognition;
	if (SpeechRecognition) {
		
		var recognition = new (window.SpeechRecognition || window.webkitSpeechRecognition || window.mozSpeechRecognition || window.msSpeechRecognition)();

		console.log("Your Browser supports speech Recognition");
		
		recognition.continuous = true;

		var parentDiv = "#desktopNavBar";
		/* if ($(".searchopenmobile").is(":visible")) {
			parentDiv = ".searchopenmobile";
		} */
		function micBtnClick() {
			
			if ($(".searchopenmobile").is(":visible")) {
				parentDiv = ".searchopenmobile";
			}
			if ($(parentDiv + " .voice-button i").hasClass("icon-microphone"))
			{
				
				recognition.start();
				recognition.addEventListener("start", startSpeechRecognition);
				recognition.addEventListener("end", endSpeechRecognition);
				recognition.addEventListener("result",resultOfSpeechRecognition);
				
				
				
				//to stop mic in ios
				
				function stopRecogition() {
						  recognition.stop();
						}
				
				
							
				
				  if (/iPhone|iPod|iPad/.test(navigator.userAgent) && /CriOS/.test(navigator.userAgent)) {
			        	
					   setTimeout(stopRecogition, 10000);
					  $(".search-list-a").click(function()
								{
						  
									 recognition.stop();
								}		
								
								);
								
					  
					  
			}

			        if (/iPhone|iPod|iPad/.test(navigator.userAgent) && /EdgiOS/.test(navigator.userAgent)) {
			        	 setTimeout(stopRecogition, 10000);
			        	 $(".search-list-a").click(function()
			     				{
			     					 recognition.stop();
			     				}		
			     				
			     				);
			     				
			        	 
			        	 
			        	}  
			        
			        if (/^((?!chrome|android).)*safari/i.test(navigator.userAgent)){
			         setTimeout(stopRecogition, 10000);
			          $(".search-list-a").click(function()
			     				{
			     					 recognition.stop();
			     				}		
			     				
			     				);
			     				
			        	
			        	 }  

}

			else {
				recognition.stop();
			}

		}

		

		//mouse leave
		$(".dropdown-menu.megamenu").mouseleave(function() {
			recognition.stop();
		});
		
		function startSpeechRecognition() {
			
			var parentDiv = "#desktopNavBar";
			if ($(".searchopenmobile").is(":visible")) {
				parentDiv = ".searchopenmobile";
			}

			$(parentDiv + " .voice-button i").removeClass("icon-microphone");
			$(parentDiv + " .voice-button i").addClass("icon-microphone-off");
			$(parentDiv + " .custom-global-search-input").focus();
			console.log("Voice activated, SPEAK");
		}

		function endSpeechRecognition() {
			var parentDiv = "#desktopNavBar";
			console.log("inside end speech recognition");
			if ($(".searchopenmobile").is(":visible")) {
				parentDiv = ".searchopenmobile";
			}

			$(parentDiv + " .voice-button i")
					.removeClass("icon-microphone-off");
			$(parentDiv + " .voice-button i").addClass("icon-microphone");
			$(parentDiv + " .custom-global-search-input").focus();
			console.log("Speech recognition service disconnected");
		}

		function resultOfSpeechRecognition(event) {
			console.log("In results of speech recognition");
			var parentDiv = "#desktopNavBar";
			if ($(".searchopenmobile").is(":visible")) {
				parentDiv = ".searchopenmobile";
			}

			const current = event.resultIndex;
			const transcript = event.results[current][0].transcript.replace(
					".", "");

			console.log("transcript-", transcript);
			if (transcript.toLowerCase().trim() === "stop") {
				recognition.stop();
			} else if (!$(parentDiv + " .custom-global-search-input").val()) {
				$(parentDiv + " .custom-global-search-input").val(
						transcript.replace(".", ""));
				searchSuggestion();//latest
			} else {
				if (transcript.toLowerCase().trim() === "search") {
					// searchSuggestion(); latest comment
					redirectToSearchResultsPage();

				} else if (transcript.toLowerCase().trim() === "reset") {
					$(parentDiv + " .custom-global-search-input").val("");
					$(parentDiv + ' .custom-search-suggestions').empty();//latest
				} else {
					$(parentDiv + " .custom-global-search-input").val(
							transcript.replace(".", ""));
					searchSuggestion();
				}
			}
		}

		
	}//speech recogition
	else {
		console.log("Your Browser doesn't support speech Recognition");
	}

	/* $(window).click(function() {
		recognition.stop();
		}); */
	

	$(".dropdown-menu.megamenu").click(function(e) {
		e.stopPropagation();
	});

	
	function isAlphaNumeric(event, selectednm) {
		if ((event.keyCode >= 48 && event.keyCode <= 57)
				|| (event.keyCode >= 65 && event.keyCode <= 90)
				|| (event.keyCode >= 97 && event.keyCode <= 122)
				|| event.keyCode == 8 || (event.keyCode == 32)
				|| event.keyCode==13) {

			return true;
		} else {
			return false;
		}
	}
	
	function browserDetect(){
       let userAgent = navigator.userAgent;
       
       var edgeMacRegex = /Macintosh.*Edg/i;

   
    var isMacEdge = edgeMacRegex.test(userAgent);
           
       
       
         if(userAgent.match(/firefox|fxios/i)|| isMacEdge)
           {
         		 $(".search-menu .voice-button").hide();
         		$(".search-menu .search-icon").show();
         		
             } 
           
         }   
	
         $(document).ready(function() {
        	 browserDetect();
         });
	
	
	

	 function getSearchTermString(keyword)
	{
		 if(document.contains(document.getElementById("searchQueryScript"))) {
	            document.getElementById("searchQueryScript").remove();
	            var searchSchema = document.createElement('script');
	    		searchSchema.type = 'application/ld+json';
	    		searchSchema.id="searchQueryScript";
	    		searchSchema.text = JSON.stringify({
	    			  "@context": "https://schema.org/",
	    			  "@type": "WebSite",
	    			  "name": "Bank of India - Online Internet Banking and Personal Banking Services",
	    			  "url": "https://bankofindia.co.in/",
	    			  "potentialAction": {
	    			    "@type": "SearchAction",
	    			    "target": "https://bankofindia.co.in/search?q="+keyword,
	    			    "query-input": "required name="+keyword
	    			  }
	    		    },null,2);
	    		
	    		  document.querySelector('head').appendChild(searchSchema);
		 }
		
		 else
			 {
		var searchSchema = document.createElement('script');
		searchSchema.type = 'application/ld+json';
		searchSchema.id="searchQueryScript";
		searchSchema.text = JSON.stringify({
			  "@context": "https://schema.org/",
			  "@type": "WebSite",
			  "name": "Bank of India - Online Internet Banking and Personal Banking Services",
			  "url": "https://bankofindia.co.in/",
			  "potentialAction": {
			    "@type": "SearchAction",
			    "target": "https://bankofindia.co.in/search?q="+keyword,
			    "query-input": "required name="+keyword
			  }
		    },null,2);
		
		  document.querySelector('head').appendChild(searchSchema);
			 }
		  
	} 
	
	</script>



	



	
	
					
				
			
		
	
	


