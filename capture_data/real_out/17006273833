          <script>
              $(document).ready(function () {
            if(Liferay.ThemeDisplay.isSignedIn()){
                
                  $(".Personal").hide();
                  $(".Business").hide();
                  $(".Banking").hide();
                  $(".NRI").hide();
                  $(".Agri").hide();
                  $(".Wealth").hide();
                  $(".Training").hide();
                  $(".Insure").hide();
                  }
              var pageurl = window.location.href;
             
              
              if(pageurl =="http://dev.corp.bankofindiaweb.com:8080/web/guest/home?p_p_id=com_liferay_login_web_portlet_LoginPortlet&p_p_lifecycle=0&p_p_state=maximized&p_p_mode=view&saveLastPath=false&_com_liferay_login_web_portlet_LoginPortlet_mvcRenderCommandName=%2Flogin%2Flogin" || pageurl == "http://dev.corp.bankofindiaweb.com:8080/web/guest/home?p_p_id=com_liferay_login_web_portlet_LoginPortlet&p_p_lifecycle=0&p_p_state=maximized&saveLastPath=false"){
                 
                 $(".portlet-boundary_com_liferay_site_navigation_menu_web_portlet_SiteNavigationMenuPortlet_").hide();
                 $("#portlet_com_liferay_journal_content_web_portlet_JournalContentPortlet_INSTANCE_footer").hide();
                 $("#portlet_com_liferay_site_navigation_breadcrumb_web_portlet_SiteNavigationBreadcrumbPortlet").hide();
              }
              // main navigation functoion
              // function for dynamic language size fix
                //const selectLang = document.querySelector('li.languge-sel span#selected');
                // console.log(selectLang.textContent);
                const selectLangtext = "en_US";
                
                const getChar = function(links,langText) {
                  $(links).each(function() {
                    //console.log('----' + $(this).text());
                    //console.log('link length ' + $(this).text().length);
                    // const testSubLink = $(this).closest('.childsubchild').find('li.list-item').length;
                    switch (langText) {
                      case ("ml_IN"):
                        
                          $(this).addClass('dynamic-link-ml');
                                  
                                                                    
                        break;
                      case ("ta_IN"):
                       
                          $(this).addClass('dynamic-link-ta');
                                                               
                        break;  
                        case ("te_IN"):
                       
                          $(this).addClass('dynamic-link-te');
                                                               
                        break; 
                        case ("kn_IN"):
                       
                          $(this).addClass('dynamic-link-kn');
                                                               
                        break; 
                      default:
                        //console.log("Empty switch received.");
                        if($(this).text().length > 28) {
                          $(this).addClass('dynamic-link');
                        }
                    }  
                  })  
                }

                $('.tab-block-wrapper > li.nav-item.megamenu-li').on('mouseenter keypress', function (e) {
                  
                      const currentElm = e.currentTarget;
                      // nav link border and arw change on nav link click
                          
            
                      if($(currentElm).hasClass('nav-border') && $(currentElm).find('a.nav-link > img').hasClass('arw-change')) {
            
                        $(currentElm).removeClass('nav-border');
            
                        $(currentElm).find('a.nav-link > img').removeClass('arw-change');
            
                        // console.log('true');
            
                      } else {
            
                        
            
                        $('.tab-block-wrapper > li').not(this).removeClass('nav-border');
            
                        $('.tab-block-wrapper > li').not(this).find('a.nav-link > img').removeClass('arw-change');
            
                        $(currentElm).addClass('nav-border');
            
                        $(currentElm).find('a.nav-link > img').addClass('arw-change');
            
                        // console.log('false');
            
                      }
                      
                      // two extra line code for navigation overlapping issue fix
            
                      $(currentElm).find('ul.dropdown-submenu-ul > li').removeClass('active');
            
                      $(currentElm).find('ul.dropdown-submenu-ul > li:nth-child(1)').addClass('active');
                      
                      $(currentElm).find('ul.dropdown-submenu-ul > li:nth-child(1)').addClass('active');

                      //new logic one add for dynamic nav height on load
                      const firstSubMenu = currentElm.querySelector('ul.dropdown-submenu-ul');
                      const firstChildsub = currentElm.querySelector('ul.childsubchild');
                       //new rules to get anchor tag
                       const childLink = $(currentElm).find('ul.dropdown-submenu-ul > li.childsubmenu.active ul.childsubchild > li > a');
                       //console.log(firstChildsubLink.length);

                    if(firstSubMenu && window.matchMedia('(min-width: 1200px)').matches) {
                        getChar(childLink,selectLangtext);
                        if($(firstChildsub).children().length <= 2) {
                            //console.log('2 or below 2');
                            $(firstChildsub).addClass('height-100');
                            $(firstSubMenu).closest('.megamenu').removeClass('img-dynamic');
                            $(firstSubMenu).closest('.megamenu').removeClass('img-dynamic-lg');
                            $(firstSubMenu).closest('.megamenu').removeClass('megamenu-350');
                            $(firstSubMenu).closest('.megamenu').removeClass('megamenu-400');
                            $(firstSubMenu).closest('.megamenu').removeClass('megamenu-460');
                        } else if($(firstChildsub).children().length <= 4) {
														//console.log('4 or below 4');
                            $(firstChildsub).addClass('height-175');
                            $(firstChildsub).removeClass('height-100');
                            $(firstSubMenu).closest('.megamenu').removeClass('img-dynamic');
                            $(firstSubMenu).closest('.megamenu').removeClass('img-dynamic-lg');
                            $(firstSubMenu).closest('.megamenu').removeClass('megamenu-350');
                            $(firstSubMenu).closest('.megamenu').removeClass('megamenu-400');
                            $(firstSubMenu).closest('.megamenu').removeClass('megamenu-460');
                        } else if($(firstChildsub).children().length === 10) {
                            //console.log('when 10');
                            $(firstChildsub).addClass('height-350');
                            $(firstSubMenu).closest('.megamenu').addClass('img-dynamic-lg');
                            if($(childLink).hasClass('dynamic-link')) {
                              //console.log('has dynamic');
                              $(firstSubMenu).closest('.megamenu').addClass('megamenu-460');
                            } else {
                              //console.log('no dynamic');
                              $(firstSubMenu).closest('.megamenu').removeClass('megamenu-460');
                              $(firstSubMenu).closest('.megamenu').addClass('megamenu-400');
                            }
                            //console.log(childLink);
                        }
                      } if(firstSubMenu && window.matchMedia('(min-width: 1400px)').matches) {
                        getChar(childLink,selectLangtext);
                        if($(firstChildsub).children().length === 10) {
                          $(firstChildsub).removeClass('height-350');
                          //$(firstSubMenu).closest('.megamenu').addClass('megamenu-460');
                          $(firstSubMenu).closest('.megamenu').removeClass('megamenu-350');
                          $(firstSubMenu).closest('.megamenu').removeClass('megamenu-400');
                          if($(childLink).hasClass('dynamic-link')) {
                              //console.log('has dynamic');
                              $(firstChildsub).addClass('height-450');
                              $(firstSubMenu).closest('.megamenu').addClass('megamenu-500');
                            } else {
                              //console.log('no dynamic');
                              $(firstSubMenu).closest('.megamenu').removeClass('megamenu-500');
                              $(firstSubMenu).closest('.megamenu').addClass('megamenu-460');
                            }
                        }
                      } if(firstSubMenu && window.matchMedia('(min-width: 1920px)').matches) { 
                          getChar(childLink,selectLangtext);
                          if($(firstChildsub).children().length <= 4) {
                            $(firstSubMenu).closest('.megamenu').addClass('megamenu-400');
                          } else if($(firstChildsub).children().length === 10) {
                            $(firstChildsub).removeClass('height-400');
                            $(firstChildsub).addClass('height-450');
                            $(firstSubMenu).closest('.megamenu').removeClass('megamenu-460');
                            $(firstSubMenu).closest('.megamenu').addClass('megamenu-550');
                          }
                      }



                      // new logic one ends
            
                      if($(currentElm).find('div.main-eligibility').hasClass('d-none')) {
                        $(currentElm).find('div.main-eligibility').removeClass('d-none');
                        $(currentElm).find('div.main-eligibility').addClass('d-block');
                              
          
                      } else {
                        $(currentElm).find('div.main-eligibility').removeClass('d-block');
                        $(currentElm).find('div.main-eligibility').addClass('d-none');                             
          
                      }
                      $('.tab-block-wrapper > li').not(this).find("div.main-eligibility").removeClass('d-block');
                      $('.tab-block-wrapper > li').not(this).find("div.main-eligibility").addClass('d-none');
                      

                    });
                    // sub menu navigation function
                     $('.dropdown-submenu-ul > li').on('mouseenter keypress',function(e) {
                       const currentElm = e.currentTarget;
                      //console.log(currentElm);

                      const subChildLink = $(currentElm).find('ul.childsubchild > li > a.dropdown-item');
                      //console.log(subChildLink.length);
                      //getChar(subChildLink);

                                        // new logic two add for dynamic nav height on sub menu nav hover
                                        const subChild = this.querySelector('.childsubchild');
                      // console.log('test ' + $(subChild).children().length);
                      //console.log(subChild.clientHeight);
                      // console.log('test2 ' + subChild.children.length);

                    if(window.matchMedia('(min-width: 1200px)').matches) {
                        getChar(subChildLink,selectLangtext);
                        if($(subChild).children().length <= 2) {
                          $(subChild).addClass('height-100');
                          $(subChild).closest('.megamenu').removeClass('megamenu-350');
                          $(subChild).closest('.megamenu').removeClass('megamenu-400');
                          $(subChild).closest('.megamenu').removeClass('megamenu-460');
                          $(subChild).closest('.megamenu').removeClass('megamenu-500');
                          $(subChild).closest('.megamenu').removeClass('img-dynamic');
                          $(subChild).closest('.megamenu').removeClass('img-dynamic-lg');
                        } else if($(subChild).children().length <= 4) {
                          $(subChild).closest('.megamenu').removeClass('megamenu-350');
                          $(subChild).closest('.megamenu').removeClass('megamenu-400');
                          $(subChild).closest('.megamenu').removeClass('megamenu-460');
                          $(subChild).closest('.megamenu').removeClass('megamenu-500');
                          $(subChild).closest('.megamenu').removeClass('img-dynamic');
                          $(subChild).closest('.megamenu').removeClass('img-dynamic-lg');
                          $(subChild).removeClass('height-245');
                          $(subChild).removeClass('height-175');                         
                          $(subChild).addClass('height-150');
                        } else if($(subChild).children().length <= 6 ) {
                          $(subChild).addClass('height-245');
                          $(subChild).closest('.megamenu').removeClass('megamenu-350');
                          $(subChild).closest('.megamenu').removeClass('img-dynamic');
                          $(subChild).closest('.megamenu').removeClass('img-dynamic-lg');
                          $(subChild).closest('.megamenu').removeClass('megamenu-400');
                          $(subChild).closest('.megamenu').removeClass('megamenu-460');
                          $(subChild).closest('.megamenu').removeClass('megamenu-500');
                        } else if($(subChild).children().length > 6 && $(subChild).children().length < 10) {
                          $(subChild).addClass('height-290');
                          $(subChild).closest('.megamenu').addClass('megamenu-350');
                          $(subChild).closest('.megamenu').addClass('img-dynamic');
                        } else if($(subChild).children().length === 10) {
                          $(subChild).closest('.megamenu').removeClass('megamenu-350');
                          $(subChild).closest('.megamenu').addClass('img-dynamic-lg');
                          $(subChild).removeClass('height-450');
                          if($(subChildLink).hasClass('dynamic-link')) {
                              //console.log('has dynamic');
                              $(subChild).addClass('height-350');
                              $(subChild).closest('.megamenu').addClass('megamenu-460');
                            } else {
                              //console.log('no dynamic');
                              $(subChild).closest('.megamenu').removeClass('megamenu-500');
                              $(subChild).closest('.megamenu').addClass('megamenu-400');
                            }
                        }
                      } if(window.matchMedia('(min-width: 1400px)').matches) {
                          getChar(subChildLink,selectLangtext);
                          //console.log('1400-2');
                          if($(subChild).children().length <= 2) {
                            $(subChild).removeClass('height-245');
                          } else if($(subChild).children().length === 3) {
                            $(subChild).closest('.megamenu').removeClass('megamenu-350');
                          } else if($(subChild).children().length <= 4) {
                            $(subChild).removeClass('height-245');
                          } else if($(subChild).children().length <= 6 ) {
                              $(subChild).addClass('height-245');
                              $(subChild).closest('.megamenu').addClass('img-dynamic');
                              //$(subChild).closest('.megamenu').addClass('megamenu-350');
                          } else if($(subChild).children().length > 6 && $(subChild).children().length < 10) {
                            $(subChild).closest('.megamenu').removeClass('megamenu-350');
                            $(subChild).closest('.megamenu').addClass('megamenu-400');
                          } else if($(subChild).children().length === 10) {
                            $(subChild).closest('.megamenu').removeClass('megamenu-350');
                            $(subChild).closest('.megamenu').removeClass('megamenu-400');
                            $(subChild).closest('.megamenu').addClass('img-dynamic-lg');
                            if($(subChildLink).hasClass('dynamic-link')) {
                              //console.log('has dynamic');
                              $(subChild).addClass('height-450');
                              $(subChild).closest('.megamenu').addClass('megamenu-500');
                            } else {
                              //console.log('no dynamic');
                              $(subChild).closest('.megamenu').removeClass('megamenu-500');
                            }
                        }
                      } if(window.matchMedia('(min-width: 1920px)').matches) {
                        getChar(subChildLink,selectLangtext);
                        if($(subChild).children().length <= 2) {
                          $(subChild).removeClass('height-175');
                          $(subChild).closest('.megamenu').addClass('megamenu-400');
                        } else if($(subChild).children().length <= 4) {
                          $(subChild).removeClass('height-150');
                          $(subChild).addClass('height-175');
                          $(subChild).closest('.megamenu').addClass('megamenu-400');
                        } else if($(subChild).children().length <= 6 ) {
                          $(subChild).addClass('height-250');
                          $(subChild).closest('.megamenu').removeClass('img-dynamic');
                          $(subChild).closest('.megamenu').addClass('megamenu-400');
                          $(subChild).closest('.megamenu').removeClass('megamenu-350');
                        } else if($(subChild).children().length > 6 && $(subChild).children().length < 10) {
                          $(subChild).removeClass('height-290');
                          $(subChild).addClass('height-350');
                          $(subChild).closest('.megamenu').removeClass('img-dynamic');
                          $(subChild).closest('.megamenu').addClass('img-dynamic-lg');
                          $(subChild).closest('.megamenu').removeClass('megamenu-400');
                          $(subChild).closest('.megamenu').addClass('megamenu-500');
                        } else if($(subChild).children().length === 10) {
                            $(subChild).closest('.megamenu').removeClass('megamenu-460');
                        }
                      }
                    // new logic two ends

                      if($(currentElm).hasClass('active')) {
                        // $(this).removeClass('active');
                        // console.log('has active')
                      }
                      else {
                        // $('.dropdown-submenu-ul > li').not(this).removeClass('active');
                        $(this).closest('.dropdown-submenu-ul').find('li:nth-child(1)').removeClass('active');
                        $(this).closest('.dropdown-submenu-ul').not(this).find('li.active').removeClass('active');
                        $(this).addClass('active');
                        // console.log("has not active");
                        
                      }
                      e.stopPropagation();
                      // e.preventDefault();				
                    })
                    
                
                  
                 
          
                    // non desktop navigation function starts
                    $('#tab-nondesk li.parent-list:nth-child(2)').addClass('nondesk-active');
                    $('#tab-nondesk li.nondesk-active li.child-list:nth-child(1)').addClass('child-active');
            
                    $('#tab-nondesk .parent-link').off('click').on('click', function(e) {
                      const currentElm = e.currentTarget;
                      // const parentActive = currentElm.closest('li.nondesk-active');
                      if(currentElm.closest('li.nondesk-active')) {
                        $(currentElm).parent('.parent-list').removeClass('nondesk-active');
                      } else {
                        $('.parent-list').not(this).removeClass('nondesk-active');
                        $(currentElm).parent('.parent-list').addClass('nondesk-active');
                      }
            
                    })
            
                    $('#tab-nondesk li.child-list').off('click').on('click', function(e){
                      const parentList = e.currentTarget;
                     
                       if($(parentList).hasClass('child-active')) {
                        // show sub-child
                        $(parentList).find('div.sub-child-div > ul.sub-child-ul').removeClass('d-none');
                          $(parentList).find('div#sub-child-div').removeClass('d-none');
                        
                        // new function for body scroll stop
                        $('body').addClass('jsNav-bodyScroll-stop');
                       
          
                      }
                      else {
                        // add child-active class on list
                        $(this).addClass('child-active');
                        // remove child-active from other list
                        $('#tab-nondesk li.child-list').not(this).removeClass('child-active');
                        // show sub-child
                        $(this).find('div.sub-child-div > ul.sub-child-ul').removeClass('d-none');
                          
                        // new function for body scroll stop
                       // $('body').addClass('jsNav-bodyScroll-stop');
                        
                      }
                    })
                    
                    
                    // non desktop navigation function ends
          
          
                  //   Home Active class 
                  
               
              
          
              
              });
          
         var links = document.querySelectorAll('.home-icon a');
          
          for (link of links) {
              if (window.location.pathname == link.getAttribute('href')) {
                  link.classList.add('active')
              } else {
                  link.classList.remove('active')
              }
          }
        
                  
             function navigateBack(ele){    
              
                $(ele).closest("#sub-child-div").addClass("d-none");
                $(ele).closest(".child-list").removeClass("child-active");
            }
                  
              
    
    
    function showNavDisclaimer(arg)
    {
    var getLink=arg.trim();
    var disctext=`Dear Customer,<br>  It is our constant endeavour to provide hassle free, secure banking experience to our customers. In the series, we are upgrading our portal with latest features for Secured Internet communication. The upgrade of security may require newer versions of Internet browser at User end. We request all our esteemed customers to ensure minimum browser specifications for better security as given below:<p><br>  Internet Explorer 9 & above <br> Mozilla Firefox	34 & above<br> Chrome	40 & above<br> Safari	7 & above with screen resolution of 1024 x 768<p>  For any support or clarification please contact Toll-free Call Centre No.1800220229`;
    
   
     
	 
	   
	    // $(".navExternalLinkPopup .modal-body").html(disctext);
		 $("#externalLinkPopup .modal-body").html(disctext);
		
		// $(".megamenu").hide();
		 //alert("hi");
	    //$(".navExternalLinkPopup").show();
		
		$("#externalLinkPopup").show();
		//console.log("after show");
	    $(".confirmRedirect").data('discHref', getLink);
	    
	    }
	   
	   function closePopup(x)
	   {
	   
	    $(".ExternalLinkPopup").hide();
	   }
	   function closeRedirectPopup(ele)
	   {
	   $(".ExternalLinkPopup").hide();
	  
	   var redirectURL=$(".confirmRedirect").data('discHref');
	   window.open(redirectURL,'_blank');
	  }
	   
	             
