          <script type="text/javascript">
    
    
    function showFloatingLinksDisclaimer(arg,externalLink,disctext)
    {
    var getLink=arg;
    
    console.log("inside showDisclaimer function arg"+getLink +"external "+externalLink+"disctext"+disctext);
    
     var getSelected=externalLink;
  if (getSelected=="yes")
	   {
	   console.log("inside ext script");
	     $("#externalLinkFloatingIconsPopup .modal-body").html(disctext);
	    $("#externalLinkFloatingIconsPopup").show();
	    $(".confirmRedirect").data('discHref', getLink);
	    
	    }
	    
	    else
	    {
	      window.location.href =getLink;   
	    }
	    
	   }
	   
	   function closePopup(x)
	   {
	   console.log("get x"+x);
	    $(".ExternalLinkPopup").hide();
	   }
	   function closeRedirectPopup(ele)
	   {
	   $(".ExternalLinkPopup").hide();
	  // window.location.href =
	   var redirectURL=$(".confirmRedirect").data('discHref');
	   window.open(redirectURL,'_blank');
	  }
	   
	    </script>
