<script id="pageActualJson" >

function getActualBreadcrumbJson(){
var breadcrumbItemListElement = [];
var productObj;
  const pages=$("#pageDetailsId").val().split("&&");

  var actualBreadcrumbJson="";
for(i=1;i<pages.length;i++)
{

let elementBreadcrumbObj = {
        "@type" :"ListItem",
        "position":pages[i].split("%")[0],
         "name":pages[i].split("%")[1],
        "item":pages[i].split("%")[2],
         
      };
      
     breadcrumbItemListElement.push(elementBreadcrumbObj)
  
}

actualBreadcrumbJson+=JSON.stringify(breadcrumbItemListElement);
return actualBreadcrumbJson;
}



var breadcrumbSchema = document.createElement('script');
  breadcrumbSchema.type = 'application/ld+json';

  breadcrumbSchema.text = JSON.stringify({
        "@context": "https://schema.org/",
        "@type": "BreadcrumbList",
        "itemListElement": JSON.parse(getActualBreadcrumbJson())
    },null,2);

  document.querySelector('body').appendChild(breadcrumbSchema);

</script>
	
