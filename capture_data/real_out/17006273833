$(document).ready(function(){
$('.see-less-more-btn').click(function(){
       /* if ($.trim($(this).text()) === 'See Less') {
         $(this).html(`Additional links <img src="/o/boi-global-theme/images/boi/arrowup.png" height="20" class="ml-1" alt="See more">`);
         $('#footer-show').hide();         
        } else {
         $('#footer-show').show();
         $(this).html( `See Less <img src="/o/boi-global-theme/images/boi/arrow-down.svg" alt="See Less" height="20">`);         
        }*/
        $('#footer-show').hide(); 
				 $(this).hide();
				 $(".see-more-btn").show();
    });
         
         $(".see-more-btn").click(function(){
					 $('#footer-show').show(); 
					$(this).hide();
					$(".see-less-more-btn").show();
				 });
                });
    //small jquery for footer link new tab issue fix starts
    
    //select all footer links
    var footerLinks = document.querySelectorAll('.footer a');
    // console.log(footerLinks);
    //add event preventDefault for showing pop up and to stop anchor default behaviour
  for (link of footerLinks) {
  $(link).on('click', function (e) {
    e.preventDefault();
    const currentElm = e.currentTarget;
    const parentDiv = currentElm.closest('.popularmenu');
    //console.log((parentDiv));
    //console.log(currentElm);
    if (parentDiv) {
      //console.log('popular');
      window.location = $(this).attr('href');
      //console.log(currentElm.href);
    } else {
      e.preventDefault();
    }
  });
}
    
    //small jquery for footer link new tab issue fix ends
    
    
    function showDisclaimer(arg,externalLink,disctext)
    {
    var getLink=arg;
    
    //console.log("inside showDisclaimer function arg"+getLink +"external "+externalLink+"disctext"+disctext);
    console.log("inside show disclaimer for mobile social lik rd btn is "+externalLink);
     var getSelected=externalLink.trim();
  if (getSelected=="yes")
	   {
	   console.log("inside ext script");
	     $("#externalLinkPopup .modal-body").html(disctext);
	    $("#externalLinkPopup").show();
	    $(".confirmRedirect").data('discHref', getLink);
	    
	    }
	    
	    else
	    {
	      window.location.href =getLink;   
	    }
	    
	   }
	   
	   function closePopup(x)
	   {
	   console.log("get x"+x);
	    $(".ExternalLinkPopup").hide();
	   }
	   function closeRedirectPopup(ele)
	   {
	   $(".ExternalLinkPopup").hide();
	  // window.location.href =
	   var redirectURL=$(".confirmRedirect").data('discHref');
	   window.open(redirectURL,'_blank');
	  }
	   

    
         
         
         $(".collapse.show").each(function () {
             $(this)
                 .prev(".card-header")
                 .find(".ba")
                 .addClass("bi bi-chevron-up")
                 .removeClass("bi-chevron-down");
         });
  
         // Toggle plus minus icon on show hide of collapse element
  
         $(".collapse")
           .on("show.bs.collapse", function () {
                   $(this)
                       .prev(".card-header")
                       .find(".bi")
                       .removeClass("bi bi-chevron-down")
                       .addClass("bi bi-chevron-up");
               })
           .on("hide.bs.collapse", function () {
                   $(this)
                       .prev(".card-header")
                       .find(".bi")
                       .removeClass("bi bi-chevron-up")
                       .addClass("bi bi-chevron-down");
               });
               Liferay.Service(
  '/boi_pagevisitor.pagevisitor/get-page-visitors-total-count',
  function(obj) {
   
    $(".custom-visit-count").html($(".custom-visit-count").text()+
    "<b>"+obj+"</b>");
  }
);
               
