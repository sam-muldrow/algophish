	<script>
		function submitFeedbackShare(){
			   $.ajax({
	                 type: "POST",
	                 url: "https://bankofindia.co.in/home?p_p_id=com_feedback_share_web_FeedbackSharePortlet&p_p_lifecycle=2&p_p_state=normal&p_p_mode=view&p_p_resource_id=saveFeedbackShare&p_p_cacheability=cacheLevelPage",
	                 data: {
	                    "_com_feedback_share_web_FeedbackSharePortlet_rate": $("input[name='_com_feedback_share_web_FeedbackSharePortlet_rate']:checked").val()
	                 },
	                 async: false,
	                 cache: false,
	                 success: function (result) {
						
						
	                 }

	              });
		}
	</script>


	
	
					
				
			
		
	
	


