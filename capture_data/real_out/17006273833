          <script>   
  $( document ).ready(function() {
  $('.owl-carousel').owlCarousel({
  
  loop: true,
  
  margin: 10,
  
  responsiveClass: true,
  
  nav: true,
  
  navText: ["<div class='nav-btn prev mr-1'>   <img src='/o/boi-global-theme/images/boi/Prev-icon-carousel.svg' alt='previous slide'><span class='d-none d-md-inline'>Prev</span> </div>", "<div class='nav-btn next'><span class='d-none d-md-inline'>Next</span><img src='/o/boi-global-theme/images/boi/Next-icon-carousel.svg' alt='next slide'></div>"],
  
  
  
  responsive: {
  
  0: {
  
  items: 1,
  
  nav: true
  
  },
  
  600: {
  
  items: 1,
  
  nav: true
  
  },
  
  768: {
  
  items: 2,
  
  nav: true
  
  },
  
  1000: {
  
  items: 3,
  
  nav: true,
  
  loop: false,
  
  margin: 25
  
  }
  
  }
  
  })
  //this script only for owl carousal dot navigation accessibility
  $('.owl-carousel').find('.owl-dot').each(function(index) {
    //Add one to index so it starts from 1
    $(this).attr('aria-label', 'button' + ' ' + (index + 1));
  });
   $('.owl-carousel').find('button').each(function() {
    //Add one to index so it starts from 1
    $(this).removeAttr('role');
  });
// show testimonial video on focus event
$('.testimonial-box').each(function() {
    $(this).keypress(function(e) {
      if(e.key === 'Enter' || e.key === ' ') {
        const elm = e.currentTarget;
        const videoLink = $(elm).find('a').attr('href')
        //console.log(videoLink);
        window.open(videoLink,'_blank');
      }
    })
  })
});
  </script>
