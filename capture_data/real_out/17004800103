<script>
// show or hide login button
var rmad = false;
var isLogin = false;

tkValid = 0;
// 3 = active , subscriber, has ads free
if (tkValid > 0) {
    isLogin = true;
} 
if (tkValid == "3") {
    rmad = true;
}

jQuery(document).ready(function() {
if (tkValid > 0) {
    jQuery('.btn_login').hide();
    jQuery('.login_menu').show();
    jQuery('.m-login-menu').show();
    jQuery('.m-login-button').attr("style", "display: none !important"); // Override css
} else {
    jQuery('.btn_login').show();
    jQuery('.login_menu').hide();
    jQuery('.m-login-button').show();
    jQuery('.m-login-menu').hide();
}
});
</script>
<!-- Piano -->
<script type="text/javascript">
var pageType = jQuery('meta[name=pageType]').attr("content");
if(pageType == "article"){
var contentType = jQuery('meta[name=content_tier]').attr("content").toLowerCase();
tp = window["tp"] || [];
tp.push(["init", function() 
{        
tp.push(["setCustomVariable", "userType", "anonymous"]);
    
    if(tkValid == 3)
        tp.push(["setCustomVariable", "isAdsFree", "true"]);
    else
        tp.push(["setCustomVariable", "isAdsFree", "false"]);
if(contentType == "branded" || contentType == "complimentary")
    tp.push(["setTags", "free"]);
else if(contentType == "na")
    tp.push(["setTags", "metered"]);
else
    tp.push(["setTags", contentType]);
tp.push(["addHandler", "setResponseVariable",
  function(eventParams){
            console.log("Paywall: " + eventParams.responseVariables.enablepaywall.toString());
    console.log("LTS: " + eventParams.responseVariables.storytype);
    console.log("Segment: " + eventParams.responseVariables.usersegment);

            if(eventParams.responseVariables.enablepaywall.toString() == "false" && eventParams.responseVariables.storytype.toString() == "metered")
            { 
                var contentStoryId = jQuery('meta[name=content_id]').attr("content");
                var ecs = jQuery("#hecs").val();
                jQuery.ajax({
                  url: '/rfsc',
                  type: "POST",
                  data: {
                      cs: ecs,
                      storyID: contentStoryId
                  },
                  success: function (data) {
                      if (data["content"]) {
                          jQuery("#hecs").remove();
                          jQuery("#noticeDivTemp").remove();
                          jQuery("#story-body").html(data["content"]);
                          jQuery("#user_engagement_div").show();
                      }

                  }
                });
            }
            else
			{
                jQuery("#hecs").remove();
                jQuery("#noticeDivTemp").remove();
                jQuery("#cx_paywall").css("clear","both");
			}
  }
]);
    tp.push(["addHandler", "checkoutCustomEvent",    
        function(event){      
            switch(event.eventName) {        
                  case "subscribe":       
                     window.dataLayer = window.dataLayer || []; window.dataLayer.push({'event': 'paywall_click','paywall_type' : 'subscribe'});
                     break;
            }
        }
    ]);
}

]);
}
</script>
