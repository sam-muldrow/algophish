<script>
	var slcontent_0_dramatic_story = document.getElementById("slcontent_0_dramatic_story");
	if (document.getElementById("slcontent_0_drama_time") != null && document.getElementById("slcontent_0_drama_time") != 'undefined') {
        if (slcontent_0_drama_time.style.display != '')
            document.getElementById("non_dramatic_weather").style.display = "block";
    }
    else {
        if(slcontent_0_dramatic_story != null){
            if (slcontent_0_dramatic_story.style.display == 'none'){
                document.getElementById("non_dramatic_weather").style.display = "block";
    		}
        }
    }
	if (document.getElementById("klse_lastdone") !== null && document.getElementById("klse_lastdone") !== 'undefined') {
		document.getElementById("klse_lastdone").innerHTML = klselastdone + ' ';
		if (klsechange.indexOf('+') !== -1) {
			document.getElementById("klse_change").innerHTML = klsechange;
			document.getElementById("klse_change").className = "up";
			document.getElementById("klse_tri").className = "triangle triangle-up";
		} else {
			document.getElementById("klse_change").innerHTML = klsechange;
			document.getElementById("klse_change").className = "down";
			document.getElementById("klse_tri").className = "triangle triangle-down";
		}
	}
</script>
