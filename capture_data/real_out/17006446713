<script integrity="sha256-2Pmvv0kuTBOenSvLm6bvfBSSHrUJ+3A7x6P5Ebd07/g=" crossorigin="anonymous" src="https://code.jquery.com/jquery-3.7.0.min.js"></script>
<script type="text/javascript">
$(document).ready(function() {
    var status_url = 'https://7d1n8m3wvdxf.statuspage.io/api/v2/components.json';

    var app_client_id = 'p3zmh66knzfk';
    var ngdp_api_id = '04t9b285vkm0';
    var ngdp_nemlogin_id = 'w1b8q8q444zl';
    var web_client_id = 'wkdsv8h0vwwv';

    var app_client = null;
    var ngdp_api = null;
    var ngdp_nemlogin = null;
    var web_client = null;

    (function getstatus() {
        var jqxhr = $.get(status_url, function(data) {
            if (!data.components) return;

            for (var i=0;i<data.components.length;i++) {
                switch(data.components[i].id) {
                    case app_client_id:
                        app_client = data.components[i];
                        break;
                    case ngdp_api_id:
                        ngdp_api = data.components[i];
                        break;
                    case ngdp_nemlogin_id:
                        ngdp_nemlogin = data.components[i];
                        break;
                    case web_client_id:
                        web_client = data.components[i];
                        break;
                }
            }

            var status = '';
            if(web_client && web_client.status == 'under_maintenance') {
                if(app_client && app_client.status == 'under_maintenance') {
                    status = 'e-Boks er i gang med en opdatering. Det betyder, at det i &#248;jeblikket ikke er muligt at logge p&#229;. Du kan i mellemtiden f&#229; vist din post fra det offentlige p&#229; Borger.dk og Virk.dk. Vi beklager ulejligheden.';
                }
                else {
                    status = 'e-Boks er i gang med en opdatering. Det betyder, at det i &#248;jeblikket ikke er muligt at logge p&#229; vores webl&#248;sning. Du kan i mellemtiden logge p&#229; e-Boks&#39; app og her f&#229; vist din post. Vi beklager ulejligheden.';
                }
            }
            else if(web_client && web_client.status != 'operational') {
                if(app_client && app_client.status != 'operational') {
                    if(web_client.status == 'major_outage') {
                        status = 'Vi oplever i &#248;jeblikket tekniske problemer, som kan medf&#248;re, at du ikke kan logge p&#229;. Du kan i mellemtiden f&#229; vist din post fra det offentlige p&#229; Borger.dk og Virk.dk. Vi arbejder p&#229; en l&#248;sning og beklager ulejligheden.';
                        status = 'Vores underleverand&#248;r oplever i &#248;jeblikket tekniske problemer, som kan medf&#248;re, at du ikke kan logge p&#229;. Du kan i mellemtiden f&#229; vist din post fra det offentlige p&#229; Borger.dk og Virk.dk. Vores underleverand&#248;r arbejder p&#229; at f&#229; l&#248;st fejlen. Vi beklager ulejligheden.';
                        status = 'Vores driftsleverand&#248;r KMD oplever i &#248;jeblikket tekniske problemer, som kan medf&#248;re, at du ikke kan logge p&#229;. Du kan i mellemtiden f&#229; vist din post fra det offentlige p&#229; Borger.dk og Virk.dk. KMD arbejder p&#229; at f&#229; l&#248;st fejlen. Vi beklager ulejligheden.';
                    }
                    else {
                        status = 'Vi oplever i &#248;jeblikket tekniske problemer, som kan give l&#230;ngere svartider, n&#229;r du logger p&#229; eller vil se din post. Vi arbejder p&#229; en l&#248;sning og beklager ulejligheden.';
                    }
                }
                else {
                    if(web_client.status == 'major_outage') {
                        status = 'Vi oplever i &#248;jeblikket tekniske problemer p&#229; vores webl&#248;sning, som kan medf&#248;re, at du ikke kan logge p&#229;. Du kan i mellemtiden logge p&#229; e-Boks&#39; app og her f&#229; vist din post fra private virksomheder. Vi arbejder p&#229; en l&#248;sning og beklager ulejligheden.';
                    }
                    else {
                        status = 'Vi oplever i &#248;jeblikket tekniske problemer p&#229; vores webl&#248;sning, som kan give l&#230;ngere svartider, n&#229;r du logger p&#229; eller vil se din post. Vi arbejder p&#229; en l&#248;sning og beklager ulejligheden.';
                    }
                }
            }
            else if(ngdp_api && ngdp_api.status != 'operational') {
                status = 'Udbyderen af Digital Post fra det offentlige oplever i &#248;jeblikket tekniske problemer, som kan medf&#248;re, at du ikke kan logge p&#229; e-Boks. Fejlen er rapporteret til Digitaliseringsstyrelsen og afventer deres fejlrettelse. Du kan i mellemtiden logge p&#229; e-Boks&#39; app og her f&#229; vist din post fra private virksomheder.';
                status = 'Udbyderen af Digital Post fra det offentlige oplever i &#248;jeblikket tekniske problemer, som kan medf&#248;re, at du ikke kan logge p&#229; e-Boks eller se din post fra offentlige myndigheder. Fejlen er rapporteret til Digitaliseringsstyrelsen og afventer deres fejlrettelse.';
            }
            else if(ngdp_nemlogin && ngdp_nemlogin.status != 'operational') {
                status = 'Der opleves tekniske problemer med den offentlige NemLog-In l&#248;sning, som kan medf&#248;re, at du ikke kan logge p&#229; e-Boks med MitID eller NemID. Fejlen er rapporteret til Digitaliseringsstyrelsen og vi afventer deres fejlrettelse. Du kan i mellemtiden logge p&#229; e-Boks&#39; app og her f&#229; vist din post.';
                status += '<br /><br />Brug for at læse post fra en privat virksomhed? <a href="https://www.e-boks.dk/privat?logontype=appcode">Log på med din e-Boks app</a>'
            }
            else {
                    $('.eboks-web-status').removeClass('status-bar--active');
                    $('.eboks-web-status .container').empty();
            }
            if(status != '') {
                    $('.eboks-web-status').addClass('status-bar--active');
                    $('.eboks-web-status .container').html('<p>' + status + '</p>');
            }
        });
    })();
});
