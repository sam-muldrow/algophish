    <script type='text/javascript'>
        var start = $(window).scrollTop();
        var delta = 0;
        var animate = false;

        $(function () {
            $(window).live('scroll', onScrollFunction);
        });

        function onScrollFunction(e) {
            delta = Math.abs($(window).scrollTop() - start);
            if (delta > 10 && animate == false) {
                animate = true;
                $('.chamster-link').animate({ marginTop: 0 }, 800);
            }
        }
    </script>
</head>
