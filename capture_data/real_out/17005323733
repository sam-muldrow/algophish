<script>
    var flag=true;

    jQuery(window).scroll(function() {
        var top_of_element = jQuery(".stats-panel").offset().top;
        var bottom_of_element = jQuery(".stats-panel").offset().top + jQuery(".stats-panel").outerHeight();
        var bottom_of_screen = jQuery(window).scrollTop() + jQuery(window).innerHeight();
        var top_of_screen = jQuery(window).scrollTop();

        if ((bottom_of_screen > top_of_element) && (top_of_screen < bottom_of_element) && flag){
            jQuery('.count').each(function () {
                jQuery(this).prop('Counter',0).animate({
                    Counter: jQuery(this).text()
                }, {
                    duration: 4000,
                    easing: 'swing',
                    step: function (now) {
                        jQuery(this).text(Math.ceil(now));
                    }
                });

                flag = false;
            });
        }
    });
</script>
