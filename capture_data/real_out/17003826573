<script nonce="27s4xPqUF9CgWuYq+nuTXYx2GpD+iLsMc2tvCZaWKEM=" type="text/javascript" src="/staticx/udemy/js/webpack/entry-main-manifest.de7dba69f370430bb64c.js" defer></script>
<script nonce="27s4xPqUF9CgWuYq+nuTXYx2GpD+iLsMc2tvCZaWKEM=" type="text/javascript" src="/staticx/udemy/js/webpack/entry-main-vendor.46a7c5aed287d46110d9.js" defer></script>
<script nonce="27s4xPqUF9CgWuYq+nuTXYx2GpD+iLsMc2tvCZaWKEM=" type="text/javascript" src="/staticx/udemy/js/webpack/jsi18n-en-us-discovery-core.fc4a072b5a2da623b378.js" defer></script>
<script nonce="27s4xPqUF9CgWuYq+nuTXYx2GpD+iLsMc2tvCZaWKEM=" type="text/javascript" src="/staticx/udemy/js/webpack/entry-main.2c152d261ec6ca9e2120.js" defer></script>
<script nonce="27s4xPqUF9CgWuYq+nuTXYx2GpD+iLsMc2tvCZaWKEM=">
    window.handleCSSToggleButtonClick = function (event) {
        var target = event.currentTarget;
        var cssToggleId = target && target.dataset && target.dataset.cssToggleId;
        var input = cssToggleId && document.getElementById(cssToggleId);
        if (input) {
            if (input.dataset.type === 'checkbox') {
                input.dataset.checked = input.dataset.checked ? '' : 'checked';
            } else {
                input.dataset.checked = input.dataset.allowToggle && input.dataset.checked ? '' : 'checked';
                var radios = document.querySelectorAll('[name="' + input.dataset.name + '"]');
                for (var i = 0; i < radios.length; i++) {
                    if (radios[i] !== input) {
                        radios[i].dataset.checked = '';
                    }
                }
            }
        }
    };
    (function () {
        var cssToggleButtons = document.querySelectorAll('[data-css-toggle-id');
        for (var i = 0; i < cssToggleButtons.length; i++) {
            cssToggleButtons[i].addEventListener('click', window.handleCSSToggleButtonClick);
        }
    })();
</script>
