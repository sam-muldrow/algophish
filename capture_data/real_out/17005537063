var hot_products = JSON.parse(hot_products_json);
$(document).ready(function() {
	selected_hot_product = $.cookie('hot_product');
	if (selected_hot_product == undefined)
	{
		selected_hot_product = 1;
	}
	$(".hot_products").siblings().removeClass("selected");
	$(".hot_products").filter(function( index ) {
		return $( this ).attr( "rel" ) == selected_hot_product;
	}).addClass("selected");
	$(".gadget_wrap").empty();
	hot_products[selected_hot_product].map((data, index) => {
		$(".gadget_wrap").append('<a href="' + data.url + '"><img src="https://cdn09.dcfever.com' + data.image + '"><span class="name">' + data.model_no + '</span></a>').show();
	});
});

$(".hot_products").click(function(e) {
	e.preventDefault();
	id = $(this).attr("rel");

	date = new Date();
	minutes = 30;
	date.setTime(date.getTime() + (minutes * 60 * 1000));
	$.cookie('hot_product', id, { expires : date } );

	$(this).siblings().removeClass("selected");
	$(this).addClass("selected");
	$(".gadget_wrap").css("min-height", $(".gadget_wrap").height()).empty();
	/* reset the scroll position */
	$(".gadget_wrap").scrollLeft(0);
	hot_products[id].map((data, index) => {
		$(".gadget_wrap").append('<a href="' + data.url + '"><img src="https://cdn09.dcfever.com' + data.image + '"><span class="name">' + data.model_no + '</span></a>').show();
	});
});
</script>
