<script>
$(document).ready(function() {
	$(".nav_level_one_user").click(function(e) {
		e.preventDefault();
		e.stopPropagation();
		$('.nav_level_one_user_menu').toggleClass("active");
		$('.nav_level_one_user_menu').fadeToggle(100, function() {
			if ( $.isScrollToFixed('.main_nav_level_two_wrapper')) {
				$('.main_nav_level_two_wrapper').trigger('detach.ScrollToFixed')
			} else {
				$(".main_nav_level_two_wrapper").scrollToFixed({
					preFixed: function() { $(this).css("paddingTop", "8px"); },
			        postFixed: function() { $(this).css("paddingTop", "0px"); }
				});
			}
		});
	});
	$(document).click(function() {
		if ($('.nav_level_one_user_menu').hasClass("active"))
		{
			$(".nav_level_one_user").click();
		}
	});
});
</script>		<div class="row">
