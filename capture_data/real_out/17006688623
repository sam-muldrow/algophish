<script>
    function getCookie(name) {
        var v = document.cookie.match('(^|;) ?' + name + '=([^;]*)(;|$)');
        return v ? v[2] : null;
    }
    function locationHashChanged() {
        var currentTab = $('.nav-tabs .active a').data('type');
        // If the hash does not contain the currentTab, change the active tab to the hash
        if (window.location.hash.indexOf(currentTab) === -1) {
            $('[href="'+ window.location.hash +'"]').click();
        }
    }
      
    $(document).ready(function() {
        const subState = getCookie('userState') || 'Anonymous';
        // Set a freeStates var for non-users
        const freeStates = [
            'Anonymous'
        ];
        var myAnimeNavList = document.querySelector('.my-anime-list');
        if (myAnimeNavList && freeStates.indexOf(subState) === -1) {
            // Show element for subscribers
            $('.my-anime-list').removeClass('hidden');
            var isAccountsPage = window.location.href.indexOf('/account/');
            if(isAccountsPage && window.location.hash) {
                window.onhashchange = locationHashChanged;
            }
        } else {
            // Hide element for non-subscribers
            $('.my-anime-list').addClass('hidden');
        }
    });
</script>
