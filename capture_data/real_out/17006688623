<script>

		jQuery(function() {
			var timeout = null;
			/* Get the data for predictive search/autocomplete from Tivo */
			var getData = function (request, response) {
				clearTimeout(timeout);

				timeout = setTimeout(function () {
					if(request){
						if(basicAuth) {
							jQuery.ajax({
								xhrFields: {withCredentials: true},
								url: '/pred-auto-search/&q='+ request + '/',
								type: 'GET',
								dataType: 'json',
								beforeSend: function(jqXHR) {
									jqXHR.setRequestHeader('Territory', KANE.territory);
									jqXHR.setRequestHeader('Authorization', basicAuth);
									jqXHR.setRequestHeader('X-CSRFToken', KANE.getCookie('csrftoken'));
									return true;
								},
								success: function(dataa) {
									var titles = [];
									$.each(dataa, function (key, data) {
										var t = stripHtml(data.title);
										if(t != "")
										{
											if ($.inArray(t, titles) == -1) titles.push(t);
										}
									})
									response(titles);
								}
							});
						} else {
							jQuery.ajax({
								url: '/pred-auto-search/&q='+ request + '/',
								type: 'GET',
								dataType: 'json',
								success: function(dataa) {
									var titles = [];
									$.each(dataa, function (key, data) {
										var t = stripHtml(data.title);
										if(t != "")
										{
											if ($.inArray(t, titles) == -1) titles.push(t);
										}
									})
									response(titles);
								}
							});
						}
					}
				}, 200);
			};
            $("#search-bar-input").keydown(function(event){
                if(event.keyCode == 13) {
                	console.log("input keydown");
                    var search_term = $("#search-bar-input").val()
                    if(search_term.length > 2) {
                        event.preventDefault();
                        $("#headerSearch").submit()
                        $("#search-bar-input").val() = search_term
                    }
                }
            });
            /* Universal search bar autocomplete */
			/*$("#search-bar-input").autocomplete({
				//dropdownStyle:{width:'0px', height:'-1px'},
				source: [getData],
				type:'remote',
				minLength: 2,
				limit:6,
				appendMethod:'replace',
			});*/

			/* Tivo search results page videos tab shows auto complete */
			$("#search_shows_auto").autocomplete({
				dropdownStyle:{overflow:'auto', height:'150px'},
				source: [getData],
				minLength: 2,
				delay: 15000,
				limit:6,
				appendMethod:'replace',

			}).on('selected.xdsoft',function(e,val){

			 	var updated_url = updateUrlParameter(window.location.href,"q",val);
				str = updated_url.replace(/&\s*$/, "");
				str1 = str.replace(/[&]+/g, '&');
				window.location.href = str1;

			});

		});

		function stripHtml(html) {
		  var tmp = document.createElement("DIV");
		  tmp.innerHTML = html;
		  return tmp.textContent || tmp.innerText || "";
		}



		/* Add/Update query string parameters */
		function updateUrlParameter(uri, key, value) {
			// remove the hash part before operating on the uri
			var i = uri.indexOf('#');
			var hash = i === -1 ? ''  : uri.substr(i);
				 uri = i === -1 ? uri : uri.substr(0, i);
			var re = new RegExp("([?&])" + key + "=.*?(&|$)", "i");
			var separator = uri.indexOf('?') !== -1 ? "&" : "?";
			if (uri.match(re)) {

				if (value) {
					uri = uri.replace(re, '$1' + key + "=" + value + '$2');
				}else{
					uri = uri.replace(re, '$1' + '$2');
				}
			} else {
				uri = uri + separator + key + "=" + value;
			}
			return uri + hash;  // finally append the hash as well
		}


</script>


</aside>


