            sc.src = "https://cdn.smartclip-services.com/v1/Storage-a482323/smartclip-services/wrappers/sc__sync.js"
            sc.onload = function () {
                __sc__sync(["544ba24d-d72f-4e71-940c-23676a5ad5f4"]) // id de la zona
            }
            document.head.appendChild(sc)
