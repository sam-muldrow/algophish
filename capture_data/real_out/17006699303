<script>
// Fixing the mobile filter button
// Function to check if a specific class exists in a div with a given data-selector
function doesClassExist(dataSelector, className) {
  // Find the div element by its data-selector
  const divElement = document.querySelector(`[data-selector="${dataSelector}"]`);

  // Check if the element exists and if it contains the specified class
  if (divElement && divElement.classList.contains(className)) {
    return true; // The class exists in the div
  } else {
    return false; // The class does not exist in the div or the div doesn't exist
  }
}

// Function to check if a specific class exists and modify its style
function modifyClassStyles(className, newStyles) {
  // Find the elements with the specified class
  const elementsWithClass = document.querySelectorAll(`.${className}`);

  // Loop through the elements and apply the new styles
  elementsWithClass.forEach((element) => {
    Object.assign(element.style, newStyles);
  });
}

// Captures the dataSelector for the div where the styles should be changed
// Captures the className that needs to be checked
const dataSelector = "sticky-banner-footer";
const classNameToCheck = "hide-below";

// Scroll Y position where the event should be triggered
const scrollPositionToTriggerEvent = 100;

// Function to handle the scroll event
function handleScroll() {
  // Get the current vertical scroll position
  const scrollY = window.scrollY;

  // Check if the scroll position has reached the threshold point
  if (scrollY >= scrollPositionToTriggerEvent) {
    // The scroll position has reached the desired point
    // Trigger the event to change the styles in the desired class
    const classNameToModify = "search-filter--mobile-footer";
    const newStyles = {
      bottom: "107px",
      zIndex: "1"
    };

    modifyClassStyles(classNameToModify, newStyles);

  } else {
    const classNameToModify = "search-filter--mobile-footer";
    const newStyles = {
      bottom: "0px"
    };

    modifyClassStyles(classNameToModify, newStyles);
  }
}

// Scroll event listener to the check the window
document.addEventListener('scroll', handleScroll);
</script>

