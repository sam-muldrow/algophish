    if (!!ccpConf.gcmActive) {
        window.dataLayer = window.dataLayer || [];
        if (typeof gtag !== 'function') {
            function gtag(){dataLayer.push(arguments);}
        }
        var gtagDefaultConsent = {wait_for_update: parseInt(ccpConf.gcmWaitForUpdate) || 2000};
        if (ccpConf.gcmStorageList.length) {
            ccpConf.gcmStorageList.forEach(storage => {
                if (storage.indexOf(':') !== -1) {
                    gtagDefaultConsent[storage.split(':')[0]] = "denied"
