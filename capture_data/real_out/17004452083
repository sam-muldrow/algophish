    var cookieConsentValue = cookieConsentValue || new RegExp('(?:^|; )cookiesGloballyAccepted=([^;]*)').exec(document.cookie);
    window.ce = window.ce || { ice: {} };
    window.ce.ice = window.ce.ice || {};
