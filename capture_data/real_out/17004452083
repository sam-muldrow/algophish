                var c = '(Googlebot|Bingbot|Slurp|DuckDuckBot|BingPreview|Baiduspider|YandexBot|Exabot|facebookexternalhit|SearchmetricsBot|facebot|ia_archiver|Twitterbot|rogerbot|linkedinbot|embedly|quora link preview|showyoubot|outbrain|pinterest|slackbot|vkShare|W3C_Validator|Msnbot|Sistrix crawler|AhrefsBot|Seznambot|SemrushBot|AdsBot-Google|conrad-prerenderer|Google-Structured-Data-Testing-Tool|Blog-Conrad-AT-Plugin|WhatsApp|LinkChecker|Discord|Viber|Google-InspectionTool|AhrefsSiteAudit)';
                var a = new RegExp(c, 'i');
                var b = navigator.userAgent;
                return a.test(b);
            };
