<script type="text/javascript" language="javascript">
                        jQuery(document).ready(function() {
                            var frameWidth = 640;
                            var frameHeight = 360;
                            jQuery(".videolink").fancybox({
                                helpers: {
                                    overlay: {
                                        css: {
                                            'background': 'rgba(21, 25, 29, 0.85)'
                                        }
                                    }
                                },
                                type: "html",
                                margin: 20,
                                scrolling: 'no',
                                minWidth: frameWidth,
                                minHeight: frameHeight,
                                padding: 5,
                                beforeLoad: function() {
                                    _videoHref = this.element.data("videourl");
                                    _videoPoster = typeof this.element.data("poster") !== "undefined" ? this.element.data("poster") : "";
                                    _videoWidth = typeof this.element.data("width") !== "undefined" ? this.element.data("width") : frameWidth;
                                    _videoHeight = typeof this.element.data("height") !== "undefined" ? this.element.data("height") : frameHeight;
                                    _dataCaption = typeof this.element.data("caption") !== "undefined" ? this.element.data("caption") : "";

                                    this.title = _dataCaption ? _dataCaption : (this.title ? this.title : "");
                                    this.content = "<video id='video_player' src='" + _videoHref + "'  poster='" + _videoPoster + "' width='" + _videoWidth + "' height='" + _videoHeight + "'  controls='controls' preload='none' ></video>";
                                },
                                beforeShow: function() {
                                    var $videoplayer = new MediaElementPlayer('#video_player', {
                                        videoWidth: _videoWidth,
                                        videoHeight: _videoHeight
                                    });
                                }
                            });
                        });
                    </script>
