			function CerezKabulEt(a,r) {
				const d = new Date();
                d.setTime(d.getTime() + (400 * 24 * 60 * 60 * 1000));

				const cdomain = window.location.host.indexOf(".morpakampus.com") > -1 ? ".morpakampus.com" : "";

				jQuery.cookieClearAll({domain:cdomain});			
				
                jQuery.cookie("ctercih", a + "-" + r, {
                     expires: d
                    , domain: cdomain
                });

                jQuery('#footer_kvkk').addClass('gizle');
			}

			jQuery(document).ready(function(){
				
				
                if (!jQuery.cookie('ctercih') || jQuery.cookie('ctercih') == '') {
                    if (window.location.href.indexOf("s=cevapgir") == -1 && window.location.href.indexOf("page=minisinav") == -1 && window.location.href.indexOf("page=cereztercihleri") == -1){
					    jQuery('#footer_kvkk').removeClass('gizle');
					}
				}


				CreateTooltip('[title]');
				if(isMobile2()) {
					jQuery('body').attr('id','v2_mobile');
					jQuery('.sitetasiyici').removeAttr('id');
					jQuery('.menu-icon').click(function() {
						jQuery('.side_menu_content, .menu-overlay').fadeToggle(350);
						jQuery('body').toggleClass('oFlow');
						if(jQuery('.side_menu_content').is(':visible')) {
							jQuery('.side_menu_content ul li a').width(jQuery('.side_menu_content').width());
							jQuery('.side_menu_content').height(jQuery(window).height()-50);
						}
					});
					jQuery('.menu-overlay').click(function(e){
						jQuery('.side_menu_content, .menu-overlay').fadeToggle(350);
						jQuery('body').toggleClass('oFlow');
					});
				}				
				//jQuery('.sitetasiyici').show();
				jQuery('video,audio').mediaelementplayer({
					loop: false,
					videoWidth: '100%',
					videoHeight: '100%',
					enableAutosize: true,
					features: ['playpause','progress','current','duration','tracks','volume','fullscreen'],
					alwaysShowControls: false
				});
				
				if(!isMobile2()) {
					setTimeout(function(){
						jQuery('.flybanner').fadeIn();
						if(jQuery.cookie('flybanner_durum') != 'kapali') {
							jQuery('.flybanner').animate({left:'+=200'},500);
						} else {
							jQuery('.flybanner').css('left','0');
							jQuery('.flybanner').fadeIn();
						}
					},4000);
				}
				
				jQuery('.flybanner-slide').click(function(){
					
					if(jQuery('.flybanner').hasClass('kapali')) {
						jQuery('.flybanner a, .flybanner-slide').animate({
							left:'+=200'
						},500);
						jQuery('.flybanner').removeClass('kapali');
						
						jQuery.cookie('flybanner_durum','');
					} else {
						jQuery('.flybanner a, .flybanner-slide').animate({
							left:'-=200'
						},500);
						jQuery('.flybanner').addClass('kapali');
						jQuery.cookie('flybanner_durum','kapali',{expires:1});
					}
					
				});
				if(jQuery.cookie('flybanner_durum') == 'kapali') {
					jQuery('.flybanner').addClass('kapali');
				}				


			});

	        function goTop() {
	            jQuery('html, body').stop().animate({ scrollTop: 0 }, 1000);
	        }
	        
	        function IsChrome() {
	            return /chrom(e|ium)/.test(navigator.userAgent.toLowerCase());
	        }

	        function IsIe8() {
	            return jQuery.browser.version.substring(0, 2) == "8.";
	        }

	        function IsIe9() {
	            return jQuery.browser.version.substring(0, 2) == "9.";
	        }

	        function IsIe10() {
	            return jQuery.browser.version.substring(0, 3) == "10.";
	        }
	        
	        function IsFf() {
	            if (jQuery.browser.mozilla)
	                return true;
	            return false;
	        }
			
			function isMobile() {
				var ua = navigator.userAgent.toLowerCase();
				var im = (ua.indexOf("mobile") > -1) || (ua.indexOf("android") > -1) || (ua.indexOf("iphone") > -1) || (ua.indexOf("ipad") > -1); //|| (ua.indexOf("macintosh") > -1); //&& ua.indexOf("android");
				var isAndroid = (ua.indexOf("android") > -1);
				return im;
            }

            function isMobile2() {
			    
			    return isMobile();
			}
	        
	        function getPositionTop(target) {
	            /*if (IsChrome() || IsIe8()) {
	                return target.position().top;
	            }*/
	            if(target.position())
	                return target.position().top;
	            return 0;
	        }
	        
	        jQuery(window).scroll(function() {
	            var scrollTop = 0;
	            if (IsIe8()) {
	                scrollTop = jQuery(document).context.documentElement.scrollTop;
	            } else {
	                scrollTop = jQuery(document).scrollTop();
	            }
	            if (scrollTop >= 100) {
	                jQuery("#scrolltop").show(400);
	            }
	            else {
	                jQuery("#scrolltop").hide(400);
	            }
				
				
				if(!isMobile2()){
					if (scrollTop >= 50) {
						jQuery(".header_menu").hide();
						jQuery("#desktopheader #morpalogo").addClass('scrolling');
						jQuery(".scrollmenu").show();
					}
					else {
						jQuery(".header_menu").show();
						jQuery(".scrollmenu").hide();
						jQuery("#desktopheader #morpalogo").removeClass('scrolling');
					}
				}
				
	        });
	        
		    function owAKTIVASYON() {
			    window.location.href = 'https://aktivasyon.morpakampus.com/uyelik.asp';
		    }

		    function owAKTIVASYONOdemeTip() {
		        window.location.href = 'https://aktivasyon.morpakampus.com/uyelik.asp?page=ogrenci_odemetip';
		    }

		    function owHATIRLATMA(tb) {
				window.location.href = '/?page=hatirlatma&tb=' + (tb ? tb : '0');
		    }
			
			function owDENEMATIK() {
				window.open('http://www.denematik.com','_blank');
		    }
			
			
			function CreateTooltip(selector) {
				jQuery(selector).qtip({
					style: {
						background: '#414243'
						, color: 'white'
						, textAlign: 'center'
						, fontWeight: 'bold'
						, border: {
							width: 2,
							radius: 5,
							color: '#414243'
						}
						, tip: { corner: 'bottomMiddle', color: '#414243', size: { x: 20, y: 10} }
						, name: 'dark' // Inherit the rest of the attributes from the preset dark style
					}
					, position: {
						corner: {
							target: 'topMiddle'
							, tooltip: 'bottomMiddle'
						}
					}
				});
			}
