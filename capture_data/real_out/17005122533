<script type="text/javascript" language="javascript">
			    var bnID = 0;
			    var BannerSayisi = jQuery("#anasayfabanner ul li").length;
			    var TimeOutObj;
			    for (var i = 0; i < BannerSayisi; i++) {
			        var bannerH = "<a href='javascript:void(0);' bid='" + i + "' onclick='BannerGO(" + i + ")'>&nbsp;</a>";
			        jQuery("#anabannerpager").append(bannerH);
			    }
			    jQuery("#anabannerpager a:eq(0)").addClass("secili");
			    
			    function BannerNext() {
			        jQuery("#anasayfabanner ul li").hide();
			        var ulSty = jQuery("#anasayfabanner ul li:eq(" + bnID + ")").attr("ulstyle");
			        jQuery("#anasayfabanner ul").attr("style", ulSty);
			        jQuery("#anasayfabanner ul li:eq(" + bnID + ")").show();

			        jQuery("#anabannerpager a").removeClass("secili");
			        jQuery("#anabannerpager a:eq(" + bnID + ")").addClass("secili");

			        bnID = bnID + 1;
			        if (bnID === BannerSayisi) bnID = 0;
			        TimeOutObj = setTimeout(BannerNext, 10000);
			    }

			    function BannerGO(bID) {
			        clearTimeout(TimeOutObj);
			        bnID = bID;
			        BannerNext();
			    }
			    
			    BannerNext();
			</script>
