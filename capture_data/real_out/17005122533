<script>
			function open_detay_incele_popup(paket, sinif) {
				jQuery("#paket_incele_pop_img").attr("src",isrc = "/i/u/ekpaket-urun-" + paket + "-pop-" + sinif + ".png")
				jQuery("#full_paket_incele_popup").show();
			}
			jQuery("#full_paket_incele_popup").click(function (event) {
				if (event.target.id === "full_paket_incele_popup") {
					jQuery("#full_paket_incele_popup").hide();
				}
			});
			jQuery("#canliders_paket_incele_popup").click(function (event) {
				if (event.target.id === "canliders_paket_incele_popup") {
					jQuery("#canliders_paket_incele_popup").hide();
				}
			});
			jQuery(".incele-close").click(function(){
				jQuery("#full_paket_incele_popup, #canliders_paket_incele_popup").hide();
			});
		
		</script>
