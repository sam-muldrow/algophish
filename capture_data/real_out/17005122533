<script type="text/javascript">

	function KKod_GizleGoster(){
		var c = document.getElementById('kkod_gizle');
		if(document.getElementById('username').type == 'password'){
			document.getElementById("kkod_gorunum").className = "kkod_goster";
			document.getElementById('username').type = 'text';
		} else {
			document.getElementById("kkod_gorunum").className = "kkod_gizle";
			document.getElementById('username').type = 'password';
		}
	}
    
    function submit_login() {
        var btn = document.getElementById('girisbtn');
        if (btn.style.display == 'none') return false;

        var usr = document.getElementById('username');
        var pwd = document.getElementById('password');

        if (usr.value == '') { return false; }
        if (pwd.value == '') { return false; }

        btn.style.display = 'none';
        return true;
    }
	var $video_player, _videoHref, _videoPoster, _videoWidth, _videoHeight, _dataCaption, _player, _isPlaying = false

    jQuery(document).ready(function() {

		var frameWidth = 640;
		var frameHeight = 360;
		if(isMobile()) {
			var frameWidth = (jQuery(window).width()-50);
			var frameHeight = (360/640)*(jQuery(window).width()-50);
		}

		jQuery("a[data-file]").fancybox({
			type:"html",
			margin:20,
			minWidth: frameWidth,
			minHeight: frameHeight,
			padding:5,
			beforeLoad : function () {
				_videoHref   = this.element.data("file");
				_videoPoster = typeof this.element.data("poster")  !== "undefined" ? this.element.data("poster")  :  "";
				_videoWidth  = typeof this.element.data("width")   !== "undefined" ? this.element.data("width")   : frameWidth;
				_videoHeight = typeof this.element.data("height")  !== "undefined" ? this.element.data("height")  : frameHeight;
				_dataCaption = typeof this.element.data("caption") !== "undefined" ? this.element.data("caption") :  "";
				
				this.title = _dataCaption ? _dataCaption : (this.title ? this.title : "");
				this.content = "<video id='video_player' src='https://morpa.akamaized.net/mobil/i/" + _videoHref + "'  poster='https://morpa.akamaized.net/mobil/i/" + _videoPoster + "' width='" + _videoWidth + "' height='" + _videoHeight + "'  controls='controls' preload='none' ></video>";
				/*this.width = _videoWidth;
				this.height = _videoHeight;*/
			},
			beforeShow : function () {
				var $videoplayer = new MediaElementPlayer('#video_player', {
					videoWidth : _videoWidth,
					videoHeight : _videoHeight,
					success: function(mediaElement, domObject){
						if(jQuery('#video_player[src*=mrp_tnt]').length > 0) {
							var playerID = jQuery('#video_player[src*=mrp_tnt]').parents('.mejs-container').attr('id');
							mediaElement.addEventListener('ended', function(e) {
								jQuery('#'+playerID).append('<div class="player-btns"><a class="uyeol" href="javascript:void(0);" onclick="owAKTIVASYONOdemeTip();">&nbsp;</a><a class="kesfet" href="/1-sinif">&nbsp;</a></div><div class="player-btns-overlay"></div>');
							}, false);
							mediaElement.addEventListener('play', function(e) {
								jQuery('.player-btns').remove();
							}, false);
						}
					}
				});
			}
		});
		

    });
</script>
