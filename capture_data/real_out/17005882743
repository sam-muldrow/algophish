        /*!
        loadCSS: load a CSS file asynchronously.
        [c]2014 @scottjehl, Filament Group, Inc.
        Licensed MIT
        */
        function loadCSS(href, before, id, media, delay) {
           "use strict";
           delay = delay ? delay : 0;
           var ss = window.document.createElement("link");
           var ref = before || window.document.getElementsByTagName("script")[0];
           var sheets = window.document.styleSheets;
           ss.rel = "stylesheet";
           ss.href = href;
           ss.media = "only x";
           ss.id = id;

           function ready(cb) {
               if (document.readyState.toLowerCase() === 'complete') {
                   return cb();
               }
               setTimeout(function() {
                   ready(cb);
               })
           }
           ready(function() {
               ref.parentNode.insertBefore(ss, ref);
           });

           var onloadcssdefined = function(cb) {
               var resolvedHref = ss.href;
               var i = sheets.length;
               while (i--) {
                   if (sheets[i].href === resolvedHref) {
                       return cb();
                   }
               }
               setTimeout(function() {
                   onloadcssdefined(cb);
               })
           };

           function loadCB() {
               if (ss.addEventListener) {
                   ss.removeEventListener("load", loadCB);
               }
               ss.media = "all";
           }

           if (ss.addEventListener) {
               ss.addEventListener("load", loadCB, {"passive":true});
           }else{
                onloadcssdefined(loadCB);
           }
           return ss;

        }

        function loadJS(){
            function lazyScript(href) {
                    var app = document.createElement('script');
                    app.type = 'text/javascript';
                    app.async = true;
                    app.src = href;
                    var x = document.getElementsByTagName('script')[0];
                    x.parentNode.insertBefore(app, x);
            }
            window.removeEventListener("load",loadJS);

            setTimeout(function(){
                lazyScript('https://assets-1.housingcdn.com/website/javascripts/uni-user.js');
            }, 4000);
            //lazyScript('//static.makaan.com//static.makaan.com//static.makaan.com/scripts/app.js'); //LAZYSCRIPT
            //lazyScript('//static.makaan.com//static.makaan.com//static.makaan.com/scripts/dependency/commonBundle.js'); //LAZYSCRIPT
            //lazyScript('//static.makaan.com//static.makaan.com//static.makaan.com/scripts/common/pageLoadTracking.js'); //LAZYSCRIPT
        }

        window.addEventListener("load", loadJS,{"passive":true});
   </script><noscript><img src="//static.makaan.com/images/dummyPX.e679fbd4.png" alt="Google Analtics Tracking Pixel"/><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-WQK44RJ" height="0" width="0" style="display:none;visibility:hidden"></iframe> </noscript><div id="pageBottomCss"><script> loadCSS("//static.makaan.com/styles/css/home-lazyload.c0d2c084.css"); </script></div></body></html>