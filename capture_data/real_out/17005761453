    <script>
        var california;
        var snackBarIsGone;
        
       

        function disableScroll() {
            // Get the current page scroll position 
            scrollTop = window.pageYOffset || document.documentElement.scrollTop;
            scrollLeft = window.pageXOffset || document.documentElement.scrollLeft,

                // if any scroll is attempted, set this to the previous value 
                window.onscroll = function () {
                    window.scrollTo(scrollLeft, scrollTop);
                };
        }

        function enableScroll() {
            window.onscroll = function () { };
        } 

         function showSnackbar() {
            var x = document.getElementById("snackbar");
            x.className = "show";

            var y = document.getElementById("loginarrowwrapper");
            if(y) y.style.bottom = "50px";


            window.localStorage.setItem("snackbarClosedDate", "1970/01/01 00:00 AM");
            
        }
        function closeSnackbar() {
            var x = document.getElementById("snackbar");
            setTimeout(function(){ x.className = x.className.replace("show", "hide"); }, 0);
            var y = document.getElementById("loginarrowwrapper");
            if(y) y.stylebottom = "0px";

            var nowStr = new Date().toString();
            window.localStorage.setItem("snackbarClosedDate", nowStr);
            document.getElementById('extrafooter').remove();

            if (california == true) {
                document.getElementById("caoptoutlink").style.display = "inline";
            }
            

        }

        var snackbarUpdateDate = new Date(Date.parse("2018/05/24 00:00 AM"));
        var snackbarClosedDateStr = window.localStorage.getItem("snackbarClosedDate");        
        var snackbarClosedDate = new Date(Date.parse("1970/01/01 00:00 AM"));
        if(snackbarClosedDateStr) {
            try { 
                snackbarClosedDate = new Date(Date.parse(snackbarClosedDateStr));
            } catch(e) {
            }
        }        
        if (snackbarClosedDate == null || snackbarClosedDate < snackbarUpdateDate) {
            showSnackbar();
        }
        else {
            snackBarIsGone = true;
        }
       


          var onGeoSuccess = function(location) { 
        if(location.countryCode == "US" && location.region == "California") {
            california = true;
            onCalifornia();
        }
      } 
      var onGeoError = function(error) {         
        console.log("GeoIP error - Geo not detected");
      }
      var onCalifornia = function() { 
        document.getElementById("cookieinfo").style.display = "none";
        document.getElementById("cacookieinfo").style.display = "inline";
          if ($('#snackbar').hasClass("hide") || snackBarIsGone) {
              document.getElementById("caoptoutlink").style.display = "inline";
          }
      }   



        var teamSelect = 0;
        var teamScroller;

        function getUrlVars() {
            var vars = {};
            var parts = window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function (m, key, value) {
                vars[key] = value;
            });
            return vars;
        }
        function getUrlParam(parameter, defaultvalue){
    var urlparameter = defaultvalue;
    if(window.location.href.indexOf(parameter) > -1){
        urlparameter = getUrlVars()[parameter];
        }
    return urlparameter;
}


        function elementInViewport(el) {
            var top = el.offsetTop;
            var left = el.offsetLeft;
            var width = el.offsetWidth;
            var height = el.offsetHeight;

            while (el.offsetParent) {
                el = el.offsetParent;
                top += el.offsetTop;
                left += el.offsetLeft;
            }

            return (
                top >= window.pageYOffset &&
                left >= window.pageXOffset &&
                (top + height) <= (window.pageYOffset + window.innerHeight) &&
                (left + width) <= (window.pageXOffset + window.innerWidth)
            );
        }

        $(document).ready(function () {         

            $('#cheeseburgerCollapse').on('show.bs.collapse', function () {
                $('#cover')[0].style.display = "block";
                if ($('#elHeader')[0].style.display != "none") {
                    $('#elHeader')[0].style.display = "none";
                    $('#ghostBurger')[0].style.display = "block";
                }
               
                disableScroll();
            });

            $('#cheeseburgerCollapse').on('hidden.bs.collapse', function () {
                $('#cover')[0].style.display = "none";
                $('#ghostBurger')[0].style.display = "none";
                if (!elementInViewport(document.getElementById("login-header"))) {
                    header.style.display = "block";
                }
                enableScroll();
            });

            
            var error = getUrlParam('error', 'Empty');
            var loginError = getUrlParam('loginerror', 'Empty');
            var nav = getUrlParam('nav', 'Empty');
            if (loginError != 'Empty') {
                loginError = decodeURI(loginError);
                document.getElementById("loginError").innerHTML = loginError;
                $('#login-modal').modal('show');

            }
            else if (nav != 'Empty')
            {
                nav = decodeURI(nav);
                if (nav == "aboutus") {

                    $('#about-us-modal').modal('show');
                }
                else if (nav == "privacypolicy") {

                    $('#privacy-modal').modal('show');
                }
                else if (nav == "login") {

                    $('#login-modal').modal('show');
                }
                else if (nav == "advertisersignup") {
                    var signupform = document.getElementById('signupform');
                    signupform.action = '/newuser.aspx?nav=advertisers';
                    if (error != 'Empty') {
                        error = decodeURI(error);
                        document.getElementById("errorText").innerHTML = error;
                    }
                    $('#signup-modal').modal('show');
                }
                else if (nav == "publishersignup") {
                    var signupform = document.getElementById('signupform');
                    signupform.action = '/newuser.aspx?nav=publishers';
                    if (error != 'Empty') {
                        error = decodeURI(error);
                        document.getElementById("errorText").innerHTML = error;
                    }
                    $('#signup-modal').modal('show');
                }

            }
            
        
           
           
            var header = document.createElement("div");
            header.style.width = "100%";
            header.className = "detachable-header"

            header.style.backgroundColor = "white";
            header.innerHTML = "<img class='header-logo' src='/images/newsite/bottom-logo.png'><div class='header-login'><a  data-toggle='modal' href='#login-modal' style = 'color: black; text-align:left; margin-right:14px;'>   LOGIN</a><a  data-toggle='collapse' data-target='#cheeseburgerCollapse' aria-expanded='false' aria-controls='cheeseburgerCollapse'><img class='hamburger' src='/images/newsite/hamburger2.png' /></a></div>";
            header.style.display = "none";
            header.style.position = "fixed";
            header.style.top = "0px";
            header.id = "elHeader";
            header.style.borderBottomColor = "#7b7b7b";
            header.style.borderBottomWidth = "2px";
            header.style.borderStyle = "solid";
            header.style.borderTopWidth = "0px";
            header.style.borderLeftWidth = "0px";
            header.style.borderRightWidth = "0px";
            document.body.appendChild(header);

            var ghostBurger = document.createElement("div");
            ghostBurger.id = "ghostBurger";
            ghostBurger.style.display = "none";
            ghostBurger.style.position = "fixed";
            ghostBurger.innerHTML = '<a data-toggle="collapse" data-target="#cheeseburgerCollapse" aria-expanded="false" aria-controls="cheeseburgerCollapse"><img style="width:25px;height:20px" src="/images/newsite/hamburger.png"></a>';
            ghostBurger.style.zIndex = 10001;
            ghostBurger.style.top = "18px";
            ghostBurger.style.right = "13px";
            document.body.appendChild(ghostBurger);


            $('#team-modal').on('show.bs.modal', function () {
                teamSelect = 0;
                setTeamScroll();
            });

            $('#team-modal').on('hide.bs.modal', function () {
                turnOffTeamScroll();
            });

             $('#login-modal').on('hidden.bs.modal', function () {
                document.getElementById('loginError').innerHTML = "";
            });
           

            for (i = 0; i < 7; i++) {
                var currentPic = document.getElementsByClassName("circle-pic-team")[i];
                currentPic.style.filter = "gray";
                currentPic.style.filter = "grayscale(100%)";
                 $(currentPic).bind('mouseover', function () {
                     turnOffTeamScroll();
                     this.style.filter = "none";
                 });
                $(currentPic).bind('mouseleave', function () {
                     
                     setTeamScroll();
                 });

            }

            $(window).scroll(
                function () {
                    if (!elementInViewport(document.getElementById("login-header"))) {
                        if ($('#cover')[0].style.display != "block") {
                            header.style.display = "block";
                        }
                        
                    }
                    else {
                        header.style.display = "none";
                    }

                }
            );
        }

        );

        

        //impressions counter
        setInterval(function () {
            document.getElementsByClassName('count')[0].style.color = "rbga(0, 0, 0, 0)";
            document.getElementsByClassName('count')[0].innerHTML = 24000000000;
            $('.count').each(function () {
                $(this).prop('Counter', 0).animate({
                    Counter: $(this).text()
                }, {
                        duration: 2000,
                        easing: 'swing',
                        step: function (now) {

                            $(this).text(Math.ceil(now).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ","));
                            document.getElementsByClassName('count')[0].style.color = "#32C5FF";
                        }
                    });
            })

        }, 4000);


        function setTeamScroll() {
            //team scroller


            for (i = 0; i < 7; i++) {
                var currentPic = document.getElementsByClassName("circle-pic-team")[i];
                currentPic.style.filter = "gray";
                currentPic.style.filter = "grayscale(100%)";
            }
         

               //tried putting this in a function since it is the same code inside of the setInterval() below...
            //for some reason it doesn't behave the same way
               var lastCircle;
              
                if (teamSelect != 0) {
                    lastCircle = document.getElementsByClassName("circle-pic-team")[teamSelect - 1];
                    lastCircle.style.filter = "gray";
                    lastCircle.style.filter = "grayscale(100%)";
                }
                else {
                    lastCircle = document.getElementsByClassName("circle-pic-team")[7];
                    lastCircle.style.filter = "gray";
                    lastCircle.style.filter = "grayscale(100%)";
               }
                if (window.innerWidth < 576 && teamSelect == 5) {
                   teamSelect++;
               }
               else if (window.innerWidth > 576 && teamSelect == 7) {
                   teamSelect = 0;
               }
                var currentCircle = document.getElementsByClassName("circle-pic-team")[teamSelect];
                currentCircle.style.filter = "none";
               if (teamSelect == 7) {
                   teamSelect = 0;
               }
               else {
                   teamSelect++;
               }
               
       

           teamScroller = setInterval( function () {               var lastCircle;
              
                if (teamSelect != 0) {
                    lastCircle = document.getElementsByClassName("circle-pic-team")[teamSelect - 1];
                    lastCircle.style.filter = "gray";
                    lastCircle.style.filter = "grayscale(100%)";
                }
                else {
                    lastCircle = document.getElementsByClassName("circle-pic-team")[7];
                    lastCircle.style.filter = "gray";
                    lastCircle.style.filter = "grayscale(100%)";
               }
                if (window.innerWidth < 576 && teamSelect == 5) {
                   teamSelect++;
               }
               else if (window.innerWidth > 576 && teamSelect == 7) {
                   teamSelect = 0;
               }
                var currentCircle = document.getElementsByClassName("circle-pic-team")[teamSelect];
                currentCircle.style.filter = "none";
               if (teamSelect == 7) {
                   teamSelect = 0;
               }
               else {
                   teamSelect++;
               }
               
             console.log("setInterval function completed");
                

            } , 3000);
        }

        function turnOffTeamScroll(){
            clearInterval(teamScroller);
           
            for (i = 0; i < 7; i++) {
                var currentPic = document.getElementsByClassName("circle-pic-team")[i];
                currentPic.style.filter = "gray";
                currentPic.style.filter = "grayscale(100%)";
                //$(currentPic).bind('mouseover', function () {
                //    currentPic.style.filter = "none";
                //});
            }

        }

    </script>
