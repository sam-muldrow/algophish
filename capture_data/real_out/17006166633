			function setInfoItem() {
				var win = jQuery(window).width();
				if (win >= 1200) {
					var info_item_height = jQuery('#sj_image_slideshow_9965657741700616664 .info-item').height();
					jQuery('#sj_image_slideshow_9965657741700616664 .info-item').css('margin-top', (392 - info_item_height)/2);
				}
				else {
					jQuery('#sj_image_slideshow_9965657741700616664 .info-item').css('margin-top', '0px');
				}
			}
			setInfoItem();

			jQuery(window).on('resize', function() {
				setInfoItem();
			})
		})
	</script>
