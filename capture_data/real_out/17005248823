            (function () {
                var pmd = document.querySelector(".pmd-news");
                var scrolling = true;
                var pmdLeft = 0;
                pmd.innerHTML = pmd.innerHTML + pmd.innerHTML;
                var pmdList = document.querySelectorAll(".pmd-news li");
                var pmdFirstLeft = pmdList[0].offsetLeft;
                var pmdTimer = setInterval(() => {
                    if (scrolling) {
                        pmdLeft--;
                        if (pmdList[pmdList.length / 2].offsetLeft <= pmdFirstLeft) pmdLeft = pmdList[pmdList.length / 2].offsetLeft;
                        pmd.style.marginLeft = pmdLeft + "px";
                    }
                }, 40);
                pmd.addEventListener('mouseover',function(){
                    scrolling = false;
                })
                pmd.addEventListener('mouseout',function(){
                    scrolling = true;
                })
            })();
