 afLogger.logError('GTM data layer missing', 'GTM');
 }
 if (!window.af_ua_trackEvent) {
 afLogger.logError('UA track event function missing', 'GTM');
 }
 }

 af_addEvent(window, 'load', checkTagging, false);
 })();
 </script>
