<script type="text/javascript">
	window['tolstoycomments'] = window['tolstoycomments'] || [];
	window['tolstoycomments'].push({
		action: 'init',
		values: {
						url: 'https://mobile-review.com/all/reviews/smartphones/obzor-xiaomi-13t-optimus-prajs/',
			title: 'Обзор Xiaomi 13T – «оптимус прайс»',
			desktop_class: 'tolstoycomments-feed',
			comment_class: 'tolstoycomments-cc',
			comment_render: function (val) {
				this.innerHTML = '<i class="fa fa-comment"></i> ' + val + " комментари" + GetNumEnding(val, "й", "я", "ев");
			}
		}
	});
	var GetNumEnding = function (n, a, b, c) {
	    var v = parseInt(n % 100);
	    if (v >= 11 && v <= 19) {
	        return c;
	    } else {
	        v = parseInt(n % 10);
	        if (v == 1) {
	            return a;
	        } else {
	            if (v >= 2 && v <= 4) {
	                return b;
	            } else {
	                return c;
	            }
	        }
	    }
	}
</script>
