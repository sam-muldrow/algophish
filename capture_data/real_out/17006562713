			if($(".searchtourBox").length>0){
				if($(".searchtourBox.showTransparent").length>0){
					html = html.replace("<div class=\'searchtour\'>","<div class=\'searchtour showTransparent\'>");
					html = html.replace("<div class=\'searchtour-dropicon\'></div>","");
				}
				$(".searchtourBox").replaceWith(html);
				this.initEvent();
			}
		},
		initEvent:function(){
			var isTransparent=$(".searchtour.showTransparent").length>0;
			$('.searchtour-form-text').click(function(){
				if($('.searchtour-cityList').is(":visible")){
					$('.searchtour-cityList').hide();
				}else{
					$('.searchtour-cityList').show();
				}
			});

			$(document).bind('click', function(e) {				
				var e = e || window.event;
				var elem = e.target || e.srcElement;
				while(elem){
					if(elem.id && (elem.id=='searchtour-form-text' || elem.id=='searchtour-cityList')){
						if(!isTransparent){
							$('.searchtour-form-text').css('color','#000');
						}
						return;
					}
					elem = elem.parentNode;
				}
				$('.searchtour-cityList').hide();
				if(!isTransparent){
					if($('.searchtour-form-text').val() == '' || $('.searchtour-form-text').val() == 'I want to travel to...'){
						$('.searchtour-form-text').css('color','#ccc');
					}
				}
			});
			
			$(".countryList li input[name='country']").click(function(){				
				if($(this).is(':checked')){
					$(".countryList li input[name='country']").not($(this)).attr('checked', false);
					$('.searchtour-form-text').val($(this).val());
					
//					if($('.searchtour-form-text').val() != '' && $('.searchtour-form-text').val() != 'I want to travel to...' ){
//						$('.searchtour-form-text').val($('.searchtour-form-text').val() + ', ' + $(this).val());
//					}else{
//						$('.searchtour-form-text').val($(this).val());	
//					}
				}else{
//					if($('.searchtour-form-text').val().indexOf($(this).val()) == 0){
//						if($('.searchtour-form-text').val().indexOf($(this).val() + ',') == 0)
//							$('.searchtour-form-text').val($('.searchtour-form-text').val().replace($(this).val()+', ',''));	
//						else
//							$('.searchtour-form-text').val($('.searchtour-form-text').val().replace($(this).val(),''));	
//					}else{
//						$('.searchtour-form-text').val($('.searchtour-form-text').val().replace(', '+$(this).val(),''));
//					}
					
					$('.searchtour-form-text').val($('.searchtour-form-text').val().replace($(this).val(),''));	
					
					if($('.searchtour-form-text').val() == ''){
						$('.searchtour-form-text').val('I want to travel to...');				
					}
				}
			});

			$(".searchtour-form-btn").click(function(){
				var newUrl = '/tour/searchg.html?country=&page=1&ft=1#content';
				if($('.searchtour-form-text').val() != '' && $('.searchtour-form-text').val() != 'I want to travel to...' ){
					newUrl = '/tour/searchg.html?country='+ $('.searchtour-form-text').val().replace(new RegExp(',', 'g'),'|') +'&page=1&ft=1#content';	
				}
				window.open(newUrl, '_self');
				$(".countryList li input[name='country']").each(function() {
					$(this).attr('checked', false);
				});
				$('.searchtour-form-text').val('I want to travel to...');
			});

		},
