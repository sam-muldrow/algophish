<script type="text/javascript">   

	 function PageInit() {
		 //auto tour price.20200603.richard
		 $('.tourPrice').each(function(){
			 var _this=$(this);
			 var u=_this.attr('target');
			 u=$('#'+u).attr('href');
			//console.log(u);	
			$.post('/inc/qa/_api/ajaxHandler.ashx?cmd=home.price&url='+u,function(d){
				_this.data("currencynumsign",1).data("currencynum",d).html('USD'+d);
				if (!!window.Currency) {
					Currency.reshow();
				}
			});
		});
		//
	 
		
		 //bannerImg();
		 chgAd();
		 //showBrief();
		 searchBox.init();
		 
		(function(win, doc) {
			if (win.postMessage) {
				if (win.addEventListener) {
					win.addEventListener("message",
					function(e) {
						var data=e.data;
						if(data.type&&data.type=="slider-reviews-size"){
							$("#reviews-iframe").height(data.height);
						}
					},
					false);
				}
			}
		})(window, document);
	 }
	 	 
	var bannerImg = function () {
		
		var slide= [{"img":"china-great-wall.jpg","imgMobile":"china-great-wall-s.jpg","alt":"Great Wall, Beijing"},{"img":"terracotta-warriors.jpg","imgMobile":"terracotta-warriors-s.jpg","alt":"Terracotta Warriors, Xian"},{"img":"pandas-of-chengdu.jpg","imgMobile":"pandas-of-chengdu-s.jpg","alt":"Giant Pandas, Chengdu "}];  
		var preUrl="https://data.travelchinaguide.com/images/background/index/";
		var slideLi = "";
		$(slide).each(function (i) {
		var _this = $(this)[0];
		var _style = "position: absolute;z-index:" + (i == 0 ? "1" : "0");
		slideLi += '<li class="slide" style="' + _style + '"><img title="'+_this.alt+'" alt="'+_this.alt+'" src="' + ($(window).width() > 480 ? preUrl+_this.img : preUrl+_this.imgMobile) + '"/></li>';
		});
		
		$("#index-banner").prepend('<ul class="banner-lb">' + slideLi + '</ul>');
		
		//return;
		$('ul.banner-lb').bxSlider({
			mode: 'fade',
			auto: true,
			autoControls: true,
			speed: 1000,
			slideMargin: 0
		});
	};
	var showBrief= function(){
		var img='<img src="https://data.travelchinaguide.com/images/background/index/combo10.png" class="imgArrow" style="vertical-align: middle;">';
		var w=$(window).width();
		if(w<=480){
			
			$(".popular-top-content").each(function(i){
				var t=$(this).attr("t");
				var cutStr= t.substr(0,100);
				var idx=cutStr.lastIndexOf(" ");
				//alert(idx);
				var new_cutStr2=cutStr.substring(0,idx);
				$(this).html(new_cutStr2+" ... "+img);
				
				
				$(".imgArrow").live("click",function(){
					var imgO=$(this);
					var $div=imgO.parent(".popular-top-content");
					var colp=$div.attr("isColp");
					var brief=$div.attr("t");
					var cutStr= brief.substr(0,100);
					var idx=cutStr.lastIndexOf(" ");
					//alert(idx);
					var new_cutStr=cutStr.substring(0,idx);
					if(colp=="0"){
						$div.html(brief+" "+img);
						$div.attr("isColp","1");
						$div.find(".imgArrow").addClass("test-inverse");
					}else{
						$div.html(new_cutStr+" ... "+img);
						$div.attr("isColp","0");
						$div.find(".imgArrow").removeClass("test-inverse");
					}
					
				});
			
			});
			
			
		}
	};
	var chgAd=function(){
		var w=$(window).width();
		if(w<=610){
			$('#index-ad-2020').html('<img src="https://data.travelchinaguide.com/images/tcg/adv/adv-453-17y.jpg" title="12% off on 2021 China Tours" style="width:100%;">');
		}
	}; 	


