            <div id="paginate-ekslayt" class="ekslaytnoktalari">
                        <a href="javascript:void(0)" ref="0" class="sirano aktif"></a>
                        <a href="javascript:void(0)" ref="1" class="sirano"></a>
                        <a href="javascript:void(0)" ref="2" class="sirano"></a>
                        <a href="javascript:void(0)" ref="3" class="sirano reklam tooltip ust" data-title="İlandır"></a>
                        <a href="javascript:void(0)" ref="4" class="sirano"></a>
                        <a href="javascript:void(0)" ref="5" class="sirano"></a>
                        <a href="javascript:void(0)" ref="6" class="sirano"></a>
                        <a href="javascript:void(0)" ref="7" class="sirano"></a>
                        <a href="javascript:void(0)" ref="8" class="sirano"></a>
                        <a href="javascript:void(0)" ref="9" class="sirano"></a>
                        <a href="javascript:void(0)" ref="10" class="sirano"></a>
                        <a href="javascript:void(0)" ref="11" class="sirano"></a>
                        <a href="javascript:void(0)" ref="12" class="sirano"></a>

            </div>
            <script>
                var benimsiralamam = 1,
                    interval=6000,
                    count,
                    benimtimer,
                    hoversira;

                $(document).ready(function () {
                    count = $("#paginate-ekslayt a").length;
                    count = count;
                    timerkur();
                })

                function timerkur(ozelsira) {
                    if (ozelsira != "" && ozelsira != null && ozelsira != "undefined" || ozelsira == "0") {
                        benimtimer = setInterval(function () {
                            OtomatikDegis(ozelsira);
                        }, interval);
                        benimsiralamam = ozelsira;
                        ozelsira = null;
                        clearInterval(benimtimer);
                        timerkur();
                    }
                    else {
                        benimtimer = setInterval(function () {
                            OtomatikDegis(benimsiralamam);
                        }, interval);
                    }

                }

                function OtomatikDegis(sira) {
                    if (benimsiralamam >= count) {
                        benimsiralamam = 0;
                    }
                    else {
                        $(".ekslayt .ekoge").css("display", "none");
                        $(".ekslayt .ekoge[ref=" + sira + "]").css("display", "inline-block");
                        $(".ekslaytnoktalari .sirano").removeClass("aktif");
                        $(".ekslaytnoktalari .sirano[ref=" + sira + "]").addClass("aktif");
                        benimsiralamam = benimsiralamam + 1
                        SliderChanged($(".ekslayt .ekoge[ref=" + sira + "]"));
                    }
                }


                $(".sirano").mouseover(function () {
                    $(".ekslaytnoktalari .sirano").removeClass("aktif");
                    $(this).addClass("aktif");
                    var kacinci = $(".ekslaytnoktalari .sirano.aktif").index();
                    $(".ekslayt .ekoge").css("display", "none");
                    $(".ekslayt .ekoge[ref=" + kacinci + "]").css("display", "inline-block");
                    clearInterval(benimtimer);
                    hoversira = kacinci;
                    SliderChanged($(".ekslayt .ekoge[ref=" + kacinci + "]"));
                });
                $(".ekslaytkapsam *").mouseover(function () {
                    clearInterval(benimtimer);
                });
                $(".ekslaytkapsam").mouseout(function () {
                    hoversira = $(".ekslaytnoktalari .sirano.aktif").index();
                    timerkur(hoversira);
                });

                function SliderChanged(element) {
                    if (!IsSafari()) {
                        PlayVideoPreview(element);
                    }
                }

                function PlayVideoPreview(element) {
                    let videoId = $(element).attr("data-videoId");
                    if (videoId && videoId > 0) {
                        var el = $(element).find(".ekonizleme");
                        VideoPreview.StartVideoPreview(el, el, videoId, 3, 
                            //preview yuklendikten sonra
                            function () {
                                $(element).find(".sliderTitle").css("display", "none");
                                $(element).find(".previewTitle").css("display", "inherit");
                            });
                    }
                }

                function IsSafari() {
                    var is_chrome = navigator.userAgent.indexOf('Chrome') > -1;
                    var is_safari = navigator.userAgent.indexOf("Safari") > -1;
                    return is_safari && !is_chrome;
                }
            </script>
        </div>
    </div><!--slayt-alani-->

