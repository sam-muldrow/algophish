        <script type="text/javascript">
            $(document).ready(function () {
                $(window).scroll(function () {
                    if ($(this).scrollTop() > 300) {
                        $('#toplink').fadeIn();
                    } else {
                        $('#toplink').fadeOut();
                    }
                });
                $('#toplink').click(function () {
                    if (isTop) {
                        isTop = false;
                        $('html, body').animate({ scrollTop: 0 }, 800, function () {
                            isTop = true;
                        });

                    }
                });
            });
