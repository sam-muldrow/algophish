            if (!response.HasError) {
                    $.each(response.Data, function (index, data) {
                    var countHolder = $(".bakanlar .count[data-id=".concat(data.NewsId, "]"));
                    countHolder.text(data.Session.Total);
                    countHolder.parent().parent().parent().show()
                    })
                }
            else {
                console.error("live history failed");
            }
        }
        });
    </script>
    <script>
        function ArticleNewsLike(that, id) {
            var likeClass = 'pasif', totalLike = '.sayi';
            //class ismi yanlıs yazılmıs css tarafında. 
            let liked = $(that).hasClass(likeClass) === false;

            LikeMessage(id, liked, function (total) {
                if (total === 0) {
                    $(that).find(totalLike).text("Oy Ver")
                }
                else {
                    $(that).find(totalLike).text(total);
                }
            })
            if (liked)
                $(that).addClass(likeClass);
            else
                $(that).removeClass(likeClass);
        }

        function BlogNewsLike(that, id) {
            var likeClass = 'active', totalLike = '.gundem-yap-buton-info';

            $(that).toggleClass(likeClass);

            LikeMessage(id, $(that).hasClass(likeClass), function (total) {

                if (!$(that).hasClass(likeClass) || total === 0) {
                    $(that).find(totalLike).text("Oy Ver")
                }
                else {
                    $(that).find(totalLike).text(total);
                }
            })
        }
    </script>
    <script>
        new Blazy({
            offset: 500 // Loads images 500px before they're visible
        });
    </script>

