<script>
    $(".kripto-paralar .uzat").click(function () {
        $(this).text($(this).parent().toggleClass("acik").hasClass('acik') ? 'DAHA AZ GÖSTER' : 'DAHA FAZLA GÖSTER');
        resort();
    })

    function makeCorsRequestcrypto() {

        var tempHtml = "";
        $.getJSON("/api/prod/web/api/CryptoJson", function (data) {

            $.each(data.Data, function (index, value) {
                var digit = 2;
                if (('' + value.price_usd).split(".")[0].length == 5) {
                    digit = 4;
                }
                var cryptoUriName = value.name.toLowerCase()
                    .replace(/\s/g, '-')
                    .replace(/\./g, '-');

                switch (value.symbol.toLowerCase()) {
                    //case "btc":
                    //case "eth":
                    //case "xrp":
                    //case "waves":
                    //case "ada":
                    //case "usdt":
                    //case "link":
                    //case "xtz":
                    //case "chz":
                    //case "ont":
                    //case "xlm":
                    //case "ltc":
                    //case "atom":
                    //case "rvn":
                    //case "neo":
                    //case "trx":
                    //case "bat":
                    //case "eos":
                    //case "bch":
                    //case "hot":
                    //case "doge":
                    //case "btt":
                    //    tempHtml += "<a href=\"https://www.paribu.com/#/market/"
                    //        .concat(value.symbol.toLowerCase(), "-tl/?utm_source=Donanim_haber&utm_medium=cpc&utm_campaign=Ilk_Adim\" class=\"tek\" target=\"_blank\"><span class=\"ad\" title=\"");
                    //    break;
                    default:
                        tempHtml += "<a href=\"https://coinmarketcap.com/currencies/"
                        .concat(cryptoUriName, "/\" class=\"tek\" target=\"_blank\"><span class=\"ad\" title=\"");
                        break;
                }

                tempHtml += value.name + "\"><img loading=\"lazy\" src=\"https://www.donanimhaber.com/images/kripto/"
                    .concat(cryptoUriName, ".png\" alt=\"")
                    .concat(value.name, "\">")
                    .concat(value.symbol, "</span><span class=\"fiyat\">")
                    .concat(value.price_usd.toLocaleString("en-US", { maximumSignificantDigits: digit }).replace(',', '.'), "$</span><span class=\"degisim ")
                    .concat(parseFloat(value.percent_change_24h) < 0.0 ? 'negatif' : 'pozitif', "\">")
                    .concat(value.percent_change_24h.toLocaleString("en-US"), "%</span><span class=\"hacim\">")
                    .concat(value.volume_usd_24h.toLocaleString("en-US").replace(/,/g, '.'), "$</span></a>");
            })

            $('.crypto_holder').empty().html(tempHtml);

        });

    }

    makeCorsRequestcrypto();

</script>

