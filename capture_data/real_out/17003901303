<script>
    var boxLoading = false;
    var boxContainer = document.querySelector(".box-container-column");
    var boxContainerFooter = boxContainer.querySelector(".box-container-footer")
    var boxPageNumber = 1;
    var boxPageSize = 10;
    if (boxContainer) {
        document.addEventListener("scroll", (e) => {
            if (boxLoading)
                return;

            if (document.querySelector(".box-container-column").scrollHeight <= window.pageYOffset + 500) {
                boxLoading = true;
                $.get("/Api/Boxes/ListBoxes", { pageNumber: ++boxPageNumber, pageSize: boxPageSize, view: 2 })
                    .done(function (response) {
                        if (response.Data && response.Data.length > 0) {
                            if (boxContainerFooter)
                                $(boxContainerFooter).before(response.Data);
                            else
                                $(boxContainer).append(response.Data);
                            boxLoading = false;
                            new Blazy({
                                offset: 500 // Loads images 500px before they're visible
                            });
                        }
                    })
                    .fail(function (err) {
                        console.log(err);
                        boxLoading = false;
                    })
            }
        })
    }
