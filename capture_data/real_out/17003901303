            var recommendedSystem = {
                ChangeType: function (id) {
                    $('.system-results').hide();

                    var result = $('.system-results[data-id=' + id + ']');
                    $('#advice-group-url').attr('href', result.attr('data-url'));
                    result.fadeIn();

                    recommendedSystem.Resize();
                },
                Resize: function () {
                    var wrapper = $('#recommendedSystem').closest('.masonry');
                    if (wrapper.length > 0) {
                        wrapper.masonry({
                            itemSelector: '.oge'
                        });
                    }
                }
            };
