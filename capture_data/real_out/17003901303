        <script>
            $(document).ready(function () {

                var trendNewsInterval = setInterval(reloadTrendNews, 10000);
                $('#trend-news-module').hover(function () { clearInterval(trendNewsInterval); }, function () { trendNewsInterval = setInterval(reloadTrendNews, 10000); })

            });

            function reloadTrendNews() {
                $.ajax({
                    url: '/data/GetLastTrendNews',
                    success: function (response) {
                        $('#trend-news-module .liste').hide().empty().append(response).fadeIn();
                    }
                })
            }
        </script>




