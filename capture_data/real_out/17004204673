<!-- Biz login modal -->

<script>
$(window).on('load', function() {
  function is_mobile() {
    let useragents = [
      'Android',        // 1.5+ Android
      'dream',          // Pre 1.5 Android
      'CUPCAKE'        // 1.5+ Android
      ];
    let pattern = new RegExp(useragents.join('|'), 'i');
    return pattern.test(navigator.userAgent);
  }

  if(!is_mobile()) {
      $('.disp_android').hide();
  }
  });
</script>

