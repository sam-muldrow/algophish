    $(function(){
        if(!$('#top_Visit').hasClass("loaded"))
        {
            $('#top_Visit').addClass("loaded");
            $.get('/sites/2/top_visit.html',function(data){
                if(data!=null && data!="" && data.indexOf("<li>") !== -1)
                    $('#top_Visit').html(data.split("<li>").join("<li><i class=\"fa fa-circle\"></i>"));
            });
        }
        $('.tab_list_latest a').click(function(){
            $('.latestnews_tab_content').hide();
            $('.top_visit_tab_content').hide();
            $('.'+$(this).attr('data-container')).show();
             $(this).parent().parent().find('li').removeClass("active");
            $(this).parent().addClass("active");
            return false;
        });
    });
    </script>
