<script> 	
  function rebind_info() {
    var locsobj,jstm,rng=1;
  	locsobj=locs_getmloins();
  	if (!window.location.href.match(/\/\?ug$/i) && "object" == typeof(locsobj)) {
  		jstm=Math.floor(new Date().getTime() / 1000);
  		var dst=jstm-locsobj.tm;
  		if (dst <= 5) { rng=0; }
    }  	
  	if (rng) {
      $("#idifrmlogn").attr("src","https://www.pcstore.com.tw/adm/locstog.php");
      $("#idifrmlogn").on("load",function(){ locs_display(); });
    }
    else {
      locs_display();
    }
  }

	function locs_getmloins() {
    var ifys,locsobj,rfg=0;
    if (typeof(Storage) !== "undefined") { 
      try { ifys=localStorage.getItem("mloins"); rfg=1; }
      catch (e) { return; } 	    
    }
    if (rfg) {    	
      try { locsobj=JSON.parse(ifys); } catch (e) { return; }      
      if (locsobj == null) { return; }
      return locsobj; 
    }    
  }

  function locs_display() {
  	var locsobj;
  	locsobj=locs_getmloins(); 	
  	if ("object" != typeof(locsobj)) { return; }
    if (1 == locsobj.fg) {
      $("#myloginok").show(); $("#my_logout").show(); $("#mylogin").hide(); $("#myloginok").html(locsobj.nk+'ï¼æ¨å¥½');  
    }
    else { 
      $("#myloginok").hide(); $("#my_logout").hide(); $("#mylogin").show(); 
    }
  }
 
