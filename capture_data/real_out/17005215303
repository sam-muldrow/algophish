    <script>
  dlApi.cmd.push(function (dlApi) {
    function hasVendorsConsent () {
      dlApi.hasVendorsConsent(function (consent) {
        let consents = {};
        consents['zgody_uzytkownika'] = JSON.parse('{"kat_glowna":"pozostale","kat_pelna":"pozostale","typ_strony":"pozostale","typ_uzytkownika":"uzytkownik-indywidualny","zgody_uzytkownika":{"yield_bird":false,"ads_profiling":false,"adocean":false,"content_stream":false,"exponea":false,"pixel_fb":false,"traqli":false,"okazjon":false,"hotjar":false,"plista":false,"pixel_dfp":false,"google_adwords":false,"synerise":false,"rtb_house":false,"performance_analytics":false,"google_analytics":true,"dfp":true,"twitter_widget":true,"youtube":true,"vimeo":true,"xlink":true,"tvp_player":true,"gemius":true}}')['zgody_uzytkownika'];
        if (consent) {
          for (var key in consents['zgody_uzytkownika']) {
            consents['zgody_uzytkownika'][key] = true;
          }
        }

        PPDataLayer.push(consents);
        PPDataLayer.push(() => { PPDataLayer.event(PPDataLayer.Events.RODO, 'rodo'); });
      });
    }

    dlApi.on('consentsChange', hasVendorsConsent);
    hasVendorsConsent();
  });
</script>


