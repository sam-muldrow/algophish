<script>
    function getCookieEmail(name) {
        var cookieStr = document.cookie;
        if (cookieStr.length > 0) {
            var cookieArr = cookieStr.split(";");
            for (var i = 0; i < cookieArr.length; i++) {
                var cookieVal = cookieArr[i].split("=");
                if (cookieVal[0].trim() == name) {
                    return unescape(cookieVal[1]);
                }
            }
        }
    }
    function setCookieEmail(name, value) {
        var exp = new Date();
        exp.setTime(exp.getTime() + 30 * 24 * 60 * 60 * 1000);
        document.cookie = name + "=" + escape(value) + ";path=/; domain=geekbuying.com;expires=" + exp.toGMTString();
    }
    //close top banner
    var geekSubscribeFlag = getCookieEmail("geekSubscribeFlag");
    var scUrl = window.location.search;
    function closeSub() {
        $(".sub_bg").hide();
        setCookieEmail("geekSubscribeFlag", "1");
    }
    function GetSubScribeRegisterPointInfo() {
        $.ajax({
            url: "/Service/GetSubScribeRegisterPointInfo",
            type: "get",
            dataType: "jsonp",
            success: function (data) {
                if (data && !data.isLoginNow) {
                    $("#registerCouponAmount").html(data.subScribeMoney + " OFF");
                    $("#registerCouponQty").html(" Coupon * " + data.CouponQty);
                    $("#subScribeAndRegisterPoint").html(data.SubScribeAndRegisterPoint + " G-coins");
                    $("#subscribeRegisterPointConvertAmount").html("Instant Discount " + data.subscribeRegisterPointConvertAmount);
                    if (geekSubscribeFlag && geekSubscribeFlag != "1" && (scUrl.indexOf("utm_campaign") == -1)) {
                        $(".sub_bg").show();
                    }
                    else {
                        $(".sub_bg").hide();
                    }
                }
            }
        });
    }
    $(function () {
        GetSubScribeRegisterPointInfo();
    });
</script>


