<script type="text/javascript">
    ScarabQueue.push(["recommend", {
        logic: "PERSONAL",
        containerId: "personal-recs",
        templateId: "my-recommender-template",
        limit: 8,
        success: function (SC, render) {
            if (SC.page.products.length <= 0) {
                var container = SC.recommender.container;
                container.style.display = "none";
            }
            else {
                render(SC);
                $.lazyLoad.loadImgs(".lazy_emarsys_img");
                //LoadProductPrice();
            }
        }
    }]);
    ScarabQueue.push(['include', 'description', 'has', 'US']);
    ScarabQueue.push(['exclude', 'custom_label_1', 'has', '4']);
    ScarabQueue.push(['goAsync']);
</script>
