    var COOKIESDIAMON = ".geekbuying.com";
    var TrcookieCountry = getTrCookie("vaya_googleTr");
    var multDomArray = [];
    function multDom() {
        for (var i = 0; i < multDomains.length; i++) {
            multDomArray.push(multDomains[i].Domain.toLocaleLowerCase());
        }
    }
    multDom();
    if ($.inArray(hostDom, multDomArray) > -1) {
        if (TrcookieCountry == null) {
            for (var i = 0; i < multDomains.length; i++) {
                if (multDomains[i].Domain.toLocaleLowerCase() == hostDom) {
                    var CurrentTime = new Date();
                    CurrentTime.setDate(CurrentTime.getDate() + 7);
                    expiresday = CurrentTime.toString();
                    setTrCookieCountry("vaya_googleTr", multDomains[i].CountryAlpha2, expiresday, COOKIESDIAMON);
                    isneedReload = true;
