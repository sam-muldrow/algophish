    <script>
        $.ajax({
            url: "/service/GetUserInfo",
            type: "get",
            success: function (data) {
                var userId = "unknown";
                if (data.userId) {
                    userId = data.userId;
                }
                (rtbhEvents = window.rtbhEvents || []).push(
                    {
                        eventType: 'home'
                    },
                    {
                        eventType: 'uid',
                        id: userId
                    });
            }
        });
    </script>
