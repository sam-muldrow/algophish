<input id="copyCouponSz" type="text" style="position:absolute; left:-900%; top:-900%;" />
<script>
    function closeCouponsz() {
        $(".sz_custom_win,.sz_custom_bg").hide();
    }
    var closeCopied = "";
    function copyCouponsz(obj) {
        var coupon_code = $(obj).attr("data-coupon");
        var coupon_link = $(obj).attr("data-link");
        var inputObj = "";
        var closeCopied = "";
        var clearCoupon= "";
        closeCopied = function () {
            setTimeout(function () {
                $(".coupon_copied").fadeOut(1000);
            },2000);
        }
        clearCoupon = setTimeout(function () {
            window.location.href = coupon_link;
        }, 2000);
        if (coupon_code !="") {
            inputObj = $('#copyCouponSz').val(coupon_code)[0];
            inputObj.select();
            inputObj.setSelectionRange(0, inputObj.value.length),
                document.execCommand("Copy");
            inputObj.blur();
        }
       
        if (coupon_link != "") {
            closeCouponsz();
            $(".coupon_copied").show();
            closeCopied();
            clearCoupon();
            clearTimeout(clearCoupon);
        } else {
            closeCouponsz();
            $(".coupon_copied").show();
            closeCopied();
            clearTimeout(closeCopied);
        }
    }
</script>


<script>
    var cookie_prefix = "vaya_";
    var cookie_key =cookie_prefix+"AllowCookie";

    function AllowCookie_SetCookie() {
        var currentDate = new Date();
        var expiresDate = new Date(currentDate.setYear(currentDate.getFullYear() + 1));

        document.cookie = cookie_key + "=1; path=/;expires=" + expiresDate.toGMTString();
        $(".gdpr-cookie").remove();
    }
    (function () {
        if (GetCookie(cookie_key) != "1") {
            $('<div class="gdpr-cookie"><p>We use cookies to ensure that we give you the best experience on our website. By continuing to browse the site, you agree to our use of <a href="/help/about/privacy_policy#AboutCookies" target="_blank">Cookies</a> .<span class="cookies-close" onclick="AllowCookie_SetCookie()">I agree</span></p></div>').appendTo("body");
            $(".gdpr-cookie").show();
        } else {
            $(".gdpr-cookie").remove();
        }
        function GetCookie (name) {
            var cookieStr = document.cookie;
            var cookieByName = '';
            if (cookieStr.length > 0) {
                var cookieArr = cookieStr.split(";"); 
                for (var i = 0; i < cookieArr.length; i++) {
                    var cookieVal = cookieArr[i].split("="); 
                    if ($.trim(cookieVal[0]) == name) {
                        cookieByName = unescape(cookieVal[1]); 
                    }
                }
            }
            return cookieByName;
        };
    })();
</script>

<script type="text/javascript">



    if (window["sensors"]) {
        try {
            sensors.use('WebPopup', {
                api_base_url: 'https://geekbuying.sfo-aws-virginia-01.saas.sensorsdata.cn/api/v2',
                popup_campaign_listener: {
                    /*
                    * return true receive message，false decline message
                    */
                    shouldStart: function (SFCampaign) {
                        return true;
                    },
                    onStart: function (SFCampaign) {
                        CreatePopUp(SFCampaign);
                    },
                    onEnd: function (SFCampaign) {
                        $(".sub_bg").hide();
                    },
                    onFailed: function (SFCampaign, errorCode, errorMessage) {
                        $(".sub_bg").hide();
                    }
                }
            });
        }
        catch (error) {
            concole.log(error);
        }
    }
</script>
<script type="text/javascript">
    $("#SendEmail").click(function () {
        var location = window.location.pathname.toLowerCase();
        if (window["sensors"]) {
            try {
                sensors.track('ClickSubscribeButton', {
                    entrance_source: (location == "/account/mygcoin" ? "个人中心-订阅,注册页面引导订阅勾选项" : "底部订阅按钮")
                });
            }
            catch (error) {
                console.log(error);
            }
        }
    });
    function GetSubScribeRegisterPointInfo() {
        $.ajax({
            url: "/Service/GetSubScribeRegisterPointInfo",
            type: "get",
            dataType:"jsonp",
            success: function (data) {
                if (data) {
                    $("#email").attr("placeholder", "Enter email to get " + (data.subScribeMoney) + " OFF (coupon * " + (data.CouponQty) + " ) and " + (data.SubScribePoint) + " G-coins worth " + data.SubScribePointAmount);
                }
            }
        });
    }
</script>
   
