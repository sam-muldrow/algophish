    var COOKIESDIAMON = ".geekbuying.com";
    var subcookieCountry = getsubCookie("vaya_subcountry");

    var isneedReload = false;
    var expiresday = "";
    var paypalEnv = "production";
    if (paypalEnv != "sandbox") {
        if (host != "www.geekbuying.com") {
            if (subcookieCountry == null) {
                for (var i = 0; i < multDomain.length; i++) {
                    if (multDomain[i].Domain.toLocaleLowerCase() == host) {
                        var CurrentTime = new Date();
                        CurrentTime.setDate(CurrentTime.getDate() + 7);
                        expiresday = CurrentTime.toString();
                        setsubcookieCountry("vaya_subcountry", multDomain[i].CountryAlpha2, expiresday, COOKIESDIAMON);
                        isneedReload = true;
                        break;
                    }
                }
            }
            else {
                var cookieCountry = subcookieCountry.toLocaleLowerCase();
                var language = "";
                switch (cookieCountry) {
                    case "jp":
                        language = "ja";
                        break;
                    case "gr":
                        language = "el";
                        break;
                    case "il":
                        language = "he";
                        break;
                    default:
                        language = cookieCountry;
                        break;
                }
                if (language + ".geekbuying.com" != host) {
                    for (var i = 0; i < multDomain.length; i++) {
                        if (multDomain[i].Domain.toLocaleLowerCase() == host) {
                            var CurrentTime = new Date();
                            CurrentTime.setDate(CurrentTime.getDate() + 7);
                            expiresday = CurrentTime.toString();
                            setsubcookieCountry("vaya_subcountry", multDomain[i].CountryAlpha2, expiresday, COOKIESDIAMON);
                            isneedReload = true;
                            break;
                        }
                    }
                }
            }
        }
        else {
            if (subcookieCountry != null) {
                delCookie("vaya_subcountry", COOKIESDIAMON);
                window.location.reload();
            }
        }
        if (isneedReload == true) {
            window.location.reload();
        }
    }
