<script>
    function getTrCookie(name) {
        var arr, reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");
        if (arr = document.cookie.match(reg)) {
            return unescape(arr[2]);

        } else {
            return null;
        }
    }
    function setTrCookieCountry(key, value, expiresday, domain) {
        var source_cookie = "" + key + "=" + value + ";expires=" + expiresday + ";path=/; domain=" + domain;
        document.cookie = source_cookie;
    }

    function delTrCookie(name, domain) {
        var exp = new Date();
        exp.setDate(exp.getDate() - 14);
        var cval = getTrCookie(name);
        if (cval != null) document.cookie = name + "=" + cval + ";expires=" + exp.toGMTString() + ";path=/; domain=" + domain;
    }

    function deleteGoogtransCookie(cookieDomain) {
        document.cookie = "googtrans=; expires=Thu, 01 Jan 1970 00:00:00 UTC;  path=/;";
        document.cookie = "googtrans=; expires=Thu, 01 Jan 1970 00:00:00 UTC; domain=" + cookieDomain + "; path=/;";
    }
    var isneedReload = false;
    var hostDom = window.location.host.toLocaleLowerCase();
