    } else {
          try {
              function googleTranslateElementInit() {
                  console.log("googleTranslateNow");
                  new google.translate.TranslateElement({ pageLanguage: 'en', includedLanguages: 'en,it,de,es,fr,pt,sk,hu,sl,lt,sv,el,da,iw,hr,pl,ro,cs,ja,nl', autoDisplay: false, gaTrack: true, gaId: 'UA-33904344-1' }, 'google_translate_element');
                  var orginlanguage = "";
                  var new_lang = "";
                  new_lang = localStorage.getItem("gaT");
                  try {
                      orginlanguage = "en";
                      if (window["getCookie"] && new_lang !="en" ) {
                          orginlanguage = getCookie("googtrans");
                          if (orginlanguage) {
                              orginlanguage = orginlanguage.replaceAll("/en/", "");
                          }
                      }
                  } catch (err) {
                      console.log(err, "orginlanguage");
                  }

                  new_lang = localStorage.getItem("gaT");
                  if (new_lang) {
                      var stFun = function (new_lang) {
                        if (document.querySelectorAll(".goog-te-combo option").length > 0)  {
                              var st = document.querySelector(".goog-te-combo");
                              st.value = new_lang;
                              var ev = document.createEvent("HTMLEvents");
                              ev.initEvent("change", false, true);
                              st.dispatchEvent(ev);
                              localStorage.removeItem("gaT");
                          } else {
