	// $(".number-slider .slick-prev").html("<i class='fal fa-arrow-left'></i>");
	// $(".number-slider .slick-next").html("<i class='fal fa-arrow-right'></i>");


    
    let items = document.querySelectorAll('.abc .carousel-item')

items.forEach((el) => {
    const minPerSlide = 4
    let next = el.nextElementSibling
    for (var i=1; i<minPerSlide; i++) {
        if (!next) {
    // wrap carousel by using first child
    next = items[0]
}
let cloneChild = next.cloneNode(true)
el.appendChild(cloneChild.children[0])
next = next.nextElementSibling
}
})
	
