    function addfavorite() {
      var url = location.href;
      var explorer = window.navigator.userAgent;
      try {
        if (explorer.indexOf("Chrome") >= 0) {
          alert("æ¨çæµè§å¨ä¸æ¯ææ­¤åè½ï¼è¯·ä½¿ç¨å¿«æ·é®Ctrl+Dæå·¥å å¥æ¶èå¤¹");
