<script>
  updateGaDesignData = function(){
    var design_divs = document.querySelectorAll('.design_div');
    for(var i = 0; i < design_divs.length; i++) {
      //update product data for further use
      var design_anchor_divs = design_divs[i].querySelectorAll('a[data-ga-design-data]')
      // As there are multiple anchor tags in one design div
      for(var j = 0; j < design_anchor_divs.length; j++) {
        var product_data = JSON.parse(design_anchor_divs[j].dataset['gaDesignData']);
        product_data['list'] = product_data['list'] || gon.ga_product_list || 'category';
        product_data['position'] = product_data['position'] || i+1;
        design_anchor_divs[j].setAttribute('data-ga-design-data', JSON.stringify(product_data))
      }
    }
  }
  
  gaSetImpression = function(){
    var design_divs = document.querySelectorAll('.design_div .design-impression-container[data-ga-design-data]');
    for(var i = 0; i < design_divs.length; i++) {
      ga('ec:addImpression', JSON.parse(design_divs[i].dataset['gaDesignData']))
    }
  }
  
  updateGaDesignData();
  gaSetImpression();
  ga('send', 'pageview');
</script>

