<script>
	$(window).on('load', function(){
		$('.loading').fadeOut();
		if (navigator.userAgent.match(/iPhone|Android.+Mobile/)){
		}else{
			var sqxe_header_height = $('#sqexHeader-black_rsp').innerHeight();
			var height = $(window).innerHeight() - sqxe_header_height;
			$('#contents').css('height', height);

			$('#contents ul li').on('mouseover', function(){
				$(this).siblings().addClass('is--hover');
			});

			$('#contents ul li').on('mouseout', function(){
				$(this).siblings().removeClass('is--hover');
			});

		}
	});
</script>
