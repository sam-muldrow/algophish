<script async type="text/javascript" src="https://player.vimeo.com/api/player.js"></script>

<script type="text/javascript">
    var rewardsPlayer, carouselPause = false;
    ScriptHelper.OnLoad(function () {
        var videoCheck = window.setInterval(function () {
            if (typeof Vimeo == "undefined" || !Vimeo.Player) {
                return;
            }

            window.clearInterval(videoCheck);
            const rewardsVideoiframe = document.querySelector('iframe#rewards_video_player');
            if(rewardsVideoiframe != null || rewardsVideoiframe != undefined) {
                rewardsPlayer = new Vimeo.Player(rewardsVideoiframe);
                rewardsPlayer.ready().then(() => {
                    rewardsPlayer.on('pause', function (data) {
                        //To auto start the carousel after the video is done playing
                        slideCarousel('cycle');
                    });
                    rewardsPlayer.on('play', function (data) {
                        carouselPause = true;
                        //Pause the carousel rotation
                        pauseCarousel();
                    });
                    rewardsPlayer.on('ended', function () {
                        //To auto start the carousel after the video is done playing
                        slideCarousel('cycle');
                    });
                }).catch((err) => console.log(err));
            }
        }, 200);
    });

    function startCarousel() {
        $(".carousel.main-banner-crsl").carousel({
            ride: 'carousel',
            interval: 6000,
            pause: false,
            keyboard: false
        });
    }

    function pauseCarousel() {
        var crslSlider = $(".carousel.main-banner-crsl").carousel();
        crslSlider.carousel('pause');
    }

    function slideCarousel(dir) {
        var crslSlider = $(".carousel.main-banner-crsl").carousel();
        crslSlider.carousel(dir);
        if (carouselPause) {
            rewardsPlayer.pause();
        }
        carouselPause = false;
    }

    $(document).ready(function () {
        startCarousel();

        $(".carousel.main-banner-crsl").on("mouseenter", function () {
            pauseCarousel();
        });

        $(".carousel.main-banner-crsl").on("mouseleave", function () {
            if (!carouselPause) {
                slideCarousel('cycle');
            }
        });

        $('.carousel.main-banner-crsl').on("touchstart", function (event) {
            if (carouselPause) {
                rewardsPlayer.pause();
            }
            carouselPause = false;
        });
    });

    var bnrCarousel = $(".carousel.main-banner-crsl");
    var bnrCrslNav = bnrCarousel.find(".car-nav");
    var carouselId = bnrCarousel.attr("id");
    bnrCrslNav.append("<ol class='carousel-indicators'></ol>");
    var indicators = $(".carousel-indicators");
    bnrCarousel.find(".carousel-inner").addClass("js-loaded");
    bnrCarousel.find(".carousel-inner").children(".carousel-item").each(function (index) {
        if (index === 0) {
            if (!$(this).hasClass("active")) {
                $(this).addClass("active");
            }
            indicators.append("<li data-target='#" + carouselId + "' data-slide-to='" + index + "' class='active'></li>");
        } else {
            if ($(this).hasClass("active")) {
                $(this).removeClass("active");
            }
            indicators.append("<li data-target='#" + carouselId + "' data-slide-to='" + index + "'></li>");
        }
    });

</script><input type=hidden id=PageValid /></div>


