<script type="text/javascript">
    $(document).ready(function () {
        //Medallia code has been loaded
        if (window.KAMPYLE_ONSITE_SDK) {
            OnMedalliaFooterLoad();
        } else {
            window.addEventListener('neb_OnsiteLoaded', OnMedalliaFooterLoad);
