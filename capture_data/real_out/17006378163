                                    newrelic.setCustomAttribute('st-' + st[i].name, st[i].duration);
                                if (st[i].description) tt += "desc=" + st[i].description + "&";
                            }
                        }
                        tt += "ua=" + navigator.userAgent + "&";
                        navigator.sendBeacon('/Error/LogMetrics', new URLSearchParams({ MetricData: tt }));
                    }
                }
            }
