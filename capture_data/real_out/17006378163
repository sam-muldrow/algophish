    <script type="text/javascript">
    
    function PreLoadAddToCart()
    {
        Loading.Show();
        $('#hdrPATCCanvas').hide();
        $('#hdrPATCCanvas').html("<svg class='icon mr-2' style=''><use xlink:href='/Images/Quill/shared/icons.svg?v=resSCkfJLceXHyReQLnkWA#svg-approve-contained'></use></svg> <span id='items_addedToCart_Count' class='font-weight-black js-cnvsTitle'></span>");

        $('.js-postATC-btn').removeClass('d-none');
        $('.js-miniCart-btn').addClass('d-none');
    }

        $('#js-header-cart').click(function (e) {
            if ($('#hdnCtaOption').val() == "0" || $('#hdnCtaOption').val() == "1") {
                Loading.Show();
                $('#hdrPATCCanvas').html("<span  class='font-weight-black'>Cart preview</span>");
                $('#canvas-minicart').html("<div class='justify-content-center p-4'><img src='/Images/shared/spinners.gif?v=FFJVzvI3VQZthGBUbphguw' /> <span class='pl-2'>Loading your cart..</span></div>");
                LoadMiniCartModal();
            }
            else if ($('#hdnCtaOption').val() == "2") {
                window.location.href = "/cart/cart";
            }
    });

    $('#js-header-sharedcart').click(function (e) {
       Loading.Show();
         $.ajax({
             url: '/Master/GetSharedCartPopover',
             type: 'GET',
             dataType: 'html',
             success: function (data) {
                 Loading.Hide();
                $('#divSharedCartPopover').html(data);
                showOffCanvas('#SharedCartPopover');
            },
             error: function (request, status, err) { Loading.Hide(); }
        });
    });

    </script>
    <!-----------------------------------mini cart end-------------------------------------------------------------->


<script type="text/javascript">
        var OptliEvent = {
            type: "",
            eventName: "",
            tags: {
                revenue: "",
                value: ""   // Optional as float
            }
        };

        $(document).bind(AnalyticsEvents.AnalyticsActions, function (event, args) {
            // ----------------------------------------------------------------------------------    Static Events    ----------------------------------------------------------------------------------------//

            if (args.src && args.src.type && args.src.type === "addtocart" && args.src.items.length > 0 && args.src.message === "Success") {
                //trigger SKUPageATC event
                if (Analytics.traffic.pagename === "qviewaccessoriespopup") {
                    var sku = Utilities.getQueryString('sku');
                    if (sku == args.src.items[0].sku) {
                        OptliEvent = {
                            type: "event",
                            eventName: "SKUPageATC",
                            tags: {
                                revenue: Math.round(args.src.items[0].total * 100),
                                value: args.src.items[0].total // Optional as float
                            }
                        };
                        TrackEvent(OptliEvent);

                        OptliEvent = {
                            type: "event",
                            eventName: "SKUPagePrimaryATC",
                            tags: {
                                revenue: Math.round(args.src.items[0].total*100),
                                value: args.src.items[0].total // Optional as float
                            }
                        };
                        //Call to Method to push to client & server side Optimizely
                        TrackEvent(OptliEvent);
                    }
                }
                else if (Analytics.traffic.pagename === "sku") {
                    OptliEvent = {
                        type: "event",
                        eventName: "SKUPageATC",
                        tags: {
                            revenue: Math.round(args.src.items[0].total * 100),
                            value: args.src.items[0].total // Optional as float
                        }
                    };
                    TrackEvent(OptliEvent);

                    var sku = Analytics.tracking.items["MainProduct_" + Analytics.tracking.product.sku].s;
                    if (sku == args.src.items[0].sku) {
                        OptliEvent = {
                            type: "event",
                            eventName: "SKUPagePrimaryATC",
                            tags: {
                                revenue: Math.round(args.src.items[0].total * 100),
                                value: args.src.items[0].total // Optional as float
                            }
                        };
                        //Call to Method to push to client & server side Optimizely
                        TrackEvent(OptliEvent);
                    }
                    else {
                        OptliEvent = {
                            type: "event",
                            eventName: "SKUPageNonPrimaryATC",
                            tags: {
                                revenue: Math.round(args.src.items[0].total * 100),
                                value: args.src.items[0].total // Optional as float
                            }
                        };
                        //Call to Method to push to client & server side Optimizely
                        TrackEvent(OptliEvent);
                    }
                }
                else if (Analytics.traffic.pagename === "searchresults") {
                    var skuresults = $.grep(Object.keys(Analytics.tracking.items), function (i) {
                        return Analytics.tracking.items[i].s == args.src.items[0].sku && Analytics.tracking.items[i].loc=="Searchresults";
                    });

                    if (skuresults.length>0) {
                        OptliEvent = {
                            type: "event",
                            eventName: "SearchResultATC",
                            tags: {
                                revenue: Math.round(args.src.items[0].total * 100),
                                value: args.src.items[0].total // Optional as float
                            }
                        };
                        //Call to Method to push to client & server side Optimizely
                        TrackEvent(OptliEvent);
                    }
                }
                else if (Analytics.traffic.pagename === "browse") {
                    var skuresults = $.grep(Object.keys(Analytics.tracking.items), function (i) {
                        return Analytics.tracking.items[i].s == args.src.items[0].sku && Analytics.tracking.items[i].loc == "Browseresults";
                    });

                    if (skuresults.length > 0) {
                        OptliEvent = {
                            type: "event",
                            eventName: "BrowseResultsATC",
                            tags: {
                                revenue: Math.round(args.src.items[0].total * 100),
                                value: args.src.items[0].total // Optional as float
                            }
                        };
                        //Call to Method to push to client & server side Optimizely
                        TrackEvent(OptliEvent);
                    }
                }

                //trigger GlobalATC event
                OptliEvent = {
                    type: "event",
                    eventName: "GlobalATC",
                    tags: {
                        revenue: Math.round(args.src.items[0].total*100),
                        value: args.src.items[0].total // Optional as float
                    }
                };
                TrackEvent(OptliEvent);
            };

            if (args.src && args.src.type && args.src.type === "addtocartpoint" && args.src.items.length > 0 && Analytics.traffic.pagename == "rewardscenter") {

                OptliEvent = {
                    type: "event",
                    eventName: "RcPointsRedeemed",
                    tags: {
                        value: args.src.items[0].total
                    }
                };
                TrackEvent(OptliEvent);
            }

            if (args.src && args.src.type && args.src.type === "RewardsApply" && args.src.coupons.length > 0 && args.src.coupons[0].message === "Success" && Analytics.traffic.pagename == "rewardscenter") {

                var points = args.src.coupons[0].TotalPoints != null && args.src.coupons[0].TotalPoints > 0 ? args.src.coupons[0].TotalPoints : 0;

                OptliEvent = {
                    type: "event",
                    eventName: "RcPointsRedeemed",
                    tags: {
                        value: points
                    }
                };
                TrackEvent(OptliEvent);
            }





            // ----------------------      Temp Event Section  -  Add experiment specific events below here with notes to allow easy cleanup once experiment finishes     ----------------------------------- //



        });

    //trigger below events only on Purchase ( only from OrderConfirmation page)
    $(document).ready(function () {

        if (typeof window.Analytics !== 'undefined') {
            TriggerOptlyEvents();
        }
        else {
            if (AnalyticsEvents) {
                $(document).bind(AnalyticsEvents.AnalyticsLoaded, function () {
                    TriggerOptlyEvents();
                });
            }
        }
    });

    function TriggerOptlyEvents() {
            
            
            let ckval = cookie.Get({ bucket: cookie.buckets.func, name: "rusag" });
            if (ckval === "")
            {
                OptliEvent = {
                type: "event",
                eventName: "optlysync",
                tags:
                    {
                    value: 1
                }
                };
                TrackEvent(OptliEvent);
            }
            

        if (typeof window.Analytics !== 'undefined' && window.Analytics.traffic.pagename === 'checkoutconfirmation' && document.referrer !== ''
            && typeof window.Analytics.tracking.orderconfirm !== 'undefined') {

            OptliEvent = {
                type: "event",
                eventName: "Purchase_ID",
                tags: {
                    //revenue: Analytics.tracking.orderconfirm.orderrevenue,
                    value: Analytics.tracking.orderconfirm.purchaseid
                }
            };
            TrackEvent(OptliEvent);

            OptliEvent = {
                type: "event",
                eventName: "OrderRevenue",
                tags: {
                    revenue: Math.round(Analytics.tracking.orderconfirm.orderrevenue * 100)
                }
            };
            TrackEvent(OptliEvent);

            if (typeof Analytics.tracking.orderconfirm.rewardinfo !== undefined) {
                OptliEvent = {
                    type: "event",
                    eventName: "PointsRedeemed",
                    tags: {
                        value: Analytics.tracking.orderconfirm.rewardinfo.ptsredeemed
                    }
                };
                TrackEvent(OptliEvent);
            }

            var totalunitsold = 0;
            Analytics.tracking.orderconfirm.items.forEach(function (item) {
                totalunitsold = totalunitsold + item.qty;
            });

            OptliEvent = {
                type: "event",
                eventName: "UnitsSold",
                tags: {
                    value: totalunitsold
                }
            };
            TrackEvent(OptliEvent);
        }
    }



        function TrackEvent(OptliEvent) {
           window['optimizely'] = window['optimizely'] || [];
           window['optimizely'].push(OptliEvent);


            if ('True' == "True") {
                if (navigator.sendBeacon) {
                    var body = new URLSearchParams({ eventData: JSON.stringify(OptliEvent) });
                    navigator.sendBeacon('/Optimizely/TrackEvent', body);
                } else {
                    $.ajax({
                        url: "/Optimizely/TrackEvent",
                        type: "Post",
                        data: { eventData: JSON.stringify(OptliEvent) }
                    });
                }

            }
        }
</script>
