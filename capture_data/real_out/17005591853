<!-- GA with Optimize Anti-flicker snippet  -->
<style>.async-hide { opacity: 0 !important} </style>
<script>
// Optimize anti flicker
(function(a,s,y,n,c,h,i,d,e){s.className+=' '+y;h.start=1*new Date;
h.end=i=function(){s.className=s.className.replace(RegExp(' ?'+y),'')};
(a[n]=a[n]||[]).hide=h;setTimeout(function(){i();h.end=null},c);h.timeout=c;
})(window,document.documentElement,'async-hide','dataLayer',4000, {'GTM-WGQRMLW':true});

// GA Snippet
(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
})(window,document,'script','//www.google-analytics.com/analytics.js','ga');

// UA ID
ga('create', 'UA-2913525-12', {'siteSpeedSampleRate' : 1});

// Add customTask field to tracker for Optimize
ga('set', 'customTask',  function(model) {

    // Make sure the new hit is only generated once
    var hasNewHitBeenGenerated = false;
    var globalSendTaskName = '_' + model.get('trackingId') + '_sendHitTask';
    var originalSendTask = window[globalSendTaskName] = window[globalSendTaskName] || model.get('sendHitTask');

    model.set('sendHitTask', function(sendModel) {
        var ga = window[window['GoogleAnalyticsObject']];
        var hitPayload = sendModel.get('hitPayload');

        var page = hitPayload.match(/dp\=([^&]+)/);
        if(page != null) {
            page = decodeURIComponent(page[1]);
        }
        
        if((page == null || !page.startsWith('/virtual/')) && sendModel.get('hitType') === 'pageview' && fetch_cookie('_gaexp') && !hasNewHitBeenGenerated) {
            var tracker = sendModel.get('name');
            originalSendTask(sendModel);
            ga(tracker + '.send', 'event', 'Optimize', fetch_cookie('_gaexp'), {nonInteraction: true});
            hasNewHitBeenGenerated = true;
            return;
        }
        originalSendTask(sendModel);
    });
});

// Optimize ID
ga('require','GTM-WGQRMLW');

ga('set', 'dimension1', 'home');
ga('set', 'dimension2', 'guest');
ga('set', 'dimension5', parseInt(fisherBucket));

ga('set', 'dimension6', UADimension6);
ga('set', 'dimension7', UADimension7);

ga('send', 'pageview');

</script>

