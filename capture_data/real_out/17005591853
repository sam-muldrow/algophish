<script type="module">
    window.gb = new window.growthbook.GrowthBook({
        apiHost: "https://gb-api.tsr.tools",
        clientKey: "sdk-CSYWvqPYtJOtbj",
        enableDevMode: false ? true : false,
        trackingCallback: (experiment, result) => {
            window.TSRDataLayer.push({
                event: {
                    eventType: 'experiment',
                    experimentId: experiment.key,
                    variationId: result.key,
                    featureId: result.featureId
                }
            });
        },
        onFeatureUsage: (featureKey, result) => {
            console.log("feature", featureKey, "has value", result.value);
        },
    });

    await gb.loadFeatures({ autoRefresh: true });

    function setGrowthBookAttributes() {
        if (typeof window.TSRDataLayer.userData === 'undefined') {
            setTimeout(setGrowthBookAttributes, 100);
        } else {
            let {userId: sha1UserId, ...remainingUserData} = window.TSRDataLayer.userData;
            gb.setAttributes({
                userId: 0,
                ...remainingUserData,
                ...window.TSRDataLayer.pageData,
            });
        }
    }

    setGrowthBookAttributes();
</script>


