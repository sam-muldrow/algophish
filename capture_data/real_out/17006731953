<script>
                  //要求授權
                  function LineAuth() {
                    var URL = 'https://access.line.me/oauth2/v2.1/authorize?';
                    URL += 'response_type=code';
                    URL += '&client_id=1656466508';
                    URL += '&redirect_uri=https://www.crazymike.tw/member_login_line.asp';
                    URL += '&state=Wc61C0';
                    URL += '&scope=openid%20profile%20email';
                    URL += '&nonce=nKDHQj';
                    window.location.href = URL;
                  }
                </script>
</div>
