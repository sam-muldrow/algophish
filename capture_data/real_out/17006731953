	  ac.push($(this).val());
	});
	$pids5Val = ac.toString().replace(/,/g, "|@|");
	//alert($pids5Val);
	
	var $pids6Val = '';
	var ac = [];
	$('.pids6').each(function () {
	  ac.push($(this).val());
	});
	$pids6Val = ac.toString().replace(/,/g, "|@|");
	//alert($pids6Val);
	
	  var $poptions0Val = '';
	  var ac = [];
	  $('.poptions0').each(function () {
		ac.push($(this).val());
	  });
	  $poptions0Val = ac.toString().replace(/,/g, "|@|");
	  //alert($poptions0Val);
	  
	  var $poptions1Val = '';
	  var ac = [];
	  $('.poptions1').each(function () {
		ac.push($(this).val());
	  });
	  $poptions1Val = ac.toString().replace(/,/g, "|@|");
	  //alert($poptions1Val);
	  
	var $pprices0Val = '';
	var ac = [];
	$('.pprices0').each(function () {
	  ac.push($(this).val());
	});
	$pprices0Val = ac.toString().replace(/,/g, "|@|");
	//alert($pprices0Val);
	
	var $pprices1Val = '';
	var ac = [];
	$('.pprices1').each(function () {
	  ac.push($(this).val());
	});
	$pprices1Val = ac.toString().replace(/,/g, "|@|");
	//alert($pprices1Val);
	
	var $pprices2Val = '';
	var ac = [];
	$('.pprices2').each(function () {
	  ac.push($(this).val());
	});
	$pprices2Val = ac.toString().replace(/,/g, "|@|");
	//alert($pprices2Val);
	
	var $pnotes0Val = '';
	var ac = [];
	$('.pnotes0').each(function () {
	  ac.push($(this).val());
	});
	$pnotes0Val = ac.toString().replace(/,/g, "|@|");
	//alert($pnotes0Val);
	
	var $pnotes1Val = '';
	var ac = [];
	$('.pnotes1').each(function () {
	  ac.push($(this).val());
	});
	$pnotes1Val = ac.toString().replace(/,/g, "|@|");
	//alert($pnotes1Val);
	
	var $pnotes2Val = '';
	var ac = [];
	$('.pnotes2').each(function () {
	  ac.push($(this).val());
	});
	$pnotes2Val = ac.toString().replace(/,/g, "|@|");
	//alert($pnotes2Val);
	
	var $pnotes3Val = '';
	var ac = [];
	$('.pnotes3').each(function () {
	  ac.push($(this).val());
	});
	$pnotes3Val = ac.toString().replace(/,/g, "|@|");
	//alert($pnotes3Val);
	
	
	if ($pids0Val != '') {
	  $.ajax({
		url: "order_updateCart.asp",
		data: {
		  
		  pids0 : $pids0Val,
		  
		  pids1 : $pids1Val,
		  
		  pids2 : $pids2Val,
		  
		  pids3 : $pids3Val,
		  
		  pids4 : $pids4Val,
		  
		  pids5 : $pids5Val,
		  
		  pids6 : $pids6Val,
		  
			poptions0 : $poptions0Val,
			
			poptions1 : $poptions1Val,
			
		  pprices0 : $pprices0Val,
		  
		  pprices1 : $pprices1Val,
		  
		  pprices2 : $pprices2Val,
		  
		  pnotes0 : $pnotes0Val,
		  
		  pnotes1 : $pnotes1Val,
		  
		  pnotes2 : $pnotes2Val,
		  
		  pnotes3 : $pnotes3Val,
		  
		},
		type: "POST",
		dataType: "text",
		success: function(data,textStatus,jqXHR) {
		  console.log(data);
		  if (data == 'PARA') {
			swal({
			  title: '系統參數錯誤',
			  text: '系統將於 2 秒後自動重新整理網頁',
			  type: 'error',
			  timer: 2000
			}).then(
			  function () {
				location.reload();
			  },
			  function (dismiss) {
				if (dismiss === 'timer') {
				  location.reload();
				}
			  }
			)		
		  } else if (data == 'ERR') {
			swal(
			  '系統發生錯誤',
			  '請聯絡我們以取得進一步協助',
			  'error'
			);
		  } else {
			var okArr = data.split('|@|');
			//console.log(okArr[0]);
			//console.log(okArr[1]);
			//console.log(okArr[2]);
			//console.log(okArr[3]);
			//console.log(okArr[4]);
			//console.log(okArr[5]);
			//console.log(okArr[6]);
			//console.log(okArr[7]);
			//console.log(okArr[8]);
			//console.log(okArr[9]);
			//console.log(okArr[10]);
			//console.log(okArr[11]);
			if (okArr[0] != 'OK') {
			  swal(
				'發生不明錯誤',
				'請聯絡我們以取得進一步協助',
				'error'
			  );
			} else {
				
				  $('#hd_tt').text(okArr[1]);
				
				$('#hd_ta').text(okArr[2]);
				$('#ct_total').text(okArr[1]);

				
				  $('#ct_paySID').html(okArr[8]);
				  $('#ct_payTotal').text(okArr[10]);
				
				calGrandTotal(okArr[1]);
			}
		  }
		},
		error: function() {
			swal(
			  '系統發生錯誤',
			  '請聯絡我們以取得進一步協助',
			  'error'
			);
		},
		complete: function() {
		  //alert('complete');
		}
	  });
	} else {
	  clearCart();
	}
  }
  
  //清空購物車: 詢問
  function clearComfirm() {
	swal({
	  title: '您確定要清空購物車的內容嗎?',
	  text: '',
	  type: 'warning',
	  showCancelButton: true,
	  confirmButtonColor: '#d33',
	  confirmButtonText: '確定',
	  cancelButtonColor: '#094',
	  cancelButtonText: '取消',
	}).then(function() {
	  clearCart();
	  swal({
		title: '您的購物車已經清空',
		text: '',
		type: 'success',
		timer: 1500
	  })	
	})										
  }
  
  //清空購物車: 執行
  function clearCart() {
	$.ajax({
	  url: "order_clear.asp",
	  data: {
		act : ''
	  },
	  type: "POST",
	  dataType: "text",
	  success: function(data,textStatus,jqXHR) {
		//alert(data);
		if (data == 'ERR') {
			  swal(
				'系統發生錯誤',
				'請聯絡我們以取得進一步協助',
				'error'
			  );
		} else {
		  var okArr = data.split('|@|');
		  if (okArr[0] != 'OK') {
				swal(
				  '發生不明錯誤',
				  '請聯絡我們以取得進一步協助',
				  'error'
				);
		  } else {
			  
				$('#hd_tt').text(okArr[1]);
			  
			  $('#hd_ta').text(okArr[2]);
			  $('#ct_total').text(okArr[1]);
			  

			  if ($("#mainCart").length > 0) {	//更新主要購物車
				$('#mainCart').html('');
				$('#mainCart').prepend('<tr><td colspan="10"><p class="m-40 align-left">-- 購物車是空的 --</p></td></tr>');
			  }

			  
			  
			  $('#paymentBlock').fadeOut(300);
			  $('#checkoutBlock').fadeOut(300);
			  $('.fillsArea').fadeOut(300);
		  }
		}
	  },
	  error: function() {
			  swal(
				'系統發生錯誤',
				'請聯絡我們以取得進一步協助',
				'error'
			  );
	  },
	  complete: function() {
		//alert('complete');
	  }
	});
  }
  
  //重新計算總金額
  function calGrandTotal(t) {
	var newTotal = parseInt(t);
	//console.log(newTotal);
	
	var $loadingIcon = $('#myCheckout .loadingIcon');
	$loadingIcon.show(100);
	
	setTimeout(function() {
	  
		newTotal = newTotal + parseInt($('#ct_payTotal').text());
		//console.log('ct_payTotal+'+parseInt($('#ct_payTotal').text())+'='+newTotal);
	  
	  $('#ct_grandTotal').text(newTotal);
	  $loadingIcon.hide(100);
	  
	  $.ajax({
		url: "order_updateTotal.asp",
		data: {
		  
			pt : parseInt($('#ct_payTotal').text()),
		  
		  gt : newTotal
		},
		type: "POST",
		dataType: "text",
		success: function(data,textStatus,jqXHR) {
		  //console.log(data);
		  if (data == 'ERR') {
			swal(
			  '系統發生錯誤',
			  '請聯絡我們以取得進一步協助',
			  'error'
			);
		  }
		},
		error: function() {
			swal(
			  '系統發生錯誤',
			  '請聯絡我們以取得進一步協助',
			  'error'
			);
		},
		complete: function() {
		  //alert('complete');
		}
	  });
	}, 1500);
  }
</script>
