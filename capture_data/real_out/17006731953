<script>
	var appID = '1193379731072791';
	
	function statusChangeCallback(response) {
//	  Status
//	  connected:		用戶已登入 Facebook，且已登入網頁
//	  not_authorized:	用戶已登入 Facebook，但尚未登入網頁
//	  unknown:			用戶未登入 Facebook，因此無法得知對方是否已登入網頁；或者之前已呼叫 FB.logout()，因此無法連結至 Facebook
	  //console.log(response.status);
	  //alert(response.status);
	  
	  if (response.status === 'connected') {				//A. 用戶已登入 Facebook，且已登入網頁
		let uid = response.authResponse.userID;
		let uat = response.authResponse.accessToken;
		//console.log('uid='+uid);	//用戶編號
		//console.log('uat='+uat);	//用戶存取權杖
		
		
		  if (document.getElementById('FBLoginNow').value === '1') {
			window.location.href = 'member_loginFB.asp?at='+uat;
		  }
		
	  } else if (response.status === 'not_authorized' || response.status === 'unknown') {	//B. 用戶未登入 Facebook 或未登入網頁
		
		  if (document.getElementById('FBLoginNow').value === '1') {
			window.location.href = "https://www.facebook.com/v11.0/dialog/oauth?client_id=1193379731072791&redirect_uri=https://www.crazymike.tw/member_login_facebook.asp&scope=public_profile,email";
//			FB.login(function(response) {
//			  //console.log('FB.login='+response);
//			  if (response.status === 'connected') {
//				let uid = response.authResponse.userID;
//				let uat = response.authResponse.accessToken;
//				//alert("Facebook 登入成功!");
//				//console.log('uid='+uid);	//用戶編號
//				//console.log('uat='+uat);	//用戶存取權杖
//				//window.location.href = 'member_loginFB.asp?at='+uat;
//			  } else {
//				alert("Facebook 帳號無法登入");
//			  }
//			}, {scope: 'public_profile,email'});
		  }
		
	  }
	}
	
	window.fbAsyncInit = function() {
	  FB.init({
		appId      : '1193379731072791',
		cookie     : true,  // enable cookies to allow the server to access the session
		xfbml      : true,  // parse social plugins on this page
		version    : 'v11.0'
	  });
	  FB.AppEvents.logPageView();
	  checkLoginState();
	};
	
	// Load the SDK asynchronously
	(function(d, s, id) {
	  var js, fjs = d.getElementsByTagName(s)[0];
	  if (d.getElementById(id)) return;
	  js = d.createElement(s); js.id = id;
	  js.src = "//connect.facebook.net/zh_TW/sdk.js";
	  fjs.parentNode.insertBefore(js, fjs);
	}(document, 'script', 'facebook-jssdk'));

	// 檢查用戶的登入狀態
	function checkLoginState() {
	  FB.getLoginStatus(function(response) {
		statusChangeCallback(response);
	  });
	}
	
	//登入
	function fblogin() {
	  document.getElementById('FBLoginNow').value = '1';
	  checkLoginState();
	}
	
	//登出
	function fblogout() {
	  //FB.logout(function(response) { //一併登出 Facebook
		 window.location.href = 'member_logoff.asp';
	  //});	  
	}
