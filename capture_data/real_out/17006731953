<script>
  
	var $ct_payTitle = $('#ct_payTitle');	//名稱
	var $ct_payTotal = $('#ct_payTotal');	//金額
	
	$('#ct_paySID').on('change', function() {
	  //console.log($(this).val());
	  var element = $(this).find('option:selected'); 
      var myAtTitle = element.attr("data-title"); 
      var myAtTotal = element.attr("data-total"); 
	  //console.log(myAtTitle);
	  //console.log(myAtTotal);
	  $('#ct_payTitle').text(myAtTitle);
	  $('#ct_payTotal').text(myAtTotal);
	  $('.payDecpt').hide();
	  $('.payDecpt_'+$(this).val()).show();
	  
	  if ($(this).val() != '' && $(this).val() != 0) {
		$.ajax({
		  url: "order_updatePayShp.asp",
		  data: {
			payId : $(this).val()
		  },
		  type: "POST",
		  dataType: "text",
		  success: function(data,textStatus,jqXHR) {
			//console.log(data);
			if (data == 'PARA') {
			  swal({
				title: '系統參數錯誤',
				text: '系統將於 2 秒後自動重新整理網頁',
				type: 'error',
				timer: 2000
			  }).then(
				function () {
				  location.reload();
				},
				function (dismiss) {
				  if (dismiss === 'timer') {
					location.reload();
				  }
				}
			  )		
			} else if (data == 'ERR') {
			  swal(
				'系統發生錯誤',
				'請聯絡我們以取得進一步協助',
				'error'
			  );
			} else if (data == 'OK') {
			  calGrandTotal($('#ct_total').text());
			} else {
			  swal(
				'發生不明錯誤',
				'請聯絡我們以取得進一步協助',
				'error'
			  );
			}
		  },
		  error: function() {
			  swal(
				'系統發生錯誤',
				'請聯絡我們以取得進一步協助',
				'error'
			  );
		  },
		  complete: function() {
			//alert('complete');
		  }
		});
	  }
	});
  
  
  //變更購買數量 & 小計
  function changeCart(n,m) {
	var $ct_amountVal = $('.ct_amount'+n).val();
	var $ct_amountMax = $('.ct_amount'+n).attr('max');
	var $ppricesVal = $('.pprices0'+n).val();
	var $pnotes = $('.pnotes0'+n);
	var $ct_subtotal = $('.ct_subtotal'+n);
    
    $ct_amountVal_t=0;
    for($v=0;$v<m;$v++)
    {
        $ct_amountVal_t += parseInt($('.ct_amount'+$v).val());
    }
    
    if (parseInt($ct_amountVal_t)>parseInt($ct_amountMax))
    {
        swal({
            text: '購買數，最多只能購買 '+ $ct_amountMax +' 個!',
            type: 'warning'
        }).then(
            function () {
                location.reload();
            },
            function (dismiss) {
                if (dismiss === 'timer') {
                location.reload();
                }
            }
        )
    }
    else
    {
        if ($ct_amountVal != 'undefined' && parseInt($ct_amountVal) >= 1 && parseInt($ct_amountVal) <= parseInt($ct_amountMax)) {
        $('.pids6'+n).val($ct_amountVal);
    
        //更新小計
        
                    $pnotes.val($ct_amountVal * $ppricesVal);
                    $ct_subtotal.text($ct_amountVal * $ppricesVal);
                    //alert($ct_amountVal * $ppricesVal);
                    updateCart('1');
        
        } else {
        alert('請輸入正確的數量');
        }
    }
  }
  
  //更新備註
  function changeNotes(n,v){
	//alert(n);
	//alert(v);
	var $pnotes = $('.pnotes1'+n);
	//alert($pnotes.val());
  
	if (v != '') {
	  $pnotes.val(v);
	} else {
	  $pnotes.val('EMPTY');
	}
	//alert($pnotes.val());
	updateCart('0');
  }
  
  //刪除商品: 詢問
  function deleteComfirm(i,g,t,a) {
	//alert(i);	//行數標記
	//alert(g);	//商品群組編號
	//alert(t);	//商品類型
	//alert(a);	//刪除加購商品警語
	swal({
	  title: '您確定要刪除這個商品嗎?',
	  text: a,
	  type: 'warning',
	  showCancelButton: true,
	  confirmButtonColor: '#d33',
	  confirmButtonText: '確定',
	  cancelButtonColor: '#094',
	  cancelButtonText: '取消',
	}).then(function () {
	  deleteCart(i,g,t);
	})										
  }
  
  //刪除商品: 執行
  function deleteCart(i,g,t) {
	//alert(i);	//行數標記
	//alert(g);	//商品群組編號
	//alert(t);	//商品類型
	var $cartItemRow = $('.cartItemRow'+i);		//刪除第 N 行
	var $masgRows = $('.masg_'+g);				//商品群組
	//alert($masgRows.length);
	
	if (t == '1') {								//刪除加購商品
	  $cartItemRow.fadeOut(300, function() {
		$cartItemRow.remove();
		updateCart('1');
		//alert('刪除加購商品');
	  });
	} else {									//刪除主商品，此商品所屬的加購商品也需一併刪除
	  $masgRows.fadeOut(300, function() {
		$masgRows.remove();
		updateCart('1');						//每移除一行就更新一次
		//alert('刪除商品群組');
	  });
	}
	rollToMyPosition($('.cartArea'));
  }
  
  //更新購物車, 商品小計
  //a: 是否執行折價券檢查 (1,0)
  function updateCart(a) {
	
	var $pids0Val = '';
	var ac = [];
	$('.pids0').each(function () {
	  ac.push($(this).val());
	});
	$pids0Val = ac.toString().replace(/,/g, "|@|");
	//alert($pids0Val);
	
	var $pids1Val = '';
	var ac = [];
	$('.pids1').each(function () {
	  ac.push($(this).val());
	});
	$pids1Val = ac.toString().replace(/,/g, "|@|");
	//alert($pids1Val);
	
	var $pids2Val = '';
	var ac = [];
	$('.pids2').each(function () {
	  ac.push($(this).val());
	});
	$pids2Val = ac.toString().replace(/,/g, "|@|");
	//alert($pids2Val);
	
	var $pids3Val = '';
	var ac = [];
	$('.pids3').each(function () {
	  ac.push($(this).val());
	});
	$pids3Val = ac.toString().replace(/,/g, "|@|");
	//alert($pids3Val);
	
	var $pids4Val = '';
