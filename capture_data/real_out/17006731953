<script>
  var $form = $('#checkoutForm');
  var $subchk = $('.subchk');
  var $sendBtn = $('#checkoutForm #sendBtn');
  var $loadingIcon = $('#checkoutForm .loadingIcon');
    
  

  //送出表單時檢查是否勾選同意
  $form.submit(function() {
	hideBtns();
	if ($subchk.val() == '') {
	  checkAgree();
	  return false;
	}
  });
  
  function checkAgree(form) {
	//alert($('input.iagree').prop('checked'));
	
	if ($('input.iagree').prop('checked') == false)  {
	  alert('請勾選同意按鈕才能進行結帳喔!');
	  showBtns();
	} else {
	  hideBtns();
  
	  //表單欄位驗證
	  var chkvar = '';	
	  $('#checkoutForm .nk-error').each(function () {
		if ($(this).text() != ''){
			chkvar = 'R';
			return false;
		}
	  });
	  //alert(chkvar);
	  
	  if (chkvar != '') {
		showBtns();
	  } else {
		form.submit();
	  }
	}
  }
  
  //按鈕失效，避免重複送出
  function hideBtns() {
	$sendBtn.prop('disabled',true);
	$sendBtn.css('opacity','0.8');
	$loadingIcon.show(100);
  }
  //恢復按鈕
  function showBtns() {
	$sendBtn.prop('disabled',false);
	$sendBtn.css('opacity','1');
	$loadingIcon.hide(100);
  }
  
  
</script>

<script src="js/post.js"></script>
<script>
	//執行 (可套用多組地址)  ----------------------------------------------------------------->>
	
      initCounty(document.form1.odr_province)
      initZone(document.form1.odr_province, document.form1.odr_city, document.form1.odr_zip);
      function init1(v1,v2,v3){
        document.form1.odr_province.options[0].value= v1;
        document.form1.odr_province.options[0].text= v1;
        document.form1.odr_city.options[0].value= v2;
        document.form1.odr_city.options[0].text= v2;
        document.form1.odr_zip.value= v3 ;
      }
      init1("1.請選擇縣市","2.再選擇區域","");
	
      initCounty(document.form1.odr_province2)
      initZone(document.form1.odr_province2, document.form1.odr_city2, document.form1.odr_zip2);
      function init2(v1,v2,v3){
        document.form1.odr_province2.options[0].value= v1;
        document.form1.odr_province2.options[0].text= v1;
        document.form1.odr_city2.options[0].value= v2;
        document.form1.odr_city2.options[0].text= v2;
        document.form1.odr_zip2.value= v3 ;
      }
      init2("1.請選擇縣市","2.再選擇區域","");
	
  </script>
<script>
	//地址同上
	function setAddrSame(form) {
	  var name1 = form.odr_name;
	  var sex1 = form.odr_sex;
	  var email1 = form.odr_email;
	  var cellphone1 = form.odr_cellphone;
	  var tel1 = form.odr_tel;
  //	var ext1 = form.odr_ext;
  //	var fax1 = form.odr_fax;
	  var odr_zip1 = form.odr_zip;
	  var odr_province1 = form.odr_province;
	  var odr_city1 = form.odr_city;
	  var odr_address1 = form.odr_address;
	  var odr_country1 = form.odr_country;
	  var name2 = form.odr_name2;
	  var email2 = form.odr_email2;
	  var sex2 = form.odr_sex2;
	  var cellphone2 = form.odr_cellphone2;
	  var tel2 = form.odr_tel2;
  //	var ext2 = form.odr_ext2;
  //	var fax2 = form.odr_fax2;
	  var odr_zip2 = form.odr_zip2;
	  var odr_province2 = form.odr_province2;
	  var odr_city2 = form.odr_city2;
	  var odr_address2 = form.odr_address2;
	  var odr_country2 = form.odr_country2;
	  
	  if (form.addrSame.checked) {
		name2.value = name1.value;
		email2.value = email1.value;
		tel2.value = tel1.value;
//		ext2.value = ext1.value;
		cellphone2.value = cellphone1.value;
//		fax2.value = fax1.value;
		
		for (var i = 0; i < sex1.length; i++) {
			if (sex1[i].checked == true) {
				sex2[i].checked = true;
			}
		}
		$('input.jUI').checkboxradio('refresh');
		
			init2(odr_province1.value,odr_city1.value,odr_zip1.value);
			odr_address2.value = odr_address1.value;
			odr_province2.selectedIndex = 0;
			odr_city2.selectedIndex = 0;
		
	  }
	}
  </script>
