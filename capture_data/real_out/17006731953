<script>
  
	//加入購物車
	//jp:	直接前往購物車 (Y,N)
	//bq:	購物或詢價 (B,Q)
	function addToCart(jp,bq) {
	  var $ct_type = $('form.addToCart1 .ct_type1');
	  var $ct_kind = $('form.addToCart1 .ct_kind1');
	  var $ct_pid = $('form.addToCart1 .ct_pid1');
	  var $ct_option = $('form.addToCart1 .ct_option1');
	  var $ct_attrVal = '';
	  var $prodAmt = $('form.addToCart1 .prodAmt1');
	  var $addToCartBtn = $('form.addToCart1 .addToCartBtn1');
	//  alert($prodAmt.val());
	//  alert(parseInt($prodAmt.val()));
	//  alert($prodAmt.attr('max'));
	//  alert($addToCartBtn.attr('disabled'));
	
	  
	
	  if ($prodAmt.val() != 'undefined' && parseInt($prodAmt.val()) >= 1) {
		$.ajax({
		  url: "order_addToCart.asp",
		  data: {
			ct_type : $ct_type.val(),
			ct_kind : $ct_kind.val(),
			ct_pid : $ct_pid.val(),
			ct_attr : $ct_attrVal,
			ct_option : $ct_option.val(),
			ct_amount : $prodAmt.val(),
			
			
		  },
		  type: "POST",
		  dataType: "text",
		  success: function(data,textStatus,jqXHR) {
			//console.log(data);
			if (data == 'PARA') {
			  swal({
				title: '系統參數錯誤',
				text: '系統將於 2 秒後自動重新整理網頁',
				type: 'error',
				timer: 2000
			  }).then(
				function () {
				  location.reload();
				},
				function (dismiss) {
				  if (dismiss === 'timer') {
					location.reload();
				  }
				}
			  )
			} else if (data == 'NEED1') {
			  swal(
				'您尚未加入SPEED S.緹花石墨烯能量微整褲(三代)x3件(贈JD佐登 洗衣精x2)',
				'請先搶購主商品，才可搶購喔!',
				'warning'
			  );
			} else if (data == 'EOF' || data == 'EOFOP') {
			  swal(
				'找不到這筆商品或商品資料已移除',
				'請聯絡我們以取得進一步協助',
				'error'
			  );
			} else if (data == 'NOF') {
			  swal(
				'對不起，贈品只能領取一次喔!',
				'',
				'warning'
			  );
			} else if (data == 'EXIST') {
			  $.Toast('已加入過此項目', {'class': 'alert'});
			} else if (data == 'AMT0') {
			  swal(
				'數量不可少於 1',
				'',
				'warning'
			  );
			} else if (data == 'AMT1') {
			  swal(
				'商品庫存不足，請減少您的數量',
				'',
				'warning'
			  );
			} else if (data == 'AMT2') {
			  swal(
				'對不起，超過數量上限',
				'您最多只能加入 '+ $prodAmt.attr('max') +' 個喔!',
				'warning'
			  );
			} else if (data == 'ERR') {
			  swal(
				'系統發生錯誤',
				'請聯絡我們以取得進一步協助',
				'error'
			  );
			} else {
			  var okArr = data.split('|@|');
	//		  alert(okArr[0]);
	//		  alert(okArr[1]);
	//		  alert(okArr[2]);
	//		  alert(okArr[3]);
	//		  alert(okArr[4]);
	//		  alert(okArr[5]);
	//		  alert(okArr[6]);
	//		  alert(okArr[7]);
			  if (okArr[0] != 'OK') {
				if (okArr[0] == 'AMT3') {
				  swal(
					'庫存不足',
					'\"'+ okArr[2] +'\" 只剩下 '+ okArr[1] +' 個庫存喔!',
					'warning'
				  );
				} else if (okArr[0] == 'AMT4') {
				  swal(
					'對不起，庫存不足',
					'您最多只能加購 '+ okArr[1] +' 個 \"'+ okArr[2] +'\" 喔!',
					'warning'
				  );
				} else if (okArr[0] == 'AMT5') {
				  swal(
					'對不起，您已超過加購數量上限',
					'您最多只能加購 '+ okArr[1] +' 個 \"'+ okArr[2] +'\" 喔!',
					'warning'
				  );
                } else {
				  swal(
					'發生不明錯誤',
					'請聯絡我們以取得進一步協助',
					'error'
				  );
				}
			  } else {
				$('#hd_tt').text(okArr[1]);
				$('#hd_ta').text(okArr[2]);
				$('#ct_total').text(okArr[1]);

				if ($("#mainCart").length > 0) {	//更新主要購物車
				  $('#mainCart').html('');
				  $('#mainCart').prepend(okArr[8]);
				}

				
				
				if (jp == 'N') {
				  var okMsg;
				  if (bq == 'Q') {
					okMsg = '已成功放入詢價單';
				  } else {
					okMsg = '已成功放入購物車';
				  }
				  $.Toast(okMsg, {'class': 'alert'});
				  var $catttop = $('#hd_ta');
				  var int = window.setTimeout(moveCatttop,2000);
				  var int = window.setTimeout(moveCatttop,800);
				  function moveCatttop() {
					  $catttop.toggleClass('moveCatttop');
				  };
				}
				$('#paymentBlock').fadeIn(300);
				$('#checkoutBlock').fadeIn(300);
				$('.fillsArea').fadeIn(300);
				calGrandTotal(okArr[1]);
			  }
			}
		  },
		  error: function() {
			  swal(
				'系統發生錯誤',
				'請聯絡我們以取得進一步協助',
				'error'
			  );
		  },
		  complete: function() {
			//alert('complete');
		  }
		});
	  } else {
			  swal(
				'請正確選擇商品規格及數量喔!',
				'',
				'warning'
			  );
	  }
	}
  
  
  $(document).ready(function() {
	//啟動 validate 表單驗證
	var validator1 = $('form.addToCart1').valid();
	
	
	  initOP2();
	
  });
  
  
	var $colorImg = $('.colorImg');
	var $OP2Block = $('#OP2Block');
  
	$colorImg.on('click', function() {		//選擇款式選項 1 ，並且變更名稱、商品圖片、款式選項 2 、價格、數量
	  var opId = $(this).attr('opid');
	  var opNo = $(this).attr('opno');
	  //alert(opId);
	  //alert(opNo);
	  //alert($(this).attr('title'));
	  
	  $colorImg.removeClass('isSelected');	//移除其他選取中 class
	  $(this).addClass('isSelected');		//增加目前選取中 class
	  $('.ct_option1').val(0);				//先將 ct_option 的值歸零
	  
  
	  $.ajax({
		url: "product_getOptions.asp",
		data: {
		  pid : '511',
		  opid : opId
		},
		type: "POST",
		dataType: "text",
		success: function(data,textStatus,jqXHR) {
		  //console.log(data);
		  if (data == 'NOMAIN') {
			$OP2Block.html('找不到商品資料');
			clearAmt('1');
		  } else if (data == 'EMPTY') {
			$OP2Block.html('此規格 1沒有任何規格 2');
			clearAmt('1');
		  } else if (data == 'ERR') {
			swal(
			  '系統發生錯誤',
			  '請聯絡我們以取得進一步協助',
			  'error'
			);
			clearAmt('1');
		  } else {
			$OP2Block.html(data);			//變更款式選項 2 內容
			initOP2();
			
			//價格
			var $phcabtn1_price1 = $('button.phcabtn').first().attr('opkprice1');
			var $phcabtn1_price2 = $('button.phcabtn').first().attr('opkprice2');
			var $phcabtn1_priceLabel = $('button.phcabtn').first().attr('opkpricelabel');
			//alert($phcabtn1_price1);
			setPrice($phcabtn1_price1,$phcabtn1_price2,$phcabtn1_priceLabel);	//變更價格
			
			//數量
			var $phcabtn1_amt = $('button.phcabtn').first().attr('opkamt');
			//alert($phcabtn1_amt);
			
			if (parseInt($phcabtn1_amt) == 0) {
			  setSoldout('1');					//顯示售完
			} else {
			  setAmt($phcabtn1_amt,'1');		//變更購買數量
			}
			
			var $phcabtn1_id = $('button.phcabtn').first().attr('opkid');	//取得款式選項 2 第一個內容的 ID
			$('.ct_option1').val($phcabtn1_id);	//變更 ct_option 的值
		  }
		},
		error: function() {
			swal(
			  '系統發生錯誤',
			  '請聯絡我們以取得進一步協助',
			  'error'
			);
			clearAmt('1');
		},
		complete: function() {
		  //alert('complete');
		}
	  });
	});
  
	function initOP2() {	//設定款式選項 2
	  var $phcabtn = $('button.phcabtn');
	
	  $phcabtn.on('click', function() {		//選擇款式選項 2 ，並且變更價格、數量
		var opkId = $(this).attr('opkid');
		//alert(opkId);
		
		$phcabtn.removeClass('isSelected');	//移除其他選取中 class	
		$(this).addClass('isSelected');		//增加目前選取中 class
		$('.ct_option1').val(opkId);		//變更 ct_option 的值
	
		$.ajax({
		  url: "product_getOptions.asp",
		  data: {
			pid : '511',
			opkid : opkId
		  },
		  type: "POST",
		  dataType: "text",
		  success: function(data,textStatus,jqXHR) {
			//console.log(data);
			var okArr = data.split('|@|');
			if (okArr[0] != 'OK') {
			  if (data == 'ERR') {
				swal(
				  '系統發生錯誤',
				  '請聯絡我們以取得進一步協助',
				  'error'
				);
				clearAmt('1');
			  } else if (data == 'EMPTY') {
				swal(
				  '找不到資料',
				  '請選擇其他項目',
				  'error'
				);
				clearAmt('1');
			  } else {
				swal(
				  '發生不明錯誤',
				  '請聯絡我們以取得進一步協助',
				  'error'
				);
				clearAmt('1');
			  }
			} else {
			  setPrice(okArr[1],okArr[2],okArr[3]);	//變更價格
			  if (parseInt(okArr[4]) == 0) {
				setSoldout('1');					//顯示售完
			  } else {
				setAmt(parseInt(okArr[4]),'1');		//變更購買數量
			  }
			}
		  },
		  error: function() {
			  swal(
				'系統發生錯誤',
				'請聯絡我們以取得進一步協助',
				'error'
			  );
			  clearAmt('1');
		  },
		  complete: function() {
			//alert('complete');
		  }
		});
	  });
	}
  

  function setPrice(p1,p2,pb) {		//設定價格
	$('#generalPrice').text(p1);
	$('#specialPrice').text(p2);
	$('#specialLabel').text(pb);
  }

  
	function setSoldout(n) {		//顯示售完
	  var $amtBlock = $('#amtBlock');
	  $amtBlock.html('<p class="soldout"> 搶購一空！明日請早 </p>');
	  $('.addToCartBtn'+n).attr('disabled','disabled').find('.buyTitle').text('搶購一空！明日請早');
	  $('.addToCartBtn'+n).addClass('soldout');
	}
	
	function clearAmt(n) {			//刪除購買數量按鈕
	  var $amtBlock = $('#amtBlock');
	  $amtBlock.html(' --- ');
	  $('.addToCartBtn'+n).attr('disabled','disabled');
	}
	
	function setAmt(a,n) {			//設定購買數量按鈕 (a: 最大值 max)
	  var $amtBlock = $('#amtBlock');
	  clearAmt('1');
	  
		var amtBlockContents = '<select class="prodAmt1 form-control" name="ct_amount">';
		for (var i=1; i<=1; i++) {
		  amtBlockContents += '<option value="'+i+'">'+i+'</option>';
		}
		amtBlockContents += '</select>';
		$amtBlock.html(amtBlockContents);
	  
	  $('.addToCartBtn'+n).removeAttr('disabled').find('.buyTitle').text('加入購物車');
	  $('.addToCartBtn'+n).removeClass('soldout');
	}
  
</script>
<script>
    // carousel for products
	var tween = window.TweenMax;
    $('.nk-product-carousel').each(function () {
        var $carousel = $(this).find('.nk-carousel-inner');
        var $thumbs = $(this).find('.nk-product-carousel-thumbs');
        var $thumbsCont = $thumbs.children();
        var curX = 0;
        var thumbsW = 0;
        var thumbsContW = 0;
  
        function updateValues() {
            if ($thumbsCont[0]._gsTransform && $thumbsCont[0]._gsTransform.x) {
                curX = $thumbsCont[0]._gsTransform.x;
            } else {
                curX = 0;
            }
            thumbsW = $thumbs.width();
            thumbsContW = $thumbsCont.width();
        }
  
        $thumbsCont.on('click', '> div', function () {
            var index = $(this).index();
            $carousel.flickity('select', index);
        });
  
        $carousel.on('cellSelect', function () {
            var api = $carousel.data('flickity');
            if (!api) {
                return;
            }
            // set selected nav cell
            var $selected = $thumbsCont.children().removeClass('active').eq(api.selectedIndex).addClass('active');
  
            // scroll nav
            updateValues();
            var selectedLeft = $selected.position().left;
            if (selectedLeft < 0) {
                tween.to($thumbsCont, 0.2, {
                    x: curX - selectedLeft
                });
            } else {
                var selectedW = $selected.width();
                if (selectedLeft + selectedW > thumbsW) {
                    tween.to($thumbsCont, 0.2, {
                        x: curX - (selectedLeft + selectedW - thumbsW)
                    });
                }
            }
        });
  
        var startX = false;
        var mc = new Hammer.Manager($thumbs[0]);
        mc.add(new Hammer.Pan({
            pointers: 1,
            threshold: 0
        }));
        mc.on('pan press', function (e) {
            e.preventDefault();
  
            // init
            if (startX === false) {
                updateValues();
                startX = curX;
            }
  
            // move
            if (thumbsContW > thumbsW) {
                curX = Math.min(0, Math.max(e.deltaX + startX, thumbsW - thumbsContW));
                tween.set($thumbsCont, {
                    x: curX
                });
            }
            if (e.isFinal) {
                startX = false;
            }
        });
    });
  </script>
