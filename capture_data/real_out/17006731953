<script type="text/javascript">
      //Google reCAPTCHA v2
      var onloadCallback = function() {
        //console.log("grecaptcha is ready!");
        grecaptcha.render('g-recaptcha', {
          'sitekey' : '6LczlgslAAAAAGZEArpVguIex0EbD6O7HQRouXiv',
          'callback' : verifyCallback,
          'theme' : 'light'
          //'theme' : 'dark'
        });
      };
      var verifyCallback = function(response) {
        //console.log(response);
        if (response != '') {
          $('.grecaptchaBtn').attr('disabled',false).toggleClass('grecaptchaBtn');
		  $('.google-recaptcha-mark').val('1');
        }
      };	
	  var resetCallback = function() {
		grecaptcha.reset('g-recaptcha');
	  };	
    </script>
<div id="g-recaptcha"></div>
<script src="https://www.google.com/recaptcha/api.js?onload=onloadCallback&render=explicit" async defer></script>
<input type="hidden" class="google-recaptcha-mark" name="ggrcm" value="0" />
