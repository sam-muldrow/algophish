			try {
				getDataLayer();
				dataLayer.page.pageInfo.level1 = "Home";
				dataLayer.page.pageInfo.pageType = "Home";
			} catch (e) {
				app.console.error("DataLayer Error: Unable to set page info", e);
			}
		</script>
