<script type="text/javascript">
function pagenavi(o, a) {
    var n = '1861';
    if (n == "") {
        return
    }
    var c = '1';
    var url = 'http://www.anige-sokuhouvip.com/page-1.html';
    if (url === "") url = '';
    var ext = ".html";
    var i = -1;
    if (url.slice(-5) === ext) {
        i = url.lastIndexOf("-");
    } else {
        ext = "";
        i = url.lastIndexOf("=")
    }
    var base = url.slice(0, i + 1);
    n = Number(n);
  
    var pn = c == 1 ? "<span>1</span>" :'<a href="' + base + "0" + ext + '">' + 1 + "</a>";
    var s, e, t, w = a - 4;
    if (a >= n) {
        s = 2;
        e = n - 1;
    } else if (c <= w / 2 + 2) {
        s = 2;
        e = a-2;
    } else if(c > n - (w / 2) - 1){
        s = n - a + 3 ; e = n-1;
    } else {
        s = c - Math.floor(w / 2); e = s + w - 1;
    }
    if (s > 2) pn += '<a href="' + base + ((t= Math.ceil(s / 2))-1) + ext + '" title="' + t + '" class="dot">…</a>';
    for (i = s; i <= e; i++) {
        if (i == c) {
            pn += "<span>" + i + "</span>"
        } else {
            pn += '<a href="' + base + (i - 1) + ext + '">' + i + "</a>"
        }
    }
    if (e < n - 1) pn += '<a href="' + base + ((t = Math.ceil((e + n) / 2))-1) + ext + '" title="' + t + '" class="dot">…</a>';  
    if(n > 1) pn += c == n ? "<span>" + n + "</span>" : '<a href="' + base + (n - 1) + ext + '">' + n + "</a>";
    document.getElementById(o).innerHTML = pn;
};
/* 第２引数は表示したいリンク数に変更する */
pagenavi("pagenavi", 11);
</script>



