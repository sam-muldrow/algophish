<script type="text/javascript">
jQuery(function($) {
 
var nav = $('#scroll'),
    offset = nav.offset();
 
$(window).scroll(function () {
  if($(window).scrollTop() > offset.top - 10) {
    nav.addClass('fixed');
  } else {
    nav.removeClass('fixed');
  }
});
 
});
</script>




