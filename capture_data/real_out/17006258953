<script>

    function toggleRrss(){
        $('#sideRrss').toggleClass('r0');
        $('#velo-rrss').toggleClass('velo-rrss');
    } 

    function buscar(query,seccioncod){
        $('#query').val(query);
        $('#seccioncod').val(seccioncod);
        $('#buscador_menu').submit();
    }

$(document).ready(function(){
    /* SECCIÓN: MENU */    
    var menu_nav = $("#menu-nav");
    
        // Mostrar Menú 
        $('body').on('click', '#menu-header', function(){
            menu_nav.addClass('active');
            return false;
        });
        
        // Cerrar menú
        $('body').on('click', '.menu-header-close', function(){
            menu_nav.removeClass('active');
        });

        $(document).click(function(event){
            if(menu_nav.hasClass('active')){
                menu_nav.removeClass('active');
            }    
        });

        //  $(menu_nav).click(function(event){
        //     event.stopPropagation();
        // });

        $('.toggler-icon').click(function(){
            icon = $(this).find("i");
            if(icon.hasClass("fa-plus")){
                icon.addClass("fa-minus").removeClass("fa-plus");
            }else{
                icon.addClass("fa-plus").removeClass("fa-minus");
            }
        })
       
        // Expandir subcategoría 
        $('#menu-nav .categorias-menu').on('click', '.categoria .toggler-icon', function(e){
           e.stopPropagation();
           $(this).parents('.categoria').addClass('expanded');
           $(this).parents('.categoria').find('.sub-categorias').slideDown(300);
        });
        
        // Collaparse
        $('#menu-nav .categorias-menu').on('click', '.categoria.expanded .toggler-icon', function(e){
            e.stopPropagation();
           $(this).parents('.categoria').removeClass('expanded');
           $(this).parents('.categoria').find('.sub-categorias').slideUp(300);
        });
        
        
        // Esconder la linea de separación al hacer over en un li
        $('body').on('mouseenter', '#menu-nav .nav-container nav .categorias-menu > li', function(){
            var categoriaAnterior = $(this).prev();
            
            if(typeof categoriaAnterior != 'undefined') {
                categoriaAnterior.addClass('no-separation-line');
            }
            
            $(this).addClass('no-separation-line');
        });
        
        // Mostrar la linea de separación al hacer over en un li
        $('body').on('mouseleave', '#menu-nav .nav-container nav .categorias-menu > li', function(){
            var categoriaAnterior = $(this).prev();
            
            if(typeof categoriaAnterior != 'undefined') {
                categoriaAnterior.removeClass('no-separation-line');
            }
            
            $(this).removeClass('no-separation-line');
            
            
        });
        
})
    </script>



<script>
// COMIENZO DE MODAL SUSCRIPCION
    let config = {
        'url': '/js/suscripcion_deu.json?v=0.1',
        'email': $("#correo").length != 0 ? $("#correo").val():'',
        'timeout': 5,
    }

</script>

