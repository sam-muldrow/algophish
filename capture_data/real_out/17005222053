                <script type="text/javascript">
    var userLoggedIn = false;
    var loginUrl = "/auth/login";
    var logoutUrl = "/auth/logout";
    var registerUrl = "/account/register";
    var userUrl = "/auth/user?slide=false";

$(document).ready(function(){
    (function () {
        if (window.buienradar.enableOneAccount.toLowerCase() === 'true') {
            $(".login").show();

            getUser()
                    .then(function (user) {
                        if (userLoggedIn) {
                            $(".account-settings-link").attr("href", window.buienradar.openId);
                            $('.login-required').hide();
                            $('.login-not-required').show();

                            const hasBrPlus = cookieManager.getCookie("br-plus");

                            if (hasBrPlus === 'true') {
                                $('.br-plus-promotions').removeClass("display-none-voordeelacties")
                                $(".br-plus-promotions").attr("href", "/weerzine/brplusvoordeelacties");
                            }
                        }
                        else {
                            $('.login-not-required').hide();
                            $('.login-required').show();
                            $('.br-plus-promotions').hide();
                            sessionStorage.removeItem('UID');
                            sessionStorage.removeItem('claims');
                        }
                    }).catch(function (err) {
                        console.error(err);
                    });
        }
        else {
            $(".login").hide();
        }
    })();

    async function getUser() {
        var req = new Request(userUrl, {
            headers: new Headers({
                'X-CSRF': '1'
            })
        })

        var response = await fetch(req);
        if (response.ok) {
            userLoggedIn = true;

            let claims = await response.json();

            let logoutUrlClaim = claims.find(claim => claim.type === 'bff:logout_url');
            if (logoutUrlClaim) {
                logoutUrl = logoutUrlClaim.value;
            }

            sessionStorage.setItem('claims', JSON.stringify(claims));
        } else if (response.status === 401) {
            userLoggedIn = false;
            deleteCookie("br-plus")
        }

        return response;
    }

    $(document).on("click", function (event) {

        var $trigger = $(".login");

        if ($trigger !== event.target && !$trigger.has(event.target).length) {
            $(".dropdown-content").removeClass("display-block")
            $(".arrow-up").hide();
            $(".arrow-down").show();
        }
    });
});

    function deleteCookie(name) {
        document.cookie = name + "=;path=/;expires=Thu, 01 Jan 1970 00:00:01 GMT";
    }

    function login() {
        deleteCookie("br-plus")
        window.location = loginUrl;
    }

    function logout() {
        deleteCookie("br-plus");
        sessionStorage.removeItem('UID');
        sessionStorage.removeItem('claims');
        window.location = logoutUrl;
        window.buienradar.claims = "";
    }

    function register() {
        deleteCookie("br-plus")

        if (window.buienradar.enableOidcRegistration.toLowerCase() === 'true') {
            window.location = window.buienradar.openIdAuthority + "/register" + window.buienradar.returnUrl;
        }
        else {
            window.location = registerUrl;
        }
    }

    function openLogin() {
        if ($(".dropdown-content").hasClass("display-block"))
        {
            $(".dropdown-content").removeClass("display-block");
        }
        else {
            $(".dropdown-content").addClass("display-block");
        }

        if($(".arrow-down").is(":visible")){
            $(".arrow-up").show();
            $(".arrow-down").hide();
        }
        else{
            $(".arrow-up").hide();
            $(".arrow-down").show();
        }
    }

    function openMyPhotosAndVideos(){
        window.location = "/foto-en-video/mijn-fotos-en-videos/" + window.gigyaUserInfo.newZuluID;
    }
</script>

