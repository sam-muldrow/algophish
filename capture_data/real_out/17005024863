(function() {
var searchInput = document.getElementById('fs');
searchInput.addEventListener('focus', lazyloadAutocomplete);
function lazyloadAutocomplete(event) {
_gh.ajax.loadGHPak('autocomplete').then(function(autocomplete) {
document.getElementById('autocomplete-skeleton').innerHTML = autocomplete.html_patty;
autocomplete.init();
});
searchInput.removeEventListener('focus', lazyloadAutocomplete);
}
})();
</script>
