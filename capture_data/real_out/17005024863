<script>    var d = window.datadfr || [];
    for (var i = 0; i < d.length; i++) {
        var n = document.createElement(d[i].node);
        n.rel = d[i].rel;
        n.href = d[i].href;
        n.type = d[i].type;
        window._headEl = window._headEl || document.getElementsByTagName('head')[0];
        if (d[i].dest === 'head') window._headEl.appendChild(n);
        else if (d[i].dest === 'head_weak') window._headEl.insertBefore(n, window._headEl.querySelector('script'));
    }
    var dcss = document.querySelectorAll('.lddcss');
    [].forEach.call(dcss, function(css) { css.disabled = false; });
</script></body></html>
