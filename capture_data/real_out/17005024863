[...document.querySelectorAll('.tarife-widget .quantity-stepper .button')].forEach(function (b) {
b.addEventListener('click', function () {
let newV =
parseInt(b.parentElement.querySelector('.textfield__input').value) + parseInt(b.getAttribute('data-incr'));
newV = Math.min(Math.max(newV, 1), 9);
b.parentElement.querySelector('.textfield__input').value = newV;
document.getElementById('tarife-at__strom-kwh').value = (newV + 1) * 1000;
});
});
document.getElementById('tarife-at__m2').addEventListener('input', function (event) {
let m2 = Math.min(Math.max(event.target.value, 1), 1000);
let newV = m2 * 140;
document.getElementById('tarife-at__gas-kwh').value = newV;
});
