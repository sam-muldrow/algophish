<script type="text/plain" data-cm="C0004">
var adRefreshTimeout;
function refreshAds() {
clearTimeout(adRefreshTimeout);
if (window.googletag && typeof window.googletag.pubads === 'function') googletag.pubads().refresh();
}
window.googletag = window.googletag || {cmd: []};
googletag.cmd.push(function() {
var ghwidth = window.document.body.offsetWidth;
googletag.pubads().enableSingleRequest();
googletag.pubads().collapseEmptyDivs();
if (window._gh_gpa_price_range) { googletag.pubads().setTargeting('preis', window._gh_gpa_price_range); }
googletag.pubads().addEventListener('slotRenderEnded', function(event) {
if (!event.isEmpty) { document.getElementById(event.slot.getSlotElementId()).classList.add('gh-ad__loaded'); }
});
googletag.enableServices();
if (ghwidth > 1024) {googletag.defineSlot('/6514/www.geizhals.at'+(window._gh_top_category_path || '/homepage'), [[300,250],[300,600]], 'div-gpt-ad-start').addService(googletag.pubads());
googletag.display('div-gpt-ad-start');}if (ghwidth > 1400) {googletag.defineSlot('/6514/www.geizhals.at'+(window._gh_top_category_path || '/homepage'), [[190,42],[190,43]], 'div-gpt-ad-takeover').addService(googletag.pubads()).setTargeting('pos', 'top');
googletag.display('div-gpt-ad-takeover');}if (ghwidth > 1700) {googletag.defineSlot('/6514/www.geizhals.at'+(window._gh_top_category_path || '/homepage'), [[120,120]], 'div-gpt-ad-pagepeel').addService(googletag.pubads());
googletag.display('div-gpt-ad-pagepeel');} else if (ghwidth > 1060) {googletag.defineSlot('/6514/www.geizhals.at'+(window._gh_top_category_path || '/homepage'), [[80,80]], 'div-gpt-ad-pagepeel').addService(googletag.pubads());
googletag.display('div-gpt-ad-pagepeel');}
adRefreshTimeout = window.setTimeout(refreshAds, 40*1000);
});
</script>
