<script type="module">
window.ghpakLazyload("startpage").then(function(i) {
window.ghpak = window.ghpak || [];
window.ghpak.startpage = i;
i.init();
});
window.ghpakLazyload('galleryview').then(function(i) {
window.galleryview = i;
});
window.ghpakLazyload('dots-dropdown').then(function (i) {
window.dotsDropdown = i;
});
(function () {
if (sessionStorage.getItem('udelNotificationText') != null) {
let notificationText = sessionStorage.getItem('udelNotificationText');
sessionStorage.removeItem('udelNotificationText');
window.lazyloadDeferredUtils().then(function (utils) {
utils.showNotification(notificationText, { type: 'info' });
});
}
})();
</script>
