return performance.now()}function forEach(list,cb){if(!cb||!list)return;var len=list.length||0;for(var i=0;i<len;i++){cb(list[i],i)}}function someList(list,cb){if(!cb||!list)return;var len=list.length||0;for(var i=0;i<len;i++){if(cb(list[i],i)){return true}}return false}function pureUrl(str){return str?str.replace(/^https?:\/\//i,"//"):str}})();

  const isPreHot = window.location.href.indexOf('lzd_open_type=pre_hot') >= 0;
  if (!isPreHot) {
    window.fspTracker.install();
    window.fspTracker.listeners = [
      function (fsp) {
        window.gcpIsDebug && console.log('[fsp-report-in-solution]', fsp); // timestamp, cost, domSize
        window.__FSP = fsp;
      },
    ];

    window.gcpIsDebug && console.log('[fsp-registered-in-solution]');
  }
