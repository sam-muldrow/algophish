        <script type="text/javascript">
            var carouselDebouncer;

            $(function () {
                if ($(window).width() >= 768) {
                    initSlick();
                }

                $(window)
                    .resize(function () {
                        clearTimeout(carouselDebouncer);
                        carouselDebouncer = setTimeout(initSlick, 500);
                    });
            });

            var slickInited = false;

            function initSlick() {
                if ($(window).width() >= 768) {
                    if (!slickInited) {
                        $.ajax({
                            url: '/api/HomePageHeroCarousel',
                            data: { id: $('.component-hero-carousel').data('id') },
                            success: function (result) {
                                $('div.hero-container-area').html(result);

                                $('.component-hero-carousel.active')
                                    .slick({
                                        autoplay: true,
                                        autoplaySpeed: 7000,
                                        slidesToShow: 1,
                                        arrows: true,
                                        centerMode: true,
                                        dots: true,
                                        speed: 600,
                                        easing: 'swing',
                                        centerPadding: '0px',
                                        onBeforeChange: function (index) { },
                                        onInit: function (index) { },
                                        prevArrow: '<span class="screenreaderOnly">Previous slide</span><button data-role="none" class="slicks-prev" id="hero-prev" aria-label="Previous slide"></button>',
                                        nextArrow: '<span class="screenreaderOnly">Next slide</span><button data-role="none" class="slicks-next" id="hero-next" aria-label="Next slide"></button>'
                                    });

                                $('.component-hero-carousel ul.slick-dots').before('<button id="pause-carousel" aria-label="Pause Slides" tabindex="0" title="Pause Slides"><img src="Images/Icon_pause.png" alt="Pause Slides" /></button>');
                                $('#pause-carousel')
                                    .click(function () {
                                        if ($('#pause-carousel').attr('aria-label') === 'Pause Slides') {
                                            $('.component-hero-carousel.active').slick('slickPause');
                                            $('#pause-carousel').attr('aria-label', 'Play Slides').attr('title', 'Play Slides').html('<img src="Images/Icon_play.png" alt="Play Slides" />');
                                        }
                                        else {
                                            $('.component-hero-carousel.active').slick('slickPlay');
                                            $('#pause-carousel').attr('aria-label', 'Pause Slides').attr('title', 'Pause Slides').html('<img src="Images/Icon_pause.png" alt="Pause Slides" />');
                                        }
                                    });
                                slickInited = true;
                            }
                        });
                    }
                }
                else {
                    $('.component-hero-carousel.active').filter('.slick-slider').slick('unslick');
                    $('#pause-carousel').remove();
                    slickInited = false;
                }
            }
        </script>
            
