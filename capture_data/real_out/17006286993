    <script type="text/javascript">
    $(function() {
        becu.loginSubmitOpenaccount = 'https://onlinebanking.becu.org/BECUBankingWeb/Accounts/OpenNewAccounts.aspx';
        becu.loginSubmitApply = 'https://onlinebanking.becu.org/BECUBankingWeb/Loans/LoanApplication.aspx';
