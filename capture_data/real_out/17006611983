					//è·åcookieï¼åæ°nameæå®è¦è·åçcookieçåç§°
					function getCookie(name) {
						var start = document.cookie.indexOf(name + "="); //å¾å°cookieå­ç¬¦ä¸²ä¸­çåç§°
						var len = start + name.length + 1; //å¾å°ä»èµ·å§ä½ç½®å°ç»æcookieä½ç½®çé¿åº¦
						//å¦æèµ·å§æ²¡æå¼ä¸nameä¸å­å¨äºcookieå­ç¬¦ä¸²ä¸­ï¼åè¿ånull
						if ((!start) && (name != document.cookie.substring(0, name.length))) {
							return null;
						}
						if (start == -1) return null; //å¦æèµ·å§ä½ç½®ä¸º-1ä¹ä¸ºnull
						var end = document.cookie.indexOf(';', len); //è·åcookieå°¾é¨ä½ç½®
						if (end == -1) end = document.cookie.length; //è®¡ç®cookieå°¾é¨é¿åº¦
						return unescape(document.cookie.substring(len, end)); //è·åcookieå¼
					}
					function analysis(){
					console.log(getCookie("un_ticket_mofcom_"));
						$.ajax({
							async:false, 
							type: "GET",
							dataType:"jsonp",
							jsonpCallback:"uc",
