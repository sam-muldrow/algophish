	<script type="text/javascript">
    $("#searchValue").autocomplete({
	   minLength: 1,
	   source: function( request, response ) {
		   $.ajax({
				url : "http://search.mofcom.gov.cn/getTips",
				type : "post",
				dataType : "json",
				crossDomain:true, //è®¾ç½®è·¨åä¸ºtrue
				contentType: 'application/json',
				data :JSON.stringify({"searchCode":"se2022041901","keyword":$("#searchValue").val(),"size":10}),
			   success: function( data ) {
					 console.log(data);
					 response( $.map( data.dataList, function( item ) {
						   return {
							 label: item,
							 value: item
						   }
					 }));
			   }
		  });
	   },
	   focus: function( event, ui ) {
		   //$("#keywords").val( ui.item.value );
			 return false;
		   },
	   select: function( event, ui ) {
		   $("#searchValue").val( ui.item.value );
		   return false;
	   }
	});
		// $(".iMenu_l").fixedScroll({
		// 	navEls: $("#LoutiNav li"),
		// 	hookEls: $('.main .iColumn')
		// });
		var oNav = $('#LoutiNav');//å¯¼èªå£³
		var aNav = oNav.find('li');//å¯¼èª
		var aDiv = $('.main .iColumn');//æ¥¼å±
		var oTop = $('#goTop');
		//åå°é¡¶é¨
		$(window).scroll(function(){
			var winH = $(window).height();//å¯è§çªå£é«åº¦
			var iTop = $(window).scrollTop();//é¼ æ æ»å¨çè·ç¦»

			//if(iTop>=$('#header').height()){
			if(iTop>=710){
				oNav.fadeIn();
				oTop.fadeIn();
				//é¼ æ æ»å¨å¼æ¹å
				aDiv.each(function(){

					if(winH+iTop - $(this).offset().top>winH/2){
						// console.log($(this).find('.iTitle_01 em').text(),$(this).offset().top);
						if($(this).height()<winH/2&&$(this).height()>=$(this).offset().top){
							// console.log($(this).find('.iTitle_01 em').text(),$(this).height());
						}
						aNav.removeClass('active');
						aNav.eq($(this).index()).addClass('active');
					}
				})
			}else{
				oNav.fadeOut();
				oTop.fadeOut();
			}
		})
		//ç¹å»topåå°é¡¶é¨
		oTop.click(function(){
			$('body,html').animate({"scrollTop":0},500)
		})
		//ç¹å»åå°å½åæ¥¼å±
		aNav.click(function(){
			var that = $(this),
			t = aDiv.eq($(this).index()).offset().top;
			$('body,html').animate({"scrollTop":t},500,function(){
				that.addClass('active').siblings().removeClass('active');
			});
		});

		// æ¶æ¿è¦é»ãæ°é»åå¸ åæ¢
		jQuery(".j-slideTxtBox").slide({titCell:".hd em",mainCell:".bd",trigger:"mouseover",titOnClassName:'z-crt'});

