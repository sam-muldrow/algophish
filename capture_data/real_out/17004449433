                <script type="text/javascript">
                    $(document).ready(function () {

                        $("#mostused_apps").click(function () {
                            if ($(this).height() == 35) {
                                $("#mostused_apps").attr('style', 'height: auto !important');
                                $(".plusmenuicon").css('transform', 'rotate(0deg)');
                            }
                            else {
                                $("#mostused_apps").attr('style', 'height: 35px !important');
                                $(".plusmenuicon").css('transform', 'rotate(90deg)');
                            }
                        });

                        var sbox = false;
                        $(".fieldset_InfoBox").click(function () {
                            if ($(this).height() == 40) {
                                $(".fieldset_InfoBox").attr('style', 'height: auto !important');
                                $(".infomenuicon").css('transform', 'rotate(0deg)');
                                sbox = true;
                            }
                            else if (sbox == true) {
                                $(".fieldset_InfoBox").attr('style', 'height: 40px !important');
                                $(".infomenuicon").css('transform', 'rotate(90deg)');
                            }
                        })

                    })
                </script>
