            <div id="middle">
                <div class="box4search">
                    <script type="text/javascript">
                        $(document).ready(function () {
                            $('#txtSearchAjaxBox').keyup(function (e) {

                                delayKeyUp(function () {  //delay of 350ms.

                                    var keyword = $('#txtSearchAjaxBox').val();
                                    keyword = keyword.replace(/'/g, '').replace(/\\/g, '');

                                    if (keyword.length >= 3) {

                                        $('#Loading').show();
                                        $('#LoadingFix').hide();

                                        ajdata = "{'search_keyword':'" + keyword + "'}";

                                        $.ajax({

                                            type: "POST",

                                            url: "/Default.aspx/SearchKeyword",

                                            data: ajdata,

                                            contentType: "application/json; charset=utf-8",

                                            dataType: "json",

                                            success: function (result) {
                                                $("#SearchResult").empty();
                                                $("#SearchResult").append("<div style='text-align:center;padding:10px;border-bottom:1px solid #ccc;'><a target='_blank' style='color:#dbbb3b;' href='https://www.google.com/search?q="
                                                    + keyword + "+site%3Asoftgozar.com'>جـسـتـجـوی بیـشـتر</a> | <a style='color:#ef5350;' href='https://www.softgozar.com/prosearch'>جـسـتـجـوی پیشرفته</a></div>");

                                                for (var i = 0; i < result.d.length; i++) {
                                                    $("#SearchResult").append("<div class='ResultSearchRow'><a style='color:#434343' target='_blank' href='/" +
                                                        result.d[i].En_Url + "_/" + "'><div style='float: right;'><img src='https://www.softgozar.com/Image/Softwares/Icon/" +
                                                        result.d[i].IconImage + "' alt='" + result.d[i].Title + "' title='" + result.d[i].Title + "' /></div>" +
                                                        "<div class='SearchResultRowsItem'>" + result.d[i].Title + "</div>" +
                                                        "<div class='SearchResultRowsItem2'>" + result.d[i].Summary + "</div></a></div>");
                                                }

                                                $("#SearchResult").fadeIn();
                                                $('#Loading').hide();
                                                $('#LoadingFix').show();
                                            },
                                            error: function (xhr, ajaxOptions, thrownError) {
                                                alert(xhr.responseText);
                                            }
                                        });
                                    }
                                    else {
                                        $("#SearchResult").fadeOut();
                                        $('#Loading').hide();
                                        $('#LoadingFix').show();
                                    }

                                }, 350);
                                return false;

                            });

                            $(document).click(function (event) {
                                if (!$(event.target).closest('#SearchResult').length) {
                                    if ($('#SearchResult').is(":visible")) {
                                        $('#SearchResult').hide()
                                    }
                                }
                            })

                            $(document).keyup(function (e) {
                                if (e.keyCode == 27) {
                                    $('#SearchResult').hide(); // esc
                                    $('#txtSearchAjaxBox').val('');
                                }
                            });

                            $('#txtSearchAjaxBox').keyup(function (e) {

                                var keyword1 = $('#txtSearchAjaxBox').val();
                                keyword1 = keyword1.replace(/'/g, '').replace(/\\/g, '');

                                if (e.keyCode == 13 && keyword1.length >= 3) {
                                    window.location.replace("/search?q=" + keyword1 + "#divH1");
                                    $('#txtSearchAjaxBox').val('');
                                    $('#txtSearchAjaxBox').attr('placeholder', 'در حال جستجو...');
                                    $('#txtSearchAjaxBox').attr('disabled', 'disabled');

                                    $('#txtSearchAjaxBox').each(function () {
                                        var elem = $(this);
                                        setInterval(function () {
                                            if (elem.css('opacity') == '0.9') {
                                                elem.css('opacity', '0.4');
                                            } else {
                                                elem.css('opacity', '0.9');
                                            }
                                        }, 300);
                                    });
                                }
                            });

                            $('#srchicon').click(function () {
                                var keyword1 = $('#txtSearchAjaxBox').val();
                                keyword1 = keyword1.replace(/'/g, '').replace(/\\/g, '');
                                window.location.replace("/search?q=" + keyword1 + "#divH1");

                            });

                            var delayKeyUp = (function () {
                                var timer = 0;
                                return function (callback, ms) {
                                    clearTimeout(timer);
                                    timer = setTimeout(callback, ms);
                                };
                            })();

                        });


                    </script>
                    <div class="SearchBox">
                        
                        <div id="srchicon">
                            <img src="/Image/Base/icons8-search.svg" alt="جستجو" />
                        </div>
                        <input name="ctl00$txtSearchAjaxBox" type="text" id="txtSearchAjaxBox" class="TextBox_MainSearch" placeholder="جستجو در نرم افزارهای سایت ..." autocomplete="off" onkeydown="if(event.which || event.keyCode){if ((event.which == 13) || (event.keyCode == 13)) {document.getElementById(&#39;txtSearchAjaxBox&#39;).click();return false;}} else {return true}; " />
                        &nbsp;
                    </div>
                    <div id="SearchResult"></div>
                </div>
