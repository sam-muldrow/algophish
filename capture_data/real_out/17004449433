        <script type="text/javascript">
            $(document).ready(function () {

                $(".menubtn").click(function myfunction() {
                    $('#sidebar').css("right", "0px");
                    $('#backgray').show();
                    $('body').css('overflow', 'hidden');
                });
                $("#backgray").click(function myfunction() {
                    $('#sidebar').css("right", "-270px");
                    $('#backgray').hide();
                    $('body').css('overflow', 'initial');
                });

                $('.closeMenu').click(function () {
                    $('#sidebar').css("right", "-270px");
                    $('#backgray').hide();
                    $('body').css('overflow', 'initial');
                });

                $(document).scroll(function () {
                    var scroll = $(this).scrollTop();
                    var topDist = $(".MainBanner").position();
                    if (scroll > topDist.top) {
                        $(".MainBanner").css({ "display": "none" });
                        $('#respons-header').css({
                            "position": "fixed", "top": "0", "z-index": "997",
                            "display": "block"
                        });

                    } else {
                        $(".MainBanner").css({ "display": "block" });
                        $('#respons-header').css({
                            "position": "", "top": "", "z-index": "",
                            "display": "none",
                            "background-image": ""
                        });
                    }

                });

            });

        </script>
