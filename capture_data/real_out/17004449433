            <script type="text/javascript">
                $(document).ready(function () {
                    $(".searchheadres").click(function myfunction() {
                        $('.logoheadres').fadeOut();
                        $('#inp-search-res').fadeIn();
                        $('#inp-search-res').css("width", "67%");
                        $('#inp-search-res').focus();
                        $('#loadimg').css('animation', 'spin 3s linear infinite');

                        if ($('#inp-search-res').css({ 'width': '67%' }) && $('#inp-search-res').val() != '' && $('#inp-search-res').val().length > 2) {
                            $('#inp-search-res').fadeOut();
                            $('#waitforsearch').fadeIn();
                            window.location.replace("/search?q=" + $('#inp-search-res').val());
                        }
                    });


                    $('#inp-search-res').keyup(function (e) {
                        if (e.keyCode == 13 && $(this).val() != '' && $(this).val().length > 2) {
                            $('#waitforsearch').fadeIn();
                            $('#inp-search-res').fadeOut();
                            window.location.replace("/search?q=" + $(this).val());
                        }
                    });

                    $('#MasterTable').bind('touchstart click', function () {
                        $('#inp-search-res').css({ "display": "none" });
                        if ($(window).width() < 800) {
                            $('.logoheadres').css({ "display": "block" });
                        }
                        else $('.logoheadres').css({ "display": "none" });
                    })
                });
            </script>
