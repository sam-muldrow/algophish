            for (var i = 0; i < newJsArr.length; i++) {
              let runtimejs = newJsArr[i];
              setTimeout(function() {
                loadJSWithPromise(runtimejs, true);
              }, (_wtCookie == null) ? 1 * 500 : 0);
            }
            setTimeout(function() {
              for (var i = 0; i < newJsLazyArr.length; i++) {
                let timeout = (_wtCookie == null) ? 0.5 * 2000 : 0;
                if (newJsLazyArr[i].indexOf('player') > -1) {
                  timeout = (_wtCookie == null) ? 2 * 2000 : 2000;
                }
                let src = newJsLazyArr[i];
                if (timeout) {
                  setTimeout(function() {
                    loadJSWithPromise(src, true)
                  }, timeout);
                } else {
                  loadJSWithPromise(src, true)
                }
              }
            }, (_wtCookie == null) ? 1 * 2000 : 1000);
            // for unsupported audio format like x-mpegurl we need to load this script before shaka-palyer
            setTimeout(() => loadJSWithPromise('/mux-min.js', true, null, document.getElementsByTagName("script")[0]), 5900)
          } catch (e) {
            console.log(e);
          } 
        });
        try {
            registerSync = async () => {
                try {
                    const registration = await navigator.serviceWorker.ready;
                    await registration.sync.register('load-prefetch-data');
                    try {
                        var minIntervalFromForm = 1000 * 60 * 60 * 12;
                        await registration.periodicSync.register('load-periodic-data', {
                            minInterval: minIntervalFromForm
                        });
                    } catch (e) {}
                } catch (e) {
                    console.log(e.message);
                }
            }
        } catch (e) {
            console.log(e.message);
        }
        if ('serviceWorker' in navigator) {
            window.addEventListener('load', () => {
                navigator.serviceWorker.register('/sw.js');
            });
            setTimeout(function() {
                registerSync();
            }, 30000);
            try {
                if ('serviceWorker' in navigator) {
                    navigator.serviceWorker.getRegistrations().then(function(registrations) {
                        for (let registration of registrations) {
                            if (registration.active.scriptURL == 'https://gaana.com/serviceworker.js') {
                                registration.unregister();
                            }
                        }
                    }).catch(function(err) {
                        console.log('Service Worker registration failed: ', err);
                    });
                }
            } catch (e) {
                console.log(e);
            }
        }
