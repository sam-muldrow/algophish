        var loadJSWithPromise = function(src, isAsync, cb, ref) {
          ref = ref || document.getElementById("webjs");
          var script = document.createElement("script");
          script.src = src;
          script.defer = isAsync ? isAsync : false; // made async to defer for LCP improvement on 17 December 2021
          script.crossorigin = "anonymous";
          ref.parentNode.insertBefore(script, ref);
          if (cb && typeof(cb) === "function") {
            script.onload = cb;
          }
          var promise = new Promise((resolve, reject) => {
            setTimeout(() => {
              resolve(script)
            }, 100)
          })
          return promise;
        };
