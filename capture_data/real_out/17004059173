                element.defer = true;
                element.onload = function() {
                    jssoObj = new JssoCrosswalk('gaana.com', 'WAP', 'https://jsso.indiatimes.com', 'https://socialappsintegrator.indiatimes.com');
                }
                document.body.appendChild(element);
            } catch (e) {
                console.log(e.message);
            }
        }
