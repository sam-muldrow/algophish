    var page = "Default";
    $(document).on('click', '.sb-close', function () {
        try {
            dataLayer.push({ 'event': "click_standard", 'eventCategory': page, 'eventAction': 'Smartbanner', 'eventLabel': 'Fermer' }); 
        } catch (e) {
            if (srpComponent.options.debug == true) {
                console.log(e);
            }
        }
    });
    $(document).on('touchstart', '.sb-icon', function () {
        try {
            dataLayer.push({ 'event': "click_standard", 'eventCategory': page, 'eventAction': 'Smartbanner', 'eventLabel': 'Ouvrir' }); 
        } catch (e) {
            if (srpComponent.options.debug == true) {
                console.log(e);
            }
        }
    });
    $(document).on('touchstart', '.sb-info', function () {
        try {
            dataLayer.push({ 'event': "click_standard", 'eventCategory': page, 'eventAction': 'Smartbanner', 'eventLabel': 'Ouvrir' }); 
        } catch (e) {
            if (srpComponent.options.debug == true) {
                console.log(e);
            }
        }
    });
    $(document).on('touchstart', '.sb-button', function () {
        try {
            dataLayer.push({ 'event': "click_standard", 'eventCategory': page, 'eventAction': 'Smartbanner', 'eventLabel': 'Ouvrir' }); 
        } catch (e) {
            if (srpComponent.options.debug == true) {
                console.log(e);
            }
        }
    });
</script>


    



