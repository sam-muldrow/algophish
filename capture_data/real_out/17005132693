    <script src="https://d11l21ofu1mzsa.cloudfront.net/assets/application-2bcdb5f5d08fb212b2c0502bba5c20ce70cebe05453bd66a797aaec3b63c2ed9.js" defer="defer"></script>
    




<script>
  const ready = fn => {
    if (document.readyState != 'loading') {
      fn();
    } else {
      document.addEventListener('DOMContentLoaded', fn);
    }
  }
</script>

    <script>
  function isDomainEqualSogi() {
    const urlString = document.location.hostname.toString();
    const domain = urlString.split(".")[1];
    return domain === "sogi";
  }

  function ga4Event(parameters) {
    if (isDomainEqualSogi()) {
      window.dataLayer = window.dataLayer || [];
      window.dataLayer.push(parameters);
    }
  }

  ready(() => {

    document.addEventListener("click", (event) => {
      const selector = '[event-name="sogi.click"]';
      const targetElement = event.target.closest(selector);

      const isTarget = !!targetElement?.matches(selector);

      if (isTarget) {
        ga4Event({
          'event':          targetElement.getAttribute('event-name'),
          'page_section':   targetElement.getAttribute('event-page-section'),
          'page_function':  targetElement.getAttribute('event-page-function'),
          'page_element':   targetElement.getAttribute('event-page-element'),
          'element_target': targetElement.getAttribute('event-element-target'),
        });
      }
    });

  });
</script>


    <script>
      googletag.cmd.push(function() {
        search_ad = googletag.defineSlot('/1001754/13', [280, 14], 'div-gpt-ad-1596433653296-0').addService(googletag.pubads());
        googletag.pubads().enableSingleRequest();
        googletag.pubads().collapseEmptyDivs();
        googletag.enableServices();
      });

      function search_dfp(){
        googletag.cmd.push(function() {
          googletag.pubads().refresh([search_ad]);
        });
      };

      ready(function () {
        $('#cellphone_compare').attr('class', "tab-pane active");
        setTimeout(function() {
          var TAd = $.merge(GlobalAd,SingleAd);
          var LackAd = new Array();
          for(i=0;i<(TAd.length - 1);i++){
            if(typeof(TAd[i]) == "undefined"){
              LackAd.push(TAd[i])
            }
          }
          console.log("LackAd is %s",LackAd.length )
          if(LackAd.length > 0){
            for(i=0;i<(LackAd.length - 1);i++){
              googletag.pubads().refresh([LackAd[i]])
            }
          }

          setTimeout(function() {
            if (window.billboard){ billboard(); }
            if (window.adv_one){ adv_one(); }
            if (window.adv_two){ adv_two(); }
          }, 900);
        },900);
      });

    </script>
    <!-- Single -->
    

  <script>
    ready(function () {
      // responsiveImg
      $('.img-responsive').responsImg({
          breakpoints: {
              mobile: 1,
              tablet: 768,
              desktop: 992
          }
      });

      // lazy loading
      $('.js-lazy').lazyload({threshold:1000,effect : "fadeIn" });
      $('#new_arr').find('img').lazyload({container: $("#new_arr"),effect : "fadeIn" });
      $('#top_mobi').find('img').lazyload({container: $("#top_mobi"),effect : "fadeIn" });
      $('#top_tab').find('img').lazyload({container: $("#top_tab"),effect : "fadeIn" });
      $('#top_oth').find('img').lazyload({container: $("#top_oth"),effect : "fadeIn" });
      $('#comp_series').find('img').lazyload({container: $("#comp_series"),effect : "fadeIn" });
      $('#comp_popular').find('img').lazyload({container: $("#comp_popular"),effect : "fadeIn" });

      function getCompareEcInfoLazyLoad(elements) {
        const options = {
        };

        const callback = (entries, observer) => {
          entries.forEach(entry => {
            if (entry.isIntersecting) {

              observer.unobserve(entry.target);

              const product_id = $(entry.target).attr('id').split('_')[1];
              getCompareEcInfo(product_id);
            }
          });
        };

        const observer = new IntersectionObserver(callback, options);

        elements.forEach(el => {
          observer.observe(el);
        });
      }

      function getEcInfoLazyLoad(elements) {
        const options = {
        };

        const callback = (entries, observer) => {
          entries.forEach(entry => {
            if (entry.isIntersecting) {

              observer.unobserve(entry.target);

              const product_id = $(entry.target).attr('id').split('_')[1];
              getEcInfo(product_id);
            }
          });
        };

        const observer = new IntersectionObserver(callback, options);

        elements.forEach(el => {
          observer.observe(el);
        });
      }

      getCompareEcInfoLazyLoad( document.querySelectorAll(".compare_price") );
      getEcInfoLazyLoad( document.querySelectorAll(".ec_price") );

    });
  </script>

  <script>

    function fetchText(url, init) {
      return fetch(url, init)
              .then(function(response) { return response.text() })
              .catch(function(err)     { console.log('fetch failed', err) })
    }

    function promisesForElement(el) {
      var resources = JSON.parse(el.dataset.resources);

      var fetchInit = {
        headers: new Headers({
          'Accept': 'text/html',
        })
      };

      return resources.map(function(resource) {
        return fetchText(resource, fetchInit);
      });
    }

    function handlePromises(el, promises) {
      return Promise.all(promises)
        .then(function(results) {
          results.forEach(function(text) {
            var child = document.createRange().createContextualFragment(text);
            el.appendChild(child);
          })
        });
    }

</script>


  <script>
    ready(function() {
      var el = document.querySelector('#news_guide');

      if (el) {

        lazyloadFor([el], (target) => {
          var promises = promisesForElement(target);
          handlePromises(target, promises)
            .then(() => { target.children[0].remove() });
        });

      }
    });

  </script>


  <script>
    ready(function() {

      var collection = document.querySelectorAll(".shopping-guide_product");

      if (collection.length > 0) {

        lazyloadFor(collection, (target) => {
          handleProductInfos(target.dataset.productId)
        });

      }

    });
  </script>



