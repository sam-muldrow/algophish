<section  data-anchor="#homepage---v5---march-2022---hero" data-entity-id="55816" class="homepage-hero component-theme-dark anchorable">
  
  
  <div class="homepage-hero__wrapper">
    <div class="homepage-hero__panels">
                  


<div  data-anchor="#homepage---v5---march-2022---hero-winning-back-public-trust-in-automated-driving" data-entity-id="160621" data-content-id="160621" class="homepage-hero-panel component-theme-dark anchorable">
  <div class="homepage-hero-panel__media">
                
  

  <div data-entity-id="198966" class="paragraph paragraph--type--image-or-video paragraph--view-mode--disable-lazy-load">
              <div class="responsive-image">
  
  
  
