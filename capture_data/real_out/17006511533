        function runtwitter(){
            if ($("div.twitch_trigger").length > 0){
                if($('div.twitch_trigger').visible() == true){
                    $('div.twitch').each(function(index, value){
                        var tw = $(this).html();
                        if( !$(this).find('iframe').length ){
                            $(this).html('<iframe src="https://www.twitch.tv/'+tw+'/embed" frameborder="0" scrolling="no" height="390" width="640"></iframe>')
                        }
                    });
                    return true;
                }
            }
            return false;
        }
        $(document).ready(function() {
            $window = $(window);
            var aside = $('#aside').height() + 23;
            var offset = $('#aside').offset().top
            var cure = $(window).scrollTop();
            var maxWidth = ''; // Max width for the image
            var ratio = '';  // Used for aspect ratio
            var width = '';    // Current image width
            var height = '';  // Current image height
            var show_aside = true;

            /*if(cure < aside+offset){
            $('#main_tabs-1 img').each(function() {
            if ($(this).width() > 727)
            {
            $(this).width(727);
            }
            });
            } */
            if(cure > aside+offset){
                $('#aside').hide();
                $("#main_tabs-1").css("width", "100%");
                $('#main_tabs-1 img').each(function() {
                    //$(this).css('width', 'auto');
                    $(this).css('max-width', '950px');
                });
                show_aside = false;
            }
            $('div#main_tabs > ul > li > a').click(function(){
                if($(this).data('mark') != 'main'){
                    $('#aside').hide();
                }else{
                    $('#aside').show();
                }
            });
            //var a = false;
            //runtwitter();
            $window.scroll($.throttle( 250, function () {
                var selected = $("#main_tabs").tabs('option', 'active');
                //if(a == false){
                //    a = runtwitter();
                //}
                if(selected == 0){
                    cure = $(window).scrollTop()
                    if(cure >= aside+offset){
                        if(show_aside == true){
                            $('#aside').hide();
                            $("#main_tabs-1").css("width", "100%");
                            /*$('#main_tabs-1').animate({
                                width: "100%"
                                }, 500, function() {

                            });**/
                            $('#main_tabs-1 img').each(function() {
                                //$(this).css('width', 'auto');
                                $(this).width('');
                                $(this).css('max-width', '950px');
                            });
                            show_aside = false;
                        }
                    }
                    if($('#aside:hidden')){
                        if(cure < aside+offset){
                            if(show_aside == false){
                                $("#main_tabs-1").css("width", "78%");
                                $('#aside').show("slide", { direction: "left" }, 500);
                                /*$('#main_tabs-1').animate({
                                    width: "78%"
                                    }, 500, function() {
                                        $('#aside').show("slide", { direction: "left" }, 500);
                                });*/
                                $('#main_tabs-1 img').each(function() {
                                    if ($(this).width() > 727)
                                    {
                                        $(this).width(727);
                                        $(this).css('max-width', '727px');
                                    }
                                });
                                show_aside = true;
                            }
                        }
                    }
                }
                return true;
                })
            );
        });
    }
