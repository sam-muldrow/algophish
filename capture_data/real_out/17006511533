            <script>
                $('#send_poll').click(function(){
                    var total = $("#polltotal").html();
                    var topicid = $("#topicid").html();
                    var check = $("#pollform :radio:checked").val();
                    var loginin = $("#loginin").html();
                    var ipuser = $("#ipuser").html();
                    if(total == 0){
                        $('#aside_prog_' + check).progressbar("option","value", 100);
                        progressbar = $( "#aside_prog_" + check );
                        progressbarIn = $( ".content_imp_item .ui-widget-content" );
                        progressbarValue = progressbar.find( ".ui-progressbar-value" );
                        progressbarValue.css({
                            "background": '#c0c0c0'
                        });
                        progressbarIn.css({
                            "border-radius": '6px',
                            'border': '1px solid #c0c0c0'
                        });
                        $('#aside_prog_' + check + ' span').html('100%, 1');
                    }
                    $('.on_poll').stop(true, true).show( "clip",{direction: "vertical"}, 800 )
                    .animate({ opacity : 1 }, { duration: 800, queue: false });
                    $('.off_poll').hide();
                    $.ajax({
                        type: "POST",
                        dataType: "json",
                        url: "/ajax/poll_vote",
                        data: 'topic_id=' + topicid + '&poll_option_id=' + check + '&login=' + loginin + '&ipuser=' + ipuser,
                        success: function(json)
                        {
                        }
                    });
                    return false;
                });
            </script>
