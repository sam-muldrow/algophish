    <script>
	(function(){

		if(jt_read_lang_cookies('jt_hide_lang_popup') !== 'hide'){

			var language_popup = document.querySelector('.language-popup');
			var user_lang  = "en";
			var site_lang = "";

			user_lang = navigator.language;
			user_lang = user_lang.split('-')[0];

		    site_lang =  document.getElementsByTagName("html")[0].getAttribute("lang");
			site_lang = site_lang.split('-')[0];

			if(language_popup && !(user_lang === site_lang)){

				var _e = {};
		        var lang_name = document.querySelector('.language-select__open');
				var lang_text = document.querySelector('.language-popup__text');
				var lang_btn = document.querySelector('.language-select__choose');
				var lang_btn_inner = document.querySelector('.language-select__choose span');
				var sites = {};
				var default_url = "";

				sites = {
					ko : "/",
					en : "/en",
					ja : "/jp",
					zh : "/cn"
				};

				// Set default language if a `user_lang` version of site is not available
				if (typeof sites[user_lang] == "undefined"){
					user_lang = 'en';
				}

				if(site_lang !== jt_read_lang_cookies("jt_user_lang")){

					if(user_lang === "ko"){
						_e.lang_name = "한국어";
						_e.lang_text = "언어를 선택하세요";
						_e.lang_btn = "선택";
					}else if(user_lang === "ja"){
						_e.lang_name = "日本語";
						_e.lang_text = "言語を選択してください";
						_e.lang_btn = "選択";
					}else if(user_lang === "zh"){
						_e.lang_name = "中文 (简体)";
						_e.lang_text = "请选择语言。";
						_e.lang_btn = "选择";
					}else{
						_e.lang_name = "English";
						_e.lang_text = "Please select a language";
						_e.lang_btn = "Choose";
					}

					lang_name.textContent = _e.lang_name;
					lang_text.textContent = _e.lang_text;
					lang_btn_inner.textContent = _e.lang_btn;

					lang_btn.setAttribute("href", sites[user_lang]);
					lang_btn.setAttribute("data-lang", user_lang);

					//  Add lang attr for seo and styling
					language_popup.setAttribute("lang", user_lang);

					// Display popup
					language_popup.style.display = "block";

				}

			}

		}

	    // language popup display check (vanilla js for speed)
		function jt_read_lang_cookies(name) {
			var nameEQ = name + "=";
			var ca = document.cookie.split(';');
			for(var i=0;i < ca.length;i++) {
				var c = ca[i];
				while (c.charAt(0)==' ') c = c.substring(1,c.length);
				if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
			}
			return null;
		}


	})();
	</script>

