    <script>
	// strap popup display check (vanilla js for speed)
	function jt_read_cookies(name) {
		var nameEQ = name + "=";
		var ca = document.cookie.split(';');
		for(var i=0;i < ca.length;i++) {
			var c = ca[i];
			while (c.charAt(0)==' ') c = c.substring(1,c.length);
			if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
		}
		return null;
	}

    // Helper to fix ie11 date.parse 이슈
    function jt_popup_time_parse_helper(date_str){

        if ( date_str ) {

            var a = date_str.split(" ");
            var d = a[0].split("-");
            var t = a[1].split(":");

        }

        return new Date(d[0],(d[1]-1),d[2],t[0],t[1],t[2]);

    }

    if(jt_read_cookies('hide_popup_strap') == 'hide'){

        var strap = document.querySelector('.jt-popup--strap');
		if(strap){
			strap.style.display = "none";
			document.body.classList.remove('show_popup');;
		}
    }

    // only multi banner...
    // swiper option
    var $slider_item      = document.getElementsByClassName('jt-popup__item');
    var $slider_play      = document.getElementsByClassName('swiper_play_state');
    var $slider_separator = document.getElementsByClassName('jt-popup__close-separator');

    // Manually remove slider if expire (server side cache fix)
    $.each($slider_item, function (index, value) {

        var start = value.dataset.start;
        var end = value.dataset.end;
        var always =  value.dataset.always;

        if(always == "1") { return; }

        if(start && end){

            var start_time = Date.parse(jt_popup_time_parse_helper(start));
            var end_time = Date.parse(jt_popup_time_parse_helper(end));
            var current_time = Date.now();

            // If opposite to the backend logic

            if(!(current_time >= start_time && current_time <= end_time )){
                $slider_item[index].remove();
            }

        }

    });

    // remove pagenation, autoplay
    if ( $slider_item.length <= 1 ) {

        if ( $slider_separator[0] ) {
            $slider_separator[0].remove();
        }

        if ( $slider_play[0] ) {
            $slider_play[0].remove();
        }

    }

	</script>


