<script>
    // Target
    const visualSlider = document.querySelector('.main-visual');

    // 비주얼 노출 빈도 적용
    let initSlide = 0;
    let initTemp = [];

    visualSlider.querySelectorAll('.main-visual__slide').forEach(function( item, idx ){
        const grade = parseInt( item.getAttribute('data-importance').replace(/[^0-9]/g, '') );
        for( let i = 0 ; i < grade ; i++ ) { initTemp.push(idx); }
    });

    initSlide = initTemp[ Math.floor(Math.random() * initTemp.length) ];

    // Slide init
    const visualSwiper = new Swiper(visualSlider, {
        loop: true,
        init: true,
        speed: 1000,
        slidesPerView: 1,
        initialSlide: initSlide,
        effect: 'fade',
        followFinger: false,
        fadeEffect: {
            crossFade: true
        },
        navigation: {
            nextEl: '.main-visual .main-visual__btn--next',
            prevEl: '.main-visual .main-visual__btn--prev'
        },
        pagination: {
            el: '.main-visual__slider_wrap .swiper-pagination',
            clickable: true,
            renderBullet: function (index, className) {
                return '<span class="' + className + '"><i><em class="sr_only">' + (index + 1) + '</em></i></span>';
            }
        },
        autoplay: {
            delay: 8000,
            disableOnInteraction: false
        }
    });

    // Autoplay state fix
    visualSwiper.autoplay.stop();

    // Update count
    visualSlider.querySelector('.main-visual__pagination-current').innerText = visualSwiper.realIndex + 1;
</script>
</div><!-- .main-visual -->

