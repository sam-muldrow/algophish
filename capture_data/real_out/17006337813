        layui.use(['form', 'jquery', 'element'], function() {
            var form = layui.form;
            var element = layui.element;
            var $ = layui.jquery;

            /*form.on('submit(formDemo)', function(data) {
                layer.msg(JSON.stringify(data.field));
                return false;
            });*/
            $("#vercode").click(function() {
                var loginname = $.trim($("#loginname").val());
                var loginpsw = $.trim($("#loginpsw").val());

                if (loginname == '') {
                    alert('用户名不能为空');
                    return false;
                }
                if (loginpsw == '') {
                    alert('密码不能为空');
                    return false;
                }
                $("#vercode").attr("disabled", true);
                $.post('/user/index/get_vcode', {
                    loginname: loginname,
                    loginpsw: loginpsw
                }, function(response) {
                    $("#vercode").attr("disabled", false);
                    if (response.msg) {
                        alert(response.msg);
                        // layer.msg(response.msg);
                    }
                }, 'json');


            })

            $("#login_bt").click(function() {
                var username = $.trim($("#loginname").val());
                var pwd = $.trim($("#loginpsw").val());
                var captcha = $.trim($("#captcha").val());
                if (username == '' || pwd == '') {
                    layer.msg('用户名或密码不能为空');
                    return false;
                }
                if (captcha == '') {
                    layer.msg('邮件验证码不能为空');
                    return false;
                }

                $.ajax({
                    type: 'POST',
                    url: "/user/index/login_action23",
                    data: {
                        loginname: username,
                        loginpsw: pwd,
                        captcha: captcha,
                        hash: 'loginaction',
                        qqex_validate: ''
                    },
                    success: function(response) {
                        if (response == '') {
                            layer.msg('亲，用户名或密码没有填写');
                        } else {
                            eval(response);
                        }
                    }
                });
            })
            $("#login_bt2").click(function() {
                var username = $.trim($("#loginname").val());
                var pwd = $.trim($("#loginpsw").val());
                var captcha = $.trim($("#captcha").val());
                var hash = 'loginaction';
                if (username == '' || pwd == '') {
                    layer.msg('用户名或密码不能为空');
                    return false;
                }
                if (captcha == '') {
                    layer.msg('邮件验证码不能为空');
                    return false;
                }

                $.ajax({
                    type: 'POST',
                    url: "/user/index/login_action23",
                    data: {
                        loginname: username,
                        loginpsw: pwd,
                        captcha: captcha,
                        hash: 'loginaction',
                        haiwai_submit: '1',
                        qqex_validate: ''
                    },
                    success: function(response) {
                        if (response == '') {
                            layer.msg('亲，用户名或密码没有填写');
                        } else {
                            eval(response);
                        }
                    }
                });
            })

            $("#login_bt_none").click(function() {
                var username = $.trim($("#loginname").val());
                var hash = 'loginaction';
                if (username == '') {
                    layer.msg('用户名不能为空');
                    return false;
                }

                $.ajax({
                    type: 'POST',
                    url: "/user/index/login_action",
                    data: {
                        loginname: username,
                        hash: 'loginaction',
                    },
                    success: function(response) {
                        if (response == '') {
                            layer.msg('亲，用户名没有填写');
                        } else {
                            eval(response);
                        }
                    }
                });
            })
        });
    </script>
