            <script lang="javascript" type="text/javascript">

                $(document).mouseup(function (e) {
                    var container = $(".searchMenu");

                    if (!container.is(e.target) // if the target of the click isn't the container...
                        && container.has(e.target).length === 0) // ... nor a descendant of the container
                    {
                        // $("div.searchlist").hide();
                        $("div.thise").show();

                        var text = $('.textSrch').val("");
                        //$(".searchMenu").hide();
                    }
                    else {
                        $(this).show();
                    }


                    var container1 = $(".mobileSearchContainer");

                    if (!container1.is(e.target) // if the target of the click isn't the container...
                        && container1.has(e.target).length === 0) // ... nor a descendant of the container
                    {
                        var text = $('.mobtextSrch').val("");
                    }
                    else {
                        //$(this).show();
                    }
                });


                $(".mobtextSrch").bind("keyup", function () {
                    GetData('.mobtextSrch');
                });

                $(".textSrch").bind("keyup", function () {
                    GetData('.textSrch');
                });

                function GetData(textbox) {
                    var text = $(textbox).val().toLowerCase();
                    var mode = "1-2-3-4-";

                    if (text.length > 1) {

                        Search(text, mode);
                    }
                    else {
                        $("div.searchlist").hide();
                        $("div.thise").show();
                    }
                }
                function Search(text, mode) {
                    var p = "<p class='srchload hide'>درحال جستجو...</p><span class='separator'></span>";
                    $(".searchlist").html(p);
                    $("div.thise").hide();
                    $("div.searchlist").show();
                    $("p.srchload").show();
                    var voteData = "{key: '" + text + "', mode: '" + mode + "'}";
                    $.ajax({
                        type: "POST",
                        url: "/Search.asmx/SearchIt",
                        data: voteData,
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function (results) {

                            $("p.srchload").hide();
                            $(".searchlist").html(p + results.d);
                        }
                    });
                }

            </script>
            
