            function isValidEmailAddress(n) { return new RegExp(/^(("[\w-\s]+")|([\w-]+(?:\.[\w-]+)*)|("[\w-\s]+")([\w-]+(?:\.[\w-]+)*))(@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$)|(@\[?((25[0-5]\.|2[0-4][0-9]\.|1[0-9]{2}\.|[0-9]{1,2}\.))((25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\.){2}(25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\]?$)/i).test(n) } $(".newsletter input:text[name='name']").focus(function () { $(".newsletter div.alarm").html(), $(".newsletter div.alarm").hide() }), $(".newsletter input:text[name='family']").focus(function () { $(".newsletter div.alarm").html(), $(".newsletter div.alarm").hide() }), $(".newsletter input:text[name='email']").focus(function () { $(".newsletter div.alarm").html(), $(".newsletter div.alarm").hide() }), $(".newsletter input:text[name='mobile']").focus(function () { $(".newsletter div.alarm").html(), $(".newsletter div.alarm").hide() }), $("#register").click(function () { var u; $(".newsletter div.alarm").html(), $(".newsletter div.alarm").hide(), $(".newsletter img").hide(); var i = $(".newsletter input:text[name='name']").val(), r = $(".newsletter input:text[name='family']").val(), n = $(".newsletter input:text[name='email']").val(), t = $(".newsletter input:text[name='mobile']").val(); if (!(i.trim() && r.trim() && n.trim() && t.trim())) return $(".newsletter div.alarm").html("لطفاً همه اطلاعات را وارد نمایید."), void $(".newsletter div.alarm").show(); if (!CheckMobileNumber(t)) return $(".newsletter div.alarm").html("شماره موبایل صحیح نیست."), void $(".newsletter div.alarm").show(); if (!isValidEmailAddress(n)) return $(".newsletter div.alarm").html("پست الکترونیکی صحیح نیست."), void $(".newsletter div.alarm").show(); u = "{name: '" + i + "' ,family: '" + r + "' ,email: '" + n + "' ,mobile: '" + t + "'}", $(".newsletter img").show(), $.ajax({ type: "POST", url: "/NewsletterSubscribe.asmx/AddToNewsletter", data: u, contentType: "application/json; charset=utf-8", dataType: "json", success: function (n) { swit