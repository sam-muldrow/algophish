            function CheckMobileNumber(n) { var t, i; return "" == n ? !0 : (t = new RegExp(/^((\+98|98){1}|(0){1})\d{10}/), t.test(n)) ? !0 : (i = n.replace(/۱/g, "1").replace(/۲/g, "2").replace(/۳/g, "3").replace(/۴/g, "4").replace(/۵/g, "5").replace(/۶/g, "6").replace(/۷/g, "7").replace(/۸/g, "8").replace(/۹/g, "9").replace(/۰/g, "0"), !!t.test(i)) }
            $(".getDate").find("div").attr('class', 'bigSelect');

            var culture = 'fa-IR';
            var productGroup = $("#MainContentPlaceHolder_ProductsCategoryDropDownList");
            var product = $("#MainContentPlaceHolder_ProductsDropDownList");
            var pid = $("#MainContentPlaceHolder_ProductIdHiddenField").val();

            $.ajax({
                type: "POST",
                url: "/Fill.aspx/GetPricesGroups",
                data: "{'culture': '" + culture + "'}",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (msg) {
                    productGroup.get(0).options.length = 0;
                    productGroup.removeAttr('disabled');
                    $.each(msg.d, function (index, item) {
                        if (item.Value != "39") {   //Ignoring Currency Group
                            productGroup.get(0).options[productGroup.get(0).options.length] = new Option(item.Text, item.Value);
                        }
                    });
                    productGroup.get(0).options[0] = new Option("«انتخاب گروه»", "");
                    productGroup.get(0).selectedIndex = 0;
                    productGroup.removeAttr('disabled');
                    product.get(0).options[0] = new Option("«انتخاب کالا»", "");
                    product.get(0).selectedIndex = 0;
                    product.removeAttr('disabled');
                },
                error: function () {
                    //alert("خطا در به روز رسانی");
                }
            });







            product.change(function () {
                //product.attr('disabled', 'disabled');
                if (product.val().length > 0) {
                    if (product.val() != "0") {
                        $(".fillPro").hide();
                        $("#MainContentPlaceHolder_Button1").removeAttr('disabled');
                        $("#MainContentPlaceHolder_ProductIdHiddenField").val(product.val());
                    }
                    else {
                        $("#MainContentPlaceHolder_ProductIdHiddenField").val(product.val());
                        $("#MainContentPlaceHolder_Button1").attr('disabled', 'disabled');

                        $(".fillPro").show();
                    }
                }
                else {
                    $("#MainContentPlaceHolder_Button1").removeAttr('disabled');
                    $("#MainContentPlaceHolder_ProductIdHiddenField").val("");
                }
            });

            productGroup.click(function () {
                $(".fillPro").hide();
            });


            $("#MainContentPlaceHolder_Button1").click(function (e) {

                if (product.val().length > 0) {
                    if (product.val() != "0") {
                        $(".fillPro").hide();
                        $("#MainContentPlaceHolder_Button1").removeAttr('disabled');
                        $("#MainContentPlaceHolder_ProductIdHiddenField").val(product.val());
                    }
                    else {
                        $("#MainContentPlaceHolder_ProductIdHiddenField").val(product.val());
                        $("#MainContentPlaceHolder_Button1").attr('disabled', 'disabled');
                        e.preventDefault();
                        $(".fillPro").show();
                    }
                }
                else {
                    $("#MainContentPlaceHolder_Button1").removeAttr('disabled');
                    $("#MainContentPlaceHolder_ProductIdHiddenField").val("");
                }
            });


            productGroup.change(function () {
                product.attr('disabled', 'disabled');
                $(".fillPro").hide();
                $("#MainContentPlaceHolder_Button1").removeAttr('disabled');

                if (productGroup.val().length > 0) {
                    $("#MainContentPlaceHolder_ProductsDropDownList").val(productGroup.val());
                    $("#MainContentPlaceHolder_ProductGroupIdHiddenField").val(productGroup.val());
                    $("#MainContentPlaceHolder_ProductIdHiddenField").val("");

                    $.ajax({
                        type: "POST",
                        url: "/Fill.aspx/GetProducts",
                        data: "{'groupId':" + productGroup.val() + ", 'culture': '" + culture + "'}",
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function (msg) {
                            product.get(0).options.length = 0;
                            product.removeAttr('disabled');

                            product.get(0).options[0] = new Option("«انتخاب کالا»", "");
                            product.get(0).selectedIndex = 0;

                            $.each(msg.d, function (index, item) {

                                if (item.Text != 'نفت' && String(item.Text).indexOf('نفت خام') == -1) {
                                    product.get(0).options[product.get(0).options.length] = new Option(item.Text, item.Value);
                                }
                            });
                            $("#MainContentPlaceHolder_ProductIdHiddenField").val(product.val());



                        },
                        error: function () {
                            alert("Error On Update");
                        }
                    });
                }
                else {
                    $(".fillPro").hide();

                    $("#MainContentPlaceHolder_ProductGroupIdHiddenField").val("");
                    //$("#MainContentPlaceHolder_ProductIdHiddenField").val("");
                    product.removeAttr('disabled');
                    product.get(0).selectedIndex = 0;

                }
            });
        });
