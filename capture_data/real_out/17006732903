(function () {
  var googletag = window.googletag = window.googletag || {};
  googletag.cmd = googletag.cmd || [];

  googletag.cmd.push(function () {
    googletag.pubads().addEventListener('slotRenderEnded', function (event) {
      if (!event.isEmpty) {
        (window._matherq = window._matherq || []).push(['getTracker', function (tracker) {
          tracker.trackUnstructEvent('Ad Impression', { eaid: event.lineItemId, ebuy: event.campaignId, eadv: event.advertiserId, ecid: event.creativeId, eenv: 'j', epid: event.slot.getAdUnitPath() || '', esid: null, size: event.size[0] + 'x' + event.size[1], rendered: event.isEmpty ? '0' : '1' });
        }]);
      }
    });
  });
})();

// Register event handlers to observe lazy loading behavior.
googletag.pubads().addEventListener('slotRequested', function(event) {
  updateSlotStatus(event.slot.getSlotElementId(), 'fetched');
});

googletag.pubads().addEventListener('slotOnload', function(event) {
  updateSlotStatus(event.slot.getSlotElementId(), 'rendered');
});

googletag.pubads().addEventListener('impressionViewable', function(event) {
      updateSlotStatus(event.slot.getSlotElementId(), ' became viewable');
});	
		
// /DFP SB
googletag.enableServices();
});

function updateSlotStatus(slotId, state) {
	console.log(slotId + '-' + state);
}
