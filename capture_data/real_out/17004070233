                <!--SonDakika-->
                
<div class="col-xs-12 col-12 col-lg-8">

    <!-- vodafone sondakika reklam -->
    
    <div class="sliderWrapper">
        <div class="lastMinNews boxShadowSet view20">
            <a href="/son-dakika-haberleri" target="_blank" class="link" title="Son Dakika Haberleri">SON<br><br>DAKÄ°KA</a>
        </div>
    </div>
</div>

<script>
    $(document).ready(function () {
        $.getJSON(unescape('/getsondakikabandasjson'), null, function (data) {
            var isIE = !window.navigator.msPointerEnabled;

            if (data.Success && data.List.length > 0) {
                if (data.List.length % 2 != 0)
                    data.List.splice(data.List.length - 1, 1);

                var _title, _url, _date;

                var _itemHtml = '<ul class="items">';
                for (i = 0; i < data.List.length; i++) {
                    _title = data.List[i].Title;
                    _url = data.List[i].Url;
                    _date = data.List[i].Date;

                    _itemHtml += '<li>';
                    _itemHtml += '  <a title="{TITLE}" href="{URL}" target="_blank">'
                    _itemHtml += '      <i>{DATE}</i>';
                    _itemHtml += '      <em>{TITLE}</em>';
                    _itemHtml += '  </a>';
                    _itemHtml += '</li>';

                    _itemHtml = _itemHtml
                        .replace(/{TITLE}/g, _title)
                        .replace(/{URL}/g, _url)
                        .replace(/{DATE}/g, _date);
                }
                _itemHtml += '</ul>';

                $(".lastMinNews").append(_itemHtml);

                $('.lastMinNews ul').slick({
                    dots: false,
                    vertical: true,
                    infinite: true,
                    speed: 300,
                    slidesPerRow: 1,
                    rows: 2,
                    autoplay: isIE,
                    autoplaySpeed: 3000,
                    arrows: false,
                    touchThreshold: 100,
                    pauseOnDotsHover: true,
                    touchThreshold: 100,
                    responsive: [{
                        breakpoint: 993,
                        settings: {
                            slidesToShow: 3,
                            slidesToScroll: 1,
                            slidesPerRow: 1,
                            rows: 2
                        }
                    }]
                });
            }
        });
    });
</script>
                <div class="col-xs-12 col-12 col-lg-4">
    <div class="listen-all-index view20">
        <a href="https://www.sabah.com.tr/seslidinle" target="_blank" class="link-btn">
