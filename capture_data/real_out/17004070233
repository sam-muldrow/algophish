<script>
    var currGrandHeadline = GetSlickIndexForArticle();
    var grandHeadline = new Swiper('.grand-headline .swiper', {
        on: {
            init: function (swiper) {
                $(swiper.$el.parent()).on('mouseover', '.swiper-pagination-bullet', function () {
                    swiper.slideTo($(this).index() + 1);
                });
                if (currGrandHeadline > -1)
                    $('body').addClass('socialExtraMain');

            },
            slideChange: function (swiper) {
                swiper.realIndex == currGrandHeadline ? $('body').addClass('socialExtraMain') : $('body').removeClass('socialExtraMain')
            }
        },
        speed: 300,
        slidesPerView: 1,
        spaceBetween: 0,
        slidesPerGroup: 1,
        loop: true,
        loopFillGroupWithBlank: true,
        longSwipesRatio: 0.1,
        initialSlide: (currGrandHeadline < 0 ? 0 : currGrandHeadline),
        preloadImages: false,
        lazy: {
            loadPrevNext: true
        },
        loadOnTransitionStart: true,
        pagination: {
            el: '.grand-headline .swiper-pagination',
            renderBullet: function (index, className) {
                var _link = $("a", this.slides[index + 1]).attr("href");
                return '<a href="' + _link + '" class="' + className + '" target="_blank">' + (index + 1) + '</a>';
            }
        }
    });

    function GetSlickIndexForArticle() {
        var _index = -1,
            _hashValue = window.location.hash;

        if (_hashValue.indexOf("#a") == 0) {
            var _article = _hashValue.substring(2),
                _image = $(".grand-headline .headline-new img[data-id='" + _article + "']");

            if (_image.length > 0) {
                _index = _image.parents(".swiper-slide").index();

                history.replaceState("", document.title, window.location.pathname + window.location.search);
            }
        }

        return _index;
    }
</script>

