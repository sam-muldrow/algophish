          bdNtnStr = '7';
          bdSetCookie('bdNtn', bdNtnStr, expireDate.toGMTString(), '/');
          break;
        case 5094890100:
          $('<img/>').attr({src:'https://pubads.g.doubleclick.net/activity;dc_iu=/31110078,22727463451/DFPAudiencePixel;ord=1;dc_seg=838069577?',height:1,width:1}).appendTo('body');
          bdNtnStr = '8';
          bdSetCookie('bdNtn', bdNtnStr, expireDate.toGMTString(), '/');
