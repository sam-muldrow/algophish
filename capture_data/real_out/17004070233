          bdNtnStr = '3';
          bdSetCookie('bdNtn', bdNtnStr, expireDate.toGMTString(), '/');
          break;
        case 5095027360:
          $('<img/>').attr({src:'https://pubads.g.doubleclick.net/activity;dc_iu=/31110078,22727463451/DFPAudiencePixel;ord=1;dc_seg=838357642?',height:1,width:1}).appendTo('body');
          bdNtnStr = '4';
          bdSetCookie('bdNtn', bdNtnStr, expireDate.toGMTString(), '/');
          break;
        case 5095047004:
          $('<img/>').attr({src:'https://pubads.g.doubleclick.net/activity;dc_iu=/31110078,22727463451/DFPAudiencePixel;ord=1;dc_seg=840096987?',height:1,width:1}).appendTo('body');
          bdNtnStr = '5';
          bdSetCookie('bdNtn', bdNtnStr, expireDate.toGMTString(), '/');
          break;
        case 5095332401:
          $('<img/>').attr({src:'https://pubads.g.doubleclick.net/activity;dc_iu=/31110078,22727463451/DFPAudiencePixel;ord=1;dc_seg=838069487?',height:1,width:1}).appendTo('body');
          bdNtnStr = '6';
