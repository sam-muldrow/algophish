    <section class="twoColsGrid pageGrid-left">                <h2 class="titleDefault  ">
    <span class="titleDefault-text">encore plus de news</span>
</h2>                
<ul class="articleList horizontalScrollOnMobile">
                        <li class="articleList-item">
                

<article class="articleDefaultCard horizontalDesktopOnly" data-wide-target>
    <div class="articleDefaultCard-image">
                                                                <div class="ratio-1-1"><picture class="">
