               function techLogger(level, label, payload) {
                    var msg = payload ? JSON.stringify(payload) : "";
                    var category = label;
                    var data = { level, msg, category };
                    fetch("/v2.1/logging/tech-log" , {
                     method: "POST",
                     body: JSON.stringify(data),
                     headers: {
                       "content-type": "application/json",
                       "source": window.screen > 800 ? "desktop" : "mobile"
                     },
                    })
                     .then((res) => res.text())
                     .then(console.log);
              }
          </script><script>
              if("serviceWorker" in navigator) {
                navigator.serviceWorker.getRegistration().then(r => {
                    if(!r) return; 
                    console.log("unregister service worker", r)
                    techLogger("warning","Service Worker - Unregister Attempt")
                    r.unregister().then(() => {
                        techLogger("warning","Service Worker - Unregister success");
                        console.log("service worker - Unregister success");
                    }).catch(error => {
                        techLogger("warning","Service worker - Unregister failed");
                    })
                })
              }</script></body></html>