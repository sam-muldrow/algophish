<script language="javascript">
function updateEndTime(){
 var date = new Date();
 var time = date.getTime();
 var timeDivs = document.getElementsByTagName("td");
 for(var i=0;i<timeDivs.length;i++){
  if(timeDivs[i].className == "settime"){
   var endDate = timeDivs[i].getAttribute("endTime");
   var fid = timeDivs[i].getAttribute("fid");
   var endTime = endDate*1000;
   var lag = (endTime - time) / 1000;
   if(lag > 0){
     var s = Math.floor(lag % 60);     
     var m = Math.floor((lag / 60) % 60);
     var h = Math.floor((lag / 3600) % 24);
     var d = Math.floor((lag / 3600) / 24);

   if(d<=0){
   document.getElementById('d'+fid).style.display='none';
   document.getElementById('dd'+fid).style.display='none'; 
   }else{
   document.getElementById('d'+fid).style.display='inline';
   document.getElementById('dd'+fid).style.display='inline';    
   }	 
   if(h<=0){
   document.getElementById('h'+fid).style.display='none';
   document.getElementById('hh'+fid).style.display='none'; 
   }else{
   document.getElementById('h'+fid).style.display='inline';
   document.getElementById('hh'+fid).style.display='inline';    
   }
   if(m<=0){
   document.getElementById('m'+fid).style.display='none';
   document.getElementById('mm'+fid).style.display='none'; 
   }else{
   document.getElementById('m'+fid).style.display='inline';
   document.getElementById('mm'+fid).style.display='inline';    
   }	 
	 
   if(d < 10){ d= "0"+d;}
   if(h < 10){ h= "0"+h;}
   if(m < 10){ m= "0"+m;}
   if(s < 10){ s= "0"+s;}	 

     document.getElementById("msg"+fid).style.display="none";
     document.getElementById("msgtime"+fid).style.display="inline";   
     document.getElementById("d"+fid).innerHTML=d;
     document.getElementById("h"+fid).innerHTML=h;
     document.getElementById("m"+fid).innerHTML=m;
     document.getElementById("s"+fid).innerHTML=s; 
    }else{
    document.getElementById("msg"+fid).style.display="inline";
    document.getElementById("msgtime"+fid).style.display="none";	
	}
   }
 }
 setTimeout("updateEndTime()",1000);
}
updateEndTime();
</script>
<script type="text/javascript">
var g_shop_detail_bar = "1";
function shop_show_style(v,v1,v2){
	if(g_shop_detail_bar == v) {
		return;
	}
	$_sH('s1687283');
	$("shop_div_1").style.cssText = v2;
	$("shop_div_2").style.cssText = v2;
	$("shop_div_3").style.cssText = v2;
	$("shop_div_4").style.cssText = v2;
	$("shop_div_5").style.cssText = v2;
	$("shop_div_6").style.cssText = v2;
	$("shop_div_7").style.cssText = v2;
	$("shop_content_div_1").style.display = "none";
	$("shop_content_div_2").style.display = "none";
	$("shop_content_div_3").style.display = "none";
	$("shop_content_div_4").style.display = "none";
	$("shop_content_div_5").style.display = "none";
	$("shop_content_div_6").style.display = "none";
	$("shop_content_div_7").style.display = "none";
    
    $("shop_div_" + v).style.cssText = v1;
	$("shop_content_div_" + v).style.display = "";
	g_shop_detail_bar = v;
	$_eH('s1687283');
}
</script>

