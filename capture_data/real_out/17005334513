<script type="text/javascript">
function oho_isWindow(obj) {
	return obj && typeof obj === "object" && "setInterval" in obj;
}

function oho_height(elem) {
	if(oho_isWindow(elem)) {
		alert("sss");
		var docElemProp = elem.document.documentElement[ "clientHeight" ], body = elem.document.body;

		return elem.document.compatMode === "CSS1Compat" && docElemProp || body && body[ "clientHeight" ] || docElemProp;
	} else if ( elem.nodeType === 9 ) {	
		//documentçè¯
			return Math.max(
				elem.documentElement["clientHeight"],
				elem.body["scrollHeight"], elem.documentElement["scrollHeight"],
				elem.body["offsetHeight"], elem.documentElement["offsetHeight"]
			);
	} else {
		var orig = oho_css(elem, "height"), ret = parseFloat( orig );

		return oho_isNumeric( ret ) ? ret : orig;
	}
}

function oho_isNumeric(obj) {
	return !isNaN( parseFloat(obj) ) && isFinite( obj );
}

function oho_css(elem, name) {
	var nType = elem.nodeType;

	if ( !elem || nType === 3 || nType === 8 || nType === 2 ) {
		return;
	}

	var ret = elem.getAttribute( name );

	return ret === null ?  undefined : ret;
}

function oho_getWindow( elem ) {
	return oho_isWindow( elem ) ?
		elem :
		elem.nodeType === 9 ?
			elem.defaultView || elem.parentWindow :
			false;
}

function oho_scrollTop(elem, val) {
	if ( val === undefined ) {
		if ( !elem ) {
			return null;
		}

		var win = oho_getWindow( elem );

		
		var ret = null;
		if(win) {
			if("pageXOffset" in win) {
				ret = win[ "pageYOffset" ];
			} else {
				//alert("sss");
				var boxModel = $("oho_boxModel").offsetWidth === 2 ? true : false;
				ret = boxModel && win.document.documentElement[ "scrollTop" ] || win.document.body[ "scrollTop" ];	
			}
		} else {
			ret = elem[ "scrollTop" ];
		}

		return ret;
	}

	var win = oho_getWindow( elem );

	if(win) {
		
	} else {
	}
}

function oho_attachEvent(obj, evt, func, eventobj) {
	eventobj = !eventobj ? obj : eventobj;
	if(obj && obj.addEventListener) {
		obj.addEventListener(evt, func, false);
	} else if(eventobj && eventobj.attachEvent) {
		obj.attachEvent("on" + evt, func);
	}
}

function oho_backScroll() {
	var st = oho_scrollTop(document);	

	//alert(st);
	if(st > 0) {
		$("backToTop").style.display = "block";
	} else {
		$("backToTop").style.display = "none";
	}
}

</script>

