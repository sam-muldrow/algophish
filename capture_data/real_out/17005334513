	var g_game_s1541121_gameid = "";
	function game_list_gz_s1541121(gameid) {
        if(typeof gameid == "undefined") {
            gameid = g_game_s1541121_gameid;
        } else {
            g_game_s1541121_gameid = gameid;
        }
        var u = "mod/sort2/quest.php?op=gz&gameid=" + gameid + "&ts=" + new Date().getTime();
        startRequest("GET", u, "", function(r) {
            if(r == "2") {
                //æªç»å½
                var u = "/mod/mad/quest.php?op=quick_login&quick_script=game_list_gz_s1541121";
	            mad_mask_menu(null, u);
            } else {
                //å·²å³æ³¨
                if($("game_gz_div_" + gameid + "_s1541121")) {
                    $("game_gz_div_" + gameid + "_s1541121").style.display = "none";
                }

                if($("game_no_gz_div_" + gameid + "_s1541121")) {
                    $("game_no_gz_div_" + gameid + "_s1541121").style.display = "";
                }
            }
        });
    }

    function game_list_no_gz_s1541121(gameid) {
        if(typeof gameid == "undefined") {
            gameid = g_game_s1541121_gameid;
        } else {
            g_game_s1541121_gameid = gameid;
        }
        var u = "mod/sort2/quest.php?op=no_gz&gameid=" + gameid + "&ts=" + new Date().getTime();
        startRequest("GET", u, "", function(r) {
            if(r == "2") {
                //æªç»å½
                var u = "/mod/mad/quest.php?op=quick_login&quick_script=game_list_no_gz_s1541121";
	            mad_mask_menu(null, u);
            } else {
                //åæ¶å³æ³¨
                if($("game_gz_div_" + gameid + "_s1541121")) {
                    $("game_gz_div_" + gameid + "_s1541121").style.display = "";
                }

                if($("game_no_gz_div_" + gameid + "_s1541121")) {
                    $("game_no_gz_div_" + gameid + "_s1541121").style.display = "none";
                }
            }
        });
    }
