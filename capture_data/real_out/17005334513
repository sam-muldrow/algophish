	var g_game_s38892183_gameid = "";
	function game_list_gz_s38892183(gameid) {
        if(typeof gameid == "undefined") {
            gameid = g_game_s38892183_gameid;
        } else {
            g_game_s38892183_gameid = gameid;
        }
        var u = "mod/sort2/quest.php?op=gz&gameid=" + gameid + "&ts=" + new Date().getTime();
        startRequest("GET", u, "", function(r) {
            if(r == "2") {
                //æªç»å½
                var u = "/mod/mad/quest.php?op=quick_login&quick_script=game_list_gz_s38892183";
	            mad_mask_menu(null, u);
            } else {
                //å·²å³æ³¨
                if($("game_gz_div_" + gameid + "_s38892183")) {
                    $("game_gz_div_" + gameid + "_s38892183").style.display = "none";
                }

                if($("game_no_gz_div_" + gameid + "_s38892183")) {
                    $("game_no_gz_div_" + gameid + "_s38892183").style.display = "";
                }
            }
        });
    }

    function game_list_no_gz_s38892183(gameid) {
        if(typeof gameid == "undefined") {
            gameid = g_game_s38892183_gameid;
        } else {
            g_game_s38892183_gameid = gameid;
        }
        var u = "mod/sort2/quest.php?op=no_gz&gameid=" + gameid + "&ts=" + new Date().getTime();
        startRequest("GET", u, "", function(r) {
            if(r == "2") {
                //æªç»å½
                var u = "/mod/mad/quest.php?op=quick_login&quick_script=game_list_no_gz_s38892183";
	            mad_mask_menu(null, u);
            } else {
                //åæ¶å³æ³¨
                if($("game_gz_div_" + gameid + "_s38892183")) {
                    $("game_gz_div_" + gameid + "_s38892183").style.display = "";
                }

                if($("game_no_gz_div_" + gameid + "_s38892183")) {
                    $("game_no_gz_div_" + gameid + "_s38892183").style.display = "none";
                }
            }
        });
    }
