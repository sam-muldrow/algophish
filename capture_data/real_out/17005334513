var g_oho_muli_login_timer_s50872245 = null;
var g_oho_multi_login_left_offset_s50872245 = -100;

var g_oho_msg_timer_s50872245 = null;


function oho_multi_login_more_click(blockid) {
    var block_div = "oho_multi_login_more_" + blockid;
    var block_div_click = "oho_multi_login_more_click_" + blockid;

    if($(block_div) && $(block_div_click)) {
        var block_pop_div = "oho_multi_login_pop_more_" + blockid;
        if($(block_pop_div)) {
            //å·²ç»å­å¨
        } else {
            //è¿æ²¡æå­å¨ï¼éè¦åå»º
            var aa_div = document.createElement('div');
            aa_div.innerHTML = $(block_div).innerHTML;
            aa_div.id = block_pop_div;
            aa_div.style.position = "absolute";
            aa_div.style.display = "none";
            aa_div.style.zIndex = 1000;
            document.body.insertBefore(aa_div, document.body.childNodes[0]);
            //setMenuPosition($(block_div_click), aa_div, 0);
            var str = "oho_setMenuPosition($(block_div_click), aa_div, 0, 0, g_oho_multi_login_left_offset_" + blockid + ");";
            eval(str);
        }

        $(block_pop_div).onmouseover = function() {
            //alert(blockid);
            oho_multi_login_more_click(blockid);
        }

        $(block_pop_div).onmouseout = function() {
            oho_multi_login_hide(blockid);
        }

        eval("clearTimeout(g_oho_muli_login_timer_" + blockid + ");");
        $(block_pop_div).style.display = "";
    }
} 

function oho_multi_login_hide(blockid) {
    var block_pop_div = "oho_multi_login_pop_more_" + blockid;

    if($(block_pop_div)) {
        var str = "g_oho_muli_login_timer_" + blockid + "=setTimeout('oho_multi_login_hide_script(\"" + block_pop_div + "\")', 500);";
        eval(str);
    }
}

function oho_multi_login_hide_script(block_pop_div) {
    $(block_pop_div).style.display = "none";
}


function oho_msg_click(id, blockid) {
    var block_div = "oho_msg_more_" + blockid;
    var block_div_click = "oho_msg_click_" + id + "_" + blockid;

    if($(block_div) && $(block_div_click)) {
        var block_pop_div = "oho_msg_pop_more_" + blockid;
        if($(block_pop_div)) {
            //å·²ç»å­å¨
            var aa_div = $(block_pop_div);
        } else {
            //è¿æ²¡æå­å¨ï¼éè¦åå»º
            var aa_div = document.createElement('div');
            aa_div.innerHTML = $(block_div).innerHTML;
            aa_div.id = block_pop_div;
            aa_div.style.position = "absolute";
            aa_div.style.display = "none";
            aa_div.style.zIndex = 1000;
            document.body.insertBefore(aa_div, document.body.childNodes[0]);
        }

        var str = "oho_setMenuPosition($(block_div_click), aa_div);";
        eval(str);

        $(block_pop_div).onmouseover = function() {
            //alert(blockid);
            oho_msg_click(id, blockid);
        }

        $(block_pop_div).onmouseout = function() {
            oho_msg_hide(blockid);
        }

        eval("clearTimeout(g_oho_msg_timer_" + blockid + ");");
        $(block_pop_div).style.display = "";
    }
} 

function oho_msg_hide(blockid) {
    var block_pop_div = "oho_msg_pop_more_" + blockid;

    if($(block_pop_div)) {
        var str = "g_oho_msg_timer_" + blockid + "=setTimeout('oho_msg_hide_script(\"" + block_pop_div + "\")', 500);";
        eval(str);
    }
}

function oho_msg_hide_script(block_pop_div) {
    $(block_pop_div).style.display = "none";
}

</script>
