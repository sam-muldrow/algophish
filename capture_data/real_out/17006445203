			var element = document.getElementsByTagName("a");
      		for (var i = 0; i < element.length; i++) {
      		let text = element[i].toString();
     	 	let position = text.search("italiadomani.gov.it/it");
	  		if (position>2) {element[i].setAttribute('target', '_blank');}
