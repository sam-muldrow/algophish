          <h2 class="c-title002 c-title02--center u-mb40">ãµã¤ãæ´æ°æå ±</h2>
          <div class="c-card003__slide" id="hotnews">
          </div>
          <script>
            fetch('/top-page/assets/js/libs/hotnews.json').then(response => response.json()).then(data => {
              const newsContainer = document.getElementById('hotnews');
              data.forEach(item => {
                  let cardHTML = `
                  <div class="c-card003 p-products-top-news003-card">
                      <a href="${item.href}?from=top_site_update" target="_blank" rel="noopener noreferrer" class="c-card003__link">
                          <figure class="p-products-top-news003-card__image">
                              <div class="c-card003__image">
                                  <picture>
                  `;
                  item.images.forEach(image => {
                      if (image.src) {
                          cardHTML += `<img src="${image.src}" alt="${image.alt}" width="${image.width}" height="${image.height}">`;
                      } else {
                          cardHTML += `<source type="${image.type}" media="${image.media}" srcset="${image.srcset}">`;
                      }
                  });
                  cardHTML += `
                                  </picture>
                              </div>
                          </figure>
                          <div class="c-card003__text-box">
                              <div class="c-card003__meta">
                                  <time class="c-card003__time" datetime="${item.date}">${item.date}</time>
                                  <span class="c-card003__category">${item.category}</span>
                              </div>
                              <p class="c-card003__text">${item.text}</p>
                          </div>
                      </a>
                  </div>
                  `;
                  newsContainer.insertAdjacentHTML('beforeend', cardHTML);
            });
          }).catch(error => console.error("Error loading hot news items:", error));
          </script>
