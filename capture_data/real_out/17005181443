                Coveo.initSearchbox(globalSearchbox, "https://www.usg.edu/search");
                Coveo.initSearchbox(globalOffCanvasSearchbox, "https://www.usg.edu/search");
				
				
				
				
				
				
				
				
				
				
				
				
				
				

              } else {
