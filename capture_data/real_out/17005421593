<script async defer>
    const processorSelect1 = document.getElementById('processorSelect1');
    const processorSelect2 = document.getElementById('processorSelect2');
    const gpuSelect1 = document.getElementById('gpuSelect1');
    const gpuSelect2 = document.getElementById('gpuSelect2');

    window.addEventListener("load", function(){
        if (processorSelect1 !== null && processorSelect2 !== null) {
            const tailProcessorSelect1 = searchDropdownHelper.createNewProcessorDropdown(processorSelect1);
            const tailProcessorSelect2 = searchDropdownHelper.createNewProcessorDropdown(processorSelect2);

            processorSelect1.addEventListener('change', function(e){
                let hardware1 = {};
                let hardware2 = {};
                hardware1.id = e.srcElement.options[e.srcElement.selectedIndex].value;
                hardware1.name = e.srcElement.options[e.srcElement.selectedIndex].innerText;
                hardware2.id = processorSelect2.options[processorSelect2.selectedIndex].value;
                hardware2.name = processorSelect2.options[processorSelect2.selectedIndex].innerText;
                compareHardware(hardware1, hardware2, 'cpu');
            });

            processorSelect2.addEventListener('change', function(e){
                let hardware1 = {};
                let hardware2 = {};
                hardware1.id = e.srcElement.options[e.srcElement.selectedIndex].value;
                hardware1.name = e.srcElement.options[e.srcElement.selectedIndex].innerText;
                hardware2.id = processorSelect1.options[processorSelect2.selectedIndex].value;
                hardware2.name = processorSelect1.options[processorSelect2.selectedIndex].innerText;
                compareHardware(hardware1, hardware2, 'cpu');
            });
        }

        if (gpuSelect1 !== null && gpuSelect2 !== null) {
            const tailGpuSelect1 = searchDropdownHelper.createNewGraphicsCardDropdown(gpuSelect1);
            const tailGpuSelect2 = searchDropdownHelper.createNewGraphicsCardDropdown(gpuSelect2);

            gpuSelect1.addEventListener('change', function(e){
                let hardware1 = {};
                let hardware2 = {};
                hardware1.id = e.srcElement.options[e.srcElement.selectedIndex].value;
                hardware1.name = e.srcElement.options[e.srcElement.selectedIndex].innerText;
                hardware2.id = gpuSelect2.options[gpuSelect2.selectedIndex].value;
                hardware2.name = gpuSelect2.options[gpuSelect2.selectedIndex].innerText;
                compareHardware(hardware1, hardware2, 'gpu');
            });

            gpuSelect2.addEventListener('change', function(e){
                let hardware1 = {};
                let hardware2 = {};
                hardware1.id = e.srcElement.options[e.srcElement.selectedIndex].value;
                hardware1.name = e.srcElement.options[e.srcElement.selectedIndex].innerText;
                hardware2.id = gpuSelect1.options[gpuSelect1.selectedIndex].value;
                hardware2.name = gpuSelect1.options[gpuSelect1.selectedIndex].innerText;
                compareHardware(hardware1, hardware2, 'gpu');
            });
        }

        function compareHardware(hardware1, hardware2, type) {
            var newestHardware;
            var oldestHardware;
            var typeId;
            var selectEmptyCheck;

            console.log(hardware1, hardware2)

            if (type == 'gpu') {
                selectEmptyCheck = 'Select a Graphics Card...';
                typeId = 'gid';
            } else {
                selectEmptyCheck = 'Select a Processor...';
                typeId = 'pid';
            }

            if (
                hardware1.name == selectEmptyCheck || hardware2.name == selectEmptyCheck ||
                hardware1.name == null || hardware2.name == null ||
                hardware1.name == '' || hardware2.name == ''
            ) {
                return false;
            }

            if (hardware1.id > hardware2.id) {
                newestHardware = hardware1;
                oldestHardware = hardware2;
            } else {
                newestHardware = hardware2;
                oldestHardware = hardware1;
            }

            window.location = '/' + type + '/index.php?' + typeId + '=' + newestHardware.id + '&' + typeId + '2=' + oldestHardware.id + '&compare=' + newestHardware.name + '-vs-' + oldestHardware.name;
        }
    });
</script>

