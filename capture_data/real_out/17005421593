    let navbar = document.getElementById("gd__mobile-navigation");
    let navbarBtn = document.getElementById("gd__nav-bar-btn");

    navbarBtn.addEventListener("click", function () {
        navbar.classList.toggle("gd__mobile-navigation--open");
        navbarBtn.classList.toggle("gd__nav-bar-btn--open");
        if (navbar.classList.contains("gd__mobile-navigation--open")) {
            navbarBtn.getElementsByTagName("span")[0].innerHTML = "Close";
        } else {
            navbarBtn.getElementsByTagName("span")[0].innerHTML = "Menu";
        }
    });
</script>

