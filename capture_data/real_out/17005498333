<script>
    window.dataLayer = window.dataLayer || [];

    // This function will modify dataLayer.push() to a modified version
    // This will clear the previous ecommerce object if the event is on the List of events
    const oldPush = window.dataLayer.push;
    window.dataLayer.push = function() {
        const states = [].slice.call(arguments, 0)
        // List of events that will trigger the clearing of ecommerce object before pushing
        const events = ['view_item', 'view_cart', 'add_to_wishlist', 'begin_checkout', 'add_shipping_info', 'add_payment_info', 'purchase']
        if ('event' in arguments[0] && events.includes(arguments[0].event)) {
            window.dataLayer.push({ecommerce: null})
        }
        // Re-apply the push function to its original function
        return oldPush.apply(window.dataLayer, states)
    }
