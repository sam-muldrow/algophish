  <script>
    document.addEventListener('DOMContentLoaded', () => {
      TranslateInit();
      setTimeout(()=>{
        document.querySelector('html').removeAttribute('style');
        document.querySelector('body').removeAttribute('style');
        window.scrollTo(0,0)
      }, 1_000)

      let cookieGoogle = document.cookie;
      let strToArr = cookieGoogle.split(';');
      let lang = null;
      let reg = /^\sgoog.+$/gi
      strToArr.find((el) => {
        //Ð¸Ð· Ð½ÐµÐ¾Ð±ÑÐ¾Ð´Ð¸Ð¼Ð¾ÑÑÐ¸ Ð¸ÑÐ¿Ð¾Ð»ÑÐ·ÑÐµÑÑÑ Ð½Ðµ ==
        if (el == reg.exec(el)) {
          lang = el.replace(' ', '')
        }
      })

      if (lang !== null) {
        let cookieLang = lang.match(/\w+\/\w+/i, '$1, $2');
        if (cookieLang !== null) {
          //ÑÑÐ·Ð¸Ð¼ ÑÑÑÐ¾ÐºÑ
          let str = cookieLang[0].match(/\/\w+/i, '$1')[0].replace('/', '');

          let currentLang = document.querySelectorAll('.language-current i i');
          currentLang.forEach((el) => {
            el.classList = "";
            el.classList.add(`language-flag`);
            el.classList.add(`flag-${str}`);
          })
        }
      }

    })
  </script>

