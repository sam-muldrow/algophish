<script>
var loadingYT = false;
var loadedYT = false;
var videoWrappersInfo = [];

function YT_load() {
    if (!loadedYT && !loadingYT) {
        console.log('[YT_API] Carico YT');
        mmLoader({
            src: "https://www.youtube.com/iframe_api",
            async: true
        });
        loadingYT = true;
    }
}

function load_YTvideo(video) {
    if (loadedYT) {
        loadingYT = false;
        console.log('[YT_API] Carico il video');
        let videoid = video.querySelector(':scope > .videowrapper').dataset.id;

        let videoAutoplay = 1;
        let videoMute = 0;
        
        let player = new YT.Player('tw-videowrapper-' + videoid, {
            width: 1280,
            height: 720,
            videoId: videoid,
            embedConfig: {
                'adsConfig': {
                    'adTagParameters': {
                        'iu': '/4758/pianetatech/videopfp',
                        'pp': null
                    },
                    'nonPersonalizedAd': false
                },
            },
            events: {
                onReady: onPlayerReady
            },
            playerVars: {
                mute: videoMute,
                autoplay: videoAutoplay
            }
        });
        console.log('[YT_API] YT caricato ' + videoid);
    } else {
        console.log('[YT_API] YT non  ancora pronto');
        // se yt non  pronto chiamo ricorsivamente la funzione
        YT_load();
        setTimeout(function(){
            load_YTvideo(video);
        }, 250);
    }

}
function onYouTubeIframeAPIReady() {
    // Script for more than one video on the page
    console.log('[YT_API] Iframe API Caricata');
    loadedYT = true;
}

function onPlayerReady(event) {
    // per forzare il play del video in mobile dove autoplay viene ignorato
    console.log('[YT_API] video pronto - forzo autoplay');
    event.target.playVideo();
}

function YT_click(article = document) {
    // metto gli eventi al click dei video per gestisre la logica di caricamento
    let videos = article.querySelectorAll(".tw-container-video.youtube");
    if(videos.length > 0){
        videos.forEach(video => {
            video.addEventListener('click', () => {
                // passo comunque dalla funzione che carica le risorse YT per sicurezza (mobile ecc)
                YT_load();
                load_YTvideo(video);
            }, { once: true });
            video.addEventListener('mouseover', () => {
                // anticipo il load delle risorse YT al mouseover
                YT_load();
            }, { once: true });
            
        });
    }
}
// load iniziale per il primo articolo e i listing
YT_click();
</script>


<script>(function realtime(){let cards = document.querySelectorAll('.tw-card-image[data-realtime][data-info]');if (cards) {cards.forEach(card => {let carddate = card.dataset.realtime;/* prendo la data attuale */const now = Date.now();/* converto la data della card in unix time */let datecard = new Date(carddate);datecard = datecard.getTime();/* numero di millisecondi di differenza tra la card e oggi */let seconds = now - datecard;/* giorni di differenza */let days = Math.floor(seconds / (1000*60*60*24));if (days < 1) {let hours = Math.floor(seconds / (1000*60*60));print = hours + ' ore fa';if (hours < 2) {print = "un'ora fa";if (hours < 1) {let minutes = Math.floor(seconds / (1000*60));print = minutes + ' minuti fa';if (minutes < 2) print = 'un minuto fa';}}card.dataset.info = print;}});}})();</script>


		
	
<script>
    window.ambiente = window.ambiente || 'prod';
    // PERMUTIVE CONSENT CONFIG
    var permutiveEnabled = false;
    // GET CONSENT TOKEN (euconsent-v2)
    let token = (document.cookie.match(/(?:iub)?euconsent-v2=([^;]*)/) || [])[1];

    function setupPermutive() {
        __tcfapi('getTCData', 2, (tcData, success) => {
            if (success && tcData) {
                if(tcData.purpose.consents["1"] && tcData.purpose.consents["3"] && tcData.purpose.consents["5"] && tcData.purpose.consents["7"] && tcData.purpose.consents["9"] && tcData.vendor.consents["361"]) {
                    permutiveEnabled = true;
                    window._exaudiMediaset = window._exaudiMediaset || [];
                    window._exaudiMediaset.push(["S",["_cm","PERMUTIVE"]]);
                }

                if(!token) {
                    token = tcData.tcString;
                }
            }
        });
    }

    postTcfReady(setupPermutive);
    let adMantxLoaded = false;


function startAdvChain2022() {
    let startTime = performance.now();

    const startAdmantx = function () {

        /* ADMANTX CONF */
        // la tag admantx  sempre smartworld sui 3 siti
        let admantxOptions = `{"tag": "smartworld.it","key": "4c1d28029914072d7b96a278ef7ef6fd25809a20638848ee8590da4a3cf07bc4","method": "descriptor","filter": "default","decorator": "template.smartworld","type": "URL","mode": "async","body": "${escape(document.location.href)}"}`;
        let decodedAdmantx = 'https://euasync01.admantx.com/admantx/service?request='+admantxOptions;
        let urlAdmantx = encodeURI(decodedAdmantx);

        return mmLoader({
            src: urlAdmantx,
            async: true
        },"advChain");
    }

    const startNeodata = function () {
        /* NEODATA CONF */
        let urlNeodata = `https://c.neodatagroup.com/mediaset.js`;

        if(permutiveEnabled) {
            return mmLoader({
                src: urlNeodata,
                async: true
            },"advChain");
        }

        return false;
    }

    const promises = [startAdmantx(), startNeodata()];

    Promise.allSettled(promises).
    then((results) => {
        console.log('[startAdvChain2022] neodata e admantx elaborati', performance.now() - startTime);
        console.log('[startAdvChain2022]',results)

        //Permutive listen event
        adMantxLoaded = true;
        var adMantxReady = new Event("adMantxReady");
        window.dispatchEvent(adMantxReady);

       
              
        return mmLoader({
            src: "https://adv.mediamond.it/hpsponsor/smartworld/header_cls.v3.js",
            defer: true
        },"advChain").then( () => window.dispatchEvent(new Event("introCanStart") ))
                

    }).then(e => {
        console.log('[startAdvChain2022] mm caricato', performance.now() - startTime);
        const advChainEvent = new Event('advChainLoaded');
        window.dispatchEvent(advChainEvent);
    })
}

postTcfReady(startAdvChain2022);


    function startPermutive() {
        if(permutiveEnabled) {
            // SETUP CONSENT ON PERMUTIVE
            permutive.consent({ "opt_in": true, "token": token });
            console.log(`[permutive] setup consent with token`, token);

            // SETUP PERMUTIVE USER IDENTITIES
            let identities = [
                {"id": getGaClientId(), "tag": "ame_publisher_id", "priority": 1} // AME PUBLISHER ID
            ];
            permutive.identify(identities);
            console.log(`[permutive] setup user identities`, identities);

            // ADDON PERMUTIVE
            adMantxLoaded ? addonPermutive() : window.addEventListener('adMantxReady', () => addonPermutive());
        }
    }


    function addonPermutive() {
        let label = !(ADX_label.includes(ADX_lemmas)) ? `${ADX_label},${ADX_lemmas},${ADX_people},${ADX_categories},${ADX_places},${ADX_companies}` : ADX_label;
        let semantic_label = label.split(',');
        console.log('[Permutive label]' + semantic_label);

        const hour = new Date().getHours();
        let time_of_day = (hour < 12 ? 'Morning' : hour < 17 ? 'Afternoon' : hour < 22 ? 'Evening' : 'Night');

        const d = new Date();
        const weekday = ["Domenica","Luned","Marted","Mercoled","Gioved","Venerd","Sabato"];
        let day_of_week = weekday[d.getDay()];

        let platform = document.documentElement.clientWidth < 480 ? 'web-mobile' : 'web-desktop';
        let addonData = {
            "page":{
                "brand": "Smartworld", // "text"
                "site_name": "smartworld", // "text"
                "publisher": "Mondadori", // "text"
                "platform":`smartworld-${platform}`,
                time_of_day, // "text"
                day_of_week,
                semantic_label, // ["list", "of", "strings"]
                "page": {
                    
                                        
                    "type": "Categoria" // "text"
                },
                
                                
                
                                
                "article": {
                    "authors": "smartworld", // "text"
                    "title": "", // "text"
                },
                "classifications_watson": {
                    "categories": "$alchemy_taxonomy",
                    "keywords": "$alchemy_keywords"
                }
            }
        }
        permutive.addon('web', addonData);
        console.log(`[permutive] addon web`, addonData);
    }

    function postPermutiveIdGeneratorReady() {
        if(typeof permutiveIdGeneratorReady !== 'undefined') {
            startPermutive();
        } else {
            window.addEventListener('permutiveIdGeneratorReady', function () {
                startPermutive();
            });
        } 
    }
    postTcfReady(postPermutiveIdGeneratorReady);

(function(){

    let iframeVideo = document.querySelector('.tw-container-video');
    if(iframeVideo != null) {
        console.log('[pushNoConsent] bloccata per presenza video in pagina');
        return false;
    }
  
    let trackAnalytics = false;

    function startPushNoConsent() {
        if(typeof AVPushRunningInstances !== "undefined"){
            if(typeof AVPushRunningInstances[0] !== "undefined"){
                AVPushRunningInstances[0].manager.destroy();
                console.log('[Debug no-cons] Distruggo la push gi avviata e faccio partire la no consent!');
            }
        }
        console.log('[Debug no-cons] Richiesta No consent in corso...');
        window.initStartPushNoConsent = 1;
        console.log('[Debug no-cons] track init');
        if (typeof gtag === "function" && trackAnalytics == true) {
            gtag("event", 'init', {
              event_category: 'pushNoConsentV1',
              event_label: document.location.href,
              non_interaction: true
            });
        }

        window.GlobalPusTimerStart = new Date().getTime();

        let obj = {
			...pushObjConf,
            eventListener: (eventName, eventData) => {
                console.log('[Debug no-cons] evento:', eventName);
                const eventBlocking = [
                    "adblocker_detected",
                    "preroll_not_served",
                    "autoplay_denied",
                    "triggered_fallback",
                    "preroll_error"
                ];
                if(eventName === 'preroll_started'){
                    console.log('[Debug no-cons]', 'track preroll started');
                     if (typeof gtag === "function" && trackAnalytics == true) {
                        gtag("event", 'preroll_started', {
                        event_category: 'pushNoConsentV1',
                        event_label: document.location.href,
                        non_interaction: true
                        });
                    }
                }
                // [ NPA ] event listeners per interstital ADK (adv fill se non c' preroll)
                if (eventBlocking.includes(eventName)) {
                    if (!adkPageSlots.displayed.adk_interstitial) {
                        console.debug('[ NPA ] init interstitial ADK causa preroll non erogato');
                        window.adkaoraCanShowInterstitial = true;
                    }
                }
            }
        }

        obj['targetElementId'] = 'flyfirst-mobile-placement';
        if (window.innerWidth >= 1000) obj['targetElementId'] = 'flyfirst-placement';

        (function (a, l, t, e, r) {
            a[e] = a[e] || function (s) { (a[e].run = a[e].run || []).push(s) };
            let g, z = l.getElementsByTagName(t)[0];
            if (l.getElementById(e)) { return; }
            g = l.createElement(t); g.id = e; g.src = r; g.async = 1;
            z.parentNode.insertBefore(g, z);
        }(window, document, 'script', 'AVPushLoader', "https://ptp.stbm.it/pushdown/loader/av/pushdown-loader.js"));

        console.log('[pushNoConsent]', obj);

        AVPushLoader(obj);
    }



    // interstitial NPA se preroll non presente

    let startInterstitialNoConsent = () => {
        // [ NPA ] event listeners per interstital ADK (adv fill se non c' preroll)
        const eventBlocking = [
            "adblocker_detected",
            "preroll_not_served",
            "autoplay_denied",
            "triggered_fallback",
            "preroll_error"
        ];
        eventBlocking.forEach(function(e) {
            window.addEventListener(e, function() {
                if (!adkPageSlots.displayed.adk_interstitial) {
                        console.debug('[ NPA ] init interstitial ADK causa preroll non erogato');
                        window.adkaoraCanShowInterstitial = true;
                }
            });
        })
    }

    const ADVNoConsentFlow = () => {
        let NPAtarget = window.innerWidth < 1000 ? 'adv-gpt-box-mobile-container1' : 'adv-gpt-box-container1';
        document.getElementById(NPAtarget).parentNode.parentNode.setAttribute('id','box300NPA');

        setTimeout(function() {
            mmLoader({
                src: "https://cdn.adkaora.space/mondadori/smartworld/"+window.ambiente+"/npa/adk-init.js",
                async: true,
                type: 'module'
            }, "ADVNoConsentFlow")
            .then(element => {
                document.addEventListener("PWTBidComplete", function(e) {
                    console.debug('PWTBid completed.')
                    // check slot vuoti:
                    let emptyNPASlots = {};
                    if(ifrmAdvMh.offsetHeight === 0){
                        Object.assign(emptyNPASlots, { 'masthead': divslotnameStrip });
                    } else {
                        AmeMh.strip_animation();
                    }
                    if(ifrmAdvBox.offsetHeight === 0){
                        Object.assign(emptyNPASlots, { 'box300': divslotnameBox });
                    }
                    document.dispatchEvent(new CustomEvent("EmptyNPASlot", {
                        detail:  emptyNPASlots
                    }));

                });
            }, "ADVNoConsentFlow")
            .then( element => mmLoader({
                src: "https://adv.mediamond.it/librerie/js-no-consent/ame/adv_no_consent.js",
                async: true
            }, "ADVNoConsentFlow"))
            .then(element => {
                startPushNoConsent();
                startInterstitialNoConsent();
            }, "ADVNoConsentFlow");
        },500);

    }

    postConsentRejected(ADVNoConsentFlow);

    // debug only:
    document.addEventListener("EmptyNPASlot", function(e) {
        console.debug('[NPA] EmptyNPASlot event:', e.detail);
    });

})();

function startAdsense() {
	setTimeout(() => {
		mmLoader({
		"data-ad-client": "ca-pub-7835681492882497",
		src: "https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js",
		async: true
		}, "AdsenseChain").then(
		element => {
			refreshAdsense();
		});
	}, 2000);
}
function refreshAdsense() {
    let injectedAd = document.querySelectorAll('.tw-adv-slot-adsense.tw-fragment-injected');
    if (device == 'desktop' && injectedAd.length>0) {
        injectedAd.forEach((el)=>{
            el.remove();
        });
    }
	let blankAdSlots = document.querySelectorAll(".adsbygoogle:not([data-adsbygoogle-status='done'])");
	if(blankAdSlots.length > 0){
		for (var i = 0; i < blankAdSlots.length; i++) {
			(adsbygoogle = window.adsbygoogle || []).push({});
		}
	}				
}


postTcfReady(startAdsense);


</script>

	

    
