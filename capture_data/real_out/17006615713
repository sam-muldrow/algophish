<script>
    function createNode(element) {
        return document.createElement(element);
    }

    function append(parent, el) {
        return parent.appendChild(el);
    }

    const onwar = document.getElementById('onwar');
    const top_onwar = document.getElementById('top_onwar');

    //Set post author api url.
    const author_url = 'https://blogs.tapuz.co.il/wp-json/authorpost/v1/get_get_author_listing/?count=22';

    async function author_function() {
        await fetch(author_url)
            .then((resp) => resp.json())
            .then(function(data) {
                let codes = data.code;
                let authors = data.data;
                //console.log(data.data);

                if (codes == 'success') {
                    var author_counter = 0;
                    return authors.map(function(author) {
                        if (author_counter == 0) {
                            //Map first element in top onwar section

                            //initialize top_ownwar elements
                            let st_img = createNode('div');
                            let st_img_a = createNode('a');
                            let st_img_img = createNode('img');
                            let contants = createNode('div');
                            let contants_h3 = createNode('h3');
                            let contants_h3_a = createNode('a');
                            let contants_div = createNode('div');
                            let contants_div_a = createNode('a');

                            //Set Classe for top_ownwar elements
                            st_img.className = 'st_img';
                            contants.className = 'contants';

                            //Set values for top_ownwar elements
                            st_img_img.src = author.image_url;
                            st_img_a.href = author.link;
                            contants_h3_a.href = author.link;
                            contants_div_a.href = author.link;
                            contants_h3_a.innerHTML = `${author.title}`;
                            contants_div_a.innerHTML = `${author.content}`;

                            //Include elements in top_ownwar section
                            append(st_img_a, st_img_img);
                            append(st_img, st_img_a);
                            append(contants_h3, contants_h3_a);
                            append(contants, contants_h3);
                            append(contants_div, contants_div_a);
                            append(contants, contants_div);
                            append(top_onwar, st_img);
                            append(top_onwar, contants);

                            author_counter++;
                            //console.log(author_counter);

                        } else {

                            //initialize ownwar elements
                            let on_men = createNode('div');
                            let on_men_a_first = createNode('a');
                            let on_men_a_first_img = createNode('img');
                            let on_men_a_second = createNode('a');
                            let on_men_a_second_p = createNode('p');

                            //Set Classes for ownwar elements
                            on_men.className = 'on_men';

                            //Set values for ownwar elements
                            on_men_a_first.href = author.link;
                            on_men_a_first_img.src = author.image_url;
                            on_men_a_second.href = author.link;
                            on_men_a_second_p.innerHTML = `${author.title}`;
                            on_men_a_second_p.style = 'color:' + author.title_color;

                            //Include elements in ownwar section
                            append(on_men_a_first, on_men_a_first_img);
                            append(on_men_a_second, on_men_a_second_p);
                            append(on_men, on_men_a_first);
                            append(on_men, on_men_a_second);
                            append(onwar, on_men);

                            author_counter++;
                            //console.log(author_counter);
                        }
                    })
                } else {
                    /*console.log(data.code);
                    console.log(data.message);*/
                    pdiv.innerHTML = `${data.message}`;
                }
            })
            .catch(function(error) {
                console.log(error);
            });
    }

    author_function();


    const text_img_inner = document.getElementById('text_img_inner');
    //Set post forum api url.
    const forum_url = 'https://blogs.tapuz.co.il/wp-json/authorpost/v1/get_get_forum_listing/?count=22';

    async function forum_function() {
        await fetch(forum_url)
            .then((resp) => resp.json())
            .then(function(data) {
                let codes = data.code;
                let forums = data.data;
                //console.log(data.data);

                if (codes == 'success') {
                    var forum_counter = 0;
                    return forums.map(function(forum) {
                        //initialize ownwar elements
                        let text1 = createNode('div');
                        let text1_div = createNode('div');
                        let text1_div_first_p = createNode('p');
                        let text1_div_first_p_a = createNode('a');
                        let text1_div_second_a = createNode('p');
                        let text1_div_second_p_a = createNode('a');
                        let text1_img = createNode('img');

                        //Set Classes for ownwar elements
                        text1.className = 'text1';
                        text1_div.className = 'text1_parent';

                        //Set values for text_img_inner elements
                        text1_div_first_p_a.href = forum.forum_url;
                        text1_div_first_p_a.innerHTML = `${forum.title}`;

                        text1_div_second_p_a.href = forum.director_url;
                        text1_div_second_p_a.innerHTML = `${forum.director_name}`;

                        text1_img.src = forum.image_url;


                        //Include elements in ownwar section
                        append(text1_div_first_p, text1_div_first_p_a);
                        append(text1_div, text1_div_first_p);
                        append(text1_div_second_a, text1_div_second_p_a);
                        append(text1_div, text1_div_second_a);
                        append(text1, text1_div);
                        append(text1, text1_img);
                        append(text_img_inner, text1);

                        forum_counter++;
                        //console.log(forum_counter);

                    })
                } else {
                    /*console.log(data.code);
                    console.log(data.message);*/
                    text_img_inner.innerHTML = `${data.message}`;
                }
            })
            .catch(function(error) {
                console.log(error);
            });
    }

    forum_function();
</script>

