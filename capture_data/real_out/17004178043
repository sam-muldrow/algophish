                            TrakedPollImp = true;
                        } else {

                        }

                    }

                    $(window).scroll(function () {
                        if ($("#HpPollDiv").isOnScreen() && $("#HpPollEnd").isOnScreen()) {
                            if (TrakedPollImp == false) {
                                console.log("poll shown");
                                TrackEvent("Hp Poll Impresion", "ما-توقعك-لنتيجة-مباراة-مانشستر-سيتي-ضد-ليفربول-في-بريميرليج-", "HpPoll Impresion");
                                TrakedPollImp = true;
                            } else {

                            }

                        }

                    });

                }, 5000);

