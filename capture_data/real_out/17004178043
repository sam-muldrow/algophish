    <script>
        setTimeout(function () {

                    $(function () {
                        PollVote.CheckPrevVote();
                    });


                    var PollChoiceTxt = "";
                    var PollVote = {
                        VoteObj: {
                            PollId: 0,
                            ChoiceId: 0,
                        },
                        LoacalStorageName: "PollStorage-355",
                        SubmitVote: function () {
                            if (PollVote.CheckUSelect()) {
                                PollVote.VoteObj.PollId = $("#PollId").val();
                                PollVote.VoteObj.ChoiceId = $("#PollChoiceDiv").find("input[type='radio']:checked").attr('ChoiceId');
                                PollChoiceTxt = $("#PollChoiceDiv").find("input[type='radio']:checked").attr('ChoiceTxt');
                                var url = AppPath + "/Poll/insertPollVote";
                                TrackEvent("Hp Poll", "ما-توقعك-لنتيجة-مباراة-مانشستر-سيتي-ضد-ليفربول-في-بريميرليج-", PollChoiceTxt);
                                $.ajax({
                                    type: "POST",
                                    dataType: "json",
                                    contentType: "application/json; charset=utf-8",
                                    data: JSON.stringify(PollVote.VoteObj),
                                    url: url,
                                    success: function (result) {
                                        if (result == true) {
                                            PollVote.SubmitVoteSuccess(result);

                                        }
                                    }
                                });
                            }
                            else {
                                //alert("you must choose one option");
                                $(".Popup").show();
                                setTimeout(function () {
                                    $('.Popup').fadeOut('fast');
                                }, 3000);
                            }

                        },
                        SubmitVoteSuccess: function (Result) {
                            if (Result == true) {
                                var PrevVote = General.GetCookie(PollVote.LoacalStorageName);
                                if (PrevVote != "") {
                                    var PrevRateObj = JSON.parse(PrevVote);
                                    PrevRateObj.VotesNo = PrevRateObj.VotesNo + 1;
                                    $("#vcounter").remove();
                                    $("#vcounter2").show();
                                    General.SetCookie(PollVote.LoacalStorageName, JSON.stringify(PrevRateObj), GetPollExpireTime(3))
                                }
                                else {
                                    var Rate = {
                                        PollId: PollVote.VoteObj.PollId,
                                        ChoiceId: PollVote.VoteObj.ChoiceId,
                                        VotesNo: 1,
                                        VotesNoUnit:0,
                                        LastVote: new Date()
                                    }
                                    General.SetCookie(PollVote.LoacalStorageName, JSON.stringify(Rate), JSON.stringify(PrevRateObj), GetPollExpireTime(3))
                                }
                            }

                            $("#PollChoiceDiv").hide();
                            $("#PollResultDiv").show();

                            //if (Result == true) {
                            //    var Rate = {
                            //        PollId: PollVote.VoteObj.PollId,
                            //        ChoiceId: PollVote.VoteObj.ChoiceId,
                            //    }
                            //    PollVote.SaveUserVote(Rate);
                            //    $("#PollChoiceDiv").hide();
                            //    $("#PollResultDiv").show();
                            //}
                        },
                        PollSaveObj: {
                            PollId: 0,
                            ChoiceId: 0,
                        },
                        SaveUserVote: function (RateSaveObj) {
                            var Rates = [];
                            Rates = PollVote.GetUserVotes();
                            Rates.push(RateSaveObj);
                            localStorage.setItem(PollVote.LoacalStorageName, JSON.stringify(Rates));
                        },
                        GetUserVotes: function () {
                            var Rates = JSON.parse(localStorage.getItem(PollVote.LoacalStorageName));
                            if (Rates == null) {
                                var Array = []
                                return Array;
                            }
                            else {
                                return Rates;
                            }
                        },
                        CheckVote: function (PollId) {
                            //var Rates = PollVote.GetUserVotes();
                            //var Uservote = Rates.find(x => x.PollId === PollId);
                            //if (Uservote != undefined || Uservote != null) {
                            //    return Uservote;
                            //}
                            //else {
                            //    return false;
                            //}
                            var PrevVote = General.GetCookie(PollVote.LoacalStorageName);
                            if (PrevVote != "") {
                    
                                return true;

                            }

                            else {
                                return false;
                            }
                        },
                        CheckPrevVote: function () {
                            var PollId = $("#PollId").val();
                            var Result = PollVote.CheckVote(PollId);
                            if (Result == false) {
                                $("#PollChoiceDiv").show();
                            } else {
                                $("#PollResultDiv").show();
                            }
                        },
                        CheckUSelect: function () {
                            if ($('input[name=poll]:checked').length > 0) {
                                return true;
                            } else {
                                return false;
                            }
                        }




                    }

                    $('#PollVote').on("click", function () {
                        if (PollVote.CheckUSelect()) {
                            PollVote.VoteObj.PollId = $("#PollId").val();
                            PollVote.VoteObj.ChoiceId = $("#PollChoiceDiv").find("input[type='radio']:checked").attr('ChoiceId');
                            PollChoiceTxt = $("#PollChoiceDiv").find("input[type='radio']:checked").attr('ChoiceTxt');
                            var url = AppPath + "/Poll/insertPollVote";
                            TrackEvent("Hp Poll", "ما-توقعك-لنتيجة-مباراة-مانشستر-سيتي-ضد-ليفربول-في-بريميرليج-", PollChoiceTxt);
                            $.ajax({
                                type: "POST",
                                dataType: "json",
                                contentType: "application/json; charset=utf-8",
                                data: JSON.stringify(PollVote.VoteObj),
                                url: url,
                                success: function (result) {
                                    if (result == true) {
                                        PollVote.SubmitVoteSuccess(result);

                                    }
                                }
                            });
                        }
                    });

                    function GetPollExpireTime(VotesNo) {
                        var hours = 24 / VotesNo;
                        return hours / 24;
                    }
                }, 5000);

    </script>
    <script>
        setTimeout(function () {

                    var TrakedPollImp = false;

                    if ($("#HpPollDiv").isOnScreen() && $("#HpPollEnd").isOnScreen()) {
                        if (TrakedPollImp == false) {
                            console.log("poll shown");
