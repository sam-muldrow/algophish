    <!--LoadTourStats-->
    <script>
        $(".LoadTourStats").click(function () {
            var tourId = $(this).attr("TourId");
            if ($("#Stats-" + tourId).length > 0) {
                $(".stats").hide();
                $(".TourStats-" + tourId).show();
            }
            else {
                $.ajax({
                    type: "GET",
                    dataType: "html",
                    contentType: "application/json; charset=utf-8",
                    url: AppPath + "/Tour/TourStats",
                    data: { tourId: tourId, page: 'home' },
                    success: function (result) {
                        $(".stats").hide();
                        $(".TourStats-" + tourId).prepend(result);
                        $(".TourStats-" + tourId).show();
                    },
                    error: function (result) {
                        console.log("error geting tour stats of tour " + tourId);
                    }
                });
            }
        });


    </script>

