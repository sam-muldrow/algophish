	<script type="application/ecmascript">
		$(function() {

	
	
			$(window).scroll(scrolls)
			scrolls()
	
			function scrolls() {
				var sTop = $(window).scrollTop();
				if (sTop <= 800) {
					$(".siterightbox").hide()
	
				} else {
					$(".siterightbox").show()
		
				}
			}
	
	
		})
	</script>
