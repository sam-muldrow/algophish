<script type="text/javascript">
  const url = window.location.href;
  const hasCustomerPosted = url.includes('customer_posted=true#contact_form');
  const hasNewsletterTag = url.includes('?contact%5Btags%5D=newsletter&form_type=customer#contact_form');

  if (hasCustomerPosted || hasNewsletterTag) {
    window.addEventListener('load', function() {
      const customActionName = "Sign up for Email";
      const proxyUrl = `${window.Rivo.global_config.proxy_paths.loy}/loy/customers/${window.Rivo.common.customer.id}/record_custom_action`;
      const data = { custom_action_name: customActionName };

      fetch(proxyUrl, {
        method: "POST",
        headers: {
          Accept: "application/json",
          "Content-Type": "application/json",
        },
        body: JSON.stringify(data),
      })
      .then(function (res) {
        if (res.ok) {
          return res.json();
        }
        throw new Error('Network response was not ok ' + res.statusText);
      })
      .then(function (data) {
        console.log('Success:', data);
      })
      .catch(function (error) {
        console.error('There has been a problem with your fetch operation:', error);
      });
    });
  }
</script>


