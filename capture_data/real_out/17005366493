<script>
  $(".level1 ").mouseover(function(e){
    $('.menu-navigation').hide();
    $(this).find('.menu-navigation').show();
  });
  $(document).on('click', 'body', function (e) {
    if((e.target.id != 'level2-menu') && (e.target.id != 'menu-navigation')){
      $('.menu-navigation').hide();
    }
  });
  $(document).on('mouseout', 'body', function (e) {
    $('.menu-navigation').hide();
  });

</script>



