<script>

    if ($(window).width() < 680) {

      let lastScrollTop = 0;
      let cloned = false;
      var header = $('.site-header-main');
      let height = header.outerHeight();

      $(window).scroll(function(event){

        let st = $(this).scrollTop();

        if (st > height && !cloned) {
          header.clone(true).prependTo('body').addClass('scrolling');
          cloned = true;
        } else if (st < height && cloned) {
          $('.site-header-main.scrolling').remove();
          cloned = false;
        }

        if (st < lastScrollTop && cloned && st > height * 2.33){
          $('.site-header-main.scrolling').css('top', '0');
        } else {
          $('.site-header-main.scrolling').css('top', '-' + height + 'px');
        }

        lastScrollTop = st;

      });

      $('.site-header-main .site-header-menu-toggle').click(function() {$('#site-mobile-nav').attr('data-open', 'true');
        $('.mobile-nav-panel').addClass('allow-scroll-while-locked').attr('data-animation-state', 'open');
      });
      $('.mobile-nav-close').click(function() {
        $('#site-mobile-nav').attr('data-open', 'false');
        $('.mobile-nav-panel').removeClass('allow-scroll-while-locked').attr('data-animation-state', 'close');
      });

    }

    /*
    const body = document.body;
    const scrollUp = "scroll-up";
    const scrollDown = "scroll-down";
    let lastScroll = 0;

    window.addEventListener("scroll", () => {
      const currentScroll = window.pageYOffset;
      if (currentScroll <= 0) {
        body.classList.remove(scrollUp);
        return;
      }

      if (currentScroll > lastScroll && !body.classList.contains(scrollDown)) {
        // down
        body.classList.remove(scrollUp);
        body.classList.add(scrollDown);

      } else if (
        currentScroll < lastScroll &&
        body.classList.contains(scrollDown)
      ) {
        // up
        body.classList.remove(scrollDown);
        body.classList.add(scrollUp);
      }
      lastScroll = currentScroll;
    });
  */
</script>

  <script>  
$(document).on("keyup", "[data-live-search-input]", function(e){  
  var term = $(this).val();
  if ((e.which != 40) && (e.which != 38)) {
    if ($(this).val().length > 1) {
      predictive(term);
      showSearch();
    }
    else {
      hideSearch();
    }
  }			
});  

$(document).on("click", "[data-live-search-input]", function(e){
  showInitialSearch();
  if($(this).val() != "") {
    predictive($(this).val());
    showSearch();
    hideInitialSearch();
  }
});

$(document).on("click", "body", function (e) {  
  let searchcontent = $("#predictive-search-results");
  if ($("#predictive-search").css("display") == "block") {
  
    if ((e.target.id != "predictive-search-results") && (e.target.id != "psbox") && searchcontent.has(e.target).length === 0) {
      hideSearch();
    }
  }  
});

$(document).on("click", "[data-trending-search-li]", function(item){
  const searchTerm = item.target.innerHTML
  $("[data-live-search-input]").val(searchTerm);
  const linkQuery = '/search?q=' + searchTerm
  window.location.href = linkQuery
});

$(document).on("click", "[data-live-search-takeover-cancel]", function(){
  $("[data-live-search-input]").val("");
  hideSearch();  
  hideInitialSearch();
});  

$(window).scroll(function (e) {
  var scroll = $(window).scrollTop();
  if (scroll > 6) {
    hideInitialSearch();
  }
});

function hideSearch(){
  $(".algolia-autocomplete").hide();
  $("[data-live-search-takeover-cancel]").hide();
  $("#predictive-search").hide();
  

}

function showInitialSearch() {
  $("#trending-search-terms").show();
}

function hideInitialSearch() {
  $("#trending-search-terms").hide();
}

function showSearch() {
  
      $("#predictive-search").show();
      hideInitialSearch();
  

  $("[data-live-search-takeover-cancel]").show();
}

function predictive(term) {
  jQuery.getJSON("/search/suggest.json", {
    "q": term,
    "resources": {
      "type": "product",
      "limit": 5,
      "options": {
        "unavailable_products": "hide",
        "fields": "title"
      }
    },
  }).done(function (response) {
    var productSuggestions = response.resources.results.products;
    if (productSuggestions.length > 0) {
              
      $("#predictive-search ul").empty();
      $.each(productSuggestions, function (index, item) {
        let suggestion_name = item.title;
        let suggestion_img = item.image;

        if(term != ''){
          if (new RegExp(term).test(suggestion_name.toLowerCase())) {
            suggestion_name = suggestion_name.replace(new RegExp('('+ term +')',"gi"), "<strong>$1</strong>");
          }
        }
        $("#predictive-search ul").append(
          $('<li>').append(
            $('<a>').attr('href', productSuggestions[index].url)
              .attr('class', 'search-link font-btn-sm')
              .append($('<div class="item"><img src="'+`${suggestion_img}`+'"/>')
              .append('<span>'+suggestion_name+'</span>'))
          ));
      });
    }  
  });
}
  </script>


</div>
  
       
