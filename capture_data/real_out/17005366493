<script>

    const cookieDelay = 3000
    const displayPopup = localStorage.getItem('cookie-popup')
    if(!displayPopup) {
        setTimeout(function () {
            document.getElementsByClassName("window")[0].style.display = "flex"
        }, cookieDelay)
    }

    const cookieButton = document.getElementById('cookie-btn')
    cookieButton.addEventListener('click', function() {
        document.getElementsByClassName("window")[0].style.display = 'none'
        localStorage.setItem('cookie-popup', 'seen')
    })
</script>

