<!DOCTYPE html><html dir="ltr" lang="de" data-template="home"><head><meta charSet="utf-8"/><meta name="viewport" content="initial-scale=1.0, width=device-width"/><title>Berliner Verkehrsbetriebe: BVG – Weil wir dich lieben | BVG</title><script>// SET VARIABLE TO 'QA' OR 'LIVE' TO START TEST IN QA OR LIVE ENVIRONMENT
var TEST_ENV = 'LIVE';

// DEFINE MVT CLASS
class conMVT { constructor(a) { this.testName = a.testName, this.groups = a.groups, this.errorTimeout = a.errorTimeout || 1e4, this.unHideTimeout = a.unHideTimeout || 5e3, this.retryTimeout = a.retryTimeout || 50, this.segmentor = a.segmentor || function () { return this.groups[Math.floor(Math.random() * (this.groups.length))] }, this.cookiemode = a.cookiemode || !0, this.variantCodes = [], this.retryTimer = !1, this.retryHideTimer = !1, this.unhidden = !1 } setCookie(a, b, c) { var e = new Date; e.setTime(e.getTime() + 1e3 * (60 * (60 * (24 * c)))); var d = "expires=" + e.toUTCString(); document.cookie = a + "=" + b + "; " + d + ";path=/" } getCookie(a) { for (var b, d = a + "=", e = decodeURIComponent(document.cookie), f = e.split(";"), g = 0; g < f.length; g++) { for (b = f[g]; " " == b.charAt(0);)b = b.substring(1); if (0 == b.indexOf(d)) return b.substring(d.length, b.length) } return "" } getQueryParameter(a) { var b = null, c = []; return location.search.substr(1).split("&").forEach(function (d) { (c = d.split("="))[0].toLowerCase() === a.toLowerCase() && (b = decodeURIComponent(c[1])) }), b } pageHide() { var a = document.querySelector("body"); if ("undefined" != typeof a && null !== a) { var b = document.createElement("div"); b.style.cssText = "width: 100%;height: 100%;position: fixed;top: 0;left: 0;z-index: 2147483647;background-color: white;", b.id = "conMVTPageHideDiv", a.prepend(b), setTimeout(this.unHide.bind(this), this.errorTimeout) } else this.retryHideTimer = setTimeout(this.pageHide.bind(this), 5) } unHide() { !1 !== this.retryTimer && clearTimeout(this.retryTimer), !1 !== this.retryHideTimer && clearTimeout(this.retryHideTimer); var a = document.querySelector("#conMVTPageHideDiv"); "undefined" != typeof a && null !== a && a.remove(), this.unhidden = !0 } addVariantCode(a, b) { this.variantCodes[a] = b } run() { if (this.unhidden) return void console.log("Test was already unhidden, aborting"); var a = this.getQueryParameter("conMVT"), b = this.getCookie("conMVT_" + this.testName), c = 1; if (-1 != this.groups.indexOf(a) && "function" == typeof this.variantCodes[a]) this.cookiemode && this.setCookie("conMVT_" + this.testName, a, 30), c = this.variantCodes[a](); else if (-1 != this.groups.indexOf(b) && "function" == typeof this.variantCodes[b]) c = this.variantCodes[b](); else { var d = this.segmentor(); "function" == typeof this.variantCodes[d] && (this.cookiemode && this.setCookie("conMVT_" + this.testName, d, 30), c = this.variantCodes[d]()) } 1 == c ? this.unHide() : (this.retryTimer = setTimeout(this.run.bind(this), this.retryTimeout), setTimeout(this.unHide.bind(this), this.unHideTimeout)) } };

// TEST CONFIG
var testConfig = {
    testName: "startseite_jelbi-teaser-bild-01",
    groups: ["original", "variant"],
    errorTimeout: 5000, //Timeout bis selbst bei Fehler im Testcode das PageHiding sich entfernt
    unHideTimeout: 2500, //Timeout bis zu dem per retry die Testgenerierung versucht wird
    retryTimeout: 50, //Timeout für den Retry, default 50ms
    cookiemode: true,
    segmentor: function () {
        return this.groups[Math.floor(Math.random() * (this.groups.length))];
    }
};

// CONDITION TO CHECK TEST ENVIRONMENT
if (TEST_ENV == 'QA' && document.location.search.indexOf('abtestpreview=1') >= 0) {
    runTest();
} else if (TEST_ENV == 'LIVE') {
    runTest();
}

// TEST FUNCTION
function runTest() {
    var mvt = new conMVT(testConfig);
    mvt.pageHide();

    // #region original
    mvt.addVariantCode(mvt.groups[0], function () {
        if (document.readyState == 'complete') {
            window.dataLayer = window.dataLayer || [];
            window.dataLayer.push({
                'event': 'customEvent',
                'isNonInteractive': 'true',
                'eventCategory': 'abtest',
                'eventAction': mvt.testName,
                'eventLabel': "original_startseite_jelbi-teaser-bild-01"
            });
            return 1; // trigger unhide
        } else {
            return 2; // do not unhide and retry
        }
    });
    // #endregion

    // #region variant1
    mvt.addVariantCode(mvt.groups[1], function () {
        if (document.readyState == 'complete') {

            let jelbiTeaser = document.querySelector('img[class^= "ContentHeroTeaser_image__"]');
            if (!jelbiTeaser) return 2;

            jelbiTeaser.setAttribute("src" , "/.imaging/mte/bvg-theme/1400w/dam/BVG-Bilder/Startseite/BVG-Jelbi-Mobilitaets-App-Sharing-Berlin.jpg/jcr:content/BVG-Jelbi-Mobilitaets-App-Sharing-Berlin.jpg");
            jelbiTeaser.setAttribute("srcset" , "");
            jelbiTeaser.setAttribute("alt" , "");
           
            window.dataLayer = window.dataLayer || [];
            window.dataLayer.push({
                'event': 'customEvent',
                'isNonInteractive': 'true',
                'eventCategory': 'abtest',
                'eventAction': mvt.testName,
                'eventLabel': "variant_startseite_jelbi-teaser-bild-01"
            });
            return 1; // trigger unhide
        } else {
            return 2; // do not unhide and retry
        }
    });
    // #endregion

    mvt.run();
}</script><meta name="description" content="Dein öffentlicher Nahverkehr in Berlin: Fahrplanauskunft, Tickets, Abos, Apps, Verkehrsmeldungen &amp; Infos für Touristen."/><link rel="manifest" href="/site.webmanifest"/><link rel="icon" type="image/svg+xml" href="/favicon.svg"/><link rel="icon" type="image/svg+xml" sizes="200x200" href="/favicon.svg"/><link rel="alternate icon" href="/favicon.ico"/><link rel="icon" type="image/png" sizes="192x192" href="/android-chrome-192x192.png"/><link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png"/><link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png"/><link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png"/><meta name="msapplication-TileColor" content="#ffffff"/><meta name="theme-color" content="#f0d722"/><meta name="robots" content="index,follow"/><link rel="canonical" href="https://www.bvg.de/de"/><link rel="alternate" hrefLang="de" href="https://www.bvg.de/de"/><link rel="alternate" hrefLang="en" href="https://www.bvg.de/en"/><meta name="next-head-count" content="19"/><link rel="preload" href="/_next/static/css/c96eaea86d57fad6.css" as="style"/><link rel="stylesheet" href="/_next/static/css/c96eaea86d57fad6.css" data-n-g=""/><link rel="preload" href="/_next/static/css/3a088cb295c90508.css" as="style"/><link rel="stylesheet" href="/_next/static/css/3a088cb295c90508.css" data-n-p=""/><link rel="preload" href="/_next/static/css/afc123d3b9de9f2e.css" as="style"/><link rel="stylesheet" href="/_next/static/css/afc123d3b9de9f2e.css"/><link rel="preload" href="/_next/static/css/9b114f34ea58e445.css" as="style"/><link rel="stylesheet" href="/_next/static/css/9b114f34ea58e445.css"/><link rel="preload" href="/_next/static/css/fea4dae3b0fec253.css" as="style"/><link rel="stylesheet" href="/_next/static/css/fea4dae3b0fec253.css"/><link rel="preload" href="/_next/static/css/119910a989e0b845.css" as="style"/><link rel="stylesheet" href="/_next/static/css/119910a989e0b845.css"/><link r