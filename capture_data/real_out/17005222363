<script>
  (function () {
    if (!(navigator.userAgent.match(/Android/i) || navigator.userAgent.match(/iPhone/i) || navigator.userAgent.match(/iPad/i))) {

      var a = document.querySelectorAll(".customer-care-number-link");
      for (i = 0; i < a.length; i++) {
        a[i].href = "javascript.void(0)"
      }
    }
    else {
      var a = document.querySelectorAll(".customer-care-number-link");
      for (i = 0; i < a.length; i++) {
        const b = a[i].innerHTML;
        const noSpecialChars = b.replace(/[^a-zA-Z0-9 ]/g, '');
        const c = noSpecialChars.split(" ").join("");
        a[i].href = "tel:+" + c;
      }
    }
  })();
</script>

