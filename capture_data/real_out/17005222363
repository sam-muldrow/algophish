<script>
    let urlParams = window.location.origin;
    getGa(
        urlParams +
        "/content/air-india/in/en/cookie-policy/jcr:content/root/container/container/envconfig.model.json"
    );
    let envParam = {};
    async function getGa(file) {
        await fetch(file)
            .then((res) => res.json())
            .then((response) => {
                envParam = response;
            });
        if (envParam.environment === "prod") {
            window.dataLayer = window.dataLayer || [];
            function gtag() {
                dataLayer.push(arguments);
            }

            gtag("consent", "default", {
                ad_storage: "denied",
                analytics_storage: "denied",
                wait_for_update: 1500,
            });

            (function (w, d, s, l, i) {
                w[l] = w[l] || [];
                w[l].push({
                    "gtm.start": new Date().getTime(),
                    event: "gtm.js",
                });
                var f = d.getElementsByTagName(s)[0],
                    j = d.createElement(s),
                    dl = l != "dataLayer" ? "&l=" + l : "";
                j.async = true;
                j.src = "https://www.googletagmanager.com/gtm.js?id=" + i + dl;
                f.parentNode.insertBefore(j, f);
            })(window, document, "script", "dataLayer", "GTM-TNLHQDB");

            {

            }

            (function (i, s, o, g, r, a, m) {
                i["GoogleAnalyticsObject"] = r;
                (i[r] =
                    i[r] ||
                    function () {
                        (i[r].q = i[r].q || []).push(arguments);
                    }),
                    (i[r].l = 1 * new Date());
                (a = s.createElement(o)), (m = s.getElementsByTagName(o)[0]);
                a.async = 1;
                a.src = g;
                m.parentNode.insertBefore(a, m);
            })(
                window,
                document,
                "script",
                "https://www.google-analytics.com/analytics.js",
                "ga"
            );
            ga("send", "pageview");

            var _siteID = "AEM_NEW_SITE";
            ga("set", "dimension1", _siteID);

            function consentGrantedforad() {
                gtag("consent", "update", {
                    ad_storage: "granted",
                    analytics_storage: "denied",
                });
            }
            function consentGrantedforga() {
                gtag("consent", "update", {
                    ad_storage: "denied",
                    analytics_storage: "granted",
                });
            }
            function consentGrantedforboth() {
                gtag("consent", "update", {
                    ad_storage: "granted",
                    analytics_storage: "granted",
                });
            }
            // function getCookie(name) {
            //     let cookie = {};
            //     document.cookie.split(";").forEach(function (el) {
            //         let [k, v] = el.split("=");
            //         cookie[k.trim()] = v;
            //     });
            //     return cookie[name];
            // }

            // var cookieCategories = [];
            // console.log("dfghjkjhgfdfghj");
            // setTimeout(function () {
            //     cookieCategories = JSON.parse(getCookie("ai_cookie")).categories;

            //     // for (let i = 0; i < cookieCategories.length; i++) {
            //     if (cookieCategories[1] == "enable_performance"
            //         && cookieCategories[2] == "enable_targeting") { consentGrantedforboth(); }
            //     else {
            //         if (cookieCategories[2] == "enable_targeting") { consentGrantedforad(); } else if (
            //             cookieCategories[1] == "enable_analytics" || cookieCategories[1] == "enable_performance") { consentGrantedforga(); }
            //     }
            //     // }
            // }, 10000);
        }
    }
</script>
    

    

    
    

        
