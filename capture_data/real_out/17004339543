<script data-experiment-id="wpp_homepage_merchandising_live_demo_series">(function main () {
  try {
    window.StripeExperiments.useTreatment(
      document.currentScript,
      document.currentScript.dataset.experimentId
    );
  } catch (caughtErr) {
    const message = `Could not swap components for experiment ${document.currentScript.dataset.experimentId}`;
    throw new Error(message, { cause: caughtErr });
  }
})();
</script>

