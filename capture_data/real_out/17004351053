    <script data-type="lazy" data-src="https://x.nab.com.au/cbx/nab-chat-widget/khoros/nab-chat-widget.js"></script>
    <script>
        window.addEventListener('load', () => {
            const loadScriptsTimer = setTimeout(triggerScriptLoader, 2500);
            // Interaction events
            const events = ['mouseover', 'keydown', 'touchmove', 'touchstart'];
            events.forEach((evt) => {
                window.addEventListener(evt, triggerScriptLoader, {
                    passive: true
                });
            })
            function triggerScriptLoader(event) {
                clearTimeout(loadScriptsTimer);
                events.forEach((evt) => {
                    window.removeEventListener(evt, triggerScriptLoader, {
                        passive: true
                    })
                });
                loadScripts();
            }
            function loadScripts(event) {
                document.querySelectorAll(`script[data-type='lazy']`).forEach((ele) => {
                    ele.setAttribute('src', ele.getAttribute("data-src"));
                })
            }
        }
    )
    </script>
    <nab-chat-widget></nab-chat-widget>


                </div>
            </main>
