<script>
    if(typeof renderScript !== 'function'){
        window.renderScript = (enabled, begin, expire) => {
            if (!enabled) return;
            const now = Date.now();
            if (begin) {
                if (expire) {
                    return (now >= Date.parse(begin)) && (now <= Date.parse(expire));
                };
                return now >= Date.parse(begin);
            } else {
                if (expire){
                    return now <= Date.parse(expire);
                } else {
                    return true;
                };
            };
        }
    }
    if(typeof addStyle !== 'function'){
        window.addStyle = (styleString) => {
            const style = document.createElement('style');
            style.textContent = styleString;
            document.head.append(style);
        }
    }
</script>

<div tcom-injected-javascript>
    
    
    
    
</div>

<div tcom-injected-css>
    
    
        
        
    
    
        
            <script>
                if (renderScript("true" === 'true', "", "")){
                    addStyle(`.tcom-able__palette-misc-black .tcom-countdown__wrapper .tcom-countdown__close-button-icon use {
  fill: white !important;
}

@media (max-width: 767.95px) {
  .tcom-ncampaignbanner-edgeImage img {
    width: 100% !important;
  }
}

@media (min-width: 768px) {
  .tcom-ncampaignbanner-mainImage {
    border-radius: 0 !important;
  }
}`);
                }
            </script>
        
    
    
</div>
