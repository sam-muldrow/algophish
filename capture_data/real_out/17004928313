  <script>
    var digitalData = digitalData || {};
    var attDiv = "", attSubDiv = "", attSubDivUnit = "";
    var primaryCategory = "corporate " || window.location.pathname.split('/')[1] || "";
    switch (primaryCategory) {
      case 'small-business':
        attDiv = 'TD'; attSubDiv = 'TBE' ; attSubDivUnit = 'TBE' ;
        break;
      case 'business-enterprise':
        attDiv = 'TD'; attSubDiv = 'TBE' ; attSubDivUnit = 'TBE' ;
        break;
      default: 
        attDiv = 'TD'; attSubDiv = 'TR' ; attSubDivUnit = 'TR' ;
    }
    digitalData.page = {
        pageInfo: {
            pageName: "telstra corporate",
            destinationURL: "\/content\/tcom",
            referringURL: document.referrer,
            sysEnv: window.navigator.userAgent,
            variant: "default",
            language: "en_AU",
            deployEnv: "production"
        },
        category: {
            primaryCategory:primaryCategory,
            secondaryCategory: "" || window.location.pathname.split('/')[2] || ""
        },
        attributes: {
          division: "" || attDiv,
          subDivision: "" || attSubDivUnit,
          subDivisionUnit: "" || attSubDivUnit
        }
    };
    // Enterprise
    if (primaryCategory === 'business-enterprise') {
      digitalData.page.category.tertiaryCategory = "" || window.location.pathname.split('/')[3] || ""
    }
  </script>
  
  
  
  



