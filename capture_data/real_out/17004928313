<script>
    if(typeof renderScript !== 'function'){
        window.renderScript = (enabled, begin, expire) => {
            if (!enabled) return;
            const now = Date.now();
            if (begin) {
                if (expire) {
                    return (now >= Date.parse(begin)) && (now <= Date.parse(expire));
                };
                return now >= Date.parse(begin);
            } else {
                if (expire){
                    return now <= Date.parse(expire);
                } else {
                    return true;
                };
            };
        }
    }
    if(typeof addStyle !== 'function'){
        window.addStyle = (styleString) => {
            const style = document.createElement('style');
            style.textContent = styleString;
            document.head.append(style);
        }
    }
</script>

<div tcom-injected-javascript>
    
    
    
    
</div>

