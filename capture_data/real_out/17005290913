<script type="module">  import { getCLS, getFCP, getFID, getLCP, getTTFB } from 'https://unpkg.com/web-vitals@2.1.4/dist/web-vitals.js?module';

  (function() {
      
    var webVitalsInfo = {};
    getCLS(function(CLS) {
        webVitalsInfo.CLS = CLS.value;
    }, true);
    getFCP(function(FCP) {
        webVitalsInfo.FCP = FCP.value;
    });
    getFID(function(FID) {
        webVitalsInfo.FID = FID.value;
    });
    getLCP(function(LCP) {
        webVitalsInfo.LCP = LCP.value;
    }, true);
    getTTFB(function(TTFB) {
        webVitalsInfo.TTFB = TTFB.value;
    });
    
    var eventFired = false;
    function fireWebVitalsEvent() {
        if(eventFired) return;
        var eventData = Object.assign({
           "transport_type": "beacon",
           "non_interaction": true
          },
        webVitalsInfo,
      );
        
      gtag("event", "webvitals_submit", eventData);
      eventFired = true;
    }
    
    var vcTimeout = false;
    setTimeout(function() {vcTimeout=true;}, 30000);
    window.addEventListener('beforeunload', fireWebVitalsEvent);
    window.addEventListener('visibilitychange', function() {
        if (document.hidden && vcTimeout) fireWebVitalsEvent();
    });
    
  })();</script>
