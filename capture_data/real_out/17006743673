$('.new_foot_social').html($('.more_main_inside .big_menu_right').html());</script><script type="text/javascript">if(window_width<=800) {
var social_url_generator = function(url_main){
    url_main = url_main.split('/');
    //console.log(url_main);

    for( var i = 0; i < url_main.length; i++){ 
        if ( i > 3) {
            url_main.splice(i, 1);
            i--; 
        }
    }
    return url_main.join('/');
}

$('.content_detail_each_group').each(function(i,v){
    var share_title = $(this).find('h1[itemprop="headline"]').text();
    /*var url_single = $(this).find('meta[itemprop="mainEntityOfPage"]').attr('content');
    var url_encode = social_url_generator('https:'+url_single);*/
    //console.log(url_encode);
    var fburl = 'https://www.facebook.com/sharer/sharer.php?u='+encodeURI(jw_meta_url)+'&app_id=436163869915872';
    var twitterurl = 'https://twitter.com/intent/tweet?text='+share_title+'&url='+encodeURI(jw_meta_url);
    var viberurl = 'viber://forward?text='+share_title+' '+encodeURI(jw_meta_url);
    var whatsurl = 'whatsapp://send?text='+share_title+' '+encodeURI(jw_meta_url);
    var social_push = '<div class="roundicons aligncenter">'
    +'<a title="Facebook" href="'+fburl+'" target="_blank"><span class="icon icon_facebook"></span><span class="t">Facebook</span></a>'
    +'<a title="Twitter" href="'+twitterurl+'" target="_blank"><span class="icon icon_twitter"></span><span class="t">Twitter</span></a>'
    +'<a title="Viber" href="'+viberurl+'" target="_blank"><span class="icon icon_viber"></span><span class="t">Viber</span></a>'
    +'<a title="Whatsapp" href="'+whatsurl+'" target="_blank"><span class="icon icon_whatsapp"></span><span class="t">Whatsapp</span></a>'
    +'</div>';
    $(this).find('.additional_info_container').append(social_push);
});

