        var today_iftar = new Date(monthNames[d.getMonth()] + ' ' + d.getDate() + ', ' + d.getFullYear() + ' ' + sehri_and_iftar_today[1]);
        var today_sehri = new Date(monthNames[d.getMonth()] + ' ' + d.getDate() + ', ' + d.getFullYear() + ' ' + sehri_and_iftar_today[0]);
        var tomorrow_iftar = new Date(monthNames[d.getMonth()] + ' ' + (d.getDate() + 1) + ', ' + d.getFullYear() + ' ' + sehri_and_iftar_tomorrow[1]);
        var tomorrow_sehri = new Date(monthNames[d.getMonth()] + ' ' + (d.getDate() + 1) + ', ' + d.getFullYear() + ' ' + sehri_and_iftar_tomorrow[0]);
        //alert(today_sehri.getHours());
        //alert(parseInt(Date.parse(d)));
        function delays_new_time() {
            d = new Date();
            sehri_iftar_set_time();
        }

        function sehri_iftar_set_time() {
            //alert(parseInt(Date.parse(d)));
            if ((Date.parse(today_sehri) - Date.parse(d)) < 0) {
                document.querySelector('.bt_sehri').innerHTML = '<div class="bt_sehri_text">কাল সেহরি</div>' + '<div class="btSehriTimeBox staticTimeBox">' + formatNumbers(tomorrow_sehri.getHours().toString()) + ':' + formatNumbers(('0' + tomorrow_sehri.getMinutes().toString()).slice(-2)) + '</div>';
            } else {
                document.querySelector('.bt_sehri').innerHTML = '<div class="bt_sehri_text">আজ সেহরি</div>' + '<div class="btSehriTimeBox staticTimeBox">' + formatNumbers(today_sehri.getHours().toString()) + ':' + formatNumbers(('0' + today_sehri.getMinutes().toString()).slice(-2)) + '</div>';
            }
            if ((Date.parse(today_iftar) - Date.parse(d)) < 0) {
                document.querySelector('.bt_iftar').innerHTML = '<div class="bt_iftar_text">কাল ইফতার</div>' + '<div class="btIftarTimeBox staticTimeBox">' + formatNumbers((tomorrow_iftar.getHours() - 12).toString()) + ':' + formatNumbers(tomorrow_iftar.getMinutes().toString()) + '</div>';
            } else {
                document.querySelector('.bt_iftar').innerHTML = '<div class="bt_iftar_text">আজ ইফতার</div>' + '<div class="btIftarTimeBox staticTimeBox">' + formatNumbers((today_iftar.getHours() - 12).toString()) + ':' + formatNumbers(today_iftar.getMinutes().toString()) + '</div>';
            }
            document.querySelector('.iftar_and_sehri').style.display = 'block';
        }
        var display = document.querySelector('#timerBox');
        //alert(totalSecondsLeft);
        if ((parseInt(today_iftar - d)) < 60 * 60000 && (parseInt(today_iftar - d)) > 0) {
            document.querySelector('.iftar_and_sehri').style.display = 'none';
            startTimer(display);
            display.style.display = "grid";
        } else {
            display.style.display = "none";
            sehri_iftar_set_time();
        }

        function startTimer(display) {
            var __timer = setInterval(function() {
                d = new Date();
                var totalSecondsLeft = (Date.parse(today_iftar) - Date.parse(d)) / 1000;
                var timer = totalSecondsLeft,
                    minutes, seconds;
                hoursLeft = parseInt(timer / 3600, 10);
                minutes = parseInt(timer / 60, 10);
                seconds = parseInt(timer % 60, 10);

                minutes = minutes > 59 ? minutes % 60 : minutes;
                minutes = minutes < 10 ? "0" + minutes : minutes;
                seconds = seconds < 10 ? "0" + seconds : seconds;
                //display.querySelector('.btHours').textContent = formatNumbers(hoursLeft.toString());
                display.querySelector('.bt_minutes').textContent = formatNumbers(minutes.toString());
                display.querySelector('.bt_seconds').textContent = formatNumbers(seconds.toString());
                //display.textContent =  + " মিনিট :" + formatNumbers(seconds.toString())+" সেকেন্ড";

                if (--timer < 0) {
                    clearInterval(__timer);
                    demo();
                    display.style.display = "none";
                    delays_new_time();
                    sehri_iftar_set_time();
                    timer = duration;
                }
            }, 1000);
        }
