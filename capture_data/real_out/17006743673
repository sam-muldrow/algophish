var adSlotInnerA2C1, adSlotInnerA2T1, adSlotInnerA2B1, adSlotInnerA2AfterImage;
function moreAd_BT_A2() {
    googletag.cmd.push(function() {
        adSlotInnerA2T1 = googletag.defineSlot('/67573540/BT_inner_A2_T1', [[970, 90], [728, 90], [320, 100], [320, 50]])
            .addService(googletag.pubads());
        adSlotInnerA2T1.defineSizeMapping(mapping_970x728x320);
        
        adSlotInnerA2C1 = googletag.defineSlot('/67573540/BT_inner_A2_C1', [300, 250])
            .addService(googletag.pubads());
            
        adSlotInnerA2B1 = googletag.defineSlot('/67573540/BT_inner_A2_B1', [[970, 90], [728, 90], [320, 100], [320, 50]])
            .addService(googletag.pubads());
        adSlotInnerA2B1.defineSizeMapping(mapping_970x728x320);
            
        adSlotInnerA2AfterImage = googletag.defineSlot('/67573540/BT_inner_A2_after_image', [[728, 90], [320, 100], [320, 50]])
            .addService(googletag.pubads());
        adSlotInnerA2AfterImage.defineSizeMapping(mapping_728x320x100x50);

        let divInnerA2T1 = document.createElement('div');
        let divInnerA2C1 = document.createElement('div');
        let divInnerA2B1 = document.createElement('div');
        let divInnerA2AftrImg = document.createElement('div');
        divInnerA2T1.id = adSlotInnerA2T1.getSlotElementId(); // auto-generated by GPT
        divInnerA2C1.id = adSlotInnerA2C1.getSlotElementId(); // auto-generated by GPT
        divInnerA2B1.id = adSlotInnerA2B1.getSlotElementId(); // auto-generated by GPT
        divInnerA2AftrImg.id = adSlotInnerA2AfterImage.getSlotElementId(); // auto-generated by GPT
        document.getElementById('bT_inner_A2_T1_cont').appendChild(divInnerA2T1);
        document.getElementById('bT_inner_A2_C1_cont').appendChild(divInnerA2C1);
        document.getElementById('bT_inner_A2_B1_cont').appendChild(divInnerA2B1);
        document.getElementById('bT_inner_A2_AftrIma_cont').appendChild(divInnerA2AftrImg);
        googletag.pubads().disableInitialLoad();
        googletag.display(adSlotInnerA2T1);
        googletag.display(adSlotInnerA2C1);
        googletag.display(adSlotInnerA2B1);
        googletag.display(adSlotInnerA2AfterImage);
    });
}
