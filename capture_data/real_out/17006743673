		var jw_progressive_load_items = $('.jw_progressive_load');
		var jw_progressive_load_counter = 0;
		var jw_progressive_load_before = $(window).height() * 2;
		var jw_progressive_loading = false;
		function jw_progressive_load(){
			if( jw_progressive_load_counter >= jw_progressive_load_items.length ){
				return;
			}
			var scrollpos = $(window).scrollTop();
			var wrapper = $(jw_progressive_load_items[jw_progressive_load_counter]);
			var offset = wrapper.offset().top;
			var id = wrapper.attr('id');
			var id = id.split(/\_/)[1];
			if( scrollpos >= offset - jw_progressive_load_before ){
				if( !jw_progressive_loading ){
					var ths = $('#ajax_load_more_');
					//ajax load
					jw_progressive_loading = true;

					//if local data available then serve from there and do not ajax call
					if( typeof(_jw_progresssive_load_data[id]) != 'undefined' ){
						$(ths).css({visibility:'visible'});
						wrapper.html(_jw_progresssive_load_data[id]);
						jw_progressive_load_counter++;
						jw_progressive_loading = false;
					}
										else{
						$.ajax({
							beforeSend: function(){
								$(ths).css({visibility:'visible'});
								},
							complete: function(){
								jw_progressive_loading = false;
								},
							cache:true,
							type: 'get',
							url: jw_progressive_load_url,
							data: 'progressiveload='+ id,
							dataType:'json',
							success: function(jsondata){
								if( jsondata && jsondata['html'] ){
									wrapper.html(jsondata['html']);
									}
								jw_progressive_load_counter++;
							},
							error: function(msg){
								jw_progressive_loading = false;
								}
							});
						}
									}
			}
		}
		$(document).ready(function(){
			$(window).scroll(jw_progressive_load);
		});</script><div class="foot-portion"><div class="footer_logo_and_applink"><div class="foot-middle-container"><div class="foot_site_logo"><a href="/"><img height="60" src="//cdn.banglatribune.net/contents/themes/public/style/images/logo.svg" alt="Bangla Tribune" /></a></div><div class="app_links"><a target="_blank" href="https://itunes.apple.com/gb/app/bangla-tribune/id1453374223?mt=8"><img height="40" src="//cdn.banglatribune.net/contents/themes/public/style/images/appstore.png" alt="app store" /><span>Available for<br>iPhones & iPads</span></a>&nbsp;<a target="_blank" href="https://play.google.com/store/apps/details?id=com.banglatribune.banglatribune"><img height="40" src="//cdn.banglatribune.net/contents/themes/public/style/images/googleplay.png" alt="google play" /><span>Available in<br>Google Play</span></a></div></div></div><div class="foot_wrap_bg" style="
				
