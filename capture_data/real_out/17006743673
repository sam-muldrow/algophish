//grab the chnaged attribute
var adPlaceForNextArticles = function(attributeValue){
    if(attributeValue.includes('content_item_1')) { //2nd article
        $('.content_detail_each_group.content_item_1 .news_divider').after('<div id="bT_inner_A2_T1_cont" class="aligncenter"></div>');
        $('.content_detail_each_group.content_item_1 .jw_article_body p:nth-child(2)').after('<div id="bT_inner_A2_C1_cont" class="aligncenter pb10 pt10"></div>');
        $('.content_detail_each_group:eq(1) .featured_image').after('<div id="bT_inner_A2_AftrIma_cont" class="aligncenter pb10 pt10"></div>');
        $('.content_detail_each_group.content_item_1 .detail_inner .initials').after('<div id="bT_inner_A2_B1_cont" class="aligncenter pb10 pt10"></div>');
        moreAd_BT_A2();
        googletag.cmd.push(function() {
            googletag.pubads().refresh([adSlotInnerA2T1,adSlotInnerA2C1,adSlotInnerA2B1]);
        });
        //console.log(attributeValue);
    } else if (attributeValue.includes('content_item_2')) { //3rd article
        $('.content_detail_each_group.content_item_2 .news_divider').after('<div id="bT_inner_A3_T1_cont" class="aligncenter"></div>');
        $('.content_detail_each_group.content_item_2 .jw_article_body p:nth-child(2)').after('<div id="bT_inner_A3_C1_cont" class="aligncenter pb10 pt10"></div>');
        moreAd_BT_A3();
        googletag.cmd.push(function() {
            googletag.pubads().refresh([adSlotInnerA3T1,adSlotInnerA3C1]);
        });
        //console.log(attributeValue);
    } else if (attributeValue.includes('content_item_3')) { //4th article
        $('.content_detail_each_group.content_item_3 .news_divider').after('<div id="bT_inner_A4_T1_cont" class="aligncenter"></div>');
        $('.content_detail_each_group.content_item_3 .jw_article_body p:nth-child(2)').after('<div id="bT_inner_A4_C1_cont" class="aligncenter pb10 pt10"></div>');
        moreAd_BT_A4();
        googletag.cmd.push(function() {
            googletag.pubads().refresh([adSlotInnerA4T1,adSlotInnerA4C1]);
        });
        //console.log(attributeValue);
    } else if (attributeValue.includes('content_item_4')) { //5th article
        $('.content_detail_each_group.content_item_4 .news_divider').after('<div id="bT_inner_A5_T1_cont" class="aligncenter"></div>');
        $('.content_detail_each_group.content_item_4 .jw_article_body p:nth-child(2)').after('<div id="bT_inner_A5_C1_cont" class="aligncenter pb10 pt10"></div>');
        moreAd_BT_A5();
        googletag.cmd.push(function() {
            googletag.pubads().refresh([adSlotInnerA5T1,adSlotInnerA5C1]);
        });
        //console.log(attributeValue);
    } else if (attributeValue.includes('content_item_5')) { //6th article
        $('.content_detail_each_group.content_item_5 .news_divider').after('<div id="bT_inner_A6_T1_cont" class="aligncenter"></div>');
        $('.content_detail_each_group.content_item_5 .jw_article_body p:nth-child(2)').after('<div id="bT_inner_A6_C1_cont" class="aligncenter pb10 pt10"></div>');
        moreAd_BT_A6();
        googletag.cmd.push(function() {
            googletag.pubads().refresh([adSlotInnerA6T1,adSlotInnerA6C1]);
        });
        //console.log(attributeValue);
    } else {
        //nothing
    }
}
if(!worldCup2023) {
    window.addEventListener('locationchange', function(){
        $('.content_detail_each_group').each(function(i, v){
            var dataJWmedia = $(this).find('.jw_content_detail_each').attr('data-jwitemdata');
            if(dataJWmedia === undefined && typeof dataJWmedia == 'undefined') {
                return;
            }
            dataJWmedia = JSON.parse(dataJWmedia);
            if('https:'+dataJWmedia.url == window.location.href && !v.classList.contains('ad_placed_'+i) && i != 0) {
                adPlaceForNextArticles(v.className);
                v.classList.add('ad_placed_'+i);
                console.log(dataJWmedia.url + ': class :' + v.className);
            }
            
        });
    })
