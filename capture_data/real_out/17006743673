        var d = new Date();
        //alert(d.getHours());
        var sehri_and_iftar_today = sehri_iftar[d.getDate()].split(",");
        if (d.getDate() > 30)
            var sehri_and_iftar_tomorrow = sehri_iftar[1].split(",");
        else {
            var sehri_and_iftar_tomorrow = sehri_iftar[d.getDate() + 1].split(",");
