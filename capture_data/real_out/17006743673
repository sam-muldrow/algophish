					function jw_mega_menu_open_style(){

						if( !$('.more_main_menu').hasClass('opened') ){
							$('.more_main_menu_wrap').toggle();
							setTimeout(function(){
								$('.more_main_menu_wrap').toggleClass('opened'); 
							},100)
							
							$('body').toggleClass('no-scrollbar');
							$('.more_main_menu').toggleClass('opened');
							$('.mh_toggle').toggleClass('db');
						}
						else{
							$('.more_main_menu_wrap').toggleClass('opened');
							setTimeout(function(){
								$('.more_main_menu_wrap').toggle();
								$('body').toggleClass('no-scrollbar');
								$('.more_main_menu').toggleClass('opened');
								$('.mh_toggle').toggleClass('db');
							},500)
							
							
						}
						
					}
					$('.more_main_menu').click(function(){ 
						jw_mega_menu_open_style();
						});
					$('.close_more_main').click(function(){
						jw_mega_menu_open_style();
						});
					$('.more_main_menu_wrap' ).click(function(e){
						if(e.target !== e.currentTarget) return;
						jw_mega_menu_open_style();
					});


					$('.big_menu_all > ul > li').hover(function(){
						$(this).addClass('opennow');

						ths_height = $(this).height();

						var sth = 70;
						
						var t = $(this).offset().top - sth - $(window).scrollTop();
						
						var uh = $(this).find('>div').height();
						var window_height = $(window).height();
						
						var container_height = $('.more_main_menu_inner').height();

						var sh = $('.more_main_menu_wrap').scrollTop();

						var container_top = $('.more_main_menu_inner').offset().top - sth - $(window).scrollTop();

						

						var available = t-container_top;

						var y = t;
						
						$(this).find('>ul').wrap('<div>');

						$(this).find('>div >ul').css({'top': y +'px'});

						return;

						var ys = 0;

						if( available > uh  ){
							ys = 200;
						}

						if( uh > available  ){
							y = t + ( available - uh - ths_height );
						}

						if( uh > container_height ){
							t = container_height;
							ys = 0;
						}
						

						if( ys ){
							$(this).find('>ul').css({'bottom':'-'+ys+'px'});
							$(this).find('>ul').css({'top':'auto'});

						}
						else{
							
							$(this).find('>ul').css({'top':'-'+ y +'px'});
							$(this).find('>ul').css({'bottom':'auto'});

						}
						
						
					},function(){
						$(this).removeClass('opennow');
					});

				var __t = 0;

				$('.trending_menu_wrap .next').click(function(){
					__t += 3;
					var x = $('.trending_menu_wrap li').length;
					if( __t > x ){ 
						__t = x-1;
						$('.trending_menu_wrap .next').hide();
					}
					$('.trending_menu_wrap .prev').css({display:'flex'});
					$('.trending_menu_wrap li').get(__t).scrollIntoView(false);
				});

				$('.trending_menu_wrap .prev').click(function(){
					__t -= 3;
					if( __t < 0 ){
						__t = 0;
						$('.trending_menu_wrap .prev').hide();
					}
					$('.trending_menu_wrap .next').css({display:'flex'});
					$('.trending_menu_wrap li').get(__t).scrollIntoView(false);
				});

									
				var jw_element_pos = $('.header_menu_wrap').offset();
				var jw_change_me_now = true;
				var sticky_menu_interactive = function(){
					if($(window).scrollTop() > jw_element_pos.top){
						if( jw_change_me_now ){
							$('body').addClass('sticky_menu');
							jw_change_me_now = false;
							}	
						}
					else{
						if( !jw_change_me_now ){
							$('body').removeClass('sticky_menu');
							jw_change_me_now = true;
							}
						}
					}
				$(window).scroll(sticky_menu_interactive).resize(sticky_menu_interactive);</script><div class="header_ad_block advertisement"><script>//9th anniversary logo
