<script>
    function mobileLogonMenuButtonClick() {
    var isIphone = [
        'iPhone Simulator',
        'iPhone',
    ].includes(navigator.platform);
    var isIpad = (navigator.platform === 'MacIntel' && navigator.maxTouchPoints > 0) || navigator.platform === 'iPad';

    var androidAppName = 'https://apiebank3.danskebank.com/com.danskebank.mobilebank3.dk';
    var iphoneAppName = 'dbmobilebank3dk://';
    var ipadAppName = 'dbmobilebank3dk://'
    var playStoreLocation = 'https://play.google.com/store/apps/details?id=com.danskebank.mobilebank3.dk';
    var appStoreIphoneLocation = 'https://apps.apple.com/dk/app/den-nye-mobilbank-dk-danske-bank/id1133885137?l=da&platform=iphone';
    var appStoreLocation = 'https://apps.apple.com/dk/app/den-nye-mobilbank-dk-danske-bank/id1133885137?l=da';
    var storeLocation = playStoreLocation;
    var appLocation = androidAppName;
    if (isIpad) {
        storeLocation = appStoreLocation;
        appLocation = ipadAppName;
    }

    if (isIphone) {
        storeLocation = appStoreIphoneLocation;
        appLocation = iphoneAppName;
    }

    var installed = false;
    if (isIpad || isIphone) {
        window.onblur = function () {
            installed = true;
        };
        window.onfocus = function(){
            installed = false;
        }
    }
    
    setTimeout(function () { 
        if(!installed) {
        window.location = storeLocation;
     }
    }, 700);
    window.location = appLocation;
}
</script>
