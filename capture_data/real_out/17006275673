    <script>
        onDemandScript("/-/media/files/forms/buy-budget/js/3.chunk.js");
onDemandScript("/-/media/files/forms/buy-budget/js/2.chunk.js");
onDemandScript("/-/media/files/forms/buy-budget/js/main.chunk.js");

function onDemandScript( url, callback ) {
    callback = (typeof callback != 'undefined') ? callback : {};

    $.ajax({
         type: "GET",
         url: url,
         success: callback,
         dataType: "script",
         cache: true
     });    
}



