<script>
function hdv3HomeSearchFuncIn() {
$('#hdv3HomeSearchButtonID').addClass('hiddenAtLoad');
$('#hdv3HomeSearchProcessingID').removeClass('hiddenAtLoad');
window.addEventListener("unload", function(event) {
$('#hdv3HomeSearchButtonID').removeClass('hiddenAtLoad');
$('#hdv3HomeSearchProcessingID').addClass('hiddenAtLoad');
});
window.addEventListener("pageshow", function(event) {
if (event.persisted) {
}
$('#hdv3HomeSearchButtonID').removeClass('hiddenAtLoad');
$('#hdv3HomeSearchProcessingID').addClass('hiddenAtLoad');
});
document.getElementById('hdv3HomeSearchFormID').submit();
return false;
}
</script>
