	var mobileWidth = 0;
	var md = new MobileDetect(window.navigator.userAgent);
	if (screen.width <= 768) {
		mobileWidth = screen.width;
	}

	var graphRatio = .4;
	if (mobileWidth) {
		graphWidth = mobileWidth - 30;
	} else {
		graphWidth = 560;
	}

	graphHeight = graphRatio * graphWidth;

	graphView(index_value_dsbi, index_value_dses, index_value_ds30, index_value_cdset);

	var test = window.matchMedia("(orientation: portrait)");
	test.addListener(function (m) {

		var mobileWidth = 0;
		var md = new MobileDetect(window.navigator.userAgent);
		if (screen.width <= 768) {
			mobileWidth = screen.width;
		}

		var graphRatio = .4;
		if (mobileWidth) {
			graphWidth = mobileWidth - 30;
		} else {
			graphWidth = 560;
		}

		graphHeight = graphRatio * graphWidth;
		graphView(index_value_dsbi, index_value_dses, index_value_ds30, index_value_cdset);
	});

	function graphView() {
		g = new Dygraph(
			document.getElementById("graphdiv2"),

			"Date,Index\n" +
			index_value_dsbi, {
				legend: 'always',
				animatedZooms: false,
				title: 'DSE Broad Index',
				titleHeight: 19,
				colors: ['#2865a0', '#2865a0'],
				series: {
					'Index': {
						fillGraph: true,
						strokeWidth: 1,
						highlightCircleSize: 3
					}
				}
			});
		g.resize(graphWidth, graphHeight)
		g2 = new Dygraph(
			document.getElementById("graphdiv3"),

			"Date,Index\n" +
			index_value_dses, {
				legend: 'always',
