		$(".profiles").find(".tabs").find("a").click(function(){
		    tab = $(this).attr("href");
		    $(".profiles").find(".tab").hide();
		    $(".profiles").find(".tabs").find("a").removeClass("active");
		    $(tab).show();
		    $(this).addClass("active");
		    return false;
		});

