        <script type="text/javascript">
            function addPageLoadToDataLayer(){
                const loadTime = window.performance.timing.domContentLoadedEventEnd - window.performance.timing.navigationStart;
                const localStorageData = JSON.parse(localStorage.getItem("mage-cache-storage"));
                let user = undefined;

                if (localStorageData && Object.entries(localStorageData).length && localStorageData.customer) {
                    if (localStorageData.customer.dish_customer_id) {
                        user = {
                            authState: "loggedIn",
                            subscriberId: localStorageData.customer.dish_customer_id
                        }
                    }
                }

                const screenLoadObj = {
                    event: "screen_load",
                    web: {
                        webPageDetails: {
                            pageViews: {
                                value: 1,
                            },
                            name              : "BMWeb|home",
                            language          : "en",
                            siteSection       : "home",
                            qsp               : "irclickid=xfMU4GW\u002DdxyIW\u002D4xqmX7jS8OUkFXQ\u002D2FxzbfRI0\x26irgwc=1\x26im_rewards=1\x26utm_source=impact\x26utm_medium=Honey%20Science%20Corporation.\x26utm_campaign=Online%20Tracking%20Link\x26utm_term=",
                            siteSubSection    : "home",
                            siteSubSubSection : "home",
                            platform          : "AEM:PROD:boost",
                            loadTime          : loadTime,
                            domain            : window.location.hostname,
                            url               : window.location.href.split(/[?#]/)[0],
                        }
                    }
                };

                if (user) {
                    screenLoadObj.web.user = user;
                }

                window.adobeDataLayer = window.adobeDataLayer || [];
                window.adobeDataLayer.push(screenLoadObj);
            }
            function addClickToDataLayer(){
                window.adobeDataLayer = window.adobeDataLayer || [];
                $("a, button").click(function(){
                    window.adobeDataLayer.push({
                        event: 'ui_click',
                        web: {
                            webInteraction: {
                                linkClicks: {
                                    value: 1
                                },
                                clickLocation: "\/content\/boost\/us\/en",
                                name: $(this).text().trim() ? $(this).text().trim() : $(this).find("img").attr("alt") ? $(this).find("img").attr("alt") : $(this).attr("alt") ? $(this).attr("alt") : $(this).attr("aria-label") ? $(this).attr("aria-label") : $(this).attr("title") ? $(this).attr("title") : '',
                                URL: $(this).attr("href"),
                                pageName: "BMWeb|home",
                                type: 'other'
                            }
                        }
                    });
                });
            }
            window.onload = function (){
                addPageLoadToDataLayer();
                addClickToDataLayer();
            }
        </script>
    


