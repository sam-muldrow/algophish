                <script type="text/javascript">

    window.didomiOnReady = window.didomiOnReady || [];
    window.didomiOnReady.push(function (Didomi) {

        Didomi.on('consent.changed', function () {
            let htmlClassNames = document.getElementsByTagName("html")[0].className;

            if (htmlClassNames.match(new RegExp("member-not-connected")) || !htmlClassNames.match(new RegExp("premium"))) {
                checkConsentCall();
            }
        });

        $('#didomi-disagree-and-login').on('click', function () {
            Didomi.setUserDisagreeToAll();
        });

        Didomi.on('notice.clickdisagree', () => {
            location.href = 'https://abonnement.latribune.fr/offre/6117-fc26158fbe05fff94591a083f2e8d99b/mon-compte/creation';
        });

        Didomi.on('preferences.shown', function () {
            hideConsentWall();
        });

        Didomi.on('preferences.hidden', function () {
            let htmlClassNames = document.getElementsByTagName("html")[0].className;

            if (htmlClassNames.match(new RegExp("member-not-connected")) || !htmlClassNames.match(new RegExp("premium"))) {
                checkConsentCall();
            }
        });
    });
</script>




