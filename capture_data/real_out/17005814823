        <script type="text/javascript">

        function handleSubscribedUser() {
            $('#subscribe-offer').hide();
            $('.subscriber-logged-in').show();
            $('.tag-pub').remove();
        }

        window.didomiOnReady = window.didomiOnReady || [];


            if (document.cookie.match(/PHPSESSID(PREPROD)?=/)) {
                let url = "/mon-compte/info";
                pingSlgtUrl = "/selligent/customer-activity";
                var loginInfoPromise = new Promise(function (userInfoResolve, myReject) {
                    $.get(url, function (result) {
                        if (result.hasSubscription === true || ((result.connected === false || result.hasSubscription === false))) {
                            userInfoResolve(result)
                        } else {
                            myReject('test Error')
                        }
                    })
                })

                $(window).on('load', function() {
                    loginInfoPromise.then((data) => {
                        if (data.hasSubscription === true) {
                            handleSubscribedUser();
                        } else if (data.connected === false || data.hasSubscription === false) {
                            if (typeof Didomi === "undefined") {
                                checkConsentCall();
                            } else {
                                let beforeCollect = Didomi.shouldConsentBeCollected();
                                if (!beforeCollect) {
                                    checkConsentCall();
                                }
                            }
                        }
                    })
                });

                $.get(pingSlgtUrl, function (result) {
                    console.log(result)
                });
            } else {
                window.didomiOnReady.push(function (Didomi) {
                    const beforeCollect = Didomi.shouldConsentBeCollected();
                    if (!beforeCollect) {
                        checkConsentCall();
                    }
                });
            }
    </script>
