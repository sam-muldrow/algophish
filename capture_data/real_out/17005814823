    <script type="text/javascript">
    const types = {'navbar-nav': 'Menu', 'container': 'centre', 'layout-footer': 'footer'};

    document.onreadystatechange = function () {
        const links = document.querySelectorAll('a, span.thumbBlock, video');
        for (let link of links) {
            link.addEventListener('click', event => {
                const className = event.target.closest('body > *').classList[0];

                let href = event.target.href;
                if (event.target.nodeName === 'I') {
                    href = event.target.parentElement.href;
                }

                if (event.target.nodeName === 'SPAN') {
                    href = event.target.parentElement.parentElement.href;
                }

                if (event.target.nodeName === 'VIDEO') {
                    href = event.target.parentElement.parentElement.parentElement.parentElement.href;
                }

                if (isAboLink(href)) {
                    window.dataLayer = window.dataLayer || [];
                    window.dataLayer.push({
                        'event': 'subscribe_click',
                        'link_type': types[className],
                        'link': href,
                        'counter': 1
                    });

                    return;
                }

                if (isExternalLink(href)) {
                    window.dataLayer = window.dataLayer || [];
                    window.dataLayer.push({
                        'event': 'link_click',
                        'link_type': types[className],
                        'link': href,
                        'counter': 1
                    });
                }
            });
        }
    };

    function isExternalLink(href) {
        if (isValidUrl(href)) {
            const baseUrl = new URL(href).origin;
            const sites = JSON.parse('[{"url":"www.latribune.fr"},{"url":"afrique.latribune.fr"},{"url":"toulouse.latribune.fr"},{"url":"objectifnews.latribune.fr"},{"url":"objectifaquitaine.latribune.fr"},{"url":"cms.latribune.fr"},{"url":"objectif-languedoc-roussillon.latribune.fr"},{"url":"region-sud.latribune.fr"},{"url":"region-aura.latribune.fr"},{"url":"https:\/\/abonnement.latribune.fr"}]');

            let isExternal = true;
            sites.forEach(site => {
                if (baseUrl === site.url) {
                    isExternal = false;
                }
            });

            return isExternal;
        }

        return true;
    }

    function isAboLink(href) {
        if (isValidUrl(href)) {
            const baseUrl = new URL(href).origin;
            if (baseUrl === 'http://abonnement.latribune.fr' || baseUrl === 'https://abonnement.latribune.fr') {
                return true;
            }

            return false;
        }
    }

    function isValidUrl(string) {
        try {
            new URL(string);
            return true;
        } catch (err) {
            return false;
        }
    }
</script>


    <script type="text/javascript">
        var tag = new ATInternet.Tracker.Tag();

        window.didomiOnReady = window.didomiOnReady || [];
        window.didomiOnReady.push(function (Didomi) {
            var defaultstatus = Didomi.getUserConsentStatusForVendor("c:atinterne-cWQKHeJZ");
            if (defaultstatus === true) {
                tag.privacy.setVisitorOptin();
            } else {
                if (tag.privacy.getVisitorMode() == null || tag.privacy.getVisitorMode().name !== "optout") {
                    tag.privacy.setVisitorMode("cnil", "exempt");
                }
            }

            var bp = {s:600,d:1000} ;
            if (typeof window.innerWidth != 'undefined') {

                w = window.innerWidth;
                if (w < bp.s) {wojd = "smartphone";}

                else if (w < bp.d) { wojd = "tablet";}
                else if (w >= bp.d) { wojd = "desktop";}
                else { wojd = "error";}
            }

            tag.page.set({
                level2: '11',
                customObject: {
                    device: wojd
                },    name: 'Accueil::home::Accueil'
});

            
                        tag.customVars.set({
                site: {
                    6: '4',
                    4: encodeURIComponent(window.location.href)
                }
            });
                        tag.dispatch();

            var button = document.getElementById("optout-atinternet");
            if (button != null) {
                button.addEventListener("click", function () {
                    var transaction = Didomi.openTransaction();
                    transaction.disablePurpose('audiencem-xedeU2gQ');
                    transaction.disableVendor('c:atinterne-cWQKHeJZ');
                    transaction.commit();
                    tag.privacy.setVisitorOptout();
                    tag.dispatch();
                });
            }
        });


    </script>

