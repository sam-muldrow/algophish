<script type="text/javascript">
    if (document.cookie.match(/PHPSESSID(PREPROD)?=/)) {
        const url = "/mon-compte/info";
        fetch(url)
            .then((response) => response.json())
            .then((data) => {
                const email = data.vars ? data.vars.email : '';
                const status = data.hasSubscription ? 'Abonné' : 'Non abonné';

                const category = "Homepage";

                const parentCategory = "La Tribune";
                let section = parentCategory;
                if (section === "La Tribune" || !section) {
                    section = category;
                }

                const topicNews = "";
                const url = "/";

                hash(email).then((hex) => {
                    window.dataLayer = window.dataLayer || [];
                    window.dataLayer.push({
                        'user_id': hex,
                        'page_category': category,
                        'statut_adherent': status,
                        'website_section': section,
                        'theme_actu': topicNews,
                        'page_url': url,
                    });
                });
            });
    }
</script>

