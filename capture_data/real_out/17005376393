      if (typeof c.s === 'function') {
        if (c.id) t.id = c.id;
        t.textContent = '(' + c.s + ').apply(window);';
      } else {
        t.async = false;
        t.defer = true;
        t.crossOrigin = 'anonymous';
        if (c.props){
          for (var key in c.props){
            t[key] = c.props[key];
          }
