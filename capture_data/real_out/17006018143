				<div class="l-container">
	<div class="cmp-bnr001">
		<div class="wrp" id="bannerDiv"></div>
	</div>
<script>
document.addEventListener("DOMContentLoaded", function() {
var banners = [
	    {link: 'https://www.chunichi.co.jp/wadai/culture/ghibli_topic?ref=menu_link', url: 'https://static.chunichi.co.jp/chunichi/images/aside/ghibli_topic_bn.jpg', ratio: 0.33, start: '2023-11-01', end: '2024-12-31'},
	    {link: 'https://www.chunichi.co.jp/wadai/culture/fujii_eight_crowns?ref=menu_link', url: 'https://static.chunichi.co.jp/chunichi/images/aside/fujii_souta8crowns2.png', ratio: 0.33, start: '2023-08-01', end: '2024-012-31'},
	    {link: 'https://www.chunichi.co.jp/feature_pages/chigirie?ref=menu_link', url: 'https://static.chunichi.co.jp/chunichi/images/aside/bnr-chigirie.jpg', ratio: 0.33, start: '2023-08-01', end: '2024-012-31'}
	];


	var today = new Date().toISOString().slice(0, 10); // get today's date in YYYY-MM-DD format
	var availableBanners = banners.filter(banner => banner.start <= today && today <= banner.end);

	if (availableBanners.length === 0) {
	    console.log("バナーがありません");
	    return
	}

	var totalRatio = availableBanners.reduce((total, banner) => total + banner.ratio, 0);
	var randomValue = Math.random() * totalRatio;
	var cumulativeRatio = 0;
	var selectedBanner = availableBanners.find(banner => {
	    cumulativeRatio += banner.ratio;
	    return randomValue <= cumulativeRatio;
	});

	document.getElementById('bannerDiv').innerHTML = `<a href="${selectedBanner.link}" class="bnr"><img src="${selectedBanner.url}" /></a>`;
});
</script>
</div>
	

	
		






