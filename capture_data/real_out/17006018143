    var hbm = {};
    bsttag.cmd.push(function(cmd) {
      hbm = cmd.requestBids({adUnits: slots});
    });

    setTimeout(function () {
      if (hbm.isRequestSent) {
        return;
      }
      hbm.isRequestSent = true;
      googletag.cmd.push(function() {
        googletag.pubads().refresh(slots);
      });
    }, bsttag.failSafeTimeout);
  };
</script>
