    <div class="game-poster">
        <div class="game-caption hidden-xs">
            <div class="game-caption-wrapper">
                <div class="game-play-btn">
                    <a onclick="${onclick}" class="btn btn-red">{{button_text}}</a>
                </div>
            </div>
        </div>
                <a onclick="${onclick}">
            <img class="lazy_image lazyload" data-original="{{image_url:250}}"
                 src="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAAAAACH5BAEAAAAALAAAAAABAAEAAAICRAEAOw==" width="189" height="142" alt="{{application_name}}" style="display: inline;">
        </a>
    </div>

    <h5>
        <a onclick="${onclick}">{{application_name}}</a>
    </h5>
</div>
`;
        };


        var timeout = setTimeout(function () {
            // odometers
            window.odometerOptions = {duration: 10000};
        }, 1000);

        var callback = function () {
            var timeout = setTimeout(function () {
                jQuery(games_m.selector + " .lazy_image").lazyload();
                jQuery(games_d.selector + " .lazy_image").lazyload();
            }, 1000);

            jQuery(".game-item").hover(function () {
                    jQuery(this).find('.game-caption').removeClass("animated fadeOut").addClass("animated fadeIn");
                    ;
                },
                function () {
                    jQuery(this).find('.game-caption').removeClass("animated fadeIn").addClass("animated fadeOut");
                }
            );
        };


        games_d.load(params_m, callback);
        games_m.load(params_d, callback);


    });
</script>


<script>
    function show_games(filter, is_search = false) {
        var selector = '#portfoliolist .row .mix';
        if (filter === 'all' || filter === '') {
            $(selector).show();
            return;
        }

        $(selector).hide();

        if (is_search) {
            $(selector).each(function (i, el) {
                if (el.classList.value.indexOf(filter.toLowerCase()) > -1)
                    $(el).show();
            });
        } else {
            filter.split(',').forEach(function (filter) {
                $(selector + '.' + filter).show();
            });
        }
    }
</script>

