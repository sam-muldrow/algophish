        <script src="https://cdn2.playmillion.com/wp-content/themes/playmillion/js/carousel/jquery.jcarousellite.js"></script>
    <script type="text/javascript">
        jQuery(document).ready(function () {
            
            //amount: 170.34
                //currency: "CAD"
                //date: "2021-08-19 00:59:51"
                //game: "Rich Wilde and the Amulet of Dead Mobile"
                //symbol: "C$"
                //username: "Playoujo"

            const template = `
<li>
        <div class="winner">
            <div class="game-image">
                <img alt="{game}" src="https://cdn6.playmillion.com/wp-content/themes/playmillion/images/game-poster.jpg">
            </div>

            <div class="winner-name">
                <h4>{username}</h4>
            <h5>{game}</h5>
            </div>

            <div class="won-amount">
                {symbol}{amount}
            </div>
        </div>
    </li>`
            son.getWinners(winners => {
                let acc = ""
                winners.forEach(winner => {
                    let winner_html = template
                    for (const key in winner) {
                        winner_html = winner_html.replaceAll('{'+key+'}',winner[key])
                    }
                    acc += winner_html
                })
                jQuery("#winnerslist .slides").html(acc)
                jQuery("#winnerslist li:odd").addClass("odd");
                jQuery("#winnerslist").jCarouselLite({
                    vertical: true,
                    circular: true,
                    auto: 5500,
                    visible: 3,
                    scroll: 1,
                    speed: 600
                });
            })
        });
    </script>


