<script>
    $(document).ready(function () {

        const fun_buttons = {
            'SV': 'Testa Gratis',
            'FI': 'KOKEILE ILMAISEKSI',
            'NO': 'PRØV GRATIS',
            'DE': 'GRATIS TESTEN',
        };

        let fun_button_text = 'Try for free';
        if(SON_API_CONFIG.LANG_CODE in fun_buttons)
            fun_button_text = fun_buttons[SON_API_CONFIG.LANG_CODE];

        var selector_m = '#portfoliolist .row';
        var selector_d = '.home-games-list .games-list-row';


        var games_m = new CasinoGames(selector_m);
        var games_d = new CasinoGames(selector_d);
        games_m.translations = {button_text: 'Play Now'};
        games_d.translations = {button_text: 'Play Now',
                                fun_button_text: fun_button_text,
        };

        var params_d = {
            appName: SON_API_CONFIG.skin.appName,
            //language: SON_API_CONFIG['LANG_CODE'],
            country: visitor_country_code(),
            platform: 'desktop',
            limit: 12
        };
        var params_m = {
            appName: SON_API_CONFIG.skin.appName,
            //language: SON_API_CONFIG['LANG_CODE'],
            country: visitor_country_code(),
            platform: 'mobile',
            limit: 12
        };

        fun_mode = '{{fun_mode}}';

        games_d.get_funmode_template = function () {
            return `<div class="game-play-btn" style="padding-top: 5px">
                        <a onclick="return funMode.show({{internal_game_id}});" class="btn btn-lg btn-red">{{fun_button_text}}</a>
                    </div>`;
        }


        games_d.get_game_template = function (index) {
            return `
            <div class="col-xs-12 col-sm-4 col-md-4 custom-col">
                <div class="game-item">
                <div class="game-poster">
                    <div class="post-1000930 casinogames type-casinogames status-publish has-post-thumbnail hentry">
                        <div class="game-image">
                            <div class="game-caption hidden-xs">
                                <div class="game-caption-wrapper">
                                    <div class="game-play-btn">
                                        <a onclick="return openAuth()" class="btn btn-lg btn-red">{{button_text}}</a>
                                    </div>
                                    ${fun_mode}
                                </div>
                            </div>
                            <img class="lazy_image" data-original="{{image_url:250}}"
                                 src="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAAAAACH5BAEAAAAALAAAAAABAAEAAAICRAEAOw=="
                                 width="755" height="567" alt="{{application_name}}" style="display: inline;">
                        </div>
                    </div>
                </div>
            </div>
        </div>`;
        };

        games_m.get_game_template = function (index) {
            let onclick = "return openAuth();";
            if(SON_CONFIG.device === 'mobile' && fun_mode)
                onclick = "return extendedGameView.show({{internal_game_id}})";

            return `
