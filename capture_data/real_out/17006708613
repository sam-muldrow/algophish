    <script>
        $(function () {
            $(".tab_content2").hide();
            $(".tab_content2:first").show();

            $("ul.s_tabs2 li").click(function () {
                $("ul.s_tabs2 li").removeClass("active").css("color", "#333");
                //$(this).addClass("active").css({"color": "darkred","font-weight": "bolder"});
                $(this).addClass("active").css("color", "#ffcb05");
                $(".tab_content2").hide()
                var activeTab = $(this).attr("rel");
                $("#" + activeTab).fadeIn()
            });

            $(".tab_content1").hide();
            $(".tab_content1:first").show();

            $("ul.s_tabs1 li").click(function () {
                $("ul.s_tabs1 li").removeClass("active").css("color", "#333");
                //$(this).addClass("active").css({"color": "darkred","font-weight": "bolder"});
                $(this).addClass("active").css("color", "#ffcb05");
                $(".tab_content1").hide()
                var activeTab = $(this).attr("rel");
                $("#" + activeTab).fadeIn()
            });
        });
    </script>

