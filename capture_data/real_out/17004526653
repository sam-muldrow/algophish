            <section class="c-list--horizontal" aria-label="Opinion">
                
                    <h2 class="c-list__title"><a href="https://www.ctvnews.ca/opinion" class="c-list__title__link">Opinion</a> </h2>
                
                
                

                <ul class="c-list c-list--4Columns">
                    <li class="c-list__item">
    
    
    <article class="c-mosaic-item">
        <div class="c-list__item__block">


            

            

