	<script>
	(function(){
		var stackArgsForMboxDefine = [];
		var stackArgsForMboxUpdate = [];
		
		
		window.mboxCreateDelay = function(mboxName){
			mboxDefineDelay( mboxName, mboxName );
			
			var el = window.document.getElementById(mboxName);
			
			var args = [];
			for( var i = 0; i < arguments.length; i++ ){
				args.push( arguments[i] );
			}
			mboxUpdateDelay.apply( null, args );
		};
		
		
		window.mboxDefineDelay = function(id, mboxName){
			var args = [ id, mboxName ];
			
			stackArgsForMboxDefine.push( args );
		};
		
		
		window.mboxUpdateDelay = function(mboxName){
			var args = [];
			for( var i = 0; i < arguments.length; i++ ){
				args.push( arguments[i] );
			}
			
			stackArgsForMboxUpdate.push( args );
		};
		
		
		window.mboxExcuteDelay = function(){
			if( window.mboxDefine !== mboxDefineDelay ){
				for( var i = 0; i < stackArgsForMboxDefine.length; i++ ){
					window.mboxDefine.apply( null, stackArgsForMboxDefine[i] );
				}
			}
			
			if( window.mboxUpdate !== mboxUpdateDelay ){
				for( var i = 0; i < stackArgsForMboxUpdate.length; i++ ){
					window.mboxUpdate.apply( null, stackArgsForMboxUpdate[i] );
				}
			}
		};
		
		
		window.mboxCreate         = window.mboxCreate         || mboxCreateDelay;
		window.mboxDefine         = window.mboxDefine         || mboxDefineDelay;
		window.mboxUpdate         = window.mboxUpdate         || mboxUpdateDelay;
	})();
	</script>

