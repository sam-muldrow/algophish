    $(document).ready(function () {
        console.log("Widget: MainMenuMvcMVC");

        $('.drawer-side-menu a').on('click', function () {
            dataLayer.push({
                'event': 'customEvent', //Developers Note: Currently we see �customEvent� is being used for dataLayer.push events to trigger a GA event tag in TM. This can continue to be used, with additional attributes included mentioned in the following sections.
