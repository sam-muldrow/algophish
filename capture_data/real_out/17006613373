                'topMenuCategory': $(this).text()
            })
        });

        //Code moved from common js
        handleFlexExpireMessageHeader.init();


    });




    String.prototype.includes = function (str) {
        var returnValue = false;

        if (this.indexOf(str) !== -1) {
            returnValue = true;
        }

        return returnValue;
    }
    $(function () {
         console.log(false);
        if ($("#hdnLoginStatusHeader").val() === "true") {
            $("body").addClass("login");
            $(".header .float-menu").addClass("login");
            $(".header").addClass("login");
            //$(".logged-name a").html($(".logged-name a").html() + utilsHeader.getCookie("UserName"))
            if (utilsHeader.isMobile() && $(window).width() < 666) {

                $(".logout-btn").show();
            } else {
                $(".logout-btn").hide();
            }
            //$("#nav-menu").hide();
            //$("#nav-menu-login").show("slow");

            var subPlan = utilsHeader.getCookie("Subscription");

            if(subPlan != null)
            {
                if(subPlan == "1")
                {
                    handlePromoImageHeader.showLoggedInBasic();
                    //$(".registration-type").addClass('hide');
                    $(".flex-text").removeClass('hide');
                }
                else if (subPlan == "2") {
                    handlePromoImageHeader.showLoggedInFlex();
                    //$(".registration-type").removeClass('hide');
                    $(".flex-text").addClass('hide');
                }
                else {

                    //$(".registration-type").addClass('hide');
                    $(".flex-text").removeClass('hide');
                }
            }
            else {

                //$(".registration-type").addClass('hide');
                $(".flex-text").removeClass('hide');
            }


        } else {
            $(".header .float-menu").removeClass("login");
            $(".header").removeClass("login");
            $(".logout-btn").hide();
            if (document.location.pathname == '/' && utilsHeader.getCookie("trackedShimpent") != "" && utilsHeader.getCookie("trackd") == "") {
                utilsHeader.setCookie('trackd', "1", 1);
                handlePopupDialog.init($('#trackShippmentdialog'))
            }
            $("#nav-menu-login").hide();
            $("#nav-menu").show("slow");
            if (!window.location.href.toLowerCase().includes('/action/edit/'.toLowerCase())) {
                handlePromoImageHeader.showNotLoggedIn();
            }
            //$(".registration-type").addClass('hide');

        }

    })




    // Common js code moved here.

    utilsHeader = {
        isMobile: function () {
            return /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent);
        },
        setCookie: function (cname, cvalue, exdays) {
            var d = new Date();
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            var expires = "expires=" + d.toGMTString();
            var path = "path=/";
            document.cookie = cname + "=" + cvalue + "; " + expires + "; " + path;
        },

        getCookie: function (cname) {
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i].trim();
                if (c.indexOf(name) == 0)
                    return c.substring(name.length, c.length);
            }
            return "";
        }

    }

    handleFlexExpireMessageHeader = {
        init: function () {
            try {
                handleFlexExpireMessageHeader.setFlexLogoMessage();
            } catch (e) {

            }
        },
        setFlexLogoMessage: function () {
            $(".help-type").tooltip({
                'delay': {
                    show: 200,
                    trigger: "focus or hover only",
                    hide: 3000
                },
                position: {
                    my: "center bottom",
                    at: "right-10 top-10"
                }
            });

            var flectExpDate = utilsHeader.getCookie("FlexExpireDate");
            console.log("FlexExpireDate: " + flectExpDate);
            if (flectExpDate != "") {

                //$('.registration-type').attr('title', 'S&S Flex Membership until ' + flectExpDate);

                //var title = $('.registration-type').attr('title');
                //title = title + flectExpDate;
                //$('.registration-type').attr('title', title);
            }
        }
    }

    handlePromoImageHeader = {
        init: function () {
            try {

            } catch (e) {

            }
        },
        showNotLoggedIn: function () {
            $(".promo-image-not-login").attr('style', 'display:inline;');
            $(".promo-image-login").attr('style', 'display:none;');
            $(".promo-image-login-flex").attr('style', 'display:none;');
        },
        showLoggedInBasic: function () {
            $(".promo-image-not-login").attr('style', 'display:none;');
            $(".promo-image-login").attr('style', 'display:inline;');
            $(".promo-image-login-flex").attr('style', 'display:none;');
        },
        showLoggedInFlex: function () {
            $(".promo-image-not-login").attr('style', 'display:none;');
            $(".promo-image-login").attr('style', 'display:none;');
            $(".promo-image-login-flex").attr('style', 'display:inline;');
        }
    }


</script>













