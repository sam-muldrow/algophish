    <script type="text/javascript">
        function pushToDataLayer(eventCategory, eventAction, eventLabel) {

            var data = {
                event: "customEvent",
                eventCategory: eventCategory,
                eventAction: eventAction,
                eventLabel: eventLabel,
            };
            dataLayer.push(data)
        }
        $('.drawer-side-menu a').on('click', function () {
            dataLayer.push({
                'event': 'customEvent', //Developers Note: Currently we see “customEvent” is being used for dataLayer.push events to trigger a GA event tag in TM. This can continue to be used, with additional attributes included mentioned in the following sections.
                'eventCategory': 'User Engagement', //Developers Note:  Insert the Event Category for the click/user interaction. Please keep uniform per these guidelines here.
                'eventAction': 'Menu Click - Header',
                'eventLabel': $(this).text(), //Developers Note:  example options can be Home, Our Story, S&S Perfume etc.
                'topMenuCategory': $(this).text()
            })
        })

        $('.footer .panel-collapse li a').on('click', function () {
            var isMobileSection = $(this).parents().is("#collapse4")
            if (isMobileSection) {
                dataLayer.push({
                    'event': 'customEvent',//Developers Note: Currently we see “customEvent” is being used for dataLayer.push events to trigger a GA event tag in GTM. This can continue to be used, with additional attributes included mentioned in the following sections.
                    'eventCategory': 'Ecommerce', //Developers Note:  Insert the Event Category for the click/user interaction. Please keep uniform per these guidelines here.
                    'eventAction': 'App Download',
                    'eventLabel': $(this).text(), //Developers Note:  example options can be Home, Our Story, S&S Perfume etc.
                    'topMenuCategory': "No"
                });
            }
            else {
                dataLayer.push({
                    'event': 'customEvent',//Developers Note: Currently we see “customEvent” is being used for dataLayer.push events to trigger a GA event tag in GTM. This can continue to be used, with additional attributes included mentioned in the following sections.
                    'eventCategory': 'User Engagement', //Developers Note:  Insert the Event Category for the click/user interaction. Please keep uniform per these guidelines here.
                    'eventAction': 'Menu Click - Footer',
                    'eventLabel': $(this).text(), //Developers Note:  example options can be Home, Our Story, S&S Perfume etc.
                    'topMenuCategory': "No"
                });
            }

        })
        function pushToDataLayer(eventCategory, eventAction, eventLabel) {

            var data = {
                event: "customEvent",
                eventCategory: eventCategory,
                eventAction: eventAction,
                eventLabel: eventLabel,
            };
            dataLayer.push(data)
        }
        function pushToDataLayerGA4(clickText, country) {
            //deftemplate
            var countryName = country;
            if (countryName == undefined) {
                countryName = null;
            }
            var data = {
                event: "l1_header",
                click_text: clickText,
                country: countryName
            };
            dataLayer.push(data)
        }
        function pushToDataLayerTrackingGA4(event, searchStatus, searchKeyword) {
            debugger
            var data = {
                event: event,
                search_status: searchStatus,
                search_keyword: searchKeyword
            };
            dataLayer.push(data)
        }
        function pushToDataLayerShippingFees(fieldsFrom, fieldsTo, weight, unit, basicPrice, flexPrice, currency) {
            debugger
            var data = {
                event: "form_submit",
                widget_name: "Rate Calculator Widget",
                fields_from: fieldsFrom,
                fields_to: fieldsTo,
                rate_weight: weight,
                rate_weight_unit: unit,
                rate_weight_flex: flexPrice,
                rate_weight_basic: basicPrice,
                rate_weight_currency: currency
            };
            dataLayer.push(data)
        }
        function pushToDataLayerServiceUpdates(subject, regGUID, loggedIn, country, lang) {
            debugger
            var data = {
                event: "news_service_update",
                click_text: subject,
                pageType: "ServiceUpdates",
                pageName: "Service Updates page",
                content_name: "Service Updates",
                topMenuCategory: "No",
                userRegGUID: regGUID,
                userId: regGUID,
                loggedInStatus: loggedIn,
                userCountry: country,
                userStatus: "2",
                language: lang
            };
            dataLayer.push(data)
        }
        function pushToDataLayerSaveButton(eventCategory, eventAction, eventLabel) {
            try {
                var date = new Date();
                var age = date.getFullYear - Number($("#selectedYear").val());
                var addressId = $(".delivery-primary:checked").parents('tr').attr('addressId')
                var city = $("[addressid='" + addressId + "']").not(".credit-row").find("td:last-child").html()
                dataLayer.push({
                    'event': 'customEvent',
                    'eventCategory': eventCategory, //Developers Note:  Insert the Event Category for the click/user interaction. Please keep uniform per these guidelines here.
                    'eventAction': eventAction,
                    'eventLabel': eventLabel,
                    'userNationality': $("#selectedNationality").val(), //Developers Note: this attribute holds Nationality  of user selected in the profile page.
                    'userCity': city, //Developers Note: this attribute holds the city user selected in the profile page.
                    'userGender': $('#ddlGender').val(), //Developers Note: this attribute holds the gender user selected in the profile page.
                    'userAge': age,
                    'userRegGUID': $("#accPersonalInfo").attr('user-guid') //Developers Note: this attribute holds RegGUID of the user from the back end system.
                })
            }
            catch (ex) {

            }
        }

    </script>

