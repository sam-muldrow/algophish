    function setSelectedBranch(a) {
        var b = new Date,
            c = new Date;
        c.setTime(b.getTime() + 7776e6), document.cookie = "selectedBranch=" + escape(a) + ";expires=" + c.toGMTString() + "; path=/; domain=" + window.location.hostname
    }

    function getCookie(a) {
        var b, c, d, e = document.cookie.split(";");
        for (b = 0; b < e.length; b++)
            if (c = e[b].substr(0, e[b].indexOf("=")), d = e[b].substr(e[b].indexOf("=") + 1), c = c.replace(/^\s+|\s+$/g, ""), c == a) return unescape(d)
    }


    function redirect(link) {
        window.location.replace(link);
    }

    function setCountryByLanguage(browserLang, countryLanguages){
        if(countryLanguages.length <= 1){
            return countryLanguages[0];
        }
        countryLanguages.forEach(function (languageObj){
            if(languageObj.language === browserLang){
                return languageObj;
            }
        });
        //if browser language doesn't match any branch available language, select first
        return  countryLanguages[0];

    }

    function setupCountryButton(geoipData) {
        var countries = JSON.parse(document.querySelector('.countries-data').innerHTML);
        var data;
        var countryCode = (geoipData.countryCode !== undefined) ? geoipData.countryCode.toLowerCase() : 'int';
        if (countries[countryCode] === undefined) {
            data = {name: 'International (English)', url: '/int/', setCookie: true};
            $('.country-int').remove();
        } else {
            var browserLang = window.navigator.language.substr(0, 2);
            data = setCountryByLanguage(browserLang, countries[countryCode]);

        }
        var redirectCountries = ['us', 'ca', 'ar', 'bo', 'cl', 'co', 'do', 'mx', 'pe', 'ni', 'uy', 'gt', 'sv', 'cr', 'hn', 'py', 've', 'pa', 'ec', 'la', 'br', 'il'];
        if (redirectCountries.indexOf(countryCode) !== -1) {
            redirect(countryCode);
        }

        var $countryButton = $('.geoip-country-btn');
        $countryButton.attr('href', data['url']);
        $countryButton.html(data['name']);

        if (data['setCookie']) {
            $countryButton.on('click', function () {
                setSelectedBranch(data['url']);
            });
        }
    }

    var cookieValue = getCookie("selectedBranch");
    if (cookieValue) {
        redirect(cookieValue);
    }
    var baseurl = 'https://api.eset.com/v1/geoip';
    $.ajax({
        type: 'GET', // type
        url: baseurl, // send to this url
        success: function (data) {
            setupCountryButton(data);
        },
        error: function () {
            console.log('Fetching country failed');
        }
    });

