<source data-srcset="https://cdn.prod.www.manager-magazin.de/images/42fe22d2-0157-4921-83cd-bf31495a0002_w120_r1.33_fpx49_fpy50.webp 120w, https://cdn.prod.www.manager-magazin.de/images/42fe22d2-0157-4921-83cd-bf31495a0002_w240_r1.33_fpx49_fpy50.webp 240w" data-sizes="(max-width: 119px) 100vw, (min-width: 120px) and (max-width: 719px) 120px, (min-width: 720px) and (max-width: 919px) 100vw, (min-width: 920px) and (max-width: 1019px) 920px, (min-width: 1020px) 1008px" type="image/webp">
