                        <script type="text/javascript">
                            collections.subform = function(){
                                window.subform = function() {
                                    if($("#send_keyword").val().replace(/\s+/g,'')==''){
                                        $('.evc-errtip').removeClass('success').text('Please enter you keyword !').show();
                                        return false;
                                    }
                                    if($("#send_email").val().replace(/\s+/g,'')==''){
                                        $('.evc-errtip').removeClass('success').text('Please enter you email !').show();
                                        return false;
                                    }
                                    if($("#send_email").val().search(/^\w+((-\w+)|(\.\w+))*\@[A-Za-z0-9]+((\.|-)[A-Za-z0-9]+)*\.[A-Za-z0-9]+$/) == -1){
                                        $('.evc-errtip').removeClass('success').text('Please enter the correct email').show();
                                        return false;
                                    }

                                    $.ajax({
                                        url:"https://ajax.everychina.com/index.php?r=search/email",
                                        type:"post",
                                        async:true,
                                        data:{'kw':$("#send_keyword").val(),'email':$("#send_email").val()},
                                        dataType: 'json',
                                        cache: 'false',
                                        xhrFields:{withCredentials:true}, 
                                        crossDomain:true,
                                        success:function(data){
                                            $('.evc-errtip').text('Thank you for your subscribe!').addClass('success').show();
                                            $('#send_keyword').val('');
                                            $('#send_email').val('');
                                        }
                                    });
                                    return false;
                                }

                                $(".evc-form_submit").click(function(){
                                    window.subform();
                                })
                            }
                        </script>
                        <div class="evc-form">
                            <div class="evc-form_text">
                                <p>Don't miss out on FREE new product updates!</p>
                                <div class="evc-inputs">
                                    <input type="text" id="send_keyword" placeholder="Enter product keyword">
                                    <input type="text" id="send_email" placeholder="Enter your email">
                                </div>
                                <div class="evc-formbox">
                                    <span class="evc-form_submit">Subscribe</span><span class="evc-errtip"></span>
                                </div>
                            </div>
