        <!-- 左右切换 -->
        <script type="text/javascript">
            collections.lrsilder = function(){
                $.fn.silder = function() {
                    var len = $('.item', this).length;
                    var dex = Math.ceil(len / 1);
                    var i = 0;
                    var w = $('.item', this).eq(0).width();
                    var u = $('.itemwrap', this).width(len * w);
                    var self = this;
                    //绑定
                    $('.itemnext', this).on({
                        "click": function() {
                            i = i >= dex - 1 ? dex - 1 : i + 1;
                            console.log(i)
                            animateMove.call(self, i);
                        }
                    });
                    $('.itemprev', this).on({
                        "click": function() {
                            i = i <= 0 ? 0 : i - 1;
                            console.log(i)
                            animateMove.call(self, i);
                        }
                    });

                    //动画
                    function animateMove(n) {
                        console.log(this)
                        $('.itemwrap', this).animate({
                            'margin-left': -1 * n * $('.itemoverflow', this).width() + 'px'
                        }, 300);
                        //触发懒加载
                        $(document).trigger('scroll');
                    }

                }
                $(function() {
                    $('#newProduct').silder();
                    $('#newCompany').silder();
                });  
            }
        </script>
        <script type="text/javascript">
            function imgerror(obj) {  //onerror图片有错的时候显示404图片
                obj.onerror = "";
                obj.style.width = "unset";
                obj.style.height = "unset";
                obj.src = 'https://www.cdnev.com/myres/images/no_photo.jpg';
                obj.οnerrοr=null;
                obj.jqimg = '';
            }
            loadScript(null,function(){
                // jquery加载完毕之后，赶紧补一个lazyload.min.js,
                loadScript("https://www.cdnev.com/myres/js/lazy.load.js",function()
                    {
                        $(".lazy").lazyload()
                    });
                loadScript("https://www.cdnev.com/myres/js/marquee.js",null,true);
            })
        </script>
