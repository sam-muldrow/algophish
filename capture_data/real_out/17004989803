                <script type="text/javascript">
                    collections.homeslider = function(){
                      $.fn.homeslider = function() {
                        // # 监听离开隐藏
                        function watchPageLeaving(hidecall, showcall) {
                          document.addEventListener('visibilitychange', function() {
                            // 用户息屏、或者切到后台运行 （离开页面） 
                            if (document.visibilityState === 'hidden') {
                                hidecall && hidecall()
                                // console.log('hidden')
                            }
                            // 用户打开或回到页面 
                            if (document.visibilityState === 'visible') {
                                showcall && showcall()
                                // console.log('页面可见')
                            }
                          })
                        }

                        var len = $('.swiper-slide', this).length;
                        var dex = Math.ceil(len / 1);
                        var i = 0;
                        var w = $('.swiper-slide', this).eq(0).width();
                        var u = $('.swiper-wrapper', this).width(dex * w);
                        var timer = null;
                        // var first = false;

                        //动画
                        function animateMove(n) {
                          $('.swiper-wrapper').animate({
                              'margin-left': -1 * n * $('.swiper-slide').width() + 'px'
                          }, 300);
                        }

                        if (dex > 1) {
                          animateMove(0);
                          timer = setInterval(function() {
                              // console.log(i, dex)
                              i = i >= dex - 1 ? 0 : i + 1;
                              animateMove(i);
                            }, 3000);

                          watchPageLeaving(function(){
                              clearInterval(timer)
                          },function(){
                              timer = setInterval(function() {
                                  // console.log('call',i, dex)
                                  i = i >= dex - 1 ? 0 : i + 1;
                                  animateMove(i);
                              }, 3000);
                          })
                        }
                      }

                      $(function() {
                          $('.img_swiper').homeslider();
                      });
                    }
                </script>
