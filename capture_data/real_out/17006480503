<script>
// CAS front channel sign out
var logoutRequest = getParameterByName1("SAMLRequest");
var relayState = getParameterByName1("RelayState");

console.debug('logoutRequest1:' + logoutRequest + ",RelayState1:" + relayState);

if(logoutRequest) {
  console.info("Logout1, redirect to CAS, RelayState=" + relayState);
  window.location.href = "https://www.payeasy.com.tw/Cas/logout?_eventId=next&RelayState=" + relayState;
}

function getParameterByName1(name) {
  name = name.replace(/[\[\]]/g, '\\$&');
  var url = window.location.href;
  var regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)'),
      results = regex.exec(url);
  if (!results) return null;
  if (!results[2]) return '';
  return decodeURIComponent(results[2].replace(/\+/g, ' '));
}
</script>
