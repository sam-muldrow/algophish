<script type="text/javascript" language="JavaScript">
var sooqScrollTop = function() {
	return document.documentElement.scrollTop ? document.documentElement.scrollTop : document.body.scrollTop;
};
function flaotAdResize(adId,originWidth,orginHeight,L_or_R){//L_or_R 0:R  1:L
		if(L_or_R==0){
			$(adId).css("top",50);
		}else{
			$(adId).css("bottom",0);
		}
		if(($(window).width()-1200)/2<50){
			$(adId).css("height",50/originWidth*orginHeight);
			$(adId).css("width",50);
		}else if(($(window).width()-1200)/2<originWidth){
			$(adId).css("height",($(window).width()-1200)/2/originWidth*orginHeight);
			$(adId).css("width",($(window).width()-1200)/2);
		}else{
			$(adId).css("height",orginHeight);
			$(adId).css("width",originWidth);
		}
		$(adId).find("div").css("height",$(adId).height());
		$(adId).find("div").css("width",$(adId).width());
		$(adId).find("img").css("height",$(adId).height());
		$(adId).find("img").css("width",$(adId).width());
}
</script>
