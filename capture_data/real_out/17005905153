<script>
    $(function () {
        var $con = $("#shequ .con");
        var showHeight = ($con.height() - 84);
        var vis = Math.floor(showHeight / 50);
        var mr = (showHeight - (vis * 50)) / vis;
        $con.find(".bd ul li").css("margin-bottom", mr);
        //$con.slide({titCell:".hd ul",mainCell:".bd ul",autoPage:true,effect:"topLoop",autoPlay:true,vis:vis});
    });
    //轮播图组件
    $(document).ready(function() {
        var mySwiper = new Swiper('.swiper-container', {
            direction: 'vertical', // 垂直切换选项
            //loop: true, // 循环模式选项
            slidesPerView: 1,
            spaceBetween:10,
            //自动翻页
            autoplay: {
                delay: 1000
            }
        })
        window.mySwiper = mySwiper;

        $("#shequ .more").on("click",function () {
            mySwiper.off('resize');
            mySwiper.autoplay.stop();
            mySwiper.allowTouchMove=false;
            mySwiper.slideTo(0);
            $("#shequ .swiper-container").css('height','auto');
            $(this).hide();
        });
    });
</script>
