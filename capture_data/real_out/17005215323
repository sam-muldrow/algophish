<script type="text/javascript">
    // default value
    var SuccessOpenExistConversation = false;
    var isNewConversation = false;
    var whomSendAdvertiserId = null;
    var whomSendAdId = null
    var exeptConversationId = null;

    function ConnectScrollBarTOMessagePanel() {
        $(".message-panel.scrollbar-vertical").mCustomScrollbar({
            scrollButtons: { enable: false },
            axis: "y",
            scrollInertia: 200,
            theme: "minimal-dark",
            autoHideScrollbar: false,
            alwaysShowScrollbar: 0,
            callbacks: {
                onTotalScrollBack: function () {
                    $('#ws-chat-show-more').trigger('none');
                }
            }
        });
    }

    function ConnectScrollbarToConversations() {
        $(".scrollbar-conversations-vertical").mCustomScrollbar({
            scrollButtons: { enable: false },

            axis: "y",
            scrollInertia: 200,
            theme: "minimal-dark",
            autoHideScrollbar: false,
            alwaysShowScrollbar: 0,
            mouseWheel: { preventDefault: true },
            callbacks: {
                onTotalScroll: function () {
                    $('#ws-conversation-show-more').trigger('more');
                }
            }
        });
    }

    function ConnectScrollbarToElement() {
        $(".scrollbar-vertical").mCustomScrollbar({
            scrollButtons: { enable: false },
            axis: "y",
            scrollInertia: 200,
            theme: "minimal-dark",
            autoHideScrollbar: false,
            alwaysShowScrollbar: 0,
        });

        $(".mCSB_container").removeAttr("dir");
    }

    function CustomScrollbarOnUpdate() {
        $('.scrollbar-vertical').mCustomScrollbar("update");
        $(".scrollbar-conversations-vertical").mCustomScrollbar("update");
    }

    function ScrollMessagePanelToBottom() {
        $('.message-panel.scrollbar-vertical').mCustomScrollbar("scrollTo", "bottom");
    }

    /* ----------------- Dialog field height ------------------------ */
    function ConversationFieldHeightUpdate() {
        if ($('.chat-modal .chat-dialog-content').length) {
            var dialogHeaderHeight = $('.chat-modal .chat-dialog-header').outerHeight();
            var sellerHeaderHeight = $('.chat-modal .seller-header').outerHeight();
            var dialogInputAreaHeight = $('.chat-modal .chat-dialog-input-area').outerHeight();
            var dialogHeightPadding;
            var deviceWidth = $(window).width();

            if (dialogHeaderHeight == null) {
                dialogHeaderHeight = 0;
            }

            if (sellerHeaderHeight == null) {
                sellerHeaderHeight = 0;
            }

            if (dialogInputAreaHeight == null) {
                dialogInputAreaHeight = 0;
            }

            if (deviceWidth > 850) {
                dialogHeightPadding = dialogHeaderHeight + sellerHeaderHeight + dialogInputAreaHeight;
            } else {
                dialogHeightPadding = dialogHeaderHeight + dialogInputAreaHeight;
                $('textarea').attr("placeholder", "Mesazhi juaj");
            }

            $('.chat-modal .chat-dialog-content').css({ 'height': "calc(100% - " + dialogHeightPadding + "px)" });
            CustomScrollbarOnUpdate();
            $('.message-panel.scrollbar-vertical').mCustomScrollbar("scrollTo", "bottom");
        }
    }
    /* ---------------- /Dialog field height ------------------------ */
    function SetUserContactInfo(conversationId) {
        var url = '/WSChat/GetUserContactInfo';

        $.get(url, { conversationId: conversationId }, function (response) {
            $('#ws-chat-userinfo-area').html('');
            $('#ws-chat-userinfo-area').html(response);

            ConnectScrollbarToElement();
            CustomScrollbarOnUpdate();
        });
    }

    function SetDefaultValueHandleNewConversation() {
        isNewConversation = false;
        whomSendAdvertiserId = null;
        whomSendAdId = null;
    }

    function SetFirstConversationAsActive() {
        if ($('.conversation-item').length !== 0) {
            $('.conversation-item').first().trigger('click');
        }
    }

    function SetDefaultConversationItem(adId, advertiserId) {
        var updateConversationUrl = '/WSChat/GetDefaultTemplateByConversationItem';
        jQuery.ajaxSetup({ async: false });
        $.get(updateConversationUrl, { whomSendId: advertiserId, adId: adId }, function (response) {
            $('.scrollbar-conversations-vertical #ws-chat-conversations-list-item').prepend(response);
            $('.conversations-list .empty-block').addClass("hidden");
        });
    }

    function SetDefaultUserContactInfo(advertiserId) {
        var url = '/WSChat/GetDefaultUserContactInfo';
        jQuery.ajaxSetup({ async: false });
        $.get(url, { whomSendId: advertiserId }, function (response) {
            $('#ws-chat-userinfo-area').html('');
            $('#ws-chat-userinfo-area').html(response);
        });
    }

    function SetDefaultMessagePanel(adId, advertiserId) {
        var updateMessagePanelUrl = '/WSChat/GetChatMessageDefaultPanel';

        jQuery.ajaxSetup({ async: false });
        $.get(updateMessagePanelUrl, { whomSendId: advertiserId, adId: adId }, function (response) {
            try {
                $('#ws-chat-dialog-area').html('');
                $('#ws-chat-dialog-area').html(response);
            }
            catch (err)
            {
                $('.default-conversation-item')[0].click();
                //if ($('#ws-chat-modal').hasClass('conversation-on-loading')) {
                //    $('.default-conversation-item')[0].click();
                //}
            }

            $('#ws-chat-active-user').val("default-conversation-item-" + advertiserId);

            $('#ws-chat-modal').removeClass('no-active-dialogs');
            $('#ws-chat-modal').trigger('hide_loading');

            if ($('#ws-chat-dialog-area').find('.empty-block').length) {
                $('.default-conversation-item')[0].click();
            }

            var width = $(window).width();
            if (width < 850) {
                $('textarea').attr("placeholder", "Mesazhi juaj");
            }
        });
    }

    function GetExistingCnversation(conversationId) {
        var url ='/WSChat/GetConversationItem';
        $.get(url, { conversationId: conversationId }, function (response) {
            exeptConversationId = conversationId;
            $('.scrollbar-conversations-vertical #ws-chat-conversations-list-item').prepend(response);
            $("#conversation-item-" + conversationId).trigger('click');
        });
    }

    function HandleOpenExistConversation(adId, advertiserId) {
        SuccessOpenExistConversation = false;

        if (adId === null && advertiserId === null) {
            SuccessOpenExistConversation = true;
            SetFirstConversationAsActive();
            return;
        }

        var url = '/WSChat/FindConversationId';

        $.get(url, { whomSendId: advertiserId, adId: adId }, function (conversationId) {
            if (conversationId !== '') {
                var itemId = "#conversation-item-" + conversationId;

                if ($('.conversation-item').length !== 0) {
                    SuccessOpenExistConversation = true;
                    if ($(itemId).length !== 0) {
                        $(itemId).trigger('click');

                        var scrorrTo = $(itemId)[0].offsetTop - 400;
                        $('.scrollbar-conversations-vertical').mCustomScrollbar("scrollTo", scrorrTo);
                    }
                    else {
                        GetExistingCnversation(conversationId);
                    }
                }
            }

            if (SuccessOpenExistConversation) {
                return;
            }

            if (!isNewConversation) {
                SuccessOpenExistConversation = true;
                SetFirstConversationAsActive();
                return;
            }

            if (SuccessOpenExistConversation === false) {
                HandleCreateNewConversation(adId, advertiserId);
            }
        });
    }

    function HandleCreateNewConversation(adId, advertiserId) {
        $('#ws-chat-modal').trigger('show_loading');
        SetDefaultConversationItem(adId, advertiserId);
        SetDefaultUserContactInfo(advertiserId);
        SetDefaultMessagePanel(adId, advertiserId);
    }


    function HideContactInfoOnSmallDisplay() {
        if ($(document).width() <= 1399) {
            if (!$('#ws-chat-modal').hasClass('info-collapsed')) {
                $('#ws-chat-modal').addClass('info-collapsed');
            }
        }
        else {
            $('#ws-chat-modal').removeClass('info-collapsed');
        }
    }

    $(document).ready(function () {
        InitChat('0');

        $(document).on('click', '.default-conversation-item', function () {
            $('#ws-chat-modal').trigger('show_loading');

            SetDefaultUserContactInfo(whomSendAdvertiserId);
            SetDefaultMessagePanel(whomSendAdId, whomSendAdvertiserId);
        });

        $('#ws-chat-unreaded-total-count').on('none', function () {
            var url = '/WSChat/GetUnreadedCountByAllConversations';

            $.get(url, function (response) {
                $('#ws-chat-unreaded-total-count').html('');
                $('#ws-chat-unreaded-total-count').html(response);
            });
        })

        $('#ws-chat-m-unreaded-total-count').on('none', function () {
            var url = '/WSChat/GetUnreadedCountByAllConversations';

            $.get(url, function (response) {
                $('#ws-chat-m-unreaded-total-count').html('');
                $('#ws-chat-m-unreaded-total-count').html(response);
            });
         })
        /* --------------------- Loading ---------------------- */
        $('#ws-chat-modal').on('show_loading', function () {
            $('#ws-chat-modal').addClass('conversation-on-loading');
        })

        $('#ws-chat-modal').on('hide_loading', function () {
            $('#ws-chat-modal').removeClass('conversation-on-loading');
        })
        /* --------------------- /Loading ---------------------- */
        /* --------------------- Blocked ---------------------- */
        $('#ws-chat-modal').on('block_by_you', function () {
            $('#ws-chat-modal').addClass('blocked-by-you');
        })

        $('#ws-chat-modal').on('unblock_by_you', function () {
            $('#ws-chat-modal').removeClass('blocked-by-you');
        })

        $('#ws-chat-modal').on('block_by_user', function () {
            $('#ws-chat-modal').addClass('blocked-by-user');
        })

        $('#ws-chat-modal').on('unblock_by_user', function () {
            $('#ws-chat-modal').removeClass('blocked-by-user');
        })

        $('#ws-chat-modal').on('show_typing', function () {
            $('#ws-chat-modal').addClass('other-user-typing');
            CustomScrollbarOnUpdate();
        })

        $('#ws-chat-modal').on('hide_typing', function () {
            $('#ws-chat-modal').removeClass('other-user-typing');
            CustomScrollbarOnUpdate();
        })
        /* --------------------- /Blocked ---------------------- */
        /* --------------------- Conversations Loading ---------------------- */
        $('#ws-chat-conversations-list').on('none', function () {
            var filterType = $('#ws-chat-filter-type').val();
            $.get("/WSChat/ChatConversation?filter=" + filterType, function (response) {
                var activeUserId = $('#ws-chat-active-user').val();

                $('#ws-chat-conversations-list').html('');
                $('#ws-chat-conversations-list').html(response);

                ConnectScrollbarToConversations();

                if ($('.conversation-item').length !== 0) {
                    $('#' + activeUserId).addClass('active');
                }

                if ($('.conversation-item').length === 0) {
                    $('#ws-chat-modal').addClass('no-active-dialogs');
                    $('#ws-chat-dialog-area').trigger('show_empty_content');
                    $('#ws-chat-modal').trigger('hide_loading');
                }
            });
        });

        $('#ws-chat-conversations-list').on('loading', function () {
            $('#ws-chat-modal').trigger('show_loading');

            var filterType = $('#ws-chat-filter-type').val();
            $.get("/WSChat/ChatConversation?filter=" + filterType, function (response) {
                $('#ws-chat-conversations-list').html('');
                $('#ws-chat-conversations-list').html(response);

                ConnectScrollbarToConversations();
                ConversationFieldHeightUpdate();
                CustomScrollbarOnUpdate();

                HandleOpenExistConversation(whomSendAdId, whomSendAdvertiserId);

                if ($('.conversation-item').length === 0) {
                    $('#ws-chat-modal').addClass('no-active-dialogs');
                    $('#ws-chat-dialog-area').trigger('show_empty_content');
                    $('#ws-chat-modal').trigger('hide_loading');
                }
            });
        });
        /* --------------------- /Conversations Loading ---------------------- */
        /* --------------------- Search Conversation ---------------------- */
        $('#search-conversation-button').on('click', function () {
            filterList();
        })

        $('#search-conversation-input').change(function () {
            filterList();
        })
        /* --------------------- /Search Conversation ---------------------- */
        /* --------------------- Chat modal window ---------------------- */
        $(document).on('click', '.chat-show', function () {
            SetDefaultValueHandleNewConversation();
            exeptConversationId = null;
            HideContactInfoOnSmallDisplay();

            if ($(this).data("newConersation") === "true" || $(this).data("newConersation") === true) {
                whomSendAdvertiserId = $(this).data("advertiserId");
                whomSendAdId = $(this).data("id");
                isNewConversation = true;
            }

            $('.chat-modal').addClass('active');
            $('body').addClass('chat-window-show');

            $('#chat-current-status').trigger('none');
            $('#ws-chat-enable-sound').trigger('none');
            $('#ws-chat-conversations-list').trigger('loading');
        });

        $(document).on('click', '.chat-close', function () {
            $('.chat-modal').removeClass('active');
            $('body').removeClass('chat-window-show');

            $('#ws-chat-conversations-list').html('');
            $('#ws-chat-dialog-area').html('');
            $('#ws-chat-userinfo-area').html('');
        });

        $(document).mouseup(function (a) {
            var div = $(".chat-modal .chat-window");
            if (!div.is(a.target)
                && div.has(a.target).length === 0) {
                $(".chat-modal").removeClass("active");
                $("body").removeClass("chat-window-show");
            };
        });
        /* -------------------- /Chat modal window ---------------------- */
        /* --------------------- User Status popup ---------------------- */
        $('#ws-chat-enable-sound').on("none", function () {
            var self = '#ws-chat-enable-sound';

            $.get("/WSChat/IsEnableChatSound", function (response) {
                if (response === 'true') {
                    $(self).attr('checked', 'checked');
                    $('.switch-option .switch.switch-block i').addClass('no-after');
                }

                if (response === 'false') {
                    $(self).removeAttr('checked');
                    $('.switch-option .switch.switch-block i').removeClass('no-after');
                }
            });
        });

        $('#ws-chat-enable-sound').on("new_message", function () {
            var self = '#ws-chat-enable-sound';
            var isEnable = $(self).val();

            if (isEnable === 'true' || isEnable === 'on') {
                PlaySound();
            }
        });

        $('#ws-chat-enable-sound').on("change", function () {
            var url = '/Dispatcher/Push?incTypes=ToggleChatSoundSettingCommand&amp;AdvertiserID=0';
            var self = '#ws-chat-enable-sound';

            $.post(url).done(function (response) {
                $(self).trigger('none');
            })
        });

        // User Update Status
        $('#chat-current-status').on('none', function () {
            $.get("/WSChat/GetChatStatus?userId=0", function (response) {
                $('.chat-modal .chat-status-popup').removeClass('status-offline');
                $('.chat-modal .chat-status-popup').removeClass('status-busy');
                $('.chat-modal .chat-status-popup').removeClass('status-online');

                var status = "status-" + response;
                $('.chat-modal .chat-status-popup').addClass(status);

                if (response === 'online') {
                    $('.chat-modal .chat-status-popup .custom-popup-btn-value').text('Online');

                    $('.chat-status-popup .custom-popup-option a').closest('.chat-status-popup .custom-popup-panel').children().children().children().removeClass('active');
                    $('.chat-status-popup .custom-popup-option a.online').parent().addClass('active');
                }

                if (response === 'busy') {
                    $('.chat-modal .chat-status-popup .custom-popup-btn-value').text('I zënë');

                    $('.chat-status-popup .custom-popup-option a').closest('.chat-status-popup .custom-popup-panel').children().children().children().removeClass('active');
                    $('.chat-status-popup .custom-popup-option a.busy').parent().addClass('active');
                }

                if (response === 'offline') {
                    $('.chat-modal .chat-status-popup .custom-popup-btn-value').text('I padukshëm');

                    $('.chat-status-popup .custom-popup-option a').closest('.chat-status-popup .custom-popup-panel').children().children().children().removeClass('active');
                    $('.chat-status-popup .custom-popup-option a.offline').parent().addClass('active');
                }
            });
        });

        $('.custom-popup .custom-popup-option .online').on("click", function () {
            var url = '/Dispatcher/Push?incTypes=SetChatStatusCommand&amp;AdvertiserId=0&amp;Status=0';
            var status = 'online';
            var self = '.custom-popup .custom-popup-option .online';

            $.post(url, { status: status }).done(function (response) {
                $(self).closest('.custom-popup').removeClass('status-offline');
                $(self).closest('.custom-popup').removeClass('status-busy');
                $(self).closest('.custom-popup').addClass('status-online');
            })
        });

        $('.custom-popup .custom-popup-option .busy').on("click", function () {
            var url = '/Dispatcher/Push?incTypes=SetChatStatusCommand&amp;AdvertiserId=0&amp;Status=0';
            var status = 'busy';
            var self = '.custom-popup .custom-popup-option .busy';

            $.post(url, { status: status }).done(function (response) {
                $(self).closest('.custom-popup').removeClass('status-offline');
                $(self).closest('.custom-popup').removeClass('status-online');
                $(self).closest('.custom-popup').addClass('status-busy');
            })
        });

        $('.custom-popup .custom-popup-option .offline').on("click", function () {
            var url = '/Dispatcher/Push?incTypes=SetChatStatusCommand&amp;AdvertiserId=0&amp;Status=0';
            var status = 'offline';
            var self = '.custom-popup .custom-popup-option .offline';

            $.post(url, { status: status }).done(function (response) {
                $(self).closest('.custom-popup').removeClass('status-online');
                $(self).closest('.custom-popup').removeClass('status-busy');
                $(self).closest('.custom-popup').addClass('status-offline');
            })
        });
        /* -------------------- /User Status popup ---------------------- */
        /* --------------------- Conversation Chat Filter By Type ---------------------- */
        $('.custom-popup-filter-type .custom-popup-option a.chat-filter').on("click", function () {
            SetDefaultValueHandleNewConversation();
            $(this).closest('.custom-popup-panel').children().children().children().removeClass('active');
            $(this).parent().addClass('active');
            var PopupSelectedOption = $(this).children('.custom-popup-option-value').text();
            if (PopupSelectedOption.length > 0) {
                $(this).closest('.custom-popup').children().children().children('.custom-popup-btn-value').text(PopupSelectedOption);
            }

            var filterType = $(this).data("val");

            exeptConversationId = null;

            $('#ws-chat-filter-type').val(filterType);
            $('#ws-chat-user-list').trigger("none");
            $('#ws-chat-conversations-list').trigger('loading');
            $('.chat-dialog-content').empty();
        });
        /* --------------------- /Conversation Chat Filter By Type ---------------------- */
        /* ----------------- Chat User Info Collapser ------------------- */
        $(document).on('click', '.userinfo-area-collapser', function () {
            $(this).closest('.chat-modal').toggleClass('info-collapsed');
            $(this).toggleClass('active');
        });
        /* ---------------- /Chat User Info Collapser ------------------- */
        /* ----------------- Chat Dialog Area Empty Content ------------------- */
        $(document).on('show_empty_content', '#ws-chat-dialog-area', function () {
            var url = '/WSChat/ChatConversationNotSelected'

            jQuery.ajaxSetup({ async: false });
            $.get(url, function (response) {
                $('#ws-chat-dialog-area').html('');
                $('#ws-chat-dialog-area').html(response);
            });
        });
        /* ----------------- /Chat Dialog Area Empty Content ------------------- */
        /* --------------------- Chat textarea -------------------------- */
        $(document).on('input', '.chat-dialog-input-area .input-field textarea', function () {
            if (!isNewConversation) {
                this.style.height = '1px';
                this.style.height = (this.scrollHeight + 6) + 'px';
                ConversationFieldHeightUpdate();
            }
        });
        /* -------------------- /Chat textarea -------------------------- */
        /* ------------------ Select Attachment ------------------------- */
        $(document).on('change', '.chat-modal .attach-image', function () {
            if ($(this).val() != '') {
                $(this).closest('.chat-modal').removeClass('message-with-file-attachment');
                $(this).closest('.chat-modal').addClass('message-with-image-attachment');
                ConversationFieldHeightUpdate();
            }
        });

        $('.chat-modal .attach-image').change(function () {
            if ($(this).val() != '') {
                $(this).closest('.chat-modal').removeClass('message-with-file-attachment');
                $(this).closest('.chat-modal').addClass('message-with-image-attachment');
                ConversationFieldHeightUpdate();
            }
        });

        $(document).on('change', '.chat-modal .attach-file', function () {
            if ($(this).val() != '') {
                $(this).closest('.chat-modal').removeClass('message-with-image-attachment');
                $(this).closest('.chat-modal').addClass('message-with-file-attachment');
                ConversationFieldHeightUpdate();
            }
        });

        $('.chat-modal .attach-file').change(function () {
            if ($(this).val() != '') {
                $(this).closest('.chat-modal').removeClass('message-with-image-attachment');
                $(this).closest('.chat-modal').addClass('message-with-file-attachment');
                ConversationFieldHeightUpdate();
            }
        });

        $(document).on('click', '.chat-modal .attachment-file-block .btn-remove', function () {
            $(this).closest('.chat-modal').removeClass('message-with-file-attachment');
            $('.chat-modal .attachment-file-block .btn-remove').trigger('clean_data');
            ConversationFieldHeightUpdate();
        });

        $(document).on('click', '.chat-modal .attachment-image-block .btn-remove', function () {
            $(this).closest('.chat-modal').removeClass('message-with-image-attachment');
            $('.chat-modal .attachment-image-block .btn-remove').trigger('clean_data');
            ConversationFieldHeightUpdate();
        });

        $(document).on('click', '.chat-modal .send-message', function () {
            $(this).closest('.chat-modal').removeClass('message-with-image-attachment');
            $(this).closest('.chat-modal').removeClass('message-with-file-attachment');
            ConversationFieldHeightUpdate();
        });
        /* ----------------- /Select Attachment ------------------------- */
        /* -------------- Mobile Version Navigation --------------------- */
        $('.btn-back-to-contacts').on('click', function () {
            $(this).closest('.chat-modal').removeClass('m-display-conversation');
            $(this).closest('.chat-modal').removeClass('m-display-info');
            $(this).closest('.chat-modal').addClass('m-display-contacts');
            ConversationFieldHeightUpdate();
        });

        $('.btn-back-to-conversation').on('click', function () {
            $(this).closest('.chat-modal').removeClass('m-display-contacts');
            $(this).closest('.chat-modal').removeClass('m-display-info');
            $(this).closest('.chat-modal').addClass('m-display-conversation');
            ConversationFieldHeightUpdate();
        });

        $(document).on('click', '.chat-modal .conversation-item', function () {
            $(this).closest('.chat-modal').removeClass('m-display-contacts');
            $(this).closest('.chat-modal').removeClass('m-display-info');
            $(this).closest('.chat-modal').addClass('m-display-conversation');
            $('.chat-modal .conversation-item').removeClass('active');
            $(this).addClass('active');
            ConversationFieldHeightUpdate();
        });
        //$('.chat-modal .view-user-info').on('click', function () {
        //    $(this).closest('.chat-modal').removeClass('m-display-contacts');
        //    $(this).closest('.chat-modal').removeClass('m-display-conversation');
        //    $(this).closest('.chat-modal').addClass('m-display-info');
        //    $(this).addClass('active');
        //    ConversationFieldHeightUpdate();
        //});
        /* ------------- /Mobile Version Navigation --------------------- */
        $(window).on('resize', function () {
            if ($('.chat-dialog-input-area .input-field textarea').length) {
                ConversationFieldHeightUpdate();
                CustomScrollbarOnUpdate();
                ScrollMessagePanelToBottom();
            }
        });
    });

    //code here because if use partial views, standart Jquery triggers don't work
    /* ====================================================== Popups ============================================================== */
    $(document).click(function (e) {
        if ($(e.target).closest(".custom-popup").length) {
            return;
        }
        $(".custom-popup").removeClass("active");
    });

    $(document).ready(function () {
        $(document).on("click", '.custom-popup-option a', function () {
            $(this).closest('.custom-popup-panel').children().children().children().removeClass('active');
            $(this).parent().addClass('active');
            var PopupSelectedOption = $(this).children('.custom-popup-option-value').text();
            if (PopupSelectedOption.length > 0) {
                $(this).closest('.custom-popup').children().children().children('.custom-popup-btn-value').text(PopupSelectedOption);
            }
        });

        $(document).on("click", '.custom-popup .custom-popup-btn', function () {
            $('.custom-popup').removeClass('active');
            $(this).closest('.custom-popup').addClass('active');
        });

        $(document).on('click', '.custom-popup-close', function () {
            $(this).closest('.custom-popup').removeClass('active');
        });

        $(document).on('click', '.custom-popup-overlay', function () {
            $(this).closest('.custom-popup').removeClass('active');
        });
    });
    /* ===================================================== /Popups ============================================================== */
</script>
 


