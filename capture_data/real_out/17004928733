<script>
        window.addEventListener("load", function () {
            if (document.querySelector(".banner-content") != null || document.querySelector(".banner-content") != undefined) {
			
			cg = document.createElement("div");
			cg.classList.add("col-lg-4", "col-md-4","actu-fv-col-gauche");
			cd = document.createElement("div");
			cd.classList.add("col-lg-8", "col-md-8","actu-fv-col-droite");
			dest = document.querySelector(".block-pu-page-news-in-brief .container .row .content");
			
			cg.appendChild(document.querySelector(".banner-content"));
			
			cd.appendChild(document.querySelector(".block-pu-page-news-in-brief .title-wrapper"));
			cd.appendChild(document.querySelector(".block-pu-page-news-in-brief .list-actu").parentNode);
			
			
			dest.append(cg);
			dest.append(cd);
			

            }

            /*repositionnement Fil Ariane*/
            /**/
            bc = document.querySelector(".breadcrumb");
            bct = document.querySelector(".block-pu-page-news-in-brief .container.container-m");
            if (bc != null || bc != undefined) {
                /*pour home page car pas de fil ariane*/
                bct.insertBefore(bc, bct.children[0]);
            }

            ///////////////////////
            /*AUTO COL*/

            lesbev = document.querySelectorAll(".bloc-evenementiel-paragraph");

            for (i = 0; i < lesbev.length; i++) {
                if (lesbev[i].querySelector(".auto-col") != null || lesbev[i].querySelector(".auto-col") != undefined) {
                    larow = document.createElement("div");
                    larow.classList.add("row", "row-fv-banner");
                    larow.id = "larow-" + i;
                    lesbev[i].parentNode.insertBefore(larow, lesbev[i]);

                    pourlescol = lesbev[i].querySelectorAll(".auto-col");

                    nbcol = pourlescol.length;
					
					//detection si colonne large (pour 3 col uniquement)	
					for (k = 0; k < pourlescol.length; k++) {
					if(pourlescol[k].classList.contains("large")){
					var collarge=1;
					}else{
					var collarge=0;					
					}
					}

                    for (j = 0; j < pourlescol.length; j++) {
                        leconteneur = document.createElement("div");
                        leconteneur.classList.add("newfv-banner");					
					
					
                        pourlescol[j].classList.add("content");

                        lacol = document.createElement("div");
                        if (nbcol == 1) {
                            lacol.classList.add("col-xs-12", "col-sm-12", "col-lg-12");
                        }
                        if (nbcol == 2) {
                            lacol.classList.add("col-xs-12", "col-sm-6", "col-lg-6");
                        }
                        if (nbcol == 3) {
						if(collarge==0){//cas normal auto
						lacol.classList.add("col-xs-12", "col-sm-6", "col-lg-4");
						}else{//presence d'une colonne large
						
                              if(pourlescol[j].classList.contains("large")){				
                              leconteneur.classList.add("large");//pour css image entete via conteneur							  						  
							  lacol.classList.add("col-xs-12", "col-sm-6", "col-lg-6");
                              }else{
                              lacol.classList.add("col-xs-12", "col-sm-6", "col-lg-3");
                              }																
						}							
												
                            
                        }
                        if (nbcol >= 4) {
                            lacol.classList.add("col-xs-12", "col-sm-6", "col-lg-3");
                        }


                        if (pourlescol[j].hasAttribute("data-bg")) {
                            leconteneur.classList.add("pleine-couleur");
                            leconteneur.style.background = pourlescol[j].getAttribute("data-bg");
                        }

                        /*
limg=document.createElement("div");
limg.classList.add("img-conteneur");
    if(pourlescol[j].hasAttribute("data-bg-img")){
    limg.style.background=pourlescol[j].getAttribute("data-bg-img");
    }
	*/
                        limage = pourlescol[j].querySelector("img");
                        if (limage != null || limage != undefined) {
                            limg = document.createElement("div");
                            limg.classList.add("img-conteneur");
                            if (pourlescol[j].hasAttribute("data-bg-img")) {
                                limg.style.background = pourlescol[j].getAttribute("data-bg-img");
                            }

                            limg.appendChild(limage);
                            leconteneur.appendChild(limg);
                        }

                        leconteneur.appendChild(pourlescol[j]);

                        larow.appendChild(lacol);
                        lacol.appendChild(leconteneur);
                        //leconteneur.appendChild(limg);
                        //leconteneur.appendChild(pourlescol[j]);
                    }
                } //if
            }
        });

