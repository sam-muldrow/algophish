<script>
window.addEventListener('DOMContentLoaded', (event) => {
  var bell = document.querySelector('.main-page__bell');
  var notifocationInfoButton = document.querySelector('.main-page__notification-background');
  var notificationInfContainer = document.querySelector('.main-page__info-notification');
  var notifivationCloseButton = document.querySelector('.main-page__info-notification p');
  var notifivationCloseButtonMobile = document.querySelector('.main-page__info-notification-no-button-mobile');
  var addNoteButton = document.querySelector('.main-page__info-notification-button');
  var addNoteButtonMobile = document.querySelector('.main-page__info-notification-ok-button-mobile');
  var notificationInfoContainerMobile = document.querySelector('.main-page__info-notification-mobile');
  var hasClicked = JSON.parse(window.localStorage.getItem('clicked'));
  var saidNo = false;
  if(hasClicked) {
    saidNo = hasClicked.isClicked;
  }

  function userDidNotClicked() {
    bell.style.display = "block";
    notifocationInfoButton.style.display = "flex";
    notifocationInfoButton.addEventListener('click', function(){
      notificationInfContainer.style.display = "block";
      notifocationInfoButton.classList.remove('main-page__notification-animation');
    })
    notifivationCloseButton.addEventListener('click', function(){
      notificationInfContainer.style.display = "none";
    })
    addNoteButton.addEventListener('click', function() {
      setupPRNotifications();
      bell.style.display = "none";
    })
  }
  function userDidNotClickedMobile() {
    bell.style.display = "flex";
    bell.classList.add('main-page__bell-mobile');
    notificationInfoContainerMobile.style.display = "flex";

    notifivationCloseButtonMobile.addEventListener('click', function(){
      bell.style.display = "none";
      var clicked = {
        isClicked: true
      }
      window.localStorage.setItem('clicked', JSON.stringify(clicked));
      window.removeEventListener('scroll', userDidNotClickedMobile);
    })
    addNoteButtonMobile.addEventListener('click', function() {
      setupPRNotifications();
      bell.style.display = "none";
      window.removeEventListener('scroll', userDidNotClickedMobile);
    })
  }
  async function askForSubscription() {
    if(window.innerWidth >= 768) {
      var isSubscription = await findSubscription();
      if (!isSubscription) {
        userDidNotClicked();
      }
    } else {
      var isSubscription = await findSubscription();
      if (!isSubscription && !saidNo) {
        window.addEventListener('scroll', userDidNotClickedMobile);
      }
    }
  }
  if (navigator.userAgent.indexOf("Chrome") !== -1){
    if (navigator.userAgent.indexOf("Edge") === -1){
      askForSubscription();
    }
  }
});
</script>

    </div>

</div>
