<script>
    let podcastsArr = [311, 239, 277, 328, 331, 138, 112, 230];

    function getRandomIntInclusive(min, max) {
        min = Math.ceil(min);
        max = Math.floor(max);
            return Math.floor(Math.random() * (max - min + 1) + min);
    }

    let pod1 = podcastsArr[getRandomIntInclusive(0,3)];
    let pod2 = podcastsArr[getRandomIntInclusive(4,7)];

    let iframe1 = document.getElementById("iframe1");
    let iframe2 = document.getElementById("iframe2");
    iframe1.src="https://www.embedder.polskieradio.pl/podcastBox/"+pod1+""
    iframe2.src="https://www.embedder.polskieradio.pl/podcastBox/"+pod2+""
</script>
    </div>

