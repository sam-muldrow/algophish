    <div id="ctl00_ctl00_ctl00_ContentPlaceHolder1_ContentPlaceHolder1_Section01_ctl02_ContentDiv">
        <script>
  // DEDUPLICATE ARTICLES W/MAIN MOSAIC
  let seeAlsoArticles = [
    ...document.querySelector(".seealso").querySelectorAll("article"),
  ];
  if (document.querySelector('[data-idboxinstance="1300002"]')) {
    const mosaicIds = [
      ...document
        .querySelector('[data-idboxinstance="1300002"]')
        .querySelectorAll("a"),
    ].map((link) => link.href.split("/").slice(6, 7).join("").split(",")[0]);
    mosaicIds.forEach((id) =>
      seeAlsoArticles.forEach((article) =>
        article.classList.contains(`ID-${id}`) ? article.remove() : null
      )
    );
    seeAlsoArticles = [
      ...document.querySelector(".seealso").querySelectorAll("article"),
    ];
    seeAlsoArticles.slice(12).forEach((article) => article.remove());
  }
  if (document.querySelector('[data-idboxinstance="1300004"]')) {
    const mosaicIds = [
      ...document
        .querySelector('[data-idboxinstance="1300004"]')
        .querySelectorAll("a"),
    ].map((link) => link.href.split("/").slice(6, 7).join("").split(",")[0]);
    mosaicIds.forEach((id) =>
      seeAlsoArticles.forEach((article) =>
        article.classList.contains(`ID-${id}`) ? article.remove() : null
      )
    );
    seeAlsoArticles = [
      ...document.querySelector(".seealso").querySelectorAll("article"),
    ];
    seeAlsoArticles.slice(12).forEach((article) => article.remove());
  }
</script>

<style>
  @media (max-width: 767px) {
    .seealso {
      margin-top: 0;
    }
  }
</style>

    </div>

</div>

