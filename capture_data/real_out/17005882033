    <div id="ctl00_ctl00_ctl00_ContentPlaceHolder1_ContentPlaceHolder1_Section19_ctl06_ContentDiv">
        <script>
  $( document ).ready(function() {
      $('.mosaic-row .latozradiem2023').each(function(){
          var lzrPathName = this.pathname.split('/')[4];
          var lzrArticleLink = lzrPathName.split(',')[0];
          this.pathname = '/post/' + lzrArticleLink;
          this.hostname = "latozradiem.pl";
          var link = document.querySelector(".latozradiem2023");
          link.target = "_blank";
      });
      $('.lzr-articles a').each(function(index, element) {
        var url = element.href;
        if (url.includes('polskieradio.pl/522/')) {
          element.target = "_blank";
        }
      });
      $('#mosaic-modern .polskieradiodzieciom').each(function() {
          var tagName = $(this).find('.tag');
          tagName.attr('data-content', 'Polskie Radio Dzieciom');
      });
      $('.mosaic-row .radiokierowcow').each(function(){
          var rkPathName = this.pathname.split('/')[4];
          var rkArticleLink = rkPathName.split(',')[0];
          this.pathname = '/artykul/' + rkArticleLink;
          this.hostname = "radiokierowcow.pl";
      });
      $('.mosaic-row .mosaic-news').each(function(){
          var pathNamePR24 = this.pathname.toLowerCase();
          this.pathname = pathNamePR24;
          this.hostname = "polskieradio24.pl";
      });
  });
</script>

    </div>

</div>

