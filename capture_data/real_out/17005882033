<div id="ajax-wrap-body">
    
	
	<script>
		jQuery(function ($) {
			if ($.isFunction($.fn.PrsaAjaxTabs)) {
				$('._boxNaAntenieTaby').PrsaAjaxTabs({
					countElements: ($.screen.width <= 480 ? 3 : 0),
					nodeElements: '._forPlayer > article',
					onCreateTab: function (event, arg) {
						var gsid = arg.tabHead.data("gsid") || 0;

						if (gsid != 0) {
							for (var i = 0, l = SCHEDULE.programmes.length; i < l; i++) {
								if (SCHEDULE.programmes[i].gsid == gsid) {
									$('<article/>', { 'class': 'schedule' }).prependTo(arg.tab.find('._forPlayer')).SchedulePrograms({
										program: SCHEDULE.programmes[i],
										datetime: DateTime.Now(),
										template: $('#template-schedule')
									});
									break;
								}
							}
						}
					},
					onChangeTab: function (event, arg) {
						$('.schedule', arg.tab).SchedulePrograms('refresh');
						$('#Mobile-NaAntenie-Taby').trigger('click');
						$(document).trigger('lazyload.reinit');
					}
				});

				$('._boxTematycznieTaby').PrsaAjaxTabs({
					countElements: ($.screen.width <= 480 ? 3 : 0),
					nodeElements: '._forPlayer > article',
					onChangeTab: function (event, arg) {
						$('#Mobile-Tematycznie-Taby').trigger('click');
						$(document).trigger('lazyload.reinit');
					}
				});

				$('._boxOstatnioNaAntenieTaby').PrsaAjaxTabs({
					onChangeTab: function () {
						$(document).trigger('lazyload.reinit');
					}
				});
			}

			if ($.isFunction($.fn.Tabs)) {
				$('#_boxBooksTabs').Tabs();
			}

			$('#Mobile-NaAntenie-Taby, #Mobile-Tematycznie-Taby').on('click', function () {
				$(this).toggleClass('active');
				$(this).parent().toggleClass('visible');
			});

			if ($.isFunction($.fn.cycle)) {
				$.responsive.append({
					element: null,
					backup: null,
					instanceCycle: null,
					init: function () {
						this.element = $('.muzyka-hp section');
						this.backup = this.element.html();
						this.reload();
						return this;
					},
					reload: function (e) {
						if (this.instanceCycle !== null) {
							this.destroy();
							this.element.html(this.backup);
						}

						var carouselVisible = 4;
						if ($.screen.width < 768) {
							carouselVisible = 1;
						}
						else if ($.screen.width < 992) {
							carouselVisible = 2;
						}
						else if ($.screen.width <= 1280) {
							carouselVisible = 3;
						}

						setTimeout($.proxy(function () {
							this.instanceCycle = this.element.cycle({
								timeout: 0,
								slides: '.element',
								fx: 'carousel',
								swipe: true,
								carouselVisible: carouselVisible,
								carouselFluid: true,
								prev: '.muzyka-hp .prev',
								next: '.muzyka-hp .next',
								log: false
							});
						}, this), 100);
					},
					destroy: function () {
						if (this.instanceCycle !== null) {
							this.instanceCycle.cycle('destroy');
							this.instanceCycle = null;
						}
					}
				}).init();

				$.responsive.append({
					element: null,
					backup: null,
					instanceCycle: null,
					init: function () {
						this.element = $('.slider-top');
						this.backup = this.element.html();
						this.reload();
						return this;
					},
					reload: function (e) {
						if (this.instanceCycle !== null) {
							this.destroy();
							this.element.html(this.backup);
						}

						if ($.screen.width < 768) {
							setTimeout($.proxy(function () {
								this.instanceCycle = this.element.cycle({
									timeout: 0,
									fx: 'scrollHorz',
									slides: '.mosaic .for-mobile a',
									swipe: true,
									prev: '.mosaic-slider .prev',
									next: '.mosaic-slider .next',
									pager: '.mosaic-slider .pager',
									log: false
								});
							}, this), 100);
						}
					},
					destroy: function () {
						if (this.instanceCycle !== null) {
							this.instanceCycle.cycle('destroy');
							this.instanceCycle = null;
						}
					}
				}).init();
			}
		});
	</script>
