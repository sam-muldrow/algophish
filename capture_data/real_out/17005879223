    <script type="text/javascript">
        (function() {
            function getQuery(variable) {
               var query = window.location.search.substring(1);
               var vars = query.split("&");
               for (var i=0;i<vars.length;i++) {
                       var pair = vars[i].split("=");
                       if(pair[0] == variable){return pair[1];}
               }
               return(false);
            }
            var trackingNumber = getQuery('trackingNumber') || getQuery('tracking_number') || getQuery('trackingNo') || getQuery('orderNumber')
            var type = getQuery('type') || ''
            var iframe = document.getElementById('iframe')
            var url = "https://ese.ecmsglobal.com/#/language/zh?top=www"
            if (trackingNumber) {
                url = "https://ese.ecmsglobal.com/#/language/zh?top=www&orderNumber="+ trackingNumber
            }
            if (type) {
                url = url + '&type=' + type
            }
            iframe.src = url
        })();
    </script>
