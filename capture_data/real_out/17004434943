<script language="JavaScript" type="text/javascript" charset="utf-8">
try {
(function(w, d ,l){
function r(r){var e=r+"=";var n=decodeURIComponent(d.cookie);var o=n.split(";");for(var a=0;a<o.length;a++){var t=o[a];while(t.charAt(0)==" "){t=t.substring(1)}if(t.indexOf(e)==0){return t.substring(e.length,t.length)}}return""}
var consentValues = [], cookieValue = r("CONSENTMGR"), consent = cookieValue.split("|");
for (i = 0; i < consent.length; i++) {
consentPart = consent[i].split(":");
consentValues[consentPart[0]] = consentPart[1];
}
if (cookieValue !== "" && consentValues.consent === "true" && consentValues.c3 !== "0" && localStorage.getItem("telekom.poc") === "1")
//init vars
w._trboq = w._trboq || [];
w._trbo_obj = {
apiStatus: "init",
dataLayer: {},
pageType: "other"
};
//data lookup
switch (l.href) {
case "https://www.telekom.de/start":
w._trbo_obj.pageType = "home";
w._trbo_obj.dataLayer.page_content_id = "www.telekom.de.privatkunden.start";
break;
}
//push data to queue
w._trboq.push(["page", { type: w._trbo_obj.pageType }]);
w._trboq.push(["dataMatching", w._trbo_obj.dataLayer]);
d.write('<script src="//www2.telekom.de/forward/static/plugin/trbo_13490_5b689651f8825154da7c973fc704e020.js" type="text/javascript"><\/script>');
})(window, document, location);
} catch (e) {}
</script>

<script language="JavaScript" type="text/javascript" charset="utf-8">
(function(){
	var oReq = new XMLHttpRequest();
	oReq.onreadystatechange = function(){
		if(oReq.readyState === 4 && oReq.status === 200){
                    ;
		}
		else if(oReq.readyState === 4 && oReq.status === 0){			
			var container = document.querySelector(".footer-section");
			var adformsStyle = document.createElement("style");
			adformsStyle.innerHTML = '.adform-adbox{ display: none !important;}.adform-fallback{display: block !important;}';
			container.insertBefore(adformsStyle, container.firstChild);
		}
		
	}
	oReq.open("GET", "https://s1.adform.net/Banners/Elements/Files/14736/4249823/4249823.js");
	oReq.send();
})();
</script>

<script language="JavaScript" type="text/javascript" charset="utf-8">

(function() {

	function waitforJQ(initNameSpace) {
            if(window.jQuery && document.readyState === "complete" && typeof tsde !== "undefined") {
                initNameSpace();
            } else {
                setTimeout(function(){ waitforJQ(initNameSpace)},20)
            }
    }
    
	function initNameSpace() {
	

		(function($, vsINT, window, document, undefined){
			var legalNoteHelper = vsINT.legalNoteHelper = vsINT.legalNoteHelper || (function(){
				const lnClass = "legal-notes__asterix js-mod", lnDataUrl = "/unterwegs/ajax?ssb_block=dynamic-mapping&toGet=footnotes-popup&footnoteId=", lnTaDataUrl = "/unterwegs/ajax?ssb_block=dynamic-mapping&toGet=footnotes-popup&productId=", dataURL = "";
				
				var injectLegalNote = function(legalNoteId, isHidden){
					let newElemId = getNewElemIdForLegalNote();
					console.log(newElemId);
					
					let newln = createLegalNoteById(legalNoteId, newElemId, isHidden);
					
					insertLegalNoteInDom(newln);
					
					 if ( legalNoteId.match(/tariff:/) ) {
						registerLegalNote(newElemId, legalNoteId, lnTaDataUrl);
					} else {
						registerLegalNote(newElemId, legalNoteId, lnDataUrl);
					}
					
					return newln;
				};

				var createLegalNoteById =  function(legalnoteid, elemId, isHidden){
					var ln = document.createElement("span");
					ln.className = lnClass;
					
					if ( legalnoteid.match(/tariff:/) ) {
						ln.setAttribute("data-url", lnTaDataUrl + legalnoteid);
					} else {
						ln.setAttribute("data-url", lnDataUrl + legalnoteid);
					}

					ln.setAttribute("id", "legal-notes-"+ elemId);
					if(isHidden){
						ln.setAttribute("style", "display:none");
						return ln;
					}
					else{ return ln; }	
				};
				
				var insertLegalNoteInDom = function(legalnote){
					try{
						document.body.appendChild(legalnote);							
						return true;
					}
					catch(e){ console.error(e); return false;}
				};

				var registerLegalNote = function(index, lnId, dataUrl){
					tsde.Core.start("legal-notes-"+index, 'ajaxify-popup', null, {eventName: 'click', url: dataUrl + lnId, popupClass:'generic-popup', preventDefaultAction: true})
				};
				
				var getNewElemIdForLegalNote = function(){
					var currentIndex = getHighestLegalNoteIndex();
					return currentIndex + 1;
				};
				
				var getHighestLegalNoteIndex = function(){
					let highestIndex = 0;
					var allLegalNotes = document.getElementsByClassName('legal-notes__asterix');
					
					if(allLegalNotes !== 'null'){
						for (var i = 0; i < allLegalNotes.length; i++){
						let lnId = allLegalNotes[i].id.replace('legal-notes-','');
						var indexAsInteger = parseInt(lnId, 10);
						if(indexAsInteger > highestIndex){
							highestIndex = indexAsInteger;
						}
					}
					return highestIndex;
					}
				};

				return {
					injectLegalNote: injectLegalNote
				}
			}());
		}(jQuery, window._vsINTv2 = window._vsINTv2 || {}, window, document));


		(function($, vsINT, window, document, undefined){
			var legalNoteHelper = vsINT.legalNoteHelper = vsINT.legalNoteHelper || (function(){
				const lnClass = "legal-notes__asterix js-mod", lnDataUrl = "/unterwegs/ajax?ssb_block=dynamic-mapping&toGet=footnotes-popup&footnoteId=", lnTaDataUrl = "/unterwegs/ajax?ssb_block=dynamic-mapping&toGet=footnotes-popup&productId=", dataURL = "";
				
				var injectLegalNote = function(legalNoteId, isHidden){
					let newElemId = getNewElemIdForLegalNote();
					console.log(newElemId);
					
					let newln = createLegalNoteById(legalNoteId, newElemId, isHidden);
					
					insertLegalNoteInDom(newln);
					
