<script>
var Notfallbox = (function (config) {
    this.config = config;

    let _this = this;

    this.boxTemplate = "<div class='advice' id='teaser-notfall' data-teaser-name='EmergencyBox_Responsive_normal' style='margin-bottom:12px; margin-top:6px;'>" +
        "    <div class='advice_css'>" +
        "        <p class='advice_copy'>" +
        "            <strong>{{HEADLINE}}</strong><br>" +
        "            {{CONTENT}}" +
        "        </p>{{LINK}}" +
        "    </div>" +
        "</div>";
    
    this.linkTemplate = "<a href='{{URL}}'><span class='button __aero __dark' data-no-hover-bubble=''>{{BUTTON_TEXT}}</span></a>"

    this.start = function() {
        if (this.config.contents.headline) {
            let link = "";
            if (this.config.contents.link) {
                link = this.linkTemplate.replace("{{URL}}", this.config.contents.link.url).replace("{{BUTTON_TEXT}}", this.config.contents.link.text);
            }
            let html = _this.boxTemplate.replace("{{HEADLINE}}", this.config.contents.headline).replace("{{CONTENT}}", this.config.contents.content).replace("{{LINK}}", link);
            $(".category-nav").before($(html));
        }
    }
});

let RemoteAlerts = function(config) {
    this.config = config;
    this.start = new Date().getTime();
    this.ck = Math.floor(this.start / 600000)

    if (document.location.href.indexOf("cachekiller=true") > 0) {
        this.ck = this.start;
    }

    let _this = this;

    this.init = function() {
        console.log(typeof $);
        this.getJQuery(_this.checkAlerts);
    };

    this.checkAlerts = function() {
        console.log("check alerts");
        console.log(new Date().getTime() - _this.start);
        $.get(_this.config.contentApiUrl, {
            "clientId": "MESSAGES",
            "_ck": _this.ck
        }, function() {}, "json").done(function(data) {
            console.log("get alert data: ", data);
            _this.createAlerts(data);
        }).fail(function(error) {
            console.log("get alert error:", error);
        });
    }

    this.createAlerts = function(data) {
        console.log("create alerts...");

        let contents = {
            headline: "+++ HEADLINE +++",
            content: "Lorem ipsum..."
        };

        data.incidents.forEach((incident) => {
            console.log(incident);

            $.when(
                $.get(incident.links[0].href, {
                    "clientId": "MESSAGES",
                    "_ck": _this.ck
                }, function() {}, "json").done(function(data) {
                    console.log("get notification data: ", data);
                    let notification = data.notification;
                    console.log("notification: ", notification)

                    contents.headline = notification.notificationHeadline;
                    contents.content = notification.notificationText;

                    if (notification.notificationLinkURL) {
                        contents.link = {
                            url: notification.notificationLinkURL,
                            text: notification.notificationLinkText,
                            style: "brand"
                        };

                    }
                }).fail(function(error) {
                    console.log("get alert error:", error);
                })
            ).done(function() {
                let notfallbox = new Notfallbox({
                    contents: contents
                });

                notfallbox.start();
            });

        });
    }

    this.getJQuery = function(callback) {
        if (typeof $ !== "function") {
            console.log("...");
            setTimeout(function() { _this.getJQuery(callback); }, 100);
        } else {
            console.log("call callback");
            callback.call();
        }
    }

    this.init();
}

let remoteAlerts = new RemoteAlerts({
    contentApiUrl: "https://www.telekom.de/content-api/hilfe/channel/589012",
    insertSelector: function() {

    }
});

