<script type="text/javascript" src="/js/jquery-3.6.0.min.js"></script>
<script type="text/javascript" src="/plupload-2.1.2/js/plupload.full.min.js"></script>
<script type="text/javascript">
if(typeof String.prototype.trim !== 'function') {
	String.prototype.trim = function() {
		return this.replace(/^\s+|\s+$/g, '');
	}
}

function getCookie(cname) {
	var name = cname + "=";
	var ca = document.cookie.split(';');
	for(var i = 0; i < ca.length; i++) {
		var c = ca[i].trim();
		if (c.indexOf(name) == 0) {
			return c.substring(name.length,c.length);
		}
	}
	return "";
}

(function() {
	$('#advanced_pdf_file_uploader').show();
	$('#pdf_file_uploader').hide();

	var uploader = new plupload.Uploader({
        runtimes : 'html5,html4,flash,silverlight',

        browse_button : 'advanced_pdf_file',
        container: 'advanced_pdf_file_uploader',

        url : "/upload_pdf.php",

        multipart: true,
        chunk_size : '1mb',
        rename : true,
        dragdrop: true,
        unique_names : false,
        multipart_params : {"PHPSESSID" : getCookie('PHPSESSID')},
        multi_selection: false,

        filters : {
            max_file_size : '25mb',
            mime_types: [
                {title : "PDF file", extensions : "pdf"}
            ]
        },

        flash_swf_url : '/plupload-2.1.2/js/Moxie.swf',
        silverlight_xap_url : '/plupload-2.1.2/js/Moxie.xap',

        preinit : {
            Init: function(up, info) {
            	$('#convert_pdf_to_jpg_button').attr('disabled', 'disabled');
            	$('#convert_pdf_to_jpg_button').removeAttr('onclick');
            	$('#convert_pdf_to_jpg_button').click(function() {
            		if (up.files.length > 0) {
	            		$('#advanced_pdf_file_uploader').hide();
	            		$('#pdf_progress_bar_container').show();
	            		$('#convert_pdf_to_jpg_button').attr('disabled', 'disabled');
	            		uploader.start();
	            	}
	            	else {
	            		alert('Please select a PDF file');
	            	}
            		return false;
            	});
            },
        },

        init : {
            Error: function(up, args) {
            	alert("An error occured: " + args.message);
            },

	        FilesAdded: function(up, files) {
	        	var filesToRemove = Array();
	        	plupload.each(up.files, function(existingFile) {
	        		var found = false;
	            	plupload.each(files, function(newFile) {
	            		if (existingFile.id == newFile.id) {
	            			found = true;
	            		}
	            	});

	            	if (! found) {
	            		filesToRemove.push(existingFile);
	            	}
	        	});

	        	plupload.each(filesToRemove, function(file) {
	        		up.removeFile(file);
	        	});

	            plupload.each(files, function(file) {
	            	$('#advanced_pdf_file_message').html(file.name);
            		$('#convert_pdf_to_jpg_button').removeAttr('disabled');
								$('#convert_pdf_to_jpg_button').addClass('btn-primary');
								$('#advanced_pdf_file').removeClass('btn-primary');
								$('#advanced_pdf_file').addClass('btn-outline-primary');
	            });
	        },

            UploadProgress: function(up, file) {
            	$('#pdf_progress').attr('aria-valuenow', parseInt(file.percent));
            	$('#pdf_progress').css('width', parseInt(file.percent) + '%');
            	$('#pdf_progress_caption').html(file.percent + '%');
            },

            FileUploaded: function(up, file, info) {
            	window.location.href = '/start_pdf_to_jpg_conversion.php?quality=' + $('#quality').val();
            }
        }
    });

	uploader.init();
})();
</script>

