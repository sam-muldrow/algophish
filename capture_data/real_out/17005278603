      (function folAd(id, x, y) {
        x = id ? "banner-970x250-area-" + id  : "banner-970x250-area";
        y = document.getElementById(x);
        if (y) { return folAd(id ? (id+1) : 2); }

        var banner = document.getElementById("banner-970x250-1625453968792575");
        if (banner) banner.setAttribute('id', x);
      })();
    </script>
  </div>
</div>


