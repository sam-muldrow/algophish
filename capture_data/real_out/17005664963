        const ls = JSON.parse(localStorage.getItem('recent-search-context'))?.["uk"];
        if (ls) {
          document.addEventListener("DOMContentLoaded", function(event) {
            const searchBar = document.querySelector('sb-bar-connector') || document.querySelector('tct-search-panel');
            searchBar.context = {
              market: "uk",
              locale: "en-GB",
              destination: ls.destination,
              airport: ls.airport,
