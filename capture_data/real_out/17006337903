<div class='cookie_disclaimer' style="display: none">
            <div class='cookie_container'>
                <div class='cookie_text'>We use cookies for functional, analytical, and advertising purposes.<br/>Review our <a rel='noreferrer noopener' href='https://www.usafis.org/privacy-policy/' target='_blank' >Privacy Policy</a> to learn more, including how to adjust your cookie settings.</div>
                <div class='button_container'>
                <button id='disclaimerDismiss'>Accept</button>
                <button id='adjustCookie'><a rel='noreferrer noopener' href='https://www.usafis.org/privacy-policy/' target='_blank'>Adjust cookie settings</a></button>
                </div>
            </div>
</div>


<script>
   try {
            document.querySelector('#disclaimerDismiss').addEventListener('click', () => {
                document.querySelector('.cookie_disclaimer').style.display='none';
    
                localStorage.setItem('seenCookie', true);
            });
            const seenCookie = JSON.parse(localStorage.getItem('seenCookie'));
            if(seenCookie !== true) document.querySelector('.cookie_disclaimer').style.display='block';
        } catch (error) {
            
        }
</script>


