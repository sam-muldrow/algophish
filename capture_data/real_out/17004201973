    $(function () {
        var currentScrollPosition;

        $('[data-hamburger]').click(function () {
            currentScrollPosition = $(window).scrollTop();
            $(this).addClass('u-hidden');
            $('body').addClass('has-navigation-open');
            $('html, body').height($('.c-navigation__list').height() +  $('.c-navigation__header').height());
            window.scrollTo(0, 0);
        });

        $('[data-navigation-header-close]').click(function () {
            $('[data-hamburger]').removeClass('u-hidden');
            $('body').removeClass('has-navigation-open');
            $('html, body').height('');
            window.scrollTo(0, currentScrollPosition);
        });
    });
</script></header><ul class="c-navigation__list c-navigation__list--level-0"><li class="c-navigation__item "><a         href="/actualite-economique/"
