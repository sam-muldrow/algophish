    (function() {
        try {
            BOOMR.init({
                beacon_url: "/bucky/v1/send/",
                log: null,
                BW: {
                    base_url: "/bandwidth-test-images/",
                    block_beacon: true,
                    test_https: true
                }
            });

            BOOMR.subscribe('before_beacon', function (eventData) {

                var metricsToKeep = [
                    'pt.fp',
                    'pt.fcp',
                    'mem.total',
                    'mem.limit',
                    'mem.used',
                    't_resp',
                    't_page',
                    't_done'
                ];

                var keys = Object.keys(eventData),
                    dataLength = keys.length;

                for (var j = 0; j < dataLength; j++) {

                    var newKey = 'www-boursorama-com.homepage.' + keys[j],
                        oldKey = keys[j];

                    if (metricsToKeep.indexOf(oldKey) !== -1) {
                        Object.defineProperty(eventData, newKey, Object.getOwnPropertyDescriptor(eventData, oldKey));
                        eventData[newKey] += '|ms';
                    }
                    delete eventData[oldKey];
                }
            })

        } catch(e) {
            // BOOMR not available
        }

    })();
