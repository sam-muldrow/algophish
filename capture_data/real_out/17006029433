<script type="text/javascript">
  mnewsApp.controller('background', function ($scope,$element,$http,$document) {
	   angular.element($document).ready(function () {
		    var pageType =  "Home";
			var backImage = "";
			var enabelArticleBackImage = "false";
			var backColor = "";
			var enabelArticleBackColor = "false";
			var domElem = document.getElementById("commonwrap");			
			if(backImage) {
				if(pageType=='Article') {
					if(enabelArticleBackImage=='true') {
						domElem.style.backgroundImage = "url("+backImage+")";
						domElem.style.backgroundRepeat = "no-repeat";
					}
				} else {
					domElem.style.backgroundImage = "url("+backImage+")";
					domElem.style.backgroundRepeat = "no-repeat";
				}
			}
		
			if(backColor) {
				if(pageType=='Article') {
					if(enabelArticleBackColor=='true') {
						domElem.style.backgroundColor= backColor;
					}
				} else {
					domElem.style.backgroundColor= backColor;
				}
			}
	    });
  });

</script>


