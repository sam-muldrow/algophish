<footer class="footer footer-bg authorfix" id="footerElement"><!-- ADD bg -->	
	
<div></div>   
    
<script>
$( document ).ready(function() {
   	var domain = 'mnews';
	$.getJSON("/mnews/breakingnews?domain=" + domain, function(data){
		var op = ''; 
		if(data.enabled == "true"){
			op += '<div class="breaking-news-footer-block bg-red">';
			op += '<div class="mm-container breaking-news-inside">';
			op += '<div class="breaking-news-title bg-red">'+data.title+'</div>';
			op += '<div class="breaking-news-blk"><ul>';
			$.each(data.articleDetails, function(i, val){
				var con = (val.url != '') ? '<li><a href="'+val.url+'" target="'+val.target+'">'+val.title+'</a></li>' : '<li>'+val.title+'</li>' ; 
				op += con;
			});
			op += '</ul></div>';
			op += '<div class="breaking-news-close bg-red"></div>'
			op += '</div></div></div>';
			$('#bknews').html(op);
            utils.bNews();
			
		}else{
			$('#bknews').empty(); 
		}
		        
	});

	});	
	
	</script>
