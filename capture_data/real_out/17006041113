$(".page-id-13 .leadsource,.page-id-10170 .leadsource,.page-id-10226 .leadsource,.page-id-10215 .leadsource,.page-id-10200 .leadsource,.page-id-10264 .leadsource,.page-id-10265 .leadsource,.page-id-10266 .leadsource").val("iManila Web Inquiry - DM");
