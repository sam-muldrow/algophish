  $('.uk-background-norepeat.uk-background-cover.uk-background-center-center.uk-section.uk-padding-remove-bottom').each(function(){
    if(isScrolledIntoView($(this))){
      $('#lets-work').addClass( "viewed" );
      $('#lets-work').removeClass("not" );
    }
  });
});

      
      
function isScrolledIntoView(elem){
    var $elem = $(elem);
    var $window = $(window);

    var docViewTop = $window.scrollTop();
    var docViewBottom = docViewTop + $window.height();

    var elemTop = $elem.offset().top;
    var elemBottom = elemTop + $elem.height();

    return ((elemBottom <= docViewBottom) && (elemTop >= docViewTop));
}
      
$( document ).ready(function() {
    cookie();
});
$("#cn-accept-cookie").click(function() {
    cookieHide();
});
      
	 function cookie(){
		var myEle = document.getElementById("cookie-notice");
		var navBar = document.getElementsByClassName("uk-navbar-container");
		if(myEle.style.display != 'none'){
			/*alert('test');*/
			navBar[1].style.margin = "60px 0 0 0";
		}else{
			navBar[1].style.margin = "0";
		};
	};
      
	 function cookieHide(){
		var navBar = document.getElementsByClassName("uk-navbar-container");
		navBar[1].style.margin = "0";
	};
  });
