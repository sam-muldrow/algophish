    <script>
        function acceptCookies() {
            var expiryDate = new Date();
            expiryDate.setFullYear(expiryDate.getFullYear() + 1);
            document.cookie = "cookieconsent=1; expires =" + expiryDate.toUTCString() + ";";
            $('#cookieModal').removeClass('show');
        }
    </script>

