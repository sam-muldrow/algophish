        )(`esports-hub/csgo/pinnacle-cup`),

        [`${langRegex}\/RU-CSGO-Odds$(\/)?`]: decorate(
          'ru',
          '?utm_source=RU_twitch_tv&utm_medium=live_stream&utm_campaign=22_q2_pinnaclecup_csgo&utm_content=ru_chatbot_odds&btag=pinn_ru_chatbot_odds&refer=Q2PinnacleCup22CSGOspshp&fwd_search=1&bounce=1'
        )(`esports-hub/csgo/pinnacle-cup`),

        // landing pages -------------

        // !!!! README !!!!
        // the following append a &bounce=1 param which forces a hard redirect
        // as these urls are external

        [`${langRegex}\/Welcome$(\/)?`]: decorate(
          'en',
