        )(`esports-hub/csgo/pinnacle-cup`),

        [`${langRegex}\/CSGO-Probabilidades$(\/)?`]: decorate(
          'pt',
          '?dvc=d&utm_source=br_twitch_tv&utm_medium=live_stream&utm_campaign=23_pc_csgo&utm_content=br_chatbot_odds&btag=pinn_br_chatbot_odds&refer=23pinnaclecupcsgospshp&fwd_search=1&bounce=1'
