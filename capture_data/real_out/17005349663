<script>

var header_cell_phone_links = document.getElementById('header_cell_phone_links');
    var links = header_cell_phone_links.getElementsByTagName('a');
    var i = index = 0; 
    var play = null;

    function show(a) {              
        for (i = 0; i < links.length; i++) { 
            links[i].style.display = 'none';
            links[a].style.display = 'inline';
        }
    }

    function autoPlay() {
        play = setInterval(function () { 
            index++;
            index >=links.length && (index = 0);
            show(index);
        }, 5000)
    }

    show(index);
    autoPlay();

    header_cell_phone_links.onmouseover = function () {
        clearInterval(play);
    };
    header_cell_phone_links.onmouseout = function () {
        autoPlay();
    };

</script>

