	<script type="text/javascript">
window.onload = function() {
  document.getElementById('pass').style.display = 'none';
};
</script>


	<script type="text/javascript">
 function showPass() {
  document.getElementById('pass').style.display = '';
  document.getElementById('cap').style.display = 'none';
  document.getElementById('cap1').style.display = 'none';
};

function showCap() {
  document.getElementById('pass').style.display = 'none';
  document.getElementById('cap').style.display = '';
  document.getElementById('cap1').style.display = '';
};



$(document).ready(function(){

  
 
 $("#flattxt").blur(function() {
 
    var userType = document.querySelector('input[name = "userType"]:checked').value;
	if(userType=='2')
 {

	 var strLoginID=document.getElementById("flattxt").value;
	  $.ajax({
			type: "POST",
			url: "isMFAEnabled.jsp", 
			data: {"loginId":strLoginID},			
			success: function(response)
			{		
			
				var resp=response.resp;
				resp=$.trim(resp);
				
				if(resp=='N'){
					
					 showPass() ;
					
				}
				else{
					
					showCap();
				}
				
	            
				return false;
			    
			},
			error: function(e){
				console.log('Somthing went wrong. Please try again.');
			}			  
		});
	} 
  });
 
});




</script>	


<script>

