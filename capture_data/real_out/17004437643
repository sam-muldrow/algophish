<script type="text/javascript">
    $(document).ready(function()
    {
        if( $.cookie('promo') && $.cookie('aff_promo_close') != 1 )
        {
                $("#aff-promo").show();
        }

        $('.aff-promo-close, .aff-promo-button').click(function() 
        {
            $.cookie('aff_promo_close', 1, { expires: 1, path: '/', domain: '.caribbeancom.com' });
            $('#aff-promo').hide();
        });
    });
</script>

	
