			// nacteni scriptu
			var loadScript = function(src, cb, crossorigin) {
				var script = document.createElement("script");
				script.src = src;
				script.type = "text/javascript";
				script.async = true;
				script.onload = function() {
					cb && cb();
				};
				if (crossorigin) {
					script.setAttribute("crossorigin", "");
				}
				document.head.appendChild(script);
			};

		</script>
		
