// pre defined func
function LJ(scriptUrl, async = true, callback) {
  var script = document.createElement('script');
  script.type = 'text/javascript';
  script.src = scriptUrl;
  if (async) { script.async = true; }
  script.onload = callback; // Optional callback function to execute when the script is loaded
  document.head.appendChild(script);
}


//1. Anura invalid traffic filter
var keyEx = [];
var NoPassAN = "N";
var GoodSource = "Unknown"; // invalid traffic filtering result
var gpt_js_load = false;
var prebid_js_load = false;
(function () {
    var anura = document.createElement('script');
    if ('object' === typeof anura) {
        var request = {
            instance: 2238638024,
            source: 'techtimes',
            campaign: '',
            exid: 'anura_techtimes_' + Math.floor(1E12 * (Math.random() + 1)),
            callback: '_anuraResFun'
        };
        var params = [];
        for (var x in request) params.push(x + '=' + encodeURIComponent(request[x]));
        params.push(Math.floor(1E12 * Math.random() + 1));
        anura.type = 'text/javascript';
        anura.async = true;
        anura.src = 'https://script.anura.io/request.js?' + params.join('&');
        var script = document.getElementsByTagName('script')[0];
        script.parentNode.insertBefore(anura, script);
    }
})();

function _anuraResFun(response) {
    if (response.getId() || response.getExId()) {
        //console.log("Anura exid: "+response.getExId());
        //getResult(response);
        _anuObj = response.getObject();
        console.log("Anura : result - " + _anuObj.result);
        if (_anuObj.result == "bad") {
            keyEx.push("|ple| NoPassAN");
            NoPassAN = "Y";
            GoodSource = "Bad";
        } else {
            var this_site = window.top.location.hostname.split(".");
            this_site = this_site[this_site.length - 2];
            
        }
        if (_anuObj.result == "good") {
            GoodSource = "Yes";
        }
    } else {
        console.log("Anura : Error");
        console.log("Anura : exid: " + response.getExId());
    }
}

// 2. prebid CMP
var _geo = ''; // defined by php
if(_geo=='EU') {
    var consentPrebid = {
        gdpr: {
            cmpApi: 'iab',
            timeout: 8000,  // isn't this too long?
            defaultGdprScope: true
        }
    }
} else if(_geo=='US') {
    var consentPrebid = {
        usp: {
            cmpApi: 'iab', 
            timeout: 8000 // isn't this too long?
        }
    }
}

// 3. prebid
// 3-1. load gpt.js
LJ('https://www.googletagservices.com/tag/js/gpt.js', false, function() {
	console.log('ad log - gpt.js is loaded');
});

// 3-3. gam
if(GoodSource!='Bad') {
    var adslot = '/21697271410/techtimes';
    var _device = '';

    // ------- GAM -------
    if(_device=='mobile') {
        var gpt_top_sizes = [
            [300, 250],
            [336, 280]
        ];
        
        var gpt_right1_sizes = [
                [300, 250],
                [300, 600]
            ];
            var gpt_right2_sizes = [
                [300, 250],
                [300, 600]
            ];
            var gpt_right3_sizes = [
                [300, 250],
                [300, 600]
            ];
            var gpt_right4_sizes = [
                [300, 250],
                [300, 600]
            ];        
        var gpt_dynamic_vsizes = [
            [300, 250],
            [336, 280]
        ];
        var gpt_dynamic_hsizes = [
            [300, 250],
            [336, 280]
        ];
    } else {
        var gpt_top_sizes = [
            [728, 90]
        ];
        var gpt_right1_sizes = [
                [300, 250],
                [300, 600]
            ];
            var gpt_right2_sizes = [
                [300, 250],
                [300, 600]
            ];
            var gpt_right3_sizes = [
                [300, 250],
                [300, 600]
            ];
            var gpt_right4_sizes = [
                [300, 250],
                [300, 600]
            ];        
        var gpt_bottom_sizes = [
            [728, 90],
            [970, 250]
        ];
        var gpt_dynamic_vsizes = [
            [300, 250]
        ]; 
        var gpt_dynamic_hsizes = [
            [728, 90],
            [970, 250]
        ]; 
    }

    var googletag = googletag || {};
    googletag.cmd = googletag.cmd || [];
    
	googletag.cmd.push(function() {
		googletag.defineSlot(adslot, gpt_top_sizes, 'gpt_top').addService(googletag.pubads()).setTargeting("pos", 'top');
		googletag.defineSlot(adslot, gpt_right1_sizes, 'gpt_right1').addService(googletag.pubads()).setTargeting("pos", 'right1');

		googletag.pubads().disableInitialLoad();
		googletag.enableServices();
	});  
    // -------// GAM -------

    // ------- amazon apstag -------
	var aps_slots = [{
		slotID: 'gpt_top',
		slotName: adslot,
		sizes: gpt_top_sizes
	},
	{
		slotID: 'gpt_right1',
		slotName: adslot,
		sizes: gpt_right1_sizes
	},
	{
		slotID: 'gpt_right2',
		slotName: adslot,
		sizes: gpt_right2_sizes
	},
	{
		slotID: 'gpt_right3',
		slotName: adslot,
		sizes: gpt_right3_sizes
	},
	{
		slotID: 'gpt_right4',
		slotName: adslot,
		sizes: gpt_right4_sizes
	},
	{
		slotID: 'gpt_bottom',
		slotName: adslot,
		sizes: gpt_bottom_sizes
	}];
	
    //Load the APS JavaScript Library
    !function(a9,a,p,s,t,A,g){if(a[a9])return;function q(c,r){a[a9]._Q.push([c,r])}a[a9]={init:function(){q("i",arguments)},fetchBids:function(){q("f",arguments)},setDisplayBids:function(){},targetingKeys:function(){return[]},_Q:[]};A=p.createElement(s);A.async=!0;A.src=t;g=p.getElementsByTagName(s)[0];g.parentNode.insertBefore(A,g)}("apstag",window,document,"script","//c.amazon-adsystem.com/aax2/apstag.js");

    //Initialize the Library
    apstag.init({
        pubID: '10f2e1c5-d495-4ec1-a8e3-2556d5c64ce9',
        adServer: 'googletag',
        deals: true
    });

    apstag.fetchBids({
        slots: aps_slots,
        timeout: 2e3
    }, function(bids) {
        googletag.cmd.push(function(){
            apstag.setDisplayBids();
            googletag.pubads().refresh();
        });
    });
    // -------// amazon apstag -------

    /**
     * @param: string; position - right1, right2
     * @param: string; div id value
     * @param: string; ad shape (h:horizontal, v:vertial)
     */
	function _lazyAdLoad(pos, _div_id, _ad_shape) {
        
		if(_div_id!=undefined) {	// gpt tag
            var _new_obj;
			googletag.cmd.push(function() {
                console.log('_ad_shape : '+_ad_shape);
                let gpt_dynamic_sizes = {};
                if(_ad_shape!=undefined && _ad_shape=='h') {
                    gpt_dynamic_sizes = gpt_dynamic_hsizes;
                } else {
                    gpt_dynamic_sizes = gpt_dynamic_vsizes;
                }
				_new_obj = googletag.defineSlot(adslot+'/'+pos, gpt_dynamic_sizes, _div_id).addService(googletag.pubads()).setTargeting("pos", pos).setTargeting("refresh", "L");
				googletag.display(_div_id);

                let _aps_slots = [{
                    slotID: 'gpt_dynamic',
                    slotName: adslot+'/'+pos,
                    sizes: gpt_dynamic_sizes
                }];
                
                apstag.fetchBids({slots: _aps_slots, timeout: 2e3}, function(bids) {
                    apstag.setDisplayBids();
                    googletag.pubads().refresh([_new_obj]);
                });
			});
		}
	}
}

//------- lazyAdLoading -------
let debounceTimer;
const debounce = (callback, time) => {
  window.clearTimeout(debounceTimer);
  debounceTimer = window.setTimeout(callback, time);
};

var _used_ad_pos = ['top', 'right1'];

function _measureScrollTopForAll() {
    var _scrollTop = window.scrollY;
    var targetDivs = document.querySelectorAll("._lazy_ad_load");
    
    let _gap = 1200; //px

    debounce(() => {
        let cnt = 0;
        targetDivs.forEach(function(targetDiv) {
            let _offsetTop = targetDiv.offsetTop;
            // console.log("Scroll Top: " + _scrollTop+" ::: ScrollTop "+cnt+' = target div: ' + _offsetTop);

            if(_offsetTop-_scrollTop<=_gap) {
                let _curr_ad_id = targetDiv.getAttribute('id');
                let _ad_shape = targetDiv.getAttribute('attr');
                let _used = _used_ad_pos.includes(_curr_ad_id);
                if(_curr_ad_id!=undefined && _curr_ad_id!='' && _used==false) {
                    // console.log('lazy ad call : '+_curr_ad_id);
                    _lazyAdLoad('right2', _curr_ad_id, _ad_shape);
                    _used_ad_pos.push(_curr_ad_id);
                }
            }
            cnt++;
        });
    }, 200);
}

window.addEventListener("scroll", _measureScrollTopForAll);
//-------// lazyAdLoading -------
