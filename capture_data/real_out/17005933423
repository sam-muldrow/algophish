<script>
    var vimeoPlaylistStyles = document.getElementById("vimeo-playlist-styles");
    if (!vimeoPlaylistStyles) {
        document.body.insertAdjacentHTML('afterend', '<div class="modal micromodal-slide" id="modal-1" aria-hidden="true" aria-modal="true"> <div class="modal__overlay" tabindex="-1" data-micromodal-close="true"> <button data-micromodal-close="true" class="close-button" aria-label="Close modal" type="button"><span aria-hidden="true">&times;</span></button> <div class="modal__container" role="dialog" aria-modal="true" aria-labelledby="modal-1-title"> <div class="player"> <div class="vimeo-video-loader"> <span class="loader"></span> </div> </div> </div> </div> </div>');
        function loadFile(FILE_URL, async = true, id, type) {
            let el = ''
            if (type === "js") {
                el = document.createElement("script");
                el.setAttribute("src", FILE_URL);
                el.setAttribute("async", async);
                el.setAttribute("type", "text/javascript");
                document.body.appendChild(el);
            }
            if (type === "css") {
                el = document.createElement("link");
                el.setAttribute("href", FILE_URL);
                el.setAttribute("rel", "stylesheet");
                el.setAttribute("type", "text/css");
                document.getElementsByTagName('HEAD')[0].appendChild(el);
            }
            el.setAttribute("id", id);
            // success event 
            el.addEventListener("load", () => {
                //console.log("File loaded")
            });
            // error event
            el.addEventListener("error", (ev) => {
                console.log("Error on loading file", ev);
            });
        }
		
		loadFile("https://www.sjsu.edu/_resources/css/swiper-bundle.min.css", true, "vimeo-playlist-styles", "css");
		loadFile("https://www.sjsu.edu/_resources/css/sjsu-vimeo-playlist.css?v=1.1", true, "swiper-bundle", "css");
        loadFile("https://www.sjsu.edu/_resources/css/micromodal.css", true, "swiper-bundle", "css");		
		loadFile("https://player.vimeo.com/api/player.js", true, "vimeo-scripts", "js");
		loadFile("https://www.sjsu.edu/_resources/js/vendor/swiper-bundle.min.js", true, "vimeo-playlist-scripts", "js");
		loadFile("https://www.sjsu.edu/_resources/js/vendor/sjsu-vimeo-playlist.js?v=1.2", true, "vimeo-playlist-styles", "js");
        loadFile("https://www.sjsu.edu/_resources/js/vendor/micromodal.min.js", true, "micromodal", "js");		


    }
</script>
</div>	





