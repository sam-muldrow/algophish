	function add_1cak_comment(comment_post_id){
	     var comment_txt=$('#comment_txt_'+comment_post_id).val();
		 var comment_reply_id=$('#comment_reply_id_'+comment_post_id).val();
		 var comment_reply_parent_id=$('#comment_reply_parent_id_'+comment_post_id).val();
                 //alert(comment_txt);
		  

		if(comment_txt!='add your comment here..'){
                 
		     $.post('https://cdn16.1cak.com/1cak_comment.php?post_id='+comment_post_id+'&user_id='+user_id_comment+'&comment_reply_id='+comment_reply_id+'&comment_reply_parent_id='+comment_reply_parent_id+'&user_name='+user_name_comment+'&hash='+hash_comment+'&comment='+encodeURIComponent(comment_txt)+'&act=add_comment',function(data){
			      
				  if(data=='comment_short'){
				      alert('comment too short..');
				  }else if(data=='wait_for_it'){ 
				      alert('Please wait atleast one minute before your last comment');
				  }else{
                                      if(comment_reply_parent_id>0){
				        $('#replies_div_'+comment_reply_parent_id).append(data);
				      }else{
				       $('#new_comment_div_'+comment_post_id).append(data);
				      }
          			  }
				  
				  $('#comment_txt_'+comment_post_id).val('add your comment here..');
				  $('#comment_button_'+comment_post_id).html('Send');
				  document.getElementById('comment_txt_'+comment_post_id).style.color='#ddd';
				  
			 });
		 }
	}


