  <script>
  
	
	
	function setSave(saveNum){
	   $.get("/recent&setSave="+saveNum,function(data){
	      if(data=="ok"){
		     window.location.reload();
		  }else if(data=="login"){
		     window.location="/login";
		  }
	   
	   });
	}
        
         function setDark(saveNum){
           $.get("/recent&setDark="+saveNum,function(data){
                  if(data=="ok"){
                     window.location.reload();
                  }

           });
        }




