<script>
rightPositioned=false;
likeTop=$("#like_div").offset().top;
likeTop-=30;



$(window).scroll(function(){
    scrolT=$(window).scrollTop();
	
	
	if(scrolT>likeTop && !rightPositioned){
	  $("#rightnavigation").css("position","fixed");
	  likeTopX=likeTop-65;
	  $("#rightnavigation").css("top","-"+likeTopX+"px");
      rightPositioned=true;
	  
	}
	
	if(scrolT<likeTop){ 
	   $("#rightnavigation").css("position","relative");
	  
	  $("#rightnavigation").css("top","50px");
      rightPositioned=false;
	
	}
 
 });
 
 

</script>



