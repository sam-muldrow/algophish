         });

      }
		

        
	
	
	
	
	
	
	
	
	
	function showComment(pid){
	
            //window.open('/'+pid+'#c','_new'+pid);
	    $('html,body').animate({
		   scrollTop:$('#comment_div_'+pid).offset().top-250
		}, 200);
	
	    var domain='1cuk.com';
		if(pid>=23704){
		   domain='1cak.com';
		}

	
	    $('#comment_div_'+pid).html("<div id='comment_1cak_"+pid+"' style='max-width:470px;'></div><div style='margin:5px;'><h3 style='text-align:left;font-size:14px'>FACEBOOK Comments</h3></div><div style='background:#aaa'><fb:comments href='http://"+domain+"/"+pid+"'   num-posts='15'  width='450'></fb:comments></div>");
             
		
        $('#comment_1cak_'+pid).load('https://cdn16.1cak.com/1cak_comment.php?dark_mode=0&act=view_comments&post_id='+pid+'&hash='+hash_comment+'&user_name='+user_name_comment+'&user_id='+user_id_comment,function(){		
           $('.emoji_bar').click(function(){
	         var tmp_comment=$(this).parent().parent().prev().prev().val();
			 
			 if(tmp_comment=='add your comment here..'){
			   $(this).parent().parent().prev().prev().val($(this).html());
			 }else{
			    $(this).parent().parent().prev().prev().val(tmp_comment+$(this).html());
			 }
	
	        
			 return false;
	    });

         });
     	 
			 

		FB.XFBML.parse(document.getElementById('comment_div_'+pid));
		
		
	    
	}




         function createSubscribe(){
	 
	    //alert('creating subscribe');
	    FB.Event.subscribe('comment.create',
		function(response) {
			
			console.log(response);
			myPostId=response.href.replace('http://1cak.com/','');
                        myPostId=myPostId.replace('http://1cuk.com/','');
                        //console.log(myPostId);
						
						
						 FB.getLoginStatus(function(response3) {
						  console.log(response3);
						  if (response3.status!='not_authorized') {
							
							          FB.api('/'+response.commentID, function(response2) {
				
											console.log(response2);	
											comment_from=response2.from.name;				
								
										    $.post('/actlib&act=add.comment',{note_from:comment_from,post_id:myPostId},function(data){
										   //alert(data);
							 
										    });
									  }); 
							
							
							
							
							
						  } else {
						  
										comment_from='Someone ';
									$.post('/actlib&act=add.comment',{note_from:comment_from,post_id:myPostId},function(data){
										   //alert(data);
							 
										    });
						  
						  
						  
							
						  } 
						 });
						
						
			
                      

                        
			
		  }
		);
		
		
	 
	 
	 }
	 
	 setTimeout("createSubscribe()",5000);





	
	//populate posts array..
	
	var postArray=new Array();
	var postArayId=new Array();
	var curPostArrayIdx=-1;
	postArrayIdx=-1;
	
	
	function seekKeyBoard(fw){
	    postArrayIdx=0;
		postArray=new Array();
		postArayId=new Array();
		$('.upperSpan').each(function(obj){
		   postOffset=$(this).offset().top;
		   //alert(postOffset);
		   myPostId=$(this).attr('id').replace('upper_span_','');
		   postArayId[postArrayIdx]=myPostId;
		   postArray[postArrayIdx]=postOffset*1-55;
		   postArrayIdx++;
		});
	
	if(fw){
	  if(curPostArrayIdx<postArrayIdx-1){
	     curPostArrayIdx++;
	     $('html,body').animate({
		   scrollTop:postArray[curPostArrayIdx]
		}, 10);
		//alert(postArray[curPostArrayIdx]);
		 
	  }else{
	    window.location=$('#next_page_link').attr('href');
	  }
	 }else{
        if(curPostArrayIdx>0){
		 curPostArrayIdx--;
	     $('html,body').animate({
		   scrollTop:postArray[curPostArrayIdx]
		}, 10);
		  //alert(postArray[curPostArrayIdx]);
		  
	    }
     }	 
	 
	 
	   
	}
	
	var keyboard_seek=true;
	
	$(document).ready(function(){
	
	    $(document).keyup(function(e){

      if(keyboard_seek){
		if(e.keyCode == 74) {
			seekKeyBoard(1);
		 }else  if(e.keyCode == 75) {
			seekKeyBoard(0);
		 }else  if(e.keyCode == 76) {
		    postArrayIdx=0;
	   }

	postArray=new Array();
		postArayId=new Array();
		$('.upperSpan').each(function(obj){
		   postOffset=$(this).offset().top;
		   //alert(postOffset);
		   myPostId=$(this).attr('id').replace('upper_span_','');
		   postArayId[postArrayIdx]=myPostId;
		   postArray[postArrayIdx]=postOffset*1-80;
		   postArrayIdx++;
		});
			
		    $('#voteButton_'+postArayId[curPostArrayIdx]).click();
			
		 }
		});
		
		 postArrayIdx=0;
		postArray=new Array();
		postArayId=new Array();
		$('.upperSpan').each(function(obj){
		   postOffset=$(this).offset().top;
		   //alert(postOffset);
		   myPostId=$(this).attr('id').replace('upper_span_','');
		   postArayId[postArrayIdx]=myPostId;
		   postArray[postArrayIdx]=postOffset*1-80;
		   postArrayIdx++;
		});
	
	});
	
	
