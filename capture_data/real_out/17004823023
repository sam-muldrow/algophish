<script>
    // Create Analytics page name from URL
    // (Use only if urlstructure matches the application)
    var prettyPathFromURL = function(fullUrl) {
        return fullUrl.toLowerCase().replace(/^.+'#'\/|\/$/g,'').replace(/\/(\:+)?/g,':').replace(/\-/g,' ');
    };

    // Updating page properties
    var updatePageProperties = function(pageName,dataLayer) {
        
        var currentPathSplit = pageName.split(':');
        dataLayer.pageName=pageName;
       
        // Set the page Category
        var category=currentPathSplit[0];
        // Update the dataLayer
        dataLayer.pageCategory = category;
        dataLayer.pageSubSection1 = currentPathSplit.length > 1 ? currentPathSplit[0] + ":" + currentPathSplit[1] : currentPathSplit[0]+"";
        dataLayer.pageSubSection2 = currentPathSplit.length > 2 ? currentPathSplit[0] + ":" + currentPathSplit[1] + ":" + currentPathSplit[2] : "";
        dataLayer.pageSubSection3 = currentPathSplit.length > 3 ? currentPathSplit[0] + ":" + currentPathSplit[1] + ":" + currentPathSplit[2] + ":" + currentPathSplit[3] : "";
        dataLayer.pageSubSection4 = currentPathSplit.length > 4 ? currentPathSplit[0] + ":" + currentPathSplit[1] + ":" + currentPathSplit[2] + ":" + currentPathSplit[3] + ":" + currentPathSplit[4]:"";
    };

    var urlPath= '/s/SB-COZA'+window.location.pathname;
    var urlPath = urlPath.replace(/s\/SB-COZA\//g,'');
    urlPath = urlPath.split('/');
    urlPath = urlPath.slice(2);
    var currentUrl = prettyPathFromURL(urlPath.join('/'));
    updatePageProperties(currentUrl,dataLayer);
    
 </script>


