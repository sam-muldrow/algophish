<script>
function which_browser() {
    var browser = '';
    if (navigator.userAgent.indexOf("Chrome") !== -1) {
        browser = 'Chrome';
    } else if (navigator.userAgent.indexOf("Firefox") !== -1) {
        browser = 'Firefox';
    } else if (navigator.userAgent.indexOf("Safari") !== -1 && navigator.userAgent.indexOf("Chrome") === -1 && navigator.userAgent.indexOf("Firefox") === -1) {
        browser = 'Safari';
    }
    return browser;
} 

function change_webp_to_jpg_png_jpeg_images() { 
    if(which_browser() !== 'Safari'){
        return;
    }
    var imgDefer = document.getElementsByTagName('img');
    for (var i = 0; i < imgDefer.length; i++) {
        var img_src = imgDefer[i].getAttribute('src');
        var img_data_src = imgDefer[i].getAttribute('data-src');
        var img_data_msrc = imgDefer[i].getAttribute('data-msrc');
        var img_srcset = imgDefer[i].getAttribute('srcset');
        var img_data_srcset = imgDefer[i].getAttribute('data-srcset');

        if (img_src !== null && img_src.search(".webp")>=1) { 
            if(img_src.search("ext=png")>=1){
                imgDefer[i].setAttribute('src', img_src.replace(/.webp/g, '.png'));
            }
            if(img_src.search("ext=jpg")>=1){
                imgDefer[i].setAttribute('src', img_src.replace(/.webp/g, '.jpg'));
            }
            if(img_src.search("ext=jpeg")>=1){
                imgDefer[i].setAttribute('src', img_src.replace(/.webp/g, '.jpeg'));
            }  
        }

        if (img_data_src !== null && img_data_src.search(".webp")>=1) { 
            if(img_data_src.search("ext=png")>=1){
                imgDefer[i].setAttribute('data-src', img_data_src.replace(/.webp/g, '.png'));
            }
            if(img_data_src.search("ext=jpg")>=1){
                imgDefer[i].setAttribute('data-src', img_data_src.replace(/.webp/g, '.jpg'));
            }
            if(img_data_src.search("ext=jpeg")>=1){
                imgDefer[i].setAttribute('data-src', img_data_src.replace(/.webp/g, '.jpeg'));
            }
        }

        if (img_data_msrc !== null && img_data_msrc.search(".webp")>=1) { 
            if(img_data_src.search("ext=png")>=1){
                imgDefer[i].setAttribute('data-msrc', img_data_msrc.replace(/.webp/g, '.png'));
            }
            if(img_data_src.search("ext=jpg")>=1){
                imgDefer[i].setAttribute('data-msrc', img_data_msrc.replace(/.webp/g, '.jpg'));
            }
            if(img_data_src.search("ext=jpeg")>=1){
                imgDefer[i].setAttribute('data-smrc', img_data_msrc.replace(/.webp/g, '.jpeg'));
            }
        }

        if (img_srcset !== null && img_srcset.search(".webp")>=1) { 
            if(img_srcset.search("ext=png")>=1){
                imgDefer[i].setAttribute('srcset', img_srcset.replace(/.webp/g, '.png'));
            }
            if(img_srcset.search("ext=jpg")>=1){
                imgDefer[i].setAttribute('srcset', img_srcset.replace(/.webp/g, '.jpg'));
            }
            if(img_srcset.search("ext=jpeg")>=1){
                imgDefer[i].setAttribute('srcset', img_srcset.replace(/.webp/g, '.jpeg'));
            }
        }
        if (img_data_srcset !== null && img_data_srcset.search(".webp")>=1) { 
            if(img_data_srcset.search("ext=png")>=1){  
                imgDefer[i].setAttribute('data-srcset', img_data_srcset.replace(/.webp/g, '.png'));
            }
            if(img_data_srcset.search("ext=jpg")>=1){  
                imgDefer[i].setAttribute('data-srcset', img_data_srcset.replace(/.webp/g, '.jpg'));
            }
            if(img_data_srcset.search("ext=jpeg")>=1){  
                imgDefer[i].setAttribute('data-srcset', img_data_srcset.replace(/.webp/g, '.jpeg'));
            }
        }
    }
}

change_webp_to_jpg_png_jpeg_images();

</script>
        <script>
        const getDeviceType = () => {
            const ua = navigator.userAgent;
            if (/(tablet|ipad|playbook|silk)|(android(?!.*mobi))/i.test(ua)) {
                return 'tablet';
            }
            if (/Mobile|iP(hone|od|ad)|Android|BlackBerry|IEMobile|Kindle|Silk-Accelerated|(hpw|web)OS|Opera M(obi|ini)/.test(ua)) {
                return 'mobile';
            }
            return 'desktop';
        };
    </script>

