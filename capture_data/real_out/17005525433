<script>
    load_srcs(['/user/js/gordon-app-v1700221766.js'], function() {
        let tabs = document.querySelectorAll('.tabForActive');
        let loadedStreams = {};

        function setStream(stream) {
            let streamElement = document.querySelector('.js-load-stream');
            stream = loadedStreams[stream];

            streamElement.innerHTML = '';

            for(let item in stream) {
                streamElement.append(fromHTML(stream[item]));
            }
        }

        if(tabs) {
            tabs.forEach(function(tab) {
                tab.addEventListener('click', function (e) {
                    e.preventDefault();

                    let loadName = tab.dataset.mode;

                    tabs.forEach(function(crt) {
                        crt.classList.remove('active');
                    });

                    tab.classList.add('active');

                    if(loadedStreams[loadName]) {
                        setStream(loadName);
                        return;
                    }
                    
                    ajax.get(
                        lang + '/actions/mainstream?mode=' + loadName,
                        {},
                        function(data) {
                            data = JSON.parse(data);

                            if(data.success) {
                                loadedStreams[loadName] = data.list;

                                setStream(loadName);
                                place_redactor_pens();
                            }
                        }
                    );
                })
            });
        }
    });
</script>
