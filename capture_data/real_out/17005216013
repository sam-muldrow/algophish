    function SetAdobeDefaults() {

        const defaultTags = {
            'main_category': 'Commercial',
            'sub_categories': [],
            'vertical': 'Sport',
            'sub_vertical': 'Betting',
            'tri_vertical': '',
            'country': '*GLOBAL*',
            'language': 'English'
        };
        // $pageTags are used to overwrite default tags for the current page
        const pageTags = {};
        const userInfo = {};

        var adobeTags = {
            ...defaultTags,
            ...pageTags,
            ...userInfo,
            globalVariableName: "tagPagina",
        };

        const containerArray = [
            {
                linkContainerName: 'footer',
                linkContainerSelectors: ['.Covers-CoversFooter'],
            }
        ]

        var siteConfig = {
            externalScript: "https://img.covers.com/scripts/analytics/custom-data-v1.js",
            globalVariableName: "adobeTags",
            pageTagLocalNames: [
                "language",
                "country",
