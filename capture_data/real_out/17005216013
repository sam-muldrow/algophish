        "@type" : "WebSite",
        "name" : "Covers",
        "alternateName" : "Covers.com",
        "url" : "https://www.covers.com" }
    </script>


    <script>
    
        // Script to initialize home page email forms
        function initHomePageSignup(wrapperId){
            hbspt.forms.create({
                region: "na1",
                portalId: "3462941",
                formId: "405ce351-051a-4637-b404-a216c19eb864",
                formInstanceId: wrapperId,
                target: wrapperId,
                onFormSubmit: function ($form) {
                    $(".covers-emailBrick-desc").hide();
                },
            });
        }

        // Initialize Hubspot forms (runs when hsforms library is loaded)
        function initHubspotForms(){
            if ($(window).width() > 600) {
                $("#hubspot").attr("id", "signup-desktop");
                initHomePageSignup("#signup-desktop");
            }
            else {
                $("#hubspot").attr("id", "signup-mobile");
                initHomePageSignup("#signup-mobile");
            }

        }
    </script>

    <!-- Hubspot forms libraery -->
    <script charset="utf-8" type="text/javascript" defer async src="//js.hsforms.net/forms/embed/v2.js" onload="initHubspotForms()"></script>
    <script type="text/javascript" defer onload="signupModal.initModalScript('https://www.covers.com');" src="https://www.covers.com/scripts/geomodal.js"></script>


</body>
</html>
