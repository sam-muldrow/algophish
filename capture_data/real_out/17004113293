ographie du jour"}},"promo_items":{"basic":{"owner":{"id":"lexpress"},"auth":{"1":"aca83703bf269266af3a625b178c755c1d9a0d1fcaffcbd98709752db8c2588f"},"caption":"Niveaux records de gaz à effet de serre en 2022","source":{"system":"photo center","name":"AFP","edit_url":"","source_type":"wires","source_id":"3aee510c89c92f8f200eccf056052c81a6929e9c","additional_properties":{"editor":"photo center"}},"type":"image","distributor":{"mode":"reference","reference_id":"d6288a38-b381-452e-b2bd-35ff0539ed0d"},"version":"0.10.9","url":"https://cloudfront-eu-central-1.images.arcpublishing.com/lexpress/I5APB5VFT5EZZMWPXJ2RLJQ7VI.jpg","licensable":false,"planning":{"internal_note":""},"alt_text":"Niveaux records de gaz à effet de serre en 2022","credits":{"affiliation":[{"name":"afp.com/Maxence D'AVERSA, Laurence SAUBADU","type":"author"}]},"subtitle":"Niveaux records de gaz à effet de serre en 2022","width":1532,"_id":"I5APB5VFT5EZZMWPXJ2RLJQ7VI","additional_properties":{"fullSizeResizeUrl":"/resizer/OESiXIfGBMw82KqWHlf4O7HhXYE=/arc-photo-lexpress/eu-central-1-prod/public/I5APB5VFT5EZZMWPXJ2RLJQ7VI.jpg","mime_type":"image/jpeg","proxyUrl":"/resizer/OESiXIfGBMw82KqWHlf4O7HhXYE=/arc-photo-lexpress/eu-central-1-prod/public/I5APB5VFT5EZZMWPXJ2RLJQ7VI.jpg","restricted":false,"galleries":[],"originalUrl":"https://cloudfront-eu-central-1.images.arcpublishing.com/lexpress/I5APB5VFT5EZZMWPXJ2RLJQ7VI.jpg","published":true,"resizeUrl":"/resizer/OESiXIfGBMw82KqWHlf4O7HhXYE=/arc-photo-lexpress/eu-central-1-prod/public/I5APB5VFT5EZZMWPXJ2RLJQ7VI.jpg","thumbnailResizeUrl":"/resizer/aN_8YhfaEbdPIRFYYJCFQUqY7B0=/300x0/arc-photo-lexpress/eu-central-1-prod/public/I5APB5VFT5EZZMWPXJ2RLJQ7VI.jpg","version":1},"created_date":"2023-11-15T11:39:02Z","last_updated_date":"2023-11-15T11:39:03Z","slug":"image-3aee510c89c92f8f200eccf056052c81a6929e9c","height":1620,"image_type":"photograph"}},"canonical_url":"/environnement/gaz-a-effet-de-serre-une-annee-record-en-2022-G5WOUBXJTFGSZIRXFPKHYYHW3Q/","distributo