                                        var MarkerContent = $('<div class="vjs-marker" style="left:' + leftMarker + '" data-time="' + timeMarker + '"data-position="' + i + '"><span class="markerImg"><img src="' + markers[i].imgpath + '"></span><span class="markertxt">' + markers[i].text + '</span></div>');

                                        var CarLiveContent = $('<div class="item dslide_' + timeMarker + '" data-position="' + i + '" data-time="' + timeMarker + '"><div class="cont_img_live_shop" onclick="goTotheMomentVideoShopping(' + timeMarker + ',' + i + ')"><img src="' + markers[i].imgpath + '" alt="' + markers[i].text + '"></div><h1><a href="' + markers[i].link + '" target="_blank">' + markers[i].text + '</a></h1><div class="cont_item_timer">' + convertTime(timeMarker) + '</div><div class="cont_item_link"><a href="' + markers[i].link + '" target="_blank"><i class="fa fa-external-link"></i></a></div></div>');

                                        MarkerContent.click(function() {
