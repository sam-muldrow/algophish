    function creaShoppingEmoltv(idVideo, pos, tit, img, tag, permalink){
        var TvArray = {};
        //COMIENZO
        $.ajax({
            type: "GET",
            url: NewsApi + 'emoltv/videos/' + idVideo,
            cache: true,
            dataType: "json",
            async: false
        }).done(
            function(data){
                if(data){
                    var shortData = '';
                    var htmlEmoltvENVIVO = '';
                    var imgFoto = '';
				
                    if(data.hasOwnProperty('_source')){
                        shortData = data._source;
                    } else {
                        shortData = data;
                    }
					
                    if(typeof shortData.files.portraitVideo !== 'undefined'){
                        Config_h5.portraitVideo = true;
                    }
				
                    TvArray.cat = shortData.category;
                    TvArray.sub = shortData.subcategory;
                    TvArray.idV = shortData.name;
				
                    var timerActivePos = {};
                    timerActivePos.pos = pos;
                    timerActivePos.name = shortData.name;
                    timerActivePos.alertActive = false;
                    timerActivePos.continueTv = false;
                    timerActivePos.timerOff = false;
                    timerActivePos.tabActivo = true;
                    timerActivePos.zoomActive = false;
                    timerActivePos.timeInit = 0;
                    timerActivePos.contPlay = 0;
                    timerActivePos.evetMark = {
                        contMark: 0,
                        stopMark : false,
                        totalTime : 0,
                        intervalTime : 0,
                        time : 0,
                        skip : false
                    };
                    timerActivePos.Tag = 'videoPrin_PORTADA_'+pos;
                    Config_h5.posActivas[Config_h5.posActivas.length] = timerActivePos;
					
                    if(shortData.video_type == ''){
                        shortData.video_type = 'mp4';
                    }
				
                    switch(shortData.video_type)
                    {
                        case "m3u8":
                            //Config_h5.envivoPlay = true;
                            //Config_h5.abort = false;
                            //imgFoto = Config_h5.imgVideo+shortData.files.poster;
                            imgFoto = "//imgtv.ecn.cl/"+shortData.files.poster;
                            
							
                            try{
                                if(IE.isTheBrowser){
									
                                }
                            }catch(error){
                                var IE ={isTheBrowser: false};
                                IE.isTheBrowser = false;
                            }
							


                            if(IE.isTheBrowser){
                                var videIE10 = "";
                                if(shortData.files.video.indexOf('dps.live/') > -1){
                                    videIE10 = shortData.files.video.replace('_%DSP%','_lo').replace('http:','https:');
                                    htmlEmoltvENVIVO = '<video id="videoPrin_PORTADA_'+pos+'" class="video-js vjs-default-skin vjs-big-play-centered" poster="'+imgFoto+'" muted><source src="'+videIE10+'" type="application/x-mpegURL"></video>';
                                }else{
                                    videIE10 = "rtmp://broadcaster.ecn.cl/hls/" + shortData.files.video.split('/')[4].replace('_%DSP%.m3u8','_lo');
                                    htmlEmoltvENVIVO = '<video id="videoPrin_PORTADA_'+pos+'" class="video-js vjs-default-skin vjs-big-play-centered" poster="'+imgFoto+'" muted><source src="'+videIE10+'" type="application/x-mpegURL"></video>';
                                }
                            }else{
                                if(shortData.files.video.indexOf('dps.live/') > -1){
                                    htmlEmoltvENVIVO = '<video id="videoPrin_PORTADA_'+pos+'" class="video-js vjs-default-skin vjs-big-play-centered" poster="'+imgFoto+'"><source src="'+shortData.files.video.replace('_%DSP%','_lo').replace('http:','https:')+'" type="application/x-mpegURL"></video>';
                                }else{
                                    htmlEmoltvENVIVO = '<video id="videoPrin_PORTADA_'+pos+'" class="video-js vjs-default-skin vjs-big-play-centered" poster="'+imgFoto+'"><source src="'+shortData.files.video.replace('_%DSP%','_lo/index').replace('http:','https:')+'" type="application/x-mpegURL"></video>';
                                }
                            }
						
                            //$("#videoShoppingEmoltv").append('<div class="contTv_'+pos+'">'+htmlEmoltvENVIVO+'</div>');
							
