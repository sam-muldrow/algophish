                                                    try {
                                                        Config_h5.posActivas[j].lowq = cdn.mp4_144p.cdnUri.replace('http:', '');							Config_h5.posActivas[j].pseudoStreaming_date = shortData.pseudoStreaming_date;
                                                        try {
                                                            Config_h5.posActivas[j].duration = shortData.duration;
                                                        } catch (error) {}
                                                    } catch (error) {}
                                                }
                                            }
                                        } catch (e) {
                                            console.log(e.message)
                                        }
                                    }
                                }
                                /* if(Config_h5.posActivas[pos].currentTime){
                                     time = Config_h5.posActivas[pos].currentTime;
                                     player.currentTime(time);
                                     if(navigator.userAgent.toLowerCase().indexOf('firefox') > -1){
                                         setTimeout(function(){
                                             document.getElementById('videoPrin_PORTADA_Shopping_html5_api').currentTime = time;
                                         }, 1000);//wait 2 seconds
                                     }
                                 }else{*/
                                try {
                                    if (shortData.pseudoStreaming_date) {
                                        if (navigator.userAgent.toLowerCase().indexOf('firefox') > -1) {
                                            shortData.pseudoStreaming_date = shortData.pseudoStreaming_date.replace('.000Z', '');
                                            var g1 = new Date(shortData.pseudoStreaming_date);
                                        } else {
                                            var g1 = new Date(shortData.pseudoStreaming_date);
                                            var userTimezoneOffset = g1.getTimezoneOffset() * 60000;
                                            g1 = new Date(g1.getTime() + userTimezoneOffset);
                                        }
                                        var g2 = null;
                                        $.ajax({
                                            url: 'https://cache-elastic-pandora.ecn.cl/time/now.json',
                                            dataType: "json",
                                            async: false,
                                            success: function(data) {
                                                if (navigator.userAgent.toLowerCase().indexOf('firefox') > -1) {
                                                    var textTime = data.currentTime;
                                                    var arrayTime = textTime.split(",");
                                                    var arrayVal = arrayTime[1].split("-");
                                                    day = arrayVal[0].trim();
                                                    mon = arrayVal[1].replace('Jul', '7');
                                                    year = (arrayVal[2].split(" "))[0];
                                                    hour = (((arrayVal[2].split(" "))[1]).split(":"))[0];
                                                    min = (((arrayVal[2].split(" "))[1]).split(":"))[1];
                                                    seg = (((arrayVal[2].split(" "))[1]).split(":"))[2];
                                                    data.currentTime = (year + '-' + mon + '-' + day + ' ' + hour + ':' + min + ':' + seg);
                                                }
                                                g2 = new Date(data.currentTime);
                                            }
                                        });
                                        if (g1.getTime() < g2.getTime()) {
                                            autoplayPseudoStreaming = true;
                                            mutedPseudoStreaming = true;
                                            console.log('Pseudo - Time Diff');
                                            try {
                                                var diff = g2.getTime() - g1.getTime(); // this is a time in milliseconds
                                                var diff_as_date = new Date(diff);
                                                minutes = diff_as_date.getMinutes(); // minutes
                                                seconds = diff_as_date.getSeconds(); // seconds
                                                hours = diff_as_date.getHours(); // seconds
                                                var time = (minutes * 60) + seconds + ((hours * 60) * 60);
                                                var diffSeg = parseInt(diff / 1000);
                                                time = diffSeg;
                                                if (shortData.duration) {
                                                    console.log('Time: ' + time + ' - Duration : ' + shortData.duration);
                                                    if (parseInt(shortData.duration) > time) {
                                                        player.currentTime(time);
                                                        if (navigator.userAgent.toLowerCase().indexOf('firefox') > -1) {
                                                            setTimeout(function() {
                                                                document.getElementById('videoPrin_PORTADA_Shopping_html5_api').currentTime = time;
                                                            }, 1000); //wait 2 seconds
                                                        }
                                                    }
                                                } else {
                                                    console.log('Time: ' + time + ' - hour : ' + hour);
                                                    if (hours == '0') {
                                                        player.currentTime(time);
                                                        if (navigator.userAgent.toLowerCase().indexOf('firefox') > -1) {
                                                            setTimeout(function() {
                                                                document.getElementById('videoPrin_PORTADA_Shopping_html5_api').currentTime = time;
                                                            }, 1000); //wait 2 seconds
                                                        }
                                                    }
                                                }
                                            } catch (e) {}
                                        }
                                    }
                                } catch (e) {}
                                //}
						
                            });

                            player.on('pause', function() {
                                console.log('event  - pause');
                                for (var i = 0; i < VideosTv.length; i++) {
                                    if (VideosTv[i].Tag == player.id_) {
                                        try {
								
                                            for (var j = 0; Config_h5.posActivas.length > j; j++) {
                                                if (Config_h5.posActivas[j].Tag == VideosTv[i].Tag) {
                                                    var newInitTime = new Date();
                                                    var time = newInitTime - Config_h5.posActivas[j].timeInit;
                                                    eventTv("pause", VideosTv[i].idV, time, buscaFuente(VideosTv[i].fue), VideosTv[i].tit, VideosTv[i].per, VideosTv[i].cat, VideosTv[i].Typ, VideosTv[i].Tag);
                                                    Config_h5.posActivas[j].timeInit = newInitTime;
                                                }
                                            }
								
                                        } catch (e) {
                                            console.log(e.message)
                                        }
                                    }
                                }
                            });

                            player.on("loadedmetadata", function() {
                                console.log('event  - loadedmetadata');
                                var total = player.duration();
                                var playerETV = $(player.controlBar.progressControl.children_[0].el_);

                                if (!CajaCarrusel) {
