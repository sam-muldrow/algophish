                                gt.setAttribute('defer', 'defer');
                                gt.src = l + '://gabg.hit.gemius.pl/xgemius.js';
                                s.parentNode.insertBefore(gt, s);
                        } catch (e) {
                        }
                })(document, 'script');
                //--><!]]>
        </script>
        <div id="popup-share"></div>
    </body>
</html>
