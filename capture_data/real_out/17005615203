        <script>
function get_widget_serv(serv)
{
            var o = {
                type: "POST",
                url: 'https://market.cnews.ru/widget2/'+serv,
                beforeSend: function(jqXHR, setting)
                {
                    /**/
                },
                complete: function(jqXHR,textStatus)
                {
                    if (jqXHR.status === 200 && jqXHR.responseText) {
                        let $html = $(jqXHR.responseText);
                        // let $section = $html.filter('#mob_slide').find('.section_provider_choice');
                        let $section = $html.filter('#mob_slide').find('.section_provider_choice').parents('.page-container');
                        if ($section.length) {
                            $section.find('.section_top').remove();
                            $('#widget2').html('');
                            $('#widget2').append($section);
                            if(!$('link[href="https://market.cnews.ru/inc/css/style_w2.css"]').length) {
                                $('head').append('<link rel="stylesheet" href="https://market.cnews.ru/inc/css/style_w2.css" type="text/css" />');
                            }
                            if(!$('script[src="https://market.cnews.ru/inc/js/market-scripts.js?"]').length) {
                                $('head').append('<script type="text/javascript" src="https://market.cnews.ru/inc/js/market-scripts.js?" />');    
                            }
                            if(!$('script[src="https://market.cnews.ru/inc/js/widget2_script.js?"]').length) {
                                $('head').append('<script type="text/javascript" src="https://market.cnews.ru/inc/js/widget2_script.js?" />');
                            }
                            /*$('head').append('<link rel="stylesheet" href="https://market.cnews.ru/inc/css/style_w2.css" type="text/css" />');
                            $('head').append('<script type="text/javascript" src="https://market.cnews.ru/inc/js/market-scripts.js?" />');
                            $('head').append('<script type="text/javascript" src="https://market.cnews.ru/inc/js/widget2_script.js?" />');*/
                        }
                    }
                }
            }
            if(typeof data !== "undefined")
            {
                o.data = data;
            }
            if(typeof callback !== "undefined")
            {
                o.success = callback;
            }

            $.ajax(o);
}
get_widget_serv('');

            var timerId;


            function set_to()
            {
                if(timerId)
                {
                    clearTimeout(timerId);
                }


                return this;
            }

            $('.hidden-filter-btn').find('a').click(function(){
                set_to();
                $('.hidden-filter-btn').find('a').addClass("load");
                $('.hidden-filter-btn').find('img').addClass("load");
            });


            function set_checkbox_box()
            {
                $('.checkbox-container').click(function(){
                    let el	=	document.getElementsByClassName('checkbox-container-clicked');


                    for(let i=0;i<el.length;i++)
                    {
                        el[i].classList.remove('checkbox-container-clicked');
                    }


                    var $el = $(this).addClass("checkbox-container-clicked");
                    $('#company_list_full').addClass("checkbox-container-clicked");

                    set_to();
                    timerId=setTimeout(function() {
                        $el.removeClass('checkbox-container-clicked');
                        $('#company_list_full').removeClass('checkbox-container-clicked');
                        $('.hidden-filter-btn').find('a').removeClass("load");
                        $('.hidden-filter-btn').find('img').removeClass("load");
                    },5000);
                });
            }

            set_checkbox_box();
            $('.p-filter-value input').keyup(function(){
                let el	=	document.getElementsByClassName('checkbox-container-clicked');


                for(let i=0;i<el.length;i++)
                {
                    el[i].classList.remove('checkbox-container-clicked');
                }

                var $elInput = $(this).closest('.p-filter').addClass('checkbox-container-clicked');
                set_to();
                timerId=setTimeout(function(){
                    $elInput.removeClass('checkbox-container-clicked');
                    $('.hidden-filter-btn').find('a').removeClass("load");
                    $('.hidden-filter-btn').find('img').removeClass("load");
                },5000);
            });
        </script>
    </div>
</section>

