    <script defer="defer">
        
        if(!$('.exclusive-banner').length) {
          if ($(window).width() >= 1600) {
            /*
            var script = document.createElement('script');
            script.src = 'https://cnb.cnews.ru/rotator.php?zone=379';
            document.head.appendChild(script);
            */
            url = "https://cnb.cnews.ru/rotator.php?zone=379";
            // document.write('<script src="' + url + '"></scr' + 'ipt>')
            let script = document.createElement("script");
            script.setAttribute("type", "text/javascript");
            script.src = url;
            script.onload = function() {
              var skyscraper_banner = $(".cnb-zone-354");
              var skyscraper = skyscraper_banner.find(".skyscraper");
              if(skyscraper.length) {
                $('.cnb-zone-379').css('display', 'block');
                // $('body').css('background', '#070e07');
              }
            }
            document.body.appendChild(script);
          }
        }
        
    </script>

