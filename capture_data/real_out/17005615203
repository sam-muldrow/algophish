		{
			ga_time_on_page++;
			t=setTimeout(timeCount,1000);
		}
		t=setTimeout(timeCount,1000);
		function send2ga()
		{	try {
				if (pageTracker) pageTracker._trackEvent("Page", "Unload", encodeURIComponent(document.location.href), ga_time_on_page);
			} catch(err) { }
		}
		window.onbeforeunload = send2ga;
	</script>
