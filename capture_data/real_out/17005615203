<script>
$(document).ready(function(){
    /*вызов модального окна с баннером на мобильных*/
    $(function () {
        if ($(window).width() <= 576) {
            const modal = $('#mobile-banner');
	    var el	=	document.getElementById('z_140');
	    if(el)
	    {
		    document.getElementById('mz_140').innerHTML	=	el.innerHTML;
		    el.innerHTML	=	'';
	    }
            openModal(modal);
        }
        
        function openModal(modal) {
            setTimeout(function () {
                modal.modal('show');
            }, 2000);
            
            setTimeout(function () {
                modal.modal('hide');
            }, 30000);
        }
    });
});
</script>


