<script>
    $(document).ready(function(){

        $('.goods-list .btn.start a').mouseenter(function(){
            var oldhtml=$(this).data("oldhtml");
            if(!oldhtml){
                var innerhtml=$(this).html();
                $(this).data("oldhtml",innerhtml);
            }
            $(this).html("去收藏");
        });

        $('.goods-list .btn.start a').mouseleave(function(){
            $(this).html( $(this).data("oldhtml"));
        });
    });
</script>

