		a();
	});

	var F_showTwoDimeBox = function(){
	$('.list-good .dimension').parents('li').hover(
			function(){
				var $self = $(this);
				var $selfDimension = $self.find('.dimension');
				var imgHtml = '<img id="dime_img" src="" width="97"/>';
				var imgSrcUrl = $self.find('.img_inputid').attr('d_src');
				if( $self.find('#dime_img').length == 0 ){
					$self.find('.dime-pic').append(imgHtml);
				}
				$self.find('#dime_img').attr('src', imgSrcUrl);
				$self.find('.two-iphone-n').attr('class', 'two-iphone');
				$selfDimension.show();
				var curSpanWid = $selfDimension.find('span').width();
				if(curSpanWid > 25){
					$selfDimension.find('.unit').hide();
				}
			}, function(){
				$(this).find('.two-iphone').attr('class', 'two-iphone-n');
				$(this).find('.dimension').hide();
			}
		);
	}
	F_showTwoDimeBox();
	
</script>


