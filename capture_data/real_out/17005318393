		var position1 = vtr_shows[0];
		var position2 = vtr_shows[1];
		var position3 = vtr_shows[2];
		var position4 = vtr_shows[3];
	    var loaded_images = 0;
	    preload_image_object = new Array();
		function load_next_images()
		{
			for(i=0;i<8 && loaded_images<vtr_codes.length;i++)
			{
				preload_image_object[loaded_images] = new Image();
			    preload_image_object[loaded_images].src = 'https://www.e-shop.gr/images/'+vtr_codes[loaded_images].substring(0,3)+'/SMALL/'+vtr_codes[loaded_images]+'.jpg';
				loaded_images++;
			}
		}
		load_next_images();
	  		
		function getPosition(arrayName,arrayItem)
		{ 
	    	for(var i=0;i<arrayName.length;i++) if(arrayName[i]==arrayItem) return i;
		}
	
		function getStarter()
		{
			var temp1 = getPosition(vtr_codes,vtr_starter);
			if ((vtr_codes.length-1)==temp1) temp1=0;
			else temp1++;
			vtr_starter = vtr_codes[temp1];
			return temp1;
		}
	
		function gotolocation(id)
		{
			var position = '';
			if (id==1) position = position1; else if (id==2) position = position2; else if (id==3) position = position3; else if (id==4) position = position4;		
			window.location = position;
			return;
		}
		
		function SetOpacity(object,opacityPct)
		{
	  		/* IE */ 		object.style.filter = 'alpha(opacity=' + opacityPct + ')';
		  	/* firefox */ 	object.style.MozOpacity = opacityPct/100;
	 	 	/* Safari */ 	object.style.KHTMLOpacity = opacityPct/100;
		  	/* else */		object.style.opacity = opacityPct/100;
		}
	
		function ChangeOpacity(id,msDuration,msStart,fromO,toO)
		{
	  		var element=document.getElementById(id);
	 	 	var opacity = element.style.opacity * 100;
		  	var msNow = (new Date()).getTime();
		  	opacity = fromO + (toO - fromO) * (msNow - msStart) / msDuration;
		  	if (opacity<0) 
		    	SetOpacity(element,0)
		  	else if (opacity>100)
		    	SetOpacity(element,100)
		  	else
		  	{
	 		   	SetOpacity(element,opacity);
		    	element.timer = window.setTimeout("ChangeOpacity('" + id + "'," + msDuration + "," + msStart + "," + fromO + "," + toO + ")",1);
	 	 	}
		}
	
		function FadeIn(id)
		{
	  		var element=document.getElementById(id);
	  		if (element.timer) window.clearTimeout(element.timer); 
	  		var startMS = (new Date()).getTime();
	  		element.timer = window.setTimeout("ChangeOpacity('" + id + "',1000," + startMS + ",0,100)",1);
		}
	
		function FadeOut(id)
		{
	  		var element=document.getElementById(id);
	  		if (element.timer) window.clearTimeout(element.timer); 
	  		var startMS = (new Date()).getTime();
	  		element.timer = window.setTimeout("ChangeOpacity('" + id + "',1000," + startMS + ",100,0)",1);
		}
	
		function FadeInImage(foregroundID,newImage,backgroundID)
		{
	  		var foreground=document.getElementById(foregroundID);
	  		if (backgroundID)
	  		{
	    		var background=document.getElementById(backgroundID);
	    		if (background)
	    		{
	      			background.style.backgroundImage = 'url(' + foreground.src + ')';
	      			background.style.backgroundRepeat = 'no-repeat';
	    		}
	  		}
	  		SetOpacity(foreground,0);
	 		foreground.src = newImage;
	  		if (foreground.timer) window.clearTimeout(foreground.timer); 
	  		var startMS = (new Date()).getTime();
	  		foreground.timer = window.setTimeout("ChangeOpacity('" + foregroundID + "',1000," + startMS + ",0,100)",10);
		}
		
		function create_vitrina()
		{
			vid = getStarter();
			position1 = vtr_shows[vid];
			FadeInImage('photo1',preload_image_object[vid].src,'vitrina_photo1');
			document.getElementById("vitrina_title1").innerHTML = vtr_devs[vid]+'<br><a href="'+vtr_shows[vid]+'" class="home-protaseis-title-link">'+vtr_titles[vid]+'</a>';
			document.getElementById("vitrina_price1").innerHTML = vtr_prices[vid]+' &euro;';
			document.getElementById("vitrina_extra1").innerHTML = '<a class="home-protaseis-cta" href="'+vtr_shows[vid]+'">περισσότερα</a>';
	
			vid = getStarter();
			position2 = vtr_shows[vid];
			FadeInImage('photo2',preload_image_object[vid].src,'vitrina_photo2');
			document.getElementById("vitrina_title2").innerHTML = vtr_devs[vid]+'<br><a href="'+vtr_shows[vid]+'" class="home-protaseis-title-link">'+vtr_titles[vid]+'</a>';
			document.getElementById("vitrina_price2").innerHTML = vtr_prices[vid]+' &euro;';
			document.getElementById("vitrina_extra2").innerHTML = '<a class="home-protaseis-cta" href="'+vtr_shows[vid]+'">περισσότερα</a>';
	
			vid = getStarter();
			position3 = vtr_shows[vid];
			FadeInImage('photo3',preload_image_object[vid].src,'vitrina_photo3');
			document.getElementById("vitrina_title3").innerHTML = vtr_devs[vid]+'<br><a href="'+vtr_shows[vid]+'" class="home-protaseis-title-link">'+vtr_titles[vid]+'</a>';
			document.getElementById("vitrina_price3").innerHTML = vtr_prices[vid]+' &euro;';
			document.getElementById("vitrina_extra3").innerHTML = '<a class="home-protaseis-cta" href="'+vtr_shows[vid]+'">περισσότερα</a>';
	
			vid = getStarter();
			position4 = vtr_shows[vid];
			FadeInImage('photo4',preload_image_object[vid].src,'vitrina_photo4');
			document.getElementById("vitrina_title4").innerHTML = vtr_devs[vid]+'<br><a href="'+vtr_shows[vid]+'" class="home-protaseis-title-link">'+vtr_titles[vid]+'</a>';
			document.getElementById("vitrina_price4").innerHTML = vtr_prices[vid]+' &euro;';
			document.getElementById("vitrina_extra4").innerHTML = '<a class="home-protaseis-cta" href="'+vtr_shows[vid]+'">περισσότερα</a>';
	
			load_next_images();
		}
	
		setInterval(create_vitrina,12000);
	
		</script>
	
