                setTargeting("authorized", window.Drupal.settings.site.uid>0 ? 'auth' : 'anon' ).
                setTargeting("page-type", (!!window.Drupal.settings.site.type ? window.Drupal.settings.site.type : 'other_page_type') ).
                setTargeting('product-tid', (tids.length>0 ? tids.split(',') : 'no_tids') ).
                setTargeting('product-nid', (!!window.page_tag_info.prod_id ? window.page_tag_info.prod_id+'p' : 'no_product') ).
                setTargeting('review-author', (window.Drupal.settings.site.type === 'review' ? window.Drupal.settings.site.author_uid+'a' : 'no_review_author') ).
                setTargeting("Rand09", ''+Math.floor(Math.random() * 10) );

                if(window.adserver.no_rules) googletag.pubads().setTargeting("Rand09", 'no-rules' );

            }catch(e){}

            googletag.enableServices();


        });







        if(window.adserver.debug){
            googletag.cmd.push(function() {
                googletag.pubads().addEventListener('slotRequested', function(event) {
                    var slot = event.slot;
                    console.log('Slot', slot.getSlotElementId(), 'has been requested.');
                });
            });
        }





    
