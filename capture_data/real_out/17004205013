<script>
    $("body").on("contextmenu", "img", function(e) { return false; });
    //对联显示隐藏控制
    var divContentl=$("#"+sl).html();
    var divContentr=$("#"+sr).html();
    if(divContentl==0||divContentl==null){
        $("#dl").hide();
        $("#dr").hide();
    }else{
        $("#dl").show();
        $("#dr").show();
    }

    $("#dl").click(function(){
        $("#"+sl).hide();
        $("#"+sr).hide();
        $("#dl").hide();
        $("#dr").hide();
    });
    $("#dr").click(function(){
        $("#"+sl).hide();
        $("#"+sr).hide();
        $("#dl").hide();
        $("#dr").hide();
    });
    //banner2显示隐藏控制
    var divContentBanner02=$("#"+sb2).html();

    if(divContentBanner02==0||divContentBanner02==null){
        $("#banner02z").hide();
        $(".dver").hide();
    }else{
        $("#banner02z").show()
    }
    $("#banner02z").click(function(){
        $("#"+sb2).hide();
        $("#banner02z").hide();
    });

    //浮窗显示隐藏控制

    var divContentTan=$("#"+st).html();

    if(divContentTan==0||divContentTan==null){
        $("#tanchuang").hide()
    }else{
        $("#tanchuang").show()
        $(".mark").show()
    }
    $("#tanchuang").click(function(){
        $("#"+st).hide();
        $("#tanchuang").hide();
    });

    function formatMsgTime(timespan) {
        var dateTime = new Date(timespan);
        var year = dateTime.getFullYear();
        var month = dateTime.getMonth() + 1;
        var day = dateTime.getDate();
        var hour = dateTime.getHours();
        var minute = dateTime.getMinutes();
        var second = dateTime.getSeconds();
        var now = new Date();
        var now_new = now.getTime();  //typescript转换写法
        console.log(now_new);
        var seconds = 0;
        var timeSpanStr;
        seconds = Math.floor((now_new - timespan) / 1000);
        if (seconds <= 0) {
            seconds = 0
        }
        if (seconds == 0) {
            timeSpanStr = '刚刚';
        } else if (seconds < 30) {
            timeSpanStr = seconds + '秒以前'
        } else if (seconds >= 30 && seconds < 60) {
            timeSpanStr = '半分钟前';
        } else if (seconds >= 60 && seconds < 60 * 60) {
            var minutes = Math.floor(seconds / 60);
            timeSpanStr = minutes + '分钟前';
        } else {
            var hours = Math.floor((seconds / 60) / 60);
            if (hours <= 6) {
                timeSpanStr = hours + '小时前';
            } else if (hours > 6 && hours <= 24) {
                timeSpanStr = '今天';
                if ((now.getDate() - day) >= 1) {
                    timeSpanStr = '昨天'
                }
            } else {
                var days = Math.floor(seconds / (24 * 60 * 60))
                if (days <= 7) {
                    timeSpanStr = days + '天前'
                } else {

                }
            }
        }
        timeSpanStr = year + '-' + month + '-' + day;
        return timeSpanStr;
    }
    $(function () {
        $('.time').each(function (index, elm) {
            if ($(elm).attr('data-time')) {
                $(elm).html(formatMsgTime(parseInt($(elm).attr('data-time'))));
            }
        })

        $('#recommend01').hide();
        $('#recommend02').hide();
        $('#recommend03').hide();
        $('#recommend-more01').click(function(){
            $('#recommend-more01').hide();
            $('#recommend01').show();
        });
        $('#recommend-more02').click(function(){
            $('#recommend-more02').hide();
            $('#recommend02').show();
        });
        $('#recommend-more03').click(function(){
            $('#recommend-more03').hide();
            $('#recommend03').show();
        });


    })
    $(function () {
        $("a[title^='站长统计']").hide();

    });


</script>
