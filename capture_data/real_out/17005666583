                                            		return 'https://www.emory.edu/home/events/trumba.html?trumbaEmbed=%26view%3Devent%26eventid%3D' + eventId;
                                            	});
                            		            $.template.addFormatter("addClassToTagInHTMLString", function (value, context, tagName, classString) {
