.scroll-to-bottom .scroll-down {
    position: relative;
    width: 1px;
    height: 100px;
    margin-top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    overflow: hidden;
    -webkit-transform: rotate(180deg);
    transform: rotate(180deg);
}

