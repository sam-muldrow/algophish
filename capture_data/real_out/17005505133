function analyticsStart() { //called from sourcepoint backend
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());
  gtag('config', 'UA-2058906-1');

  var ga = document.createElement('script');
  ga.type = 'text/javascript';
  ga.async = true;
  ga.src ='https://www.googletagmanager.com/gtag/js?id=UA-2058906-1';
  document.head.appendChild(ga);
}
</script>
