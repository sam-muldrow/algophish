  // Sourcepoint configuration
  window._sp_ = {
    config: {
      accountId: 1061,
      baseEndpoint: 'https://cdn.privacy-mgmt.com',
      isSPA: true,
      targetingParams: {
        acps: 'false'
      },
      events: {
        onConsentReady: function(consentUUID, euconsent, consentStatus) {
          if (consentStatus && consentStatus.consentedToAll === true) {
			  var sfa = document.createElement('script');
				  sfa.type = 'text/javascript';
				  sfa.src ='/sfa.js';
				  document.head.appendChild(sfa);
			  
			  if (typeof window.onconsentcallback === "function") { 
					window.onconsentcallback(consentUUID, euconsent, consentStatus)
			  }
          }
        }
      }
    }
  };
