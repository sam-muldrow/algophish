<script type='text/javascript'>
  var fscroll;

  onDomReady(function() {
    //fscroll = new DragDivScroll( 'ferien_long_panel_ferien_655be4fac98a4', 'body', 'toggle noYBarHide NOSTATUS touchOnlyY' );
    var ajaxSend = false;
    var month = 2;

    fscroll = new sfUI.DragScroller('ferien_long_panel_ferien_655be4fac98a4',{
      onEndX: function(dragScroller, newScroll){
        if (!ajaxSend) {
          ajaxSend = true;
          var myAjax = new Request.HTML({
            url: '/?action=mt&controller=kalender&m='+month,
            method: 'get',
            headers: {'X-AjaxCache': 'true'},
            append: this.element,
            evalScripts: false,
            onComplete: function(){
              ajaxSend = false;
              month++;
              dragScroller.resetEndEventsSended();
              attach_tooltip();
            }
          });
          myAjax.send();
        }
      },
      endEventOffset: { x:300, y:0 }
    });

    var scroll = new Fx.Scroll('ferien_long_panel_ferien_655be4fac98a4', {
      wait: false,
      duration: 500,
      offset: {'x': -100, 'y': 0},
      transition: Fx.Transitions.Quad.easeInOut
    });

    scroll.toElement('today_655be4fac98a4');
    //$('ferien_long_panel_ferien_655be4fac98a4').scrollLeft = $('today_655be4fac98a4').getPosition($('ferien_long_panel_ferien_655be4fac98a4')).x;
  });

  //expand();
</script></div>
