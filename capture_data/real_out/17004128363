    $('.signoutConfrm__btn-grp .cancel-button').html(signout.cancelText);
    $('.signoutConfrm__btn-grp .submit-button').html(signout.signoutText);
    $('.signoutConfrm__btn-grp .mm-btn--primary').prop('title', signout.cancelText);
