<div class="worldcup-wrapper">
<div class="game1-static">
<span id="game1-stat"></span>
</div>
<div class="game1-title-container">
<span id="game1-title"></span>
</div>
<div class="game1-score">
<span id="game1-scoreone"></span>
<span id="game1-scoretwo"></span>
</div>
</div>
<script>
  console.log("connected");
  const scoreone = document.getElementById("game1-scoreone");
  const scoretwo = document.getElementById("game1-scoretwo");

  const teamone = document.getElementById("game1-teamone");
  const teamtwo = document.getElementById("game1-teamtwo");
  const stat = document.getElementById("game1-stat");
  const title = document.getElementById("game1-title");

  function getLiveScoreOne() {
    fetch("https://camtrack.reachableads.com/CricketScoreAPI")
      .then((response) => response.json())
      .then((data) => {
        console.log(data.final_result);

        if (data.final_result == false) {
          stat.innerText = "";
        } else {
          stat.innerText = data.final_result;
        }

        if (data.title == false) {
          title.innerText = "";
        } else {
          title.innerText = data.title;
        }

        if (data.score1 == false || data.score1 == "Match Info") {
          scoreone.innerText = "Yet to come";
        } else {
          scoreone.innerText = data.score1.replace(" Ov", "");
        }

        if (data.score2 == false || data.score2 == "Match Info") {
          scoretwo.innerText = "Yet to come";
        } else {
          scoretwo.innerText = data.score2.replace(" Ov", "");
        }

      });
  }

  function getLiveScoreTwo() {
    fetch("https://camtrack.reachableads.com/CricketScoreAPI2/")
      .then((response) => response.json())
      .then((data) => {
        console.log(data.final_result);

        if (data.final_result == false) {
          gameTwoStat.innerText = "";
        } else {
          gameTwoStat.innerText = data.final_result;
        }

        if (data.title == false) {
          gameTwoTitle.innerText = "";
        } else {
          gameTwoTitle.innerText = data.title;
        }

        if (data.score1 == false || data.score1 == "Match Info") {
          gameTowScoreOne.innerText = "Yet to come";
        } else {
          gameTowScoreOne.innerText = data.score1.replace(" Ov", "");
        }

        if (data.score2 == false || data.score2 == "Match Info") {
          gameTwoScoreTwo.innerText = "Yet to come";
        } else {
          gameTwoScoreTwo.innerText = data.score2.replace(" Ov", "");
        }
      });
  }

  getLiveScoreOne();
  setInterval(function () {
    getLiveScoreOne();
  }, 5000);
</script>
