<script>
$(document).ready(function(){
    $.ajax({
        url: './english/jsonleadnews',
    })
    .done(function(data) {
        var lead = '';
        var other_news = '';
        
        if(data[0].vertical_img!='' && data[0].vertical_img!= null){
            lead += '<img class="lazy-load" src="english/public/img/common-pic.png" data-src="/english/public/uploads/'+data[0].date.replace("-", "/").replace("-", "/")+'/vertical/'+data[0].vertical_img+'" alt="'+data[0].head+'">';
        }else if(data[0].img!='' && data[0].img!= null){
            lead += '<img class="lazy-load" src="english/public/img/common-pic.png" data-src="/english/public/uploads/'+data[0].date.replace("-", "/").replace("-", "/")+'/thumbnail/'+data[0].img+'" alt="'+data[0].head+'">';
        }else{
            lead += '<img class="lazy-load" src="english/public/img/common-pic.png">';
        }
        
        // lead += '<h2><a href="/english/'+data[0].menu_info.workable_menu+'/news/bd/'+data[0].n_id+'.details">'+data[0].head+'</a></h2> <br> <p>'+data[0].details.slice(0, 150)+'...</p>';
        
        lead += '<h2><a href="/english/'+data[0].menu_info.workable_menu+'/news/bd/'+data[0].n_id+'.details">'+data[0].head+'</a></h2>';
        
        $("#eng_lead").html(lead);
        
        
        delete data[0];
        var onews = data;
        onews.forEach(otherFunction);
        
        $("#eng_other").html(other_news);
        
        function otherFunction(item, index) {
            other_news += '<li><a href="/english/'+item.menu_info.workable_menu+'/news/bd/'+item.n_id +'.details"> <div class="col-md-3"> <div class="row">';
            
            if(item.img!='' && item.img!= null){
                other_news += '<img class="lazy-load" src="'+'english/public/uploads/'+item.date.replace("-", "/").replace("-", "/")+'/mobile/'+item.img+'" alt="'+item.head+'">';
            }else{
                other_news += '<img class="lazy-load" src="english/public/img/common-pic.png" alt="'+item.head+'">';
            }

            other_news += '</div></div>';

            other_news += '<div class="col-md-9 link"> <p>'+ item.head + '</p> </div> </a></li>'; 
        }
    });
});
</script>
