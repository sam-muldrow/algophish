<script type="text/javascript" language="javascript">
	

	jQuery(document).ready(function($){
		
		if($('.wpcf7-form-control.wpcf7-repeater-add').length>0){
			$('.wpcf7-form-control.wpcf7-repeater-add').on('click', function(){
				wpdp_refresh(jQuery, true);
			});
		}
		
	
});
var wpdp_refresh_first = 'yes';
var wpdp_counter = 0;
var wpdp_month_array = [];
var wpdp_dateFormat = "dd.mm.yy";
var wpdp_defaultDate = "";
function wpdp_refresh($, force){
				if(typeof $.datepicker!='undefined' && typeof $.datepicker.regional["en-GB"]!='undefined'){
					
				wpdp_month_array = $.datepicker.regional["en-GB"].monthNames;
									
				}
		
		
				

				

				if($("#profile_birth_date").length>0){
					
				$("#profile_birth_date").attr("autocomplete", "off");
					
				//document.title = wpdp_refresh_first=='yes';
				force = true;
				if(wpdp_refresh_first=='yes' || force){
					
					if(typeof $.datepicker!='undefined')
					$("#profile_birth_date").datepicker( "destroy" );
					
					$("#profile_birth_date").removeClass("hasDatepicker");
					wpdp_refresh_first = 'done';
					
				}
				$('body').on('mouseover, mousemove', '#profile_birth_date', function(){//#profile_birth_date
				if ($(this).val()!= "") {
					$(this).attr('data-default-val', $(this).val());
				}		
							
				if(wpdp_counter>2)
				clearInterval(wpdp_intv);		
				
				if(!$("#profile_birth_date").hasClass('hasDatepicker')){

				
					
				$("#profile_birth_date").datepicker($.extend(  
					{},  // empty object  
					$.datepicker.regional[ "en-GB" ],       // Dynamically  
					{  
 					dateFormat: wpdp_dateFormat
  } // your custom options 
				)); 
				
				$("#profile_birth_date").attr('readonly', 'readonly');
				
				
				
				
				$("#profile_birth_date").datepicker( "option", "dateFormat", "dd.mm.yy" );
									
					$.each($("#profile_birth_date"), function(){
						if($(this).data('default-val')!= ""){
							$(this).val($(this).data('default-val'));
						}
						
					});
						
				
				}
				});
				}

				

				if($("#egmont_util_restricted_words_emails_disable_date").length>0){
					
				$("#egmont_util_restricted_words_emails_disable_date").attr("autocomplete", "off");
					
				//document.title = wpdp_refresh_first=='yes';
				force = true;
				if(wpdp_refresh_first=='yes' || force){
					
					if(typeof $.datepicker!='undefined')
					$("#egmont_util_restricted_words_emails_disable_date").datepicker( "destroy" );
					
					$("#egmont_util_restricted_words_emails_disable_date").removeClass("hasDatepicker");
					wpdp_refresh_first = 'done';
					
				}
				$('body').on('mouseover, mousemove', '#egmont_util_restricted_words_emails_disable_date', function(){//#egmont_util_restricted_words_emails_disable_date
				if ($(this).val()!= "") {
					$(this).attr('data-default-val', $(this).val());
				}		
							
				if(wpdp_counter>2)
				clearInterval(wpdp_intv);		
				
				if(!$("#egmont_util_restricted_words_emails_disable_date").hasClass('hasDatepicker')){

				
					
				$("#egmont_util_restricted_words_emails_disable_date").datepicker($.extend(  
					{},  // empty object  
					$.datepicker.regional[ "en-GB" ],       // Dynamically  
					{  
 					dateFormat: wpdp_dateFormat
  } // your custom options 
				)); 
				
				$("#egmont_util_restricted_words_emails_disable_date").attr('readonly', 'readonly');
				
				
				
				
				$("#egmont_util_restricted_words_emails_disable_date").datepicker( "option", "dateFormat", "dd.mm.yy" );
									
					$.each($("#egmont_util_restricted_words_emails_disable_date"), function(){
						if($(this).data('default-val')!= ""){
							$(this).val($(this).data('default-val'));
						}
						
					});
						
				
				}
				});
				}
		
		
			
		
		$('.ui-datepicker').addClass('notranslate');
}
	var wpdp_intv = setInterval(function(){
		wpdp_counter++;
		wpdp_refresh(jQuery, false);
	}, 500);

	
	
	</script>
