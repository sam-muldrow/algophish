    function LoadOthersVideos(cattegoryId, vid, type) {
        // Store
        // alert(type+cattegoryId+vid);
        var newclick = 0;
        newclick = parseInt(localStorage.getItem('cattegoryId' + cattegoryId));
        if (isNaN(newclick)) {
            newclick = 0;
        }
        if (type === 'more') {
            newclick += 1;
            localStorage.setItem('cattegoryId' + cattegoryId, newclick);
        } else {
            newclick -= 1;
            localStorage.setItem('cattegoryId' + cattegoryId, newclick);
        }

// Retrieve
        newclick = parseInt(localStorage.getItem('cattegoryId' + cattegoryId));

        //$("#Loading" + cattegoryId).show();
        Â $.ajax({
            type: "POST",
            url: "https://col3negoriginal.lk/page/loadrecentVideos?cid=" + cattegoryId + "&click=" + newclick + "&type=" + type,
            complete: function (r) {
                var list = r.responseText;
                // alert(list);
                $(".LoadedVideo" + cattegoryId).hide();
                //$("#Loading" + cattegoryId).hide();
                $(".LoadedVideo" + cattegoryId).html("");
                $("#LoadedVideo" + cattegoryId).html("");
                $("#LoadedVideoid" + cattegoryId).html(list);
            },
            error: function (xhr, ajaxOptions, thrownError) {

                // $("#message-error").show();
            }
        });
    }

