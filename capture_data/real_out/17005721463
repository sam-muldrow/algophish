           sideScroll(scrollRow, direction, 10, distance, step);\r\n            }\r\n            function sideScroll(element, direction, speed, distance, step) {\r\n                let scrollAmount = 0;\r\n                const slideTimer = setInterval(() =\u003E {\r\n                    if (direction === 'left') {\r\n                        element.scrollLeft -= step;\r\n                    } else {\r\n                        element.scrollLeft += step;\r\n                    }\r\n                    scrollAmount += step;\r\n                    if (scrollAmount \u003E= distance) {\r\n                        window.clearInterval(slideTimer);\r\n                    }\r\n                }, speed);\r\n            }\r\n        }, 100);\r\n    })();\r\n\u003C\u002Fscript\u003E","type":"html-with-script","node_title":"minty - xscroll carousels common","uuid":"8c2fd46c-61d8-4efd-9ac4-2cc54f4ce370"},{"html":"\u003Cstyle\u003E\r\n    .minty .x-scroll.top-carousel .product-row {\r\n        grid-template-columns: repeat(20, 1fr);\r\n    }\r\n    .minty .x-scroll.items-11 .product-row {\r\n        grid-template-columns: repeat(11, 1fr);\r\n    }\r\n    .minty .x-scroll.items-8 .product-row {\r\n        grid-template-columns: repeat(8, 1fr);\r\n    }\r\n    .minty .x-scroll.items-9 .product-row {\r\n        grid-template-columns: repeat(9, 1fr);\r\n    }\r\n    .minty-hp .x-scroll.top-carousel {\r\n        margin-bottom: 0;\r\n    }\r\n    .minty-hp .x-scroll.top-carousel .product-grid {\r\n        margin-left: 2.2vw;\r\n    }\r\n    .minty-hp .product-grid.top-carousel .scroll-header header h2 {\r\n        margin-right: -13.25rem;\r\n        font-size: 2vw;\r\n    }\r\n    .minty.minty-hp .x-scroll a.buttoncta.rounded {\r\n        font-size: 0.875rem;\r\n        font-family: Mier-A, sans-serif;\r\n        padding: 0.5rem;\r\n        color: #666;\r\n        background-color: #fff;\r\n        border-radius: 1.5rem;\r\n        min-width: 0;\r\n        text-transform: none;\r\n    