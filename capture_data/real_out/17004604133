function initHighlights(){
let deferred = $.Deferred();
var template = $('#highlights-template').html();
var placeholder = $(".hm-cmp");
$(".hm-cmp").css("display","none");
var rendered = Mustache.render(template, HighlightsCampaigns);
placeholder.html(rendered);
deferred.resolve(true);
return deferred.promise();
}
$(window).on("load",function(){
initHighlights().then(function () {
if (typeof replaceFirstElementHighlight !== "undefined" )
replaceFirstElementHighlight(false);
});
$(".hm-cmp").css("display","");
checkFlexibility();
if(typeof executeAfterHightlightsCampaignsIsLoaded !== "undefined"){
executeAfterHightlightsCampaignsIsLoaded();
}
