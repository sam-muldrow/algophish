$(document).ready(function() {
if (window.innerWidth < 640) {
$('.round , .round + .boton-hazte-client').hide();
$('.round ').prev().hide();
$('.boton-hazte-client').not('.round').hide();
$('.container-hazte-cliente').not('.round').show();
$('.boton-hazte-client.round.bg-Yellow').show();
} else {
$('.container-hazte-cliente').not('.round').hide().prev().hide();
$('.round').show();
}
});
</script>
