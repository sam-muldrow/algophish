<script type="text/javascript">
$(document).ready(function(){
$(".center-carousel .slick-slide").find('a').click(function(e) {
if(!$(this).parents('.slick-slide').hasClass('slick-active')) {
e.preventDefault();
let slideno = $(this).parents('.slick-slide').attr('data-slick-index');
//console.log(slideno);
$('.slick-initialized').slick('slickGoTo', slideno);
}
});
});
</script>
