<script type="text/javascript">
$(document).ready(function(){
if($('head').data('dynamic') || (typeof neoCurrentChannelDynamic !== 'undefined' && neoCurrentChannelDynamic )){
let isMetaVelocityLanguage = false;
$("head [rel='alternate']").each(function () {
if ($( this ).is("[hreflang]"))
isMetaVelocityLanguage = true;
});
if(isMetaVelocityLanguage) {
$('#links-top .links-group-panel-links ul li').each(function() {
var lang = $( this ).find( "a" ).attr('hreflang');
var hlink=$('head link[rel="alternate"][hreflang='+lang+']');
if(typeof lang !== 'undefined' && typeof hlink !== 'undefined' && hlink.length==0){
$( this ).remove();
} else if(typeof hlink !== 'undefined' && hlink.attr('href')) {
$(this).find( "a" ).attr('href',hlink.attr('href'));
}
});
if($('#links-top .links-group-panel-links ul li').length==0){
$('#links-top').hide();
}
}
}
