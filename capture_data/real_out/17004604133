cookiePresearch = true;
}
} else cookiePresearch = true;
if (cookiePresearch && (typeof presearchLoaded === "undefined" || presearchLoaded === false)) {
presearchLoaded = true;
let opt = {
nSearch2Show: 5,
nDays2Expire: 30
};
jQuery.getScript("/deployedfiles/common/JavaScript/PresearchFunctions/presearch-functions.js")
.done(function () {
LastSearches.init(opt);
}).fail(function () {
console.log("error get the file presearch-functions.js");
});
jQuery('<link/>', {
rel: 'stylesheet',
type: 'text/css',
href: '/deployedfiles/common/JavaScript/PresearchFunctions/presearch-functions.css'
}).appendTo('head');
}
if (typeof varSpeechScrip === 'undefined' || varSpeechScrip === false) {
varSpeechScrip = true;
jQuery.getScript("/deployedfiles/common/R2016/Estaticos/js/jquery.speech.js")
.done(
function () {
jQuery.getScript("/deployedfiles/common/R2016/Estaticos/js/speechScript.js")
.fail(function () {
console.log("error get the file speechScript.js");
});
})
.fail(function () {
console.log("error get the file jquery.speech.js");
});
}
}
</script>
