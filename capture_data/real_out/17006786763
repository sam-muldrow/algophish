<script type="text/javascript">
	function mobileModeUrl(isTopic) {
	    var currentUrl = window.location.href;
	    var mobileModeUrl = currentUrl.replace("forcePcVersion=true", "forcePcVersion=false");
	    if (isTopic) {
	    	mobileModeUrl = mobileModeUrl.replace("topicNews.ksn", "m/topic.ksn");
	    }
	    window.location.href = mobileModeUrl;
	}
	jQuery(".MdGHD03Search").SearchBox({});
	jQuery(".MdSB08Tab").Category({});
</script>



