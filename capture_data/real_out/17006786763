	<script type="text/javascript" charset="utf-8">
		$(".MdGHD03Search").SearchBox();
		function trim(str) {
		    return str.replace(/^(\s|　)+|(\s　)+$/g,"");
		}
		function validation() {
			var searchWord = $("#searchWord").val();

			searchWord = trim(searchWord);
			if (searchWord.length < 2) {
				alert('二文字以上入力してください。');
				return false;
			}
			re = /[-~!@#$%^&*\()\=+_;,/?"'’”‘。…\s]/gi;
			var testSearchWord = searchWord.replace(re, '');
			if (!testSearchWord){
			    alert('特殊文字のみの入力はできません。');
				return false;
			}
			if (testSearchWord.length < 2) {
			    alert('特殊文字以外に、二文字以上入力してください。');
			    return false;
			}
			$("#searchWord").val(searchWord);
			return true;
		}
		function searchClick() {
		    if (validation()) {
		        $("#searchForm").submit();
		    }
		}

		var SUGGEST_URL = "/api/recommendTopic.ksn";
	    if (typeof suggestOption == "undefined") {
	        suggestOption = {Url : SUGGEST_URL, domain: "https://news.kstyle.com", param : {official: 1}};
	    }
    </script>
    <script type="text/javascript">
		$(function(){
	        nj.kNews.suggest.init(suggestOption);
	    });
	</script>

