                function showad(){
               		var new_timestamp = Math.round(new Date() / 1000);
 			console.log('new_time:' + new_timestamp)
			//2019-10-18 6:0:0?1?72019-10-19 22:0:0靠
			var start_time = Math.round((new Date('2022-05-02 0:0:0')).getTime() / 1000);
			var end_time = Math.round((new Date('2022-05-09 23:59:59')).getTime() / 1000);
                     	console.log(start_time+'-----'+end_time)
			if(new_timestamp >=start_time && new_timestamp<=end_time ){
                     if(c_start==-1){
                        adId.style.display="block";
                        setTimeout(hidead, 7000);
              // //     document.cookie="ad_isshow_ad1";
                        setCookie("ad_isshow_ad1","true",1);
                     }
            }
		}
showad();
                 function hidead(){
                     adId.style.display="none";
                    // ming 17/11/2018:
//                     document.getElementById('bottomAd').style.display='block';
               }
            function setCookie(cname,cvalue,exdays){
			          var d = new Date();
			          d.setTime(d.getTime()+(exdays*4*60*60*1000));
			         var expires = "expires="+d.toGMTString();
			         document.cookie = cname + "=" + cvalue + "; " + expires;
			      }
             closeAd.onclick = function(){
                    hidead();

                //setTimeout(showad, 1000);

              };

            document.getElementById('adClose2').onclick = function() {
                document.getElementById('bottomAd').style.display = 'none'
            };

       }


