<script>

    function navApp() {
        var imgLike = document.getElementsByClassName("nav-app")[0];
        imgLike.onmouseout = function () {
            document.getElementsByClassName("app-code")[0].style.display = 'none';
        };
        imgLike.onmouseover = function () {
            document.getElementsByClassName("app-code")[0].style.display = 'block';
        }

        var personLike = document.getElementsByClassName("username")[0];
        personLike.onmouseover = function () {
            document.getElementsByClassName("person-operation")[0].style.display = 'block';
        }

        var personOptions = document.getElementsByClassName("person-operation")[0];
        personOptions.onmouseleave = function () {
            document.getElementsByClassName("person-operation")[0].style.display = 'none';
        };
    }
</script>

