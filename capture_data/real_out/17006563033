        $(document).ready(function () {

            // 尝试获取用户信息
            let uid = window.localStorage.getItem('uid')
            let username = window.localStorage.getItem('username')

            if (uid && username)
            {
                $('.username').html(username + '<svg  viewBox="64 64 896 896" focusable="false" data-icon="down" width="1em" height="1em" fill="currentColor" aria-hidden="true" style="vertical-align: middle;"><path d="M884 256h-75c-5.1 0-9.9 2.5-12.9 6.6L512 654.2 227.9 262.6c-3-4.1-7.8-6.6-12.9-6.6h-75c-6.5 0-10.3 7.4-6.5 12.7l352.6 486.1c12.8 17.6 39 17.6 51.7 0l352.6-486.1c3.9-5.3.1-12.7-6.4-12.7z"></path></svg>')
                $('.not_login').css('display','none')
                $('.login').css('display','inline')
                $('.main-yibuluo-person').attr('href','https://www.yeeyi.com/user/profile/'+uid)
            }
            $('.login_out').click(function () {
                window.localStorage.removeItem('uid')
                window.localStorage.removeItem('username')
                window.localStorage.removeItem('authcode')
                window.localStorage.removeItem('tel')
                location.reload();
            })

            $('.box_skitter_large').skitter({
                theme:
                    'clean', numbers_align: 'right', progressbar: true, dots: false, preview:
                    false
            });
            $("img.lazy").lazyload({effect: "fadeIn"});

            // $('#login_place').load('addon/checklogin.php');

            var t;
            $(".forecast").hover(function () {
                clearInterval(t);
            }, function () {
                t = setInterval(function () {
                    var ul = $(".list");
                    var liHeight = ul.find("li:last").height();
                    ul.animate({marginTop: liHeight + "px"}, 1000, function () {
                        ul.find("li:last").prependTo(ul);
                        ul.find("li:first").hide();
                        ul.css({marginTop: 0});
                        ul.find("li:first").fadeIn(800);
                    });
                }, 3000);
            }).trigger("mouseleave");
            var winWidth = $(window).width();
            var _pagewidth = 1020;
            $backToTopFun = function () {
                //var st = $(document).scrollTop(), winh = $(window).height();
                var winw = window.innerWidth;
                if (winw < _pagewidth) {
                    $('.adsboth').hide();
                }
                if (winw > _pagewidth) {
                    $('.adsboth').show();
                    _left = winw / 2 - _pagewidth / 2 - 100;
                    _right = winw / 2 + _pagewidth / 2 - 5;
                    $('#lovexin12').css("left", _left + "px");
                    $('#lovexin16').css("left", _left + "px");
                    $('#lovexin14').css("left", _right + "px");
                    $('#lovexin18').css("left", _right + "px");
                }
            }

            $(window).bind("scroll", $backToTopFun);
            $("body").bind("scroll", $backToTopFun);
            $(window).bind("resize", $backToTopFun);
            $("body").bind("resize", $backToTopFun);
            $(function () {
                $backToTopFun();
            });

            let cityId = window.localStorage.getItem('cityFilter');
            let cityName  = '';
            switch (cityId) {
                case '1':
                    cityName = 'melbourne';
                    break
                case '2':
                    cityName = 'sydney';
                    break
                case '3':
                    cityName = 'gold-coast';
                    break
                case '4':
                    cityName = 'brisbane';
                    break
                case '5':
                    cityName = 'adelaide';
                    break
                case '6':
                    cityName = 'canberra';
                    break
                case '7':
                    cityName = 'perth';
                    break
                case '8':
                    cityName = 'darwin';
                    break
                case '10':
                    cityName = 'hobart';
                    break
                case '11':
                    cityName = 'others';
                    break
                case '12':
                    cityName = 'wollongong';
                    break
                case '13':
                    cityName = 'central-coast';
                    break
                case '14':
                    cityName = 'geelong';
                    break
                case '15':
                    cityName = 'ballarat';
                    break
                default:
                    cityName = 'melbourne';
                    break;
            }
            $('#nav a').each(function(){
                switch ($(this).text()) {
                    case '分类信息':
                        $(this).attr('href','https://www.yeeyi.com/classified-advertisement/au/'+cityName);
                        break;
                    case '生活服务':
                        $(this).attr('href','https://www.yeeyi.com/local-service/au/'+cityName);
                        break;
                    case '超级市场':
                        $(this).attr('href','https://www.yeeyi.com/classified-advertisement/au/'+cityName+'/supermarkets/');
                        break;
                    case '求职招聘':
                        $(this).attr('href','https://www.yeeyi.com/classified-advertisement/au/'+cityName+'/jobs/');
                        break;
                    case '车辆交易':
                        $(this).attr('href','https://www.yeeyi.com/classified-advertisement/au/'+cityName+'/car-sale/');
                        break;
                    case '打折团购':
                        $(this).attr('href','https://www.yeeyi.com/classified-advertisement/au/'+cityName+'/groupbuy-discount/');
                        break;
                    case '机场接送':
                        $(this).attr('href','https://www.yeeyi.com/local-service/au/'+cityName+'/airport-transfer/');
                        break;
                    case '房屋交易':
                        $(this).attr('href','https://www.yeeyi.com/classified-advertisement/au/'+cityName+'/property-sale/');
                        break;
                    case '生意买卖':
                        $(this).attr('href','https://www.yeeyi.com/classified-advertisement/au/'+cityName+'/business-sale/');
                        break;
                    case '二手市场':
