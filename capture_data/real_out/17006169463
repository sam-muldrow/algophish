<!-- page type = ARTICLE --><!-- page id = 6443fe54f3a98810VgnVCM1000001f81b40aRCRD --><script>
const locale =
        document.querySelector('html').getAttribute('lang') === 'fr-CA'
            ? 'fr_CA'
            : 'en_CA';

    const isEnglish = (locale === "en_CA");
    const url = "/json/currentRates.json";

    const PRODUCTS_CODE = {
        gic18month: '3500_1.5_CAD'
    };

    (function ($) {
        $(document).ready(function () {
            const selectors = $("[data-toggle=rate]");
            //fetch rates from currentRates.json if a page has data-toggle attributes on any of the elements
            if (selectors && selectors.length > 0) {
                $.get(url).then(function (response) {
                    getProductRatesAndInjectValues(selectors, response.rates, response.exchange_rates);
                })
            }
        });



        function getProductRatesAndInjectValues(selectors, rates, exchangeRates) {
            const productRates = mapProductsRatesDates(rates, exchangeRates);
            selectors.each((index) => {
                const element = selectors[index];
                const elemAttrs = {};
                for (const attr of element.attributes) {
                    elemAttrs[attr.name.replace("data-", "")] = attr.value;
                }
                let rateObj;
                console.log(PRODUCTS_CODE[elemAttrs.type]);
                rateObj = productRates[PRODUCTS_CODE[elemAttrs.type]];
                console.log(rateObj);

                if (rateObj) {
                    if (elemAttrs.toggle === "rate") {
                        element.innerHTML = isEnglish ? rateObj["rate_en"] : rateObj["rate_fr"]
                    }
                }
            })
        }

        function mapProductsRatesDates(rates, exchangeRates) {
            const produtsMapObj = {};
            rates.forEach((product) => {
                //handle prime rate exception
                if (product.group === "prime_rate") {
                    produtsMapObj["PRIME"] = product;
                }
                if (product.apr_value_en || product.apr_value_fr) {
                    produtsMapObj["Mortgage_CAD"] = product;
                }
                //populate the rateKey in format : PRODUCT_TERM_CURRENCY. Ex: 3000_1_CAD
                let rateKey = product.profile_product_type || product.group;
                const rateTerm = !!product.min_rate_balance ? product.min_rate_balance : product.term;
                rateKey = !!rateTerm ? `${rateKey}_${rateTerm}_${product.currency}` : `${rateKey}_0_${product.currency}`;
                produtsMapObj[rateKey] = product;
            });
            exchangeRates.forEach((product) => {
                // handling exchange rates on currentRates.json
                if (!!product.buy && product.sell) {
                    const productTerm = !!product.term ? product.term : "0";
                    const buyProduct = JSON.parse(JSON.stringify(product));
                    buyProduct["rate_en"] = product["buy_rate_en"];
                    buyProduct["rate_fr"] = product["buy_rate_fr"];
                    produtsMapObj[`Buying_${productTerm}_${product.currency}`] = buyProduct;

                    const sellProduct = JSON.parse(JSON.stringify(product));
                    sellProduct["rate_en"] = product["sell_rate_en"];
                    sellProduct["rate_fr"] = product["sell_rate_fr"];
                    produtsMapObj[`Selling_${productTerm}_${product.currency}`] = sellProduct;

                }
            });
            return produtsMapObj;
        }
    })(jQuery);
</script>
