<script type="text/javascript">
    $(document).ready(function () {
        userid = '0';
        $('#feedbacksubmit').click(function (e) {
            $('#feedbacksubmit').prop('disabled', true);
            e.preventDefault();
            feedbacksubmission(e);
        });
        $('#feedbackdiv').click(function () {
            $('#feedbackformin')[0].reset();
            $('#feedbacksubmit').prop('disabled', false);
            $('#feedback_overlay').show();
            $('#thanksfeedback').hide();
            $('#errormsg').hide();
            $('#feedbackformin').show();
            $('#feedbackform').show();
        });
        $('#feedbackdivclose').click(function () {
            createCookie('feedbackflag', 1, 15);
            $('#feedback_overlay').hide();
            $('#feedbackform').hide();
        });
        feedbackcookiecheck = readCookie('feedbackflag');
        if (feedbackcookiecheck != 1 && feedbackcookiecheck != '1') {
//            setTimeout(function () {
//                $('#feedbackdiv').click();
//            }, 45000);
        }
//        $("#feedbackdiv").click(function () {
//            $('html, body').animate({
//                scrollTop: $("#feedbackform").offset().top-200
//            }, 500);
//        });
    });

    function validateUser() {
        var email = $('#txtSignupEmail').val();
        var mob = $('#txtSignupMobile').val();
        //alert(mob);
        var mobFilter = /^[7-9][0-9]{9}$/;
        var emailFilter = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i;
        if (emailFilter.test(email)) {
            if (mobFilter.test(mob)) {
                return;
            } else {
                $('#errormsg').html('Enter a valid Mobile number');
                $('#errormsg').show();
                setTimeout(function () {
                    $('#errormsg').hide();
                }, 5000);
                $('#txtSignupMobile').focus();
                return false;
            }

        } else {
            $('#errormsg').html('Enter a valid Email id');
            $('#errormsg').show();
            setTimeout(function () {
                $('#errormsg').hide();
            }, 5000);
            $('#txtSignupEmail').focus();
            return false;
        }
    }

    function slideMsg(goDown, toShow) {
        var msg = toShow;
        msg = '#' + msg;
        var ihtml = $(msg).html();
        if (ihtml == '+ Add a Comment') {
            javascript:slideContent(goDown);
            $(msg).html('- Add a Comment');
            return;
        } else {
            javascript:slideContent(goDown);
            $(msg).html('+ Add a Comment');
            return;
        }
    }

    function feedbacksubmission(e) {
        $('#errormsg').html('');
        $('#errormsg').hide();
        emailid = '';
        mobileno = '';
        if (userid == '' || userid == undefined || userid == '0') {
            uservalidation = validateUser();
            if (uservalidation == false) {
                $('#feedbacksubmit').prop('disabled', false);
                return false;
            }
            emailid = $('input[name=txtSignupEmail]').val();
            mobileno = $('input[name=txtSignupMobile]').val();
        }
        question1 = $('input[name=websiteLook]:checked').val();
        question2 = $('input[name=dealQuality]:checked').val();
        question4 = $('input[name=siteFun]:checked').val();
        var checkboxArr = $('input[name=support]:checked').map(function ()
        {
            return $(this).val();
        }).get();
        question5 = encodeURIComponent(checkboxArr.join());
        question6 = $('#txtopinion').val();
        if (question1 == '' || question1 == undefined) {
            $('#errormsg').html('Please attempt question 1');
            $('#errormsg').show();
            setTimeout(function () {
                $('#errormsg').hide();
            }, 5000);
            $('#feedbacksubmit').prop('disabled', false);
            return false;
        }
        if (question2 == '' || question2 == undefined) {
            $('#errormsg').html('Please attempt question 2');
            $('#errormsg').show();
            setTimeout(function () {
                $('#errormsg').hide();
            }, 5000);
            $('#feedbacksubmit').prop('disabled', false);
            return false;
        }
        if (question4 == '' || question4 == undefined) {
            $('#errormsg').html('Please attempt question 3');
            $('#errormsg').show();
            setTimeout(function () {
                $('#errormsg').hide();
            }, 5000);
            $('#feedbacksubmit').prop('disabled', false);
            return false;
        }
        if (question5 == '' || question5 == undefined) {
            $('#errormsg').html('Please attempt question 4');
            $('#errormsg').show();
            setTimeout(function () {
                $('#errormsg').hide();
            }, 5000);
            $('#feedbacksubmit').prop('disabled', false);
            return false;
        }
        if (question6 != '' && question6.length > 1000) {
            $('#errormsg').html('Feedback cannot be more than 1000 characters');
            $('#errormsg').show();
            setTimeout(function () {
                $('#errormsg').hide();
            }, 5000);
            $('#feedbacksubmit').prop('disabled', false);
            return false;
        }
        var dataStr = "ajax=1&websiteLook=" + question1 + "&dealQuality=" + question2 + "&siteFun=" + question4 + "&support=" + question5 + "&txtopinion=" + escape(question6) + "&txtSignupEmail=" + emailid + "&txtSignupMobile=" + mobileno;

        $.ajax({
            url: homepageurl + "feedback-landing.php",
            data: dataStr,
            type: "POST",
            success: function (data) {
                if (data == 'SUCCESS') {
                    $("#thanksfeedback").show();
                    $("#feedbackformin").hide();
                    $('#feedbackformin')[0].reset();
                    $('#feedbacksubmit').prop('disabled', false);
//                    setTimeout(function () {
//                        $('#feedback_overlay').hide();
//                        $('#feedbackform').hide();
//                    }, 10000);
                } else {
                    $('#errormsg').html('Please check your form again');
                    $('#errormsg').show();
                    setTimeout(function () {
                        $('#errormsg').hide();
                    }, 5000);
                    $('#feedbacksubmit').prop('disabled', false);
                    return false;
                }
            }
        });
    }

</script><!--Replaced scrollTop-->
