<script type="text/javascript">
    $(document).ready(function () {
        $('#submitlink').click(function (e) {
            e.preventDefault();
            submitAppLink();
        });
    });
    function submitAppLink() {
//        e.preventDefault();
        var mobno = $.trim($('#appsmsmobileno').val());
        if (mobno.charAt(0) == '+' && mobno.charAt(1) == '9' && mobno.charAt(2) == '1') {
            mobno = mobno.substring(3);
        } else if (mobno.charAt(0) == '0') {
            mobno = mobno.substring(1);
        }
        if (mobno == '' || mobno == 'Enter your mobile no') {
            alert('Please enter your mobile no.');
            $('#appsmsmobileno').val('');
            $('#appsmsmobileno').focus();
            return false;
        } else {
            if (!(mobno.match(/^[789]\d{9}$/)))
            {
                alert('Please enter valid mobile no!');
                $('#appsmsmobileno').val('');
                $('#appsmsmobileno').focus();
                return false;
            }

            if ((mobno.length < 10 || mobno.length > 10))
            {
                alert('Please enter valid mobile no!');
                $('#appsmsmobileno').focus();
                $('#appsmsmobileno').val('');
                return false;
            }
        }

        var queryString = "actionFlag=1&mobileno=" + encodeURI(mobno)+'&page=1';  // alert(queryString);
        $.ajax({url: "https://www.mydala.com/mobile-app.php", async: false, data: queryString, type: "POST",
            success: function (data) {
                //alert(data);
                $('#tnxText').show();
                $('#inputContainer').hide();
            }
        });
    }

    function showContainer() {
        $('#tnxText').hide();
        $('#appsmsmobileno').val('');
        $('#inputContainer').show();
    }

</script>
