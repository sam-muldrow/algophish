<script type="text/javascript">
    function check1(t, userMobileNo) {
        if (t) {
            var n = $.trim(t.input);
            var type = $.trim(t.type);
        }
        if (!n)
            n = '';
        $.ajax({
            url: homepageurl + 'signuppopup.php',
            success: function (data) {
                $('#facebox').html("<div class='popup'>"+data+"</div>");
                if (n == '' && userMobileNo == undefined) {

                } else {

                    if (userMobileNo == undefined && n != '')
                    {
                        if (type == 'email') {
                            $('#emailid23').attr('value', n);
                        } else if (type == 'mobile') {
                            $('#mobile23').attr('value', n);
                        }
                    } else {
                        $('#emailid23').attr('value', t);
                        $('#mobile23').attr('value', userMobileNo);
                        $('#mobile23').attr('disabled', 'disabled');
                    }
                }

            }
        });
    }
    //  }
</script>
