    function imgGallery() {
      $("#level-1-inspiration-gallery .pimg").each(function () {
        var heightGallery = $("#level-1-item-gallery img").height() + 30;
        $(this).css("height", heightGallery + "px");
      });
    }
    imgGallery();
    window.onresize = function () {
      imgGallery();
    };
    imgGallery();
    $("#level-1-item-gallery, #level-1-inspiration-gallery").css(
      "animation-name",
      "opacitymenu"
    );
  });
