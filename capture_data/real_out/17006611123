        // Callback function to execute when mutations are observed
        var callback = function(mutationsList) {

        updateBlancTournant();
        forceIcon1hVisibleOnList();
        }; // callback
        
        // Create an observer instance linked to the callback function
        const observerBlancTournant = new MutationObserver(callback);
        
        // add an observer if infinite scroll on search result content
        function monitorInfiniteScrool() {
        var targetNode = document.getElementsByClassName('search-result-content');
        if (targetNode.length > 0) { // on est sur une liste
        
        var targetNode2 = document.getElementsByClassName('product-listing');
        if (targetNode2.length > 0) { // je monitore le container de la liste
        // Options for the observer (which mutations to observe)
        var config = { attributes: false, childList: true, subtree: true };
        // Start observing the target node for configured mutations
        observerBlancTournant.observe(targetNode2[0], config);
        } // if targetnode2
        } //if targetnode
        }
        
        
        function updateBlancTournant() {
        // console.log("updateBlancTournant");
        var nopadding = ["A","Z","T", "U"];
        var cssselectors = [  // attention, l'ordre est important
        { cssstring: "div.content-slot div.product-univers  img", padding:1.5, padding_except:0.75,  paddingunit:"rem"},                                   // Slider univers sur page master
        { cssstring: "div.univers-swiper  img", padding:1.5, padding_except:0.75 , paddingunit:"rem"},                                  // swiper univers dans fiches produits
        { cssstring: "ul.search-result-items  img", padding:3, padding_except:1,  paddingunit:"rem"},                               // Product listing page
        { cssstring: "div.product-univers  img", padding:3, padding_except:0,  paddingunit:"rem"},                                   // Slider univers sur page master
        { cssstring: "div.product-thumbnails img", padding:1,  padding_except:0 , paddingunit:"rem"},
        { cssstring: "div.pdp-main-slider img", padding:5 , padding_except:5, paddingunit:"rem"},                                    // slider des grosses photos sur les fiches produits
        { cssstring: "div.cart-row-content  img", padding:2, padding_except:0,  paddingunit:"rem"},                                  // produits dans le caddie
        { cssstring: "div.cart-favorites-swiper  img", padding:2, padding_except:0,  paddingunit:"rem"},                                  // swipper wishlist dans la page fr-fr panier
        ];
        
        for (j=0; j < cssselectors.length; j++) { // pour le tableau des selectors css
        elements = document.querySelectorAll(cssselectors[j].cssstring);
        for (i = elements.length - 1; i > -1; i--) { // pour toutes les images trouvées
        if (elements[i].hasAttribute("blanctournant")) {
        break;
        }
        if ( ( elements[i].hasAttribute("src") || elements[i].hasAttribute("data-src") ) && !(elements[i].hasAttribute("blanctournant"))) {
        var attrib=null;
        if (elements[i].hasAttribute("src")) {
        attrib =  elements[i].getAttribute("src");
        }
        if (elements[i].hasAttribute("data-src")) {
        attrib =  elements[i].getAttribute("data-src");
        }
        var arr = attrib.match(/-([0-9]*)-([A-Z])+-([0-9]*)\.(png|jpg)/);  // si le src de l'image est bien une image produit
        if (arr != null) {
        if (nopadding.indexOf(arr[2]) > -1 ) { // si le type est dans la liste des types qui doivent ne pas avoir de paddings
        elements[i].style.padding = 0;     // je mets un padding à 0
        }
        else {
        
        var pad = cssselectors[j].padding.toString() + cssselectors[j].paddingunit
        if (elements[i].src.toLowerCase().match(/assiette.*dessert/)) {
        var pad1 =  cssselectors[j].padding + cssselectors[j].padding_except;
        pad = pad1.toString() + cssselectors[j].paddingunit
        }
        elements[i].style.padding = pad; // sinon, je mets le padding prevu dans le tableau des selectors
        } // fin nopadding.indexOf
        elements[i].setAttribute('blanctournant','done');
        // si le ref est  ... truc muche .... tu peux faire qq chose avec le <img src ou le div parent
        // console.log("image ref produit : " + i + " : " + arr[1]);
        /*  if (arr[1] == "25882902") {
        var atag = elements[i].parentNode;
        atag.style.backgroundColor = "#1e4b5f";
        } */
        } // fin arr != null
        }// fin hasAttribute("src")
        
        } // fin for for(i)
        }//  fin for(j)
        
        
        preloadPlaceHolderLazyloading();
        
        observerlozad.observe();
        var nbvousavezvu = document.querySelectorAll('p[id="vousavezvu"]').length;
        if ( nbvousavezvu == 0) {
        try {
        if (document.querySelectorAll('p[id="vous avez vu"]').length < 1)  {
        nbres = document.querySelectorAll("p.nb-results")[0].innerText.replace(/[^0-9]/gi,"");
        nbpdt = document.querySelectorAll("ul[id='search-result-items'] > li").length;
        divplusdepdt = document.querySelectorAll("div.simplemore")[0];
        buttonplusdeproduct = document.querySelectorAll("button.show-more-products")[0];
        
        var p = document.createElement("P");
        p.setAttribute("id","vousavezvu");
        p.setAttribute("style","text-align:center; margin-bottom: 30px; margin-top:30px; font-size: 1.5rem");
        var t = document.createTextNode("vous avez vu " + nbpdt + " articles sur  " + nbres + "");
        p.appendChild(t);
        
        divplusdepdt.insertBefore(p, buttonplusdeproduct);
        // en cas de clic sur le bouton, masquer ce petit texte
        $("button.show-more-products").click(function() {
        // console.log("bouton clic");
        $("#vousavezvu").hide();
        });
        
        }
        }
        catch(err) { };
        }
        
        
        
        }
        
        function preloadPlaceHolderLazyloading() {
        // pre chargement des images des PLP avec un pixel transparent pour faire un placeholder
        //
        
        
        if (!('IntersectionObserver' in window)) { return ;}
        
        var cssselectors = [
        { cssstring: "div.swiper-slide img.lozad:not([src])", src:"data:image/gif;base64,R0lGODlhAQABAIAAAP///wAAACH5BAEAAAAALAAAAAABAAEAAAICRAEAOw=="},
        { cssstring: "div.swiper-slide-next img.lozad[src='data:image/gif;base64,R0lGODlhAQABAIAAAP///wAAACH5BAEAAAAALAAAAAABAAEAAAICRAEAOw==']",   src:"/on/demandware.static/-/Library-Sites-Alinea-Shared/default/v7f8c8932ea298ece590bf7efa1efdafd5f7e2ef9/navigation/loading_alinea.gif?version=1.543.505.802.000"},
        
        ];
        
        for (j=0; j < cssselectors.length; j++) { // pour le tableau des selectors css
        elements = document.querySelectorAll(cssselectors[j].cssstring);
        for (i = 0; i < elements.length; i++) { // pour toutes les images trouvées
        
        if (elements[i].hasAttribute("data-src")) {
        s=document.cookie;
        if ( window.innerWidth< 1024  || s.indexOf("gridmode=alternate") > 0 ) {
        var lg = "256";
        if (window.innerWidth < 500 && s.indexOf("gridmode=alternate") < 0) { lg= "126"; }
        var x =  elements[i].getAttribute("data-src") ;
        var low_res = x.replace(/(sw|sh)=[0-9]*/gi,"$1=" + lg);
        elements[i].setAttribute("src",cssselectors[j].src);
        elements[i].setAttribute("data-src", low_res);
        }
        }
        }
        }
        }
        
        
        function updateLinksStaging() {
        var loc=window.location.href;
        if (loc.startsWith("https://staging-eu01-alinea.demandware.net")) {
        console.log("updateLinksStaging -> we are on staging environment");
        elements = document.querySelectorAll('a');
        for (i = 0; i < elements.length; i++) {
        if (elements[i].hasAttribute("href")) {
        var href = elements[i].href;
        // console.log("gotcha : "  + href);
        if (href== "https://staging-eu01-alinea.demandware.net/s/ALI_FR/fr-fr/home") {
        elements[i].href="https://staging-eu01-alinea.demandware.net/on/demandware.store/Sites-ALI_FR-Site";
        }
        else if (!(href.startsWith("https://staging-eu01-alinea.demandware.net/s/ALI_FR"))) {
        if (href.startsWith("https://staging-eu01-alinea.demandware.net/fr-fr/")) {
        var newhref = href.replace("https://staging-eu01-alinea.demandware.net/fr-fr/", "https://staging-eu01-alinea.demandware.net/s/ALI_FR/fr-fr/");
        // console.log("updateLinksStaging -> update " + href + " ==> " + newhref);
        elements[i].href=newhref;
        }
        }
        
        }
        }
        }
        }
        
        
        function removeMissingTechnicalImage() {
        elements = document.querySelectorAll('img.technical-img');
        for (i = 0; i < elements.length; i++) {
        if (elements[i].hasAttribute("src")) {
        var href = elements[i].src;
        if (href.endsWith("noimagetechnical.png")) {
        elements[i].style="display: none";
        }
        }
        }
        }
        
        function updateLink3XSansFrais() {
        // console.log("3x sans frais");
        elements = document.querySelectorAll('div.split-price');
        for (i = 0; i < elements.length; i++) {
        // console.log(i + " : 3x sans frais");
        // console.log(i + " html :" + elements[i].innerHTML);
        var html= elements[i].innerHTML;
        var link = "<a href='http://www.alinea.com/fr-fr/media/financement.html#3xsansfrais' style='text-decoration: none' >";
        var img3x = "<img src='https://www.alinea.com/on/demandware.static/-/Sites/default/dwbf1aa083/paymentMethod/pastille%203x.png' style='width: 20px; position:relative; top:2px'>";
        var span = "<span style='font-size: 1.6rem; letter-spacing: -1px;'>";
        elements[i].innerHTML=html.replace(/3 x (.*) €/igm,link  + span + img3x + " $1  €</span></a>");
        }
        }
        
        function updateMerciPourVotreCommande() {
        var x = location.href;
        if (x.includes('.alinea.com/fr-fr/confirmationCommande') ) {
        element = document.querySelector('div.order-confirmation-content > p');
        if (element) {
        element.innerHTML = element.innerHTML + "<br />ou pour IMPRIMER VOTRE FACTURE";
        }
        }
        }
        
