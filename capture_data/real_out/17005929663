    <script>
    function getCookie(name) {
        const cookies = {}
        document.cookie.split('; ').map(item => {
            cookies[item.split('=')[0]] = item.split('=')[1];
        });
        return cookies[name];
    }
    const colorSchemeCookie = getCookie("color_scheme");
    if (!colorSchemeCookie) {
        const systemPreference = window.matchMedia("(prefers-color-scheme: dark)").matches ? "darkmode" : "lightmode";
        document.querySelector("body").classList.add(systemPreference);
    }else{
        document.querySelector("body").classList.add(colorSchemeCookie);
    }
</script>
    
    
    
