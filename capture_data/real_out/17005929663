var googletag = googletag || {};
googletag.cmd = googletag.cmd || [];
googletag.cmd.push(function() {
googletag.pubads().disableInitialLoad();
});

var pbjs = pbjs || {};
pbjs.que = pbjs.que || [];

pbjs.que.push(function() {
pbjs.setConfig({
consentManagement: {
gdpr: {
cmpApi: 'iab',
timeout: 8000,
defaultGdprScope: true
}
},
userSync: {
filterSettings: {
iframe: {
 bidders: ['adform'],
 filter: 'include'
}
}
}
});
pbjs.addAdUnits(adUnits);
pbjs.setConfig({
priceGranularity: customConfigObject
});
pbjs.requestBids({
bidsBackHandler: initAdserver,
timeout: PREBID_TIMEOUT
});
});

function initAdserver() {
if (pbjs.initAdserverSet) return;
pbjs.initAdserverSet = true;
googletag.cmd.push(function() {
pbjs.que.push(function() {
pbjs.setTargetingForGPTAsync();
googletag.pubads().refresh();
});
});
}
// in case PBJS doesn't load
setTimeout(function() {
initAdserver();
}, FAILSAFE_TIMEOUT);    
  
  googletag.cmd.push(function() {
    switch(pageType) {
  case "mainpage":
    googletag.defineSlot('/34550012/Blikk/blikk_970x250_desktop_1', [[970, 500], [728, 90], [970, 250], [970, 90]], 'div-gpt-ad-1672409533463-0').addService(googletag.pubads());
    googletag.defineSlot('/34550012/Blikk/blikk_sky-left_desktop_1', [[120, 600], [160, 600]], 'div-gpt-ad-1672418286968-0').addService(googletag.pubads());
    googletag.defineSlot('/34550012/Blikk/blikk_sky-right_desktop_1', [[120, 600], [160, 600]], 'div-gpt-ad-1672418324126-0').addService(googletag.pubads());
    googletag.defineSlot('/34550012/Blikk/blikk_300x600_desktop_1', [[300, 250], [300, 600]], 'div-gpt-ad-1672417564101-0').addService(googletag.pubads());
    googletag.defineSlot('/34550012/Blikk/blikk_300x600_desktop_2', [[300, 250], [300, 600]], 'div-gpt-ad-1672417613837-0').addService(googletag.pubads());
    googletag.defineSlot('/34550012/Blikk/blikk_300x600_desktop_3', [[300, 250], [300, 600]], 'div-gpt-ad-1672418136074-0').addService(googletag.pubads());
