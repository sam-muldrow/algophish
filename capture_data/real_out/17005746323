    <script type="text/javascript">

      function popoverSignIn() {

        var shownCountCookieName = 'sign_in_shown_count';

        if (shownCount() >= 2 || $('#login-register-page').length > 0) {
          return;
        }

        var $popoverSignIn = $('.popover-sign-in');

        $.cookie(shownCountCookieName, shownCount() + 1, { path: '/' });

        $popoverSignIn.popover('show');

        $('body').on('click', function (e) {
          var $popover = $('.popover');
          if ($popover.has(e.target).length === 0) {
            $popover.popover('hide');
          }
        });

        $popoverSignIn.each(setDuration);

        function setDuration() {
          var $this = $(this)
          var duration = $this.attr('data-duration');
          setTimeout(onTimeout, duration || 10000);

          function onTimeout() {
            $this.popover('hide');
          }
        }

        function shownCount() {
          var count = Number($.cookie(shownCountCookieName));
          return isNaN(count) ? 0 : count;
        }

      }
      $(popoverSignIn)
    </script>

</li>
