            <div id="search" class="search">
	<form id="searchForm" name="searchForm">
		<input
			aria-autocomplete="both"
			aria-describedby="initialInstructions"
			aria-label="Search Keyword or Item Number"
			aria-owns="searchResults"
			autocomplete="off"
			data-bind="attr: {'aria-activedescendant': selectedSuggestionId, 'aria-expanded': isVisible}, value: searchTerm, valueUpdate: ['afterkeydown', 'input'], event: {keydown: onSearchTermKeydown}"
			enterkeyhint="search"
			id="searchField"
			data-testid="search-field-input"
			name="q"
			placeholder="Search keyword or item #"
			type="search"
			value=""
			oninput="checkSearchField()"
		/>
		<div>
			<span aria-label="clear search field" role="button" id="clear-search" onclick="clearSearchField()" onkeypress="keypress(event)" tabindex="0" style="display: none">X</span>
		</div>
		<button aria-label="Search" type="submit" class="btn btn-primary">
			<div aria-hidden="true" class="icon-font icon-magnify"></div><span class="hidden-xs">Search</span>
		</button>
		<div class="collapsing-holder" data-bind="visible: isVisible" style="display: none;">
			<ul data-bind="foreach: suggestions()" id="searchResults" data-testid="search-results" role="listbox" tabindex="-1">
				<li data-bind="attr: {'aria-selected': isSelected, id: id}" role="option">
					<a data-bind="html: name, attr: {class: className, 'data-track-id': dataTrackId, 'data-track-value': $index(), href: link}" tabindex="-1"></a>
				</li>
			</ul>
		</div>
		<span class="sr-only" id="initialInstructions">When autocomplete results are available use up and down arrows to review and enter to select.  Touch device users, explore by touch or with swipe gestures.</span>
		<div aria-live="assertive" class="sr-only"></div>
	</form>
</div>

<script>
	function keypress(e) {
		let keyCode = (e.keyCode ? e.keyCode : e.which);
		if (keyCode == '13' || keyCode == '32') {
			e.preventDefault()
			clearSearchField();
		}
	}
</script>

<script>
	function checkSearchField() {
		if($("#searchField").val() === "") {
			disableClearSearchButton();
		} else {
			enableClearSearchButton();
		}
	}
	function clearSearchField() {
		$('[aria-owns="searchResults"]').val('');
		$("#searchField").trigger("change");
		disableClearSearchButton();
	}
	function disableClearSearchButton() {
		$("#searchField").css("padding-right", "0px");
		$("#clear-search").css("display", "none");
	}
	function enableClearSearchButton() {
		$("#searchField").css("padding-right", "30px");
		$("#clear-search").css("display", "block");
	}
</script>

<script type="text/javascript">
	require(
			['core/search/redux/AutocompleteViewModelSuggestRedux'],
			function (AutocompleteViewModelSuggest) {

			var $search = $('#search');

			// Note: This is required because the initial value for the search term is rendered on the server side via Grails.
			//       I don't want to resort to the hack provided by assets/javascripts/core/ko/bindings/value.es6. It is more
			//       logical to establish initial state for the View Model to match that of the DOM.
			var $input = $search.find('input');
			ko.applyBindings(new AutocompleteViewModelSuggest($input.val()), $search[0]);
			var $form = $search.find('form');
			$form.submit(
				function(e) {
					e.preventDefault();
					var inputValue = $input.val();
					if (inputValue) {
						window.location.href = '/search?search-type=user-generated&' + $input.attr('name') + '=' + encodeURIComponent(inputValue);
					}
				}
			);
		}
	);
</script>

