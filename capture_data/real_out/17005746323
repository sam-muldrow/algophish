<script>
  var timesRun = 0;
  var checkExist = setInterval(function() {
    timesRun++;
    if (timesRun < 10) {
      if ($('#rebates').length) {
        $('#rebates').prependTo('#heading-description')
        $('#heading-description').prepend('<h3>Special Offer<\/h3>')
        $('#rebates').append('<br /><br />')
        clearInterval(checkExist);
      } else {}
    } else {
      clearInterval(checkExist);
    }
  }, 500);
</script>


