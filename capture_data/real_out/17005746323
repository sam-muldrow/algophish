<script>
  var elementCheck = function(element) {
    return new Promise(function(resolve, reject) {
      var timesRun = 0;
      var intervalId = setInterval(function() {
        if (timesRun < 21) {
          if ($(element).length) {
            resolve();
            clearInterval(intervalId);
          }
        } else {
          reject();
          clearInterval(intervalId);
        }
        timesRun++;
      }, 500);
    });
  };
</script>



