});

</script>

<!-- Full Scale Cookies & Features -->
<script>
  let cookies = ["bcp_active__ship_code_updates_enabled"];
  let needsReload = false;
  // loop through cookies
  $.each(cookies, function(cookie, name) {
    if ($.cookie(name) !== "Y") {
      $.cookie(name, "Y", {
        expires: 14,
        path: "/"
      });
      //ensure browser accepts cookies to avoid infinite loop
      if ($.cookie(name)) {
        needsReload = true;
      }
    }
  });
  // reload if needed
  if (needsReload) {
    location.reload();
  }
  


// Email a Gift Message on Signin Page
function showGiftNoticeMessage() {
  $('.login-header').append('<div class="alert alert-success no-icon" role="alert"><div class="alert-body"><p>Sign into your account to send a gift notification to your email address.</p><br><p>Don\'t have an account? Your can register for one below.</div></div>');
}
if (window.location.href.indexOf('gift-notice') > -1) {  
  $(window).bind("load", function() {
    showGiftNoticeMessage();
  });
}  
</script>

