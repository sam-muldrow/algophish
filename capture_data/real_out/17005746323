<script>
function closeCcpaDisclosure () {
  $("#ccpa_notice").hide();
    $.cookie('ccpa_notice_shown', 'Y', { expires: 10000, path: '/' });
}

$(document).ready(function () {
    if ($.cookie('ccpa_notice_shown')) {
        //Do nothing
    } else {
      $("#ccpa_notice").show();
      
      $(document).click(function clickDocument (e) 
      {
          //console.log("CLICK!");
          var container = $("#ccpa_notice");

          // if the target of the click isn't the container nor a descendant of the container
          if (!container.is(e.target) && container.has(e.target).length === 0) 
          {
            closeCcpaDisclosure();
            $(document).unbind( 'click', clickDocument );    
          }
      });
    }
});
</script>
<!-- California Consumer Privacy Act Notice End -->

