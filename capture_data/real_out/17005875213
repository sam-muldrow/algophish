	function movie_blockUI(url)
	{
		var scrW	= document.documentElement.clientWidth;		// 모니터화면에 보이는 브라우져 가로 전체크기
		var scrH	= document.documentElement.clientHeight;	// 모니터화면에 보이는 브라우져 세로 전체크기
		var w		= 900;
		var h		= 600;
		var bw		= (scrW/2) - (w/2);
		var bh		= (scrH/2) - (h/2);

		$.blockUI({
			cursorReset	: 'default',
			message		: "<iframe src='"+url+"?autoplay=1&loop=1' frameborder='0' scrolling='no' width='"+w+"' height='"+h+"' allowtransparency='true'>",
			css			: {
				width	: w,
				height	: h,
				left	: bw,
				top		: bh,
				border	: "1px solid #AAAAAA"
			}
		});

		$('.blockOverlay').attr('title','닫기').click($.unblockUI);
	}

	function pop_blockUIClose()
	{
		$.unblockUI();
