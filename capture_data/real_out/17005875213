<script type="text/javascript">
	function newsTabView(objID){
		for(var i=1;i<=5;i++){
			if(document.getElementById("newsTab"+i) != null) {
				document.getElementById("newsTab"+i).style.display="none";

			}
		}
		document.getElementById(objID).style.display="block";
	}  
</script>
