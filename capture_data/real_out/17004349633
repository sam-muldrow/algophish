    <script>
            var sclass='NC'
            function showLocation(e){
                switch(e.content.address_detail.city){
                    case 'ÄÏ²ýÊÐ': sclass='NC';break;
                    case '¾Å½­ÊÐ': sclass='JJ';break;
                    case 'Ó¥Ì¶ÊÐ': sclass='YT';break;
                    case 'ÉÏÈÄÊÐ': sclass='SR';break;
                    case '¾°µÂÕòÊÐ': sclass='JDZ';break;
                    case 'ÐÂÓàÊÐ': sclass='XY';break;
                    case 'ÒË´ºÊÐ': sclass='YC';break;
                    case 'Æ¼ÏçÊÐ': sclass='PX';break;
                    case '¼ª°²ÊÐ': sclass='JA';break;
                    case '¸§ÖÝÊÐ': sclass='FZ';break;
                    case '¸ÓÖÝÊÐ': sclass='GZ';break;
                    default:sclass='NC';
                }
                var divs = $(".allcity").find('div');
                for(var i=0;i<divs.length;i++){
                    if($(divs[i]).hasClass(sclass)){
                        $(divs[i]).show();
                    }else{
                        $(divs[i]).hide();
                    }
                }
            }
        </script>
        <script src="https://api.map.baidu.com/location/ip?ak=K8E64tu6FWhGYvSc8jDMye6KydZn3DgH&callback=showLocation"></script>
