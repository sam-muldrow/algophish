    var _tc_string = '';
    var _user_interactions = 0;
    __tcfapi('addEventListener', 2, function (tcData, success) {
        if (tcData.gdprApplies === true) {
            if (tcData.eventStatus === "useractioncomplete" || tcData.eventStatus === "tcloaded") {
                ++window._user_interactions;
            }
            if (window._tc_string === '') {
                window._tc_string = tcData.tcString
            } else if (window._tc_string !== tcData.tcString && window._user_interactions > 1) {
                                window.location.reload()
            }
        }
    });
</script>

