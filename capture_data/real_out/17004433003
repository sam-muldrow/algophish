        }

        function checkUrlForBannerTest() {
            if (window.location.hash.indexOf('displayTestBanners')>-1) { document.cookie='displayTestBanners=true; max-age=31536000; path=/'; window.location=window.location.href.substr(0, window.location.href.indexOf('#')); }
            if (window.location.hash.indexOf('hideTestBanners')>-1) { document.cookie='displayTestBanners=true; max-age=0; path=/'; window.location=window.location.href.substr(0, window.location.href.indexOf('#')); }
        }
        window.addEventListener('hashchange', checkUrlForBannerTest, false);
        window.onload = checkUrlForBannerTest;
        if(document.cookie.indexOf('displayTestBanners=true')>-1) { googletag.pubads().setTargeting('a_oth', ['display-test-banners']); }

        googletag.pubads().addEventListener('slotOnload', function (e) {
            var closeAdNonEligible = ["mobile-sticky", "screen-sticky", "outstream-native", "screen-bottom", "mobile-inline"];
            var slotId = e.slot.getSlotElementId();
            if (slotId.indexOf('div-gpt-ad')===-1) return;
            var closeRegEx = /div-gpt-ad-(\w*)-(\w*)-([\w-]*)/gm;
            var closeSlotName = closeRegEx.exec(slotId)[3];
            var adSlot = document.getElementById(slotId);
            var adSlotIframe = adSlot.querySelector('iframe').width;
            if (closeAdNonEligible.indexOf(closeSlotName) == -1 && adSlotIframe > 1 && window.location.hostname.indexOf('www.aktuality.sk') >-1 && adsPag == "article") {
                setTimeout(() => {
                    var divClose = document.createElement("div");
                    divClose.setAttribute('id', `close-ad-${slotId}`);
                    divClose.style = 'text-align: right; margin: 0 auto; display:block;';

                    var closeA = document.createElement('a');
                    closeA.href='https://www.aktuality.sk/plus/?affiliate=header_button&utm_source=header&utm_medium=menu&utm_campaign=header+button';
                    closeA.target='_blank';
                    closeA.setAttribute('id', `a-${slotId}`);
                    closeA.innerText='Zavrieť reklamu';
                    closeA.style='display:inline-block; cursor:pointer; color:#444; background-color:#eceff5; padding: 7px; text-shadow: 0px 1px 0px rgb(255 255 255 / 60%); letter-spacing: .4px; font-family: Arial CE,Arial,Helvetica CEś,Helvetica,sans-serif; text-decoration: none; font-size: 15px;';
                    var checkIfExist = document.getElementById(`a-${slotId}`);
                    if(checkIfExist == null){
                        divClose.appendChild(closeA);
                        adSlot.prepend(divClose);
                        divClose.style.width = adSlotIframe + "px";
                    }

                    divClose.addEventListener('click', function(){
                        (function() {
                            var a = String(Math.floor(Math.random() * 10000000000000000));
                            new Image().src = 'https://pubads.g.doubleclick.net/activity;dc_iu=/31040149/DFPAudiencePixel;ord=' + a + ';dc_seg=6993352392?';
                        })();
