        });

        googletag.pubads().addEventListener('slotRenderEnded', function(e) {if(!e.isEmpty&&e.size[0]===1&&e.size[1]===1&&e.advertiserId!='5038627866'&&e.advertiserId!='4661988891'){document.getElementById(e.slot.getSlotElementId()).style.display='none';}});

