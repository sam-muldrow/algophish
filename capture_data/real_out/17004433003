                                    'event': 'reklama'});
                                document.cookie = "closeAdButton="+closeButtonText+""+";domain=.aktuality.sk"+"; max-age=1800 ; path=/;";
                        }, 1000);
                    })

                }, 200);

            }

        })

        try{if(adsPag&&"article"===adsPag[0]){window.adsKey=[];var rawKeywords=document.querySelector('meta[name="news_keywords"]').content,parsedKeywords=JSON.parse("["+rawKeywords+"]");parsedKeywords.forEach(e=>{adsKey.push(e.title.replace(/\s+/g,"-"))})}}catch(e){console.log(e)};

        ["adsPag", "adsSec", "adsCid", "adsCat", "adsKey", "adsLoc", "adsSeg"].forEach(function (param) {
