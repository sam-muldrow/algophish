$(document).ready(function() {
    /* Ð´Ð»Ñ Ð²Ð½ÐµÐ´ÑÐµÐ½Ð¸Ñ Ð² ÐºÐ¾Ð½ÐºÑÐµÑÐ½ÑÐ¹ Ð¿Ð¾ÑÑ*/
    if ($('#post8456290').length) { // ÐºÐ¾Ð¿Ð¸ÑÑÐµÐ¼ Ð±Ð»Ð¾Ðº Ð² ÑÐµÐ»Ð¾ Ð½Ð¾Ð²Ð¾ÑÑÐ¸
