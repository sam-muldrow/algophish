<script>
document.addEventListener('DOMContentLoaded', function() {
  document.fonts.ready.then( function() {
    var articleTitle = document.querySelector('article div.title');
    var articleLead = document.querySelector('article .lead');
    var articleBreadcrumbs = document.querySelector('.breadcrumb_v2');
    var articlePubDatetime = document.querySelector('.pub_time_date');
    var articleVideoPlayer = document.querySelector('.gl_plugin.player.video_player');
    if (!!articleTitle) articleTitle.classList.add('font-loaded');
    if (!!articleLead) articleLead.classList.add('font-loaded');
    if (!!articleBreadcrumbs) articleBreadcrumbs.classList.add('font-loaded');
    if (!!articlePubDatetime) articlePubDatetime.classList.add('font-loaded');
    if (!!articleVideoPlayer) articleVideoPlayer.classList.add('font-loaded');
  });
})
</script>

        

