    <script>
      (function () {
        var wrap = document.querySelector("#flmp_carousel");
        var viewPort = wrap.querySelector(".opap-carousel-viewport");
        var prevBtn = wrap.querySelector(".previous");
        var nextBtn = wrap.querySelector(".next");
        var dots = wrap.querySelector(".opap-carousel-dots");
        var embla = EmblaCarousel(viewPort, { loop: true, speed: 75, skipSnaps: true, align: 'start', inViewThreshold: 1 });
        var delta = null;

        // auto-play
        var interval = Number(0);
        var autoPlay = null;
        var resetAutoPlay = function () {
          if (interval) {
            if (autoPlay) {
              clearInterval(autoPlay);
            }
            autoPlay = setInterval(function () { if (!document.hidden) { embla.scrollNext() }}, interval);
          }
        }

        // click events
        prevBtn.addEventListener('click', embla.scrollPrev, false);
        nextBtn.addEventListener('click', embla.scrollNext, false);

        Array.prototype.slice.call(dots.children).forEach((n, i) => {
          n.addEventListener("click", () => embla.scrollTo(i), false);
        });

        // change events
        var updateDelta = function () {
          delta = embla.slidesInView().length - 1;
          embla.rootNode().classList.toggle("no-controls", embla.slidesInView().length >= 4);
        }

        var onChange = function () {
          if (delta === null) {
            updateDelta();
          }

          if (embla.slidesInView(true).length <= delta) {
            embla.scrollTo(embla.previousScrollSnap());
          } else {
            dots.children[embla.previousScrollSnap()].classList.remove("is-selected");
            dots.children[embla.selectedScrollSnap()].classList.add("is-selected");
            resetAutoPlay();
          }
        };

        embla.on("init", onChange);
        embla.on("select", onChange);
        embla.on("resize", updateDelta);
      })();
    </script>


		
		




			
			
	
	







	
	
					
				
			
		
	
	


