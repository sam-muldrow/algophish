                <a href="javascript:void(0)" class="opap-carousel-control previous shadow-lg" role="button" aria-hidden="true">
                    <i class="icon-chevron-left"></i>
                    <span class="sr-only">Previous</span>
                </a>
                <a href="javascript:void(0)" class="opap-carousel-control next shadow-lg" role="button" aria-hidden="true">
                    <i class="icon-chevron-right"></i>
                    <span class="sr-only">Next</span>
                </a>
