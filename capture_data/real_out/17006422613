    <script>
        (function () {
            let OPAP_ANNOUNCEMENTS_KEY = 'showSingleAnnouncementDialog';
            let OPAP_ANNOUNCEMENTS_UUID = "1180872";
            window.OPAPNotificationDismiss_com_liferay_asset_publisher_web_portlet_AssetPublisherPortlet_INSTANCE_globalAnnouncements_ = function (toggle) {
                let container = document.querySelector('#_com_liferay_asset_publisher_web_portlet_AssetPublisherPortlet_INSTANCE_globalAnnouncements_announcement');
                // defaults to true;
                let show = sessionStorage.getItem(OPAP_ANNOUNCEMENTS_KEY + OPAP_ANNOUNCEMENTS_UUID) !== 'false';
                if (toggle !== undefined) {
                    show = !!toggle;
                    sessionStorage.setItem(OPAP_ANNOUNCEMENTS_KEY + OPAP_ANNOUNCEMENTS_UUID, show === true ? 'true' : 'false');
                }
                container.classList.toggle('d-none', !show);
            };
            Liferay.on('allPortletsReady', function () {
                Array.prototype.forEach.call(document.querySelectorAll('#_com_liferay_asset_publisher_web_portlet_AssetPublisherPortlet_INSTANCE_globalAnnouncements_announcement .opap-dismiss-announcement'), function(el){
                    el.addEventListener('click', function (e){
                        e.preventDefault();
                        OPAPNotificationDismiss_com_liferay_asset_publisher_web_portlet_AssetPublisherPortlet_INSTANCE_globalAnnouncements_(false)});
                });
                window.OPAPNotificationDismiss_com_liferay_asset_publisher_web_portlet_AssetPublisherPortlet_INSTANCE_globalAnnouncements_();
            });
        })();
    </script>


		
		




			
			
	
	





