<script type="text/javascript">

setTimeout(function() {
$.getScript('/js/fb.js', function() {});
}, 5000);

// Trcking blog tab click  
function blog() {
    var dataString = "blog=true";
    $.ajax({
        type: "POST",
        url: "/trblog.php",
        data: dataString,
        success: function(msg) {}
    });
}

// Auto complete - suggested search -keyword 
$('#keyword').autocomplete({
    source: function(request, response) {
        $.ajax({
            url: '/autoComplete/autosuggest.php',
            dataType: "json",
            data: {
                name_startsWith: request.term,
                type: 'keyword'
            },
            success: function(data) {
                response($.map(data, function(item) {
                    return {
                        label: item,
                        value: item
                    }
                }));
            }
        });
    },
    autoFocus: false,
    minLength: 0
});


// Zip Auto Fill and Location With Key Auto Suggestion
function auto_fill_zip() {
    if (document.searchForm.location.value != '') { // Function
        var ex = /^[(0123456789)]/;
        if ((ex.test(document.searchForm.location.value)) && document.searchForm.location.value.length == 5) { // IF condition  END 

            $('#location').autocomplete({
                source: function(request, response) {
                    $.ajax({
                        url: '/autoComplete/autosuggestzip.php',
                        dataType: "json",
                        data: {
                            zipcode: request.term,
                            type: 'zip'
                        },
                        success: function(data) {
                            response($.map(data, function(item) {
                                return {
                                    label: item,
                                    value: item
                                }
                            }));
                        }
                    });
                },
                autoFocus: false,
                minLength: 0
            });
        } else {
            $('#location').autocomplete({
                source: function(request, response) {
                    $.ajax({
                        url: '/autoComplete/autosuggestzip.php',
                        dataType: "json",
                        data: {
                            name_startsWith: request.term,
                            type: 'location'
                        },
                        success: function(data) {
                            response($.map(data, function(item) {
                                return {
                                    label: item,
                                    value: item
                                }
                            }));
                        }
                    });
                },
                autoFocus: false,
                minLength: 0
            });

        }
    }
}
</script>
