<script type="text/javascript">
$(document).ready(function() {
    $('ul.menu').hide();

    $('#main-nav').click(function() {
        $('ul.menu', this).show();
    });

    $('#main-nav').mouseleave(function() {
        $('ul.menu', this).hide();
    });

    $(".menu a li").click(function() {
        var val = $(this).attr("data-val");
        $.ajax({
            type: 'post',
            url: '/insert_intl.php',
            data: {
                country: val
            },
            success: function(data) {},
            error: function() {}
        });
    });
});
</script>
