  <script>
    const USER_EVENTS = ["mousedown","wheel","DOMMouseScroll","mousewheel","keyup","touchmove"]
    function addEventListenerToEvents(listener) {
    USER_EVENTS.forEach((event) => {
        window.addEventListener(event, listener);
    });
}
    function removeEventListenerFromEvents(listener) {
    USER_EVENTS.forEach((event) => {
        window.removeEventListener(event, listener);
    });
}
    function cleanAnchorGlobals() {
    if (window.__VUE_SSR__anchorListenerHandler) {
        removeEventListenerFromEvents(window.__VUE_SSR__anchorListenerHandler);
    }
    delete window.__VUE_SSR__anchorScrollMade;
    delete window.__VUE_SSR__anchorListenerHandler;
}
    (function anochorTest() {
    function setScrollMade() {
        window.__VUE_SSR__anchorScrollMade = true;
        cleanAnchorGlobals();
    }
    window.__VUE_SSR__anchorListenerHandler = setScrollMade;
    addEventListenerToEvents(window.__VUE_SSR__anchorListenerHandler);
})()
  </script>

