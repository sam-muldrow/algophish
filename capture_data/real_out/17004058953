<div id="block-tls-script" data-block-plugin-id="block_content:15d2d350-e54c-44b3-a823-58b8e829e2b0" class="block block-block-content block-block-content15d2d350-e54c-44b3-a823-58b8e829e2b0 block-tls-script">
  
    
      
            <div class="field field--name-body field--type-text-with-summary field--label-hidden field__item"><script type="text/javascript">
    window.addEventListener("load",(function() {
            var currentUrl = window.location.href; 
            var value = getParameterByName("ob", currentUrl);
            if(value == 1 && getCookie("insec-tls") == null)
            {
                showBanner();
            }
            function getCookie(name) {
                var dc = document.cookie;
                var prefix = name + "=";
                var begin = dc.indexOf("; " + prefix);
                if (begin == -1) {
                  begin = dc.indexOf(prefix);
                  if (begin != 0) return null;
                }
                else{
                  begin += 2;
                  var end = document.cookie.indexOf(";", begin);
                  if (end == -1) {
                   end = dc.length;
                  }
                }
                return decodeURI(dc.substring(begin + prefix.length, end));
              }
        
        function getParameterByName(name, url) {
         if (!url) 
            url = window.location.href;
         name = name.replace(/[\[\]]/g, '\\$&');
         var regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)'),
            results = regex.exec(url);
         if (!results)
          return null;
        if (!results[2]) 
          return '';
        return decodeURIComponent(results[2].replace(/\+/g, ' '));
      }

      function showBanner()
      {
          var block_to_insert = document.createElement( 'div' );
          block_to_insert.id = "section-info-tls-id";   
          block_to_insert.innerHTML = 
		 '<style>#section-info-tls-id {display: block;  position: relative; background: #fcf7f9; border: 1px solid #e5b2c1;margin-right: auto;margin-left: auto; max-width: 1232px;}</style>'+
		  '<section id="section-info-tls-id">'+
		  '<section id="section-info-tls-id container">'+
          '<div  class="msg-logo col-logo"  style="width: 40px;display: inline-block;padding-bottom: 5px;vertical-align: middle;padding-left: 0!important;padding-right: 0!important; margin: 0;border: 0;">'+
            '<span class="sch sch-exclamation-octagon-2x" style="color: #a03;"></span>'+
          '</div>'+
  
          '<div style="padding: 15x,0;vertical-align: middle;width: calc(99% - 55px);display:inline-block;padding: 15;padding: 15px,0;padding: 15px 0;font-size: 14px;line-height: 18px;">'+
            '<span>Beginning November 1, 2020, you may not be able to log in to Schwab.com because the version of the browser you’re using will be outdated. To update your browser, please visit <a href="https://www.schwab.com/help/browsers" name="&amp;lid=Chome_alert_taxInfo" target="_blank">https://www.schwab.com/help/browsers</a>. For assistance, please call 800-435-4000.</span>'+
           '</div>'+
  
          '<div  class="msg-close" style="vertical-align: top;display: inline-block;">'+
            '<a  class="pull-right" href="javascript:void(0);" role="button" aria-label="Dismiss Critical Business Message" onclick="dismissBanner();;return false;">'+
              '<em  class="sch sch-x"></em>'+
             '</a>'+
          '</div>'+'</section>';

             var container_block = document.getElementsByClassName('region-schwab-header')[0];
            if(container_block == undefined)
              container_block = document.getElementsByTagName('header')[0];
            if(container_block != undefined)
              container_block.appendChild(block_to_insert);
            else 
            {
               container_block = document.getElementsByTagName('h1')[0];
               if(container_block != undefined)
               container_block.insertBefore(block_to_insert, container_block.childNodes[0]);
               document.getElementById('logoCnt').style.marginBottom = '0px';
            }
           
            
      }
      }),false);
      function dismissBanner(){document.getElementById("section-info-tls-id").style.display='none'; setCook(); } 
      function setCook()
      {
          var cookieName = "insec-tls";
          var cookieValue = "true";
          var date = new Date();
          date.setTime(date.getTime()+(1*24*60*60*1000));
          document.cookie = cookieName + "=" + cookieValue + "; expires=" + date.toGMTString() + ";domain=.schwab.com;path=/";
      }
    </script></div>
      
  </div>
