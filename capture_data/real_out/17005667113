    var isErrorPage = Boolean();
    var isProductPage = Boolean();
    var hasCategory = !false;
    var hasName = !true;
    var hasVariant = !true;
    window.datalayer_cms = {
        "product.productCategory":isProductPage && hasCategory ? "Current Accounts": null,
        "product.productName": isProductPage && hasName ? "N/A": null,
        "product.productVariant": isProductPage && hasVariant ? "N/A": null,
        isProdView: isProductPage,
        isPreamble: Boolean(),
        isConfirmation: Boolean(),
        isErrorPage: isErrorPage,
        pageErrorMessage: isErrorPage ? document.title: null,
    };
    //added for mortgageProductPage
    if(window.location.pathname === "/products/mortgages/rates/product-page/"){
        window.datalayer_cms['product.productCategory'] = 'Mortgages';
        window.datalayer_cms['product.productName'] = 'Mortgage';
        window.datalayer_cms['isProdView'] = true;
    }
