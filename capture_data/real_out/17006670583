        var headerBiddingCall = function() {
            }

        var createImage = function(url) {
        var img = document.createElement('img');
        img.src = (('https:' == document.location.protocol) ? 'https://' : 'http://') + url;
        img.width=1;img.height=1;img.style.display='none';
        document.body.appendChild(img);
    };
    var sync_callback = function(globalId) {
        var td_url = encodeURIComponent(location.href);
        var region = td.client.host.split('.')[0];
        var account = (region === 'in' ? 'aws-':(region === 'tokyo' ? 'aws-tokyo-':'idcf-')) + td.client.writeKey.split('/')[0];
        var aone_url = 'aw.dw.impact-ad.jp/c/map/?oid=' + aone_oid + '&cid=' + globalId + '&sp=tda&_t=1&src=' + td_url + '&account=' + account;
        createImage(aone_url);
    };

    var syncGoogle = function() {
        var tdClientId = td.client.track.uuid;
        var region = td.client.host.split('.')[0];
        var account = (region === 'in' ? 'aws-us-':(region === 'tokyo' ? 'aws-tokyo-':'idcf-')) + td.client.writeKey.split('/')[0];
        var dbmUrl = 'cm.g.doubleclick.net/pixel?google_nid=treasuredata_dmp&google_cm&td_write_key=8151/fcd628065149d648b80f11448b4083528c0d8a91&td_global_id=td_global_id&td_client_id=' + tdClientId + '&account=' + account + '&td_host=' + document.location.host;
        createImage(dbmUrl);
    };

        var getSuitableSegments = function(gamKey, segmentValues) {
        return segmentValues.filter(function(segmentData) {
            return gamKey in segmentData.key;
        }).reduce(function(acc, cur) {
            return acc.concat(cur.values);
        }, []);
    };

        // fetchUserSegmentsのコールバック関数 (Success)
    var successCallback = function(values) {

        var table = {
            cat_pur:       "CDP_ACP",
            restaurant_id: "CDP_RESTAURANT",
            ruid:          "CDP_RUID",
            td_global_id:  "CDP_TDID",
            first_party_cookie: "CDP_FIRSTPARTY"
        }
        Object.keys(table).forEach(function (gamKey) {
            var segments = getSuitableSegments(gamKey, values);
            if (segments.length === 0) {
                return;
            }
            googletag.cmd.push(function() {
                googletag.pubads().setTargeting(table[gamKey], segments);
            });
        });

        // HeaderBiddingの実行
        headerBiddingCall();

        // TDと連携するためのsync処理実行
        var region = td.client.host.split('.')[0];
        var dataCenterAccount = ((region) => {
            if (region === 'in') {
                return 'aws-us-';
            } else if (region === 'tokyo') {
                return 'aws-tokyo-';
            } else {
                return 'idcf-';
            }
        })(region);
        var account = dataCenterAccount + td.client.writeKey.split('/')[0];
        var ttdUrl = 'match.adsrvr.org/track/cmf/generic?ttd_pid=vbyog0i&ttd_tpi=1&ttd_puid='+account+'&ttd_puid='+ruid;

        createImage(ttdUrl);
    };

    // fetchUserSegmentsのコールバック関数 (Error)
    var errorCallback = function(err) {
        // HeaderBiddingの実行
        headerBiddingCall();
    };


    // Receive td_global_id
    var successTrackPageViewCallback = function(td_global_id) {
        td.fetchUserSegments(
            {
                audienceToken: [acpToken, restaunratToken, ruidToken, tdidToken, firstPartyToken],
                keys: {
                    cat_pur: acpId,
                    restaurant_id: restaurantId,
                    ruid: ruid,
                    td_global_id: td_global_id,
                    first_party_cookie: ruid,
                }
            }, successCallback, errorCallback);
        sync_callback(td_global_id);
        syncGoogle();
    };

    var errorTrackPageViewCallback = function(err) {
        // HeaderBiddingの実行
        headerBiddingCall();
    };

    // trackPageviewのコールバック関数
    var trackSegmentSuccessCallback = function() {
        td.fetchGlobalID(successTrackPageViewCallback, errorTrackPageViewCallback);
    };

        (function() {
        td.set('$global', 'td_global_id', 'td_global_id');
        td.set('$global', 'ruid', ruid);
        td.set('$global', 'bk_uuid', CCIDT.dc_id);
        td.trackPageview(table, trackSegmentSuccessCallback);
    })();
    </script>

