(function(){
    window.CCIDT || (window.CCIDT = {});
    function getDCCookie(key){
        var cookies = document.cookie;
        var regex = new RegExp('(?:^|.*;\\s*)' +
            escape('DCTMkey_' + key).replace(/[\-\.\+\*]/g, '\\$&') +
            '\\s*\\=\\s*((?:[^;](?!;))*[^;]?).*');

        return regex.test(cookies) ? unescape(cookies.replace(regex , '$1')) : '';
    }
    CCIDT.dc_id || (CCIDT.dc_id = getDCCookie('dc_id'));
})();
</script>

