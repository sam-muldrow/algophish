ь, которая предает ему сил справиться со всеми испытаниями. Предлагаем посмотреть онлайн первый сезон сериала «Абсолютное зло» в нашем интернет-кинотеатре.","hru":"","subscription_ids":[106,107],"ivi_release_info":{"date_interval_min":"2023-11-20","date_interval_max":null},"season_release_date":1699228800000,"used_to_be_paid":false,"content_paid_types":["SVOD"],"allow_download":true,"allow_download_paid_types":["SVOD"],"min_episode":1,"max_episode":6,"episode_count":6,"fake":false}]},"main_action":{"caption":"Смотреть","action":"/compilation/open","action_params":{"id":17308}},"positionId":14},"35172":{"id":35172,"title":"Скорая помощь","type":"compilation","synopsis":"Новый сезон любимого медицинского сериала о непростых буднях людей, ежедневно спасающих жизни","text_color":"#ffffff","px_audit":[],"click_audit":[],"images":[{"id":22001094,"url":"https://thumbs.dfs.ivi.ru/storage4/contents/c/e/87ae8d711cc09ed4a4a904402698af.jpg","content_format":"MobilePromo-1536x468","dominant_color":"#6B7692"},{"id":22001099,"url":"https://thumbs.dfs.ivi.ru/storage4/contents/1/7/f11ec2fce90310ed0527c910ea65b5.jpg","content_format":"MobilePromo-980x330","dominant_color":"#3B547D"}],"object_info":{"hru":"skoraya-pomosch-2018","years":[2018,2019,2020,2021,2022,2023],"id":16530,"kind":2,"object_type":"compilation","genres":[277,196,128,213],"country":1,"restrict":16,"shields":[{"id":13,"name":"все серии сразу","short_name":"все серии сразу","ds_style":"dor","type":"complete-season","icon":null,"place":["promo","content_card"]},{"id":15,"name":"новый сезон","short_name":"новый сезон","ds_style":"alli","type":"new_season","icon":null,"place":["default"]}],"title_image":[{"content_format":"TitleImage-UpTo3000x3000","url":"https://thumbs.dfs.ivi.ru/storage9/contents/6/3/930161839b0fc0abbf801a4310f330.png","width":1955,"height":751,"not_show_in_cc":false}],"content_paid_types":["AVOD","SVOD"],"ivi_release_info":{"date_interval_min":null,"date_interval_max":null},"seasons":[{"season_id":103