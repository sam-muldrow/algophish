лается все более запутанной. Новым лучшим другом героя становится ушлый и циничный инспектор ДПС Миха Паламарчук, а также у него завязываются непредсказуемые отношения с бывшей содержанкой богатых и влиятельных мужчин. Предлагаем посмотреть онлайн первый сезон сериала «Везёт» в нашем интернет-кинотеатре.","hru":"","subscription_ids":[6,62,64,107,109,113],"ivi_release_info":{"date_interval_min":null,"date_interval_max":null},"season_release_date":1635984000000,"used_to_be_paid":false,"content_paid_types":["SVOD"],"allow_download":true,"allow_download_paid_types":["SVOD"],"min_episode":1,"max_episode":8,"episode_count":8,"fake":false}]},"15715":{"hru":"himera","years":[2022],"used_to_be_paid":false,"has_upcoming_episodes":false,"id":15715,"kind":2,"title":"Химера","orig_title":"","categories":[15],"genres":[217,128,213],"country":1,"ivi_rating_10":8.67,"ivi_rating_10_count":124419,"localizations":[{}],"restrict":16,"shields":[{"id":8,"name":"сериал Иви","short_name":"сериал Иви","ds_style":"alli","type":"ivi_series","icon":null,"place":["default"]}],"rating":{"ready":{"votes":124419,"main":8.67,"director":0.27,"pretty":0.27,"actors":0.3,"story":0.35}},"extra_properties":{},"posters":[{"height":1200,"width":782,"dominant_color":"#28293B","url":"https://thumbs.dfs.ivi.ru/storage15/contents/8/c/2cadf74eb2442c7c00680676d6d846.jpg","type":"poster-vertical","id":20872100,"content_format":"Posters-782x1200"},{"height":2160,"width":3840,"dominant_color":"#212030","url":"https://thumbs.dfs.ivi.ru/storage39/contents/2/7/41159eb0c8cfa0d42d66ac50960c67.jpg","type":"poster-horizontal","id":20872103,"content_format":"Posters-3840x2160"}],"uhd_available_all":true,"content_paid_types":["AVOD","SVOD"],"subscription_names":[{"id":6,"name":"Иви","brand":"ivi","style":"linus"}],"seasons":[{"season_id":10180,"number":1,"purchasable":false,"title":"","description":null,"hru":"","subscription_ids":[6,62,64,107,109,113],"ivi_release_info":{"date_interval_min":null,"date_interval_max":null},"