-years-racial-discrimination-russia-korean-uzbek-community-finds-peace-new":{"id":"183610","type":"node--stories"},"stories/2019-09-26/low-coffee-prices-are-starving-farmers-can-cartel-fix-it":{"id":"183586","type":"node--stories"},"stories/2019-09-19/us-promised-thousands-foreign-interpreters-special-immigrant-visas-now-they-re":{"id":"183530","type":"node--stories"},"stories/2019-09-18/jewish-and-arab-women-unite-defy-bedouin-voter-suppression-israeli-election":{"id":"183518","type":"node--stories"},"stories/2019-09-16/uptick-church-burnings-raises-alarm-ethiopia":{"id":"183496","type":"node--stories"},"stories/2019-08-29/colombian-farmers-combine-agriculture-ecology-solve-economic-health-woes":{"id":"183347","type":"node--stories"},"stories/2019-09-10/intersex-counted-kenyas-census-victory":{"id":"183454","type":"node--stories"},"stories/2019-09-06/lead-colombian-elections-woman-mayoral-candidate-latest-assassination-victim":{"id":"183449","type":"node--stories"},"stories/2019-09-05/fast-friends-rohingya-refugees-exile-rebuild-their-lives-malaysia":{"id":"183407","type":"node--stories"},"stories/2019-08-21/china-s-gen-x-moms-turn-ivf-desperate-quest-second-baby":{"id":"183243","type":"node--stories"},"stories/2019-08-19/south-koreas-sex-doll-industry-stirs-controversy-life-size-imports":{"id":"183252","type":"node--stories"},"stories/2019-08-12/south-korea-s-no-japan-boycott-new-wounds-are-old":{"id":"183240","type":"node--stories"},"stories/2019-08-09/without-respect-indigenous-rights-climate-change-will-worsen-un-says":{"id":"183232","type":"node--stories"},"stories/2023-11-20/existing-and-resisting-black-quilombo-communities-fight-land-rights-brazil":{"id":"190824","type":"node--stories"},"stories/2023-11-20/when-phones-go-dead-gaza":{"id":"190823","type":"node--stories"},"stories/2023-11-17/guatemalas-newly-elected-president-faces-legal-challenges":{"id":"190820","type":"node--stories"},"stories/2023-11-16/food-writer-celebrates-tastes-her-hometown-lagos-nige