$(document).ready(function(){
    var change_countdown_tab_M201101_43575__P017c01000308 = new change_countdown_tab('M201101_43575__P017c01000308');});
function getLimitStockStore(opt_module_id,items,status){
    var module_id = opt_module_id;
    var self = this;
    items = items;
    this.changeWord = function(num,qty_id,btn_id) {
        if(parseInt(num)){
          $('#'+qty_id).text('剩餘'+num+'組'); 
        }else if(num=='0'){
          $('#'+qty_id).text(''); 
          $('#'+btn_id).html('<button class="button button2">已完售</button>'); 
        }   
    }
    this.main = function(){
        if(status==3){
          for(var i in items){
              var item_id = items[i];
              var item_url = '/stock/showlimitstock/'+item_id+'/home'      
              $.ajax({
                  url: item_url,
                  success: function(msg) {
                    if(msg){
                      var msg_arr=msg.split('||');
                      var qty_id = 'qty_'+msg_arr[0]+'_'+module_id;
                      var btn_id = 'btn_'+msg_arr[0]+'_'+module_id;                   
                      if(msg_arr[1]) self.changeWord(msg_arr[1],qty_id,btn_id)
               
                    } 
                  }
              });      
          }
        }
    }
    //從這開始
    this.main();     
}
</script>
