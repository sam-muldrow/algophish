            <script>
  function openAuth(a) {
    if (a.href) {
      var href = a.href;
      window.open(href, 'auth', 'width=750,height=425');
      a.removeAttribute('href');
      window.setTimeout(function () {
        a.setAttribute('href', href);
      }, 1000);
    }
    return false;
  }
</script>


