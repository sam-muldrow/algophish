	<script type="text/javascript">
		var popupFlag = getCookie("todayPopup");

		if (popupFlag != "false") {
			var body = $('body');
			body.append($('<div class="layerArea"></div>'));
			$('.layerArea').append($(".layerPop"));
			$(".layerPop").show();
			$(document).mouseup(
					function(e) {
						var layerPop = $('.layerPop');
						if (!layerPop.is(e.target)
								&& layerPop.has(e.target).length === 0) {
							/* layerHide(); */
						}
					});
			//alert($('#leng').val());
			//main layer popup slide
			if ($('#main .layerArea').length !== 0 && $('#leng').val() > 1) {

				$('#main .layerArea .popWrap').slick({
					autoplay : true,
					arrows : true,
					dots : true,
					draggable : true
				});

			}
		}
	
		
		function popTodayClose() {
				 setCookie('todayPopup', 'false', 1); 
				 layerHide();
					}
		
		
		function popClose() {
			/* if ($("#popCheckBox").prop("checked")) {
				setCookie('todayPopup', 'false', 7);} /* name=Ethan, 7일 뒤 만료됨 */
			 
			layerHide();
		}

		function setCookie(name, value, exp) {
			var date = new Date();
			date.setTime(date.getTime() + exp * 24 * 60 * 60 * 1000);
			document.cookie = name + '=' + value + ';expires=' + date.toUTCString() + ';path=/';
		}

		function getCookie(name) {
			var value = document.cookie.match('(^|;) ?' + name + '=([^;]*)(;|$)');
			return value ? value[2] : null;
		}

	</script>



