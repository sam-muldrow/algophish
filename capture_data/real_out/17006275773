<script type="text/javascript">
    function goPage(url,idx, menuId){
        location.href = url;
    }
    function logout_kita(geturl){
        location.href='https://ssl.kita.net/auth/logoutAuth.do?rtnUrl=https://www.kita.net';
    }
    function fn_goPage(menuNm,rtnUrl,userSeq) {
        var popupWidth = 655;
        var popupHeight = 700;
        var halfScreenWidth = window.screen.width / 2;
        var halfScreenHeight = window.screen.height / 2;
        var halfPopupWidth = popupWidth / 2;
        var halfPopupHeight = popupHeight / 2;
        var popupX = halfScreenWidth - halfPopupWidth;
        var popupY = halfScreenHeight - halfPopupHeight;
        var	userSeq = $('#sUserSeq').val(); /* 사용자_일련번호 */
        var	applCd = $('#sApplCd').val();       /* 응용시스템_코드 */
        if( menuNm == 'login' ){
            applCd = 'KI';
        }
        var nowUrl = 'http://www.kita.net/'; /* 처리후 리턴 페이지 */
        if( rtnUrl == null || rtnUrl == '' ){
            rtnUrl = encodeURIComponent(window.location.href);
        }
        var	url	=	"https://ssl.kita.net/member/selectMenu.do?userSeq="+userSeq+"&menuNm="+menuNm+"&applCd="+applCd+"&rtnUrl="+rtnUrl;
        window.open(url,'pop_'+menuNm,'top='+popupY+',left='+popupX+', width='+popupWidth+', height='+popupHeight +', menubar=no, status=no, toolbar=no, scrollbars=yes');
    }

    function fn_goJoin() {
        var	applCd = "KI"; /* 응용시스템_코드 */
        var rtnUrl = 'http://www.kita.net/'; /* 처리후 리턴 페이지 */
        var	url	=	"https://ssl.kita.net/member/selLoginType.do?applCd="+applCd+"&rtnUrl="+rtnUrl;
        window.open(url);
    }

    //참가신청서 조회(로그인)
    function moveBsnsSchdulAprList(memberId) {

        var rtnUrl = 'http://www.kita.net/mberJobSport/bsnsReqstSchdul/cmmrcSportBsnsCldr/kitaBsnsSchdul/kitaBsnsSchdulAprList.do'; /* 처리후 리턴 페이지 */

        if(memberId == '') {
            alert("로그인 후 이용가능한 서비스 입니다.");
            fn_goPage('login',rtnUrl);
        }else {
            location.href = rtnUrl;
        }

    }

    function checkMemberYn(){
		$.ajax({
			url:"/kita/cia/checkMemberYn.do",
			dataType:"json",
			success:function(data){
				if (data.isMember){
					location.href="/kita/cia/ownInfo.do";
				}else{
					layerPop('/cmm/popMembership.do','N');
				}
			},
			fail:function(e){
				console.log(e);
			}
		});
	}


    function onSimpleMode() {
    	document.cookie = "simple_mode=Y";
    	openLoader();
    	setTimeout(function() {
    		window.location.reload();
		}, 0);

    }

    function offSimpleMode() {
    	document.cookie = "simple_mode=N";
    	openLoader();
    	setTimeout(function() {
    		window.location.reload();
		}, 0);
    }

  //로딩 이미지 노출
    function openLoader(){
    	if(document.querySelector('.loader')){
    		document.querySelector('.loader').style.display = "block";
    	}
    }

    //로딩 이미지 제거
    function closeLoader(){
    	if(document.querySelector('.loader')){
    		document.querySelector('.loader').style.display = "none";
    	}
    }

</script>



