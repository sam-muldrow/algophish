	if($('.NotifcationsLists').hasClass('Show')){
		$('.NotifcationsLists').removeClass('Show');
	}
});
$("body").on('click', '.FollowTerm', function(event) {
	if (isUser == "false")
	{
		notUserMsg();
	}
	else
	{
		$.ajax({
			url: MyAjaxURL+'Users/FollowTerm.php',
			type: 'POST',
			data: {term_id: $(this).data("term") },
			success:function(requestResponse){
				$("body").append(requestResponse)
			}
		});
	}
});
