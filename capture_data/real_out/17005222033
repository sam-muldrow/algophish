$("body").on('keyup', '.SearchINPop form input', function(event) {
	event.preventDefault();
	searchValue = $(this).val();
	$(".ChangerSearchArea").html(spinnerLoad);
	$(".search-contentarea .title-bar h3 em").html(searchValue);
	$(".search-contentarea .title-bar > a").attr("href", HomeURL + "?s="+searchValue);
	if (searchValue != "") {
		$.ajax({
			url: MyAjaxURL + 'SearchingTwo.php',
			type: 'POST',
			data: {'search':searchValue , type:$("section.searching-section ul.search-filter li.active").data('search')},
			dataType: "html",
			success: function(response) {
				$(".ChangerSearchArea").html(response);
			}
		});
	}
});
$('body').on('click', '.PageHeader .LeftHeader .SearchBtn', function(e){
	if (!$("body").hasClass('SearchingNow')) {
		$("body").addClass('SearchingNow');
	}else{
		$("body").removeClass('SearchingNow');
	}
});
