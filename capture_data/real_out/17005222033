$("body").on('click', '.TaxPageFilterDrop ul li ', function(event) {
	$("body").addClass('filterSelected');
	$(this).addClass('active').siblings().removeClass('active');
	$(this).parent().parent().parent().find('span.HoverAfter').find('em').html($(this).data('name'));
	$(".ShowResult").addClass('Show');
});
