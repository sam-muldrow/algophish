        >
      
        <script>
const placeholder = 'What are you looking for?';
const selector = '.header-search input[type="text"]';

let attemtpsRemaining = 10;
let lookupTimeout;

const replacePlaceholder = () => {
  if (!attemtpsRemaining) return;
  attemtpsRemaining--;
  const elements = document.querySelectorAll(selector);
  if (!elements.length) {
    lookupTimeout = setTimeout(replacePlaceholder, 200);
    return;
  }

  elements.forEach(element => element.placeholder = placeholder);
}

replacePlaceholder();
</script>
      
        </div>
      



  
  

  

  
  
  

  
  


