          style=""
        >
      
        <script>
        document.addEventListener("DOMContentLoaded", function() {
        const menuParentClass = '.side-menu-account';
        const menuParent =  document.querySelector('.title-container');
        const sidebarMenuItems = document.querySelectorAll(`${menuParentClass} ul > li a`);
        if (sidebarMenuItems && menuParent) {
            const dropdown = document.createElement("select");
            dropdown.setAttribute('onchange', "location = this.value;")
            dropdown.setAttribute('class', "md:hidden side-menu-account-mobile")
            sidebarMenuItems.forEach(item => {
            const href = item.getAttribute('href');
            const option = document.createElement('option');
            const optionText = item.innerText;
            const isSelected = href == window.location.pathname;
            dropdown.options.add(new Option(optionText, href, false, isSelected));
            if(isSelected) item.setAttribute('class', 'link-active')
            });
            menuParent.append(dropdown);
        }
        });
    </script>
      
        </div>
      



  
  

  

  
  
  

  
  
  

  

