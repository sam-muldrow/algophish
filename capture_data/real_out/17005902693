<script>
    let cookieProInterval = setInterval(() => {
        const privacyChoicesBtn = document.querySelector('.nav-child-link-your-privacy-choices > a');

        if(privacyChoicesBtn){
            privacyChoicesBtn.addEventListener('click', function(e){
                e.preventDefault();
                if(OneTrust){
                        OneTrust.ToggleInfoDisplay();
                }
            });
            clearInterval(cookieProInterval);
        }
    }, 1000);

