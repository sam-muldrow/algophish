  <script >
    function renderQuickView(json) {
      const item = JSON.parse(decodeURI(json))
      luminate.applyHook('modal-toggle:product-quickview', () =>{}, item)

      nogin.updateContext({
        productId: item.sourceId
      })

      return false
    }

    nogin.on('ready', () => {
      
      
      function renderNoginCatalogFilter() {
        
      }

      

      // render our search app
      const elem = document.getElementById('nogin-search-app')
      if (elem) {
        nogin.catalog.render({
          type: 'search',
          elementId: 'nogin-search-app',
          config: {
            cardLayout: ['title', 'price', 'family']
          }
        })
      }
    })

    document.addEventListener('DOMContentLoaded', () => {
      nogin.init({
        publicToken: "clhp52qymw76qjn1s0x4uc43q",
