    <script type="text/javascript">


        $(document).ready(function () {

            $('.applyTxt').on('click', function () {
                if ($(this).hasClass('listActive')) {
                    $(this).removeClass('listActive');
                } else {
                    $(this).addClass('listActive');
                }

            });

            $(".gobuton").click(function () {

                window.location.href = "/gcb/forms/credit_cards/instant_card.htm";

            });






            $(".lftNavMo").remove();

            $(".mainNavList li a.noMegaMenu").addClass("activeTabHighlight");







            $e = Math.floor(Math.random() * 4);

            $('.flexslider').flexslider({

                controlsContainer: ".slider",

                animation: "fade",

                controlNav: true,

                slideshow: true,

                slideshowSpeed: 5000,

                manualControls: ".offerContentList li",

                touch: true,

                startAt: $e,

                start: function (slider) {

                    $('body').removeClass('loading');

                }

            });

            $(".notice-content").click(function () {

                $("#impnotcont").slideToggle(800);

            });

            //Citi Mobile app toggle
            var uagent = navigator.userAgent.toLowerCase(),
                deviceIphone = "iphone",
                deviceIpod = "ipod",
                deviceIpad = "ipad",
                deviceAndroid = "android";
            if (uagent.search(deviceIphone) > -1 || uagent.search(deviceIpad) > -1 || uagent.search(deviceIpod) > -1) {
                $('#ios').show();
                $('#android').hide();
            } else if (uagent.search(deviceAndroid) > -1) {
                $('#ios').hide();
                $('#android').show();
            } else {
                $('#ios,#android').show();
            }


        });

        function speedbump(url) {

            if (confirm('You are now leaving the Citibank website and entering a third party site. All the information you provide will be subject to confidentiality and security terms of the applicable third party site. Citibank Singapore Limited does not take responsibility for information you provide at such third party sites. Would you like to continue?')) {

                window.open(url);

            }

        }



    </script>





    <script>

        $(".new_fold").click(function () {
            $('.cS-generalInfoHolder').css('display', 'none')
        });


        var count = 0;

        setInterval(function () {

            if (count == 0) {

                $("#slideimg a img").animate({ "left": "0" }, "slow");

                $("#slideimg a img").attr("src", "images/11.gif");

                $("#slideimg a").attr("href", "landing-page1.html");

                count = 1;
            }

            else {

                $("#slideimg a img").animate({ "right": "0" }, "slow");

                $("#slideimg a img").attr("src", "images/10.gif");

                $("#slideimg a").attr("href", "landing-page.html");

                count = 0;

            }

        }, 5000);

        $(window).on('load' , function () {

            var isMobile = {
                Android: function () {
                    return navigator.userAgent.match(/Android/i);
                },
                BlackBerry: function () {
                    return navigator.userAgent.match(/BlackBerry/i);
                },
                iOS: function () {
                    return navigator.userAgent.match(/iPad|iPhone|iPod/i);
                },
                Opera: function () {
                    return navigator.userAgent.match(/Opera Mini/i);
                },
                Windows: function () {
                    return navigator.userAgent.match(/Windows Phone/i);
                },
                any: function () {
                    return (isMobile.Android() || isMobile.BlackBerry() || isMobile.iOS() || isMobile.Opera() || isMobile.Windows());
                }
            };

            if (isMobile.iOS()) {
                $('#appstore').show();
                $('#playstore').hide();
            }
            else if (isMobile.Android()) {
                $('#appstore').hide();
                $('#playstore').show();
            }
            else {
                $('#appstore,#playstore').show();

            }
        });

    </script>
