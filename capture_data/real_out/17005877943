                                                                <style>div[e0a5df880lb] { position: absolute; bottom: 100%; z-index: 1; }div[e0a5df880lb]:after { font-size: 9px; content: "REKL\200B AMA\200B "; }</style>



        <script>
        (function (d, c) {
            var scripts = document.getElementsByTagName('script'),
                script = d.currentScript || scripts[scripts.length - 1],
                slot = d.createElement('div');

                        var frame = d.createElement('div'),
                altSlot = d.createElement('div');
            frame.setAttribute(c, '');
            frame.appendChild(slot);
            frame.appendChild(altSlot);
            script.parentElement.insertBefore(frame, script);

            WP.push(function () {
                WP.gaf.registerSlot(80, slot, {
                    setNative: function (data, onViewCallback) {
                        var a = frame.nextElementSibling;
                        if (data.type === 'teaser') {
                            a.href = data.url;
                            a.target = '_blank';
                            // additional line for ads from criteo.com
                            a.getElementsByTagName('div')[1].innerHTML += '<br>';
                            a.getElementsByTagName('h2')[0].innerHTML = data.text;
                            a.getElementsByTagName('div')[0].innerHTML = '<div style="background-image:url(' + data.src + ')"></div>';
                            var l = d.createElement('div');
                            l.setAttribute('e0a5df880lb', '');
                            a.insertBefore(l, a.firstChild);
                            onViewCallback({
                                el: a.parentElement
                            });
                        }
                        if (data.type === 'alternativeSlot') {
                            onViewCallback && onViewCallback({
                                el: altSlot
                            });
                        }
                        if (data.type === 'teaserHide') {
                            a.style.display = 'none';
                            onViewCallback && onViewCallback({
                                el: a.parentElement
                            });
                        }
                        if (data.type === 'teaserShow') {
                            a.style.display = 'block';
                            onViewCallback && onViewCallback({
                                el: a.parentElement
                            });
                        }
                    },
                });
            });
                        script.parentElement && script.parentElement.removeChild(script);
        }(document, 'e0a5df880'));
    </script>


