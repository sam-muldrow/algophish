                    <script type="text/javascript">
var ta = ta || {};
var ad_server = 'ta.seatguru.com.s';
ta.has =function(key) {
    var keys = key.split('.');
    if (!this.hasOwnProperty(keys[0])) {
        return false;
    }
    var value = this[keys[0]];
    for (i = 1; i <= keys.length - 1; i++) {
        if (!value.hasOwnProperty(keys[i])) {
            return false;
        }
        value = value[keys[i]];
    }
    return value;
}
ta.retrieve = function(key) {
    var keys = key.split('.');
    var value = ta[keys[0]];
    for (i = 1; i <= keys.length - 1; i++) {
        value = value[keys[i]];
    }
    return value;
}
