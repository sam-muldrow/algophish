                                            <script>
    // used as a callback for recaptcha div
    function onSuccesfulSubmit(token) {
        if (token) {
            // validate the received recaptcha response
            validateRecaptcha(token);
        }
    }

    function validateRecaptcha(token) {
        $j.ajax({
            type: "POST",
            dataType: "json",
            url: '/ajax/validate_recaptcha.php',
            data: {
                recaptcha: token
            },
            success: function(data) {
                if(data.result === true) {
                    // disable the input to exclude it from submission since the response was already confirmed on server side
                    $j( "#g-recaptcha-response" ).prop( "disabled", true );
                    // submit the form
                    document.getElementById("home_find_seatmap").submit();
                    // enable again the input after exclusion from submit action
                    $j( "#g-recaptcha-response" ).prop( "disabled", false );
                } else {
                    recaptchaValidationFailed();

                    return false;
                }
            },
            error: function(data) {
                recaptchaValidationFailed();
                return false;
            }
        });
    }

    function recaptchaValidationFailed() {
        $j('.recaptcha_alert_main').fadeIn('slow', () => {
            setTimeout(function(){
                $j('.recaptcha_alert_main').fadeOut('slow');
            }, 3000);
            grecaptcha.enterprise.reset();
        });
        return false;
    }

    // recaptcha validation and submit seat map form on homepage
    $j('#home_find_seatmap').on('startRecaptchaValidation', function (e) {
        // recaptcha process
        grecaptcha.enterprise.execute();
    })
</script>
<script src="https://www.google.com/recaptcha/enterprise.js" async defer></script>

<div class="g-recaptcha"
     id="g-recaptcha"
     data-sitekey="6LdICjkoAAAAAMLYsObEeGfGmNY9ALDyKGXXUuDB"
     data-callback="onSuccesfulSubmit"
     data-size="invisible"
     data-badge="bottomright">
</div>
