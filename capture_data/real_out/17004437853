<script type="text/javascript">
function SystemRedirect(){
 var UserAgent = navigator.userAgent.toLowerCase();
 var IsntMsie = UserAgent.indexOf("msie")==-1; 
 var IsntChrome = UserAgent.indexOf("chrome")==-1;
 var IsntFireFox = UserAgent.indexOf("firefox")==-1;
 var IsntSafari = UserAgent.indexOf("safari")==-1;
 var IsIpad = UserAgent.indexOf("ipad")!=-1; 
 var IsIphone = UserAgent.indexOf("iphone")!=-1;
 var IsAndroid = UserAgent.indexOf("android")!=-1;
 var IsWindows = UserAgent.indexOf("windows")!=-1;
 var IsntIE11 = UserAgent.indexOf("trident/7")==-1; 
 var IsntEdge = UserAgent.indexOf("edge")==-1; 
 
 if((IsIpad || IsIphone || IsAndroid) || (IsntIE11 && IsntEdge && IsntMsie && IsntChrome && IsntFireFox && IsntSafari)){
  document.getElementById("user_input").innerHTML= '<input type="text" name="user_id" value=""  onKeyPress="enterpress();">';
  document.getElementById("password_input").innerHTML = '<input type="password" name="user_password" maxlength="20" value=""  onKeyPress="enterpress();">';
  document.getElementById("kb_block").style.display = "none";
 }
}
SystemRedirect();
</script>
