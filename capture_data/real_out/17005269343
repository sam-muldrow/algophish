<script>
    // 导航下拉菜单交互 + 搜索交互
    $(function(){
        var matchbox = {
            init : function(args){
                this.$body = $('body')
                this.$el = args.$el;
                this.$tipWrap = args.$tipWrap;
                this.$tip = this.$tipWrap.find('ul');
                this.clsTipHover = 'hover-search';
                this.cached = {};
                this.bind();
                this.render = template.compile('<%' +
                'for(var i=0,len=list.length; i<len; i++){' +
                'var item = list[i]' +
                '%>' +
                '<li data-value="<%= item.keyName%>"><a href="javascript:void(0);"><%= item.keyName%></a></li>' +
                '<% } %>');
                this.timer = null;
            },
            bind : function(){
                var _this = this;
                $(document).bind('click.djHeaderSearchBlur', function(){
                    _this.$tipWrap.hide();
                });
                this.$el.bind('click', function(e){
                    var bol_fetch = _this.$tipWrap.filter(':visible').length == 0;
                    if(bol_fetch){
                        _this.fetch();
                    }
                });
                this.$el.bind('blur', function(e){
                    _this.$tipWrap.hide();
                });
                this.$el.bind('keydown', function(e){
                    return _this.handlerKeydown(e);
                });
                this.$el.bind('keyup', function(e){
                    return _this.handlerKeyup(e);
                });
                this.$tip.delegate('li', 'mousedown', function(){
                    var $this = $(this);
                    var value = $this.attr('data-value');
                    _this.$el.val(value);
                    _this.$el.closest('form').submit();
                });
                this.$tip.delegate('li', 'mouseenter', function(){
                    $(this).addClass(_this.clsTipHover)
                });
                this.$tip.delegate('li', 'mouseleave', function(){
                    $(this).removeClass(_this.clsTipHover)
                });
            },
            build : function(data){
                var html = this.render(data);
                this.$tip.html(html);
                this.$tipWrap[data.list.length ? 'show' : 'hide']();
            },
            urlMap : {
                job : /\.ui$/.test(location.host) ? '/up/autoheader/json/search-suggest.php?job=1' : 'https://so.dajie.com/job/ajax/nav/keyword',
                corp : /\.ui$/.test(location.host) ? '/up/autoheader/json/search-suggest.php?corp=1' : 'https://so.dajie.com/corp/ajax/nav/keyword'
            },
            fetch : function(){
                var matchType = this.$el.attr('data-match');
                var _this = this;
                if(!matchType) return;
                clearTimeout(this.timer);
                this.timer = setTimeout(function(){
                    var val = $.trim(_this.$el.val());
                    var key = matchType + encodeURI(val);
                    var result = _this.cached[key];
                    if(result){
                        _this.build(result);
                        return;
                    }
                    $.ajax({
                        url : _this.urlMap[matchType],
                        type : 'get',
                        dataType : 'jsonp',
                        data : {
                            keyword : val,
                            match : matchType
                        },
                        success : function(r){
                            if(r.result == 0){
                                _this.build(r.data);
                                _this.cached[key] = r.data;
                            }
                        }

                    });
                }, 100);
            },
            handlerKeydown : function(e){
                var code = e.keyCode;
                switch(code){
                    case  38:
                        // 上一条
                        e.preventDefault();
                        this.handlerSelectItem('up');
                        break;
                    case 40 :
                        // 下一条
                        e.preventDefault();
                        this.handlerSelectItem('down');
                        break;
                }
            },
            handlerKeyup : function(e){
                var code = e.keyCode;
                switch(code){
                    case 37:
                    case 38:
                    case 39:
                    case 40 :
                        return;
                    default :
                        this.fetch();
                }
            },
            handlerSelectItem : function(type){
                var items = this.$tip.find('li');
                var length = items.length-1;
                var clsTipHover = this.clsTipHover;
                var index = items.index(items.filter('.'+clsTipHover));
                var newIndex = type == 'up' ? index -1 : index+1 > length ? 0 : index+1;
                if(index == -1){
                    items[type == 'up' ? 'last' : 'first']().addClass(clsTipHover);
                }else{
                    items.removeClass(clsTipHover);
                    items.eq(newIndex).addClass(clsTipHover);
                }
                var value = items.filter('.'+clsTipHover).attr('data-value');
                this.$el.val(value);
            }
        };
        (function(){
            var HeaderNav = (function(){
                var handler_show_menu = function(){
                    $(this).addClass('open');
                };
                var handler_hide_menu = function(){
                    $(this).removeClass('open');
                };
                var utilAjaxJsonp = function(api, callback, data){
                    $.ajax({
                        url: api,
                        dataType: 'jsonp',
                        data: data || null,
                        jsonp: 'callback',
                        success: function(r){
                            callback && callback(r)
                        },
                        error: function(){
                            console.log('error');
                        }
                    });
                };

                var HeaderNav = function(){
                    this.$headerWrap = $('#dj-header-wrap');
                    this.$djHeader = $('.dj-header');
                    this.$messageTips = $('#dj-header-wrap .message-tips');
                    this.$userBox = $('#dj-header-wrap .uesr-box');
                    this.$messageBox = this.$messageTips.find('.message-box');
                    this.$userStuff = this.$userBox.find('.user-stuff');
                    this.$searchInput = $('#dj-header-wrap .searchInput');
                    this.$searchTop = $('#dj-header-wrap .search-top');
                    this.navType = $.cookie('dj_nav') != null ? $.cookie('dj_nav') : '1'; //0=校园 1=白领
                    this.currentChannel = _staticHeader['currentChannel'];
                };
                HeaderNav.prototype.init = function(){  //C端初始化
                    var self = this;
                    self.render_current_header();
                    self.bindEvent();
                };
                HeaderNav.prototype.render_current_header = function(){//当前一级导航
                    var self = this;
                    $('#header .menu .menu-center a').each(function(){
                        if(self.currentChannel != undefined && self.currentChannel == $(this).attr('data-current')) {
                            $(this).addClass('current');
                        }
                    });
                };
                HeaderNav.prototype.bindEvent = function(){
                    var self = this;
                    self.$headerWrap.delegate('.menu-center .hasMore', 'mouseover', handler_show_menu);
                    self.$headerWrap.delegate('.menu-center .hasMore', 'mouseleave', handler_hide_menu);
                    //搜索框展开关闭
                    self.$headerWrap.delegate('.menu-right .search-hd-btn', 'click', function(e){
                        e.preventDefault();
                        self.$djHeader.addClass('close');
                        self.$searchTop.fadeIn(100);
                        self.$searchInput.focus();
                    });
                    self.$headerWrap.delegate('.search-top .closeAll', 'click', function(e){
                        e.preventDefault();
                        self.$djHeader.removeClass('close');
                        self.$searchTop.fadeOut(100);
                    });
                    //搜索框下拉交互
                    self.$headerWrap.delegate('.search-top .subnav-hd li a', 'click', self.handler_change_mode);

                    //历史搜索
                    matchbox.init({
                        $el : self.$headerWrap.find('.search-top input'),
                        $tipWrap : self.$headerWrap.find('.searchInputList')
                    });

                    //导航切换
                    $('.dj-header-changeNav a').bind('click', function(e){
                        e.preventDefault();
                        var href = $(this).attr('href');
                        var url = 'https://www.dajie.com/navigation/setnav';
                        utilAjaxJsonp(url, function(r){
                            window.location.href = href;
                        }, {version:self.navType});
                    });
                };
                HeaderNav.prototype.handler_change_mode = function(e){
                    e.preventDefault();
                    var $this = $(this);
                    var index = $this.index();
                    var href = $this.attr('data-href');
                    var name = $this.attr('data-name');
                    var holder = $this.attr('data-holder');
                    var match = $this.attr('data-match');
                    var text = $this.attr('data-text') + '<em class="imgIcon"></em>';
                    var $form = $this.closest('form');
                    var $input = $form.find('.searchInput');
                    var $span = $form.find('.searchTxt');
                    var $li = $this.parent('li');
                    $form.attr('action', href);
                    $input.attr('name', name);
                    $input.attr('placeholder', holder);
                    $input.attr('data-match', match);
                    $span.html(text);
                    $li.siblings().show();
                    $li.eq(index).hide();
                    handler_hide_menu.call($this.closest('.hasMore'));
                };

                return HeaderNav;
            })();

            var headerNav = new HeaderNav();
            headerNav.init();
        })();
    });
</script>        </div>
    </div>
    <script type="text/javascript">
        var is_login = $.cookie('dj_auth') != null || $.cookie('dj_auth_v3') != null; //必须是全局变量，别的项目有用
        window['is_login'] = is_login;
        _staticHeader = {
            isMini : false,
            isC : false,
            isRecruit : false,
            currentChannel: 'JOB',
            isBusinessCorp: false,
            isResumeSearch: false,
            isRecruitAuditing: false,
            auditStatus:0
        };
    </script>
