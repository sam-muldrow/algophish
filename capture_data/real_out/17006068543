                    breakpoint: 480,
                    settings: {
                        slidesToShow: 1,
                        arrows: false,
                        dots: true
                    }
                }
            ]
        });

        $(document).on('beforeChange', '.events-carousel', function(event, slick, currentSlide, nextSlide){
            var nextSlide = slick.$slides.get(nextSlide);
            var $slideSoureSets = $(nextSlide).find('source');
            $($slideSoureSets).each(function () {
                $(this).attr('srcset', $(this).data('lazy'));
            });
        });
    });
