    <script>
    document.addEventListener('DOMContentLoaded', function() {
        var Accordion = function(el, multiple) {
            this.el = el || {};
            this.multiple = multiple || false;
            var links = this.el.find('.formats-link');

            links.on('click', {el: this.el, multiple: this.multiple}, this.dropdown)
        }

        Accordion.prototype.dropdown = function(e) {
            var $el = e.data.el;
            $this = $(this),
            $next = $this.next();
            $next.slideToggle();
            $this.parent().toggleClass('open');

            if (!e.data.multiple) {
                $el.find('.submenu').not($next).slideUp().parent().removeClass('open');
            };
        }

        var accordion = new Accordion($('#accordion'), false);
    })
</script>
    <script>
    document.addEventListener('DOMContentLoaded', function() {
        var handleFormat = function(format) {
        $(format.listItem).click(function(e) {
                e.preventDefault();
                $(".formats-item-desktop").removeClass('open-format')
                $(format.listItem).addClass('open-format');
                $(".desk-submenu").removeClass('open-submenu');
                $(format.contentItem).addClass('open-submenu');
                var id  = $(this).attr('href'),
                    top = $(id).offset().top;
                $('body,html').animate({scrollTop: top}, 600);
            });
        };

        var formats = [
            {
                listItem: '#popsFormat',
                contentItem: '#popsInfo'
            },
            {
                listItem: '#pushnotFormat',
                contentItem: '#pushnotInfo'
            },
            {
                listItem: '#pushupFormat',
                contentItem: '#pushupInfo'
            },
            {
                listItem: '#prerollFormat',
                contentItem: '#prerollInfo'
            },
            {
                listItem: '#inpageFormat',
                contentItem: '#inpageInfo'
            },
            {
                listItem: '#skimFormat',
                contentItem: '#skimInfo'
            },
            {
                listItem: '#bannerFormat',
                contentItem: '#bannerInfo'
            },
        ];

        formats.forEach(function(format) {
            handleFormat(format);
        });
    })
</script>
    <script>
    $(function() {
        var advItem = document.getElementsByClassName('b-main__advantages__item__img');
        var advantages = document.getElementsByClassName('b-main__advantages__item');

        [...advantages].forEach((item) => {
            item.addEventListener('click', function(e) {
                if(!item.childNodes[3].childNodes[3].classList.contains('description-show')) {
                    descriptionHide();
                    item.childNodes[3].childNodes[3].classList.add('description-show');
                    item.childNodes[5].classList.add('toggle-rotate');
                } else {
                    descriptionHide();
                }
            })
        })
        
        // toggle discription display on mobile

        function descriptionHide() {
            [...advantages].forEach((item) => {
                item.childNodes[5].classList.remove('toggle-rotate');
                item.childNodes[3].childNodes[3].classList.remove('description-show');
            })
        }
        
        // show icons on scroll
        
        $(window).scroll(function() {
            var wH = $(window).height(),
                wS = $(this).scrollTop(),
                heightDiff = wH > 1100 ? wH + 350 : wH + 1000;
           
            if (wS > heightDiff){
                [...advItem].forEach((item) => {
                    item.classList.add('lift-animation');
                })
            }
        });
    });
</script>
</div>
