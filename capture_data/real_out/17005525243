      SendVirtualPageView: function (page, title, action, uid, itemid, itemtitle, reason) {
          var isQueryStringInserted = false;
          var page = page + 'vpv/' + action;
          if (reason != undefined) {
              page += '/' + reason;
          }
          if (uid != undefined) {
              page += '?uid=' + uid;
              isQueryStringInserted = true;
              title += '، اپراتور: ' + uid;
