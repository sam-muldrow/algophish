            (function ($) {
                $(document).ready(function () {
                    var slideDir = $("html").attr("dir") == "rtl" ? true : false;
                    var sliderSelector = "";

                    // Init wrapper
                    $("#block_4598d8a5b407fa1529519e0a5195ae16 .slider-container").on('init', function(event, slick){
                        $(this).addClass("slider-initialized");
                    })

                    var sliderData = $("#block_4598d8a5b407fa1529519e0a5195ae16 .slider-container").slick({
                        infinite: false,
                        slidesToScroll: 1,
                        slidesToShow: 1,
                        arrows: false,
                        dots: true,
                        centerMode: false,
                        centerPadding: "px",
                        speed: 500,
                        // speed: ,
                        autoplay: true,
                        autoplaySpeed: 5000,
                        swipe: true,
                        mobileFirst: true,
                        responsive: [
                            {
                            breakpoint: 768,
                            settings: 'unslick'
                            }
                        ]
                    })

                    .on("beforeChange", function (event, slick, currentSlide, nextSlide) {
                        $("#block_4598d8a5b407fa1529519e0a5195ae16 .slick-slide").addClass("sliding");
                        
                        var calc = ( (nextSlide) / (slick.slideCount-slick.options.slidesToShow) ) * 100;
                        
                        $('#block_4598d8a5b407fa1529519e0a5195ae16 .slick-progress').css('background-size', calc + '% 100%').attr('aria-valuenow', calc );
                        $('#block_4598d8a5b407fa1529519e0a5195ae16 .slider__label' ).text( calc + '% completed' );
                    })
                    .on("afterChange", function (event, slick, currentSlide, nextSlide) {
                        $("#block_4598d8a5b407fa1529519e0a5195ae16 .slick-slide").removeClass("sliding");
                    })
                    .on('edge', function(event, slick, direction){
                        //console.log('edge was hit');
                    });

                    // Custom carousel nav
                    $('#block_4598d8a5b407fa1529519e0a5195ae16 .previous').click(function(e){ 
                        $('#block_4598d8a5b407fa1529519e0a5195ae16 .slider-container').slick('slickPrev');
                    } );
                    
                    $('#block_4598d8a5b407fa1529519e0a5195ae16 .next').click(function(e){
                        e.preventDefault(); 
                        $('#block_4598d8a5b407fa1529519e0a5195ae16 .slider-container').slick('slickNext');
                    } );
                }); //end ready
            })(jQuery);
