            <script data-nolazy type="text/javascript">window.addEventListener('DOMContentLoaded', function() {
            (function ($) {
                function setSlideHeights() {
                    var tallestSlideHeight = 0;
                    var $slides = $('.stack-slider .slider-container .slick-slide');
                    
                    // Reset slide heights
                    $slides.css('min-height', '');

                    $slides.each(function() {
                    var slideHeight = $(this).outerHeight();
                    if (slideHeight > tallestSlideHeight) {
                        tallestSlideHeight = slideHeight;
                    }
                    });
                    
                    $slides.css('min-height', tallestSlideHeight + 'px');
                }

                $(document).ready(function () {
                    
                var slideDir = $("html").attr("dir") == "rtl" ? true : false;
                var sliderSelector = "";

                // Init wrapper
                $("#block_85cd2e413d6a4e87d46536a78006029d .slider-container").on('init', function(event, slick){
                    $("#block_85cd2e413d6a4e87d46536a78006029d").addClass("slider-initialized");

                    if (slick.slideCount > slick.options.slidesToShow) {
                        $("#block_85cd2e413d6a4e87d46536a78006029d .slider-controls-wrapper").removeClass("d-none");
                    }else{
                        $("#block_85cd2e413d6a4e87d46536a78006029d .slider-controls-wrapper").addClass("d-none");
                    }
                    
                    // If platform parts inside the slide
                    if ($('.slick-slide[data-slick-index="0"]').find('.platform-parts').length > 0) {
                        $('.slick-slide').find('.platform-parts').removeClass("animate");
                        $('.slick-slide[data-slick-index="0"]').find('.platform-parts').addClass("animate");
                    }

                    // Equal height slider
                    if ($('#block_85cd2e413d6a4e87d46536a78006029d').parents().hasClass('stack-slider')) {
                        setSlideHeights();   
                    }
                })
                $("#block_85cd2e413d6a4e87d46536a78006029d .slider-container").on('breakpoint', function(event, slick){
                    if (slick.slideCount > slick.options.slidesToShow) {
                        $("#block_85cd2e413d6a4e87d46536a78006029d .slider-controls-wrapper").removeClass("d-none");
                    }else{
                        $("#block_85cd2e413d6a4e87d46536a78006029d .slider-controls-wrapper").addClass("d-none");
                    }
                })


                // Init slider
                var sliderData = $("#block_85cd2e413d6a4e87d46536a78006029d .slider-container").slick({
                    fade: false,
                    rtl: slideDir,
                    infinite: true,
                    slidesToScroll: 1,
                    slidesToShow: 6,
                    arrows: false,
                    dots: false,
                    centerMode: false,
                    centerPadding: "px",
                    speed: 1000,
                    autoplay: true,
                    autoplaySpeed: 5000,
                    swipe: false,
                    responsive: [
                        {
                            breakpoint: 900,
                            settings: {
                                infinite: true,
                                slidesToScroll: 1,
                                slidesToShow: 4,
                                arrows: false,
                                dots: false,
                                centerMode: false,
                                centerPadding: "px",
                                speed: 1000,
                                autoplay: true,
                                autoplaySpeed: 5000,
                            },
                        },
                        {
                            breakpoint: 599,
                            settings: {
                                infinite: true,
                                slidesToScroll: 1,
                                slidesToShow: 2,
                                arrows: false,
                                dots: false,
                                centerMode: false,
                                centerPadding: "px",
                                // speed: ,
                                autoplay: true,
                                autoplaySpeed: 5000,
                                swipe: true,
                            },
                        },
                    ],
                })
                .on("beforeChange", function (event, slick, currentSlide, nextSlide) {
                    // If platform parts inside the slide
                    if ($('#block_85cd2e413d6a4e87d46536a78006029d .slick-slide[data-slick-index="'+nextSlide+'"]').find('.platform-parts').length > 0) {
                        $("#block_85cd2e413d6a4e87d46536a78006029d .slider-container .slick-slide .platform-parts").removeClass("animate");
                        $('#block_85cd2e413d6a4e87d46536a78006029d .slider-container .slick-slide[data-slick-index="'+nextSlide+'"]').find('.platform-parts').addClass("animate");
                    }
                    $("#block_85cd2e413d6a4e87d46536a78006029d .slick-slide").addClass("sliding");
                    
                    var calc = ( (nextSlide) / (slick.slideCount-slick.options.slidesToShow) ) * 100;
                    
                    $('#block_85cd2e413d6a4e87d46536a78006029d .slick-progress').css('background-size', calc + '% 100%').attr('aria-valuenow', calc );
                    $('#block_85cd2e413d6a4e87d46536a78006029d .slider__label' ).text( calc + '% completed' );
                    
                })
                .on("afterChange", function (event, slick, currentSlide, nextSlide) {
                    $("#block_85cd2e413d6a4e87d46536a78006029d .slick-slide").removeClass("sliding");

                    // Equal height slider
                    if ($('#block_85cd2e413d6a4e87d46536a78006029d').parent().hasClass('stack-slider')) {
                        setSlideHeights();   
                    }
                })
                .on('edge', function(event, slick, direction){
                    // console.log('edge was hit');
                });
                

                // Custom carousel nav
                $('#block_85cd2e413d6a4e87d46536a78006029d .previous').click(function(e){ 
                    $('#block_85cd2e413d6a4e87d46536a78006029d .slider-container').slick('slickPrev');
                } );
                
                $('#block_85cd2e413d6a4e87d46536a78006029d .next').click(function(e){
                    e.preventDefault(); 
                    $('#block_85cd2e413d6a4e87d46536a78006029d .slider-container').slick('slickNext');
                } );
                }); //end ready
            })(jQuery);
        });</script>
    </div>
</div>
</div>
	</div>
</article>
	</main>



