<script type="text/javascript">    
    backTop();
    $(window).scroll(function () {
        backTop();
    });

    $('#back-top').click(function () {
        $('body,html').animate({ scrollTop: 0 }, 500);
    });

    function backTop() {
        if ($(document).scrollTop() > 0) {
            $('#back-top').slideDown();
        } else {
            $('#back-top').slideUp();
        }
    }



    //移动端侧边栏 2020-06-24
    $(".kf-close").click(function () {
        $('.kf-more').show();
        $('.service-bar').hide();
        $('.kf-close').hide();
    });
    $(".kf-more").click(function () {
        $('.kf-more').hide();
        $('.service-bar').show();
        $('.kf-close').show();
    });

</script>
<!--右侧在线客服end-->

