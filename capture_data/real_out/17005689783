    <script type="text/javascript">
        $(document).ready(function () {
            $(function () {
                $(".home-box-hover .home-list li .text .text-list").hover(function () {
                    $(this).parents(".home-list li").css({ "top": "-10px" });
                }, function () {
                    $(this).parents(".home-list li").css({ "top": "0" });
                })
            })
        });
    </script>
