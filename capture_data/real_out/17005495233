<script>
    $(function () {
        var quizLink = document.querySelector('.businessLinks__item__linkQuiz');
        if (quizLink){
            quizLink.addEventListener('click', function(){
                goToQuiz();
            });
        }
        
        if (window.location.hash && window.location.hash.substring(1) == 'Quiz') {
            goToQuiz();
        }

        function goToQuiz(){
            $('html,body').animate({scrollTop: 9999}, 5000);

            window.addEventListener('scroll', function () {
                var element = document.querySelector('div.bxKonkursySonda__item--quiz');
                var position = element.getBoundingClientRect();

                if (position.top < 50 && position.bottom >= 0) {
                    $('html,body').stop();
                }
            });
        }
    });
</script>

