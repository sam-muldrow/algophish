                        <div class="bx banner banner-g4"><ins data-revive-zoneid="505" data-revive-block="1"
     data-revive-id="3d1e790fba67c63e0197bd8341256420"></ins>
<div id="banner-vwa_test"></div>
<script src="https://s-trojmiasto.pl/_js/a_d_s/viewable_ad.js?9"></script>

<script>
  var bxBannertriggered = [];

  function bannerExecuteLazy(banner) {
    var threshold = 200; //px
    var bxBanner = $("div#banner-" + banner.ifid).offset().top,
      windowHeight = $(window).height();
    if ($(this).scrollTop() >= bxBanner - windowHeight - threshold && !bxBannertriggered[banner.ifid]) {
      executeBanner(banner);
      bxBannertriggered[banner.ifid] = true;
    }
  }

  function executeBanner(banner) {
    return $.ajax({
      method: "GET",
      url: "https://r.trojmiasto.pl/www/delivery/asyncspc.php?zones=" + banner.zoneid + "&prefix=&block=1&loc=" + encodeURIComponent(document.location),
      dataType: "json",
      crossDomain: true,
      xhrFields: {
        withCredentials: true
      },
      cache: false
    }).done(function(data) {
      var $banner = $("div#banner-" + banner.ifid);
      var elements = $.parseHTML(data[0].html, null, true);

      $(elements).filter("div[id^='beacon_']").each(function() {
        var $img = $(this).find("img");
        if (!$img.length) return;

        var impression_url = $img.attr("src");
        $img.remove();

        var modifiedHtmlString = elements.map(function(el) {
          return el.outerHTML;
        }).join("");

        $banner.html(modifiedHtmlString);
        new ViewableAd($banner[0], impression_url).execute();
      });
    }).fail(function() {
      console.error("Error loading banner: " + banner.ifid);
    });
  }

  $(function() {
    $(window).scroll(function() {
      bannerExecuteLazy({
        zoneid: "1012",
        ifid: "vwa_test"
      });
    });
  });
</script>
</div>

