<div class="mobileversion">
    <p class="mobileversion-text">Przeglądaj wygodniej w wersji mobilnej</p>
    <a href="https://m.trojmiasto.pl/"><span class="mobileversion-button">Przełącz</span></a>
    <button class="mobileversion-close">
        <span class="trm trm-close"></span>
    </button>
</div>
<script>
    
    $(document).ready(function() {
        if (document.cookie.replace(/(?:(?:^|.*;\s*)mobile_device\s*\=\s*([^;]*).*$)|^.*$/, "$1") == 1 &&
            document.cookie.replace(/(?:(?:^|.*;\s*)showMobileVersionBanner\s*\=\s*([^;]*).*$)|^.*$/, "$1") !== "true") {
            $('.mobileversion').show();
            $('.mobileversion-close').on('click', function() {
                $('.mobileversion').fadeOut(200);
                var date = new Date();
                date.setTime(date.getTime()+(24*60*60*1000));
                document.cookie = "showMobileVersionBanner=true; expires=" + date + "; path=/; domain=trojmiasto.pl";
            });

            $('.mobileversion').css({'position': 'fixed', 'left': '2vw', 'bottom': 'none', '-webkit-transform': 'none', 'transform': 'none'});
            var overlay = document.querySelector('.mobileversion');
            var margin = 2 * document.documentElement.clientWidth / 100;

            $(window).on('scroll', function(){
                overlay.style.position = 'absolute';
                overlay.style.left = window.pageXOffset + margin * window.innerWidth/document.documentElement.clientWidth + 'px';
                overlay.style.bottom = document.documentElement.clientHeight - (window.pageYOffset + window.innerHeight) + 'px';
                overlay.style["-webkit-transform"] = "scale(" + window.innerWidth/document.documentElement.clientWidth + ")";
                overlay.style["transform"] = "scale(" + window.innerWidth/document.documentElement.clientWidth + ")";
            });

            $(window).trigger('scroll');
        }
    });
    
</script>


    
