            var search_text = 'szukaj w Trójmieście...';
            $("#search_input").val(search_text);

            $("#search_input").focus(function(){
                if($(this).val()==search_text)
                {
                    $(this).val('');
                }
            });

            $("#search_input").blur(function(){
                if($(this).val()=='')
                {
                    $(this).val(search_text);
                }
            });

