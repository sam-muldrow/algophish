<script>

var specialPrevImg = {'hot':'https://gimgs.click108.com.tw/images/index_20111118/fbox_bt01c.gif','heart':'https://gimgs.click108.com.tw/images/index_20111118/fbox_bt02c.gif', 'astroPK':'https://gimgs.click108.com.tw/images/index_20111118/fbox_bt03c.gif'};

var specialDefaultImg = {'profit':'https://gimgs.click108.com.tw/images/index_20111118/fbox_bt01a.gif','hot':'https://gimgs.click108.com.tw/images/index_20111118/fbox_bt02a.gif','heart':'https://gimgs.click108.com.tw/images/index_20111118/fbox_bt03a.gif','astroPK':'https://gimgs.click108.com.tw/images/index_20111118/fbox_bt04a.gif'};

var specialCurrentImg = {'profit':'https://gimgs.click108.com.tw/images/index_20111118/fbox_bt01b.gif','hot':'https://gimgs.click108.com.tw/images/index_20111118/fbox_bt02b.gif', 'heart':'https://gimgs.click108.com.tw/images/index_20111118/fbox_bt03b.gif', 'astroPK':'https://gimgs.click108.com.tw/images/index_20111118/fbox_bt04b.gif'};

var tplTab = '<table width="510" height="280" border="0" cellspacing="0" cellpadding="0"><tr><td align="center" valign="middle" background="https://gimgs.click108.com.tw/images/index_20111118/fbox_bg.gif">資料讀取中...</td></tr><tr><td valign="bottom" height="8"><img src="https://gimgs.click108.com.tw/images/index_20111118/fbox_foot.gif" /></td></tr></table>';

$(function(){

var $specialDIV = $('#special div');

function specialReset()
{
	jQuery.each(specialDefaultImg, function(key, value){
		$('#'+key+'Link').parent().attr('background', value);
		$('#'+key+'Link').removeClass('txt01_3').addClass('link08');
	});
}

function specialToggle()
{
	$specialDIV.hide();
	var $overing = $(this).attr('id');
	var tabName = $overing.substring(0, $overing.indexOf('Link'));
	specialReset();
	$(this).parent().attr('background', specialCurrentImg[tabName]);
	$(this).removeClass('link08').addClass('txt01_3');
	if(tabName!="profit")
		$(this).parent().prev().attr('background', specialPrevImg[tabName]);
	var $currentID = $('#'+tabName);
	$currentID.show();
	var $currentIDhtml = $currentID.html();
	if($currentIDhtml == '' || $currentIDhtml == tplTab)
	{
		$currentID.html(tplTab);
        if(tabName=="profit" || tabName=="hot"){
               $fname=tabName + '_www';
            }else{
                $fname=tabName;
            }
		$.ajax({
              url: './tpl/content/'+ $fname + '.php',  
			  success: function( data ) {
				  $currentID.html(data);
			  }
			});
	}
	return false;
}
$('#profitLink').mouseover(specialToggle);
$('#hotLink').mouseover(specialToggle);
$('#heartLink').mouseover(specialToggle);
$('#astroPKLink').mouseover(specialToggle);
var specialDefault=new Array('profit','hot','heart','astroPK');
$('#'+specialDefault[1-1]+'Link').trigger('mouseover');
});
</script>
