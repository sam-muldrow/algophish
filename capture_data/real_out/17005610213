<script language="javascript" type="text/javascript">
   function hideline(){
     $("#linebanner").hide();
   }
   $(function(){
	 var compdate='2017-02-14 10:00:00';
    var now = new Date;
    var d = new Date(compdate);  //IE祥盓厥Date()������
     if (now > d) {
        $("#linebanner").hide();
    }else{
	    $("#linebanner").hide();
	}
   });
</script>
