<script>
  $(function () {
      window.wenqu = {};
      window.wenqu.groups = [];
    $('#form251 .wq-group').each(function() {
        var group = {};
        group.wqInput = [];
        group.wqSex = [];
        group.wqDateTime = [];

        $(this).find('.wq-input').each(function() {
            group.wqInput.push(new wqInput(this));
        });

        $(this).find('.wq-select[data-type=sex]').each(function() {
            group.wqSex.push(new wqSex(this));
        });

        $(this).find('.wq-datetime-group').each(function() {
            group.wqDateTime.push(new wqDateTime(this));
        });

        window.wenqu.groups.push(group);
    });
   
    wqDateTime.prototype.dateFormat = function dateFormat(calendar, year, month, day) {
        year = parseInt(year, 10);
        month = parseInt(month, 10);
        day = parseInt(day, 10);

        var addZero = function addZero(val) {
            val = val.toString();
            if (val.length < 2) {
	val = '0' + val;
            }
            return val;
        };

        var monthStr =  addZero(month);
        var dayStr = addZero(day);
         if(month < 0){
           $('#form251').find('input[name=LeapMonth]').val('1'); 
           monthStr=addZero(Math.abs(monthStr))+'(\u958f)';		        
         }else{
            $('#form251').find('input[name=LeapMonth]').val('0');  		        
         }
         
         var yearstr='';	
         if(calendar==1){
            yearstr='\u897f\u5143';		        
         }else{
           yearstr='\u8FB2\u66c6';			        
         }
         
        return yearstr+ year + '\u5E74' + monthStr + '\u6708' + dayStr + '\u65E5';
    };  
     getBLen251 = function(str) {
          if (str == null) return 0;
          if (typeof str != "string"){
            str += "";
          }
          return str.replace(/[^\x00-\xff]/g,"01").length;
        } 
     $('#checkGo_free251').click(function (e) {
        e.preventDefault();
        var name1=$('#NickName_251').val(); 
        var valen1=getBLen251(name1); 
         if(name1.trim().length == 0){
           $("#NickName_251").val(""); $('#NickName_251').addClass('error');
         } 
        if(valen1 > 10 || /^[\w\u4E00-\u9FA5\uF900-\uFA2D]*$/.test(name1) == false){
             $('#NickName_251').addClass('error');
             (new wqPopup()).alert("姓名欄位格式錯誤，最多可輸入5個中文字或是10個英文字，謝謝！");
              
             return;
        } 
        var valid = wq_form['form251'].validation({ valid_action: "",birth_title:"生辰" });
    }); 
    wq_form['form251'] = new wqForm($('#form251')); 
});

</script>
<style>
  input::-ms-clear{display:none;}
     .wq-select{
    color :#000;
  } 
