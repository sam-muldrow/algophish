<script> 
                     var agreeGdprPop=true;
            setCookie_gdpr("agreeGdprPop",true); 
              
        function agreeGdprClose(t){
            if(t==1)  $('#FRAME_ALL').hide(); 
             setCookie_gdpr("agreeGdprPop",'2');  
             agreeGdprPop=false;  
            $.ajax({                                    
            type: "POST",
            url: "/include/gdpr/GDPR_ajax.php", 
            data: {gtype:t,from:"PC"},
            dataType:"json",
            success: function(data){ 
            }
           });
        }
        function setCookie_gdpr(name,value){
            var exp  = new Date();   
            var domain_c ="click108.com.tw";
            exp.setTime(exp.getTime() +  365 * 24 * 60 * 60 * 1000);
            document.cookie = name + "="+ escape (value) + ";domain="+domain_c +";expires=" + exp.toGMTString()+";path=/";
        }
         $(document).ready(function () {  
            $(window).scroll(function () {
            var t = document.documentElement.scrollTop || document.body.scrollTop; 
            if($('#news_footer').offset() != null){hidefooter("#news_footer"); }
            else if($('.FOOT_PC').offset() != null){hidefooter(".FOOT_PC");}
            else if($('.FOOTER').offset() != null){hidefooter(".FOOTER"); }    
            else{
                    if( (t+$(window).height()+50>$("body table").last().offset().top) ){
                        // alert(t+$(window).height()+'----'+$("body table").last().offset().top);
                        $('#FRAME_ALL').hide();
                    } else {                       
                        if(agreeGdprPop){
                            $("#FRAME_ALL").show();
                        }
                    }                    
                } 
        });
        });   
        function hidefooter(obj){
             var t = document.documentElement.scrollTop || document.body.scrollTop; 
             if($(obj).offset() != null){ 
                    if( (t+$(window).height()+50>$(obj).offset().top) ){ 
                    //alert(t+'---'+$(window).height()+'----'+$("#news_footer").offset().top) ;
                         if(t<10 && agreeGdprPop)   $("#FRAME_ALL").show();                  
                         else $('#FRAME_ALL').hide();
                    }else {
                        if(agreeGdprPop){
                            $("#FRAME_ALL").show();
                        }
                    }                   
            }
        } 
        $(function(){
            if(agreeGdprPop){ 
                $(".FRAME_ALL").show();              
            }
        });            
    </script>
    
    </div>

<script type="text/JavaScript">
$(function(){

  //sam新書專區
     $.get('/book/manage/indexbanner.php?t=' + (new Date()).getTime(),function(content){
	  $('#bookmange').html(content);
    });

    //加入我的最愛
    $("#Bookmark").click(function(e){
	// e.preventDefault(); // this will prevent the anchor tag from going the user off to the link

	var bookmarkUrl = this.href;
	var bookmarkTitle = this.title;

        if (window.sidebar && window.sidebar.addPanel) { // For Mozilla Firefox < 23
            e.preventDefault();
		window.sidebar.addPanel(bookmarkTitle, bookmarkUrl,"");
	} else if(window.sidebar && ! (window.sidebar instanceof Node)) { // Firefox >23
		    $("#Bookmark").attr('rel', 'sidebar').attr('title', bookmarkTitle);
		return true;
	} else if((window.external || document.all) && window.external.AddFavorite) { // For IE Favorite
            e.preventDefault();
		window.external.AddFavorite( bookmarkUrl, bookmarkTitle);
	} else if(window.opera) { // For Opera Browsers
            e.preventDefault();
		$("#Bookmark").attr("href",bookmarkUrl);
		$("#Bookmark").attr("title",bookmarkTitle);
		$("#Bookmark").attr("rel","sidebar");
	} else { // for other browsers which does not support
             e.preventDefault();
		 alert('加入我的最愛失敗，請使用Ctrl+D進行添加');
		 return false;
	}
    });

});
</script>
