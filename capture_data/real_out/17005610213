<script language="javascript" type="text/javascript">
  function show_video(num,cid){
    for(var i=0;i<5;i++){
        if(num==i){
        $("#video_class_menu"+i).removeClass("LINK");
        $("#video_class_menu"+i).addClass("LINK");
         $("#video_content"+i).show();
        }else{
          $("#video_class_menu"+i).removeClass("LINK");
          $("#video_content"+i).hide();
        }
    }
  }
  function show_video_pop(cid,id){
      $("#pop_videobanner_div").show();
      var src=$("#video_content_"+cid+"_"+id).attr("data_url");
      var t=$("#video_content_"+cid+"_"+id).attr("data_title");
      var rec=$("#video_content_"+cid+"_"+id).attr("data_rec");
      var r_url=$("#video_content_"+cid+"_"+id).attr("data_rec_url");
      $("#iframe_v_url").attr("src",src);
      $("#ifram_v_title").html(t);
      $("#ifram_v_rec_url").attr("href",r_url);
      $("#ifram_v_rec_title").html(rec);
  }
  function closevideob(){
    $("#pop_videobanner_div").hide();
    $("#iframe_v_url").attr("src","");
  }
</script>
