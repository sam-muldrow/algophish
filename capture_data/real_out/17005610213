<script>

function getAstroID(iDate)
{
	 var tDay , tAstroID = 0 ,tDateInfo = /^\s*(\d{4})[-/]?(\d{2})[-/]?(\d{2})\s*$/.exec(iDate);
	 (tDateInfo.length > 0) && (tDay = tDateInfo[2] + '' + tDateInfo[3]) || (tDay = '0101');
      (tDay >= '0120' && tDay <= '0218') && (tAstroID = 10) //水瓶座01/20~02/18
   || (tDay >= '0219' && tDay <= '0320') && (tAstroID = 11) //雙魚座02/19~03/20
   || (tDay >= '0420' && tDay <= '0520') && (tAstroID = 1)  //金牛座04/20~05/20
   || (tDay >= '0521' && tDay <= '0620') && (tAstroID = 2)  //雙子座05/21~06/21
   || (tDay >= '0621' && tDay <= '0722') && (tAstroID = 3)  //巨蟹座06/22~07/22
   || (tDay >= '0723' && tDay <= '0822') && (tAstroID = 4)  //獅子座07/23~08/22
   || (tDay >= '0823' && tDay <= '0922') && (tAstroID = 5)  //處女座08/23~09/22
   || (tDay >= '0923' && tDay <= '1023') && (tAstroID = 6)  //天秤座09/23~10/23
   || (tDay >= '1024' && tDay <= '1122') && (tAstroID = 7)  //天蠍座10/24~11/22
   || (tDay >= '1123' && tDay <= '1221') && (tAstroID = 8)  //射手座11/23~12/21
   || (tDay >= '1222' || tDay <= '0119') && (tAstroID = 9)  //摩羯座12/22~01/19
   || (tDay >= '0321' && tDay <= '0419') && (tAstroID = 0); //牡羊座03/21~04/19
	 return  tAstroID ;
}
function switchFreeEverydayAction(item)
    {
        var action;
        //var target = '_self';
        switch (item)
        {
            case 1:
            action = '/unit001/free00063/result_free.php';
            $("#_menustr").val("zwluck");
              break;
            case 2:
            action = '/unit001/free00063/result_free.php';
            $("#_menustr").val("mgstar");
              break;
            case 3:
              action = '/unit001/free-ziwei/result.php';
            $("#_menustr").val("zwmp");
            $('#formFreeEveryday').attr("accept-charset",'utf-8');
              break;
            case 4:
             action = '/unit001/year-fate/input.php';
             $('#ItemID').val("YH024");
            break;
            case 5:
            action = 'http://astro.click108.com.tw/daily_01.php';
            break;
             case 6:
            action = '/unit001/free00061/index.php';
            break;
             case 7:
            action = '/unit001/free00062/index.php';
            break;
             case 8:
            action = '/unit001/item00191/free.php';
            break;
            default:
              action = '';
        }

        $('#formFreeEveryday').attr("action", action);
        //form_free_everyday.attr('target', target);
    }
function compTime(firstDate, secondDate) {
        var fisrtTime;
        var secondTime;
        if (firstDate && firstDate.length > 0) {
            firstDate = firstDate.replace(" ", "-").replace(":", "-");
            var arr = firstDate.split("-");
            fisrtTime = Date.UTC(arr[0], (arr[1] - 1), arr[2], arr[3], arr[4], 0);
        }
        if ((secondDate && secondDate.length > 0)) {
            secondDate = secondDate.replace(" ", "-").replace(":", "-");
            var arrs = secondDate.split("-");
            secondTime = Date.UTC(arrs[0], (arrs[1] - 1), arrs[2], arrs[3], arrs[4], 0);
        }
        return fisrtTime > secondTime;
}
function freeEverydaySubmit(page){
    var iNiceName=$('#_iNickName').val();
     if(iNiceName==''){
        alert('請輸入您的姓名或暱稱！');
        return;
    }
     if($('input:radio[name="iSex"]:checked').val()==null){
        alert("請記得選擇您的性別");
        return;
    }
    var tYear,tMonth,tDay;
           $('#_NickName').val(iNiceName);
            $('#_Nickname').val(iNiceName);
            tYear = parseInt($("#_iYear option:selected").val(),10);
            tMonth = parseInt($("#_iMonth option:selected").val(),10);
            tDay = parseInt($("#_iDay option:selected").val(),10);

            var MaxDay = ((tMonth == 2)?(((tYear%4 == 0) && (tYear%100 != 0) || (tYear%400 == 0)) ? 29 : 28 ):((tMonth == 1 || tMonth == 3 || tMonth == 5 || tMonth == 7 || tMonth == 8 || tMonth == 10 || tMonth == 12)? 31 : 30 ));

            if (tDay > MaxDay)
            {
	alert("陽曆" + tYear + "年" + tMonth + "月只有" + MaxDay + "天哦！");
	return false;
            }
            var freeEveryDayReferkey= $('#free_key0'+page).attr('refer_key');
            //alert(freeEveryDayReferkey);
            switchFreeEverydayAction(page);
            if(page==5)       //  星座運勢
            {
	var iYear = $("#_iYear option:selected").text();
	var iMonth = $("#_iMonth option:selected").text();
	var iDay = $("#_iDay option:selected").text();
	var astroid=getAstroID(iYear + '-' + iMonth + '-' + iDay)
	$("#_iAstro").val(astroid);
	 $.get('/unit008/Publicize/doClickCount.php?pubKey=' + freeEveryDayReferkey, function(){});
	 $('#formFreeEveryday').attr("accept-charset","utf-8");
	 $('#formFreeEveryday').attr('action','/unit001/item00236/input.php?Vender=pc_free_freeday_it236'); //http://astro.click108.com.tw/daily_'+astroid+'.php
	 $('#formFreeEveryday').submit();
            }else{
	 $.get('/unit008/Publicize/doClickCount.php?pubKey=' + freeEveryDayReferkey, function(){});
	$('#formFreeEveryday').submit();
            }
}
</script>
