       var preventAutoInit = true;
        $(function(){
            window.wenqu248 = {};
            window.wenqu248.groups = [];
            $('#form248 .wq-group').each(function() {
	var group = {};
	group.wqInput = [];
	group.wqSex = [];
	group.wqDateTime = [];

	$(this).find('.wq-input').each(function() {
	    group.wqInput.push(new wqInput(this));
	});

	$(this).find('.wq-select[data-type=sex]').each(function() {
	    group.wqSex.push(new wqSex(this));
	});

	$(this).find('.wq-datetime-group').each(function() {
	    group.wqDateTime.push(new wqDateTime(this));
	});

	window.wenqu248.groups.push(group);
            });
            $('#checkGo_free248').click(function(e) {
	e.preventDefault();
     $.get("/Promote/PromoteX.php?Vender=index_freezone_it248"); 
	var action = $(this).data('href');
	form_validation248('form248', action);
            });

            function form_validation248(form_id, action) {
	var valid = true;
	var _errMsg = [];
	var _errGroupTitle = ['你的','他的'];
	//console.log(_errGroupTitle[0]);
	//console.log(_errGroupTitle[1]);
	var _errTitle = ['姓氏', '名字'];
	var validation = function(i) {
	    var _v = true;
	    var _eM = [];

	    for (var j in window.wenqu248.groups[i].wqInput) {
	        // console.log(`i=${i}, j=${j}`);
	        if(!window.wenqu248.groups[i].wqInput.hasOwnProperty(j)){
	            continue;
	        }
	        var v = window.wenqu248.groups[i].wqInput[j].validation();
	        if (!v.valid) {
	            _v = false;
	            if (v.errMsg.length > 0) {
		_eM.push(_errGroupTitle[i] + _errTitle[j] + '：' + v.errMsg + '！');
	            }
	        }
	    }
	    if (!window.wenqu248.groups[i].wqSex[0].validation()) {
	        _v = false;
	        _eM.push(_errGroupTitle[i] + '性別欄有誤！');
	    }
	    if (!window.wenqu248.groups[i].wqDateTime[0].validation()) {
	        _v = false;
	        _eM.push(_errGroupTitle[i] + '生日有誤！');
	    }

	    return [_v, _eM];
	}

	var v1 = validation(0);
	var v2 = [true, []];

	v2 = validation(1);

	valid = v1[0] && v2[0];
	_errMsg = v1[1].concat(v2[1]);

	if (valid) {
	    var confirmMsgElements = [];

	    var group1 = [];
	    group1.push({ title: _errGroupTitle[0] + '姓氏', value: window.wenqu248.groups[0].wqInput[0].elem.val() });
	    group1.push({ title: _errGroupTitle[0] + '名字', value: window.wenqu248.groups[0].wqInput[1].elem.val() });
	    group1.push({ title: _errGroupTitle[0] + '性別', value: window.wenqu248.groups[0].wqSex[0].elem.find('option:selected').text() });
	    group1.push({ title: _errGroupTitle[0] + '生日', value: window.wenqu248.groups[0].wqDateTime[0].toSolarString() });

	    confirmMsgElements.push(group1);

	        var group2 = [];
	        group2.push({ title: _errGroupTitle[1] + '姓氏', value: window.wenqu248.groups[1].wqInput[0].elem.val() });
	        group2.push({ title: _errGroupTitle[1] + '名字', value: window.wenqu248.groups[1].wqInput[1].elem.val() });
	        group2.push({ title: _errGroupTitle[1] + '性別', value: window.wenqu248.groups[1].wqSex[0].elem.find('option:selected').text() });
	        group2.push({ title: _errGroupTitle[1] + '生日', value: window.wenqu248.groups[1].wqDateTime[0].toSolarString() });

	        confirmMsgElements.push(group2);
	   

	    (new wqPopup()).confirmCustom(confirmMsgElements, {
	        form_id: form_id,
	        valid_action: action
	    }, function() {
	        for (var i = 0; i < window.wenqu248.groups.length; i++) {
	            window.wenqu248.groups[i].wqDateTime[0].applyLeapMonth();
	        }
	    });
	} else {
	    if (_errMsg.length > 0) {
	        (new wqPopup()).alert(_errMsg.join('<br>'));
	    }
	}
            }
            wq_form['form248'] = new wqForm($('#form248'));


        });
    </script> 
