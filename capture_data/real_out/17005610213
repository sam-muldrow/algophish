        });
        getBLen39 = function(str) {
            if (str == null) return 0;
            if (typeof str != "string"){
	str += "";
            }
        }
        function checkpg037POP(){
            if($("#fortune2015NamePOP").val()==''){
	$("#fortune2015NamePOP").addClass("error");
	return;
            }
            var name=$('#fortune2015NamePOP').val();
            var valen=getBLen39(name);
            if(valen > 10){
	$('#fortune2015NamePOP').addClass('error');
	(new wqPopup()).alert("姓名欄位格式錯誤，最多可輸入5個中文字或是10個英文字，謝謝！");
	return;
            }
            if($("#fortune2015EmotionPOP").val()=='-1'){
	$("#fortune2015EmotionPOP").addClass("error");
	return;
            }
            if($("#fortune2015ChildPOP").val()=='-1'){
	$("#fortune2015ChildPOP").addClass("error");
	return;
            }
            if($("#fortune2015CareerPOP").val()=='-1'){
	$("#fortune2015CareerPOP").addClass("error");
	return;
            }
            var val = parseInt($('#fortune2015YearPOP').val(), 10);

            if(val>=2016){
	(new wqPopup()).alert('您好，本流年服務未提供2016~2021年出生者批算，敬請見諒！');
	return;
            }else{
	$("#year201400301mempop").submit();
            }
        }

    </script>
