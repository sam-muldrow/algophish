    <!--
    var pic_num   = $("#show_pic li").size();
    var cur_index = 0;
    var time_out;
    function show_pic(){
        $("#show_pic li").each(function(i){
            if (i == cur_index){ $(this).fadeIn(1500); }
            else{ $(this).fadeOut(1500); }
        });
        $("#icon_num li").each(function(i){
            if (i == cur_index){ $(this).addClass('on').removeClass('off'); }
            else{ $(this).addClass('off').removeClass('on'); }
