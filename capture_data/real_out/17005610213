<script type="text/javascript">
   var samimgnum=0;
   var samimgtotal=2;   function showsamimg(){   //banner 輪播
        for(var i=0;i<samimgtotal;i++){
          if(i==samimgnum){
               $("#samimgdiv"+samimgnum).show();			         
          }else{
             $("#samimgdiv"+i).hide();			         
          }		       
        }  
       samimgnum+=1; 
      if(samimgnum >=samimgtotal){
         samimgnum=0;  
      } 
   }
    $(function(){
       showsamimg(); 
        setInterval("showsamimg();",5000);  
        });
</script>
