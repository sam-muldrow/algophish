$(function(){
  var mouse_y=$('[main_menu_id=fortune]').offset().top+$('[main_menu_id=fortune]').height();
  var back_mian_element;
  var back_sub_element;
  $("[main_menu_hover_background_image]").hover(
	  function (e) {
	     if (typeof(back_mian_element) != "undefined") {
		back_mian_element.hide();
		back_sub_element.css("background",'url('+ back_sub_element.attr('background')+')');
	     }
	    $('#index_default').hide();
	    $(this).css("background",'url('+$(this).attr('main_menu_hover_background_image')+')');
	    $('#submenu_'+$(this).attr('main_menu_id')).show();
	    //alert($(this).attr('main_menu_id'));
	  },
	  function (e) {

	    if(e.pageY<mouse_y){
		    $('#index_default').show();
		    $(this).css("background",'url('+ $(this).attr('background')+')');
		    $('#submenu_'+$(this).attr('main_menu_id')).hide();
		    //alert($(this).attr('main_menu_id'));
	    }else{
		    back_mian_element=$('#submenu_'+$(this).attr('main_menu_id'));
		    back_sub_element=$(this);
	    }
	  }
  );

 // alert($('[main_menu_id=fortune]').offset().top+$('[main_menu_id=fortune]').height());
});
</script>
