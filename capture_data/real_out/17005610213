 <script language="javascript" type="text/javascript">
       var preventAutoInit = true;
        $(function(){
            window.wenquyl024 = {};
            window.wenquyl024.groups = [];
            $('#formyl024 .wq-group').each(function() {
	var group = {};
	group.wqInput = [];
	group.wqSex = [];
	group.wqDateTime = [];

	$(this).find('.wq-input').each(function() {
	    group.wqInput.push(new wqInput(this));
	});

	$(this).find('.wq-select[data-type=sex]').each(function() {
	    group.wqSex.push(new wqSex(this));
	});

