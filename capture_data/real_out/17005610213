    function submit_relation_YF024_() {
        $('#YF024_form_input input[name="NickName"]').val($('#_NickName_YF024_').val());
        $('#YF024_form_input input[name="iSex"]').val($('#_iSex_YF024_').val());
        $('#YF024_form_input input[name="iYear"]').val($('#_iYear_YF024_').val());
        $('#YF024_form_input input[name="iMonth"]').val($('#_iMonth_YF024_').val());
        $('#YF024_form_input input[name="iDay"]').val($('#_iDay_YF024_').val());
        $('#YF024_form_input input[name="iHour"]').val($('#_iHour_YF024_').val());
        $('#YF024_form_input input[name="emotion_type"]').val($('#_emotion_type_YF024_').val());
        $('#YF024_form_input input[name="child_type"]').val($('#_child_type_YF024_').val());
        $('#YF024_form_input input[name="career_type"]').val($('#_career_type_YF024_').val());

        $('#YF024_form_input').attr('action', '/unit001/year-fate/YF024/free.php?Vender=index_freezone_YF024');
        $('#YF024_form_input').submit();
    }
    function relationSubmitForm_YF024_() {

      var payPath = "";
      $("#formYF024_").attr('action', payPath);
      $("#formYF024_").submit();
    }
    if (wq_form_pg == undefined) {
      console.log("wq_form_pg=" + wq_form_pg);
      var wq_form_pg = {};
    }

    $(document).ready(function() {
      wq_form_pg['formYF024_'] = new wqForm_pg($('#formYF024_'));
    });
    $(function() {
      //以下文曲處理
      $("#YF024_CheckGo").on("click", function(e) {

        wqInput_pg.prototype.validation = function validation() {
          var val = this.elem.val();
          var ret = true;
          var errMsg = "";
          var spaceRegExp = /\s/g;
          switch (true) {
            case val.length == 0:
              ret = false;
              this.elem.addClass("error");
              break;
            case spaceRegExp.test(val):
              ret = false;
              this.elem.addClass("error");
              break;
            case val.replace(/[^\x00-\xff]/g, "xx").length > 10:
              ret = false;
              errMsg =
	"姓名欄位格式錯誤，最多可輸入5個中文字或是10個非中文字，謝謝！";
              break;
          }
          if (!ret) {
            if (val.length == 0) {
              this.elem.val("");
            }
            this.elem.addClass("error");
          }
          return {
            valid: ret,
            errMsg: errMsg
          };
        };

        e.preventDefault();
        var action ='index.php'; //$(this).data("href");
        wq_form_pg.formYF024_.validation({
          valid_action: action,
        });
      });

      wq_form_pg.formYF024_.addCustomInput({
        name: "&#24863;&#24773;&#29694;&#27841;",
        // name: '感情現況',
        elem: $("#_emotion_type_YF024_"),
        validation: {
          check: function() {
            var val = parseInt($("#_emotion_type_YF024_").val(), 10);
            var ret = [8, 9, 11].indexOf(val) === -1 ? false : true;
            if (ret === false) {
              $("#_emotion_type_YF024_").addClass("error");
              $("#_emotion_type_YF024_").css("cssText", "background-color:#fc6!important;border:1px solid #f90!important");
            }
            return ret;
          },
          error: "",
        },
      });

      wq_form_pg.formYF024_.addCustomInput({
        name: "&#23376;&#24687;&#29694;&#27841;",
        // name: '子息現況',
        elem: $("#_child_type_YF024_"),
        validation: {
          check: function() {
            var val = parseInt($("#_child_type_YF024_").val(), 10);
            var ret = [0, 1].indexOf(val) === -1 ? false : true;
            if (ret === false) {
              $("#_child_type_YF024_").addClass("error");
              $("#_child_type_YF024_").css("cssText", "background-color:#fc6!important;border:1px solid #f90!important;");
            }
            return ret;
          },
          error: "",
        },
      });

      wq_form_pg.formYF024_.addCustomInput({
        name: "&#20107;&#26989;&#29694;&#27841;",
        // name: '事業現況',
        elem: $("#_career_type_YF024_"),
        validation: {
          check: function() {
            var val = parseInt($("#_career_type_YF024_").val(), 10);
            var ret = [2, 3, 4, 5, 6, 8, 7].indexOf(val) === -1 ? false : true;
            if (ret === false) {
              $("#_career_type_YF024_").addClass("error");
              $("#_career_type_YF024_").css("cssText", "background-color:#fc6!important;border:1px solid #f90!important;");
            }
            return ret;
          },
          error: "",
        },
      });
      $(
        "#_emotion_type_YF024_, #_child_type_YF024_, #_career_type_YF024_, #_NickName_YF024_"
      ).click(function() {
        $(this).removeClass("error");
        $(this).removeAttr("style");
        // $(this).css("cssText","background-color:#fc6!important;border:1px solid #f90!important;");
      });
      $("form").bind("keypress", function(e) {
        if (e.keyCode == 13) {
          e.preventDefault();
          return false;
        }
      });
    });
