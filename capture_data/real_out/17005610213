   function form_validationyl024(form_id, action) {
        var valid = true;
        var _errMsg = [];
        var _errGroupTitle = ['你的','他的'];
        var _errTitle = ['姓氏', '名字'];
        var validation = function(i) {
            var _v = true;
            var _eM = [];

            //for (var j in window.wenquyl024.groups[i].wqInput) {
            for (var j=0; j<window.wenquyl024.groups[i].wqInput.length; j++){
	var v = window.wenquyl024.groups[i].wqInput[j].validation();
	 let Space_test = /^[\w\u4E00-\u9FA5\uF900-\uFA2D]*$/.test(window.wenquyl024.groups[i].wqInput[j].elem.val())
	if (!v.valid ||  Space_test == false) {
	    _v = false;
	     if(Space_test == false && v.errMsg.length < 1){
            _eM.push(_errGroupTitle[i] + _errTitle[j] + '：' + "格式錯誤，僅可輸入中文字" + '！')
            window.wenquyl024.groups[i].wqInput[j].elem.addClass("error")
          }
	    if (v.errMsg.length > 0) {
	        _eM.push(_errGroupTitle[i] + _errTitle[j] + '：' + v.errMsg + '！');
	    }
	}
            }
            if (!window.wenquyl024.groups[i].wqSex[0].validation()) {
	_v = false;
	_eM.push(_errGroupTitle[i] + '性別欄有誤！');
            }
            if (!window.wenquyl024.groups[i].wqDateTime[0].validation()) {
	_v = false;
	_eM.push(_errGroupTitle[i] + '生日有誤！');
            }

            return [_v, _eM];
        }

        var v1 = validation(0);
        var v2 = [true, []];

        if ($('input[name=ilove]:checked', '#formyl024').val() != '0') {
            v2 = validation(1);
        }

        valid = v1[0] && v2[0];
        _errMsg = v1[1].concat(v2[1]);

        if (valid) {
            var confirmMsgElements = [];
            
            var leap1=window.wenquyl024.groups[0].wqDateTime[0].elem.leapmonth.val();
            var leap2=window.wenquyl024.groups[1].wqDateTime[0].elem.leapmonth.val();
            if(leap1>0){
	var m_value = window.wenquyl024.groups[0].wqDateTime[0].elem.month.find('option:selected').val();
	if(m_value > 0){
	    window.wenquyl024.groups[0].wqDateTime[0].elem.month.find('option:selected').val(-1 * m_value);
	}
            }
            if(leap2>0){
	var m_value = window.wenquyl024.groups[1].wqDateTime[0].elem.month.find('option:selected').val();
	if(m_value > 0){
	    window.wenquyl024.groups[1].wqDateTime[0].elem.month.find('option:selected').val(-1 * m_value);
	}
            }

            var group1 = [];
            group1.push({ title: _errGroupTitle[0] + '姓氏', value: window.wenquyl024.groups[0].wqInput[0].elem.val() });
            group1.push({ title: _errGroupTitle[0] + '名字', value: window.wenquyl024.groups[0].wqInput[1].elem.val() });
            group1.push({ title: _errGroupTitle[0] + '性別', value: window.wenquyl024.groups[0].wqSex[0].elem.find('option:selected').text() });
            if (window.wenquyl024.groups[0].wqDateTime[0].elem.calendar.val() == 0) {
	group1.push({
	    title: _errGroupTitle[0] + '生日',
	    value: window.wenquyl024.groups[0].wqDateTime[0].toLunarString()
	});
            }
            if (window.wenquyl024.groups[0].wqDateTime[0].elem.calendar.val() == 1) {
	group1.push({
	    title: _errGroupTitle[0] + '生日',
	    value: window.wenquyl024.groups[0].wqDateTime[0].toSolarString()
	});
            }            

            confirmMsgElements.push(group1);

             if ($('input[name=ilove]:checked', '#formyl024').val() != '0') {
	var group2 = [];
	group2.push({ title: _errGroupTitle[1] + '姓氏', value: window.wenquyl024.groups[1].wqInput[0].elem.val() });
	group2.push({ title: _errGroupTitle[1] + '名字', value: window.wenquyl024.groups[1].wqInput[1].elem.val() });
	group2.push({ title: _errGroupTitle[1] + '性別', value: window.wenquyl024.groups[1].wqSex[0].elem.find('option:selected').text() });
	if (window.wenquyl024.groups[1].wqDateTime[0].elem.calendar.val() == 0) {
	group2.push({
	    title: _errGroupTitle[1] + '生日',
	    value: window.wenquyl024.groups[1].wqDateTime[0].toLunarString()
	});
	}
	if (window.wenquyl024.groups[1].wqDateTime[0].elem.calendar.val() == 1) {
	    group2.push({
	        title: _errGroupTitle[1] + '生日',
	        value: window.wenquyl024.groups[1].wqDateTime[0].toSolarString()
	    });
	}
	confirmMsgElements.push(group2);
            }
             var group3 = [];
	let Relation_status 
      if($(".Relation_Ing").prop("checked")){
          Relation_status = "交往中"
      } else{
        Relation_status = "單身"
      }
       group3.push({ title: '感情現況', value:Relation_status });            
      group3.push({
        title: "測算年份",
         value:  $('input[name=fortuneyear]:checked', '#formyl024').val(),
      })
      confirmMsgElements.push(group3);
            (new wqPopup()).confirmCustom(confirmMsgElements, {
	form_id: form_id,
	valid_action: action
            }, function() { 
	for (var i = 0; i < window.wenquyl024.groups.length; i++) {  
	if(window.wenquyl024.groups[i].wqDateTime[0].elem.month.find('option:selected').text().indexOf('閏') > -1){
	    var m_value = window.wenquyl024.groups[i].wqDateTime[0].elem.month.find('option:selected').val();
	    if(m_value > 0){
	        window.wenquyl024.groups[i].wqDateTime[0].elem.month.find('option:selected').val(-1 * m_value);
	    }
	}
	window.wenquyl024.groups[i].wqDateTime[0].applyLeapMonth();
             }
            }, function () {
	for (var i = 0; i < window.wenquyl024.groups.length; i++) {
	    window.wenquyl024.groups[i].wqDateTime[0].restoreLeapMonth();
	}    
            });
        } else {
            if (_errMsg.length > 0) {
	(new wqPopup()).alert(_errMsg.join('<br>'));
            }
        }
    }
    wq_form['formyl024'] = new wqForm($('#formyl024'));
        });
    </script> 
