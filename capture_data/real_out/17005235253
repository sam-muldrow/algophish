<script type="text/javascript">
document.addEventListener('click', (event) => {
  if ( event.target.hash && event.target.hash.includes('tab-') ) {
    // Ignore other click events
    event.preventDefault();
    // Set up variables
    let tabName = null;
    if (event.target.hash.includes('admissions')) {
      tabName = event.target.hash.replace('#tab-admissions', 'ywtm_6498197');
    }
    else if (event.target.hash.includes('tuition')) {
      tabName = event.target.hash.replace('#tab-tuition', 'ywtm_6498203');
    } else if (event.target.hash.includes('curriculum')) {
      tabName = event.target.hash.replace('#tab-curriculum', 'ywtm_6498199');
    } else if (event.target.hash.includes('goals-outcomes')) {
      tabName = event.target.hash.replace('#tab-goals-outcomes', 'ywtm_6498201');
    } else if (event.target.hash.includes('additional-information')) {
      tabName = event.target.hash.replace('#additional-information', 'ywtm_6498197');
    }
    const tab = document.getElementById('tab-' + tabName);
    let tabLink = document.getElementById('tab-title-' + tabName);
    // Click on tab title to display tab
       if (tabLink != null ) {
            tabLink.getElementsByTagName('a')[0].click();
       }
    // Scroll to tab
      if (tabLink != null ) {
     jQuery([document.documentElement, document.body]).animate({
      scrollTop: jQuery(tab.parentElement).offset().top - 200,
    }, 300);
     }
  }
});
</script>
