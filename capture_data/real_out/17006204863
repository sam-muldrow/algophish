<!--  Google Analytics -->
<script type="text/javascript">
    if (typeof(dhd) !== 'undefined') {
        dhd.prep('ga');
        dhd.prep('azl');
        dhd.ga.area = 'home';
        dhd.ga.enabled = true;
        dhd.ga.mainCategoryId = '';
        dhd.ga.mainCategoryName = '';
        dhd.azl.categoriesTree = JSON.parse('[{\u0022id\u0022:0,\u0022name\u0022:\u0022Rubriken\u0022}]')
    }

    //tcf support aktiviert
    window['gtag_enable_tcf_support'] = true;

    const CONSENT_ID_GOOGLE = 755;

    dhd.cmp.tcfApiListener(CONSENT_ID_GOOGLE, () => {

        dhd.tracking.measurementId = 'G-FE6CTZKB58';

        (function () {
            var script = document.createElement("script");
            script.async = true;
            script.type = 'text/javascript';
            var useSSL = 'https:' === document.location.protocol;
            script.src = (useSSL ? 'https:' : 'http:') + '//www.googletagmanager.com/gtag/js?id=' + dhd.tracking.measurementId;
            var node = document.getElementsByTagName('script')[0];
            node.parentNode.insertBefore(script, node);
        })();

        window.dataLayer = window.dataLayer || [];
        window.gtag = function() {dataLayer.push('undefined' !== typeof arguments ? arguments : []);}
        gtag('js', new Date());


        // Initialize GA4
        gtag('config', dhd.tracking.measurementId,
            {
                'anonymize_ip': true,
                // 
                'site_speed_sample_rate' : 10,
                // 
                // 
            });

        // 
        gtag('event', 'main_category', {'main_category' : 'Unspezifisch'});

        // 
        gtag('event', 'content_area', {'content_area' : 'home'});

        // 
        gtag('event', 'responsive_page_relaunch', {'responsive_page_relaunch' : 'y'});

        // 
        gtag('event', 'portal', {'portal' : '0'});

        // 
        gtag('event', 'client', {'client' : 'Web'});

        // 

        gtag('event','responsive_page', {"responsive_page_all" : "y"});

        let currentWidth;
        if (typeof (dhd) != 'undefined' && typeof (dhd.breakPoint) != 'undefined' && dhd.breakPoint !== '') {
            gtag('event', 'breakpoint', {"breakpoint": dhd.breakPoint});
        } else {
            currentWidth = $(document).width();

            let breakPoint = '';
            if (currentWidth < 720) {
                breakPoint = 'xs';
            } else if (currentWidth < 1200) {
                breakPoint = 'sm';
            } else if (currentWidth >= 1200) {
                breakPoint = 'lg';
            }
            if (breakPoint !== '') {
                gtag('event', 'breakpoint', {"breakpoint": breakPoint});
            }
        }

        // 

        dhd.prep('dhd.header');
        dhd.header.loginChecked = dhd.header.loginChecked || [];

        // Nach dem Ladevorgang der Seite noch den Login-Status wegtracken ...
        dhd.header.loginChecked.push(function () {
            let userProperties = {};

            if (typeof gtag != 'undefined') {

                if (dhd.storage.isLoggedIn()) {
                    userProperties.user_id = dhd.storage.getAnalyticsUserId();
                    userProperties.is_logged_in = 'y';
                } else {
                    userProperties.is_logged_in = 'n';
                }
                // Nutzergruppe bestimmen (ga Dimension 18)
                userProperties.target_group = dhd.header.getTargetGroupId() ? 'seller' : 'potentialBuyer';
                userProperties.is_premium_member = dhd.storage.isPremiumMember() ? 'y' : 'n';
                userProperties.client = dhd.isApp() ? 'App' : 'Web';

                const speedKitConfig = dhd.cookie.readCookie("baqend-speedkit-config");

                if (speedKitConfig) {
                    const speedKitCookie = JSON.parse(decodeURIComponent(speedKitConfig));
                    userProperties.speedkit_active = speedKitCookie.group === "A" ? "y" : "n";
                }

                gtag('set', 'user_properties', userProperties)
            }
        });
        window.dispatchEvent(new CustomEvent('dtw.analytics_initialized'));
    }, true);
</script>

