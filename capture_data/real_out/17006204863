    document.addEventListener('readystatechange', function() {

        // Ziel-Element, auf das wir achten wollen
        const targetElement = document.body

        // Optionsobjekt für den MutationObserver
        const options = {
            childList: true, // Überwache Änderungen an den direkten Kindern des Ziel-Elements
            subtree: true // Überwache Änderungen in allen untergeordneten Elementen des Ziel-Elements
        };

        // Callback-Funktion, die bei Änderungen aufgerufen wird
        const mutationCallback = function(mutationsList, observer) {
            // Führen Sie die gewünschte Logik für jede Mutation aus
            let primaryNavbar = document.querySelector('.primary-navbar')
            if (!primaryNavbar) return
            if (dhd.Responsive.isSmartphone()) return

            dhd.prep('header');

            let initialInvertClasses = primaryNavbar.classList.value.split(' ').filter(function(invertClass) {return invertClass.startsWith('inverted');});

            window.addEventListener('scroll', function() {
                if (false === dhd.Responsive.isStationary()) return;

                let invertClass = '';
                if ('dtw' === dhd.portal) {
                    if ((invertClass = initialInvertClasses.filter(function(invertClass) {return invertClass.endsWith(dhd.Responsive.current());})) || '') {
                        if (invertClass.length === 0) {
                            invertClass = '';
                        } else {
                            invertClass = invertClass[0];
                        }                    }
                } else {
                    invertClass = 'inverted';
                }

                if (invertClass.length) {
                    // Invertierung ab md
                    let method = primaryNavbar.offset().top === 0 ? 'add' : 'remove';
                    primaryNavbar.classList[method](invertClass);
                }
            });
            observer.disconnect()
        };

        // Erstellen Sie eine Instanz des MutationObservers und übergeben Sie die Callback-Funktion und die Options
        const observer = new MutationObserver(mutationCallback);

        // Starten Sie die Überwachung des Ziel-Elements mit dem Observer und den Options
        observer.observe(targetElement, options);
    })

	</script>

