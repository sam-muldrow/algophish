function setConfig(){

	let setcount = 9;
	const settime = 5000;
	const setnewdate = 3;

	setcount = (setcount == 9) ? Math.floor(Math.random()*5)+1 : setcount;
	return [setcount,settime,setnewdate];
}
function newDateCheck(date){
	let newmark = '';
	const display = setConfig()[2];
	const artdate = new Date(date);
	const today = new Date();
	const ndate =(today - artdate) / (1000 * 24 * 3600);
	if(ndate <= display) newmark = '<span class="colBoxNewtxt">New!</span>';
	return newmark;
}
</script>



