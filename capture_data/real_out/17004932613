<script>
function topArticleListSet(data){
	data.pop();
	var html = '';
	var listhtml = '';
	for(var i=4;i<data.length;i++) {
		var newmark = newDateCheck(data[i]['date']);
		if(i<7) {
			html += '<div class="colBoxIndex">\n';
			if(data[i]['icon']) html += '<div class="colBoxIcon"><a href="' + data[i]['url'] + '"><img src="' + imgSrv() + data[i]['icon'] + '" alt="' + data[i]['title'] + '"></a></div>\n';
			if(data[i]['subtitle']) html += '<div class="colBoxSubTitle"><h5>' + data[i]['subtitle'] + '</h5></div>\n';
			html += '<div class="colBoxTitle"><h3><a href="' + data[i]['url'] + '">' + data[i]['title'] + '</a>' + newmark + '</h3></div>\n';
//			if(data[i]['abs']) html += '<div class="colBoxDescription"><p>' + cutString(data[i]['abs'],155) + '</p></div>\n';
			 html += '<div class="colBoxIndexDate"><p><span class="indexDate">i' + data[i]['date'] + 'j</span></p></div>\n';
			html += '</div>\n';
		} else {
			listhtml += '<li>\n';
			listhtml += '<div class="colBoxSubTitle"><h5>' + data[i]['subtitle'] + '</h5></div>\n';
			listhtml += '<div class="colBoxTitle"><h3><a href="' + data[i]['url'] + '">' + data[i]['title'] + '</a>' + newmark + '</h3><span class="indexDate">i' + data[i]['date'] + 'j</span></div>\n';
			listhtml += '</li>\n';
		}
	}
	html += '<div class="colBoxIndex">\n<div class="colBoxUlist titleList">\n<ul>\n';
	html += listhtml;
	html += '</ul>\n</div>\n</div>\n';
	return html;
}
</script>
