<script>
var gtset = function(count){
	gtset.articleset = function(data,noicon){
		var html = '',lefthtml = '',righthtml = '',title = '';
		for(var i=0;i<data.length;i++) {
			if(i > 3) break;
			var bigicon = (data[i]['bigicon']) ? data[i]['bigicon'] : (data[i]['bigicon']) ? data[i]['bigicon'] : noicon;
			var icon = (data[i]['icon']) ? data[i]['icon'] : noicon;
			var newmark = (data[i]['date']) ? newDateCheck(data[i]['date']) : '';
			if(i == 0) {
				title = cutString(data[i]['title'],82);
				var style = (data[i]['bigicon']) ? (data[i]['bigiconh'] / 3) > (data[i]['bigiconw'] / 4) ? ' GTLeftH' : ' GTLeftW' : '';
				lefthtml += '<div class="colBoxIndex GTLeft">\n';
				lefthtml += '<div class="colBoxIcon' + style + '"><a href="' + data[i]['url'] + '"><img src="' + imgSrv() + bigicon + '" onclick="cx_pp_event(\'aittop_gt_left_' + count + '\');" alt="' + data[i]['title'] + '"></a></div>\n';
				lefthtml += '<div class="colBoxSubTitle"><h5>' + data[i]['subtitle'] + '</h5></div>\n';
				lefthtml += '<div class="colBoxTitle"><h3><a href="' + data[i]['url'] + '" onclick="cx_pp_event(\'aittop_gt_left_' + count + '\');">' + title + '</a>' + newmark + '</h3></div>\n';
				lefthtml += '</div>\n';
			} else {
				title = cutString(data[i]['title'],85);
				righthtml += '<div class="colBoxIndex GTRight">\n';
				righthtml += '<div class="colBoxIcon"><a href="' + data[i]['url'] + '" onclick="cx_pp_event(\'aittop_gt_right_' + count + '\');"><img src="' + imgSrv() + icon + '" alt="' + data[i]['title'] + '"></a></div>\n';
				righthtml += '<div class="colBoxTitle"><h3><a href="' + data[i]['url'] + '" onclick="cx_pp_event(\'aittop_gt_right_' + count + '\');">' + title + '</a>' + newmark + '</h3></div>\n';
				righthtml += '</div>\n';
			}
		}
		html += '<div class="GTLeftBox">\n' + lefthtml + '</div>\n';
		html += '<div class="GTRightBox">\n' + righthtml + '</div>\n';
		return html;
	}
	gtset.dataset = function(){
		if(!count) count = 1;
		let data = (count < 6) ? partsGtData(count)['data'] : partsGtData(1)['data'];
		data.pop();
		return data;
	}
	gtset.noiconset = function(){
		if(!count) count = 1;
		var noicon = '';
		if(count == 1) {noicon = '/ait/images/np_cloud.jpg'}
		else if(count == 2) {noicon = '/ait/images/np_aiiot.jpg'}
		else if(count == 3) {noicon = '/ait/images/np_devops.jpg'}
		else if(count == 4) {noicon = '/ait/images/np_security.jpg'}
		else if(count == 5) {noicon = '/ait/images/np_jibun.jpg'}
		else {noicon = '/ait/images/np_cloud.jpg'}
		return noicon;
	}
	gtset.subtopurl = function(){
		if(!count) count = 1;
		const text = 'àÁÆÇÞ';
		let link = '';
		if(count == 1) {link = '/ait/subtop/cloud.html';}
		else if(count == 2) {link = '/ait/subtop/ai.html';}
		else if(count == 3) {link = '/ait/subtop/devops.html';}
		else if(count == 4) {link = '/ait/subtop/security.html';}
		else if(count == 5) {link = '/ait/subtop/jibun/';}
		else {link = '/ait/subtop/cloud.html';}
		const subtopurl = '<a href="' + link + '" onclick="cx_pp_event(\'aittop_gt_linkmore_' + count + '\');">' + text + '</a>'
		return subtopurl;
	}
	gtset.htmlset = function(){
		const data = gtset.dataset();
		if(!data) return false;
		const subtopurl = gtset.subtopurl();
		const noicon = gtset.noiconset();
		gtbox.style.opacity = 1;
		for(var i=0;i<gmbox.children[0].children.length;i++) {
			gmbox.children[0].children[i].classList.remove('Active');
		}
		gmbox.children[0].children[count-1].classList.add('Active');
		let html = '';
		html += '<div class="colBoxOuter">\n<div class="colBoxInner">\n' + gtset.articleset(data,noicon) + '\n</div>\n';
		html += '<div class="GTLinkMore">' + subtopurl + '</div>\n</div>\n';
		return html;
	}
	gtset.timerset = function(){
		gtbox.style.opacity = 0;
		(count == 5) ? count = 1 : count++;
		setTimeout(function(){gtbox.innerHTML = gtset.htmlset();},300);
	}
	var gtbox = document.getElementById('colBoxGT');
	var gmbox = document.getElementById('colBoxGTmenu');
	var gtarea = document.getElementById('GTArea');
	if(count == 0){
		const config = setConfig();
		count = config[0];
		gtbox.innerHTML = gtset.htmlset();
		var timeinterval = setInterval(function(){gtset.timerset();},config[1]);
		gtarea.addEventListener('mouseenter',function(){clearInterval(timeinterval);}) ;
		gtarea.addEventListener('mouseleave',function(){timeinterval = setInterval(function(){gtset.timerset();},config[1]);}) ;
	} else {
		clearInterval(timeinterval);
		gtbox.innerHTML = gtset.htmlset();
	}
}
gtset(0);
</script>
