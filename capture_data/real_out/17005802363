                    <link rel="preload" href="//cdn1.ykso.co/dist/main/fonts/FontAwesome.c585243e.woff2" as="font" type="font/woff2" crossorigin="anonymous">
            </head>

    <body class="site-dotandbo is-active-hurricane-bar header-stack-1 microsite site-dotandbo     content-page ">
                                <script>
        (function() {
    window.addEventListener("load", function() {
        onWindowLoad();
    });

    window.addEventListener(
        "scroll",
        debounce(onWindowScroll, 500),
        {
            passive: true
        });

    function checkImagesToLoad() {
        var images = getAllLazyLoadableImages();
        var imagesLength = images.length;

        while (imagesLength--) {
            var currentItem = images[imagesLength];

            if (isInNextView(currentItem) || isOnLoadImage(currentItem)) {
                var imageUrl = currentItem.getAttribute('data-src');

                // It is possible that Foundation has lazy loaded the image
                // in the orbit slider.  They will have removed data-src already.
                // Just skip and remove our class below.
                if (imageUrl) {
                    currentItem.src = imageUrl;
                }

                removeLazyClasses(currentItem);
            }
        }
    }

    function removeLazyClasses(item) {
        item.classList.remove("lazy-image");
        item.classList.remove("lazy-onload");
    }

    function onWindowScroll() {
        checkImagesToLoad();
    }

    function onWindowLoad() {
        checkImagesToLoad();
    }

    function isInNextView(item) {
        var rect = item.getBoundingClientRect(),
            isInNextVerticalView = rect.top < (window.innerHeight * 2),
            isInNextHorizontalView = rect.left < (window.innerWidth * 2),
            isImageStyledToBeVisible = rect.height > 0 && rect.width > 0;

        return isInNextVerticalView && isInNextHorizontalView && isImageStyledToBeVisible;
    }

    function isOnLoadImage(item) {
        if (item.classList.contains("lazy-onload")) {
            return true;
        }

        return false;
    }

    function getAllLazyLoadableImages() {
        return document.querySelectorAll(".lazy-image");
    }

    /**
     * Debounce taken from Underscore.
     * @param {*} func
     * @param {*} wait
     * @param {*} immediate
     */
    function debounce(func, wait, immediate) {
        var timeout, args, context, timestamp, result;

        var later = function() {
        var last = Date.now - timestamp;

        if (last < wait && last >= 0) {
            timeout = setTimeout(later, wait - last);
        } else {
            timeout = null;
            if (!immediate) {
            result = func.apply(context, args);
            if (!timeout) {
                context = args = null;
            }
            }
        }
        };

        return function() {
        context = this;
        args = arguments;
        timestamp = Date.now;
        var callNow = immediate && !timeout;
        if (!timeout) {
            timeout = setTimeout(later, wait);
        }
        if (callNow) {
            result = func.apply(context, args);
            context = args = null;
        }

        return result;
        };
    }
})();
    </script>
    
                
            <div id="ccpa-notification">
    <div>
	<p style="text-align: center;">Dot &amp; Bo collects personal information in relation to operating our site and services. We do not sell personal information, but it may be shared with third parties for certain business purposes.  Please review our <a href="/content/general/policies-privacy">Privacy Policy</a> for more information on the data we collect and your associated rights.</p>
	<span><button>Got it!</button></span>
</div>
</div>
    
        
                
                


    


