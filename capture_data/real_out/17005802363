    <div id="g_id_onload"
         data-client_id="84850429815-7srmmclaltt292c1jqp1vft0hufu2rce.apps.googleusercontent.com"
         data-context="use"
         data-callback="handleLogin"
         data-itp_support="false">
    </div>

<script type="text/javascript">

    function handleLogin(response) {
        let redirectUrl = '/login-via-google?osky_origin=opensky&osky_source=google-login&osky_entry=google-join-page';

        if (typeof window.GoogleAuthURL === 'string' || window.GoogleAuthURL instanceof String) {
            redirectUrl = window.GoogleAuthURL;
        }

        redirectUrl = appendUrlParam(redirectUrl, 'idtoken', response.credential);
        window.location.href = redirectUrl;
    }

    function appendUrlParam(url, name, value) {

        if (!url) {
            url = '';
        }

        // If the URL already has a query string, then make sure to append a new parameter instead.
        url += url.indexOf('?') === -1 ? '?' : '&';
        url += name + "=" + value;

        return url;
    }
</script>
    
