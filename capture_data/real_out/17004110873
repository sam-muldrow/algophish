 $(document).ready(function(){
	/*é¦é¡µbannerå¾åæ¢*/
	(function(){
		var curr = 0;
		$(".banner_nav .trigger").each(function(i){
			$(this).click(function(){
				curr = i;
				$(".bn_img a").eq(i).fadeIn("slow").siblings("a").hide();
				$(this).siblings(".trigger").removeClass("imgSelected").end().addClass("imgSelected");
				return false;
			});
		});
		
