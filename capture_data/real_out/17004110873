			//flag:trueè¡¨ç¤ºåç¿»ï¼ falseè¡¨ç¤ºåç¿»
			if (flag) {
				if (curr == 0) {

					todo = 2;
				} else {
					todo = (curr - 1) % 4;
				}
			} else {
				todo = (curr + 1) % 4;
			}
			$(".banner_nav .trigger").eq(todo).click();
		};
		//èªå¨ç¿»
		var timer = setInterval(function(){
			todo = (curr + 1) % 4;
			$(".banner_nav .trigger").eq(todo).click();
		},5000);
		
