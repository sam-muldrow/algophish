	<script type="text/javascript">
    $(function(){
		$(".nation_bg_modal").next($('#layer-tag'));
		$('#layer-tag .btn').click(function(){
			$(".nation_bg_modal").hide();
            $(".nation_bg_modal").css('zIndex', 4);
			$(this).parent().parent().hide();
		});

		$('div.tit-area > a.btn-write:first').click(function(){
			$(".nation_bg_modal").show();
            $(".nation_bg_modal").css('zIndex', 1000);
			$('#layer-tag').show();
		});

        $('#layer-tag input[type=text]').on('keyup', function(e) {
            if (e.which == 13) {
                e.preventDefault();
                if($(this).val()) {
                    tag_save();
                }
            }
        });

        // [닫기]
        $(".oneID-request-modal__close").click(function(e){
            // 창 닫으면 초기화
            $("input:checkbox[id='join_agree']").prop("checked", false);
            $('#input_m_id').val('');
            $('#input_m_pw').val('');

            e.preventDefault();
            var ele = $(this).parents(".modal-box");
            modalController('',ele);
        });


    });
    function hideTagPopup() {
        $('#layer-tag').hide();
        $(".nation_bg_modal").hide();
        $('#layer-tag #tag_name').val('');
    }
    function tag_save() {
        var gid = '';
        var tag_name = $('#layer-tag #tag_name').val();
        tag_name = tag_name.trim();

        var regExp = /[\{\}\[\]\/?.,;:|\)*~`!^\-_+<>@\#$%&\\\=\(\'\"\s]/gi;
        if(regExp.test(tag_name)){
            tag_name = tag_name.replace(regExp, "");
        }
        if(tag_name.length <= 0){
            return;
        }
        var url = '/product/ajax_save_tag';
        var param = 'gid='+gid+'&tag_name='+encodeURIComponent(tag_name);
        $.post(url, param, function(data) {
            var res = $.parseJSON(data);
            if(res.is_success == true) {
                hideTagPopup();
                location.reload();
            }
        });
    }
    </script>
	



