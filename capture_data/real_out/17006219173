        trackingCode.init();
        var isScriptInit = false;

        $(function () {
            showBlockAD();
            main.init();
            isScriptInit = true;
        });

        window.onpageshow = function (event) {
            if (!isScriptInit) {
                showBlockAD();
                main.init();
            }
        };

        function showBlockAD() {
            switch(util.getPathNameByIndex(2)){
                case 'article':
                case 'forum':
                case 'master':
                    //console.log('Disable BlockAD');
                    break;
                default:
                    ad.period.load("talk");
                    break;
            };
        }
