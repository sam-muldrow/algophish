    <script type="text/javascript">
      window.addEventListener('DOMContentLoaded', function(event){
        var bigboxWrap = $('#banner-bigbox');
        var bigboxBtnListWrap = bigboxWrap.find('.btn-slot-list');
        var bigboxBtnList = bigboxBtnListWrap.find('li');
        var bigboxBtn = bigboxBtnList.find('button');
        var bigboxCtrl = {
          getCurrent: function () {
            return parseInt(bigboxBtnListWrap.find('button.active').data('slot'));
          },
          changeSlot: function (slot) {
            var _self = this;
            slot = parseInt(slot);

            if (slot === _self.getCurrent())
              return false;

            bigboxBtnList.removeClass('active');
            bigboxBtn.removeClass('active');
            bigboxBtn.filter('[data-slot="' + slot + '"]').addClass('active').closest('li').addClass('active');

            var panoObjName = '_dfpBigbox_' + slot;

            googletag.cmd.push(function () {
              googletag.pubads().refresh([scope[panoObjName]]);
            });

            bigboxWrap.find('.dfp-bigbox').hide();
            bigboxWrap.find('.dfp-bigbox-' + slot).show();
          },
          init: function () {
            var _self = this;

            bigboxBtn.on('click', function (e) {
              e.preventDefault();
              var el = $(this);
              _self.changeSlot(el.data('slot'));
            });
          }
        };
        bigboxCtrl.init();
        bigboxCtrl.changeSlot(Math.floor(Math.random() * 6) + 1);
      });
    </script>

