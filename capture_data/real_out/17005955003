        align-items: center;
        flex-direction: column;
    }

    .footer-body .brand-box > .dekdlogo {
        width: 177px;
        display: block;
        margin: 0 auto;
    }

    .footer-body .brand-box > .slogan {
        font-family: 'Tahoma', Arial, sans-serif;
        font-size: 12px;
        color: #CCCCCC;
        text-align: center;
        margin: 10px 0;
        letter-spacing: 2px;
    }

    .footer-body > .site-map-wrapper {
        display: flex;
        justify-content: flex-start;
        align-items: flex-start;
        flex-wrap: wrap;
    }

    .footer-body > .site-map-wrapper > * {
        width: auto;
        margin-right: 77px;
    }

    .site-map-wrapper > .site-map-box .sitegroupname {
        font-family: 'KalatexaDisplay', Arial, sans-serif;
        font-weight: bold;
        font-size: 18px;
        text-align: left;
        color: #F37A01;
        margin-bottom: 5px;
    }

    .site-map-wrapper > .site-map-box .link {
        font-family: 'Bai Jamjuree', Arial, sans-serif;
        font-size: 14px;
        color: #CCCCCC;
        text-align: left;
        line-height: 24px;
        display: block;
        transition: 0.15s;
    }

    .site-map-wrapper > .site-map-box .link:hover {
        color: #FFFFFF;
    }

    .footer-wrapper > .footer-bottom {
        display: block;
        width: 100%;
        border-top: 1px solid rgba(225,225,225, 0.1);
        padding: 15px;
        text-align: center;
        font-family: 'Tahoma', Arial, sans-serif;
        font-size: 13px;
        color: #545454;
        box-sizing: border-box;
    }

