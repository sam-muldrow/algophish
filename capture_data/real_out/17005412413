                function s(i, m, o, n) {
                    n = typeof i == 'object';
                    if (n && i.length) {
                        for (m=0; m < i.length; m++) {
                            i[m] = s(i[m]);
                        }
                    } else if (n) {
                        for (m in i) {
                            if (m == '_consent') {
                                for (o in i[m]) {
                                    if (doesPurposeHaveConsent(o)) {
                                        return i[m][o];
                                    }
                                }
                                if (i[m]['0']) return i[m]['0'];
                            } else {
                                i[m] = s(i[m]);
                            }
                        }
                    }
                    return i;
                }
                adUnits = s(adUnits);
