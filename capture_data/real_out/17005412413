<script class="oncookielaw_4_accepted" type="text/plain">
    try { 
        loadScript('https://ats-wrapper.privacymanager.io/ats-modules/22abe735-5e63-45c0-8471-2536925961fd/ats.js', function() {
            _waitOnceFor('socialbar_enabled', function(_, user){
                if(user.email) {
                    ats.setAdditionalData({
                                        'type': 'emailHashes',
                                        'id': [md5(user.email)]
                                        })
                }
            })
        })
    } catch(e) { console.log(e); }
</script>
