                            }
                        };
                        
                    });

                    // or wait its secure-timeout
                    // in case PBJS doesn't load
                    setTimeout(() => {
                        CED.debug("Prebid Manager - pbjs FAIL - timeout reached!");
                        resolve();
                    }, PREBID_TIMEOUT);
                }),

                
