<iframe class="oncookielaw_4_accepted" data-src="/track_shop_event.php?event=pageview&url=http%3A%2F%2Fwww.leggo.it%2F&pay=0&ref=https%3A%2F%2Fwww.leggo.it%2F%3Frefresh_ce" width="1" height="1" frameborder="0"></iframe>
<script>
(function(l,m,a,o){
    o = new RegExp('(&|\\?)\\b'+m+'\\b');
    a = l.href;
    // CED.log("refresher: ",document.referrer,o.test(a),document.referrer.indexOf(l.host) == -1);
    // se accesso diretto o da referral esterni si va sulla url senza refresh
    if (o.test(a) && document.referrer.indexOf(l.host) == -1) {
        // rimozione e pulizia dei parametri
        l.href = a.replace(o,'').replace(/(\/[^?]*)&/,'$1?');
    }
    // tengo un flag per capire se l'utente ha scrollato negli ultimi 30 secondi
    var scrolled = false;
    var scroll_timeout = null;
    addEventListener('scroll', function(){
        scrolled = true;
        clearTimeout(scroll_timeout);
        scroll_timeout = setTimeout(function(){
            scrolled = false;
        }, 30*1000);
    });
    setTimeout(function(){
        // disabilito sulle pagine di preview.
        if (typeof isPreview != 'undefined' && isPreview) return; // no refresh
        if (l.hash == '#norefresh') return; // no refresh
        if (!o.test(a)) { // devo aggiungere il parametro
            if (a.indexOf('?')>-1) a += '&'+m;
            else a += '?'+m;
        }
        if (!scrolled) l.href = a;
        else { window.history.pushState({}, '', a); l.reload() };
    }, 301000);
})(window.location,'refresh_ce');
</script>
