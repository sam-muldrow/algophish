var prebidConfig = { enableSendAllBids: false, targetingControls: { alwaysIncludeDeals: true }, bidderTimeout: 1250, consentManagement: { gdpr: { cmpApi: 'iab', timeout: 400, defaultGdprScope: true } }, currency: { adServerCurrency: 'USD' }, priceGranularity: { buckets: [{ precision: 2, max: 2, increment: 0.01 }, { max: 4, increment: 0.02 }, { max: 8, increment: 0.05 }] }, userSync: { filterSettings: { iframe: { bidders: '*', filter: 'include' } }, syncsPerBidder: 0, userIds: [{ name: 'identityLink', params: { pid: '13951', notUse3P: false }, storage: { type: 'html5', name: 'idl_env', expires: 15, refreshInSeconds: 1800 } }], syncDelay: 3000 }, timeoutBuffer: 150, useBidCache: true, 'rubicon': { singleRequest: true, netRevenue: true }, 'criteo': { fastBidVersion: 'latest' } };

