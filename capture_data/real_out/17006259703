 function prev_data_lost1(){

	    var myForm = document.getElementById('submit_quick_form');
            var myForm2 = document.getElementsByName("preview");
            if(document.querySelector("#fast-reply_textarea").value!=="") { 
                  myForm.setAttribute("tabindex","3");  
                  myForm2.setAttribute("tabindex","3");  
}
	         
	  }

 function prev_data_lost2(){

            var myForm3 = document.getElementsByName("dosubmit");
            var myForm4 = document.getElementsByName("preview");
            if(document.querySelector(".rte-iframe").value!=="") { 
                myForm3[0].setAttribute("tabindex","8");  
                myForm4[0].setAttribute("tabindex","9");
}
	         
	  }
</script>
<script type="text/javascript">
function once(fn, context) { 
    var result;

    return function() { 
        if(fn) {
            result = fn.apply(context || this, arguments);
            fn = null;
        }

        return result;
    };
}

var loadXMLDocNew = once(function() {
  var hhh = document.getElementById("aj_href").getAttribute("href");
    var xmlhttp = new XMLHttpRequest();

    xmlhttp.onreadystatechange = function() {
        if (xmlhttp.readyState == XMLHttpRequest.DONE ) {
           if (xmlhttp.status == 200) {
               document.getElementById("myDiv").innerHTML = xmlhttp.responseText;
           }
           else if (xmlhttp.status == 400) {
              alert('There was an error 400');
           }
           else {
               alert('something else other than 200 was returned');
           }
        }
    };

    xmlhttp.open("GET", hhh, true);
    xmlhttp.send();
});
</script>

