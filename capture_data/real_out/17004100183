	<script>
		if (typeof utag_data === "object") {
			let pageType = utag_data.page_type,
				cartRows = [],
				addPurchaseRows = [],
				productDetails = [],
				productLists = [];
			if (pageType === 'product') {
				productDetails.push({"productId": utag_data.product_id, "sku": ''});
			}
			if (pageType === 'cart' && typeof utag_data.product_id !== 'undefined') {
				let productIds = utag_data.product_id,
					skn = utag_data.skn_product_id,
					quantity = utag_data.product_quantity,
					unitPrice = utag_data.product_price;
				for (let i = 0; i < productIds.length; i++) {
					cartRows.push({
						"productId": skn[i],
						"quantity": Number(quantity[i]),
						"sku": productIds[i].replaceAll(" ", "-"),
						"unitPrice": Number(unitPrice[i]),
						"currency": utag_data.currency_code
					});
				}
			}
			if (typeof utag_data.trans_orders !== "undefined" && pageType === 'order') {
				pageType = "purchase";
				for (let i = 0; i < utag_data.trans_orders.length; i++) {
					for (let j = 0; j <  utag_data.trans_orders[i].products_list.skn_product_id.length; j++) {
						addPurchaseRows.push({
							"purchaseId": utag_data.trans_orders[i].order_id,
							"productId": utag_data.trans_orders[i].products_list.skn_product_id[j],
							"quantity": Number(utag_data.trans_orders[i].products_list.product_quantity[j]),
							"sku": utag_data.trans_orders[i].products_list.product_id[j].replaceAll(" ", "-"),
							"unitPrice": Number(utag_data.trans_orders[i].products_list.product_qvc_price[j]),
							"currency": utag_data.currency_code
						});
					}
				}
			}
			window.monetateQ = window.monetateQ || [];
			window.monetateQ.push(['setDeviceId', quid.get()]);
			window.monetateQ.push(["setPageType", pageType]);
			window.monetateQ.push(["addCartRows",cartRows]);
			window.monetateQ.push(["addPurchaseRows",addPurchaseRows]);
			window.monetateQ.push(["addProductDetails", productDetails]);
			if(typeof utag_data.product_ats !== "undefined" && utag_data.product_ats === 'N') {
		        window.monetateQ.push(["addCategories", ["soldOut"]]);
		    }
		    if(typeof utag_data.credit_terms_code !== "undefined" && utag_data.credit_terms_code !== '') {
				window.monetateQ.push([ "addCategories", ["easyPay"]]);
		    }
			 //PLP Pages
  		    if (pageType === 'product-listing-search' || pageType === 'product-listing-category') {
  			  window.addEventListener("DOMContentLoaded", (event) => { 
  				  pageType = pageType === 'product-listing-search' ? 'search' : pageType === 'product-listing-category' ? 'index' : pageType;
		          let plpIds = JSON.parse(localStorage.productListingProducts);
		          for (let i = 0; i < plpIds.length; i++) {
		            productLists.push({"productId": plpIds[i], "sku": ''});
		          }
		          window.monetateQ.push(["setPageType", pageType]);
		       	  window.monetateQ.push(["addProducts", productLists]);
		  		  window.monetateQ.push(["trackData"]);
  			 });
  		  } else {
  			window.monetateQ.push(["trackData"]);
  		  }
  	  }
	</script>
	
