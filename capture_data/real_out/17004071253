			<div class="tCenter">

							<script type="text/javascript">
			$(document).ready(function()
			{
				// добавление полей загрузки
				var file_index = 2;
				var file_limit = 30;
				$('#add_file_inputLink').bind('click', function(){
					if ($('textarea', $ctx).length) {
						return false;
					}
					if (file_index <= 10) {
						$('#uploading_files').append('<input type="file" name="file' + file_index + '" size="30" class="upfile" />');
						file_index++;
					}
					else
					{
						alert('Лимит на количество одновременных загрузок исчерпан');
					}

					return false;
				});
				// ссылка "настройки"
				$('#settingsLink').one('click', function(){
					$('#settings').show();
					$(this).hide();

					return false;
				});
				// главный контекст для аплоада
				var $ctx = $('#uploading_files');
				// переключение на "из интернета"
				$('#switch_to_copy').bind('click', function(){
					if (!$('textarea', $ctx).length) {
						$('input', $ctx).remove();
						$ctx.append('<p id="textarea_msg">Введите до ' + file_limit + ' ссылок</p><textarea name="files" id="upload_files"></textarea>');

						$('#upload').attr({'action' : '/upload_copy'});
					}

					return false;
				});
				// переключение на "с компьютера"
				$('#switch_to_upload').bind('click', function(){
					if ($('input[name*=file]', $ctx).length == 0) {
						$('textarea,#textarea_msg', $ctx).remove();
						$('#upload').attr({'action' : '/uploadmulti'});
						$ctx.append('<input type="file" name="file[]" size="40" id="file" class="upfile" min="1" max="30" multiple />');
						file_index = 2;
					}

					return false;
				});
				// показ ограничений
				$('#limits').bind('click', function(){
					$(this).remove(); 
					$('#uploadLimits').show(); 
					return false;	
				});
				// скролл к пункту ограничений из предупреждения о хотлинке
				$('#go-to-limits').click(function(){
					$('#limits').click();
					$('html, body').animate({
						scrollTop: $("#uploadLimits").offset().top
					}, 1000);
				});
				// скролл к пункту оптимизации в настройках загрузки из предупреждения о хотлинке
				$('#go-to-opt').click(function(){
					$('#settingsLink').click();
					$('html, body').animate({
						scrollTop: $("#settingsPic").offset().top
					}, 1000);
					$(".optimization").fadeOut(1000).fadeIn(1000).fadeOut(500).fadeIn(500).fadeOut(500).fadeIn(500);
				});
				// скролл к пункту уменьшения изображения в настройках загрузки из предупреждения о хотлинке
				$('#go-to-resize').click(function(){
					$('#settingsLink').click();
					$('html, body').animate({
						scrollTop: $("#settingsPic").offset().top
					}, 1000);
					$(".resizeSettings").fadeOut(1000).fadeIn(1000).fadeOut(500).fadeIn(500).fadeOut(500).fadeIn(500);
				});
				$('.hotlink-warning').click(function(e){ 
					var offset = $(this).offset();
					if (e.pageY < offset.top) {
						// клик по "больше не показывать"
						$.cookie('fp_no_hotkey_message', 1, {expires: 365, path: '/'});
						$(this).hide();
					}
				});
				if ($.cookie('fp_no_hotkey_message') != "1") {
					$('.hotlink-warning').show();
				}
				$('.hotlink-warn').click(function(){
					$.cookie('fp_no_hotkey_message', 0, {expires: 365, path: '/'});
					$('.hotlink-warning').show();
				});
			});
			</script>
			<script type="text/javascript">
			attach_limit = 30;
			$(document).ready(function(){
				// восстановление сохраненных настроек
				var uform_params = $.cookie('fp_form_param');
				if (uform_params)
				{
					var params_obj = JSON.parse(uform_params);
					$('#upload').deserialize(params_obj);
					$('div.poster').find('input').attr('checked', 0);
					// temp
					if ($('#jpeg-quality').val() == "") $('#jpeg-quality').val(90);
					if ($('#thumb-size').val() == "") $('#thumb-size').val(150);
				}
				// сабмит формы
				$('#uploadButton').bind('click', function(){
					// сохранение настроек загрузки
					var form_obj     = $('#upload').serializeArray();
					var form_params  = JSON.stringify(form_obj);
					$.cookie('fp_form_param', form_params, {expires: 365, path: '/'});

					// проверка на заполненость полей
					if ($('#upload_files,.upfile').length > 0)
					{
						var valid_fields = false;
						$('.upfile').each(function(){
							if ($(this).val() != "") {
								valid_fields = true;
							}
						});
						if (!valid_fields && typeof $('#upload_files').val() != "undefined" && $('#upload_files').val() != "")	{
							valid_fields = true;
						}
						if (!valid_fields) {
							alert("Вы не выбрали изображение для загрузки");
							return false;
						}
					}

					var $ajax = $('#ajax-loading');
					$ajax.html('Идет загрузка на сервер...');
					//setPosition($ajax);
					$ajax.show();
				});
				// отключить все эффекты
				$('#offeffectsLink').bind('click', function(){
					var $ctx = $('#upload');
					$ctx.find('input[value=no]').attr('checked', true);
					$ctx.find('input[type=checkbox]').attr('checked', false);
					return false;
				});

				// auto check checkboxes on a slave input changes
				$('.f-image-opts').find('input[type="text"],select').each(function(){
					$(this).bind("keyup change", function(e) {
						$(this).parent().find('input[type="checkbox"]').attr('checked', true);
					});
				});
				$('.f-preview-opts').find('#thumb_text').keyup(function(){
					$('#check_thumb[value="text"]').attr('checked', true);
				});
			});
			</script>
			<form id="upload" action="/uploadmulti" method="post" enctype="multipart/form-data">
