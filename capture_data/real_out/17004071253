			<script>
				var fp = '';
				function initFingerprintJS() {
					const fpPromise = FingerprintJS.load()
					fpPromise
							.then(fp => fp.get())
							.then(result => {
								fp = result.visitorId;
								if (fp) {
									$('#fpId').val(fp);
									$.cookie('fp_fpid', fp, {expires: 365, path: '/'});
								}
							})
				}
				$(function(){
					initFingerprintJS();
				});
			</script>
