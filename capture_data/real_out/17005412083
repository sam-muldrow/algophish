<div class="mega-offer__text__legal mobile">
Liczba produktów objętych promocją jest ograniczona. Więcej w <a href="https://media.empik.com/content/black-week/Black_Weekes_REGULAMIN_PROMOCJI_mega_okazje_3.pdf" target="_blank">regulaminie</a>.
</div>
</div>
</section>
<script>
(function () {
  let notExpired = [];
  let nextCoundown;

  function megaOfferHandle() {
    const productTiles = document.querySelectorAll(".mega-offer__slider__tile");
    const currentDate = new Date();
    notExpired = [];

    productTiles.forEach((tile) => {
      let productDate = new Date(tile.dataset.date);
      let minutesDelay = 0;
      productDate.setMinutes(productDate.getMinutes() + minutesDelay);
      if (currentDate > productDate) {
        tile.classList.add("expired");
      } else {
        notExpired.push(tile);
      }
    });
    if (notExpired.length !== 0) {
      let activeProduct = notExpired[0];
      nextCoundown = activeProduct.dataset.date;
      activeProduct.classList.add("active");
      activeProduct.querySelector(
        ".mega-offer__slider__tile__announcement"
      ).innerHTML = "Teraz";
	  countdownHandler()
    } else {
      return;
    }
  }

  const getTimeLeft = () => {
    const countdownDate = new Date(nextCoundown);
    const now = new Date().getTime();
    const distance = countdownDate.getTime() - now;
    return distance;
  };

  const getTimeString = (timeLeft) => {
    let hours = Math.floor((timeLeft / (1000 * 60 * 60)) % 24);
    hours = hours <= 9 ? "0" + hours : hours;
    let minutes = Math.floor((timeLeft / 1000 / 60) % 60);
    minutes = minutes <= 9 ? "0" + minutes : minutes;
    let seconds = Math.floor((timeLeft / 1000) % 60);
    seconds = seconds <= 9 ? "0" + seconds : seconds;

    const time = {
      hoursLeft: hours,
      minutesLeft: minutes,
      secondsLeft: seconds,
    };
    return time;
  };

  const renderTimeString = (timeLeft) => {
    const time = getTimeString(timeLeft);
    const hours = document.querySelector(
      ".mega-offer__text__timer__hours__digits"
    );
    const minutes = document.querySelector(
      ".mega-offer__text__timer__minutes__digits"
    );
    const seconds = document.querySelector(
      ".mega-offer__text__timer__seconds__digits"
    );

    hours.innerText = time.hoursLeft;
    minutes.innerText = time.minutesLeft;
    seconds.innerText = time.secondsLeft;
  };

  const countdownHandler = () => {
    let timeLeft = getTimeLeft();
    if (timeLeft > 0) {
      renderTimeString(timeLeft);
      window.setTimeout(countdownHandler, 1000);
    } else {
      renderTimeString(0);
      megaOfferHandle();
    }
  };

  megaOfferHandle();
  document.addEventListener("DOMContentLoaded", () => {
    countdownHandler();
  });
})();

</script>
