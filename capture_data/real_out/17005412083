.empikHeader__logo--premium .sticky-navbar{
	left:17px;
}
body.sticky-navbar h1 .empikHeader__logo--premium{
top:0;
}
body.sticky-navbar .empikHeader__logo--premium {
    float: none;
    margin-left: 35px;
    left: 0;
    top: -10px;
    display: inline;
}
@media screen and (min-width: 1052px)
body.sticky-navbar .empikHeader__logo {
    height: 63px;
}

span.concepts {
    font-weight: 300!important;
    color: #949a9d!important;
    font-size: 13px!important;
    line-height: 15px!important;
}
