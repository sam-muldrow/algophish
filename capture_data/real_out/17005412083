<script>
(function () {  
    const buttons = document.querySelectorAll('.tina-beam-box__link');

    buttons.forEach((button, index) => {
    let camapignName = button.dataset.campaign;
    button.addEventListener('click', () => {
        dataLayer.push({
        event: "recommendation_tracking",
        key1: "click",
        key2: "empik",  
        key3: "wersja A",
        key4: "Panel " + (index + 1) + " | " + camapignName,
        key5: "Panele pod Tina"
        })
    }, { once: true });

    let observer = new IntersectionObserver((entries, observer) => {
        for (entry of entries) {
        let ratio = entry.intersectionRatio;
        if (ratio == 1 && button.getAttribute("ifshowed") == null) {
            button.setAttribute('ifshowed', 'true');
            dataLayer.push({
            event: "recommendation_tracking",
            key1: "view",
            key2: "empik",  
            key3: "wersja A",
            key4: "Panel " + (index + 1) + " | " + camapignName,
            key5: "Panele pod Tina"
            });
        }
        }
    }, {threshold: [1]})
    observer.observe(button);
    });
})();
</script>
