<script>
    document.addEventListener("DOMContentLoaded", function() {
        if (window.location.href.includes("campaign/")) {
            const links = document.querySelectorAll('a'),
                domainsToDecorate =  "dstv.co.za".split(','),
                queryParams = [
                    'utm_medium',
                    'utm_source',
                    'utm_campaign',
                    'utm_term',
                    'utm_content',
                    'utm_id'
                ];

            for (var linkIndex = 0; linkIndex < links.length; linkIndex++) {
                for (var domainIndex = 0; domainIndex < domainsToDecorate.length; domainIndex++) {
                    if (links[linkIndex].href.indexOf(domainsToDecorate[domainIndex]) > -1) {
                        links[linkIndex].href = decorateUrl(links[linkIndex].href);
                    }
                }
            }

            function decorateUrl(urlToDecorate) {
                var collectedQueryParams = [];
                for (var queryIndex = 0; queryIndex < queryParams.length; queryIndex++) {
                    if (getQueryParam(queryParams[queryIndex])) {
                        collectedQueryParams.push(queryParams[queryIndex] + '=' + getQueryParam(queryParams[queryIndex]))
                    }

                }

                var urlParts = urlToDecorate.split('#');
                urlToDecorate = collectedQueryParams.length ? (urlParts[0].indexOf('?') === -1) ? urlParts[0] + '?' : urlParts[0] + '&' : urlToDecorate;
                return collectedQueryParams.length ? ((urlToDecorate + collectedQueryParams.join('&')) + ((urlParts.length > 1) ? '#' + urlParts[1] : '')) : urlToDecorate;
            }

            function getQueryParam(name) {
                if (name = (new RegExp('[?&]' + encodeURIComponent(name) + '=([^&]*)')).exec(window.location.search)) {
                    return decodeURIComponent(name[1]);
                }
            }
        }
    });
</script>
