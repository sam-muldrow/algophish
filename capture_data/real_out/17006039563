	<script>
	$(function(){
		requirejs(['city-select-control'], function (CitySelectControl) {
			var link = document.querySelector('#cityNavigator_655d2835472e7');

			var options = {
				requestParams: {"return":null,"city":null},
				allowAllCities: true,
				allowWholeRegion: true,
				selectedGeo: {"city":0,"region":0,"federalDistrict":0,"country":0},
				initialValue: null,
				urlTemplateSaveCity : '/set/city/cityIdValue',
				defaultCallbackCityClick: function (modal) {
					modal && modal.disappear && modal.disappear();

					if (link.matches('.js-main-page-choose-dummy')) {
						baza.dummyStatistics('main-page-geo-choose', 'select-from-modal');
					}
				}
			}

			function onCreateHandler () {
				baza.eventManager.fire('select-city-popup-open');
			}

			CitySelectControl.initDesktop($(link), options, onCreateHandler);
		});
	});
	</script>
									</td>
			
