<div class="bzr-index-dirs__form bzr-index-dirs__tiles">
			<meta itemprop="url" content="https://www.farpost.ru"/>
		<form method="get" action="/dir" itemprop="potentialAction" itemscope itemtype="http://schema.org/SearchAction">
					<meta itemprop="target" content="https://www.farpost.ru/dir?query={query}"/>
				
<div class="bigSearchForm standAlone">
	<table cellpadding="0" cellspacing="0">
		<tr>
			<td>
									
					<div class="bzr-field" id="search-input-container">
											<input id="search" class="i bzr-field__text-input bzr-field__text-input_clearable" name="query" type="search" tabindex="1" itemprop="query-input" />
						<div style="display: none" class="bzr-field__clear js-clear-button">&nbsp;</div>
						<script>
							$(function () {
								var $searchInput = $('#search');
								var $container = $('.bzr-field__search');
								$searchInput
									.on('focusin', function () {
										$container.addClass('bzr-field__search_hidden');
									})
									.on('focusout', function () {
										if (!this.value) {
											$container.removeClass('bzr-field__search_hidden');
										}
									});

								$searchInput.on('keydown', function (e) {
									if (e.keyCode === 13) {
										baza.dummy.send('search_enter_on_mobile_keyboard_click');
									}
								});

								requirejs(['helpers.CleanedInputController'], function (CleanedInputController) {
									var $container = $('#search-input-container');
									var $input = $container.find('#search');
									var $button = $container.find('.js-clear-button');

									CleanedInputController
										.create($input, $button)
										.withAnalitics('search-input', "main")
									;
								});
							});
						</script>
					</div>
							</td>
							<td class="cok">
					<input class="bzr-btn bzr-btn_size_l bzr-btn_style_info" type="submit" value="Найти"/>
				</td>
					</tr>
	</table>
</div>
		<script type='text/javascript'>
	$(function () {
		function onSubmit(event) {
			event.preventDefault();

			var $form = $(this);

			requirejs(['autocomplete.metric-api'], function (MetricApi) {
				MetricApi.logSubmit($input.val()).finally(function () {
					$form.off('submit', onSubmit).submit();
				});
			}, function () {
				$form.off('submit', onSubmit).submit();
			});
		}

		function onKeyup() {
			if ($.trim($input.val())) {
				requirejs(['autocomplete.metric-api'], function (MetricApi) {
					MetricApi.logTyping();
				});

				$input.off('keyup', onKeyup);
			}
		}

		var $input = $('.bigSearchForm .i')
			.addClass('lazyLoadAutocomplete')
			.data('autocompleteOptions', {
				dummyEnable: true,
				source: '/backend/search-suggest/v2/suggest.json',
			})
			.on('keyup', onKeyup)
			.closest('form').on('submit', onSubmit).end();
	});
</script>
	</form>
	<script>
		$(function () {
			var q = $("[name=query]:visible:first").get(0);
			if (q && q.getBoundingClientRect) {
				var br = q.getBoundingClientRect();
				if (br.top > 0) q.focus();
			}
		});
	</script>

	
	
