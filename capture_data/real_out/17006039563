<script>
$(function(){
	var selectors = $('.mainTabs .selector a');
	var tabs = $('.mainTabs .tab');

	selectors.click(function(){
		selectors.removeClass('s');
		$(this).addClass('s');

		var index = selectors.index(this);
		var selectedTab = tabs.hide().eq(index);
		selectedTab.show();
		baza.candy.initAllPlaces();

		return false;
	});
})
</script>


