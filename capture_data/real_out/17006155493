	function share(){
		var winTop = (screen.height / 2) - (350 / 2);
		var winLeft = (screen.width / 2) - (520 / 2);
		window.open('https://www.facebook.com/sharer.php?s=100&p[title]=主頁&p[url]=https://www.passiontimes.hk', 'sharer', 'top=' + winTop + ',left=' + winLeft + ',toolbar=0,status=0,width=' + 520 + ',height=' + 350);
	}


	
