<script>
  function addTagArgs() {
    try {
      const topDummy = document.cookie.split(';').find(function(s) {
        return s.trim().startsWith('top_dummy=');
      });
      const var4 = topDummy ? topDummy.split('=')[1] : '';

      const topPvUid = document.cookie.split(';').find(function(s) {
        return s.trim().startsWith('top_pv_uid=');
      });
      const var5 = topPvUid ? topPvUid.split('=')[1] : '';

      i3('set', 'common', {var4:var4, var5:var5});

    } catch(e) {
      console.error(e);
    }
  }
</script>
