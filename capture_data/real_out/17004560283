			pbjs.addAdUnits(adUnits);
			pbjs.requestBids({
			  bidsBackHandler: function() {
			    adServerCallback('prebid');
			  }
			});
		});

		setTimeout(function() {
		  adServerCallback('timeout');
		}, PREBID_TIMEOUT);

  </script>	
