	refinition = (window.refinition || []);
	blogherads = blogherads || {};
	blogherads.adq = blogherads.adq || [];
	blogherads.adq.push(function() {
		// If this function not exist, this mean TreasureData is disabled.
		if ( ! blogherads.treasureDataReady ) {
			refinition.push( {
				"event": "impression",
				"sessionId": ''
			} );
			return;
		}

		blogherads.treasureDataReady().then( function( data ) {
			refinition.push( {
				"event": "impression",
				"sessionId": data?.td_ssc_id || ''
			} );
		});
	});
