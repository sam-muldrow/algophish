<script>
    $().ready(function () {
        $(".elevator_item_custom").hide();
    });

    $(window).scroll(function () {
        var scrolltop = $(this).scrollTop();
        if (scrolltop >= 200) {
            $(".elevator_item_custom").show();
        } else {
            $(".elevator_item_custom").hide();
        }
    });
</script>


