  <script>
  $('.showBtn').click(function() {
  //$('.hideme').hide();
  if ($(this).hasClass('active')) {
  $(this).removeClass('active');
  $('.hideme').slideUp();
  } else {
  $('.hideme').slideUp();
  $('.showBtn').removeClass('active');
  $(this).addClass('active');
  $(this).next().filter('.hideme').slideDown();
  }
  });
