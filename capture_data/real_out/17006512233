    <script src="https://www.google.com/recaptcha/api.js?onload=onloadCallback" async defer></script>
<script language="javascript" type="text/javascript">

var ISBNDb_apiKey = 4266452882;
var ISBNDb_version = 2;

var varpressfocus = 4;

function pressfocus(num) {
	// TODO lucy index cannot use "hid" class on these because the
	// div#quotes div rule uses display: none;
	$J('#quote' + varpressfocus).hide();
	$J('#quote' + num).show();
	varpressfocus = num;
}

function toggleSignup() {
	var alreadySignup = $J("#alreadyS");
	var newSignup = $J("#newS");
	if(alreadySignup.hasClass('hid')) {
		newSignup.addClass('hid');
		alreadySignup.removeClass('hid');
	}
	else {
		newSignup.removeClass('hid');
		alreadySignup.addClass('hid');
	}
}

function flipsignup(onoff) {
	toggleSignup();
	focusOnSignup();
}

function showJoinnow() {
	showSigninJoinnowWell('join');
}

function showSignin() {
	showSigninJoinnowWell('signin');
}

function showSigninJoinnowWell(signinorjoin) {
	var signin_container = $J('.signin_container').first();
	var alreadySignup = $J("#alreadyS");
	var newSignup = $J("#newS");
	var signin_join_well = $J('#signin_join_well');
	const sjw_hidden = signin_join_well.hasClass('hid');
	const new_hidden = newSignup.hasClass('hid');
	const signin_hidden = alreadySignup.hasClass('hid');

	if (signinorjoin === 'join' && !new_hidden && !sjw_hidden) { return; }
	else if (signinorjoin === 'signin' && !signin_hidden && !sjw_hidden) { return; }
	else {
		if (signinorjoin === 'join') {
			$J('#joinnow_button').removeClass('btn-default').addClass('btn-danger');
			$J('#signin_button').removeClass('btn-danger').removeClass('btn-default');
		}
		else {
			$J('#signin_button').removeClass('btn-default').addClass('btn-danger');
			$J('#joinnow_button').removeClass('btn-danger').removeClass('btn-default');
		}

		var duration = signin_container.hasClass('active') ? 0 : 400;
		signin_container.animate({
			top:0
		}, duration,
			function() {
				signin_container.addClass('active');
				if (signinorjoin === 'join') {
					newSignup.removeClass('hid');
					alreadySignup.addClass('hid');
					$J('#new_formusername').focus().select();
				} else {
					newSignup.addClass('hid');
					alreadySignup.removeClass('hid');
					$J('#already_formusername').focus().select();
				}
				$J('#signin_join_well').slideDown();
			});

	}
}


function indexCookieVal(cookieName) {
	thisCookie = document.cookie.split("; ")
	for (i=0; i<thisCookie.length; i++)
		{
		if (cookieName == thisCookie[i].split("=")[0]) {
			return thisCookie[i].split("=")[1]
		}
	}
	return 0;
}

function focusOnSignup() {
	try {
		var already_newid = $J('#already_new');
		if(already_newid.attr('class') == 'new') {
			$J('#new_formusername').focus();
		}
		else {
			$J('#already_formusername').focus();
		}

		var nfa = $J('#already_formusername');
		if (nfa.is(":visible") ) { nfa.focus(); }

		var nfu = $J('#new_formusername');
		if (nfu.is(":visible") ) { nfu.focus(); }
	} catch(err) {}
}

// This stuff is for the new-member password strength checking.
var keyup_handler = function() {
	var pw = $J('#new_formpassword').val();
	var score = passWordStrength(pw);
	score = (score > 3) ? 3 : score;
	console.info(score);
	var pwsm = $J('#pwsm');
	pwsm.removeClass('score0');
	pwsm.removeClass('score1');
	pwsm.removeClass('score2');
	pwsm.removeClass('score3');
	pwsm.removeClass('score4');
	pwsm.removeClass('score5');
	pwsm.addClass('score'+score);
};

var checkResetForm = function(e) {
	//return;
	var error = false;

	try {
	var uname = $J('#new_formusername');
	var unamev = uname.val();
	var pw = $J('#new_formpassword');
	var pwc = $J('#new_formpassword_confirm');
	var pwv = pw.val();
	var pwcv = pwc.val();
	var email = $J('#formemail');
	var emailv = email.val();

	//debugger;
	mmlog('checking passwords');

	if (unamev == '') {
	uname.flash();
	error = true;
	}

	if (pwv.length < 5) {
	pw.flash();
	error = true;
	}

	if (pwv != pwcv) {
	mmlog('passwords do not match!');
	pwc.flash();
	error = true;
	}

	if (emailv == '') {} // allow empty email
	else if (!pw_validateEmail(emailv)) {
	mmlog('invalid email');
	email.flash();
	error = true;
	}

	}
	catch(err) { mmlog('error thrown'); error = true; }

	if (error) {
	if (e.preventDefault) { e.preventDefault();} else {e.returnValue = false; }
	}
};

var pw_validateEmail = function(email) {
	email = email.trim();
	var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
	return re.test(email);
};

var show_pw_info = function() {
	return; /* disabled because there just isn't enough room */
	var pwe = $J('#pw_explain');
	if (pwe.is(":visible") ) {
	pwe.fadeOut();
	}
	else {
	pwe.fadeIn();
	}
};

$J(function(){

	keyup_handler();
	$J('#new_formpassword').keyup(keyup_handler);

	pressfocus(4);
	focusOnSignup(false);
	
	$J('.parallax').each(function() {
		var el = lt.elf(this);
		var parallax_start = parseFloat(el.css('background-position-y'));
		el.attr('data-parallax-start', parallax_start);
	});

	$J(window).on('scroll.parallax', function() {
		var ws = $J(window).scrollTop();
		$J('.parallax').each(function() {
			const parallax_speed = 1.0;
			var el = $J(this);
			var elt = parseFloat(el.css('top')) || 0;
			var parallax_start = parseFloat(el.attr('data-parallax-start'));
			var c = (ws * parallax_speed) + parallax_start + 'px';
			mmlog(ws);
			mmlog(elt);
			mmlog(c);
			el.css({
				'background-position-y': c
			});
		});
	});


});

</script>
