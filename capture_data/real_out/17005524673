<script>
var pikupMoreBtn = document.getElementById('pikupMoreBtn');
if (pikupMoreBtn) {
    pikupMoreBtn.addEventListener('click', function (e) {
        var artistRankHidden = document.querySelectorAll("#pickup-list li");
        if (artistRankHidden) {
            artistRankHidden.forEach(function (li) {
                li.classList.remove("d-none");
            });
        }
        pikupMoreBtn.parentNode.removeChild(pikupMoreBtn);/**/
    });
}


// pc-header に floating をつけはずし 2021.08.05
var floatPcHeader = document.getElementById("header-main");
if (floatPcHeader) {
    var changeFloat = function(){
		// contains で クラスが含まれているか確認
        if ((naviPosy + 100) < window.scrollY) {
			if (!floatPcHeader.classList.contains("is-float")) {	// floating動作付加
				floatPcHeader.classList.remove('is-hidden');
				floatPcHeader.classList.add('is-float');
				floatPcHeader.style.transition = '.3s';
			}
		} else if (naviPosy < window.scrollY) {
		// スクロール位置がグローバルナビの上端を過ぎている=ヘッダーが全部隠れている
			if (floatPcHeader.classList.contains("is-float")) {	// floatingがついていれば解除してtransitionつける
//console.log('メニュー表示中 is-float あり');
				floatPcHeader.classList.remove('is-float');
				floatPcHeader.style.transition = '.3s';
			}
			if(!floatPcHeader.classList.contains("is-hidden")){
				floatPcHeader.classList.add('is-hidden');
			}
        } else if (naviPosy >= window.scrollY) {
//console.log('スクロール中 floating 解除');
			floatPcHeader.style.transition = '0s';
            if (floatPcHeader.classList.contains("is-hidden")) {
				floatPcHeader.classList.remove('is-hidden');
			}
            if (floatPcHeader.classList.contains("is-float")) {
	            floatPcHeader.classList.remove('is-float');
			}
        }
    };
    
    // 画面の上端から、要素の上端までの距離
    var naviPosy = document.getElementById("nav-area").getBoundingClientRect().top + window.pageYOffset;
//console.log('parentPosy = ' + parentPosy + ', window スクロールPos = ' + window.scrollY);
	// ロード直後の一回
    changeFloat();
	
    window.addEventListener('scroll', function(){
        changeFloat();
    });
}

</script>