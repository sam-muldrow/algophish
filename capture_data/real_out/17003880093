    <script vmid="reve-script">
      window.reve = window.reve || {};
      window.reve.cmd = window.reve.cmd || [];

      const createUUID = () => {
        const pow = Math.pow(10, 10)
        const num1 = Math.floor(Math.random() * pow)
        const num2 = Math.floor(Math.random() * pow)

        return `${num1}.${num2}`
      }

      const getPublisherProvidedID = () => {
        // https://developer.mozilla.org/en-US/docs/Web/API/Window/sessionStorage
        const sessionKey = 'PublisherProvidedID'
        // if session exists keep using the same session
        let sessionId = window.sessionStorage.getItem(sessionKey)

        if (!sessionId) {
          sessionId = createUUID()
          // else create new uuid, set the session and return that value
          window.sessionStorage.setItem(sessionKey, sessionId)
        }

        return sessionId
      }

      // Reve script
      function loadReve() {
        // Assertive Yield test CM-8129 - disable Reve
        const disableReve = ''
        if (disableReve) return

        const appLoaded = document.querySelector('#app')
        if (!window.lg || !appLoaded) return

        const gptTargeting = typeof window._reveConfig === 'undefined' ?
          {} :
          (window._reveConfig.dfpData || {})
        gptTargeting.lg_uuid = getPublisherProvidedID()
        gptTargeting.experiment = window.lg.testpool

        if (window.REVE_LOADED) return  // only load once
        window.REVE_LOADED = true

        window.reve.cmd.push(() => {
          window.reve.setState('gptTargeting', gptTargeting);
          window.reve.setState('permutiveTargeting', window._reveConfig.permutiveData);
        })

        const reveScript = document.createElement('script');
        reveScript.src = 'https://reve.leafmedia.io/livestrong.js';
        reveScript.defer = true;
        reveScript.setAttribute('vmid', 'reve-site-script');
        reveScript.setAttribute('hid', 'reve-site-script');
        document.head.appendChild(reveScript);
      }

      window.loadReve = loadReve;

      window.addEventListener('DOMContentLoaded', () => {
        setTimeout(loadReve, 0)
      })

      // Assertive Yield test CM-8129
      function loadAssertiveYield() {
        const enableAssertiveYield = ''
        if (!enableAssertiveYield) return

        if (window.AY_LOADED) return  // only load once
        window.AY_LOADED = true 

        // AY Yield Manager script
        const yieldManager = document.createElement('script');
        yieldManager.src = '';
        yieldManager.referrerpolicy = 'no-referrer-when-downgrade';
        document.head.appendChild(yieldManager);
      }

      loadAssertiveYield();
    </script>

