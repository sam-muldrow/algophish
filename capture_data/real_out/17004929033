    <script type="text/javascript">
    	$(function(){
    		var titles = $(".game-icon-80x60 .game-title");
    		for(var i = 0; i < titles.length;i++) {
    			var thisTitle = titles[i];
    			var containerHeight = $(thisTitle).height();
    			var titleHeight = $(thisTitle).children("div").height();
    			if(titleHeight > containerHeight) {
    				$(thisTitle).append("<div class='game-title-ellipsis'>...</div>");
    			}
    		}
    	});
    </script>

<script type="text/javascript">
  
	(function() {
		var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
		po.src = 'https://apis.google.com/js/plusone.js';
		var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);
	})();

	var _AnemoneParams = {
	    getAppParams: function() {
	        return { pageLang: 'en' };
	    }
	};

		(function(d,s,id){
		var js,fjs=d.getElementsByTagName(s)[0];
		if(!d.getElementById(id)){
			js=d.createElement(s);
			js.id=id;js.src="//platform.twitter.com/widgets.js";
			fjs.parentNode.insertBefore(js,fjs);
		}
	}(document,"script","twitter-wjs"));
</script>
