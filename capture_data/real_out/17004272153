Handlebars.registerHelper('ifPositiveNumber', function (number, options) {
return parseInt(number) >= 0 ? options.fn(this) : options.inverse(this);
});
let json = '{"_DATA":[{"date":"2023-11-17 18:05:39","prepare_price":"2,469.85","code":"KSP","prepare_rate":-0.74,"price":"2,469.85","name":"코스피","mark":-1},{"date":"2023-11-17 18:05:39","prepare_price":"799.06","code":"KSQ","prepare_rate":-1.49,"price":"799.06","name":"코스닥","mark":-1},{"date":"2023-11-17 18:05:34","prepare_price":"332.06","code":"K200","prepare_rate":-0.67,"price":"332.06","name":"코스피200","mark":-1},{"date":"2023-11-19 21:00:32","prepare_price":"-30","code":"MUSD","prepare_rate":0.0,"price":"1,296.50","name":"미국 USD","mark":-1},{"date":"2023-11-20 05:53:06","prepare_price":"0","code":"KRW-BTC","prepare_rate":0.23,"price":"49,321,000","name":"비트코인","mark":1},{"date":"2023-11-20 05:53:06","prepare_price":"0","code":"KRW-ETH","prepare_rate":0.64,"price":"2,654,000","name":"이더리움","mark":1},{"date":"2023-11-20 05:53:06","prepare_price":"0","code":"KRW-XRP","prepare_rate":1.34,"price":"833","name":"리플","mark":1}]}';
