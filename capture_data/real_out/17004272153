<script>
const newsLetterUl = $("#newsLetter-ul");
const newsLetterLis = newsLetterUl.find(".news_node");
const newsLetterLisCnt = newsLetterLis.length;
for (let i = newsLetterLisCnt - 1; i > 0; i--) {
const j = Math.floor(Math.random() * (i + 1));
const temp = newsLetterLis.eq(i).html();
newsLetterLis.eq(i).html(newsLetterLis.eq(j).html());
newsLetterLis.eq(j).html(temp);
}
for (let i = 0; i < newsLetterLis.length; i++) {
newsLetterLis.eq(i).find("a").attr('data-idx', '00'+ (i+1));
}
for (let i = 4; i < newsLetterLis.length; i++) {
newsLetterLis.eq(i).remove();
}
</script>
