<script>
function sendSearchGA(isSearch) {
const depthTag = 'data-category_1depth="뉴스" data-category_2depth="홈" ';
var gaLabel = "";
if(depthTag != "") {
gaLabel = isSearch ? "직접입력" : "최근검색어";
let pushObj = {
"data-category_1depth" : "뉴스",
"data-category_2depth" : "홈",
"data-section" : "search",
"data-label" : gaLabel
};
let depth3 = "";
if(depth3 != "") {
pushObj["data-category_3depth"] = depth3;
}
dataLayer.push(pushObj);
}
}
</script>
