            var videosThumbnail = jQuery('.c-puremedias-videos__playlist .u-grid > div');

            videosThumbnail.on('click', function(){

                var videoId = jQuery(this).find('.c-video-thumbnail')[0].dataset.videoid;

                var __FOUND = config.videos.find(function(post, index) {
                    if(post.id == parseInt(videoId))
                        return true;
                });

                jad.cmd.push(function () {
                    jad.public.setTargetingOnPosition("preroll_playertop", {
                        duration_steps : "4",
                        short_video : "no"
                    });

                    jad.public.getDailymotionAdsParamsForScript(['preroll_playertop/preroll_playertop'], function (vastUrl) {

                        dailymotion.getPlayer('dailymotion-player-4694245').then((player) => {

                            
                            // update values for advertising
                            player.setCustomConfig({ customParams: vastUrl['preroll_playertop/preroll_playertop'] });

                            // load a video id in the pre loaded player
                            player.loadContent({video:__FOUND.dailymotion_id});

                            $j.Tracking.DM.trackPlayer(player, vastUrl);
                        });
                    });
                });
            })

