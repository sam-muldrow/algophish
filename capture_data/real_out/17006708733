<script type="text/javascript" defer="defer">
    var custom_dimmensions;
    $j(function() {
                    var custom_dimensions = {"hasVideoContent":true};
            // Default value
            if (typeof(custom_dimensions.hasVideoContent) === 'undefined') {
                custom_dimensions.hasVideoContent = false;
            }
        
        var adblock = (typeof wads !== 'undefined') ? (wads.public.adblockDetected() ? 'adblock' :
            'no-adblock') : 'adblock-nc';
        custom_dimensions['adblockHit'] = adblock;
        custom_dimensions['adblockSession'] = adblock;
        custom_dimensions['adblockUser'] = adblock;

        data_layer = custom_dimensions;

        $j.Tracking.Target.UA.customDimensionsAndMetrics.authorId = "dimension11";
        $j.Tracking.Target.UA.customDimensionsAndMetrics.typology = "dimension16";
        $j.Tracking.Target.UA.customDimensionsAndMetrics.content_id = "dimension17";
        $j.Tracking.Target.UA.customDimensionsAndMetrics.didomiConsent = "dimension19";
        $j.Tracking.Target.UA.customDimensionsAndMetrics.videoDisplayHostname = "dimension20";
        $j.Tracking.Target.UA.customDimensionsAndMetrics.videoPlayingID = "dimension21";
        $j.Tracking.Target.UA.customDimensionsAndMetrics.videoRelatedEntity = "dimension22";
        $j.Tracking.Target.UA.customDimensionsAndMetrics.videoRelatedEntityID = "dimension23";
        $j.Tracking.Target.UA.customDimensionsAndMetrics.triggerMethod = "dimension24";
        $j.Tracking.Target.UA.customDimensionsAndMetrics.diaporama = "dimension26";


        window.tracker = $j.Tracking({"debugMode":  false});

                    tracker.addTarget("t0", "UA", {
                "config":   { "trackingId":   "UA-8847114-1" }
            });
            tracker.send({"anonymizeIp": true}, ["t0"]);

            // Google Discover tracking
            tracker.send({"dimension25": document.referrer}, ["t0"]);

            var pageViewSent = false;

            function sendPageView(method, consent) {
                if (!pageViewSent) {
                    if (consent) {
                        tracker.send({"allowAdPersonalizationSignals": false}, ["t0"]);
                    } else {
                        tracker.send({"allowAdFeatures": false}, ["t0"]);
                    }
                    custom_dimensions.triggerMethod = method;
                    tracker.send(custom_dimensions, ["t0"]);
                    tracker.send({ "hitType": "pageview" }, ["t0"]);
                }
                pageViewSent = true;
            }

            window.didomiOnReady = window.didomiOnReady || [];
            window.didomiOnReady.push(function(Didomi) {
                const consent = Didomi.isConsentRequired() ?
                    Didomi.getUserConsentStatusForVendor("c:googleana-XMqagawa") && Didomi
                    .getUserConsentStatusForVendor("google") :
                    true;
                custom_dimensions.didomiConsent = Didomi.getUserConsentStatusForVendor(
                    "c:googleana-XMqagawa") ?? false;
                sendPageView("didomiOnReady", consent);
            });
            setTimeout(function() { sendPageView("timeout", false); }, 1000);

                tracker.addTarget("gtm", "GTM");

        tracker.addTrigger('clic', 'Click').setTargets(["t0"]);
        tracker.addTrigger('diapo', 'Diaporama');
        tracker.addTrigger('facebook', 'Facebook');
        tracker.addTrigger('twitter', 'Twitter');
        tracker.addTrigger('disqus', 'Disqus');


        tracker.addTrigger('video', 'JWPlayer');

        if (typeof(DPMTC) != 'undefined') {
            if (typeof DPMTC.analyticsRegister == 'function') {
                DPMTC.analyticsRegister(function(ev) {
                    tracker.send({
                        "hitType": 'event',
                        "category": ev.product.category,
                        "action": 'clic-marchand',
                        "label": 'brand: ' + ev.product.brand + ', price: ' + ev.product.price +
                            ', retailer:' + ev.product.retailer
                    });
                });
            }
        }

    });

    WB.defer(function() {
        // if country code is in cookie, send whithout json request
        if (WB.utils.get_cookie('country_code')) {
            var country_code = WB.utils.get_cookie('country_code')
            data_layer['visitor_country'] = country_code;
            tracker.send(data_layer, ["gtm"]);
        }
    });
</script>
