    function sendCustomPixel(method, consent, tcfStatus) {
        var pixelUrl = "https://bpt.webedia-group.com/pixel.png?/domain=" + window.location.hostname + "&referrer=" +
            document.referrer + "&site_route=" + jadConfig.page + "&event=tcfapi&method=" + method + "&consent=" +
            consent + "&tcfStatus=" + tcfStatus
