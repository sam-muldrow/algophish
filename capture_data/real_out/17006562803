  function resizeTabs (){

    //when on resize adds event listener

    $(window).bind("resize", function () {

      if ($(this).width() < 500) {
        $('#mmtabList').removeClass('nav-justified');
      } else {
        $('#mmtabList').addClass('nav-justified')
      }
    }).trigger('resize');
  }
//createTabs();
            })(jQuery);
});
