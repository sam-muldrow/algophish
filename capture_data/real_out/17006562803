  function createTabs () {
    /*Content for the empty JP*/
    //for inside the events tab
    var iconTab = {
      events: 'fa-ticket',
      holidays: 'fa-tag',
      coach: 'fa-bus',
      h5Heading: { events: 'Events', 
                  holidays: 'Holidays', 
                  coach: 'Choose Your Journey'
                 }, 
      tabHeading: { events: 'Events', 
                   holidays: 'Holidays', 
                   coach: 'Coach only'
                  }, 
      cta: {events: 'See all events', 
            holidays: 'Search for a holiday',
            coach : 'Book Now'

           }, 
      ctaHyperlink: { events: 'window.location.href="/en/events"', 
                     holidays: 'window.open("https://holidays.nationalexpress.com?utm_source=nationalexpress&utm_medium=referral&utm_campaign=jptab","_blank" )', 
                     coach : 'n/a'
                    }
    };

    var eventsContent = 'From Wembley to Glastonbury, let National Express take you to the biggest Events & Festivals across the UK '; 
    var holidaysContent = 'Create your own UK holiday package by combining our coach travel with your choice of hotels and attractions with a Lowest Price Guarantee. Or compare our range of set-itinerary UK & European coach tours.'; 
    var innerJpEvents = '<div class="row"><div class="col-lg-8 col-md-8"><p class="mmplannerFormText">' + eventsContent + '</p></div><div class ="col-xs-12 col-sm-12  col-md-4">';
    var innerJpHolidays = '<div class="row"><div class="col-lg-8 col-md-8"><p class="mmplannerFormText">' + holidaysContent + '</p></div><div class ="col-xs-12 col-sm-12  col-md-4">';
    var jpEventsButton = '<button id="jpSubmitEvents"  onclick="'+iconTab.ctaHyperlink.events+'" type="button" class="jpSubmit plannerForm__inputBelt__button primaryButton primaryButton--green">'+iconTab.cta.events+'</button></div></div>'; 
    var jpPlusButton = '<button id="jpSubmitHolidays"  onclick="window.open(\'https://holidays.nationalexpress.com/?utm_source=nationalexpress&utm_medium=referral&utm_campaign=jptab\',\'_blank\' )" "type="button" class="jpSubmit plannerForm__inputBelt__button primaryButton primaryButton--green" target="_blank">'+iconTab.cta.holidays+'</button></div></div>';   
    var jpEvents = '<div class="plannerForm "><div class="plannerForm--title"><h5 class="plannerForm__title"></h5></div>' + innerJpEvents + jpEventsButton + '</div>'; 
    //for inside the Plus tabs
    var jpPlus = '<div class="plannerForm "><div class="plannerForm--title"><h5 class="plannerForm__title"></h5></div>' + innerJpHolidays + jpPlusButton + '</div>'; 

    /*html that contains the tab navigation bar */
    var string = '<ul class="nav nav-tabs nav-justified" id="mmtabList" role="tablist"><li class="nav-item active"><a class="nav-link" id="mmjp-tab" data-toggle="tab" href="#mmjp" role="tab" aria-controls="mmjp" aria-selected="true"><div class="row"><div class="col-lg-2 col-md-2 col-xs-12 mmalignI"><i class="fa '+iconTab.coach+' mmicon"></i></div><div class="col-lg-10 col-md-10 col-xs-12 mmalignT"><h5 class="plannerForm__title">' + iconTab.tabHeading.coach + '  </h5>  </div>  </div></a>  </li><li class="nav-item"><a class="nav-link" id="mmplus-tab" data-toggle="tab" href="#mmplus" role="tab" aria-controls="mmplus" aria-selected="true"><div class="row"><div class="col-lg-2 col-md-2 col-xs-12 mmalignI"><i class="fa ' + iconTab.holidays + ' mmicon"></i>  </div><div class="col-lg-10 col-md-10 col-xs-12 mmalignT holidays-tab"><h5 class="plannerForm__title holidays-tab">' + iconTab.tabHeading.holidays + '  </h5>  </div>  </div></a>  </li>  </ul>';
    /*string to wrap all JP into nav containers*/
    var tabContent = '<div class="tab-content" id="mmmyTabContent"/>'; 
    /*wrap current JP around this div*/
    var taby = ' <div class="tab-pane fade active in " id="mmjp" role="tabpanel" aria-labelledby="mmjp-tab"/>';
    /*Other tabs that contain Events and Plus */
    var nav = '<div class="tab-pane fade" id="mmevents" role="tabpanel" aria-labelledby="mmevents-tab">' + jpEvents + '</div><div class="tab-pane fade" id="mmplus" role="tabpanel" aria-labelledby="mmplus-tab">' + jpPlus + '</div>';
    /*wrap everything in jp in a container*/
    $('div.plannerForm').wrapAll(taby);
    /*JP is wraped around div with id of mmjp. attach the other nav panels after it*/
    $('#mmjp').after(nav);
    /*wrap everything with the tab content*/
    $('div.tab-pane').wrapAll(tabContent);
    /*add the navigation bar*/
    $('#mmmyTabContent').before(string);
    /*active the first tab (the JP)*/


  }
