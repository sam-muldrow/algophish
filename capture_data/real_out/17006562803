let tikTokLogo = document.querySelector(`#TikTok img`);

function setAttributes(el, attrs) {
  for(let key in attrs) {
    el.setAttribute(key, attrs[key]);
  }
}

setAttributes(tikTokLogo , {"width": "19", "height": "22"});
</script>
