<script>
    
    // Check to see if I131seen is present in localStorage, if it is, then do not run this campaign
    const i131seen = localStorage.getItem('I131seen');
    
    if (!i131seen) {
        document.addEventListener("DOMContentLoaded", () => {

        // Insert the overlay element
        document.querySelector('#oContainer').insertAdjacentHTML('afterbegin', `
            <div class='I131-container'>
                <div class='I131-close'>
                    <div>
                        <i class="fa fa-times" aria-hidden="true"></i>
                    </div>
                </div>
                <div class='I131-info'>
                  <h1>Sign up for My Account</h1>
                  <ul class='I131-list'>
                    <li><span>No booking fee</span></li>
                    <li><span>Faster checkout</span></li>
                    <li><span>Easily manage your booking online</span></li>
                  </ul>
                  <div class='I131-button-container'>
                    <div class='I131-button'>
                      <p>Sign up now</p>
                    </div>
                  </div>
                </div>
            </div>
        `);
  
        // Add eventlistener for I131 button to act as a link
        document.querySelector('.I131-button').addEventListener('click', () => {
            window.location.href = "https://book.nationalexpress.com/my-account/#/welcome";
        });
  
        // Function to hide My Account overlay
        document.querySelector('.I131-close > div > i').addEventListener('click', () => {
            const element = document.querySelector('.I131-container');
            element.style.transition = "opacity 150ms ease";
            element.style.opacity = 0;
            setTimeout(() => {
                element.remove();
            }, 150);
        });
        
         // On mobile devices 500px and less hide My Account overlay when user interacts with JP
        document.querySelectorAll('.plannerForm__inputBelt__field').forEach((field) => {
            field.addEventListener('click', function () {
                var mobMediaQuery = window.matchMedia("(max-height: 635px)");

                if (mobMediaQuery.matches) {
                    const element = document.querySelector('.I131-container');
                    element.style.transition = "opacity 150ms ease";
                    element.style.opacity = 0;
                    setTimeout(() => {
                        element.remove();
                    }, 150);
                }
            })
        });
        
        // Once done, write to local storage to indicate this campaign has been seen
        localStorage.setItem('I131seen', true);
      });
    }
    
  </script>

<!-- My Account Overlay END-->

