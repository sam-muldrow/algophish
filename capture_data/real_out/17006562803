<script>
let heroImg = document.querySelector(".hero__img img");

function setAttributes(el, attrs) {
  for(let key in attrs) {
    el.setAttribute(key, attrs[key]);
  }
}

setAttributes(heroImg , {"width": "1920", "height": "452"});

</script>
