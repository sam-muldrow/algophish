            <script>
                $.ajax({
                    url: 'https://datacenter.cngold.org/city_oil/all_price_by_year/',
                    success: function(res) {
                        if (res.returnCode === 0 && res.data && res.data.length) {
                            var list = res.data
                            __list = res.data
                            var count = 0
                            for (var i = 0; i < list.length; i++) {
                                var item = res.data[i]
                                for (var j = 0; j < 5; j++) {
                                    var el = $('.indexPrice tbody').children().eq(j + 1)
                                    var child = $(el).children().eq(2)
                                    if (!child) continue
                                    if ($(child).find('span').html() !== '-') {} else {
                                         var rid = $(child).find('span').attr('id') && $(child).find('span').attr('id').replace('n92_price_', '')
                                        if (item.regionId === +rid) {
                                            $('#n92_price_'+rid).html((_.isNil(item.n92) ? '-' : item.n92))
                                            $('#n95_price_'+rid).html((_.isNil(item.n95) ? '-' : item.n95))
                                            $('#n98_price_'+rid).html((_.isNil(item.n98) ? '-' : item.n98))
                                            $('#n0_price_'+rid).html((_.isNil(item.n0) ? '-' : item.n0))
                                            count++
                                        }
                                    }
                                }
                                if (count >= 5) break
                            }
                        }
                    }
                })
            </script>
            <div class="advLinkS mt8">
               <div class="jams" spm="1.0.1.1.18"></div>
               <div class="jams" spm="1.0.1.1.19"></div>
            </div>
        </div>
    </div>
    <div class="pageAdv mt12"><div class="jams" spm="1.0.1.1.20"></div></div>
