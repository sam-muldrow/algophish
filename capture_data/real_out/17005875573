                <script>
                function getGoldPriceSpan(param){
                    var url = "//www.cngold.org/sgapp/price/gold/pageData.do?currentPage=1&pageSize=1&brandId=" + param.brandId + "&variable=json";
                    if(param.productId){
                    	url = "//www.cngold.org/sgapp/price/gold/pageData.do?currentPage=1&pageSize=1&brandId=" + param.brandId + "&productId=" + param.productId + "&variable=json";
                    }
                    $.getScript(url, function() {
                        var data = json[0].data;
                        var brandName = "";
                        var productId = "";
                        var productName = "";
                        var updateTime = "";
                        var infos = data[0].infos;
                        for ( var j = 0; j < infos.length; j++) {
                            value = infos[j];
                            if (value.updateTime != null) {
                                updateTime = value.updateTime;
                                // updateTime=updateTime.substring(0,updateTime.indexOf(" "));
                            } else {
                                updateTime = "-";
                            }
                            if (productId != value.productId) {
                                productId = value.productId;
                                productName = value.productName;
                                brandName = value.brandName;
                            }
                            var price = "-";
                            var raiseDownType = "";
                            if (value.price && value.price != "null") {
                                price = value.price;
                                // alert(price);
                                price = parseFloat(price);
                                if (value.raiseDownType == 1) {
                                    raiseDownType = "<font color='red'>æ¶¨</font>";
                                    price = "<font color='red'>" + price + "</font>";
                                } else if (value.raiseDownType == 2) {
                                    raiseDownType = "<font color='green'>è·</font>";
                                    price = "<font color='green'>" + price + "</font>";
                                } else {
                                    raiseDownType = "<font color='gray'>å¹³</font>";
                                }
                            }
                        }
                        //$('#'+ param.tableId).html(result);
                        $('#'+ param.spanId + ' .priceTime').html(updateTime);
                        $('#'+ param.spanId + ' .priceNum').html(price);
                        $('#'+ param.spanId + ' .priceType').html(raiseDownType);
                    });
                }
                        getGoldPriceSpan({"brandId":2880,"spanId":"gold_price_span01","productId":57});
                        getGoldPriceSpan({"brandId":9819,"spanId":"gold_price_span02","productId":1265});
                        getGoldPriceSpan({"brandId":9704,"spanId":"gold_price_span03","productId":1157});
                </script>
