            var useAdBlock = 1;
        } else {
            var useAdBlock = 0;
        }
        $.ajax({
            url: "/usuario/registerAdBlock",
            type: "POST",
            data: {adBlock: useAdBlock}
        });
    </script>
    

<script type="text/javascript">$( document ).ready(function() {$('#busqueda').focus();_gaq.push(['_trackPageview','/Home']);});function loginModal(){$( "#UnsubscribeForm" ).dialog({modal: true,height:250,width:600,autoOpen: false,resizable: false,show: {effect: "blind",duration: 10},hide: {effect: "explode",duration: 1000},close: function(event, ui) {null;}});var url = '/Contenido/MostrarPopUpUnsubscribe';$('#UnsubscribeForm').parent().css({position:"fixed"});$('#UnsubscribeForm').load(url,0,function(){}).dialog("open").show();}function completeQue(){value = $.trim($('#busqueda').val());if (value.length < 3 && $('#frmBusqueda').attr('action')== '/searches') {/*$('#busqueda').val($.trim($('#busqueda').val()));*/$('#busqueda').val('USA');}}$("#frmBusqueda").submit(function(event){completeQue();});$(".selectDropDown li a").click(function(e){e.preventDefault();var dataText= $(this).text() + ' <span class="caret"></span>';$(".categorySearchText").each(function(index,val){$(this).html(dataText);});var dataValue= $(this).data('value');switch(dataValue){case 1:$('#divdonde').show();action = '/searches';placeholder = 'What job are you looking for?';$('.formGral').attr('action',action);$('.inputque').attr('placeholder',placeholder);break;case 2:$('#divdonde').hide();action = '/news';placeholder = "Tips/News";$('.formGral').attr('action',action);$('.inputque').attr('placeholder',placeholder);break;default:break;}});$('input#busqueda').keydown(function(e){if (e.keyCode==13){$('#frmBusqueda').submit();return false;}});$('input#donde').keydown(function(e) {if (e.keyCode==13){focusSelector('input[type=submit]#btn');}});$('input#btn, input#btnSearch').click(function() {completeQue();return true;});function focusSelector(selector){var donde = $(selector);donde.focus();}</script>
