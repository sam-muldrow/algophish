<section  class="wdgt-base wdgt wdgt-super-widget wdgt-theme-light" ><div class="wdgt-content"><div class="row" id="thumbnail-grid-block-2">
        <div class="col-tablet-6 col-desktop-4 col-large-desktop-3" data-item_id="47189">
        <div class="media-card mc-list" data-event-track="click:ab.super-wdgt-home.thumbnail-grid-block-2" data-href>
            <div class="media-card-image">
                <enx-image layout="responsive" aspect="1.2"><noscript class="enx-image-fallback">
