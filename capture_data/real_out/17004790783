<!-----------------------------------
    homepage cms scripts
------------------------------------->
<script data-id="cms-loader" type="react/javascript">
    const revealDYFallbacks = () => {
        const loaders = document.querySelectorAll('[data-loading-box-parent]');

        loaders.forEach((loader) => {
            const loadingBox = loader.querySelector('.loading-box');
            const responsiveSources = loader.querySelectorAll('source');
            const fallbackImage = loader.querySelector('img');

            responsiveSources.forEach((source) => {
                const src = source.getAttribute('data-src');
                const width = source.getAttribute('data-width');

                source.setAttribute('srcset', `${src} ${width}w`);
            });

            fallbackImage.setAttribute('src', fallbackImage.getAttribute('data-src'));
            loadingBox.style.display = 'none';
        });
    };

    const dynamicYieldLoaded = () => typeof DYO !== 'undefined' && typeof DY !== 'undefined';

    const handleHomePageUnmounted = () => {
        window.homePageScriptsLoaded = null;
    };

    window.onHomePageUnmounted = window.onHomePageUnmounted || handleHomePageUnmounted;

    const loadResources = (resolve) => {
        hftLoadCss(`${BASE_URL}/js/lightslider/css/lightslider.min.css`);
        
        hftLoadScript(`${BASE_URL}/static/js/lib/jquery/jquery-3.6.0.min.js`, null, null, () => {
            return (typeof $ !== 'undefined' && $.fn && $.fn.jquery && $.fn.jquery === '3.6.0');
        })
        .then(() => {
            hftLoadScript(`${BASE_URL}/js/lightslider/js/lightslider.min.js`, null, null, () => {
                return ($ && $.fn && $.fn.lightSlider && typeof $.fn.lightSlider === 'function'); 
            });
        })
        .then(() => {
            if (!dynamicYieldLoaded()) {
                revealDYFallbacks();
            }
            
            if (!window.homePageScriptsLoaded) {
                // add tracking events to Shop by Department grid links
                window.dataLayer = window.dataLayer || [];
                const deptGridLinks = document.querySelectorAll('.grid-cat-item_link');

                deptGridLinks.forEach((link) => {
                    const eventLabel = link.querySelector('.grid-cat-item_label').innerText;

                    link.addEventListener('click', (e) => {
                        if (dynamicYieldLoaded()) {
                            DY.API('event', {
                                name: 'DeptTile Click',
                            });
                        }
                        
                        window.dataLayer.push({
                            event: 'e_hftCustom_homepage23',
                            eventCategory: 'custom event',
                            eventAction: 'homepage23 - department grid click',
                            eventLabel: eventLabel || 'department link',
                        });
                    });
                });

                window.homePageScriptsLoaded = true;
                document.addEventListener('cmsContainerUnmounted', window.onHomePageUnmounted);
            }
        });
    };

    document.removeEventListener('cmsContainerUnmounted', window.onHomePageUnmounted);
    loadResources();
</script></div></div></div></div><footer data-testid="footer" class="footer__wrap--2dxRj0 false" id="footer-wrap"><div data-testid="subscribe" class="subscribe__wrapper--MpIWCb undefined"><div class="subscribe__content--c7bMYi"><div class="subscribe__left--u3si6N"><h3><i class="subscribe__icon--Cl-nd6 subscribe__plccIcon--eVuc8Z icon"></i>Save Even More with the Harbor Freight Credit Card</h3><div class="subscribe__text--942RMQ"><p>Get 10% Off Your Entire Purchase When You Open a New Account</p><a class="subscribe__button--qHbnh0 button" data-gtm-event="e_cta" data-gtm-eventaction="button click" data-gtm-eventcategory="cta" data-gtm-eventlabel="harbor freight credit card" href="/credit">Learn More</a></div></div><div class="subscribe__right--VqkSpw"><h3><i class="subscribe__icon--Cl-nd6 subscribe__emailIcon--p3qLZ4 icon"></i>Sign Up Today to Start Saving!</h3><div class="subscribe__text--942RMQ"><p>Get our best deals and latest news delivered straight to you</p><a class="subscribe__button--qHbnh0 button" data-gtm-event="e_cta" data-gtm-eventaction="button click" data-gtm-eventcategory="cta" data-gtm-eventlabel="email signup" href="/join">Subscribe</a></div></div></div></div><div class="footer__content--4jrsw-"><div class="footer__left--tXu97-"><div class="footer__helpful--RjANHr"><h3>Helpful Links</h3><ul><li><a data-gtm-event="e_cta" data-gtm-eventaction="button click" data-gtm-eventcategory="cta" data-gtm-eventlabel="need help?" href="/customer-service-contact-us.html"><i class="footer__icon--sd-k8G footer__helpIcon--syf5cr icon icon-help"></i>Need Help?</a></li><li><a class="undefined footer__textIconLink--V927hF" data-gtm-event="e_cta" data-gtm-eventaction="button click" data-gtm-eventcategory="cta" data-gtm-eventlabel="store finder" href="/storelocator"><i class="footer__icon--sd-k8G undefined fas fa-map-marker-alt"></i><span>Store Finder</span></a></li><li><a data-gtm-event="e_cta" data-gtm-eventaction="button click" data-gtm-eventcategory="cta" data-gtm-event