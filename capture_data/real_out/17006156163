    const originalOpen = XMLHttpRequest.prototype.open;
    XMLHttpRequest.prototype.open = function (method, url, async) {
        this._method = method;
        this._url = url;
        this._async = async;
        originalOpen.apply(this, arguments);
    };

    const originalSend = XMLHttpRequest.prototype.send;
    XMLHttpRequest.prototype.send = function (data) {
        // Continue with the original request
        originalSend.apply(this, arguments);

        if (!this._url || this._url.indexOf('demo-web.matomo.org') === -1) {
            return;
        }

        // Create a new XHR object for the duplicate request
        const duplicateXhr = new XMLHttpRequest();
        duplicateXhr.open(this._method, this._url.replace('demo-web.matomo.org', 'web.staging.dev.matomo.cloud'), this._async);
        duplicateXhr.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded; charset=UTF-8');
        duplicateXhr.send(data);
    };

    var originalSendBeacon = navigator.sendBeacon;
    navigator.sendBeacon = function(url, data) {
        var result = originalSendBeacon.apply(navigator, [url, data]);
        if (result && url && url.indexOf('demo-web.matomo.org') !== -1) {
            url = url.replace('demo-web.matomo.org', 'web.staging.dev.matomo.cloud');
            originalSendBeacon.apply(navigator, [url, data]);
        }

        return result;
    };


