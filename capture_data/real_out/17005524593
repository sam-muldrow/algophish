  width: calc((100% / var(--flex-items)) - ((var(--flex-items) - 1) * var(--gap) / var(--flex-items)));
}
@media (min-width: 600px) {
.ACategoryOverviewSlider--Vertical > li {
    --flex-items: 4;
}
}

/*** TYPOGRAPHY ***/
/*
You can have the same result without this mixin:
width: 30/16rem;

But some people like the mixins more:
.pxToRemCalc(30);
width: @pxToRemCalc;
*/
/*
You can have the same result without this mixin:
padding; 50/16rem 10/16rem 50/16rem 10/16rem;

But some people like the mixins more:
usage: .pxToRem(padding 50 10 50 10);
*/
