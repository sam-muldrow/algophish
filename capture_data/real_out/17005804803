<script>
function setCookie(e,o,s,i=!1){var c=escape(o);if(s){var t=new Date;i?t.setHours(t.getHours()+s):t.setDate(t.getDate()+s),c+="; expires="+t.toUTCString()}document.cookie=e+"="+c+"; path=/"}function getCookie(e){var o,s,i,c=document.cookie.split(";");for(o=0;o<c.length;o++)if(s=c[o].substr(0,c[o].indexOf("=")),i=c[o].substr(c[o].indexOf("=")+1),(s=s.replace(/^\s+|\s+$/g,""))===e)return unescape(i)}function closeDialog1(){setCookie("cpRej","yes",6,!0),setTimeout(function(){jQuery("#popup").addClass("close").removeClass("open")},500)}function privacyPolicyCookies(){"yes"!=getCookie("cpAcc")&&(setCookie("cpAcc","yes",365),setTimeout(function(){jQuery("#popup").removeClass("open").addClass("close")},1e3))}window.addEventListener("load",function(e){setTimeout(function(){jQuery(document).ready(function(){jQuery(".cPbtn-accept-cookies").click(function(){privacyPolicyCookies(),jQuery("#popup").removeClass("open").addClass("close")}),jQuery(".cPbtn-accept-cookies2").click(function(){jQuery("#CookiesSettings").removeClass("open").addClass("close")}),"yes"!=getCookie("cpAcc")&&jQuery("#popup").addClass("open").removeClass("close"),getCookie("cpRej")&&jQuery("#popup").addClass("close").removeClass("open"),jQuery(".cookies-close").on("click",function(e){$(".cookies-popup").remove()})}),jQuery('a[href="#CookiesSettings"]').on("click",function(e){e.preventDefault(),jQuery("#CookiesSettings").addClass("open"),jQuery("#CookiesSettings .white-box").focus()}),jQuery("#CookiesSettings, #CookiesSettings i.cookie-settings-close").on("click keyup",function(e){(e.target===this||"cookie-settings-close"===e.target.className||27===e.keyCode)&&jQuery(this).removeClass("open")})},2e3)});
