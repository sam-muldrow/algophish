<script>
// $( document ).ready(function() {
window.addEventListener('load', (event) => {
  $('.primary-nav #autoNav li').each(function() {
    if($.trim($(this).find('a').html())=='Arthritis &amp; Spine Care' || $.trim($(this).find('a').html())=='स्पाइनल अर्थराइटिस') {
      $(this).find('a').after('<ul class="loopin"><li><a href="#"><img src="https://st1.thehealthsite.com/assets/img/ipca.png" alt=""></a></li></ul>');
    }
    if($.trim($(this).find('a').html())=='Diabetes Mein Immunity') {	
      $(this).find('a').after('<ul class="loopin"><li><a href="https://zeenews.india.com/hindi/tags/diabetesmeinimmunityzaroori.html"><img src="https://st1.thehealthsite.com/assets/img/dabur-logo.jpeg" alt=""></a></li></ul>');
    } else if($.trim($(this).find('a').html())=='डायबिटीज़ में इम्यूनिटी') {
      $(this).find('a').after('<ul class="loopin"><li><a href="https://zeenews.india.com/hindi/tags/diabetesmeinimmunityzaroori.html"><img src="https://st1.thehealthsite.com/assets/img/dabur-hindi-logo.jpg" alt=""></a></li></ul>');
    } else if($.trim($(this).find('a').html())=='MY MONEY') {
      $(this).addClass('myMoney');
    } 
    if($.trim($(this).find('a').html())=='Health Summit &amp; Infra Awards') {
      $(this).addClass('health-summit');
    }
  });
});
</script>
<script type="text/javascript">
  window.addEventListener("load", function(event) {
  function create_UUID(){
  var dt = new Date().getTime();
  var UUID = 'xxxxxxxx-xxxx-8xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function(c) {
  var r = (dt + Math.random()*16)%16 | 0;
  dt = Math.floor(dt/16);
  return (c=='x' ? r :(r&0x3|0x8)).toString(16);
  });
  return UUID;
  }

  if(window.localStorage.getItem('ppid')=='' || window.localStorage.getItem('ppid')==null){
  UUID=create_UUID()
  window.localStorage.setItem('ppid', UUID);
  window.googletag = window.googletag || {cmd: []};
  googletag.pubads().setPublisherProvidedId(UUID);
  googletag.enableServices();
  }else{
  UUID=window.localStorage.getItem('ppid')
  window.googletag = window.googletag || {cmd: []};
  googletag.pubads().setPublisherProvidedId(UUID);
  googletag.enableServices();
  }
});
 
 </script>
 <!-- End PPID -->
