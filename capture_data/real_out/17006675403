<script>
	window.googletag = window.googletag || {cmd: []};

	googletag.cmd.push(function() {

		// For Leaderboard ad.
		const leaderboardMapping = googletag.sizeMapping()
			.addSize([0, 0], [[320, 50]])
			.addSize([800, 0], [[728, 90]])
			.addSize([1030, 0], [[970, 250], [970, 90], [728, 90]])
			.build();

		// For Sidebar Ads.
		const sidebarMapping = googletag.sizeMapping()
			//1x1 size included to prevent ad calls when too small to display.
			.addSize([0, 0], [[1, 1]])
			.addSize([800, 0], [[300, 250], [300, 600]])
			.build();

		// For Incontent Ads.
		const incontentMapping = googletag.sizeMapping()
			.addSize([0, 0], [[320, 50], [300, 250], 'fluid'])
			.addSize([800, 0], [[728, 90], [300, 250]])
			.build();

		// For footer ad.
		const footerMapping = googletag.sizeMapping()
		.addSize([0, 0], [[320, 50]])
		.addSize([800, 0], [[728, 90]])
		.addSize([1030, 0], [[970, 90], [728, 90]])
		.build();

		// GPT AdSlot 1 -- all templates.
		googletag.defineOutOfPageSlot('/4041849/auto-all-ads/interstitial', 'div-gpt-ad-interstitial')
			.setTargeting('position', ['interstitial'])
			.addService(googletag.pubads());
			googletag.pubads().enableSingleRequest();

		// GPT AdSlot 2 -- all templates.
		googletag.defineSlot('/4041849/auto-all-ads/lb_top', [[970,90],[970,250],[728,90],[320,50]], 'div-gpt-ad-leaderboard-top')
			.setTargeting('position', ['lb_top'])
			.defineSizeMapping(leaderboardMapping)
			.addService(googletag.pubads());

		// GPT AdSlot 7 -- all templates.
		googletag.defineSlot('/4041849/auto-all-ads/footer', [[970,90],[728,90],[320,50]], 'div-gpt-ad-footer')
			.setTargeting('position', ['footer'])
			.defineSizeMapping(footerMapping)
			.addService(googletag.pubads());

		
			// GPT AdSlot 5.
			googletag.defineSlot('/4041849/auto-all-ads/middle_1', [[320, 50],[300, 250],'fluid'], 'div-gpt-ad-middle-1')
				.setTargeting('position', ['middle_1'])
				.defineSizeMapping(incontentMapping)
				.addService(googletag.pubads());

			// GPT AdSlot 6.
			googletag.defineSlot('/4041849/auto-all-ads/middle_2', [[320, 50],[300, 250],'fluid'], 'div-gpt-ad-middle-2')
				.setTargeting('position', ['middle_2'])
				.defineSizeMapping(incontentMapping)
				.addService(googletag.pubads());

		
		
			// GPT AdSlot 3.
			googletag.defineSlot('/4041849/auto-all-ads/side_1', [[300,250],[300, 600]], 'div-gpt-ad-side-1')
				.setTargeting('position', ['side_1'])
				.defineSizeMapping(sidebarMapping)
				.addService(googletag.pubads());

			// GPT AdSlot 4.
			googletag.defineSlot('/4041849/auto-all-ads/side_2', [[300,250],[300, 600]], 'div-gpt-ad-side-2')
				.setTargeting('position', ['side_2'])
				.defineSizeMapping(sidebarMapping)
				.addService(googletag.pubads());

			// Sidebar video ads.
			googletag.defineSlot('/4041849/auto-all-ads/Autostraddle_Sidebar_Video_Player', [1, 2], 'div-gpt-ad-1679586171037-0')
				.addService(googletag.pubads());

		
		googletag.pubads()
			.setTargeting('url', ['www.autostraddle.com'])
			.setTargeting('a-plus', ['no'])
			.setTargeting('brandsafe', ['yes'])
			.setTargeting('tag', [''])
			.addEventListener( 'slotOnload', function( e ) {
				const slot = e.slot.getSlotElementId();

				// Make ads sticky once loaded.
				if ( 'div-gpt-ad-leaderboard-top' === slot ) {
					stickLeaderboardAd();
				} else if ( 'div-gpt-ad-footer' === slot ) {
					stickFooterAd();
				}
			});

		googletag.pubads().enableLazyLoad( {
			// Fetch slots within x viewports.
			fetchMarginPercent: 500,
			// Render slots within x viewports.
			renderMarginPercent: 200,
			// Alter the above values on mobile.
			mobileScaling: 2,
		} );

		googletag.enableServices();
	});

	/**
	 * Stick leaderboard ad to top of screen.
	 *
	 * @return void
	 */
	function stickLeaderboardAd() {
		const ad = jQuery( '#div-gpt-ad-leaderboard-top' );
		const stickyWrap = jQuery( '#masthead .sticky-wrap' );
		const padding = jQuery( '#masthead .adbar-padding' );

		// Ensure leaderboard ad is present.
		if ( ! ad ) {
			return;
		}

		const stickyClass = 'ad-sticky';
		let shown = false;

		ad.addClass( 'ad-filled' );

		// Listen to scroll event to make ad sticky.
		window.addEventListener(
			'scroll',
			_.throttle(
				function( e ) {
					const scrollTop = window.scrollY !== undefined ? window.scrollY : 0;
					const offsetAd = padding[0].offsetTop;

					if ( scrollTop > offsetAd ) {
						if ( shown ) {
							return;
						}

						stickyWrap.addClass( stickyClass );
						padding.addClass( stickyClass );
						shown = true;

						setTimeout(
							function() {
								stickyWrap.removeClass( stickyClass );
								padding.removeClass( stickyClass );
							},
							3000
						);
					} else {
						stickyWrap.removeClass( stickyClass );
						padding.removeClass( stickyClass );
					}
				},
				300
			),
			{ passive: true }
		);
	}

	/**
	 * Stick footer ad to top of screen.
	 *
	 * @return void
	 */
	function stickFooterAd() {
		const ad = jQuery( '#div-gpt-ad-footer' );
		const body = jQuery( 'body' );
		const stickyClass = 'ad-sticky';
		const closeButton = jQuery( '.hide-ad-footer' );

		// Ensure footer ad is present.
		if ( ! ad ) {
			return;
		}

		ad.addClass( stickyClass );
		body.addClass( 'has-footer-ad' );

		const adIframe = ad.find( 'iframe' );

		if ( ! adIframe?.length ) {
			return;
		}

		// Position close button.
		const width = adIframe[0].width;
		const margin = ( width / 2 ) - 30;

		adIframe[0].after( closeButton[0] );
		closeButton.css( 'margin-left', margin + 'px' );
		closeButton.addClass( 'display' );

		// Enable hiding ad.
		closeButton.on( 'click', function( e ) {
			ad.hide();
			closeButton.hide();
		} );
	}
</script>
