    <script>
        //è·åè®¾å¤
        function viewport() {
            var e = window, a = 'inner';
            if (!('innerWidth' in window)) {
                a = 'client';
                e = document.documentElement || document.body;
            }
            return { width: e[a + 'Width'], height: e[a + 'Height'] };
        }
        //è·åç¾åº¦/åé¾ç»è®¡ä¼ å
        var bdUrl = {
            str: 'https://qlstats.bjnews.com.cn/hm.gif?t=qianlong',
            cl: screen.colorDepth + '-bit',
            ds: viewport().width + 'x' + viewport().height,
            ln: String(navigator.language || navigator.userLanguage).toLowerCase(),
            u: encodeURIComponent(document.location.href),
            getbdURL: function () {
                this.bdUrl = this.str + '&cl=' + this.cl + '&ds=' + this.ds + '&ln=' + this.ln + '&u=' + this.u;
                console.log(this.bdUrl);
                return this.bdUrl;
            }
        }
        // bdUrl.getbdURL(); 
        var aurl = bdUrl.getbdURL();
        $.ajax({
            url: aurl,
            type: "get",
            success: function (req) {
                console.log(req)
            },
            complete: function () {
                //è¯·æ±å®æçå¤ç
            },
            error: function () {
                //è¯·æ±åºéå¤ç
                console.log('error')
            }
        })

        
    </script>
