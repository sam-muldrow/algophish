var adIsInViewport = function (el) {
const { top, bottom } = el.parentElement.getBoundingClientRect();
const vHeight = (window.innerHeight || document.documentElement.clientHeight);
return ((top > 0 || bottom > 0) && top < vHeight);
};
window.lazyLoad = function () {
var ads = document.getElementsByClassName("advert-lazyload");
for (var i = 0; i < ads.length; i++) {
var lazyAdvert = ads.item(i);
if (adIsInViewport(lazyAdvert)) {
var ad_slot = lazyAdvert.getAttribute('data-cb-ad-id');
lazyAdvert.getAttribute('data-cb-ad-id', ad_slot + '_loaded');
googletag.cmd.push(function () {
googletag.pubads().refresh([window.DG.ads[ad_slot]]);
});
}
}
}
