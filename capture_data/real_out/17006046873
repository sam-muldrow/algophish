__tcfapi('addEventListener', 2, function (tcData, success) {
if (success && tcData.eventStatus === 'useractioncomplete' || tcData.eventStatus === 'tcloaded') {
__tcfapi('getTCData', 2, (tcData, success) => {
if (success) {
doads();
}
else {
