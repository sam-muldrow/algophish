<script>

	
	var category = 'All';
	

	$('#topic-tabs a').click(function(e){
		e.preventDefault();
		$('li a.pro').unbind();
		$('#topic-tabs a').removeClass('selected');
		$(this).addClass('selected');
		category = $(this).text();
		if(category == 'All') {
			$(".accounting-topics li.accounting-topic").attr('data-display', 'true').show();
		}
		else {
			$(".accounting-topics li.accounting-topic").attr('data-display', 'false').hide();
			$(".accounting-topics li.accounting-topic").each(function(){
			    if($(this)[0].hasAttribute('data-category')) {
                    if ($(this).attr('data-category').indexOf(category) >= 0) $(this).show().attr('data-display', 'true');
                }
			});
			//$(".accounting-topics li.accounting-topic[data-category='"+category+"']").show().attr('data-display', 'true');
		}
		$('.current-topic-title').text(category + ' Topics');
		sortTopics();
	});
	$('.sort-topics a').click(function(e){
		e.preventDefault();
		$('.sort-topics a').removeClass('current');
		$(this).addClass('current');
		order = ($(this).text() == 'Alphabetical') ? 'alphabetical' : 'recommended';
		sortTopics();
		setDefaultSort(order, '0');
	});
	function sortTopics() {
		$container = $('.accounting-topics');
		var listItems;
		if(order == 'recommended') {
			$container.removeClass('alphabetical');
			var attr = (category == 'All') ? 'data-overall-position' : 'data-'+category.toLowerCase()+'-position';
			listItems = $container.find('li.accounting-topic[data-display="true"]').sort(function(a,b){ return $(a).attr(attr) - $(b).attr(attr); });

			listItems.find('span.list-order-number').remove();
			listItems.each(function(key, value) {
				var list_number = (key+1 < 10) ? '0'+(key+1) : key+1;
				$(this).find('a.button').prepend('<span class="list-order-number">'+list_number+'. </span>');
			});
		}
		else {
			$container.addClass('alphabetical');
			listItems = $container.find('li.accounting-topic[data-display="true"]').sort(function(a, b) { return $(a).attr('data-name').toUpperCase().localeCompare($(b).attr('data-name').toUpperCase()); });
			listItems.find('span.list-order-number').remove();
		}
		hiddenItems = $container.find('li.accounting-topic[data-display="false"]');
		var gluedItems = $container.find('li.accounting-topic[data-glue="true"]');
		$container.find('li.accounting-topic').remove();
		$container = $('.topic-column .accounting-topics');
		listItems.each(function(index){
			$container.append($(this));
		})
        gluedItems.show();
        $container.append(gluedItems);
		$container.append(hiddenItems);

	}

	if($('.topic-column.needs-sorting').length) {
		sortTopics();
		$('.topic-column.needs-sorting').show();
	}
</script>
