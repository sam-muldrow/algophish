<script>
function objectId() {
    let object_id = "";
    dataLayer.forEach((element, index) => {
        if (typeof element["objectId"] !== "undefined" && element["objectId"]) {
            object_id = element["objectId"];
        }
    });
    return object_id;
}

let object_id = objectId();
let domain = window.location.hostname.replace('www.','').replaceAll(".","-");
let upscorePixel = document.getElementById("upscore_pixel");
let url = window.location.href;
if (upscorePixel && url.search("staging") === -1  && url.search("dev") === -1){
    upscorePixel.setAttribute("src",`https://tp.content-score.com/${domain}.gif?id=${object_id}&u=${url}`);
}

</script>


<script>
const cleanedDomain = window.location.host.split('.').filter(x => !['dev', 'staging', 'turbo', 'www'].includes(x) ).join('.');
if (!['haleprzemyslowe.muratorplus.pl'].includes(cleanedDomain)) {
    function addUpscoreParameter(data, dataParam, param, defaultValue=null) {
        if (defaultValue !== null) {
            data[dataParam] = defaultValue;
        }
        dataLayer.forEach((element, index) => {
                if (typeof element[param] !== "undefined" && element[param]) {
                data[dataParam] = element[param];
            }
        });
    }

    function isDynamicPage() {
        let dynamic = false;
        dataLayer.forEach((element, index) => {
            if (typeof element["dynamic"] !== "undefined" && element["dynamic"]) {
                dynamic = true;
            }
        });
        return dynamic;
    }

    function isUpscorePageType(pageType) {
        const upscorePageTypes = ["article", "gallery", "quiz", "recipe"];
        return upscorePageTypes.includes(pageType);
    }

    function addUpscoreArticleAndTrackPositionsParameters() {
        const mainContentTag = {
            "article": "article",
            "gallery": "galleries",
            "quiz": "galleries",
            "recipe": "article",
        }
        dataLayer.forEach((element, index) => {
            const pageType = element["pageType"];
            if (typeof pageType !== "undefined" && pageType) {
                upscoreConfig.article =  isUpscorePageType(pageType) ? mainContentTag[pageType] : "";
                if (!isDynamicPage()) {
                    upscoreConfig.track_positions = !isUpscorePageType(pageType);
                }
            }
        });
    }

    let upscoreConfig = {};

    if(typeof upScore !== "function") {
      (function(u,p,s,c,r){u[r]=u[r]||function(p){(u[r].q=u[r].q||[]).push(p)},u[r].ls=1*new Date();
      var a=p.createElement(s),m=p.getElementsByTagName(s)[0];a.async=1;a.src=c;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//files.upscore.com/async/upScore.js','upScore');

      addUpscoreParameter(upscoreConfig, 'domain', 'domain');
      addUpscoreArticleAndTrackPositionsParameters();
      upScore({config: upscoreConfig});
    }
    let upscoreData = {};

    addUpscoreParameter(upscoreData, 'section', 'section');
    addUpscoreParameter(upscoreData, 'custom_adsoff_status', 'adsOffStatus', 'off');
    addUpscoreParameter(upscoreData, 'custom_adblock_status', 'adblockStatus');

    if (isDynamicPage()) {
        addUpscoreParameter(upscoreData, 'url', 'url');
        addUpscoreParameter(upscoreData, 'custom_page_dynamic', 'dynamic');
    }

    /* common */
    addUpscoreParameter(upscoreData, 'custom_page_type', 'pageType');
    addUpscoreParameter(upscoreData, 'object_id', 'objectId', "");
    addUpscoreParameter(upscoreData, 'pubdate', 'pubdate', "");
    addUpscoreParameter(upscoreData, 'author', 'author', "");
    addUpscoreParameter(upscoreData, 'object_type', 'objectType', "landingpage");
    addUpscoreParameter(upscoreData, 'custom_author_id', 'authorId');
    addUpscoreParameter(upscoreData, 'custom_object_title', 'title');
    addUpscoreParameter(upscoreData, 'custom_created_date', 'createdDate');
    addUpscoreParameter(upscoreData, 'custom_edit_date', 'editDate');
    addUpscoreParameter(upscoreData, 'custom_zpr_shopping', 'zprShopping', 'off');
    addUpscoreParameter(upscoreData, 'custom_adult', 'adult', 'off');
    addUpscoreParameter(upscoreData, 'custom_container', 'container');

    /* quiz */
    addUpscoreParameter(upscoreData, 'custom_option', 'customOption');
    addUpscoreParameter(upscoreData, 'custom_quiz_type', 'quizType');
    addUpscoreParameter(upscoreData, 'custom_question_count', 'quizQuestionCount');

    /* tag */
    addUpscoreParameter(upscoreData, 'custom_tag_name', 'tagName');
    addUpscoreParameter(upscoreData, 'custom_tag_service', 'tagService');

    /* article */
    addUpscoreParameter(upscoreData, 'custom_tags', 'articleTags');
    addUpscoreParameter(upscoreData, 'custom_feed_count', 'feedCount');
    addUpscoreParameter(upscoreData, 'custom_block_count', 'articleBlockCount');
    addUpscoreParameter(upscoreData, 'custom_block_video_on', 'blockVideoOn');
    addUpscoreParameter(upscoreData, 'custom_block_gallery_on', 'blockGalleryOn');
    addUpscoreParameter(upscoreData, 'custom_block_quiz_on', 'blockQuizOn');
    addUpscoreParameter(upscoreData, 'custom_special_mark', 'articleSpecialMark');
    addUpscoreParameter(upscoreData, 'custom_container_add', 'articleContainerAdd');
    addUpscoreParameter(upscoreData, 'custom_sponsor', 'articleSponsor');
    addUpscoreParameter(upscoreData, 'custom_sponsor_type', 'sponsorType');
    addUpscoreParameter(upscoreData, 'custom_orginal_object_id', 'objectId');
    addUpscoreParameter(upscoreData, 'custom_opening_component', 'openingComponent');
    addUpscoreParameter(upscoreData, 'custom_template', 'template');
    addUpscoreParameter(upscoreData, 'custom_publish', 'publish');

    /* gallery */
    addUpscoreParameter(upscoreData, 'custom_photo_count', 'galleryPhotoCount');
    addUpscoreParameter(upscoreData, 'custom_gallery_type', 'galleryType');

    upScore({data: upscoreData});
}
</script>


