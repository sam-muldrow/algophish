    <script type="text/javascript">



        $(document).ready(function () {

            if ($("#sectionAviso") != null) {

                setTimeout(function () {
                    $("#sectionAviso").slideDown(1000);
                }, 3);

                $("#lblFecharAviso").on('click', function (ev) {
                    setTimeout(function () {
                        $("#sectionAviso").fadeOut(1000, function () {
                            $("#sectionAviso").remove();
                        });

                    }, 2);
                });
            }


            $("#Processo").retringirSomenteNumeros();
            $("#Processo").bind('paste', function () {
                limparCaracteresEspeciais(this);
            });



            $('#TipoPesquisa').change(function (e) {

                var value = $(this).val();
                if (value == 4) {
                    $('#Processo').removeClass('onlynumber')

                } else {
                    $('#Processo').addClass('onlynumber')
                }
            });


            //Carousel Noticias
            var clickEvent = false;
            $('#myCarousel')
                .carousel({
                    interval: 4000
                })
                .on('click', '.list-group li', function () {
                    clickEvent = true;
                    $('.list-group li').removeClass('active');
                    $(this).addClass('active');
                })
                .on('slid.bs.carousel', function (e) {
                    if (!clickEvent) {
                        var count = $('.list-group').children().length - 1;
                        var current = $('.list-group li.active');
                        current.removeClass('active').next().addClass('active');
                        var id = parseInt(current.data('slide-to'));
                        if (count == id) {
                            $('.list-group li').first().addClass('active');
                        }
                    }
                    clickEvent = false;
                });
            //Fim Carousel Noticias

        });

        $(window).on('load resize', function () {
            var boxheight = $('#myCarousel .carousel-inner').innerHeight();
            var itemlength = $('#myCarousel .item').length;
            var triggerheight = Math.round(boxheight / itemlength + 1);
            $('#myCarousel .list-group-item').outerHeight(triggerheight);
        });

        //Eventos Ajax Consulta Processos *********************************************************
        function Begin() {
            $('.field-validation-valid').html("");
            $('#ConsultarProcessoButton').addClass('show-loading');

        }
        function Complete() {
            $('#ConsultarProcessoButton').removeClass('show-loading');
        }
        function Success(data) {

            var _html = "";
            //debugger;
            if (data.Message == null) {
                if (data.length == 1) {
                    window.open(data[0].Url, '_blank');
                }
                else {
                    $(data).each(function (key) {
                        $('#sectionProcessos').append("<a class='col-xs-12 col-sm-6 col-md-4 tj-mtop-30px btnConsultaProcessos' target='_blank' href='"
                            + data[key].Url +
                            "'><div class='tj-panel-procBt-btns'>"
                            + data[key].TipoPesquisa +
                            "</div></a>");
                    });
                    getPainelMultiploProcessos();
                }
            } else { $('.field-validation-valid').html(data.Message); }
        }
        function Failure(data) {
            //alert("error");
            //$(".msg").show(300);
            //$(".msg").addClass("alert-danger");
            //$(".msg").html(data.Status);
        }


        //Util
        function getPainelMultiploProcessos() {

            if ($("#sectionProcessos") != null) {
                setTimeout(function () {
                    $("#sectionProcessos").slideDown(1000);
                }, 3);



                $("#lblFecharProcesso").on('click', function (ev) {
                    setTimeout(function () {
                        $("#sectionProcessos").fadeOut(1000);
                        $('.btnConsultaProcessos').remove();

                    }, 2);
                });
            }

        }


                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                        //************************************************************

    </script>

    

    <script src="/Scripts/LayoutScript.js"></script>
    <script type="text/javascript">

        $(document).ready(function () {

            //Criar nova url para o link
            $(".linkDinamico").on('click', function (ev) {
                window.open(criarLink($(this)));
                ev.preventDefault();
            });




            if (jQuery(".UrlExternaTJSP") != null) {
                jQuery(".UrlExternaTJSP").on('click', function (ev) {
                    window.open($(this).attr("href"));
                    ev.preventDefault();
                });
            }


            GetSearchVert();


            //Scripts para Busca Local - Validações e aviso
            $("#buscadorLocal").on("blur", function () {

                $("#Msg").text("");

                if ($(this).val().length < 3) {
                    $("#Msg").text("A busca deve conter mais de 3 caracteres");
                }

                if ($(this).val().length == 0) {
                    $("#Msg").text("");
                }
            });

            $("#btnbuscadorLocal").on("click", function (e) {
                if ($("#buscadorLocal").val().length < 3) {
                    $("#Msg").text("A busca deve conter mais de 3 caracteres");
                    e.preventDefault();
                }
            });



            $('[data-toggle="tooltip"]').tooltip();
            $('[data-toggle="popover"]').popover();

            //*************************************************


        });
    </script>

    <script>
        //Cookie Disclaimer LGPD
        $(document).ready(function () {
            checkCookie();
        });


        function setCookie() {
            var d = new Date();
            var exdays = 7;
            var cname = "username";
            var cvalue = "tjsp";

            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));

            //d.setTime(d.getTime() + (exdays * 1 * 1 * 60 * 1000));

            var expires = "expires=" + d.toUTCString();
            //alert(expires);
            document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
            document.querySelector(".box-cookies").classList.add('hide');
        }



        function getCookie(cname) {
            var name = cname + "=";
            var decodedCookie = decodeURIComponent(document.cookie);

            var ca = decodedCookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return "";
        }


        function checkCookie() {
            var user = getCookie("username");
            if (user == "tjsp") {
            } else {
                user = "tjsp";
                if (user != "" && user != null) {
                    document.querySelector(".box-cookies").classList.remove('hide');
                }
            }
        }
    </script>

    <script>
        //Disclaimer Agendamento
        function addHide(id, classe) {
            var elemento = document.getElementById(id);
            var classes = elemento.className.split(' ');
            var getIndex = classes.indexOf(classe);

            if (getIndex === -1) {
                classes.push(classe);
                elemento.className = classes.join(' ');
            }
        }

    </script>




    
