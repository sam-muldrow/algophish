            // ajustando comportamento dos treeviews ao VLibras
            // (modifica itens de lista que abrem/fecham subitens)
            let treeviewMostrarLista = document.querySelectorAll('.mostrarLista');
            Array.prototype.slice.call(treeviewMostrarLista).forEach(item => {
                let span = document.createElement('span');
                let label = document.createTextNode(item.innerText + ' ');
                span.appendChild(label);
                item.parentNode.insertBefore(span, item);
                item.innerHTML = '<i class="fa fa-chevron-down"></i>';
            })

            // carrega VLibras
            new window.VLibras.Widget('https://vlibras.gov.br/app');
        </script>




