  function isElementInDOM(selector, all=false)
  {
    if(!all)
    {
      if(!document.querySelector(selector))
        return false;
      else
        return true;
    }
    else
    {
      if(document.querySelectorAll(selector).length == 0)
        return false;
      else
        return true;
    }
  }
  
