<script>
            window.onload = function () {
                google.accounts.id.initialize({
                    client_id: '384915369683-psls543dnqj4uq6v25846g4206i13j0e.apps.googleusercontent.com',
                    callback: handleCredentialResponse
                });
                // google.accounts.id.prompt();
                google.accounts.id.prompt((notification) => {
                    if (notification.isNotDisplayed() || notification.isSkippedMoment()) {
                        // continue with another identity provider.
                    }
                });

                function handleCredentialResponse(e) {
                    var result = parseJwt(e.credential);
                    $.post("/Registration/Login/ExternalLoginWithGoogleOneTap", { tapAttributes: result }).done(function (e) {
                        window.location.href = "/Registration/Login/RedirectToAdvertiser";
                    });
                    setTimeout(window.location.href = "/Registration/Login/RedirectToAdvertiser",3000);
                }

                function parseJwt(token) {
                    var base64Url = token.split('.')[1];
                    var base64 = base64Url.replace(/-/g, '+').replace(/_/g, '/');
                    var jsonPayload = decodeURIComponent(atob(base64).split('').map(function (c) {
                        return '%' + ('00' + c.charCodeAt(0).toString(16)).slice(-2);
                    }).join(''));
                    return JSON.parse(jsonPayload);
                };
            };
        </script>

