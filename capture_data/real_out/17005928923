<script type="text/javascript">
    var applicationServerPublicKey = 'BHWhiWB8acwJQyTtdxEvGxtwW-LhqYEFfAjbMIegSZh1HzuFoWnRJVUSNAV7HjVQoMaflFOyA8XTC_rLXiR8P2I';
    var serviceWorker = '/service-worker.js';

    //navigator.serviceWorker.ready.then(function (reg) {
    //    console.log("hello");
    //    console.log(reg.pushManager.getSubscription());
    //    reg.pushManager.getSubscription();
    //});

    $(document).ready(function () {
        if (typeof applicationServerPublicKey === 'undefined') {
            return;
        }

        Notification.requestPermission().then(function (status) {
            if (status === 'granted') {
                initialiseServiceWorker();
            }
            if (status === 'denied') {
                unregisterServiceWorker();
            }
        });
    });

    function initialiseServiceWorker() {
        if ('serviceWorker' in navigator) {
            navigator.serviceWorker.register(serviceWorker).then(handleSWRegistration);
        }
    };

    function handleSWRegistration(reg) {
        if (reg.installing) {
            saveSubscribe();
        }

        initialiseState(reg);
    }

    function initialiseState(reg) {
        if (!(reg.showNotification)) {     
            return;
        }

        if (!('PushManager' in window)) {
            return;
        }

        navigator.serviceWorker.ready.then(function (reg) {
            reg.pushManager.getSubscription();
        });
    }

    function saveSubscribe() {
        navigator.serviceWorker.ready.then(function (reg) {

            var subscribeParams = { userVisibleOnly: true };

            var applicationServerKey = urlB64ToUint8Array(applicationServerPublicKey);

            subscribeParams.applicationServerKey = applicationServerKey;

            reg.pushManager.subscribe(subscribeParams)
                .then(function (subscription) {  
                    const SERVER_URL = '/mk/Shared/CreateBrowserInfo'; 

                    var p256dh = base64Encode(subscription.getKey('p256dh'));

                    var auth = base64Encode(subscription.getKey('auth'));

                    $.post(SERVER_URL, { PushEndpoint: subscription.endpoint, PushP256DH: p256dh, PushAuth: auth });
                })
                .catch(function (e) {
                    console.log('[subscribe] Unable to subscribe to push', e);
                });
        });
    }


    function urlB64ToUint8Array(base64String) {
        var padding = '='.repeat((4 - base64String.length % 4) % 4);
        var base64 = (base64String + padding)
            .replace(/\-/g, '+')
            .replace(/_/g, '/');

        var rawData = window.atob(base64);
        var outputArray = new Uint8Array(rawData.length);

        for (var i = 0; i < rawData.length; ++i) {
            outputArray[i] = rawData.charCodeAt(i);
        }
        return outputArray;
    }

    function base64Encode(arrayBuffer) {
        return btoa(String.fromCharCode.apply(null, new Uint8Array(arrayBuffer)));
    }

    function unregisterServiceWorker() {
        navigator.serviceWorker.getRegistrations().then(function (registrations) {
            for (let registration of registrations) {
                registration.unregister();
            }
        });
    }

</script>
