        var model = {"IsAuthorized":false,"Name":null,"Id":"","DisplayTopNotification":true,"ChatResponse":{"IsMobile":false,"IdAdvertiser":0,"IsAuthorized":false,"QueueId":"c6af3545-75ee-4e16-a88e-83d972c94cfc","IsSoundEnabled":false,"UnreadedCount":"","UserName":null,"UserFirstLatter":null,"UserImgTitle":null,"UserImgDate":null},"SubscriptionPlanTypeEnum":0,"SubscriptionPlanRemainingDays":0,"PlanId":0,"IsTrusted":true,"IsSubscriptionEnabled":false};
        var IsStoreLocked = 'False' + "";
         var IsAuthorized = 'False' + "";
        var IsSubscriptionEnabled = 'False' + "";
       var PlanId = '0' + "";
        if (IsSubscriptionEnabled.toLowerCase() == "true" && (model.IsTrusted || IsStoreLocked.toLowerCase() == "true"))
            $('body').addClass("vip-user");
        else
            $('body').removeClass("vip-user");
        if (IsSubscriptionEnabled.toLowerCase() == "true" && !IsAutoRenewLoad) {
            loadAutoRenewPaymentFailedSubscription();
        }
        if (IsSubscriptionEnabled.toLowerCase() == "true" && !IsAutoRenewEnabled) {
            loadAutoRenewPaymentSubscriptionIsEnabled();
        }
        if (IsAuthorized.toLowerCase() == "true" && IsSubscriptionEnabled.toLowerCase() == "true" && PlanId == "0" && '0' == 'Starter') {
          var newDate = new Date().toDateString();
            let cookieName = "pazar3promotionevent";
            var x = getCookie(cookieName);
            if (x) {
                const dateString = x.toString(); // Assuming this is the formatted date string
                const cookieDate = new Date(dateString);
                if (cookieDate.toDateString() < newDate) {
                    loadPromotion(cookieName, newDate);
                   // setTimeout(loadPromotion, 600000, cookieName, newDate);
                }
            }
            else {
                if (!IsLoad) {
                    //setTimeout(loadPromotion, 600000, cookieName, newDate);
                    loadPromotion(cookieName, newDate);
                }
            }

             function loadPromotion(cookieName, newDate) {
                $.ajax({
                    url: "/mk/Subscription/PromotionNotification",
                    type: "post",
                    dataType: "html",
                    async: false,
                    success: function (data) {
                        IsLoad = true;
                        setTimeout(function () {
                            $('body').append(data)
                        }, 10000)
                        setCookie(cookieName, newDate, 7);
                    },
                    error: function (err) {
                    }
                });
             }


        }

    });
     function loadAutoRenewPaymentFailedSubscription() {
                $.ajax({
                    url: "/mk/Subscription/AutoRenewPaymentFailed",
                    type: "post",
                    dataType: "json",
                    async: false,
                    success: function (data) {
                        if (data.html != "") {
                            $('body').append(data.html);
                        }
                        IsAutoRenewLoad = true;
                    },
                    error: function (err) {
                    }
                });
        }

        function loadAutoRenewPaymentSubscriptionIsEnabled() {
                $.ajax({
                    url: "/mk/Subscription/AutoRenewalSubscriptionIsEnabled",
                    type: "post",
                    dataType: "json",
                    async: false,
                    success: function (data) {
                        if (data.html != "") {
                            $('body').append(data.html);
                        }
                        IsAutoRenewEnabled = true;
                    },
                    error: function (err) {
                    }
                });
            }
    function setCookie(name, value, days) {
        var expires = "";
        if (days) {
            var date = new Date();
            date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
            expires = "; expires=" + date.toUTCString();
        }
        document.cookie = name + "=" + (value || "") + expires + "; path=/";
    }
    function getCookie(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') c = c.substring(1, c.length);
            if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
        }
        return null;
    }
    function eraseCookie(name) {
        document.cookie = name + '=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;';
    }

</script>
