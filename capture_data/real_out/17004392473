            <script>
        try {
            var userPrefersDark = window.matchMedia && window.matchMedia('(prefers-color-scheme: dark)').matches;
            var userSetting = window.localStorage.getItem('zdf_use_darkmode');
            var useDarkMode = false;

            if (userSetting === 'true') {
                useDarkMode = true;
            }
            else if (userSetting === 'false') {
                useDarkMode = false;
            }
            else if (userPrefersDark) {
                useDarkMode = true;
            }

            if (useDarkMode && window.zdfsite.enableDarkMode) {
                document.body.classList.add('is-darkmode');
            }
        } catch (error) {
            // eslint-disable-next-line no-console
            console.warn('could not init darkmode setting:', error);
        }
    </script>
