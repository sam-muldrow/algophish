    <script>
        function calculateSyntheticProfileValueForSearchBar(event) {
            const profileInput = document.querySelector("input[name='syntheticProfile']");
            profileInput.value =  window.innerWidth < 640 ? "small" : "large";
            return true;
        }
    </script>

