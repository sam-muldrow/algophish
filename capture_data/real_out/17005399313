  function getScrOrientation() {
    return (window.innerHeight > window.innerWidth) ? 'portrait' : 'landscape';
  }

  if (isMobileOrTablet()) {
    var scrOrientation = getScrOrientation();

    window.addEventListener('load', function() {
      GAManager.ga('send', 'event', 'Orientation', 'load', scrOrientation, {'nonInteraction':1});
    });

    window.addEventListener('resize', function() {
      if (scrOrientation != getScrOrientation()) {
        scrOrientation = getScrOrientation();
        GAManager.ga('send', 'event', 'Orientation', 'resize', scrOrientation, {'nonInteraction':1});
      }
    });
  }
