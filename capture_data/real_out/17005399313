<script>
/*
 * This script loads all the GPT ads present in the page
 */
(function (win) {
    var  $ = win.jQuery,
        // Google Publisher Tag API object
        // https://support.google.com/dfp_premium/answer/1650154?hl=en
        maxTries = 20,
        curTry = 0,
        googletag = win.googletag;
    /*
     * This functions loads just the ads that exist on the page
     */
    function loadGPTAds() {
        // loading the ads in one single request
        if (typeof(googletag) == 'object' && typeof(googletag.pubads) == 'function') {
            var ads = win.gptAds;
            var adsPresent = [];
            $.each(ads, function (key, value) {
                if ($('div.gpt-ad[data-id="' + key + '"]').length > 0) {
                    // this line is needed for the refresh method to work correctly
                    console.log('[GPT DEBUG] googletag.display(' + key + ')');
                    googletag.display(key);
                    adsPresent.push(value);
                }
            });
            console.log('[GPT DEBUG] googletag.refresh()', adsPresent);
            googletag.pubads().refresh(adsPresent);

            if (adsPresent.indexOf(ads['gpt_320x50'])>-1) {
                setInterval(function(){
                    googletag.pubads().refresh([ads['gpt_320x50']]);
                    console.log('[GPT DEBUG] googletag.refresh()', ads['gpt_320x50']);
                }, 30000);
            }
        } else if (curTry++ < maxTries) {
            setTimeout(loadGPTAds, 500);
        }
    }

    $(function () {
        loadGPTAds();
    });

}(window));
</script>

</script>
