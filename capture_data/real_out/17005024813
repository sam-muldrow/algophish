(function () {
  var timeout;
  var attempts = 0;
  var MAX_ATTEMPTS = 5;
  var TIMEOUT_MS = 500;

  function getButton() {
    return document.getElementsByClassName('usabilla_live_button_container')[0];
  }

  function removeTabIndex() {
    var btn = getButton();

    if (btn) {
      if (timeout) {
        window.clearTimeout(timeout);
        timeout = undefined;
      }

      btn.removeAttribute('tabIndex');
    } else if (attempts < MAX_ATTEMPTS) {
      attempts++;
      timeout = window.setTimeout(removeTabIndex, TIMEOUT_MS);
    }
  }

  removeTabIndex();
})();
