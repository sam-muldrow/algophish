<script>/*<![CDATA[*/ 
        let existingAssets1505803 = [];
        let blockIds1505803 = ["1505797","1505800","1505806"];
        let currentBlock1505803 = null;
        let blockAssets1505803 = [];
        let i1505803=0,j1505803=0;
        let currentID1505803 = "";
        let thisBlock1505803 = document.querySelector("#block-1505803");
        let parentBlock1505803 = thisBlock1505803.querySelector(".hasExclusions");
        let displayCount1505803 = 5;
        let includedCount1505803 = 10;
        let debugMode1505803 = true;
        if (blockIds1505803.length > 0)
        {
            for(i1505803=0;i1505803<blockIds1505803.length;i1505803++)
            {
                currentBlock1505803=document.querySelector("#block-"+blockIds1505803[i1505803]);
                /* get all asset ids from the block */
                blockAssets1505803 = currentBlock1505803.querySelectorAll("article");
                for (j1505803=0;j1505803<blockAssets1505803.length;j1505803++)
                {
                    currentID1505803 = blockAssets1505803[j1505803].id;
                    currentID1505803 = currentID1505803.replace("card-showcase-","");
                    currentID1505803 = currentID1505803.replace("card-grid-"+blockIds1505803[i1505803]+"-","");
                    currentID1505803 = currentID1505803.replace("card-summary-"+blockIds1505803[i1505803]+"-","");
                    existingAssets1505803.push(currentID1505803);
                }
            }
            if(debugMode1505803){console.log("We have the following existing assets:",existingAssets1505803);}
            
            /* ok now we have exclusions, if there are more than zero, proceed */
            if (existingAssets1505803.length > 0)
            {
                /* we are going to loop through all the assets in our block and remove those that are in the exclusion list, make sure we aren't showing too many, then display the block */
                blockAssets1505803 = thisBlock1505803.querySelectorAll(".exclusionAsset");
                if(debugMode1505803){console.log("We have the following assets in this block",blockAssets1505803);}
                for(i1505803=0;i1505803<blockAssets1505803.length;i1505803++)
                {
                     currentID1505803 = blockAssets1505803[i1505803].dataset.uuid;
                     if(debugMode1505803){console.log("Testing "+currentID1505803);}
                     if(existingAssets1505803.includes(currentID1505803))
                     {
                         if(debugMode1505803){console.log("Found a match for "+currentID1505803);}
                         blockAssets1505803[i1505803].remove();
                         includedCount1505803--;
                     }
                }
                
                /* if includedCount is still larger than displayCount, then we need to remove assets from the bottom up */
                if(debugMode1505803){console.log("Included = "+includedCount1505803+" display = "+displayCount1505803);}
                if(includedCount1505803>displayCount1505803)
                {
                    blockAssets1505803 = thisBlock1505803.querySelectorAll(".exclusionAsset");
                    let numberToDelete1505803 = includedCount1505803-displayCount1505803;
                    if(debugMode1505803){console.log("To delete: "+numberToDelete1505803);}
                    j1505803=1;
                    for(i1505803=blockAssets1505803.length-1;i1505803>0;i1505803--)
                    {
                        blockAssets1505803[i1505803].remove();
                        j1505803++;
                        if(j1505803>numberToDelete1505803)
                        {
                            break;
                        }
                    }
                }
                parentBlock1505803.style.display='block';
            }
        }
        
