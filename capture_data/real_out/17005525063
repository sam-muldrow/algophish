<script type="text/javascript">
	$(document).ready(function () {
	  $('.clear_input').focus(function() {
	  if($(this).val() == 'Enter keyword or phrase...') {
		$(this).val("");
		}
	  });
	
	$('.clear_input').blur(function() {
	  if($(this).val() == '') {
		$(this).val('Enter keyword or phrase...');
		}
	  });
    $("#content").fitVids();

	});
