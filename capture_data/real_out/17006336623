<script type="text/javascript">


	// get specific cookie by its name
	function getCookie(cname) {
	  	var name = cname + "=";
	  	var decodedCookie = decodeURIComponent(document.cookie);
	  	var ca = decodedCookie.split(';');
	  	for(var i = 0; i <ca.length; i++) {
	    	var c = ca[i];
	    	while (c.charAt(0) == ' ') {
	      		c = c.substring(1);
	    	}
	    	if (c.indexOf(name) == 0) {
	      		return c.substring(name.length, c.length);
	    	}
	  	}
	  	return "";
	}


	// Save Category of personalized interest
	function saveInterests(){
		let interests = [];
		$('input[name="interests"]').each( function(){
			if( $(this).is(':checked') ){
				interests.push( $(this).val() );
			}
		} );
		if( interests.length > 7 ){
			alert('कुनै ७ वटा मात्र शीर्षक छान्नुहोस्।');
			return false;
		}

		interests = JSON.stringify(interests);
		var now = new Date(); //current date time
		now.setMonth( now.getMonth() + 1 ); //expire after a month
		document.cookie = "interests="+interests+"; expires="+now+";path=/";


		$.ajax({
		   type:'POST',
		   url:'/userInterests',
		   data:{
	        	interests : interests,
	        	"_token": "hmzoplQAtSLvrQQbUAVS8ZTzTmBVwgis9OYuVHTl",
		    },
		    success:function(data){
		    	if( data ){
		    		showPersonalizedContents();
		    	}
		    },
		   dataType: 'json'
		});
	}

	function createTemplate( detail ){
		let element = "";
		element += '<div class="col-12 col-lg-4 list-group">';
			element += '<div class="row text-left mx-0 pt-4 border-top">';
				element += '<div class="col-4 col-lg-4  px-0">';

					if( detail['media_type'] == 'image' ){
						element += '<figure>';
							element += '<a href="'+detail['link']+'">';
								element += '<img src="'+detail['media_url']+'">';
							element += '</a>';
						element += '</figure>';
					}else{
					  	element += '<div class="video">';
			    			element += '<iframe width="100%" src="'+detail['media_url']+'"></iframe>';
						element += '</div>';
					}

				element += '</div>';
				element += '<div class="col-8 col-lg-8 pc_item_text">';
					element += '<h1>';
						element += '<a href="'+detail['link']+'">'+detail['title']+'</a>';
					element += '</h1>';
					element += '<div>';
					  	element += '<author>';
					  		element += '<a href="'+detail['author_link']+'">'+detail['author']+'</a>';
						element += '</author>';
						element += '<time itemprop="dateLastEdit"> '+detail['time']+'	</time>';
					element += '</div>';
				element += '</div>';
			element += '</div>';
		element += '</div>';

		return element;
	}

	function hidePersonalizeForm(){
		$('.pc_form_show').fadeOut('slow');
	}

	function showPersonalizeForm(){
		$('.pc_form_show').fadeIn('slow');
	}


	function hidePersonalizeContent(){
		$('.pc_contents').fadeOut('slow');
	}

	// shows all the contents based on peronalized interests
	function showPersonalizedContents(){
		// clearing element childs
		$('#pc_contents_wrap').empty();
		$('#pc_content_titles').empty();

		$('#pc_contents_wrap').fadeIn('slow');

		let interests = getCookie('interests');
		if( interests != "" && interests != null ){
			interests = JSON.parse( interests );
		}
		if( interests.length > 0 ){
			hidePersonalizeForm();
		}

		for( let i=0; i<interests.length; i++ ){
			let j = 0;

			let element_head = "";
			let element = "";
			let count_news = 1;
			let current_cat_name = "";

			// if( $('#'+interests[i]).length > 0 ){

			// 	$( $('#'+interests[i]+' article')).each(function(){
			// 		if( count_news > 6 ){
			// 			return false;
			// 		}
			// 		// set tab body
			// 		let pc_news = [];
			// 		pc_news['media_type'] = 'image';
			// 		pc_news['cat_name'] = $($(this).find('.pc_cat_head a')).text();
			// 		pc_news['media_url'] = "";
			// 		pc_news['title'] = $( $(this).find('h1 a') ).text();
			// 		pc_news['link'] = $( $(this).find('h1 a') ).attr('href');
			// 		pc_news['author'] = $( $(this).find('author a') ).text();
			// 		pc_news['author_link'] = $( $(this).find('author a') ).attr("href");
			// 		pc_news['time'] = $( $(this).find('time') ).text();

			// 		// purpose : to set name in view all button
			// 		current_cat_name = $($(this).find('.pc_cat_head a')).text();

			// 		let video = $( $(this).find('.video') );
			// 		if( video.length > 0 ){
			// 			pc_news['media_url'] = $(video).find('iframe').attr('src');
			// 			pc_news['media_type'] = "video";
			// 		}else{
			// 			pc_news['media_url'] = $( $(this).find('img') ).attr('data-src');
			// 			if( pc_news['media_url'] == "" || pc_news['media_url'] == null || pc_news['media_url'] == undefined ){
			// 				pc_news['media_url'] = $( $(this).find('img') ).attr('src');
			// 			}
			// 		}

			// 		// set tab header
			// 		if( j === 0 ){
			// 			let cname = "";
			// 			if( i === 0 ){
			// 				cname = "active";
			// 			}

			// 			// set header tab
			// 			element_head += '<li class="nav-item">';
			// 		  		element_head += '<a class="nav-link '+ cname +'" data-toggle="tab" href="#'+interests[i]+'_tb" role="tab" aria-controls="'+interests[i]+'_tb" aria-selected="true">'+pc_news['cat_name']+'</a>';
			// 			element_head += '</li>';

			// 			// set body initial
			// 			element += '<div class="tab-pane fade show '+ cname +'" id="'+interests[i]+'_tb" role="tabpanel" aria-labelledby="'+interests[i]+'-tab">';
			// 			element += '<div class="row">';
			// 		}

			// 		let title = $( $(this).find('h1 a') ).text();
			// 		element += createTemplate( pc_news );
			// 		j++;
			// 		count_news++;
			// 	});
			// 	element += '</div>';
			// 	element += '<div class="col-12 text-center">';
			// 		element += '<a class="morelist" style="float:none;" href="https://nagariknews.nagariknetwork.com'+'/'+interests[i]+'">सबै हेर्नुहोस्</a>';
			// 	element += '</div></div>';

			// 	$('#pc_contents_wrap').append(element);
			// 	$('#pc_content_titles').append(element_head);
			// }else{

				$.ajax({
				   	type:'GET',
				   	url:'/ajax/news-by-category/'+interests[i],
				    success:function(data){
				    	var news = data['news'];
			    		// set tab header
		    			let cname = "";
		    			if( i === 0 ){
		    				cname = "active";
		    			}

		    			// set header tab
		    			element_head += '<li class="nav-item">';
		    		  		element_head += '<a class="nav-link '+ cname +'" data-toggle="tab" href="#'+interests[i]+'_tb" role="tab" aria-controls="'+interests[i]+'_tb" aria-selected="true">'+news[0]['category']+'</a>';
		    			element_head += '</li>';

		    			// set body initial
		    			element += '<div class="tab-pane fade show '+ cname +'" id="'+interests[i]+'_tb" role="tabpanel" aria-labelledby="'+interests[i]+'-tab">';
		    			element += '<div class="row">';

			    		let title = $( $(this).find('h1 a') ).text();
			    		element += data['view'];
				    	// });
				    	element += '</div>';
				    	element += '<div class="col-12 text-center">';
				    		element += '<a class="morelist" style="float:none;" href="https://nagariknews.nagariknetwork.com'+'/'+interests[i]+'">सबै हेर्नुहोस्</a>';
				    	element += '</div></div>';

				    	$('#pc_contents_wrap').append(element);
				    	$('#pc_content_titles').append(element_head);

				    },
				   dataType: 'json'
				});

			// }

		}
	}

	$(document).on('click', '#personalized_content_create', function(){
		if( $('#isloggedin').attr('data-is-login') == 'true' ){
			saveInterests();
			showPersonalizedContents();
		}else{
			$('#login_popup').modal('show');
		}
	});

	$(document).ready( function(){
		showPersonalizedContents();
	} );


	function js_author_name_format( author ){
		var authors = "";
		if( author.length == 1 ){
			authors = $author[0]['names'];
		}else{
			for( var i = 0; i<author.length; i++ ){
				if( i == ( author.length - 2 ) ){
					authors += author[i]['names']+' र ';
				}else if( i == (author.length - 1) ){
					authors += author[i]['names'];
				}else{
					authors += author[i]['names']+', ';
				}
			}
		}
		return authors;
	}
</script>
