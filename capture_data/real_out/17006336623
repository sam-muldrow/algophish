            slidesToShow: 3,
            slidesToScroll: 1,
            infinite: true
          }
        },
        {
          breakpoint: 600,
          settings: {
            slidesToShow: 2,
            slidesToScroll: 1
          }
        },
        {
          breakpoint: 480,
          settings: {
            slidesToShow: 1,
            slidesToScroll: 1
          }
        }
      ]
    })
    //.on('setPosition', function (event, slick) {
    //	slick.$slides.css('height', slick.$slideTrack.height() + 'px');
    //});
  });
</script>

		</section>
		<section class="py-2">
