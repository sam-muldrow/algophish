    </section>

    <script>
        const livescoreContainer = document.querySelector('#livescore-scroll');
        var liveScoreCurrent = document.querySelector('#current-item');
        const leftArrow = document.querySelector('#livescore-action-left');
        const rightArrow = document.querySelector('#livescore-action-right');
        const selectorList = document.querySelector('.selection__list--calendar');
        let scrollTimeout = null;

        function fadeIn(el, display) {
            el.style.opacity = 0;
            el.style.display = display || "block";
            (function fade() {
                var val = parseFloat(el.style.opacity);
                if (!((val += .2) > 1)) {
                    el.style.opacity = val;
                    requestAnimationFrame(fade);
                }
            })();
        };


        fixCalendarPosition = function () {

            console.log('match');

            if (typeof (liveScoreCurrent) == 'undefined' || liveScoreCurrent === null) {
                fadeIn(livescoreContainer);

                liveScoreCurrent = selectorList.lastElementChild;
            }

            let cLeft = livescoreContainer.getBoundingClientRect().left;
            let eLeft = liveScoreCurrent.getBoundingClientRect().left;

            let distanceSquared = Math.pow(cLeft - eLeft, 2);
            let distance = Math.sqrt(distanceSquared);

            livescoreContainer.scrollTo({
                top: 0,
                left: distance
            });

            fadeIn(livescoreContainer);
        }

        cardObserver = function () {

            let htmlCollection = document.getElementsByClassName("x-scroll__item");
            let array = [].slice.call(htmlCollection)
            for (let i = 0; i < array.length; i++) {
                let observer = new IntersectionObserver(callback, {threshold: 0.99});
                observer.observe(array[i]);
            }

        }

        document.addEventListener('DOMContentLoaded', function () {
            fixCalendarPosition();
        });

        leftArrow.addEventListener('click', function () {
            livescoreContainer.scrollTo({
                top: 0,
                left: livescoreContainer.scrollLeft - 300,
                behavior: 'smooth'
            });

        }, false);

        rightArrow.addEventListener('click', function () {
            livescoreContainer.scrollTo({
                top: 0,
                left: livescoreContainer.scrollLeft + 300,
                behavior: 'smooth'
            });

        }, false);
    </script>


            </div>
        
