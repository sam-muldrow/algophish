<script>
    $(function () {

        var currentTime = 0;
        var duration = 5000;
        var maxArticles = $('.prima-pagina-preview__item').length;

        var timer, timerPassed, finalDate, timeRemaining;
        var currentIndex = 0;
        var timeRemaining = duration;

        function setCurretPrimaPagina(index) {
            currentIndex = index;
            finalDate = new Date();
            finalDate.setMilliseconds(duration);
            $('.prima-pagina-articles__item').removeClass('prima-pagina-articles__item--active');
            $('.prima-pagina-preview__item').removeClass('prima-pagina-preview__item--active');
            $('.prima-pagina-preview__item').eq(currentIndex).addClass('prima-pagina-preview__item--active');
            $('.prima-pagina-articles__item').eq(currentIndex).addClass('prima-pagina-articles__item--active');
            $('.prima-pagina-articles__timeline-bar').css('width', '0%');
        }

        function pausePrimaPaginaTimer() {
            clearInterval(timer);
            clearInterval(timerPassed);
        }

        function nextArticle() {
            currentIndex = currentIndex + 1;
            if (currentIndex == maxArticles) {
                currentIndex = 0;
            }
            setCurretPrimaPagina(currentIndex);
        }

        function playPrimaPaginaTimer() {

            timerPassed = setInterval(function () {
                var currentDate = new Date();
                var timeDiff = (finalDate.getTime() - currentDate.getTime()) / 1000;
                timeRemaining = timeDiff < 0 ? 0 : timeDiff;
                var perc = Math.abs(Math.round(timeRemaining / duration * 100000) - 100);
                // $('.prima-pagina-preview__title').text(perc)
                $('.prima-pagina-articles__item').eq(currentIndex).find('.prima-pagina-articles__timeline-bar').css('width', perc + '%');
                if (perc === 100) {
                    nextArticle();
                }
            }, 1);
        }

        $('.prima-pagina-articles__item').on('mouseover', function () {
            setCurretPrimaPagina($(this).index());
        });

        $('.prima-pagina-preview, .prima-pagina-articles').on('mouseenter', function () {
            pausePrimaPaginaTimer();
        });

        $('.prima-pagina-preview, .prima-pagina-articles').on('mouseleave', function () {
            playPrimaPaginaTimer();
        });

        setCurretPrimaPagina(0);
        playPrimaPaginaTimer();
        $('.prima-pagina-preview__time-ago').timeago();

        $(".prima-pagina-preview__title").each(function () {
            if ($(this).text().length > 130) {
                $(this).css("font-size", "28px");
            }
        });

    })
</script>


    
