    <script>
        var mediator;
        //var cookieAccept;
        $(document).ready(function () {
            mediator = common.service.mediator.create(_);
            var communityApi = common.service.apiClient.community.create('https://vivoperlei.calciomercato.com/api', $),
                security = common.service.security.create(document, $, _, communityApi, Cookies, moment),
               //  cookieAccept = calciomercato.ui.component.cookieAccept.create($, security, mediator),
                talkApi = calciomercato.service.apiClient.talk.create('https://talk.calciomercato.com', $, moment, 'https://talk.calciomercato.com'),
                loginMenu = calciomercato.ui.component.loginMenu.create($),
                userMenu = calciomercato.ui.component.userMenu.create($, _),
                notifications = calciomercato.ui.component.notifications.create(
                    $, _, moment, 'https://vivoperlei.calciomercato.com', 'https://www.calciomercato.com/', talkApi
                ),
                notificationsCount = calciomercato.ui.component.notificationsCount.create($, _),
                polling = calciomercato.service.polling.create(),
                googleAnalyticsTracking = calciomercato.service.googleAnalyticsTracking.create($, ga),
                userState = calciomercato.ui.component.userState.create(security, mediator, talkApi, polling),
                modalLoginRegister = calciomercato.ui.component.modalLoginRegister.create($),
                cmAuthentication = calciomercato.ui.component.cmAuthentication.create($, security, modalLoginRegister, mediator),
                facebookLogin = calciomercato.ui.component.facebookLogin.create($, modalLoginRegister, communityApi, mediator),
                googleLogin = calciomercato.ui.component.googleLogin.create($, modalLoginRegister, communityApi, '937046979263-ne8qp5d05al89e3sgiqg0sp7sb587pua.apps.googleusercontent.com', mediator),
                pushNotificationTeamSelector = calciomercato.ui.component.pushNotificationTeamSelector.create($, mediator, security),
                vC = common.service.viewportChecker.create($),
                enetscoreLazyLoader = calciomercato.ui.component.enetscoreLazyLoader.create($, vC),
                deterrentCmp = calciomercato.ui.component.deterrentCmp.create(),
                pushNotifications = calciomercato.ui.component.pushNotifications.create(
                    $,
                    messaging,
                    security,
                    pushNotificationTeamSelector,
                    'https://www.calciomercato.com/api/notification/add-user-to-topic',
                    'https://www.calciomercato.com/api/notification/delete-user-to-topic',
                    'https://www.calciomercato.com/api/notification/update-token',
                    'https://www.calciomercato.com/api/notification/update-click'
                );


            pushNotificationsDialog = calciomercato.ui.component.pushNotificationsDialog.create(
                $,
                security,
                '#dialog-push-notification',
                '#not-subscribe-notifications',
                ".subscribe-notifications"
            );

            favouriteTeam = calciomercato.ui.component.favouriteTeam.create($, security);

           // moment.locale('it');

            mediator.subscribe('UpdateUI', favouriteTeam);
            mediator.subscribe('UserLoggedIn', loginMenu);
            mediator.subscribe('UserLoggedOut', loginMenu);
            mediator.subscribe('UserLoggedIn', userMenu);
            mediator.subscribe('UserLoggedOut', userMenu);
            mediator.subscribe('NotificationsReceived', notifications);
            mediator.subscribe('NotificationsCountReceived', notificationsCount);
            //mediator.subscribe('CookieAccept', pushNotificationsDialog);
            mediator.subscribe('UserState', userState);


            pushNotifications.init();
            enetscoreLazyLoader.init();
            deterrentCmp.init();


            favouriteTeam.render();
            //cookieAccept.render();
            userState.render();

            googleAnalyticsTracking.initFacebookShare();
            googleAnalyticsTracking.initTwitterShare();
            googleAnalyticsTracking.initNotification();

            cmAuthentication.init();
            modalLoginRegister.render();
            facebookLogin.init();
            googleLogin.init();
        });
    </script>

    <script>
        $(document).ready(function () {
            var facebookShare = common.ui.component.facebookShare.create($),
                twitterShare = common.ui.component.twitterShare.create($, "cmdotcom"),
                primaPagina = calciomercato.ui.component.primaPagina.create($),
                communityApi = common.service.apiClient.community.create('https://vivoperlei.calciomercato.com/api', $),
                security = common.service.security.create(document, $, _, communityApi, Cookies, moment),
                modalTeamSelector = calciomercato.ui.component.modalTeamSelector.create(
                    $,
                    "https://www.calciomercato.com",
                    ga,
                    'homepage',
                    security,
                    mediator,
                    true,
                    false,
                    'web',
                    'home',
                    'full-overlay'
                ),
                articleTeamLogo = calciomercato.ui.component.articleTeamLogo.create($, security),
                talkApi = calciomercato.service.apiClient.talk.create('https://talk.calciomercato.com', $, moment, 'https://talk.calciomercato.com'),
                polling = calciomercato.service.polling.create(),
                modalLoginRegister = calciomercato.ui.component.modalLoginRegister.create($),
                webApi = common.service.apiClient.web.create('https://www.calciomercato.com', $, _),
                viewportChecker = common.service.viewportChecker.create($),
                moreArticles = common.ui.component.moreArticlesLazyLoader.create($, webApi, mediator, viewportChecker),
                ultimaOra = common.ui.component.ultimaOra.create($, mediator, _),
                ultimaOraFilter = calciomercato.ui.component.ultimaOraFilter.create($, security, webApi, mediator, moreArticles),
                googleAnalyticsTracking = calciomercato.service.googleAnalyticsTracking.create($, ga),
                //sidebarLazyLoaderGossipNews = calciomercato.ui.component.sidebarLazyLoader.create($),
                //sidebarLazyLoaderSosfantaNews = calciomercato.ui.component.sidebarLazyLoader.create($),
                //sidebarLazyLoaderAuthorColumns = calciomercato.ui.component.sidebarLazyLoader.create($),
                lazyAdv = common.ui.component.lazyAdv.create($, viewportChecker, 1000),
                imageLazyLoader = calciomercato.ui.component.imageLazyLoader.create($);


            mediator.subscribe('UpdateUI', articleTeamLogo);
            mediator.subscribe('UpdateUI', ultimaOraFilter);
            mediator.subscribe('UpdateUI', imageLazyLoader);
            mediator.subscribe('UpdateUI', modalTeamSelector);
            mediator.subscribe('UltimaOraArticlesWasReceived', ultimaOra);
            mediator.subscribe('UltimaOraTeamFilterWasSent', moreArticles);

            articleTeamLogo.render(security.getUserPreferences());
            facebookShare.render();
            twitterShare.render();
            primaPagina.render();
            imageLazyLoader.render();

            lazyAdv.init();


            ultimaOraFilter.render(security.getUserPreferences());
            modalLoginRegister.render();
            googleAnalyticsTracking.initOverlaysquadre('homepage');

            var excludedIds = moreArticles.getExcludedArticlesIds();
            var message = {name: 'UpdateUI', data: ''};

            webApi.getArticles(excludedIds, 'html', '', 36).done(
                function (articles) {
                    mediator.publish('UltimaOraArticlesWasReceived', {articles: articles, message: message});
                }
            ).fail(function () {
            });

            moreArticles.render({queryString: '', message: message});

            //sidebarLazyLoaderGossipNews.init('#gossip-news-threshold', '#sidebar-gossip-news');
            //sidebarLazyLoaderSosfantaNews.init('#sosfanta-posts-threshold', '#sidebar-sosfanta-news');
            //sidebarLazyLoaderAuthorColumns.init('#author-columns-threshold', '#sidebar-author-columns');
        });
    </script>



