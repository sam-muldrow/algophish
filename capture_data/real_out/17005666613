<script type="text/javascript">
$(document).ready(function(){
	//스크롤시 GNB처리
	if($.cookie("scroll") !== null) window.scrollTo(0, $(window).scrollTop($.cookie("scroll")));
	
	$(window).scroll( function() {
		if($(window).scrollTop()>=20) {	//심플 GNB
			$(".header").addClass("scroll");
			$('.box_login').hide();		//로그인
			$('.box_search').hide();	//검색
		}else {	//기본 GNB
			$(".header").removeClass("scroll");
		}
		$.cookie("scroll", $(window).scrollTop());	//새로고침시 스크롤위치 저장
	});
});

//햄버거 > 메뉴클릭
function hamMenuChange(n){
	for(i=1; i<=$(".wrap-tab-list > li").length; i++){
		$("#tab-panel-"+i).hide();
	}
	if(n){
		$("#tab-panel-"+n).show();
		$(".mid").hide();	//라이브
		$(".right").hide();	//실시간 인기
	}else{
		$(".mid").show();	//라이브
		$(".right").show();	//실시간 인기
		$("#tab-panel-"+n).hide();
	}
}

//검색 점검여부 확인
function searchChk(f){
	var dt;
	dt = new Date();
	var limit = dt.getFullYear() + ("00" + (dt.getMonth() + 1)).slice(-2) + ("00" + dt.getDate()).slice(-2) + ("00" + dt.getHours()).slice(-2)  + ("00" + dt.getMinutes()).slice(-2);
	if(limit >= '302303300000' && limit <= '302303300200'){
		alert('안정적인 서비스 제공을 위하여 서버 점검이 진행될 예정입니다.\n아래 작업 시간동안 검색 서비스가 제한되오니 이용자 여러분의 많은 양해 바랍니다.\n※ 작업시간: 00:20 ~ 02:00');
		if(limit >= '302303300020' && limit < '302303300200'){
			if(f=="web_search2"){
				document.web_search2.action = '//ytn.co.kr';
				document.web_search2.submit(); 
			}else{
				document.web_search.action = '//ytn.co.kr';
				document.web_search.submit(); 
			}
		}
	}else{
		if(f=="web_search2") document.web_search2.submit();
		else document.web_search.submit();
	}
}
</script>
