	<script>
	$(document).ready(function(){
		var vodslider = $('.ytslide').bxSlider({
			auto:true,					//자동 슬라이드
			autoControls: true,			//이전,다음 버튼
			autoControlsCombine: true,	//시작,정지 버튼
			autoHover: true,			//마우스 호버 슬라이드 정지
			pause:3000,
			minSlides: 2,
			maxSlides: 2,
			moveSlides: 2,
			slideWidth: 264,
			slideMargin: 16,
			touchEnabled:false
		});
		$('.controls .play').on('click', function(){
			vodslider.startAuto();
		})
		$('.controls .pause').on('click', function(){
			vodslider.stopAuto();
		})
	});

	function campaignClick(idx, link, target){
		
		//로그처리
		var dynamic  = new jes.DynamicScript();
		dynamic.call('/_comm/ylog_campaign_banner.php?i='+idx);

		//페이지 이동
		if(target=="blank") window.open(link);
		else if($.trim(link)) location.href = link;
		else return false;
	}
	</script>
