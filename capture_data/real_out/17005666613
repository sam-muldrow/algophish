<!-- 문단 중 이미지 광고 / 이 시각 추천뉴스 -->
<script type="text/javascript">
/*
문단 내 배너 적용기준
1. 5문단 미만일 경우 - 기사내 배너 노출 안함
2. 5문단 초과일 경우 - 첫번째 배너 노출(둘째 문단 뒤 노출)
3. 하단 기사가 7문단 이상 남은 경우 15문단에 노출 - 추천뉴스 노출
	3-1. 추천 뉴스가 있는 경우 - 두번째 배너 노출(기사 밑에서 6문단에 노출)
*/
$(document).ready(function(){
    function n() {
        var n;
        if ($("#CmAdContent span").eq(1) || $("#CmAdContent span").eq(2)) {
            var e = $("#CmAdContent span").eq(0).text().length
              , t = $("#CmAdContent span").eq(1).text().length
              , o = 0;
            if ($("#CmAdContent span").eq(2) && (o = $("#CmAdContent span").eq(2).text().length),
            183 > e && 183 > t && 183 > o)
                return;
            n = e > t && e > o ? $("#CmAdContent span").eq(0).find("br") : o > t ? $("#CmAdContent span").eq(2).find("br") : $("#CmAdContent span").eq(1).find("br")
        } else {
            if ($("#CmAdContent span").eq(0).text().length < 183) return;
            n = $("#CmAdContent span").eq(0).find("br")
        }
        var s = n.length;			//총 문단수
		var bottomAdLine;		//문단 중 이미지광고2 출력위치
		if($("#CmAdContent span").eq(0).text().includes("당신의 제보가 뉴스가 됩니다")) bottomAdLine = 14;
		else bottomAdLine = 6;

        n.each(function(n) {
			if(s > 5){
				if(n == 1){	//문단 중 이미지 광고(두번째 문단)
					$(this).after("<div id='hns_mask' class='hns_mask_div' style='width:300px;height:250px;float:right; text-align:right;margin:20px 0 20px 20px;z-index:1;background-color:#E1E1E1'><iframe src='//adex.ednplus.com/xc/h/wURE9Mpr' width='300' height='250' frameborder='0' scrolling='no' topmargin='0' leftmargin='0' marginwidth='0' marginheight='0' referrerpolicy='no-referrer-when-downgrade'></iframe></div>");
				}

				var d = s-15;
				if(d>=7){	//하단 7문단이상 남은 경우 추천뉴스 노출
					if(n == 15){	//이 시각 추천뉴스
						$(this).after("<iframe frameborder='0' width='756' height='214' style='margin-left:-8px' scrolling='no' title='이 시각 추천뉴스' src='/inc_banner/recommList.html'></iframe>");
					}

					//문단 중 이미지 광고2 (기사 밑에서 6문단에 노출)
					if(n == (s-bottomAdLine)){
						$(this).after("<div id='hns_mask' class='hns_mask_div' style='width:300px;height:250px;float:right; text-align:right;margin:20px 0 20px 20px;z-index:1;background-color:#E1E1E1'><iframe src='//adex.ednplus.com/xc/h/wSHDSbDD' width='300' height='250' frameborder='0' scrolling='no' topmargin='0' leftmargin='0' marginwidth='0' marginheight='0' referrerpolicy='no-referrer-when-downgrade'></iframe></div>");
					}
				}			
			}
        })
    }
    $("#CmAdContent").length && n();
});
</script>
