			function rcSlide(idx){
				var num;
				for(i=0;i<rNews.length/2;i++) $('#rslide_'+i).removeClass( 'on' );
				$('#rslide_'+idx).attr("class", "on");
				
				num = idx*2;
				$('#reSlidea1_1').attr("class", rNews[num]['site_class']);
				$('#reSlidea1_1').attr("href", rNews[num]['site_url']);
				$('#reSlidea1_2').attr("href", rNews[num]['url']);
				$('#reSlidea1_3').attr("src", rNews[num]['img']);
				$('#reSlidea1_4').html(rNews[num]['title']);
				$('#reSlidea1_5').attr("href", rNews[num]['url']);
				
				num += 1;
				$('#reSlidea2_1').attr("class", rNews[num]['site_class']);
				$('#reSlidea2_1').attr("href", rNews[num]['site_url']);
				$('#reSlidea2_2').attr("href", rNews[num]['url']);
				$('#reSlidea2_3').attr("src", rNews[num]['img']);
				$('#reSlidea2_4').html(rNews[num]['title']);
				$('#reSlidea2_5').attr("href", rNews[num]['url']);
			}

			rcSlide(0);
		</script>

