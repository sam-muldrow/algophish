                        <div class="topics-menu-wrap clearfix">
                            <h2 style="cursor:pointer;" onclick="location.href='https://hc.nikkan-gendai.com'">健康</h2>
                        </div>
                    </div>
                    <!--.article-container-b-->
                    <ul class="article-container-b clearfix">
                                                    <li class="article-box">
