<script>
(function() {
  var count = 0;
  var listNum = $('#news-ticker li').length;
  function loop() {
    $('#news-ticker li').removeClass('active');
    $('#news-ticker li').eq(count).addClass('active');
    count++;
    count = count % listNum;
  }
  if (listNum > 1) {
    // お知らせ件数が2件以上の場合はticker表示する
    setInterval(loop, 3000);
  } else {
    // 1件の場合は固定表示
    $('#news-ticker li').css('top', '0px');
  }
})();
</script>
