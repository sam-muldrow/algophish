	<script>
var MMG = MMG || {};
MMG.cmpCmd = MMG.cmpCmd || [];

MMG.cmpCmd.push((success, tcString, tcData) => {
	if (!tcData.audience) {
		return;
	}

	var blocked = (
		typeof cahdeedaifahkuosoo0sheNgae9uluonguD2ahqu4ixophea5f === 'undefined'
		|| !cahdeedaifahkuosoo0sheNgae9uluonguD2ahqu4ixophea5f
	);

	if (blocked) {
		MMG.googleAnalytics.set('dimension3', 'adblock');
	} else {
		MMG.googleAnalytics.set('dimension3', 'no_adblock');
	}

	var isUserLogged = (
		(typeof current_user !== 'undefined' && current_user.is_logged_in)
		||	(typeof logged_in !== 'undefined' && logged_in)
	);

	if (isUserLogged) {
		MMG.googleAnalytics.set('dimension2', 'User');
	} else {
		MMG.googleAnalytics.set('dimension2', 'Guest');
	}
});
</script>

