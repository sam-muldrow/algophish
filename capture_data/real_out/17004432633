	MMG.cmpCmd = MMG.cmpCmd || [];

	MMG.cmpCmd.push((success, tcString, tcData) => {
		const random = ((min, max) => {
			return Math.floor(Math.random() * (max - min + 1)) + min;
		})(0, 4);

		if (!MMG.isAdblockDisabled()) {
			$('#vjs-rcp1').remove();

			return;
		}

		if (0 === random) {
