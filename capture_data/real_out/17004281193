    <script>
(function () {
    var linkContainer = document.createElement('li');
    var linkContainerHook = document.querySelectorAll('.topic_sec .topic.res_topic ul li:nth-of-type(5)')[2];

    function injectCCPALink() {
        if (window.ccpaAppliesPromise && window.__uspapi) {
            window.ccpaAppliesPromise.then(function (ccpaApplies) {
                if (!ccpaApplies) {
                    return;
                }

                if (window.top.screen.width >= 700) {
                    linkContainer = document.querySelector('#privacy-link-container1');
                    if (!linkContainer) {
                        return;
                    }
                } else {
                    linkContainerHook.insertAdjacentElement('beforebegin', linkContainer);
                }

                var ccpaLink = document.createElement('a');
                ccpaLink.href = '#';
                ccpaLink.innerText = 'Do not sell my info';
                ccpaLink.addEventListener('click', function (event) {
                    event.preventDefault();
                    window.__uspapi('displayUspUi');
                    return false;
                });
                linkContainer.appendChild(ccpaLink);

                // CSS rules to hide unecessary links in CCPA modal
                var style = document.createElement('style')
                style.innerText = '#qc-usp-delete-link, #qc-usp-access-link { display: none; } #qc-usp-policy-link {border-left: none;}'
                document.head.appendChild(style)
            });
        }
    }

    function injectGDPRLink() {
        if (window.__tcfapi) {
            window.__tcfapi('addEventListener', 2, function (data, success) {
                if (
                    success &&
                    (data.eventStatus === 'tcloaded' || data.eventStatus === 'useractioncomplete') &&
                    data.gdprApplies === true
                ) {
                    window.__tcfapi('removeEventListener', 2, function () { }, data.listenerId);

                    if (window.top.screen.width >= 700) {
                        linkContainer = document.querySelector('#privacy-link-container2');
                        if (!linkContainer) {
                            return;
                        }
                    } else {
                        linkContainerHook.insertAdjacentElement('beforebegin', linkContainer);
                    }

                    var link = document.createElement('a');
                    link.href = '#';
                    link.innerText = 'GDPR consent';
                    link.addEventListener('click', function (event) {
                        event.preventDefault();
                        window.__tcfapi('displayConsentUi', 2, function () { });
                        return false;
                    });
                    linkContainer.appendChild(link);
                }
            })
        }
    }

    if (document.readyState === 'loading') {  // Loading hasn't finished yet
        document.addEventListener('DOMContentLoaded', injectCCPALink);
        document.addEventListener('DOMContentLoaded', injectGDPRLink);
    } else {  // `DOMContentLoaded` has already fired
        injectCCPALink();
        injectGDPRLink();
    }
}());
    </script>

