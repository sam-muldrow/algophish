<script>
    var allowedItemsInList = 12
    function setUpShowMoreListBtn() {       

        $('.popular-items .seo-links-list a.show-more-list').each(function () {
            var parent = $(this).parent();
            var itemListCounter = parent.find('li').length;            
            if (itemListCounter > window.allowedItemsInList) {
                parent.find('a.show-more-list').css('display', 'inline-block')
            }
        })      

        $('.popular-items .seo-links-list a.show-more-list').click(function (event) {
            event.preventDefault();
            var parent = $(this).parent();
            parent.toggleClass('seo-links-list')
        })
    }
    $(document).ready(function () {
        setUpShowMoreListBtn();
    })
</script>

