nAuthor":"epi-562-site-structure","meta":{"collectionName":"categories","modelName":"category","publishable":false,"identifiable":false,"image":false,"archived":false,"hallable":false,"hierarchical":true,"affiliatizable":false,"translatable":false,"localizable":false,"redirectable":false},"id":"5bb657061ddd222e12d7cbb6","revision":1,"createdAt":"2018-10-04T18:08:06.490Z","modifiedAt":"2022-09-21T12:11:28.092Z","revisionCreatedAt":"2022-09-21T12:11:28.092Z","organizationId":"4gKgcF6pnhncm3M8T5LwigZHwNYd","isProgrammatic":true,"lang":"en-US","hierarchy":["tags","ingredient","fruit","stone-fruit","nectarine"]},{"name":"Nut Free","slug":"tree-nut-free","revisionAuthor":"Anne Dehmer","meta":{"collectionName":"categories","modelName":"category","publishable":false,"identifiable":false,"image":false,"archived":false,"hallable":false,"hierarchical":true,"affiliatizable":false,"translatable":false,"localizable":false,"redirectable":false},"id":"5bb6560fd1e704483dd0ae7f","revision":7,"createdAt":"2018-10-04T18:03:59.740Z","modifiedAt":"2022-10-19T17:15:23.203Z","revisionCreatedAt":"2022-10-19T17:15:23.203Z","organizationId":"4gKgcF6pnhncm3M8T5LwigZHwNYd","isProgrammatic":false,"lang":"en-US","hierarchy":["tags","special-consideration","tree-nut-free"]},{"name":"Baking","slug":"bake","revisionAuthor":"Sarat Tatavarthi","meta":{"collectionName":"categories","modelName":"category","publishable":false,"identifiable":false,"image":false,"archived":false,"hallable":false,"hierarchical":true,"affiliatizable":false,"translatable":false,"localizable":false,"redirectable":false},"id":"5bb655cbefec342dc6b6f701","revision":6,"createdAt":"2018-10-04T18:02:51.894Z","modifiedAt":"2022-10-19T18:11:02.069Z","revisionCreatedAt":"2022-10-19T18:11:02.069Z","organizationId":"4gKgcF6pnhncm3M8T5LwigZHwNYd","isProgrammatic":false,"lang":"en-US","hierarchy":["tags","technique","bake"]},{"name":"Summer","slug":"summer","revisionAuthor":"Anne Dehmer","meta":{"collectionName":"categories","modelName":"c