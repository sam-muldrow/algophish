            if(typeof isNativeSportApp != 'undefined' && isNativeSportApp()) {
                $('body').addClass('is-sport');
                KRNEvents.on('krn:dom-ready:init', function() {
                    $('#krn-navi-logo-link').attr('href', $('#krn-navi-logo-link').attr('href') + 'sport');
