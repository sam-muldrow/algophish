            // initialize apstag and have apstag set bids on the googletag slots when they are returned to the page
            if(typeof(apstag) !== 'undefined') {
                apstag.init({
                    pubID: '3772',
                    adServer: 'googletag',
                    deals: true,
                    bidTimeout: 400, // make sure this timeout is less than or equal to openwrap timeout
                    params: {
                        sections: 'startseite'
                    }
                });

                googletag.cmd.push(function() {
                    if(window.GPTadSlots.amazon.length !== 0 && typeof(window.OWT) !== 'undefined') {
                        apstag.fetchBids({
                            slots: window.GPTadSlots.amazon
                        }, function(bids) {
                            // set apstag bids, then trigger the first request to DFP
                            apstag.setDisplayBids();
                            googletag.pubads().refresh();
                            // this will tell openwrap that all the external bidders have returned bid
                            setTimeout(function () {
                                window.OWT.notifyExternalBiddingComplete(notifyId);
                            }, 400);
                        });
                    } else {
                        // trigger the first request to DFP
                        googletag.pubads().refresh();
                    }
                });
            }
        }
