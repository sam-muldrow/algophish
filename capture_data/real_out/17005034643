        krnInjectAsset({
            type: 'link',
            path: '/vendor/swiper/swiper.min.css',
            id: 'swiper-css'
        });
        krnInjectAsset({
            type: 'script',
            path: '/vendor/swiper/swiper.min.js',
            id: 'swiper-js',
        });

        KRNEvents.on('krn:dom-ready:init', function() {
            KRNEvents.on('krn-swiper-js-loaded', function() {
                new Swiper('.swiper-8dc00598', {
                    direction: 'vertical',
                    slidesPerView: 'auto',
                    grabCursor: true,
                    mousewheel: true,
                    freeMode: true,
                    navigation: {
                        nextEl: '.swiper-button-next',
                        prevEl: '.swiper-button-prev',
                    },
                    on: {
                        progress: function(swiper, progress) {
                            if(isNaN(progress)) {
                                return false;
                            }
                            if(swiper.isBeginning && swiper.isEnd) {
                                for (const el of [...swiper.el.querySelectorAll(':scope .slider__button')]) {
                                    el.remove();
                                }
                                for (const el of [...swiper.el.querySelectorAll(':scope .slider__gradient')]) {
                                    el.remove();
                                }
                            } else if(swiper.isBeginning) {
                                swiper.el.querySelectorAll(':scope .slider__gradient-prev')[0].classList.add('hidden');
                                swiper.el.querySelectorAll(':scope .slider__gradient-next')[0].classList.remove('hidden');
                                swiper.el.querySelectorAll(':scope .swiper-button-prev')[0].classList.add('hidden');
                                swiper.el.querySelectorAll(':scope .swiper-button-next')[0].classList.remove('hidden');
                            } else if(swiper.isEnd) {
                                swiper.el.querySelectorAll(':scope .slider__gradient-next')[0].classList.add('hidden');
                                swiper.el.querySelectorAll(':scope .slider__gradient-prev')[0].classList.remove('hidden');
                                swiper.el.querySelectorAll(':scope .swiper-button-next')[0].classList.add('hidden');
                                swiper.el.querySelectorAll(':scope .swiper-button-prev')[0].classList.remove('hidden');
                            } else {
                                swiper.el.querySelectorAll(':scope .slider__gradient-prev')[0].classList.remove('hidden');
                                swiper.el.querySelectorAll(':scope .slider__gradient-next')[0].classList.remove('hidden');
                                swiper.el.querySelectorAll(':scope .swiper-button-prev')[0].classList.remove('hidden');
                                swiper.el.querySelectorAll(':scope .swiper-button-next')[0].classList.remove('hidden');
                            }
                        },
                        afterInit: function(swiper) {
                            swiper.el.closest('.simple-swiper').classList.remove('hidden');
                        },
                        beforeInit: function(swiper) {
                            if(swiper.isBeginning) {
                                swiper.el.querySelectorAll(':scope .slider__gradient-prev')[0].classList.add('hidden');
                                swiper.el.querySelectorAll(':scope .slider__gradient-next')[0].classList.remove('hidden');
                                swiper.el.querySelectorAll(':scope .swiper-button-prev')[0].classList.add('hidden');
                                swiper.el.querySelectorAll(':scope .swiper-button-next')[0].classList.remove('hidden');
                            } else if(swiper.isEnd) {
                                swiper.el.querySelectorAll(':scope .slider__gradient-next')[0].classList.add('hidden');
                                swiper.el.querySelectorAll(':scope .slider__gradient-prev')[0].classList.remove('hidden');
                                swiper.el.querySelectorAll(':scope .swiper-button-next')[0].classList.add('hidden');
                                swiper.el.querySelectorAll(':scope .swiper-button-prev')[0].classList.remove('hidden');
                            } else {
                                swiper.el.querySelectorAll(':scope .slider__gradient-prev')[0].classList.remove('hidden');
                                swiper.el.querySelectorAll(':scope .slider__gradient-next')[0].classList.remove('hidden');
                                swiper.el.querySelectorAll(':scope .swiper-button-prev')[0].classList.remove('hidden');
                                swiper.el.querySelectorAll(':scope .swiper-button-next')[0].classList.remove('hidden');
                            }
                        },
                    },
                });
            });
        });
    </script></div></div></div></div></div><div class="row" data-nodeid="2880654-2d0134ed"><div class="box col-xs-6 c_anmod krn-dup-checker krn-regio-dup-checker" data-krn-click-id="startseite_aufmacher02" data-boxname="anmod" data-nodeid="2880654-5d1be7e9" data-tb-region-item><style type="text/css"> .box__2880654-5d1be7e9.imgteaser .imgteaser__content--inside, .box__2880654-5d1be7e9 .imgteaser:hover .imgteaser__content--inside {background: linear-gradient(to bottom, rgba(0, 0, 0, 0) 0%, rgba(0, 0, 0, 0) 15%, rgba(0, 0, 0, 0.7) 45%, rgba(0, 0, 0, 1) 70%, rgba(0, 0, 0, 1) 100%);}</style><style type="text/css"> .box__2880654-5d1be7e9 .imgteaser__pretitle span {background-color: #e3000f; box-shadow: 0.33em 0 0 #e3000f, -0.33em 0 0 #e3000f;box-decoration-break: clone; -webkit-box-decoration-break: clone;}</style><div class="imgteaser imgteaser--aufmacher-6 box__2880654-5d1be7e9" data-krn-id="3172570" data-krn-type="krn-article" data-krn-rid="[90,763]" data-krn-video=1><a href="https://www.krone.at/3172570" ><div class="imgteaser__img valign-bottom"><div class="img-lazy-responsive" style="padding-bottom: 64.042553191489%"><img src="https://imgl.krone.at/scaled/3172617/vaf8e51/470x270.jpg" alt="(Bild: Screenshot/Telegram/IDF Krone KREATIV,)" title="(Bild: Screenshot/Telegram/IDF Krone KREATIV,)" class="img-responsive no-lazy" /></div></div><div class="imgteaser__content imgteaser__content--inside align-left valign-bottom"><div class="imgteaser__pretitle krn_editable " data-obj=3172570 data-ed-kind="presenter_pretitle"><span>VorwÃ¼rfe aus Israel</span></div><div class="imgteaser__title krn_editable no-break" data-obj=3172570 data-ed-kind="presenter_title"><span>Videos sollen Hamas-Geiseln <br /> in Shifa-Krankenhaus zeigen</span></div></div></a></div></div><div class="box col-xs-6 c_slickslider" data-krn-click-id="" data-boxname="slickslider" data-nodeid="2880654-f1f9f350"><div class="krn-cls ss__outer ss__outer-- swiper swiper-2880654-f1f9f350" data-cls="80"><div class="