  <script type="text/javascript">

    //create empty apntag object if it doesn't exist
    var apntag = apntag || {};
    //create a queue on the apntag object        
    apntag.anq = apntag.anq || [];
	
	//load ast.js - async
    (function() {
       var d = document, e = d.createElement('script'), p = d.getElementsByTagName('head')[0];
       e.type = 'text/javascript';  e.async = true;
       e.src = '//acdn.adnxs.com/ast/ast.js';
       p.insertBefore(e, p.firstChild);
    })();

    //push commands to loading queue, to allow for async loading
    apntag.anq.push(function() {

        //set global page options
        apntag.setPageOpts({
            member: 13382, 
            keywords: {
                
            },
            disablePsa: true
        });

        apntag.defineTag({
            tagId: 23441444,
            targetId: 'apn_ad_habillage',
            sizes: [[1800, 1000]]
        });

        apntag.defineTag({
            tagId: 23441877,
            sizes: [[300,600],[300,250],[160,600]],
            targetId: 'apn_ad_atf'
        });

        apntag.defineTag({
            targetId: 'apn_ad_video',
            tagId: 23442327,
            sizes: [1, 1],
            allowedFormats: ['video'],
            targetingParams: {},
            rendererOptions: {
              "playerTechnology": ["html5","flash"],
              "adText": "Publicité",
              "showMute": true,
              "showVolume": true,
              "showProgressBar": true,
              "autoInitialSize": true,
              "allowFullscreen": true,
              "playVideoVisibleThreshold": 50,
              "sideStream": {
                  "enabled": true,
                  "position": "bottom-left",
                  "xOffset": 0,
                  "yOffset": 0, 
                  "maxPageOverlay": 50,
                  "width": 300,
                  "height": 250
              },
                "skippable": {
                    "videoThreshold": 15,
                    "videoOffset": 20,
                    "skipLocation": "top-left",
                    "skipText": "Fermer la vidéo dans %%TIME%% secondes",
                    "skipButtonText": "FERMER"
                }
            }
        });

        apntag.defineTag({
            tagId: 23442265,
            sizes: [[300,250],[300,600]],
            targetId: 'apn_ad_mtf'
        });

        apntag.defineTag({
            tagId: 23442946,
            sizes: [[320,50],[320,100]],
            targetId: 'apn_ad_1_1'
        });

        //start loading tags
        apntag.loadTags();
    });

    function refreshApnInterval() {
        console.log("APN refreshing...");
        var utils = new Utils();
        var isElementInView = utils.isElementInView($('#apn_ad_atf'), false);
        if(isElementInView) {
            apntag.anq.push(function () {
                apntag.refresh(['apn_ad_atf']);
            });
        }
        isElementInView = utils.isElementInView($('#apn_ad_mtf'), false);
        if(isElementInView) {
            apntag.anq.push(function () {
                apntag.refresh(['apn_ad_mtf']);
            });
        }
        isElementInView = utils.isElementInView($('#apn_ad_video'), false);
        if(isElementInView) {
            apntag.anq.push(function () {
                apntag.refresh(['apn_ad_video']);
            });
        }
        isElementInView = utils.isElementInView($('#apn_ad_habillage'), false);
        if(isElementInView) {
            apntag.anq.push(function () {
                apntag.refresh(['apn_ad_habillage']);
            });
        }    
        isElementInView = utils.isElementInView($('#apn_ad_1_1'), false);
        if(isElementInView) {
            apntag.anq.push(function () {
                apntag.refresh(['apn_ad_1_1']);
            });
        }      
    }

    var tId;
    function initReload() { 
        clearInterval(tId);
        tId = setInterval(refreshApnInterval, 20000);
    }

    function onBlur() {
        clearInterval(tId);
    };
    function onFocus() {
        initReload();
    };

    if (/*@cc_on!@*/false) { // check if it is Internet Explorer the double c on is jsript that only ie has
        document.onfocusin = onFocus;
        document.onfocusout = onBlur;
    } else {
        window.onload=window.onfocus = onFocus;
        window.onblur = onBlur;
    }

    initReload();

    </script>

