            if(!mobileCheck()){
              apntag.anq.push(function() {
                  apntag.showTag('apn_ad_habillage');
                  apntag.onEvent('adLoaded', 'apn_ad_habillage', function(adLoaded, adObj){
                    var iframe = document.querySelector("#apn_ad_habillage>#div_utif_apn_ad_habillage>iframe")
                    if (iframe.contentWindow.document.querySelector("img") && iframe.contentWindow.document.querySelector("img").width === 1800) {
                      document.getElementById("verif_conteneur").setAttribute("style", "top: 345px; margin: auto; z-index: 11;width: 1000px !important;");
                      document.getElementById("apn_ad_habillage").setAttribute("style", "top:125px;position:fixed;width:1800px;margin:0;height:1000px;border:0;padding:0;left:50%;margin-left:-900px;");
                    }
                  });
              });
            }
    </script>
    
