				var getHref = function (event) {
					var attribute = this.getAttribute("data-obf");
					document.location.href = decodeURIComponent(window.atob(attribute));
				};
				var getHrefBlank = function (event) {
					var attribute = this.getAttribute("data-obf");
					window.open(decodeURIComponent(window.atob(attribute)),'_blank');
				};
				
				document.addEventListener("DOMContentLoaded", function (event) {
					var classnameBlank = document.getElementsByClassName("obf-blank");
					for (var i = 0; i < classnameBlank.length; i++) {
						classnameBlank[i].addEventListener('click', getHrefBlank, false);
					}
					var classname = document.getElementsByClassName("obf");
					for (var i = 0; i < classname.length; i++) {
						classname[i].addEventListener('click', getHref, false);
					}
				});
			</script>

			 
