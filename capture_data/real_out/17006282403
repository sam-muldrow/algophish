            jQuery('#language-toggle').change(function($) {
                if (this.checked) {
                    window.location.href = "https://itc.ua/";
                    setCookie("lang_1", "ru", 365, '/');
                } else {
                    window.location.href = "https://itc.ua/ua/";
                    setCookie("lang_1", "uk", 365, '/');
                }
            });
