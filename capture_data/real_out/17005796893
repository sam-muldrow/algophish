		<script>
			(function() {
				const banner = document.querySelector('#eu-banner');
				const ph = document.querySelector('#eu-banner-placeholder');
				window.addEventListener('scroll', function(e) {
					banner.style.position = ph.getBoundingClientRect().top < window.innerHeight
						? 'absolute'
						: 'fixed';
				});
			})();
			$(document).ready(function () {
		  		var adFix=function(o){
		  			if (window.screen.availWidth > 1023) {
					  	o.each(function(o,i){
					  		var topOffset = $(i).offset().top;
					  		$(window).scroll(function(o){
					  			var y = $(window).scrollTop();

					  			if (y > topOffset) {
					  				$(i).css({
						                'top' : 0,   
						                'position': 'fixed'
									});
					  			} else {
					  				$(i).css({
						                'position': 'relative'
									});
					  			}
				  		})});
					}
				}

		  		adFix($(".__scroll"));
		  	});		  	
		</script>
	

