		},
	];

	netIdEvents.forEach((netIdEvent) => {
		window.addEventListener(netIdEvent.name, () => {
			window.dataLayer?.push({
				event: "ga_event",
				category: "netID",
				action: netIdEvent.action,
				label: netIdEvent.label,
				nonInteraction: netIdEvent.isNonInteraction,
			});
		});
	});

    window.onload = () => {
        injectNetIdConsentScript();
    };
</script>
