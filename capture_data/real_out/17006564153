	const netIdConsentScriptUrl = "https://te-static.technical-service.net/netid/kochbar.min.js";

	function injectNetIdConsentScript() {
		const script = document.createElement("script");
		script.async = true;
		script.src = netIdConsentScriptUrl;

		document.body.append(script);
	}

	const netIdEvents = [
		{
			name: "netid-loaded",
