    window._sp_ = {"config":{"accountId":1239,"baseEndpoint":"https:\/\/sourcepoint.kochbar.de"}};

    window.tcfString = '';

    var sourcepointProcessor = (function() {

      var maxPlayerTimeout = 2000;
      var acReady = false;

      
      function processConsentReady (tcData) {
		if (!tcData || tcData.eventStatus === 'cmpuishown') return;

                // If the user saved the (new) consent, we reload the page to start over fresh
        if (window.__TRIGGER_RELOAD__ && tcData && tcData.eventStatus === 'useractioncomplete') {
          window.__TRIGGER_RELOAD__ = false;
          window.setTimeout(function() { window.location.reload(); }, 150);
        }
                // Store the tcfString for the videoPlayer
        window.tcfString = tcData.tcString;
		window.dispatchEvent(new Event("tcfReady"));
        injectOutbrainScript();
        tryTriggeringPlayer();
      };

      function processAdControlReady () {
        acReady = true;
        tryTriggeringPlayer();
      };

      function tryTriggeringPlayer() {
        // Both preconditions have to be met
        if (window.tcfString === '' || acReady === false) {
          return;
        }
        // Trigger the player
        triggerPlayer();
      };

      function triggerPlayer (wasFallback) {
        // Fire a window-event that the player can listen on
        var initEvent = document.createEvent('Event');
        initEvent.initEvent('initCBCPlayer');
        window.dispatchEvent(initEvent);
        // Track on GA to see how often this happened
        // @todo discuss with lr
        //if (wasFallback && ga) {
        //  ga('event', 'ipFallbackFired', 'ads', maxPlayerTimeout)
        //}
      };

      function injectOutbrainScript() {
        if(window.outbrainIsInjected === true) {
          return;
        }

        //No Outbrain on content_specials
        if(window.dataLayer && window.dataLayer[0] && window.dataLayer[0].content_special !== false) {
          return;
        }

        var scriptElement = document.createElement("SCRIPT");
        scriptElement.setAttribute("src", "https://widgets.outbrain.com/outbrain.js");
        scriptElement.setAttribute("async", "true");

        document.body.appendChild(scriptElement);

        setTimeout(function(){
          buildOutbrainKoopTeaser();
          window.outbrainIsInjected = true;
        }, 500);
      }

      function buildOutbrainKoopTeaser() {
        var koopTeaser = document.getElementById("koop_teaser");
        if (!koopTeaser) {
          return;
        }
        var template = window.google_tag_manager["GTM-NG2ZP8D"].dataLayer.get("env_template") || "";
        var obId = template == "home" ? "SF_1" : template == "recipe" ? "SF_2" : template == "search" ? "SF_3" : "";
        if (obId === "") {
          return;
        }
        var teaser = document.createElement("div");
        teaser.classList.add("OUTBRAIN");
        teaser.setAttribute(
          "data-src",
          window.location.href.replace(/\?.*/g, "")
        );
        teaser.setAttribute("data-widget-id", obId);
        teaser.setAttribute("data-ob-template", "DE_Kochbar.de");
        koopTeaser.appendChild(teaser);
        window.setTimeout(function () {
          if (window.OBR) {
            // Notify Outbrain about the new div
            window.OBR.extern.renderSpaWidgets(window.location.href.replace(/\?.*/g, ""));
          }
        }, 0);
      }

      return {
        addEventListeners: function() {
          window.__tcfapi('addEventListener', 2, processConsentReady);
                    window.addEventListener('AC_player_start', processAdControlReady);
          window.setTimeout(triggerPlayer, maxPlayerTimeout, true);
        }
      };

    })();

    sourcepointProcessor.addEventListeners();
</script>
