<script type="text/javascript">
$(function() {
    var topBtn = $('#campaignFix'); 
    topBtn.hide();
    $(window).scroll(function () {
        var wnd_h = $(window).height();
        var scr_top = $(window).scrollTop();
        var doc_h = $(document).height();
        if (scr_top > 100 && scr_top < doc_h - wnd_h - 100) {
            topBtn.fadeIn();
        } else {
            topBtn.fadeOut();
        }
    });
});
</script>

