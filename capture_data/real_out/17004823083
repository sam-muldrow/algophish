<script type="text/javascript">
					
						$(document).ready(function () {
							initAutocompleteLocalisation('loc_libelle', 'loc_codeinsee', 'loc_codepostal', 'loc_pays');
						});
						function submitForm(event) {
							$('#selectCategorie').remove('error');
							$('#loc_libelle').remove('error');
							error = false;
							if($('#choixFamille').val() == "") {
								$('#selectCategorie').addClass('error');
								error = true;
							}
							/*
							if($('#loc_codeinsee').val() == "" && $('#loc_codepostal').val() == "" && $('#loc_pays').val() == "") {
								$('#loc_libelle').addClass('error');
								error = true;
							}
							*/
							if(!error) {
								choixFamille = $('#choixFamille').val();
								idChaine = choixFamille.substr(0, 1);
								if($('#loc_pays').val() == '') $('#loc_pays').val('FR')
								if(idChaine == 'H') {
									url = '/location-vacances/annoncevacancesfo/liste/listeAnnonces?dt=tt&tyhApp=on&tyhGt=on&tyhMsn=on&tyhMH=on&tyhChl=on&tyhHtl=on&tyhCA=on&tyhHH=on&tp=ofr';
									url = url + '&pa='+$('#loc_pays').val()+'&lo='+$('#loc_codepostal').val()+'&codeINSEE='+$('#loc_codeinsee').val();
								} else if (idChaine == 'V' || choixFamille == 'T_VVU00000' || choixFamille == 'T_VLO00000') {
									url = '/auto-moto/listefo/default/default?r='+choixFamille.replace('T_', '')+'&r1=&trub=';
									url = url + '&pa='+$('#loc_pays').val()+'&lo='+$('#loc_codepostal').val()+'&codeINSEE='+$('#loc_codeinsee').val();
                                    url = url + '&ray=15';
                                } else if (idChaine == 'I' || idChaine == 'T') {
									choixFamille = choixFamille.replace('T_', '');
									if(choixFamille == 'IVH00000') {
										url = '/immobilier/annonceimmofo/liste/listeAnnonces?tt=1&at=1&nbp0=99';
									} else if (choixFamille == 'ILH00000') {
										url = '/immobilier/annonceimmofo/liste/listeAnnonces?tt=5&at=1&nbp0=99';
									} else if (choixFamille == 'ILHCO000') {
										url = '/immobilier/annonceimmofo/liste/listeAnnonces?tt=6&tbDmd=1&tbOff=1&at=1&nbp0=99';
									} else if (choixFamille == 'IVHVG000') {
										url = '/immobilier/annonceimmofo/liste/listeAnnonces?tt=1&tbVia=1&at=1&nbp0=99';
									} else if (choixFamille == 'IIP00000' || choixFamille == 'TIP00000') {
										url = '/animmos/listefo/default/default?rfam=TIP00000&choixRubrique=';
									} else if (choixFamille == 'IRC00000' || choixFamille == 'TRC00000') {
										url = '/animmos/listefo/default/default?rfam=TRC00000&choixRubrique=';
									} else if (choixFamille == 'ICE00000' || choixFamille == 'TCE00000') {
										url = '/animmos/listefo/default/default?rfam=TCE00000&choixRubrique=';
									} else if (choixFamille == 'IFR00000' || choixFamille == 'TFR00000') {
										url = '/animmos/listefo/default/default?rfam=TFR00000&choixRubrique=';
									} else if (choixFamille == 'TMP00000') {
										url = '/animmos/listefo/default/default?rfam=TMP00000&choixRubrique=';
									}
									url = url + '&pa='+$('#loc_pays').val()+'&lo='+$('#loc_codepostal').val()+'&codeINSEE='+$('#loc_codeinsee').val();
								} else if (idChaine == 'B') {
									if(choixFamille == 'BEV00000') {
										url = '/geo/annonces/evenement/?r=BEV00000';
									} else {
										url = '/mondebarras/listefo/default/default/?r='+choixFamille+'';
									}
									url = url + '&pa=' + $('#loc_pays').val() + '&lo=' + $('#loc_codepostal').val() + '&codeinsee=' + $('#loc_codeinsee').val();
                                    url = url + '&ray=50';
								} else if (idChaine == 'A') {
									url = '/animmos/listefo/default/default?rfam='+choixFamille+'&chaine=A';
									url = url + '&pa=' + $('#loc_pays').val() + '&lo=' + $('#loc_codepostal').val() + '&codeINSEE=' + $('#loc_codeinsee').val();
                                    url = url + '&ray=15';
								} else if (idChaine == 'L') {
									url = '/animmos/listefo/default/default?chaine=L&rfam='+choixFamille+'';
									url = url + '&pa=' + $('#loc_pays').val() + '&lo=' + $('#loc_codepostal').val() + '&codeINSEE=' + $('#loc_codeinsee').val();
                                    url = url + '&ray=15';
								} else if (idChaine == 'E') {
									url = '/emploi/listefo/default/default?ray=15';
									url = url + '&pa=' + $('#loc_pays').val() + '&lo=' + $('#loc_codepostal').val() + '&codeinsee=' + $('#loc_codeinsee').val();
								} else {
									error = false;
								}
								if(url) $(location).attr('href', url);
							}
							event.preventDefault();
							return false;
						}
					
				</script>
