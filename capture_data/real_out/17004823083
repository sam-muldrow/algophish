<script type="text/javascript">
var tabobflink = document.getElementsByClassName('obflink');
for (i = 0; i < tabobflink.length; i++) {
    var t = tabobflink[i];
    t.addEventListener('click', gestionObFLink);
}
function gestionObFLink(event) {
    var link = "";
    if(event.currentTarget.getAttribute('data-a') != null) {
        link = atob(event.currentTarget.getAttribute('data-a')); // On décode l'url d'une balise <a>
    } else {
        link = atob(event.currentTarget.getAttribute('data-o')); // On décode l'url d'une balise non <a>
    }
    link = link.replace(/&amp;/g, "&");
    if(event.currentTarget.getAttribute('data-w') == 1) {
        location.href=link;
    } else {
        window.open(link); // On renvoie l'utilisateur vers la page
    }
}

</script>
