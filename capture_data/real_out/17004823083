<section class="min-h-100 relative bg-grey-50 bg-no-repeat bg-center-top bg-cover flex flex-col justify-center items-center md:py-8" style="background-image: url('/communfo/img/structuresite/home/fond11-hp.jpg');">
<h1 class="text-4xl font-semibold text-center md:text-lg">Les petites annonces, proches de chez vous
</h1>
<span class="font-semibold text-center text-3xl md:text-lg">
<span class="relative px-px pb-0.5 bg-gradient-to-t txt-rotate">61 049 appartements167 835 véhicules422 casques audio131 535 hébergements de vacances258 garde meubles212 442 jobs781 742 bons plans11 874 logements étudiants584 lave-vaisselles10 963 animaux de compagnie7 250 ordinateurs442 678 biens immobiliers511 scooters7 175 canapés</span>
<span class="md:clear-both md:block">à portée de clic !</span>
</span>
<script type="text/javascript">
    const words = [ "61 049 appartements","167 835 véhicules","422 casques audio","131 535 hébergements de vacances","258 garde meubles","212 442 jobs","781 742 bons plans","11 874 logements étudiants","584 lave-vaisselles","10 963 animaux de compagnie","7 250 ordinateurs","442 678 biens immobiliers","511 scooters","7 175 canapés" ];
    const nbLibelles = 14
    
        text = document.querySelector(".txt-rotate");
    // Generator (iterate from 0-3)
    function* generator() {
        var index = 0;
        while (true) {
            yield index++;

            if (index > nbLibelles-1) {
                index = 0;
            }
        }
    }
    // Printing effect
    function printChar(word) {
        let i = 0;
        text.innerHTML = "";
        let id = setInterval(() => {
            if (i >= word.length) {
                setTimeout(function(){deleteChar()},2000);
                clearInterval(id);
            } else {
                text.innerHTML += word[i];
                i++;
            }
        }, 100);
    }
    // Deleting effect
    function deleteChar() {
        let word = text.innerHTML;
        let i = word.length - 1;
        let id = setInterval(() => {
            if (i >= 0) {
                text.innerHTML = text.innerHTML.substring(0, text.innerHTML.length - 1);
                i--;
            } else {
                printChar(words[gen.next().value]);
                clearInterval(id);
            }
        }, 40);
    }
    // Initializing generator
    let gen = generator();
    printChar(words[gen.next().value]);
    
</script>
