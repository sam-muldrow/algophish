		<div class="OUTBRAIN" data-src="////live.vodafone.de/"
		data-widget-id="AR_2"></div>
	<script>
		if (GujAd !== undefined && GujAd.cmd !== undefined) {
			GujAd.cmd.push(function () {
				GujAd.registerPrivacyConsentHandler(function () {
					if (GujAd.Privacy.Choices.Marketing) {
						var s = document.createElement('script');
						s.type = 'text/javascript';
						s.async = 'true';
						s.src = "//widgets.outbrain.com/outbrain.js";
						document.body.appendChild(s);
					}
				});
			});
		}
	</script>

		<div
		class="gujAd superbanner"
		id="superbanner_2">
		<script>
			if (GujAd !== undefined && GujAd.cmd !== undefined) {
				GujAd.cmd.push(function () {

					// As requested not to load Skyscraper & Wallpaper ads in smaller screens
					// Will only push the Skyscrapper & Wallpaper if the Screen is 4-column screen
					if ('superbanner_2'.includes('skyscraper') || 'superbanner_2'.includes('wallpaper')) {
						/* innerWidth is 1666 as this needs to be rendered when screen is 4-colum,
						 which is 1366 and there's at least 300px space available for the ad */
						if (window.innerWidth >= 1666) {
							GujAd.push('superbanner_2');
						}
					}

					// Integrating Maxi ads ONLY in 3-column & 4-column (and not registering them if it's tablet 2-column)
					if ('superbanner_2'.includes('maxi')) {
						if (window.innerWidth >= 1024) {
							GujAd.push('superbanner_2');
						}
					}

					// Integrating halfpagead ONLY in 3-column screens
					if ('superbanner_2'.includes('halfpagead')){
						if (window.innerWidth >= 1024 && window.innerWidth < 1366) {
							GujAd.push('superbanner_2');
						}
						GujAd.registerAdPositionLoadedHandler (function (pos) {
                            if (pos.containsAd() && pos._name=='halfpagead_1') {
                                // contains advertising
								//halfpagead_1 returns only 300x600px banners
								document.querySelector('.gujAd.halfpagead').classList.add('d-lg-block');
								document.querySelector('.gujAd.halfpagead').classList.add('row-span-2');
								document.querySelectorAll('.card-overlapped.card-overlapped--big').forEach(e => e.classList.add('halfpageAd_hero'));
								document.querySelectorAll('.halfpagead_content').forEach(e => e.classList.remove('d-lg-flex'));
                            }else if (pos.containsAd() && pos._name.includes('halfpagead')) {
                                // contains advertising
								document.querySelector('.gujAd.halfpagead').classList.add('d-lg-block');
								//if halfpagead returns medium rectangle show one content slot
                                if (pos.ad.adHeight == 250) {
                                    document.querySelector('.halfpagead_content').classList.remove('d-lg-flex');

                                } else if (pos.ad.adHeight == 600) {
                                    //if halfpagead returns 300X600 ad we need to adjust the grid view
                                    document.querySelector('.gujAd.halfpagead').classList.add('row-span-2');
									document.querySelectorAll('.card-overlapped.card-overlapped--big').forEach(e => e.classList.add('halfpageAd_hero'));
									document.querySelectorAll('.halfpagead_content').forEach(e => e.classList.remove('d-lg-flex'));
								}
                            }
                        });
					}
					else {
						GujAd.push('superbanner_2');
					}
					GujAd.registerAllAdsLoadedHandler(handleAds);
				});
			}
		</script>
	</div>

