                        <script>
                            var searchIcon = document.getElementById('search-icon');
                            var searchBar = document.querySelector('.search-bar');
                    
                            searchIcon.addEventListener('click', function() {
                                searchIcon.style.display = 'none';
                                searchBar.classList.add('expanded');
                                var searchIconInsideBar = document.createElement('i');
                                searchIconInsideBar.classList.add('icon-search', 'eventToTrack');
                                searchBar.insertBefore(searchIconInsideBar, searchBar.firstChild);
                                // Add this line to focus the search bar
                                searchBar.focus();
                            });
                        </script>
                    
                    <a href="/login"
                         target="_self"  id="header__email" class="d-flex 
                        ">
                        <i class=" icon-email d-flex mr-l mr-md-x"></i>
                    </a>
                    <a href="https://login.vodafonemail.de/sso/login"
                         target="_self"  id="header__email__loggedin" class="d-none
                        ">
                        <i class=" icon-email d-flex mr-l mr-md-x"></i>
                    </a>
