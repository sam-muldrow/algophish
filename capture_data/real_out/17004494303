	<input type="hidden" id="concon-track-file" value="">	
	<script>
		var GujAd = GujAd || {};
		GujAd.cmd = GujAd.cmd || [];
		if (GujAd !== undefined && GujAd.cmd !== undefined) {
			GujAd.cmd.push(function () {
				if ('true' === 'true' || 'false' === 'true') {
					GujAd.getClient().setSiteType('desktop');
				}
				else {
					GujAd.getClient().setSiteType('mobile');
				}
	
				if ('') {
					GujAd.getClient().setZone('');
				}
				else {
					GujAd.getClient().setZone('homepage');
				}
	
				if ('') {
					GujAd.getClient().setIsIndexPage('');
				}
				else {
					GujAd.getClient().setIsIndexPage('true');
				}
				GujAd.registerPrivacyConsentHandler(function () {
						if (GujAd.Privacy.Choices.Analysis) {
							executeConConTrackFile();
						}
					});	
			});
		}
		
		var executeConConTrackFile = () => {
			$.getScript(document.getElementById("concon-track-file").getAttribute("value"));
		}
	</script>
	<meta name="google-site-verification" content="dpQ_opzHZoyCcFXIpfA0dqpprmDMfFQLovSgqZXqBzg" />

	
