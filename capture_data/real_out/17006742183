                            var img_url, vd_player_vdurl, vd_go_url,cookie_code;

                            function openvideo_ad() {
                                var tempcookie_code ="";
                                if (cookie_code == null || cookie_code == "") {
                                    tempcookie_code = "top_vd_ad";
                                } else {
                                    tempcookie_code = cookie_code;
                                }
                                var cookie = _getCookie(cookie_code);
                                if (cookie == null || cookie == "") {
                                    $("#mp_amg").show();
                                    $("#biloboard_dfp").hide();
                                     
                                    VDAD_Init(img_url, tempcookie_code);
                                    VDAD_Show();
                                    $(".ad-Section.top").find('.adBox.tp-1').addClass('noH'); // ì¶ê°
                                    $("#video_btn_close").html("ë«ê¸°");
                                    $("#video_btn_open").html("í¼ì³ë³´ê¸°");
                                }
                            }
                            function openvideo_ad_click() {
                                    $("#mp_amg").show();
                                    $("#biloboard_dfp").hide();
                                    VDAD_Init(img_url);
                                    VDAD_Show();
                                    $(".ad-Section.top").find('.adBox.tp-1').addClass('noH'); // ì¶ê°
                                    $("#video_btn_close").html("ë«ê¸°");
                                    $("#video_btn_open").html("í¼ì³ë³´ê¸°");
                            }
                            function openvideo_ad2() {
                                var tempcookie_code = "";
                                if (cookie_code == null || cookie_code == "") {
                                    tempcookie_code = "top_vd_ad";
                                } else {
                                    tempcookie_code = cookie_code;
                                }
                                var cookie = _getCookie(cookie_code); 
                                if (cookie == null || cookie == "") {
                                    $("#mp_amg").show();
                                    $("#biloboard_dfp").hide();
                                    VDAD_Init_youtube(tempcookie_code);
                                    VDAD_Show_youtube(vd_player_vdurl);
                                    $(".ad-Section.top").find('.adBox.tp-1').addClass('noH'); // ì¶ê°
                                    $("#video_btn_close").html("ë«ê¸°");
                                    $("#video_btn_open").html("í¼ì³ë³´ê¸°");
                                }
                            }

                            function openvideo_ad2_click() {

                                    $("#mp_amg").show();
                                    $("#biloboard_dfp").hide();
                                    VDAD_Init_youtube();
                                    VDAD_Show_youtube(vd_player_vdurl);
                                    $(".ad-Section.top").find('.adBox.tp-1').addClass('noH'); // ì¶ê°
                                    $("#video_btn_close").html("ë«ê¸°");
                                    $("#video_btn_open").html("í¼ì³ë³´ê¸°");
                            }
                            function _getCookie(name) {
                                var arg = name + "=";
                                var alen = arg.length;
                                var clen = document.cookie.length;
                                var i = 0;
                                while (i < clen) {
                                    var j = i + alen;
                                    if (document.cookie.substring(i, j) == arg)
                                        return _getCookieVal(j);
                                    i = document.cookie.indexOf(" ", i) + 1;
                                    if (i == 0) break;
                                }
                                return null;
                            }

                            function _getCookieVal(offset) {
                                var endstr = document.cookie.indexOf(";", offset);
                                if (endstr == -1)
                                    endstr = document.cookie.length;
                                return unescape(document.cookie.substring(offset, endstr));
                            }
                            function _setCookiedays(name, value, expiredays) {
                                var now = new Date(); now.setTime(now.getTime() + (86400 * 30));
                                document.cookie = name + "=" + escape(value) + "; path=/; expires=" + now.toUTCString() + ";"
                            }
</script> 
