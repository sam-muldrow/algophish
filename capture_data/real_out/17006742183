    function setBigdayAds() {

    // 220502 ë«ê¸° ì¿ í¤ì²´í¬
    if(getCookie("top_ad_0114_v1") == 'closed') return;

    var width = $(window).width();
    if(width < 1023){
    $("#bigdayModal").hide();
    $("#bigdayModal").removeClass("show");
    $(".modal-backdrop").hide();
    }else{
    $("#bigdayModal").addClass("show");
    $("#bigdayModal").show();
    $(".modal-backdrop").css("display","block");
    $(".modal-backdrop").show();
    }
    }
    $(window).on('load', function () {
		if( $('#bigdayModal').length > 0 )       
		{
		  setBigdayAds();
		}
    });
    $(window).on('resize', function () {
    		if( $('#bigdayModal').length > 0 )       
		{
		  setBigdayAds();
		}
    });
