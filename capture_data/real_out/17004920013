<script type="text/javascript">
    (runinit = window.runinit || []).push(function () {
        var igoc = 1;
        function autoChangeGocnhin() {
            $('.goc-nhin-dai-su').hide();
            $('.goc-nhin-dai-su').removeClass('hidden');
            $('.goc-nhin-dai-su[data-pos="' + igoc + '"]').fadeIn();
            igoc++;
            if (igoc > $('.goc-nhin-dai-su').length) igoc = 1;
            setTimeout(autoChangeGocnhin, 5000);
        }

        autoChangeGocnhin();

        var ipos = 1;
        function autoChangeHeadRightItem() {
            $('.head-right-item').hide();
            $('.head-right-item').removeClass('hidden');
            $('.head-right-item[data-pos="' + ipos + '"]').fadeIn();
            ipos++;
            if (ipos > $('.head-right-item').length) ipos = 1;
            setTimeout(autoChangeHeadRightItem, 5000);
        }

        autoChangeHeadRightItem();
    });
</script>




