    <script>
        (runinit = window.runinit || []).push(function () {
            hslTimeline.url = '/timelinenew/trang-{0}.htm';
            hslTimeline.zId = 0;
            hslTimeline.init();

            function addAds(ele, nthItem, zone) {
                var z = '<zone id=admzone' + zone + '></zone>';
                var s = document.createElement("script");
                s.type = "text/javascript";
                s.src = "//static.contineljs.com/cms/arf-" + zone + ".min.js";
                var p = $('<div class="adstimeline">' + z + '</div>');
                p.append(s);
                //$.when($(ele).find('.daily-hot-news').eq(nthItem).after(p)).done(function () {
                //    p.removeClass('notads');
                //});
                $(ele).children('.item').eq(nthItem).after(p);
            }
            var arrZoneAds = ['32591'];
            function checkAds(ele) {
                var j = 8;
                for (var i = 0; i < arrZoneAds.length; i++) {
                    if (!$(ele).children('.item').eq(j).next('.adstimeline').length) {
                        addAds(ele, j, arrZoneAds[i]);
                    }
                    j = j + 5;
                }
                var check = $(ele).find('.adstimeline').length;
                var timerCheckAds = setTimeout(function () {
                    checkAds(ele);
                }, 100);
                //console.log(check);
                if (check == arrZoneAds.length)
                    clearTimeout(timerCheckAds);
            }
            checkAds('.timeline .list_shnews-mid');

            btnHomeSticky.wrapper = 1160;
            btnHomeSticky.init();

            sticky.init();

            $('.news-update .news-update-li').each(function () {
                var src = $(this).find('img').attr('src');
                if (src == null || src == undefined || src == '') {
                    $(this).find('i').remove();
                }
            });

            var swiper = new Swiper('.exchange-rates', {
                slidesPerView: 'auto',
                pagination: '.swiper-pagination'
            });

            videoHD.init('.video-player.videoinit', {
                type: videoHD.videoType.homeVideo,
                useBg: false
            });

