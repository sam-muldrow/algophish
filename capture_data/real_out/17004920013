    .hotnews-item.hotpr{
        background: #F4F7FB;
    }
    .hotnews-item.hotpr .shnews_total{    padding:0 15px 5px;}
    .hotnews-item.hotpr .sub-title{
        font:normal 12px/20px 'SFPD-SemiBold';    
        font-weight: 600;
        text-transform: uppercase;
        color: #FB6942;
