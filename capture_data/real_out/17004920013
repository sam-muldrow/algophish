<script type="text/javascript">
    (runinit = window.runinit || []).push(function () {
        var $hotnews = $('.list-hotnews .hotnews-item');
        function admDecode(str) {
            return eval("unescape('" + str + "')");
        }
        function imgZoomAdm(n, t, i) {
            return t == 0 || typeof t == "undefined" ? n : n.indexOf("//adi.admicro.vn") != -1 ? (n = n.replace("//adi.admicro.vn", ""),
                String.format("{0}/zoom/{1}_{2}/{3}", "//adi.admicro.vn", t, i, n.trimStart("/"))) : n.indexOf("//cdnstoremedia.com") != -1 ? (n = n.replace("//cdnstoremedia.com", ""),
                    String.format("{0}/zoom/{1}_{2}/{3}", "//cdnstoremedia.com", t, i, n.trimStart("/"))) : n.indexOf(appSettings.imageDomain) != -1 ? n.indexOf("/zoom") != -1 || n.indexOf("/thumb") != -1 ? n : (n = n.replace(appSettings.imageDomain, ""),
                        String.format("{0}/zoom/{1}_{2}/{3}", appSettings.imageDomain, t, i, n.trimStart("/"))) : n
        }
        function covertNumber(n, t) {
            if (n > 999) {
                var i = ("" + n).length
                    , r = Math.pow
                    , t = r(10, t);
                return i -= i % 3,
                    n = Math.round(n * t / r(10, i)) / t + " KMGTPE"[i / 3],
                    n.replace(/\./g, ",")
            }
            return n
        }
        $.ajax({
            url: "https://boxpr.admicro.vn/?zone=534429",
            //dataType: "json",
            success: function (rs) {
                var data = $.parseJSON(
                    rs.replace('[[', '[').replace(']]', ']')
                );
                if (data != null && data.length > 0) {

                    let itemData = data[0].items["420"];
                    let _title = admDecode(itemData.title),
                        _img = imgZoomAdm(admDecode(itemData.image), 270,170);;

                    let $item = $hotnews.last();
                    if ($hotnews.length < 4) {
                        $item = $hotnews.last().clone();
                    } 

                    $item.addClass('hotpr');

                    let $link = $item.find('a'), $thumb = $item.find('img');
                    
                    $link.attr('title', _title);
                    $item.find('a.shnews_title').html(_title);
                    $link.attr('href', admDecode(itemData.click));
                    $thumb.attr('src', _img);
                    $thumb.attr('alt', _title);

                    if (itemData.section) {
                        let subinf = admDecode(itemData.section).split('|');
                        let sectItem = '<a href="' + subinf[1] + '" class="sub-title">' + subinf[0] + '</a>';
                        $item.find('.shnews_total').prepend(sectItem);
                    }
                    if (itemData.viewed) {
                        let viewplus = '<span class="viewplus"><svg width="12" height="12" viewBox="0 0 12 12" fill="none" xmlns="http://www.w3.org/2000/svg" style="vertical-align: text-bottom;"><g clip-path="url(#clip0)"><path fill-rule="evenodd" clip-rule="evenodd" d="M1.07037 6C1.12502 6.09443 1.19746 6.21478 1.28721 6.35374C1.51772 6.71066 1.85908 7.1853 2.30227 7.65803C3.19776 8.61322 4.44975 9.5 5.99999 9.5C7.55024 9.5 8.80223 8.61322 9.69772 7.65803C10.1409 7.1853 10.4823 6.71066 10.7128 6.35374C10.8025 6.21478 10.875 6.09443 10.9296 6C10.875 5.90557 10.8025 5.78522 10.7128 5.64626C10.4823 5.28934 10.1409 4.8147 9.69772 4.34197C8.80223 3.38678 7.55024 2.5 5.99999 2.5C4.44975 2.5 3.19776 3.38678 2.30227 4.34197C1.85908 4.8147 1.51772 5.28934 1.28721 5.64626C1.19746 5.78522 1.12502 5.90557 1.07037 6ZM11.5 6C11.9472 5.77639 11.9471 5.77622 11.947 5.77604L11.9462 5.77443L11.9445 5.77102L11.9388 5.75991C11.934 5.75064 11.9273 5.73766 11.9186 5.72122C11.9012 5.68835 11.876 5.64161 11.843 5.58299C11.7771 5.46582 11.6801 5.30076 11.5528 5.10374C11.299 4.71066 10.9216 4.1853 10.4273 3.65803C9.44775 2.61322 7.94975 1.5 5.99999 1.5C4.05024 1.5 2.55224 2.61322 1.57273 3.65803C1.07842 4.1853 0.701032 4.71066 0.447167 5.10374C0.319925 5.30076 0.222896 5.46582 0.156986 5.58299C0.124012 5.64161 0.09877 5.68835 0.0813766 5.72122C0.0726783 5.73766 0.0659375 5.75064 0.0611689 5.75991L0.0554949 5.77102L0.0537694 5.77443L0.053185 5.7756C0.0530931 5.77578 0.0527864 5.77639 0.499999 6L0.0527864 5.77639C-0.0175955 5.91716 -0.0175955 6.08284 0.0527864 6.22361L0.499999 6C0.0527864 6.22361 0.0526945 6.22342 0.0527864 6.22361L0.0537694 6.22557L0.0554949 6.22898L0.0611689 6.24009C0.0659375 6.24936 0.0726783 6.26234 0.0813766 6.27878C0.09877 6.31165 0.124012 6.35839 0.156986 6.41701C0.222896 6.53418 0.319925 6.69924 0.447167 6.89626C0.701032 7.28934 1.07842 7.8147 1.57273 8.34197C2.55224 9.38678 4.05024 10.5 5.99999 10.5C7.94975 10.5 9.44775 9.38678 10.4273 8.34197C10.9216 7.8147 11.299 7