<script>
$(document).ready(function(){
    var ic21Account = document.cookie.match(new RegExp("(^| )aSr_ic21_user=([^;]*)(;|$)"));
    if(ic21Account){
        var ic_user = ic21Account[2].split("%09");
        var userName = ic_user[0];
        var userID = ic_user[1];

        if (userID) {
//            $('#loginavatar').attr('src','https://uc2.21ic.com/avatar.php?uid='+userID+'&amp;size=small');
            $('.loginavatar').html('<iframe src="//bbs.21ic.com/api.php?mod=member:pms&ext=html&wh=26" width="40" frameborder="0"></iframe>');
            $('.loginname').text(decodeURI(userName));
            $('.s_nav').hide();
            $('.login_after').show();
        }
    }
})

</script>


