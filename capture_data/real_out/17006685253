<script language="javascript">
    var nowTime = new Date();
    var startTime = nowTime.getTime();
    $(window).bind('beforeunload',function(){
        if(document.getElementById('ad_right').style.display !="none"){
            this.doSendMes();
        }else{

        }
    });
function ad_left(){
    document.getElementById('ad_left').style.display="none";
}
function doSendMes(){
    var nowTime = new Date();
    var endTime = nowTime.getTime();
    var nowSecond = endTime - startTime;
    var _videoUrl = $("#7560975").contents().find("video[id='ad_v1']").children("source").attr("src");
    var secondNow = Math.ceil(nowSecond/1000);//总秒
    gtag('event', 'close', {
        'event_category': 'video',
        'event_action':'close',
        'event_label':_videoUrl+'--secondNow',
        'value': secondNow
    });
}
    function ad_right(){
    this.doSendMes();
    document.getElementById('ad_right').style.display="none";
}
function ad_left2(){
    document.getElementById('ad_left2').style.display="none";
}
function ad_right2(){
    document.getElementById('ad_right2').style.display="none";
}
</script>

