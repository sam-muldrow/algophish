<script type="text/javascript">
    function tuisearch() {
        var icsearch_key = document.getElementById( 's_icsearch_key' ).value;
        if ( icsearch_key == '搜索元器件...' ) {
            alert( '请输入器件型号！' )
            document.getElementById( 's_icsearch_key' ).focus();
            return false;
        }
        if ( icsearch_key.length < 3 ) {
            alert( '搜索的器件型号最少为3个字符' )
            document.getElementById( 's_icsearch_key' ).focus();
            return false;
        }
        var url = "//www.21icsearch.com/s_" + icsearch_key + ".html";

        window.open( url );

    };
</script>
