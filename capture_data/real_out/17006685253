					<!--技术子站滚动-->
					<script>
						var Picspeed = 15
						demob2.innerHTML = demob1.innerHTML
										   demob3.innerHTML = demob1.innerHTML
						function Marquee1() {
							/*if(demob2.offsetWidth-demob.scrollLeft<=0)
							demob.scrollLeft-=demob1.offsetWidth
							else{
							demob.scrollLeft++
							}*/
							var width1 = demob1.offsetWidth;
							var width2 = demob2.offsetWidth;
							var left = demob.scrollLeft;

							if ( width2 - left <= 0 )
								demob.scrollLeft -= width1
							else {
								demob.scrollLeft++
							}
						}

						var MyMar1 = setInterval( Marquee1, Picspeed )
						demob.onmouseover = function () {
							clearInterval( MyMar1 )
						}
						demob.onmouseout = function () {
							MyMar1 = setInterval( Marquee1, Picspeed )
						}

					</script>
