		<script type="text/javascript">
		var pages = [];var current_page = 1;var max_pages = 19912 + 1;
		$(function() { 	
		//	if(max_pages > 0) {
			function createPages(start) {
				pages = [];
				var n_pages = 12; var h_pages = n_pages / 2;
				min_page = start - h_pages; max_page = start + h_pages;
				if(max_page > max_pages) { max_page = max_pages; min_page = max_page - n_pages; } else if(min_page < 1) {min_page = 1;max_page = min_page + n_pages;}
				if(min_page < 1) min_page = 1;if(max_page > max_pages) max_page = max_pages;
				for(var i=min_page; i<max_page; i++) { var page = { 'page': i, 'url': '/gorace/{page}'.replace('{page}', i), }; pages.push(page); }
				$('#pagination').html('');$('#paging').tmpl({'pages': pages}).appendTo('#pagination');
			}
			if(max_pages > 12) {
				sld = $('#slider').slider({'min': 1, 'value': current_page, 'max': max_pages - 1, 'step': 10, 'handle': 'square', 'tooltip': 'hide'
				}).on('slide', function(ev) {
					var p = sld.getValue();
					createPages(p);
				}).data('slider');	
			}
			createPages(1);
		//	}
		});
		</script>
