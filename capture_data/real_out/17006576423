			{{each pages}}
			<div {{if page == current_page}}class="btn btn-danger active col-md-1 col-xs-1"{{else}}class="btn col-md-1 col-xs-1"{{/if}} onclick="window.location='${url}'"><a href="${url}">${page}</a></div>
