<script id="comment-tree" type="text/x-jquery-tmpl">
{{if index == 0}}<h4>Komentarze:</h4><p/>{{/if}}
 {{if user_info.domain == username.toLowerCase()}}
    <div class="box pull-right col-lg-${12-level} col-lg-offset-${level} col-md-${12-level} col-md-offset-${level} col-xs-${12-level} col-xs-offset-${level} self-comment">    
{{else contents[content_id] == username.toLowerCase()}}    
    <div class="box pull-right col-lg-${12-level} col-lg-offset-{level} col-md-${12-level} col-md-offset-{level} col-xs-${12-level} col-xs-offset-{level} owner-comment">
{{else}}
  <div class="box pull-right col-lg-${12-level} col-lg-offset-${level} col-md-${12-level} col-md-offset-${level} col-xs-${12-level} col-xs-offset-${level} just-comment">
{{/if}}
      {{if id == 0 }}
      <small>[Komentarz skasowany]</small>
      {{else}}
      <a id="${id}"></a>
          <div class="small">
            <div class="comment-avatar cla-${level}">
               <img src="${avatar_url}"/>
            </div>
            <strong>{{html user_url}}</strong> {{if contents[content_id] == username.toLowerCase()}}&#9998;{{/if}} {{if tag}} <span class="label label-inverse">${tag}</span> {{/if}}<small class="grayv">${created}</small>
            {{if ignored.indexOf(username.toLowerCase()) >= 0}}
              <a class="btn btn-xs btn-default" data-toggle="collapse" data-target="#ci-${id}">użytkownik ignorowany</a>
            {{else plus - minus < -3 }}
              <a class="btn btn-xs btn-default" data-toggle="collapse" data-target="#ci-${id}">poniżej poziomu</a>
            {{/if}}
<div class="voting pull-right">
<span class="minus">${minus}</span> <i class="fa fa-minus-square red"></i>
&nbsp;<span class="plus">${plus}</span> <i class="fa fa-plus-square green"></i>
</div>
                             
<p/>
        </div>
<div id="cmi-${id}"></div>        
        {{if plus - minus < -4 || ignored.indexOf(username.toLowerCase()) >= 0 }}          
          <div class="collapse" id="ci-${id}"><div class="ci">{{html content}}</div></div>
        {{else}}
          <div class="ci text-${id}">{{html content}}</div>
        {{/if}}         
        {{if is_edited == 1 }}
          <span class="label label-inverse">edytowany</span>
        {{/if}}
                                 
      {{/if}}   
      </div>         
      <div class="clearfix" id="comment-holder-${id}"></div>



</script>
