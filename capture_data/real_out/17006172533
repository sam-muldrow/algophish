        <script src="/static/js/jquery.visible.min.js"></script>
        <script type="text/javascript">
                        var current = 1;
                        var count = $('.serial_promo:visible').length;
            var stop = 0;

            $(document).ready(function() {
                $('.serial_info').html($('.c_1').html());
                $('.l_1').show('fast');
            });

            function showInfo(i){
                while (!$('.l_'+i).parent().is(':visible')) {
                    i = i+1;
                    if (i > $('.serial_promo:visible').length) {
                        i = 0;
                    }
                }
                stop = 1;
                current = parseInt(current);
                $('.serial_promo_line').hide();
                $('.serial_info .serial_promo_title a').attr('href', $('.l_'+i).data('url')).text($('.l_'+i).data('title'));
                $('.serial_info .serial_promo_real_title a').attr('href', $('.l_'+i).data('url')).text($('.l_'+i).data('titleoriginal'));
                if ($('.l_'+i).data('update').length > 0) {
                    $('.serial_info .serial_promo_last_change_date').text($('.l_' + i).data('update'));
                    $('.serial_info .serial_promo_last_change').show();
                } else {
                    $('.serial_info .serial_promo_last_change').hide();
                }
                $('.l_'+i).show('fast');
                current = i;
                if(current >= $('.serial_promo:visible').length)
                    current = 0;
            }

            $('.serial_promo').mouseout(function(){
                stop = 0;
            });

            setInterval(change, 5000);

            function change(){
                current = parseInt(current);
                if(stop == 0){
                    next = current + 1;
                    while (!$('.l_'+next).parent().is(':visible')) {
                        next = next+1;
                        if (next > $('.serial_promo:visible').length) {
                            next = 0;
                        }
                    }

                    $('.serial_promo_line').hide();
                    $('.serial_info .serial_promo_title a').attr('href', $('.l_'+next).data('url')).text($('.l_'+next).data('title'));
                    $('.serial_info .serial_promo_real_title a').attr('href', $('.l_'+next).data('url')).text($('.l_'+next).data('titleoriginal'));
                    if ($('.l_'+next).data('update').length > 0) {
                        $('.serial_info .serial_promo_last_change_date').text($('.l_'+next).data('update')).show();
                        $('.serial_info .serial_promo_last_change').show();
                    } else {
                        $('.serial_info .serial_promo_last_change').hide();
                    }

                    $('.l_'+next).show('fast');
                    current = next;
                    if(current >= $('.serial_promo:visible').length) {
                        current = 0;
                    }

                    if (!$('.l_'+next).parent().visible()) {
                        var newpos = $('.promotedInnerWrap').scrollLeft() + $('.l_'+next).parent().width() +2;
                        if (newpos > $('.promotedInnerWrap')[0].scrollWidth || current == 1) {
                            newpos = -1;
                        }
                        if (newpos == -1) {
                            $('.promotedInnerWrap').animate({
                                scrollLeft: 0
                            }, 800);
                        } else {
                            $('.promotedInnerWrap').animate({
                                scrollLeft: newpos
                            }, 800);
                        }

                    }
                    /*
                    if (($('.l_'+next).parent().offset().left + 55) > ($('.promotedInnerWrap').scrollLeft())) {
                        console.log($('.promotedInnerWrap').scrollLeft());
                        console.log($('.l_'+next).parent().width());
                        var scroll = $('.promotedInnerWrap').scrollLeft() + 55;
                        console.log(scroll);
                        $('.promotedInnerWrap').scrollLeft(scroll);
                    }*/


                }
            }
        </script>
