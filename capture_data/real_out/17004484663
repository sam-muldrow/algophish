  function retryJs(tag) {
    var tag_script	= document.createElement("script");
    var category    = tag.getAttribute("data-category");
    tag_script.src	= tag.src;
    tag_script.type	= "text/javascript";
    tag_script.addEventListener("error", function() {
      trackEvent(category, "Falha no carregamento", "73bff82e80ddf17da96a441a490478d8");
    });
    
    document.body.appendChild(tag_script);

    trackEvent(category, "Retentativa de carregamento", "73bff82e80ddf17da96a441a490478d8");
  }
