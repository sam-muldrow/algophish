              <script>
          var rich_media_allow_first_view = 'yes';
          var checkProTraffic = 'no';
          if (jQuery.cookie('richmedia') == 'yes' || rich_media_allow_first_view == 'yes') {
            googletag.cmd.push(function () {
              googletag.defineSlot('/21716583894/EW_Global_Rich_Media_1X1/EW_Global_Homepage_Rich_Media_1X1', [1, 1], 'div-gpt-ad-1533099631474-0').addService(googletag.pubads());
              if (checkProTraffic == 'yes') {
                var isProTraffic = Drupal.behaviors.generaljs.isProTraffic();
                if (isProTraffic == 'yes') {
                  googletag.pubads().setTargeting('prop', isProTraffic);
                }
              }
              googletag.enableServices();
            });
          } else {
            var date = new Date();
            var minutes = 10;
            date.setTime(date.getTime() + (minutes * 60 * 1000));
            jQuery.cookie("richmedia", "yes", {expires: date});
          }

          /*** Event Listener :: Google Ad Manager ***/
          window.addEventListener('message', function (event) {
            var $type = event.data.type;
            if (event.origin === 'https://tpc.googlesyndication.com' || event.origin === 'http://tpc.googlesyndication.com' ||
              event.origin === 'https://securepubads.g.doubleclick.net' || event.origin === 'http://securepubads.g.doubleclick.net') {

              /*** Adhesive ***/
              if ($type === 'adhesive') {
                jQuery('body').append(event.data.message);
              }

              /*** Interstitial ***/
              else if ($type === 'interstitial') {
                jQuery('body').append(event.data.message);
                if (event.data.closeTime > 0) {
                  setTimeout(function () {
                    $container = parent.document.getElementById($type + '_overlay_black');
                    $container.parentNode.removeChild($container);
                  }, event.data.closeTime);
                }
              }

              /*** Interstitial Video ***/
              else if ($type === 'interstitial_video') {
                jQuery('body').append(event.data.message);
                /*** Objects ***/
                var mediaPlayer = parent.document.getElementById($type + '_player');
                var imageMuteUnmute = parent.document.getElementById($type + '_mute_unmute_img');
                var imagePlayPause = parent.document.getElementById($type + '_play_pause_img');
                /*** Init ***/
                if (event.data.closeTime > 0) {
                  setTimeout(function () {
                    $container = parent.document.getElementById($type + '_overlay_black');
                    $container.parentNode.removeChild($container);
                  }, event.data.closeTime);
                }
                /*** Events ***/
                jQuery('#' + $type + '_replay').on('click tap', function () {
                  mediaPlayer.currentTime = 0;
                  mediaPlayer.play();
                });
                jQuery('#' + $type + '_mute_unmute').on('click tap', function () {
                  if (mediaPlayer.muted) {
                    mediaPlayer.muted = false;
                    imageMuteUnmute.setAttribute('src', 'https://dfp.expatwoman.com/DFP/assets/video-player/mute.svg?v=2.0');
                    imageMuteUnmute.setAttribute('alt', 'Mute');
                  }
                  else {
                    mediaPlayer.muted = true;
                    imageMuteUnmute.setAttribute('src', 'https://dfp.expatwoman.com/DFP/assets/video-player/unmute.svg?v=2.0');
                    imageMuteUnmute.setAttribute('alt', 'Unmute');
                  }
                });
                jQuery('#' + $type + '_play_pause').on('click tap', function () {
                  if (mediaPlayer.paused || mediaPlayer.ended) {
                    imagePlayPause.setAttribute('src', 'https://dfp.expatwoman.com/DFP/assets/video-player/pause.svg?v=2.0');
                    imagePlayPause.setAttribute('alt', 'Pause');
                    mediaPlayer.play();
                  }
                  else {
                    imagePlayPause.setAttribute('src', 'https://dfp.expatwoman.com/DFP/assets/video-player/play.svg');
                    imagePlayPause.setAttribute('alt', 'Play');
                    mediaPlayer.pause();
                  }
                });
              }

              /*** Wallpaper Takeover EW Style ***/
              else if ($type === 'wallpaper_ewstyle') {
                var $doc = jQuery(window.parent.document);
                var $body = jQuery('body');
                if ($doc.width() >= 1024) {
                  if (event.data.message) {
                    $body.append(event.data.message);
                  }
                  $doc.find('.region-container-top').each(function () {
                    this.style.setProperty('margin', '0 auto');
                    this.style.setProperty('margin-top', '5px', 'important');
                    this.style.setProperty('margin-bottom', '10px', 'important');
                    this.style.setProperty('height', '500px', 'important');
                    this.style.setProperty('cursor', 'pointer', 'important');
                  });
                  $doc.find('.bodycolright').each(function () {
                    this.style.setProperty('margin-top', '340px', 'important');
                  });
                  $doc.find('.bodyrow').each(function () {
                    this.style.setProperty('margin-top', '-345px', 'important');
                  });
                  if ($doc.width() >= 1024 && $doc.width() < 1100) {
                    $doc.find('.region-container-top').prepend('<img src="' + event.data.imageSrc1024 + '" />');
                    $doc.find('.region-container-top').each(function () {
                      this.style.setProperty('width', '1024px', 'important');
                    });
                  }
                  else if ($doc.width() >= 1100 && $doc.width() < 1200) {
                    $doc.find('.region-container-top').prepend('<img src="' + event.data.imageSrc1100 + '" />');
                    $doc.find('.region-container-top').each(function () {
                      this.style.setProperty('width', '1100px', 'important');
                    });
                  }
                  else {
                    $doc.find('.region-container-top').prepend('<img src="' + event.data.imageSrc1200 + '" />');
                    $doc.find('.region-container-top').each(function () {
                      this.style.setProperty('width', '1200px', 'important');
                    });
                  }
                  $doc.find('.region-container-top').click(function () {
                    window.open(event.data.adClickURL, '_blank');
                  });
                }
              }

              /*** Wallpaper Takeover Standard New ***/
              else if ($type === 'wallpaper_standard') {
                var $doc = jQuery(window.parent.document);
                var $body = jQuery('body');
                if ($doc.width() >= 1200) {
                  if (event.data.message) {
                    $body.append(event.data.message);
                  }
                  var isClickInside = false;
                  var specifiedElement = $doc.find('#body-wrapper');
                  specifiedElement.click(function (e) {
                    isClickInside = true;
                  });
                  $doc.find('body').click(function (e) {
                    if (!isClickInside) {
                      window.open(event.data.adClickURL, '_blank');
                    }
                    else {
                      isClickInside = false;
                    }
                  });
                  $doc.find('#body-wrapper').addClass('iswallpaper');
                  if ($doc.width() > 1400) {
                    $url = 'url("' + event.data.imageSrc1400 + '")';
                    $doc.find('body').css("backgroundImage", $url);
                  }
                  else if ($doc.width() >= 1200 && $doc.width() <= 1400) {
                    $url = 'url("' + event.data.imageSrc1200 + '")';
                    $doc.find('body').css("backgroundImage", $url);
                  }
                  $doc.find('body').css("cursor", "pointer");
                  $doc.find('body').css("backgroundPosition", "center top");
                  $doc.find('body').css("backgroundRepeat", "no-repeat");
                  $doc.find('body').css("backgroundAttachment", "fixed");
                }
              }

              /*** Video Slider ***/
              else if ($type === 'video_slider') {
                var $doc = jQuery(window.parent.document);
                jQuery('body').append(event.data.message);
                /*** Objects ***/
                var mediaPlayer = parent.document.getElementById($type + '_player');
                var imageMuteUnmute = parent.document.getElementById($type + '_mute_unmute_img');
                var imagePlayPause = parent.document.getElementById($type + '_play_pause_img');
                /*** Init ***/
                setTimeout(function () {
                  jQuery('#' + $type + '_container').css({'width': event.data.videoWidth + 'px'});
                  jQuery('#' + $type + '_controls').css({'display': 'block'});
                  jQuery('#' + $type + '_close').css({'display': 'block'});
                }, event.data.showCloseDuration);
                if (event.data.closeTime > 0) {
                  setTimeout(function () {
                    $container = parent.document.getElementById($type + '_container');
                    $container.parentNode.removeChild($container);
                  }, event.data.closeTime);
                }
                /*** Events ***/
                jQuery('#' + $type + '_replay').on('click tap', function () {
                  mediaPlayer.currentTime = 0;
                  mediaPlayer.play();
                });
                jQuery('#' + $type + '_mute_unmute').on('click tap', function () {
                  if (mediaPlayer.muted) {
                    mediaPlayer.muted = false;
                    imageMuteUnmute.setAttribute('src', 'https://dfp.expatwoman.com/DFP/assets/video-player/mute.svg?v=2.0');
                    imageMuteUnmute.setAttribute('alt', 'Mute');
                  }
                  else {
                    mediaPlayer.muted = true;
                    imageMuteUnmute.setAttribute('src', 'https://dfp.expatwoman.com/DFP/assets/video-player/unmute.svg?v=2.0');
                    imageMuteUnmute.setAttribute('alt', 'Unmute');
                  }
                });
                jQuery('#' + $type + '_play_pause').on('click tap', function () {
                  if (mediaPlayer.paused || mediaPlayer.ended) {
                    imagePlayPause.setAttribute('src', 'https://dfp.expatwoman.com/DFP/assets/video-player/pause.svg?v=2.0');
                    imagePlayPause.setAttribute('alt', 'Pause');
                    mediaPlayer.play();
                  }
                  else {
                    imagePlayPause.setAttribute('src', 'https://dfp.expatwoman.com/DFP/assets/video-player/play.svg');
                    imagePlayPause.setAttribute('alt', 'Play');
                    mediaPlayer.pause();
                  }
                });
                mediaPlayer.addEventListener('ended', function () {
                  this.autoplay = false;
                  this.load();
                }, false);
              }
            }
          }, false);
