        function toggleSearchAdvance(element) {
            const parent = element.parentElement;
            let searchAdvance = parent.querySelector('.search-advance');

            if (searchAdvance) {
                searchAdvance.style.display = (searchAdvance.style.display === 'none' || searchAdvance.style.display === '') ? 'flex' : 'none';
            }
