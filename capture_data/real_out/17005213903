<script>
    $(function () {
        getCityData(null)
        $('.navbt').on('mouseover', function () {
            $('.navdetail').removeClass('show').find($('.navdetail').eq($(this).index('.navbt')).addClass('show').css({'margin-left': '10px'}).animate({marginLeft: '0'}))
            $('.navbt').removeClass('onh').find($('.navbt').eq($(this).index('.navbt')).addClass('onh'))

        })
            .mouseout(function () {
                //
            });
    })
    var weburl = "http://api.wap.winshangdata.com/";  //"http://192.168.2.25:8081/"
    function getCityData(city) {
        var data = {cityId: city == null ? 600 : $(city).attr("cityId")};
        $.ajax({
            async: false,
            type: "POST",//请求类型
            url: "http://cq.winshang.com/getCityData",//请求的url
            data: data,
            dataType: "json",//ajax接口（请求url）返回的数据类型
            success: function (result) {
                $("#shopArea1").html(result.shopArea + "平方米/人");
                $("#perCapita1").html(result.perCapita + "个/10万人");
                $("#yiKaiGouWuct1").html(result.yiKaiGouWuct + "个");
                $("#floorRent1").html(result.floorRent + "元/月/平米");
            }
        });
    };
</script>
<script type="text/javascript">
    $(document).ready(function () {
        $(".opfz").mouseover(function () {
            $('.cutlist').show(200);
        })
    });

    function searchNews(e) {
        var evt = window.event || e;
        if (evt.keyCode == 13) {
            //回车后要干的业务代码
            let keyword = $('.keywords').val()
            window.open('http://news.winshang.com/search/'+keyword)
        }
    }

    function searchNews2(e) {
        var evt = window.event || e;
        if (evt.keyCode == 13) {
            //回车后要干的业务代码
            let keyword = $('.keywords2').val()
            window.open('http://news.winshang.com/search/'+keyword)
        }
    }

    function searchxm(e){
        var evt = window.event || e;
        if (evt.keyCode == 13) {
            //回车后要干的业务代码
            let keyword = $('.xmkeywords').val()
            window.open('http://www.winshangdata.com/projectList?value='+keyword)
        }
    }

    function searchpp(e){
        var evt = window.event || e;
        if (evt.keyCode == 13) {
            //回车后要干的业务代码
            let keyword = $('.ppkeywords').val()
            window.open('http://www.winshangdata.com/brandList?value='+keyword)
        }
    }

    function searchdown(e){
        var evt = window.event || e;
        if (evt.keyCode == 13) {
            //回车后要干的业务代码
            let keyword = $('.downkeywords').val()
            window.open('http://down.winshang.com/list.aspx?l1=&l2=&s=&m=&c=&d=&key='+keyword)
        }
    }

    function searchhd(e){
        var evt = window.event || e;
        if (evt.keyCode == 13) {
            //回车后要干的业务代码
            let keyword = $('.hdkeywords').val()
            window.open('https://ysh.winshang.com/pc/attendee/activity/search?title='+keyword)
        }
    }
</script>
