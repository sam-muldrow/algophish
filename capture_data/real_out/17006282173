                  slots_nolazy.push(me);
                }
                
                // any extra setup?
                // yes we do! in object!
                // testing yield object and set target
                /*if (typeof yl !== 'undefined') {
                  if (dfp_yieldlab_settings.hasOwnProperty(slot_name)) {
