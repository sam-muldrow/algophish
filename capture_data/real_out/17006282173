                  }
                  me = googletag.defineSlot('/' + dfp_customer_id + '/' + activeEd + '_' + slot_name, sizes, 'div-gpt-ad-' + activeEd + '-' + slot_name + msf).setTargeting('test', 'lazyload').addService(googletag.pubads());
                  slots_lazy.push(me);
                }	else { // out of page -- do not lazy load
                  // var section_slot_name = ( dfp_section_ads[slot_name] == 'allowed' ) ? section+'_'+slot_name : slot_name;
