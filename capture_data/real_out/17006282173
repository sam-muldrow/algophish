<script>
  $(document).ready(function() {
    //
    $('#cookie_dialog').css('visibility', 'visible');
    //
    $('#ck_settings').click(function() {
      $('#ck_step1').hide();
      $('#ck_step2').show();
    });
    $('#ck_accept').click(function() {
      setCookies('all');
    });
    $('#ck_save').click(function() {
      var opt = '';
      if ($('#ck_ads').prop('checked') && $('#ck_other').prop('checked')) {
        opt = 'all';
      } else if ($('#ck_ads').prop('checked')) {
        opt = 'ads';
      } else if ($('#ck_other').prop('checked')) {
        opt = 'other';
      } else {
        opt = 'no';
      }
      setCookies(opt);
    });

    function setCookies(opt) {
      var dat = new Date(new Date().setFullYear(new Date().getFullYear() + 1));
      var dateString = dat.toUTCString();
      var dat = new Date();
      var dateNow = dat.toISOString();
      var cname = "finance_cookies";
      var cval = "accept=" + opt + "|datetime=" + dateNow + "|js";
      var cookieString = cname + "=" + cval;
      document.cookie = cookieString + ";expires=" + dateString + ";domain=.deutsche-wirtschafts-nachrichten.de;path=/";
      $('#cookie_overlay').remove();
      if (opt == 'all' || opt == 'ads') { // local refresh (try function in App.js-body.local.tpl.php, like _dwn one)
        try {
          reloadAfterCookieConsent();
        } catch (error) {
          //console.error(error); // ignore error
        }
      }
      return false;
    };

  });
</script>      </div>
    </div>
