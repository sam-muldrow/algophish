                      me.setTargeting('yl_id', dfp_yieldlab_settings[slot_name]);
                      console.log('[c0wsky dfp] setTargeting yl_id', me, dfp_yieldlab_settings[slot_name]);
                    }
                  }
                }*/
                var div_settings = Boolean(dfp_slots_settings[slot_name]);
                me.setCollapseEmptyDiv(div_settings);
                //me.setCollapseEmptyDiv(true, true);
                console.log('[c0wsky dfp] Settings for slot:', slot_name, 'value:', dfp_slots_settings[slot_name]);
              } // slots
              //
              // one single listener
      				googletag.pubads().addEventListener('slotRenderEnded', function(event) {
                var did = event.slot.getAdUnitPath();
                var height = 0;
                if (!event.isEmpty) {
                  height = event.size[1];
                  //console.log('[c0wsky dfp] rendered with height: ' + height + ', path: ' + did);
                  didpts = did.split('_');
                  modul = didpts[didpts.length - 1];
                  if (height) {
                    /*if (modul == 'panorama') { // adjust top offset  //// not needed anymore
                      //if (typeof setTopOffset !== "undefined") setTopOffset(height);
                      setTimeout(() => {
                        console.log("prehd ad : scroll top");
                        window.scrollTo({ top: 0 });
                      }, 200);
                    } else*/ if (modul == 'modul1') { // infinite ads within article text: adjust height of their container
                      // prevent height change if ad is not in adcontainer
                      // for now: prevent parent being   id='main-column-inner'
                      var adparent = document.getElementById('div-gpt-ad-' + activeEd + '-modul1').parentNode;
                      //alert(adparent.id);
                      //if (adparent.id != 'main-column-inner') {
                      if (adparent.id == 'adtopart') {
                        adparent.style.height = height + 'px';
                      }
                    }
                  }
                }
      					console.log('[c0wsky dfp] somebody got rendered', did);
      					switch (did) {
      						case '/' + dfp_customer_id + '/' + activeEd + '_predoglas':
      							setTimeout(function() {
      								if (document.getElementById("predoglas-redirect") != null && document.getElementById("predoglas-redirect") != "undefined") {
      									var predoglasRedirectUrl = document.getElementById("predoglas-redirect").href;
      									var myurl = encodeURIComponent(window.location.href);
      									window.location.href = predoglasRedirectUrl + '&url=' + myurl;
      								}
      							}, 3000);
      							var containsAd = !event.isEmpty;
      						break;
      						default:
      							var containsAd = !event.isEmpty;
      					}
      				}); // end of slotRenderEnded
              //

              /*
              googletag.pubads().addEventListener('impressionViewable', function(event) {
                var slot = event.slot;
                console.log('[c0wsky dfp] ad viewable: ' + slot.getSlotElementId());
              });
              */
              
            break; // end of default
          }
          // googletag.pubads().collapseEmptyDivs(true);
          // googletag.companionAds().setRefreshUnfilledSlots(true);
          // googletag.pubads().enableVideoAds();
          //
          //
          //           // MAKE THIS EXCEPTION FOR OUT OF PAGE ADS, TEST IT ON DWN ARTICLE
          //
          // disable initial load to control when ads are requested
          googletag.pubads().disableInitialLoad();
          googletag.pubads().enableSingleRequest();
          googletag.enableServices();
          //
          // issue SRA request for non-lazy (out of page) slots
          googletag.pubads().refresh(slots_nolazy);
          //
          // enable lazy loading AFTER non-lazy request
          googletag.pubads().enableLazyLoad({
            fetchMarginPercent:   50,              renderMarginPercent:  20,              mobileScaling:       2.0             });
          //
          // issue SRA request for lazies
          googletag.pubads().refresh(slots_lazy);
          //
          /* //enable these reports for debugging if needed
          googletag.pubads().addEventListener('slotRequested', function(event) {
            console.log('[c0wsky dfp] ' + event.slot.getSlotElementId() + ' fetched');
          });
          googletag.pubads().addEventListener('slotOnload', function(event) {
            console.log('[c0wsky dfp] ' + event.slot.getSlotElementId() + ' rendered');
          }); */
        }); // push
    
        var adunits = document.querySelectorAll('div[id^="div-gpt-ad-' + activeEd + '-"]');
        var adseen = {};
        for (var i = 0; i < adunits.length; i++) {
          adid = adunits[i].getAttribute('id');
          if (typeof adseen[adid] !== 'undefined') {
            console.log('[c0wsky dfp]: skipped item since already seen: ' + adid);
            var elem = adunits[i];
            elem.parentNode.removeChild(elem);
            continue; // skip this one -- c0w 2022-03-04
          }
          adseen[adid] = true;
          console.log('[c0wsky dfp]: We got item:' + i + '/' + adunits.length + ' and AdSlot:' + adid + ' ... loaded');
          googletag.cmd.push(function() { 
            googletag.display(adid); 
          });
        }
        console.log('[c0wsky dfp]: positions called', adseen);
        //
        dfp_happened = 'dfp active';
        console.log('[c0wsky dfp] dfp_happened from App.dfp.tpl.php: ', dfp_happened);
      } // end of run_dfp

            run_dfp();
      //dfp_happened = 'dfp active';
      
          });
