<script async src="https://semalt.com/js/device.min.js"></script>


<script>
    document.body.onload = setTimeout(addElementCounters,5000);

    function addElementCounters(){
        addGoogleAnalytics();
        addGoogleTag();
        addFacebookPixel();
    }

    function addGoogleAnalytics() {
        var scriptGA = document.createElement('script');
        var codeGA = '  window.dataLayer = window.dataLayer || [];\n' +
            '        function gtag(){dataLayer.push(arguments);}\n' +
            '        gtag(\'js\', new Date());\n' +
            '        gtag(\'config\', \'UA-46334650-1\');';

        scriptGA.appendChild(document.createTextNode(codeGA));
        document.body.appendChild(scriptGA);

        var scrGTag = document.createElement('script');
        scrGTag.src = 'https://www.googletagmanager.com/gtag/js?id=UA-46334650-1';
        scrGTag.async = false;
        document.body.appendChild(scrGTag);

    }
    function addGoogleTag() {
        <!-- Google Tag Manager (noscript) -->
        var iframe = document.createElement('iframe');
        var elNoScript =  document.createElement('noscript');

        iframe.src = 'https://www.googletagmanager.com/ns.html?id=GTM-P8W7SB4';
        iframe.loading = 'lazy';
        iframe.height = 0;
        iframe.width = 0;
        iframe.style = 'display:none;visibility:hidden';

        elNoScript.appendChild(iframe);
        document.body.appendChild(elNoScript);
        <!-- End Google Tag Manager (noscript) -->

    }
    function addFacebookPixel(){
        <!-- Facebook Pixel Code -->
        var scriptFP = document.createElement('script');
        var codeFP = '            !function(f,b,e,v,n,t,s)\n' +
            '            {if(f.fbq)return;n=f.fbq=function(){n.callMethod?\n' +
            '                n.callMethod.apply(n,arguments):n.queue.push(arguments)};\n' +
            '                if(!f._fbq)f._fbq=n;n.push=n;n.loaded=!0;n.version=\'2.0\';\n' +
            '                n.queue=[];t=b.createElement(e);t.async=!0;\n' +
            '                t.src=v;s=b.getElementsByTagName(e)[0];\n' +
            '                s.parentNode.insertBefore(t,s)}(window, document,\'script\',\n' +
            '                \'https://connect.facebook.net/en_US/fbevents.js\');\n' +
            '            fbq(\'init\', \'3050646201836933\');\n' +
            '            fbq(\'track\', \'PageView\');';

        scriptFP.appendChild(document.createTextNode(codeFP));
        document.body.appendChild(scriptFP);

        var imgFP = document.createElement('img');
        var elNoScriptFP =  document.createElement('noscript');
        imgFP.src = 'https://www.facebook.com/tr?id=3050646201836933&ev=PageView&noscript=1';
        imgFP.height = 1;
        imgFP.width = 1;
        elNoScriptFP.appendChild(imgFP);
        document.body.appendChild(elNoScriptFP);
        <!-- End Facebook Pixel Code -->
    }
</script>
<script>
(function(w, d) {
	var s = d.createElement('script');
	d.getElementsByTagName('script')[0].parentNode.appendChild(s);
	s.setAttribute('data-statinside-id', 6);
	s.setAttribute('data-endpoint', 'https://semalt.com/actcntr');
	s.src = 'https://semalt.com/actcntr/counter.js';
	w._siData = w._siData || [];
	w.statInside = function() {_siData.push(arguments);}
	
	statInside('userId', 0);
	
	var path = location.pathname.replace(/^\/[a-z][a-z](\/|$)/, '/');
	if (/^\/project\/\d+$/.test(path)) {
		statInside('page', 'project');
	} else if (/^\/blog(\/|$)$/.test(path)) {
		statInside('page', 'blog');
	}
})(window, document);
</script>
