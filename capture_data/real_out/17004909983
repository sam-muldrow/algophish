                        _this.fnShowToast('è¯·åææ¡æ¬¾')
                        return false;
                    }

                    $('.mask-model').hide();
                    $('.window-model').hide();

                    axios.post('https://payv7.weishoot.com/pay/index/index', {
                            "type": '20',
                            // "pay_type": _this.paykindFlag,
                            // "UCode": _this.wsUCode ? _this.wsUCode : 501,
                            // "Code": _this.getEnglishNameFlag,
                            "pay_type": 11,
                            "UCode": 501,
                            "Code": 'taishanshibapan',
                            "num": 2,
                            'more': JSON.stringify({
                                "coin": 0,
                                "num": 2,
                            })
                        }).then(function (response) {
                            // æ¯ä»å®è´­ä¹°æå
