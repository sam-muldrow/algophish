                trim: function (s) {
                    return s.replace(/(^\s*)|(\s*$)/g, "");
                },
                //è·å url åæ°
                getQueryString(name) {
                    var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
                    var r = window.location.search.substr(1).match(reg);
                    if (r != null) return decodeURI(r[2]);
                    return null;
                },
                //Axios post æ¹æ³
                axiosPost(requestUrl, formData, contentType) {
                    if (contentType == undefined) {
                        contentType = 'application/x-www-form-urlencoded';
                    }
                    return axios({
                        url: requestUrl,
                        method: 'post',
                        data: formData,
                        transformRequest: [function (data) {
                            var ret = ''
                            for (var it in data) {
                                ret += it + '=' + data[it] + '&';
                            }
                            lastAndIndex = ret.lastIndexOf("&");
                            ret = (lastAndIndex > -1) ? ret.substring(0, lastAndIndex) : ret;
                            return ret
                        }],
                        headers: {
                            'Content-Type': contentType
                        }
                    });
                },




