<script>
    //轮播配置
    $(document).ready(function() {
        initmbfunc();
    });
    function initmbfunc(){
        $(".tab_hover li").hover(function(){
            var pobj = $(this).parents(".ecv2_panel_floor:first");
            pobj.find(".tab_hover_content").find(".ecv2_panel_bd").removeClass("on");
            pobj.find(".tab_hover_content").find(".ecv2_panel_bd").eq($(this).index()).addClass("on");
            $(this).parents(".tab_hover:first").find("li").removeClass("on");
            $(this).addClass("on");
        })
    }
</script>
