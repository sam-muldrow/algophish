		<script>
		(function(w, d) {
			var hasBbxLibrary = null;
			var ft = [];
			try {
				var c = BBX.cookies.get('bbxuid');
				ft.push('bc=' + (c ? 'cookie_ok' : 'warn_no_cookie'));
				hasBbxLibrary = true;
			} catch(e) {
				ft.push('bc=fail_bbx_cook');
				ft.push('err=' + encodeURIComponent(e.message));
			}
			try {
				var queuePush = SiteAdvert.Queue.push;
				if (typeof queuePush === 'function') {
					if (queuePush.toString().match(/_sashec_queue\.push/)) {
						ft.push('bq=fail_original');
					} else if (queuePush.toString().match(/\.size/)) {
						ft.push('bq=seems_ok');
					} else {
						ft.push('bq=fail_size_parse_missing');
					}
				} else {
					ft.push('bq=fail_queue_not_func');
				}
			} catch(e) {
				ft.push('bq=fail_queue');
				ft.push('err=' + encodeURIComponent(e.message));
			}
			try {
				ft.push('sat=' + _satellite.getVar('bbxDevice'));
			} catch(e) {
				ft.push('sat=fail_satellite');
				ft.push('err=' + encodeURIComponent(e.message));
			}
			var device = 'x';
			try {
				var s = _sashec.ad_groups.default.options;
				var lt = [
					'sd=' + s.targets.device,
					'st=' + s.targets.template,
					'ss=' + s.site,
					'sa=' + s.area,
					'sb=' + s.targets.ab
				];
				device = s.targets.device;
				ft.push(lt.join('&'));
			} catch(e) {
				ft.push('sd=fail_sashec');
				ft.push('err=' + encodeURIComponent(e.message));
			}
			try {
				ft.push('ie=' + (w.document.documentMode || 'no'));
			} catch (e) {
				ft.push('ie=warn_cannot_detect');
			}
			var noads = false;
			if(!w.bbxtrck.hasBeacon) {
				ft.push('nb=warn_no_navigator_beacon');
			}
			if(!w.bbxtrck.hasPerformance) {
				ft.push('pf=warn_no_performance_api');
			}
			if(!w.bbxtrck.hasPerformanceNow) {
				ft.push('pfn=warn_no_performance_now');
			}
			if (w._virtual_prefix) {
				ft.push('v.prefix=' + encodeURIComponent(w._virtual_prefix));
			}
			ft.push('w.width=' + w.innerWidth);
			ft.push('w.height=' + w.innerHeight);
			w.bbxtrck.hasBbxLibrary = hasBbxLibrary;
			w.bbxtrck.active.resource && w.bbxtrck.track('resource', '&' + ft.join('&'));
			if(w.bbxtrck.active.unload && w.bbxtrck.hasBeacon) {
				var once = true;
				var unload = function() {
					if (!once) {
						return;
					}
					once = false;
					var info = w.bbxtrck.getTrackUri('unload', '&sd=' + device + w.bbxtrck.getAdsDataToSend(true));
					info && navigator.sendBeacon(info);
				};
				w.addEventListener('beforeunload', unload);
				w.addEventListener('unload', unload);
			}
			if (hasBbxLibrary) {
				BBX.ready(function() {
					var stats = {
						sd: device
					};
					var domAppend = '';
					var mapping = {
						'c.lwd': '.graphicswidget'
					}
					try {
							var mappingLists = {
								'c.li': '.small-box--listing, .gallery-box, .swift-box__wrapper, .section-opener, .small-box, .video-box, .third-box',
								'c.lik': '.swift-box__wrapper',
								'c.liv': '.video-box',
								'c.lit': '.third-box',
								'c.lia': '.advert .small-box--listing, .advert .gallery-box, .advert .swift-box__wrapper, .advert .section-opener, .advert .small-box'
							};
							mapping = Object.assign(mapping, mappingLists);
						var hasRecommends = d.querySelector('.related-wrap #recommendations-replace');
						Object.keys(mapping).reduce(function(acc, name) {
							var found = d.querySelectorAll(mapping[name]).length;
							if (name === 'c.rel' && hasRecommends) {
								found--;
							}
							if (found > 0) {
								acc[name] = found;
							}
							return acc;
						}, stats);
						domAppend = new URLSearchParams(stats).toString();
					} catch(e) {
						domAppend = 'sd=' + device + '&err=' + encodeURIComponent(e.message);
					}
					w.bbxtrck.active.dom && w.bbxtrck.track('dom', '&' + domAppend);
				});
			} else {
				w.bbxtrck.active.bbxError && w.bbxtrck.track('bbxError', '&rsn=fail_bbx_lib');
			}
			w.bbxtrck.active.load && w.addEventListener('load', function() {
				w.bbxtrck.track('load', '&sd=' + device + w.bbxtrck.getAdsDataToSend());
			});

			if(w.bbxtrck.active.consentNetwork) {
				w.didomiOnReady = w.didomiOnReady || [];
				w.didomiOnReady.push(function (Didomi) {
					var connection = navigator.connection;
					var cons = ['sd=' + device];
					if (connection) {
						try {
							cons.push('artt=' + (connection.rtt !== undefined ? connection.rtt : 'unkn'));
							cons.push('aetype=' + (connection.effectiveType || 'unkn'));
							cons.push('adown=' + (connection.downlink || 'unkn'));
							cons.push('atype=' + (connection.type || 'unkn'));
						} catch (e) {
							cons.push('err=' + encodeURIComponent(e.message));
						}
					} else {
						cons.push('cnf=no_connection_api');
					}
					w.bbxtrck.track('consentNetwork', '&' + cons.join('&'));
				});
			}
		})(window, document);
		</script>
</head>

