		<script>
		(function(w) {
			var path = 'https://mer.stdout.cz/l.gif?s=pramon&v=1&e=view&';
			var id = "b:vs:aktualnehp", site = "aktualnehp", typ = "listing", is_archive = false;
			var getTime = function() {
				return new Date().getTime();
			};
			var now = getTime();
			var hasPerformance = !!w.performance;
			var hasPerformanceNow = hasPerformance && typeof w.performance.now === 'function';
			var getPerfTime = function() {
				return parseInt(
					hasPerformanceNow
					&& w.performance.now()
					|| (getTime() - now)
					|| 0
				, 10);
			};
			var resource = '&bt=' + typ + '&bs=' + site + '&bi=' + id + '&br=' + (Math.floor(Math.random() * 1e9).toString(36));
			if (is_archive) {
				var offsetReg = location.search.match(/[?&]offset=(\d+)/);
				var offset = (offsetReg && parseInt(offsetReg[1], 10)) || 0;
				resource += '&l.off=' + offset;
			}
			var hasBeacon = !!navigator.sendBeacon;
			try {
				performance.setResourceTimingBufferSize(500);
			} catch(e) { }
			w.bbxtrck = {
				startTime: now,
				perfTime: getPerfTime(),
				getPerfTime: getPerfTime,
				getTime: getTime,
				hasBeacon: hasBeacon,
				hasPerformance: hasPerformance,
				hasPerformanceNow: hasPerformanceNow,
				active: {
					dom: true,
					load: true,
					resource: true,
					unload: true,
					bbxError: true,
					consentNetwork: true,
					advStart: true,
					advPositions: true,
					advGroups: true,
				},
				setActive: function(state) {
					this.active = state || {};
				},
				used: { },
				track: function(name, params, repeat) {
					var src = this.getTrackUri(name, params, repeat);
					if (src) {
						var im = new Image();
						return im.src = src;
					}
				},
				getTrackUri: function(name, params, readOnly) {
					var tr = this.pramons[name];
					if (tr) {
						var uri = tr + '&nm=' + name + '&mt=' + now + '&bp=' + this.getPerfTime() + resource + (params || '');
						if (readOnly) {
							return uri;
						}
						if (!this.used[name]) {
							return this.used[name] = uri;
						}
					}
					return false;
				},
				adsStructure: {
					buf: [], // buffer
					cnt: {}, // counts
					con: {}, // consent
					cpe: {}, // cpex
					end: {}, // end
					grp: {}, // groups
					prf: {}, // performance
					pos: {}, // positions
					rpt: {}, // repeat
					run: [], // send
					sta: {} // start
				},
				getAdsDataToSend: function(flushLoads) {
					var data = '';
					try {
						var toSend = w.bbxtrck.adsStructure.buf.splice(0);
						if (flushLoads) {
							toSend.push(['a.loads', w.bbxtrck.adsStructure.cnt.load || 0]);
							toSend.push(['a.refresh.cnt', w.SiteAdvert && w.SiteAdvert.refresh_cnt || 0]);
						}
						data = new URLSearchParams(Object.fromEntries(toSend)).toString();
					} catch (e) {
						console.log('bbxtrck error', e.message);
					};
					return data ? '&' + data : '';
				},
				grabAdrequests: function(grp) {
					var ads = w.bbxtrck.adsStructure;
					try {
						performance.getEntriesByType('resource').filter(function(item, index) {
							var ret = item.name.match(/a\.centrum\.cz.*\/bserverj\/ball\/.*\/b\d+\/.*\/size=([a-z0-9]+)/);
							var ads = w.bbxtrck.adsStructure;
							if (ret && !ads.prf[index]) {
								var size = ret[1].slice(0,4);
								ads.prf[index] = grp;
								ads.buf.push(['a.gr_dur.' + size, ~~item.duration]);
								ads.buf.push(['a.gr_end.' + size, ~~item.responseEnd]);
								ads.buf.push(['a.gr_pos.' + size, index]);
							}
							return !!ret;
						})
					} catch(e) {
						ads.buf.push(['a.gr_err_' + grp, e.message]);
					}
				},
				adsListener: function(payload) {
					try {
						var ads = w.bbxtrck.adsStructure;
						var payload_type = payload.type || 'unknown';
						var detail = payload.detail || {};
						if (Array.isArray(detail)) {
							var timing = detail[0] || 0;
							var eventName = detail[1] || 'unk';
							var eventData = detail[2] || {};
							switch(eventName) {
								case 'page':
								case 'customRender':
									if(!ads.sta.sasic) {
										ads.sta.sasic = timing;
										ads.buf.push(['a.sasic_ms', timing]);
									}
									break;
								case 'loadAllHb':
									if(!ads.end.hb) {
										ads.end.hb = timing;
										ads.buf.push(['a.hb_ms', timing]);
									}
									break;
								case 'loadAll':
									if(!ads.cnt.load) {
										ads.cnt.load = 0;
										ads.sta.load = timing;
										ads.buf.push(['a.refresh.every', w.SiteAdvert && w.SiteAdvert.refresh_advert_every || 0]);
									}
									if (ads.cnt.load < 10) {
										ads.buf.push(['a.load_' + ads.cnt.load, timing]);
									}
									ads.cnt.load++ || w.bbxtrck.track('advStart', w.bbxtrck.getAdsDataToSend());
									break;
								case 'group':
									var gName = eventData['groupId'] || 'default';
									if(!ads.grp[gName]) {
										ads.grp[gName] = { add: timing };
										ads.buf.push(['a.gr_add_ms.' + gName , timing]);
									}
									break;
								case 'loadGroup':
									var gName = eventData['groupId'] || 'default';
									var load = eventData['loadHb'] ? 'loadHb' : 'load';
									if(ads.grp[gName] && !ads.grp[gName][load]) {
										ads.grp[gName][load] = timing;
										ads.buf.push(['a.gr_' + load + '.' + gName, timing]);
									}
									break;
								case 'renderGroup':
									var gName = eventData['groupId'] || 'default';
									var render = 'render';
									if(ads.grp[gName] && !ads.grp[gName][render]) {
										ads.grp[gName][render] = timing;
										ads.buf.push(['a.gr_' + render + '.' + gName, timing]);
										w.bbxtrck.grabAdrequests(gName);
									}
									break;
								case 'euConsentReady':
									if(!ads.con.load) {
										ads.con.load = timing;
										ads.buf.push(['a.con.load', timing]);
										ads.con.src = eventData.timeout ? 'timeout' : eventData.source || 'unk';
										ads.buf.push(['a.con.src', ads.con.src]);
									}
									break;
								case 'cpexPackageLoaded':
									if(!ads.cpe.load) {
										ads.cpe.load = timing;
										ads.buf.push(['a.cpex.load', timing]);
									}
									break;
							}
							// console.log('bbxtrck.listen', JSON.parse(JSON.stringify(ads)), detail);
						}
					} catch(err) {
					}
				},
				regAdsListener: function(listener) {
					document.addEventListener('sasic.action', listener, false);
				},
				getOrigin: function(uri) {
					var origin = '';
					try {
						var valid = /^https?:\/\/[^./]+\./.test(uri);
						if (valid) {
							origin = new URL(uri).origin
						}
					} catch(e) {
						console.log('bbxtrck error parse origin', e.message);
					}
					return origin ? '&p.orig=' + encodeURIComponent(origin) : '';
				},
				id: id,
				site: site,
				type: typ,
				resource: resource,
				pramons: {
					visit: path + 't=B23dXNkT',
					resource: path + 't=NCxsF2PK',
					dom: path + 't=HkF5SttR',
					load: path + 't=JKXHkjxM',
					bbxError: path + 't=DbfxLMIf',
					consentNetwork: path + 't=OFuP-l7Y',
					advStart: path + 't=KdpPk2AM',
					advPositions: path + 't=B26OkyRv',
					advGroups: path + 't=Ja074mVk',
					unload: path + 't=MeAOkuJ5'
				}
			};
			w.bbxtrck.track('visit', w.bbxtrck.getOrigin(document.referrer) + '&p.title=' + encodeURIComponent("Aktuálně"));
			w.bbxtrck.regAdsListener(w.bbxtrck.adsListener);
		})(window);
		</script>
