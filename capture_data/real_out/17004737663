n[0]; \n        $(document).ready(function(){\n            insertContent(xml)\n            fceResize();\n        })\n        $(window).on('load', function(){\n            fceResize();\n        })\n    }\n\n\n    function insertContent(xml){\n        var $xml = $($.parseXML(xml));\n\n        var nizsiWidget = LIB_GRAPHICS.utils.nodeToString($($xml).find('informace nizsi-widget').contents());\n\n        //nazev widgetu\n\n        widgetTitleSmall = $($xml).find('informace souvisejici').attr('titleSmall');\n        widgetTitleLarge = $($xml).find('informace souvisejici').attr('titleLarge');\n        \n        //hlavni odkaz\n        if($($xml).find('informace souvisejici').attr('odkaz-hlavni').length > 1){\n            widgetMainLink = $($xml).find('informace souvisejici').attr('odkaz-hlavni');\n\n            $('.grafika-obal .widget-wr .widgetTitle').html('<a href=\"'+widgetMainLink+'\"><h3 class=\"titleSmall\">'+widgetTitleSmall+'<\/h3><h1 class=\"titleLarge\">'+widgetTitleLarge+'<\/h1><\/a>');\n\n            $('.grafika-obal .widget-wr .widgetTitle a').on('click', function(event) {\n                sendStats('main title', widgetMainLink);\n            });\n        } else {\n            $('.grafika-obal .widget-wr .widgetTitle').text(widgetTitleSmall + widgetTitleLarge); \n        }\n\n        \n        if(nizsiWidget == \"ano\"){\n         $('.obal .grafika-obal .widget-wr').addClass('lower');\n\n        } else {\n            //souvisejici\n            var relatedStr = '';\n            $($xml).find('informace souvisejici clanek').each(function (i) {\n                var url = LIB_GRAPHICS.utils.nodeToString($(this).find('link-absolute:eq(0)').contents());\n                var title = LIB_GRAPHICS.utils.nodeToString($(this).find('title:eq(0)').contents());\n                var clStr = '';\n                if($(this).attr('cl') != undefined){\n                    clStr = ' '+$(this).attr('cl');\n                }\n                relatedStr += '<a href=\"'+url+'\" target=\"_top\" class=\"art-link'+clStr+'\"><em>'+title+'<\/em><\/a>'\n            })\n            $('.grafika-obal .main-side .related-bx').append(relatedStr);       \n        }\n\n        \n\n        //online\n        idOnline = LIB_GRAPHICS.utils.nodeToString($($xml).find('informace idonline').contents());\n\n        if(idOnline.length > 1){\n            if($(window).width()>=500){\n                nactiOnline();\n            } else {\n                onlineUrl = 'https://zpravy.aktualne.cz/r~'+idOnline+'/';\n\n\n                if(nizsiWidget == \"ano\"){\n                    $('.obal .grafika-obal .second-side').append('<a href=\"'+widgetMainLink+'\" class=\"small-btn show-special\"><em>Speciál<\/em><\/a>');    \n                }\n\n                $('.obal .grafika-obal .second-side').append('<a href=\"'+onlineUrl+'\" class=\"small-btn show-online\"><em>Online přenos<\/em><\/a>');\n                \n                $('.grafika-obal .second-side a.show-online').on('click', function(event) {\n                    sendStats('online',onlineUrl);\n                });\n            }\n        } else {\n            $('.obal .grafika-obal .second-side').hide();\n            $('.obal').addClass('no-online');\n        }\n\n        $('.grafika-obal .related-bx a.art-link').on('click', function(event) {\n            sendStats('article',$(this).attr('href'));\n        });\n\n\n        sendStatsInit();\n    }   \n\n\n    function nactiOnline(){\n\n        var d = new Date();\n        d.setHours(d.getHours() - 24, 0, 0, 0);\n        onlineAjaxUrl = '/ajax.php?m=online&op=getV2&variant=large&all=1&guid='+idOnline+'&last='+encodeURIComponent(d.toISOString().replace('T',' ').split('.')[0]);\n\n        $.ajax({\n            type: 'GET',\n            url: onlineAjaxUrl,\n            dataType: 'json',\n            success: function (data) {\n                onlineUrl = data.url;\n                $('.obal .grafika-obal .second-side').append('<a href=\"'+onlineUrl+'\" class=\"small-btn show-online\"><em>Online přenos<\/em><\/a>');\n                zobrazPosledniOnlinePrispevek(data);\n\n                $('.grafika-obal .second-side a.show-online').on('click', function(event) {\n                    sendStats('online',onlineUrl);\n                });\n                fceResize();\n            },\n            error: function () {\n                console.log('Online není dostupný');\n            }\n        });\n\n    }\n\n\n    function zobrazPosledniOnlinePrispevek(data) {\n        lastItem = data.lastItem || data.items[0].changetime;\n\n        for (var i = 0, l = data.items.length; i < l; i++) {\n            if (zobrazOnlinePrispevek(data.items[i])) {\n                $('.obal .grafika-obal .second-side').append(onlineSdeleni);\n                \n                if(i==1){break;}\n            }\n        }\n    }\n\n    function zobrazOnlinePrispevek(item) {\n        var content = $(item.content).text(); //uložení do proměnné bez html tagů - čistý text\n        if (content.length > 4){\n            var pCont = content.split(' ');\n            var sdeleni = '';\n            for (var a = 0, l = pCont.length; a < l; a++) {\n                if ((sdeleni.length + pCont[a].length) >= pocetZnaku) {\n                    sdeleni += '&hellip;';\n                    break;\n                } else if (a) {\n                    sdeleni += ' '+ pCont[a];\n                } else {\n                    sdeleni += pCont[a];\n                }\n            }\n            onlineSdeleni = '<p><span><em>'+item.timeText+'<\/em>'+sdeleni+'<\/span><\/p>'; \n            return true;\n        }\n        return false;\n    }   \n\n\n\n    function sendStatsInit(){\n        try {\n            parent.dataLayer.push({\n                \"event\": \"widget_impression\",\n                \"widget\": {\n                    \"name\": widgetTitle,\n                },\n                \"device\": {\n                    \"type\": gDevice,\n                },     \n                \"screen\": {\n                    \"resolution\": window.screen.availWidth+'x'+window.screen.availHeight\n                },\n                \"item\": {\n                    \"position\": widgetLocationType,\n                    \"url\": widgetLocation\n                },\n                _clear: true\n            });\n        } catch (e) {};\n    }\n\n    function sendStats(clickSection, clickTarget){\n        try {\n            parent.dataLayer.push({\n                \"event\": \"widget_click\",\n\n                \"widget\": {\n                    \"name\": widgetTitle,\n                },\n                \"device\": {\n                    \"type\": gDevice\n                },\n                \"screen\": {\n                    \"resolution\": window.screen.availWidth+'x'+window.screen.availHeight\n                }, \n\n                \"item\": {\n                    \"position\": widgetLocationType,\n                    \"url\": widgetLocation,\n                    \"type\": clickSection,\n                }, \n                \"click\": {\n                    \"url\": clickTarget\n                },\n                _clear: true\n            });\n            \n        } catch (e) {};\n    } \n\n\n\n<\/script>    \n\n<\/body>\n<\/html>",
				width: '100%',
				height: 1,
				onload: function () {
					widget.callFunction('run', [target, "<informace>\n    \n  <nizsi-widget>ne<\/nizsi-widget>\n  \n  <souvisejici titleSmall=\"Reportér Aktuálně.cz Martin Novák\" titleLarge=\"v Izraeli\" odkaz-hlavni=\"https://zpravy.aktualne.cz/valka-izraele-s-hamasem-2023/l~9567d86c667111eea873ac1f6b220ee8/\">  \n    <clanek><blackbox:object xmlns:blackbox=\"http://i0.cz/bbx/rss/\" bbxtype=\"article\" guid=\"ca268fc482b611eea3c0ac1f6b220ee8\"><shorttitle>Uhořet, nebo jít vstříc kulkám. Reportér na spáleništi, které bývalo vesnicí u Gazy<\/shorttitle><updated>off<\/updated><authorcontent>off<\/authorcontent><title>Uhořet, nebo jít vstříc kulkám. Reportér na spáleništi, které bývalo vesnicí u Gazy<\/title><annotation>Reportér Aktuálně.cz navštívil vypálený kibuc u hranice s Gazou. \"Teroristé křičeli hebrejsky, aby lidi zmátli,\" vypráví jeho obyvatel.<\/annotation><pubtime>2023-11-15 05:30:00<\/pubtime><changetime>2023-11-15 10:40:48<\/changetime><primary-image-type>photo<\/primary-image-type><link>https://zpravy.aktualne.cz/zahranici/uhoret-nebo-jit-vstric-kulkam-reportaz-ze-spaleniste-ktere-b/r~ca268fc482b611eea3c0ac1f6b220ee8/<\/link><link-absolute>https://zpravy.aktualne.cz/zahranici/uhoret-nebo-jit-vstric-kulkam-reportaz-ze-spaleniste-ktere-b/r~ca268fc482b611eea3c0ac1f6b220ee8/<\/link-absolute><\/blackbox:object><\/clanek>    \n    <clanek><blackbox:object xmlns:blackbox=\"http://i0.cz/bbx/rss/\" bbxtype=\"article\" guid=\"3435bee6830511eea9eeac1f6b220ee8\"><shorttitle>Reportér přímo u Gazy: Městem duchů znějí děla, symbol tragédie strhly buldozery<\/shorttitle><updated>off<\/updated><authorcontent>off<\/authorcontent><title>Reportér přímo u Gazy: Městem duchů znějí děla, symbol tragédie strhly buldozery<\/title><annotation>Městem Sderot, které leží jen kousíček od Pásma Gazy, otřásl útok Hamásu na policejní stanici. Reportér Aktuálně.cz přímo v centru dění.<\/annotation><pubtime>2023-11-16 05:30:00<\/pubtime><changetime>2023-11-16 09:35:10<\/changetime><primary-image-type>