					.addService(googletag.pubads())
					.setTargeting('LU_URL', 'd4e860ad5520c6d4ec24b1787de79c99')
					.setTargeting('LU_DOMAIN', 'www.levelup.com')
					.setCollapseEmptyDiv(true, true);
			googletag.pubads().enableLazyLoad({fetchMarginPercent: 500, renderMarginPercent: 200, mobileScaling: 2.0});
			googletag.enableServices();
			googletag.display('div-gpt-ad-1628201880601-0');
			var refreshTimer;
			googletag.pubads().addEventListener('slotVisibilityChanged', function(event) {
					var slot = event.slot;
					if (slot === targetSlot) {
							if (event.inViewPercentage >= 50) {
									clearInterval(refreshTimer);
									refreshTimer = setInterval(function(){googletag.pubads().refresh([targetSlot]);}, 30000);
							} else {
									clearInterval(refreshTimer);
							}
					}
			});
		});
	</script>
</div></div>
