   googletag.cmd.push(function () {
        defineAds();
        
        var sasRefreshEligible = ['square', 'square-scrolling'];
        var sasRefreshList = [];
        /*setInterval(function(){if(sasRefreshList.length!=0){googletag.pubads().refresh(sasRefreshList);sasRefreshList=[];}},45000);
        googletag.pubads().addEventListener('impressionViewable', function(e) {
            var slotId = e.slot.getSlotElementId();
            var regEx = /div-gpt-ad-(\w*)-(\w*)-([\w-]*)/gm;
            var slotName = regEx.exec(slotId)[3];
            if (sasRefreshEligible.indexOf(slotName)>-1 && !!sasSlots[slotName]) {
                sasRefreshList.push(sasSlots[slotName]);
            }
        });*/

        ['adsPag'].forEach(function(param) { if (window[param]) { console.warn('DFP TARGETING KEY-VALUE SET: type ->', window[param][0]); googletag.pubads().setTargeting('type', window[param][0]); }});
        ['adsCid'].forEach(function(param) { if (window[param] && window[param].length > 0 ) { console.warn('DFP TARGETING KEY-VALUE SET: id ->', window[param][0]); googletag.pubads().setTargeting('id', window[param][0]); }});
        ['adsSec'].forEach(function(param) { if (window[param] && window[param].length > 0 ) { console.warn('DFP TARGETING KEY-VALUE SET: category ->', window[param][0]); googletag.pubads().setTargeting('category', window[param][0]); }});

        googletag.pubads().addEventListener('slotRenderEnded', function(e) {
            if(!e.isEmpty){
                //document.getElementById(e.slot.getSlotElementId()).style.display='none';
                if(e.slot.getSlotElementId() === "mastheadNnh" || e.slot.getSlotElementId() === "mastheadCategory" )
                {
                    document.getElementById(e.slot.getSlotElementId()).classList.add('nx-has-ad');
                }
            }
        });

     
        googletag.pubads().enableLazyLoad({
            fetchMarginPercent: 500,
            renderMarginPercent: 200,
            mobileScaling: 2.0,
          });
        googletag.pubads().enableSingleRequest();
        //googletag.pubads().collapseEmptyDivs();
        googletag.enableServices();
    }); 

    function updateAdsTargeting() {
        googletag.cmd.push(function() {
            //console.log('updateAdsTargeting called');
            googletag.pubads().clearTargeting();
            googletag.destroySlots();
            defineAds();
            ["adsPag"].forEach(function(param) {
                if (window[param]) {
                    console.warn('DFP TARGETING KEY-VALUE SET: type ->', window[param][0]);
                    googletag.pubads().setTargeting('type', window[param][0]);
                }
            });
            ["adsCid"].forEach(function(param) {
                if (window[param] && window[param].length > 0 ) {
                    console.warn('DFP TARGETING KEY-VALUE SET: id ->', window[param][0]);
                    googletag.pubads().setTargeting('id', window[param][0]);
                }
            });
            ["adsSec"].forEach(function(param) {
                if (window[param] && window[param].length > 0 ) {
                    console.warn('DFP TARGETING KEY-VALUE SET: category ->', window[param][0]);
                    googletag.pubads().setTargeting('category', window[param][0]);
                }
            });
            googletag.pubads().refresh();
        });
        window.addEventListener('scroll', nx_bill_stick_it);
    }
    
    window.addEventListener('scroll', nx_bill_stick_it);
    function nx_bill_stick_it() {
        let nx_stick_bill_class = 'top-sticky-ad';
        let billboard_ad = document.querySelector("#mastheadNnh") || document.querySelector("#mastheadCategory");   
        let page_container = document.querySelector(".page-Post") || document.querySelector(".page-default");    
        if (window.scrollY > 40) {
            if(billboard_ad && billboard_ad.classList.contains('nx-has-ad'))
            {
                window.removeEventListener('scroll', nx_bill_stick_it);          
                page_container.classList.add("top-sticky-ad");  
                setTimeout(() => {
                  page_container.classList.remove("top-sticky-ad");  
                }, "3500");                
            }
        }
    }
