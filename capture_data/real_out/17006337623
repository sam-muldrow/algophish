<script>
if (typeof localStorage !== 'undefined') {
    const cookieContainer = document.querySelector(".cookie-container");
    const cookieButtonAccept = document.querySelector(".cookie-btn-accept");
    const cookieButtonDecline = document.querySelector(".cookie-btn-decline");

    cookieButtonAccept.addEventListener("click", () => {
        cookieContainer.classList.remove("active");
        localStorage.setItem("cookieBannerDisplayed", "true");
        
    });

    cookieButtonDecline.addEventListener("click", () => {
        cookieContainer.classList.remove("active");
        localStorage.setItem("cookieBannerDisplayed", "true");
        localStorage.setItem("cookieRefused", "true");
    });

    setTimeout(function() {
        if (!localStorage.getItem("cookieBannerDisplayed")) {
            cookieContainer.classList.add("active");
        }
    }, 2000);
}
</script>

