var $j;
if (!$j){
	// Inizializzazione jQuery
	$j = jQuery.noConflict();
}
$j(window).scroll(
		
		
	function(){ 
		var position = $j(window).position;
		if($j(window).scrollTop()!=0){ 
		 $j('.content-federation-bar-minified').css('top','0');
		}else{
			$j('.content-federation-bar-minified').css('top','auto');
		}
	});

	
	
</script>

