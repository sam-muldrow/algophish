 $j('.item:first').addClass("active");
 $j('.carousel').carousel();
});  

 $j('.hiddenP').each(function(){

   var name= $j(this).attr("name");
   var id=  $j(this).attr("id");
 
   var src = $j("[name="+name+"] >img").attr("src");
   
   $j('#'+name).css("background-image","url("+src+")");

  });

