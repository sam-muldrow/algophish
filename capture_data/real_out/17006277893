 <script>
 if(window.location.href.indexOf("mysda")==-1){
	 $j.ajax({
	       url: "https://www.sda.it/SDAitThemeDynamic/themes/html/getBracket.jsp",
		   xhrFields: {
		 		withCredentials: true
		   }, 	
		   success:function(result){
			 //inserisco il riferimento a sdait in tutti i link dei bracket
			  var nuovoResult = result.replace(/href=\"/g,'href="https://www.sda.it');
			  $j('.bracket-container').append(nuovoResult);

		   } 
	   }); 
	}
 </script>    
