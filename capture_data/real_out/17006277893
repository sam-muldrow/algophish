 <script>
 
  $j(document).ready(function(){
    var vpname =  "sda";

   if(vpname=="mysda"){
     callGetSDAitNews();
    }
    
     function callGetSDAitNews(){ 
	 $j.ajax({
	       url: "https://www.sda.it/SDAitThemeDynamic/themes/html/getNews.jsp",
		   xhrFields: {
		 		withCredentials: true
		   }, 	
		   success:function(result){
			  
			  $j('#ultimeNews').append(result);
		   } 
	   }); 
	   }
	   ;
	});
 </script>    
