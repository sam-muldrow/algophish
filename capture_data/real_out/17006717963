<script>
    if ( typeof addEvent !== 'function' ) {
        function addEvent(elem, evType, fn) {
            if (elem.addEventListener) {
                elem.addEventListener(evType, fn, false);
            }
            else if (elem.attachEvent) {
                elem.attachEvent('on' + evType, fn);
            }
            else {
                elem['on' + evType] = fn;
            }
        }
    }

    var feedbackSlWStats = {
        defaultConfig: {
            useCSS: false,
            autoStart: false,
            adaptiveHeight: true,
            onSliderLoad: function (currentIndex) {
                /* TODO: добавить динамическое получение элемента-сладера */
                feedbackSlWStats.$slider.closest('.bx-wrapper').find('.bx-controls')
                        .addClass('clearfix')
                        .prepend('<a class="link-all pull-right font16" href="/feedback/">Читать больше отзывов</a>');
            }
        },
        loadScript: function ( src, onLoadCallback ) {
            var that = this;

            var s = document.createElement('script'),
                ss = document.body;

            s.type = 'text/javascript';
            s.async = true;
            s.src = src;
            s.onreadystatechange = function (){
                var self = this;
                if ( this.readyState === 'complete' || this.readyState === 'loaded' ) {
                    setTimeout(function() {
                        self.onload();
                    }, 0); // сохранить "this" для onload
                }
            };
            s.onload = function() {
                if ( !this.executed ) { // выполнится только один раз
                    this.executed = true;
                    onLoadCallback.call(that);
                }
            };

            ss.appendChild(s);
        },
        addJQuery: function () {
            this.loadScript( '//code.jquery.com/jquery-1.11.3.min.js', this.addBXScript );
        },
        addBXScript: function () {
            this.loadScript( '//static.lpgenerator.ru/media/js/jquery.bxslider.min.js', this.init );
        },
        init: function ( config ) {/* TODO: постараться избавиться от двойного вызова, когда не подключены библиотеки */
            var config = config || this.defaultConfig;

            this.$slider = $('.feedbackSlWStats').find('.bxslider');
            this.$slider.bxSlider(config);
        }
    };

    addEvent( window, 'DOMContentLoaded', function () {
        if ( typeof ($) === 'undefined' ) {
            feedbackSlWStats.addJQuery();
        } else {
            try {
                feedbackSlWStats.init.call( feedbackSlWStats );
            } catch ( err ) {
                if ( err.message.indexOf('bxSlider is not a function') !== -1 ) {
                    feedbackSlWStats.addBXScript();
                }
            }
        }
    });
</script>

			


