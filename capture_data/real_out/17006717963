	<script>
		LPG.config.fixedMenu.alwaysVisible = true;

		var priceSwitcher = {
			data: {
				currencies: {
					1: {
						currencyId:				1,
						currencyCodeL:			'RUB',
						currencyCodeD:			643,
						currencySign:			'₽',
						currencySignHtml:		'&#8381;',
						currencySignUnicode:	'U+20BD'
					}
				},
				prices: {
					1: {
						wl:			17999,
						unlim:		4399,
						advanced:	2599,
						basic:		959
					}
				},
				discount: {
					1:		0,
					2:		0,
					3:		.1,
					4:		.1,
					5:		.1,
					6:		.2,
					7:		.2,
					8:		.2,
					9:		.2,
					10:		.2,
					11:		.2,
					12:		.3
				},
				currentCurrencyId: 1
			},
			setPrices: function ( monthCount, isInit ) {
				var isInit = isInit || false,
					prSw = this,
					ccId = prSw.data.currentCurrencyId,
					discount = monthCount > 12 ? prSw.data.discount[12] : prSw.data.discount[monthCount],
					prices = prSw.data.prices[ccId],
					$tarifs = $('.tarifs');

				for (var key in prices) {
					if ( !prices.hasOwnProperty(key) ) { continue; }
					var price = prices[key],
						discountVal = Math.ceil( price * discount ),
						total = price - discountVal,
						$cont = $tarifs.find('.' + key);

					if ( isInit ) { prSw.basePriceSet( $cont, price ); }
					prSw.totalSet( $cont, total );
					prSw.discountSet( $cont, discount, discountVal );
					prSw.clarificationSet( $cont, monthCount );
				}
			},
			basePriceSet: function ( $cont, price ) {
				$cont.find('.price-base').find('.val').html( price.toString().replace(/(\d)(?=(\d\d\d)+([^\d]|$))/g, '$1 ') );
			},
			totalSet: function ( $cont, total) {
				$cont.find('.price').html( total.toString().replace(/(\d)(?=(\d\d\d)+([^\d]|$))/g, '$1 ') );
			},
			discountSet: function ( $cont, discount, discountVal ) {
				if ( discount > 0 ) {
					var $discount = $cont.find('.discount');
					$cont.addClass('withDiscount');
					$discount.find('.val').html( discountVal.toString().replace(/(\d)(?=(\d\d\d)+([^\d]|$))/g, '$1 ') );
					$discount.children('.a_ico').attr('class', 'a_ico discount_' + ( discount * 100 ) + '_light_blue mr8 vab');
				} else {
					$cont.removeClass('withDiscount');
				}
			},
			endingGet: function ( monthCount ) {
				if ( monthCount > 1 && monthCount < 5 ) {
					return 'а';
				}
				if ( monthCount > 5 ) {
					return 'ев';
				}
				return '';
			},
			clarificationSet: function ( $cont, monthCount ) {
				var val = '',
					ending = this.endingGet( monthCount );
				val = monthCount === 12 ? 'год' : monthCount + ' месяц' + ending;
				$cont.find('.clarification').find('.val').text( val );
			},
			init: function () {
				this.setPrices( 12, true );
			}
		};

		var switcher = {
			featuresTabs: {
				parent: null,
				$switchingItem: null,
				relatedSwitch: function ( $dataElem ) {
					var tabId = $dataElem.data('tab_id');
					console.log('switch to tab whith id: ', tabId);
					this.$switchingItem.find('.featuresTabs__tabs__list').children('.featuresTabs__tab')
						.removeClass('active')
						.filter('.item-' + tabId).addClass('active');
				}
			},
			eventInit: function ( cfg ) {
				var that = this;
				cfg.$switcher.on( 'click', '.item', { relatedItem: cfg.relatedItem }, function (e) {
					var $this = $(this);
					$this.siblings().removeClass('active');
					$this.addClass('active');
					that[e.data.relatedItem].relatedSwitch( $this );
				});
			},
			init: function () {
				var that = this,
					$switcher = $('.switcher');

				$switcher.each(function () {
					var $this = $(this),
						ri = $this.data('related_item');
					if ( that.hasOwnProperty(ri) ) {
						that[ri].parent = that;
						that[ri].$switchingItem = $('.' + ri);
						that.eventInit({ $switcher: $this, relatedItem: ri });
					}
				});

			}
		};

		$(function($) {
			switcher.init();
			priceSwitcher.init();

			$('.priceSwitcher').on( 'click', '.item', function () {
				var $elem = $( this );

				$elem.siblings().removeClass('active');
				$elem.addClass('active');

				priceSwitcher.setPrices( $elem.data('month-count') );
			});
			$('.plansTable').on( 'click', '.columnHead', function () {
				if ( ( window.innerWidth || document.documentElement.clientWidth ) < 960 ) {
					var $this = $(this),
						$table = $this.closest('.plansTable'),
						activeColumnClass = $table.data('active_class') || '',
						newClass = 'show-' + $this.data('tarif_name');

					$table.removeClass( activeColumnClass );
					$table.addClass( newClass );
					$table.data( 'active_class', newClass );

					$("html:not(:animated),body:not(:animated)").animate({
						scrollTop: ( $table.find('.separators').filter(':first').offset().top - 81 )
					}, 600);
				}
			});

			var togglingHeaders = {
				isToggleOn: false,
				$items: $( '.featuresTabs__feature__item__decr' ),
				toggleTxt: function () {
					$( this ).siblings( '.collapsible-body' ).toggle( 300 );
				},
				toggleOn: function () {
					if ( !this.isToggleOn ) {
						this.$items.find( '.collapsible-body' ).hide( 0 );
						this.$items.on( 'click', 'h4', this.toggleTxt );
						this.isToggleOn = true;
					}
				},
				toggleOff: function () {
					if ( this.isToggleOn ) {
						this.$items.find( '.collapsible-body' ).show( 0 );
						this.$items.off( 'click', 'h4', this.toggleTxt );
						this.isToggleOn = false;
					}
				}
			};

			if ( ( window.innerWidth || document.documentElement.clientWidth ) < 768 ) {
				togglingHeaders.toggleOn();
			}

			$( window ).on( 'resize', function () {
				if ( ( window.innerWidth || document.documentElement.clientWidth ) < 768 ) {
					togglingHeaders.toggleOn();
				}
				else {
					togglingHeaders.toggleOff();
				}
			});


			$('.info_light_blue_micro').filter('[data-hint_id]').closest('td').on( 'mouseenter', function () {
				var $icon = $(this).find('.info_light_blue_micro'),
					bTpl,
					hintTpl;

				if ( !$icon.html() ) {
					bTpl = $('#tpl-balloon').html(),
						hintTpl = $('#tpl-info-' + $icon.data('hint_id')).html() || '';

					$icon.html( '<!--noindex-->' + bTpl.replace(/\{\{content\}\}/g, hintTpl) + '<!--/noindex-->' );
				} else {
					$icon.find('.balloon').removeClass('hidden');
				}
				$icon.find('.balloon').width( $(window).width() * .8 );
			});

			$('.info_light_blue_micro').filter('[data-hint_id]').closest('td').on( 'mouseleave', function () {
				$(this).find('.info_light_blue_micro').find('.balloon').addClass('hidden');
			});

			var emplSlider = {
				$sliderEl: $( '.bxslider0' ),
				data: {
					photosDir: LPG.STATIC_URL + 'css/images/anon_index_1216/employees/'
				},
				configLt768: {
                minSlides: 1,
					maxSlides: 1,
					slideWidth: 204,
					slideMargin: 16,
					pager: false,
					shrinkItems: true,
					nextSelector: '.emplSlider .bx-control-direction.next',
					prevSelector: '.emplSlider .bx-control-direction.prev'
				},
				configGt768: {
					minSlides: 1,
                maxSlides: 4,
					slideWidth: 204,
                slideMargin: 16,
					pager: false,
					shrinkItems: true,
					nextSelector: '.emplSlider .bx-control-direction.next',
					prevSelector: '.emplSlider .bx-control-direction.prev'
				},
				addPhotos: function () {
					var that = this,
						$items = this.$sliderEl.find( '.photo' );
					$items.each( function () {
						var $this = $( this ),
							photoName = $this.data( 'photo_name' );
						$this.html( '<img src="' + that.data.photosDir + photoName + '" alt="' + photoName + '">' );
					} );
				}
			};
			emplSlider.addPhotos();

			var partnersSider = {
				$sliderEl: $('.bxslider1'),
				configLt768: {
                useCSS: false,
                minSlides: 1,
					maxSlides: 1,
					slideWidth: 204,
					slideMargin: 16,
					pager: false,
					shrinkItems: true,
					nextSelector: '.partnersSlider .bx-control-direction.next',
					prevSelector: '.partnersSlider .bx-control-direction.prev'
				},
				configGt768: {
					useCSS: false,
					minSlides: 1,
                maxSlides: 4,
                slideWidth: 204,
                slideMargin: 16,
					pager: false,
					shrinkItems: true,
					nextSelector: '.partnersSlider .bx-control-direction.next',
					prevSelector: '.partnersSlider .bx-control-direction.prev'
				}
			};

			if ( ( window.innerWidth || document.documentElement.clientWidth ) < 768 ) {
				emplSlider.$sliderEl = emplSlider.$sliderEl.bxSlider( emplSlider.configLt768 );
				partnersSider.$sliderEl = partnersSider.$sliderEl.bxSlider( partnersSider.configLt768 );
			} else {
				emplSlider.$sliderEl = emplSlider.$sliderEl.bxSlider( emplSlider.configGt768 );
				partnersSider.$sliderEl = partnersSider.$sliderEl.bxSlider( partnersSider.configGt768 );
			}
			$( window ).on( 'resize', function () {
				if ( ( window.innerWidth || document.documentElement.clientWidth ) < 768 ) {
					emplSlider.$sliderEl.reloadSlider( emplSlider.configLt768 );
					partnersSider.$sliderEl.reloadSlider( partnersSider.configLt768 );
				} else {
					emplSlider.$sliderEl.reloadSlider( emplSlider.configGt768 );
					partnersSider.$sliderEl.reloadSlider( partnersSider.configGt768 );
				}
            });

			var topSlider = {
				$topWrapper: $('.index-top-wrapper'),
				$slider: null,
				slidesToShow: 1,
				data: {
					slides: [
						{
							txt: 'Иван повысил узнаваемость бренда в 3 раза',
							imgName: 'lpg_im00.jpg'
						},
						{
							txt: 'Инна собрала полную аудиторию на свой тренинг',
