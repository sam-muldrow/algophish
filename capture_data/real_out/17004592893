    <script type="text/javascript">
try{
    var isOverAds = false,
    windowLostBlur = function () {
        if (isOverAds) {
            let cpc = 0.0
            if(window.screen.availWidth < 992) {
                cpc = '0.22674'
            } else {
                cpc = '1.16083'
            }
            $('body').append('<img height=1 width=1 style="border-style:none;" alt="" src="https://www.googleadservices.com/pagead/conversion/936166571/?value='+cpc+'&amp;currency_code=USD&amp;label=WWVtCJrrobEBEKuJs74D&amp;guid=ON&amp;script=0" class="hidden">');
            $('body').append('<img height=1 width=1 style="border-style:none;" alt="" src="https://www.googleadservices.com/pagead/conversion/1046041325/?value='+cpc+'&amp;currency_code=USD&amp;label=HYhNCNTkjN4DEO2l5fID&amp;guid=ON&amp;script=0" class="hidden">');
            window.uetq = window.uetq || [];
            window.uetq.push('event', 'click_ads', {'event_category': 'Clicks', 'event_label': 'click_ads', 'event_value': 1, 'revenue_value': cpc, 'currency': 'USD'});
        }
    };
    $(window).focus();
    $('ins.adsbygoogle, div.adsByGPT').mouseenter(function(){
        isOverAds = true;
    });
    $('ins.adsbygoogle, div.adsByGPT').mouseleave(function(){
        isOverAds = false;
    });
    $(window).blur(function () {
        windowLostBlur();
    });
}catch(error){
    console.error(error);
}
