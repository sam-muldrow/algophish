        setTimeout(function () {
            $.ajax({
                url: '/whatsapp/checkpendingcontact',
                dataType: 'JSON',
                success: function(rdata) {
                    if ( rdata.status === 'success' ) {
                        $('#modal_contact_whatsapp').remove();
