  var slideDuration = 7000;
  var carouselWithTimer = $(".owl-carousel-full-screen").owlCarousel({
    slideSpeed: 700,
    paginationSpeed: 700,
    singleItem: true,
    navigation: true,
    items: 1,
    autoplay: true,
