<script>
function promotionbanner_blackfriday(){
    var height = parseInt(window.getComputedStyle(document.querySelector(".blackcyber")).height);
    document.querySelector('.custom_header').classList.add('blackfridayheader');
    document.querySelector('.blackcyber').innerHTML+='<style>.custom_header.blackfridayheader{margin-top:'+height+'px;}</style>';
}
$(document).ready(function(){promotionbanner_blackfriday()});
window.addEventListener("resize", promotionbanner_blackfriday);
function holiday_card_time_blackfriday() {
    document.querySelector('.custom_header').classList.remove('blackfridayheader');
    return false;
}
</script>
