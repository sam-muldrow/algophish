            <!-- FavoriteForum Transparent Overlay -->
<div id="favorite-forum-overlay" style="position:fixed;top:0;left:0;display:none;width:100%;height:100%;z-index:100;"></div>
<!-- //FavoriteForum Transparent Overlay -->

<script type="text/javascript" src="//www.ppomppu.co.kr/js/favorite-forum.js?v=2023102604"></script>
<script>
(function(){

var default_list = [];

// 1. make link to favorite-forum-item
// 2. make default list
console.time('make-favorite-forum-item');
$('.top-nav .menu07 li a').each(function(){
    var parent = this.parentNode;
    this.href.replace(/[?&]id=([^&]*)/, function(str, board_id){
        if( board_id ){
            $(parent).data('board-id', board_id);
            parent.classList.add('favorite-forum-item');

            if( parent.classList.contains('strong') ){
                default_list.push(board_id);
            }
        }
    });
});
console.timeEnd('make-favorite-forum-item');

FavoriteForum.init({
    "default_list" : default_list,
    "$btns" : $('.top-nav .menu07 .off .favorite-forum-item'),
    "hookEditMode" : function(){
        $('#favorite-forum-panel').css({"display":"block", "z-index":"200"});
        $('#favorite-forum-overlay').show();
        $('#favorite-forum-edit-btn').html('<img src="/images/forum_check_ico.svg" alt="저장하기">저장하기');
    },
    "hookNormalMode" : function(){
        $('#favorite-forum-panel').css({"display":"", "z-index":""});
        $('#favorite-forum-overlay').hide();
        $('#favorite-forum-edit-btn').html('<img src="/images/forum_plus_ico.svg" alt="즐겨찾기">즐겨찾기');
    },
    "hookBtnChecked" : function($btn){
        $btn.find('.forum-favourite-icon img').attr('src', '/images/icon-forum-check.png');
        $btn.find('.forum-favourite-icon').show();
        $btn.addClass('strong');
        $btn.removeClass('new');
    },
    "hookBtnUnchecked" : function($btn){
        $btn.find('.forum-favourite-icon img').attr('src', '/images/icon-forum-plus.png');
        $btn.find('.forum-favourite-icon').show();
        $btn.removeClass('strong new');
    },
    "hookBtnSaved" : function($btn){
        $btn.find('.forum-favourite-icon').hide();
        $btn.addClass('strong');
    },
    "hookBtnNormal" : function($btn){
        $btn.find('.forum-favourite-icon').hide();
        $btn.removeClass('strong');
    },
});

// favorite forum edit button
$('#favorite-forum-edit-btn').on('click', function(){
    FavoriteForum.toggleEditSaveMode();
});

// favorite forum overlay
$('#favorite-forum-overlay').on('click', function(){
    if( confirm('포럼 즐겨찾기를 취소하시겠습니까?') ){
        FavoriteForum.normalMode();
        FavoriteForumUnoff.normalMode();
    }
});

})();
</script>
            <!-- FavoriteForumUnoff Transparent Overlay -->
<div id="favorite-forum-overlay-unoff" style="position:fixed;top:0;left:0;display:none;width:100%;height:100%;z-index:100;"></div>
<!-- //FavoriteForumUnoff Transparent Overlay -->

<script type="text/javascript" src="//www.ppomppu.co.kr/js/favorite-forum-unoff.js?v=2023102604"></script>
<script>
(function(){

var default_list = [];

// 1. make link to favorite-forum-item
// 2. make default list
console.time('make-favorite-forum-item-unoff');
$('.top-nav .menu07 li a').each(function(){
    var parent = this.parentNode;
    this.href.replace(/[?&]id=([^&]*)/, function(str, board_id){
        if( board_id ){
            $(parent).data('board-id', board_id);
            parent.classList.add('favorite-forum-item-unoff');

            if( parent.classList.contains('strong') ){
                default_list.push(board_id);
            }
        }
    });
});
console.timeEnd('make-favorite-forum-item-unoff');

FavoriteForumUnoff.init({
    "default_list" : default_list,
    "$btns" : $('.top-nav .menu07 .unoff .favorite-forum-item-unoff'),
    "hookEditMode" : function(){
        $('#favorite-forum-panel').css({"display":"block", "z-index":"200"});
        $('#favorite-forum-overlay').show();
        $('#favorite-forum-unoff-edit-btn').html('<img src="/images/forum_check_ico.svg" alt="저장하기">저장하기');
    },
    "hookNormalMode" : function(){
        $('#favorite-forum-panel').css({"display":"", "z-index":""});
        $('#favorite-forum-overlay').hide();
        $('#favorite-forum-unoff-edit-btn').html('<img src="/images/forum_plus_ico.svg" alt="즐겨찾기">즐겨찾기');
    },
    "hookBtnChecked" : function($btn){
        $btn.find('.forum-favourite-icon img').attr('src', '/images/icon-forum-check.png');
        $btn.find('.forum-favourite-icon').show();
        $btn.addClass('strong');
        $btn.removeClass('new');
    },
    "hookBtnUnchecked" : function($btn){
        $btn.find('.forum-favourite-icon img').attr('src', '/images/icon-forum-plus.png');
        $btn.find('.forum-favourite-icon').show();
        $btn.removeClass('strong new');
    },
    "hookBtnSaved" : function($btn){
        $btn.find('.forum-favourite-icon').hide();
        $btn.addClass('strong');
    },
    "hookBtnNormal" : function($btn){
        $btn.find('.forum-favourite-icon').hide();
        $btn.removeClass('strong');
    },
});

// favorite forum edit button
$('#favorite-forum-unoff-edit-btn').on('click', function(){
    FavoriteForumUnoff.toggleEditSaveMode();
});

// favorite forum overlay
$('#favorite-forum-overlay-unoff').on('click', function(){
    if( confirm('포럼 즐겨찾기를 취소하시겠습니까?1') ){
        FavoriteForumUnoff.normalMode();
    }
});

})();
</script>

