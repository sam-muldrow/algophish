<script type="text/javascript">
var PpomHotKey = new class {
    constructor(){
        this.hotKeyList = {};
    }

    regist(key, url){
        key = key.split('|');
        key.forEach(function(value){
            this.hotKeyList[value] = url;
        }.bind(this));
    }

    run(){
        $(document).on('keydown', function(e){
            if( $(e.target).is('input') || $(e.target).is('textarea') ){
                return;
            }

            var key = e.originalEvent.code;
            var withAlt = e.originalEvent.altKey;
            var withCtrl = e.originalEvent.ctrlKey;
            if( this.hotKeyList[key] && !withAlt && !withCtrl ){
                e.preventDefault();
                location.href = this.hotKeyList[key];
            }
        }.bind(this));
    }
};
$(document).ready(function(){
PpomHotKey.regist('KeyA', 'https://www.ppomppu.co.kr');
PpomHotKey.regist('KeyF', 'https://www.ppomppu.co.kr/recent_forum_article.php');
PpomHotKey.regist('KeyZ', 'https://www.google.co.kr');
PpomHotKey.regist('KeyH', 'https://www.ppomppu.co.kr/hot.php');
PpomHotKey.regist('Digit0|Numpad0', 'https://www.ppomppu.co.kr/recent_main_article.php?type=market');
PpomHotKey.regist('Digit1|Numpad1', 'https://www.ppomppu.co.kr/zboard/zboard.php?id=ppomppu');
PpomHotKey.regist('Digit2|Numpad2', 'https://www.ppomppu.co.kr/zboard/zboard.php?id=event2');
PpomHotKey.regist('Digit3|Numpad3', 'https://www.ppomppu.co.kr/zboard/zboard.php?id=buy');
PpomHotKey.regist('Digit4|Numpad4', 'https://www.ppomppu.co.kr/zboard/zboard.php?id=help');
PpomHotKey.regist('Digit5|Numpad5', 'https://www.ppomppu.co.kr/zboard/zboard.php?id=freeboard');
PpomHotKey.regist('Digit6|Numpad6', 'https://www.ppomppu.co.kr/zboard/zboard.php?id=etc_info');
PpomHotKey.regist('Digit7|Numpad7', 'https://www.ppomppu.co.kr/zboard/zboard.php?id=free_gallery');
