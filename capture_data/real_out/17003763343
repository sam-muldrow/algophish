<script>
$(document).ready(function() {
});

$('.login_box .user_out a').on('click',function(){
    var num = $(this).index();

    $('.login_box .user_out a').removeClass('on');
    $(this).addClass('on');

    if(num==0)
    {
        if ( $('.user_box_out').css('display') == 'block' )
        {
document.location.href="/hot.php";        }
        else
        {
        	$('.user_box_out').css('display','block');
            $('.user_box_out2').css('display','none');
        }
    }
    else if(num==1)
    {
    	if ( $('.user_box_out2').css('display') == 'block' )
        {
        	document.location.href="/zboard/zboard.php?id=notice";
        }
        else
        {
        	$('.user_box_out').css('display','none');
            $('.user_box_out2').css('display','block');
        }
    }
});

$(document).ready(function(){
    var notice_cnt        = 0;
    var event_ppomppu_cnt = 4;

    if( notice_cnt > 0 || event_ppomppu_cnt > 0 ){
        $("#user_box_notice_new_icon").show();
    }
});
</script>

