    function googletag_display_mapping(id){
        if( $('#'+id).length > 0 ) googletag.display(id);
    }

    googletag.cmd.push(function() {
    	googletag.pubads().enableSingleRequest();
    	googletag.enableServices();
    });
    googletag.cmd.push(function() {
        googletag_display_mapping('div-gpt-ad-1553129599411-0');    //1. view_f
        googletag_display_mapping('div-gpt-ad-1552883278863-0');    //4.r_banner_f
        googletag_display_mapping('div-gpt-ad-1553129536230-0');    //6.top_f
        googletag_display_mapping('div-gpt-ad-1553046906552-0');    //8.main_top_f
        googletag_display_mapping('div-gpt-ad-1553129568597-0');    //9.view_bottom_f
        googletag_display_mapping('div-gpt-ad-1553129226637-0');    //10.list_f
        googletag_display_mapping('div-gpt-ad-1683858142480-0');    //11.list2_f
        googletag_display_mapping('div-gpt-ad-1553129293042-0');    //12.main_f
        googletag_display_mapping('div-gpt-ad-1575873929721-0');    //19.hot_deal
        googletag_display_mapping('div-gpt-ad-1553129385597-0');    //21.search1_f
        googletag_display_mapping('div-gpt-ad-1553129410389-0');    //22.search3_f
        googletag_display_mapping('div-gpt-ad-1640235277557-0');    //40.submain_news_f
        googletag_display_mapping('div-gpt-ad-1553129356594-0');    //50.main_under_top_f
        googletag_display_mapping('div-gpt-ad-1580350473881-0');    //51.main_under_top2_f
        googletag_display_mapping('div-gpt-ad-1580350565635-0');    //52.main_bottom
        googletag_display_mapping('div-gpt-ad-1572845617989-0');    //13.view_top
    });

    /*
    스크롤이 광고div 위치오면 광고 노출
    */
    var ads_scroll_json = "[]";
    for(i=0;i<ads_scroll_json.length;i++) ads_scroll_json[i].visible = false;
    var ads_is_scroll   = false;
    var ads_is_list     = false;

    /*
    스크롤이벤트
    */
    $(document).scroll(function() {
        ads_is_scroll = true;
        google_ad_chk();
    });

    /*
    최초한번 실행
    */
    $(document).ready(function() {
        google_ad_chk();
    });

    /*
    스크롤이 광고div 위치오면 광고 노출
    */
    function google_ad_chk()
    {
        for(i=0;i<ads_scroll_json.length;i++)
        {
            if( ads_scroll_json[i].visible == false )
            {
                var ad_scroll = $("#ad_"+ads_scroll_json[i].slot);
                var is_ad_ = isAdDivVisible( ad_scroll );
                //예외
                if( ads_is_scroll === true && ads_is_list === true && ads_scroll_json[i].pos == 10 ) is_ad_ = true;

                if( is_ad_ == true )
                {
                    $("div[id='ad_"+ads_scroll_json[i].slot+"']").html('<iframe id="if_'+ads_scroll_json[i].slot+'" src="https://www.ppomppu.co.kr/banner/google_ad.html?pos='+ads_scroll_json[i].pos+'&gb=first" scrolling="no" border="0" marginWidth="0" marginHeight="0" frameBorder="0" width="'+ads_scroll_json[i].w+'px" height="'+ads_scroll_json[i].h+'px"></iframe>');
                    ads_scroll_json[i].visible = true;
                }
            }
        }
    }


    /*
    스크롤이 광고div 위치에 있는지 체크
    */
    function isAdDivVisible( obj )
    {
        if( obj.length == 0 ) return false;
        var target           = obj.offset().top;      //타겟 위치값
        var window_height    = $(window).height();    //윈도우크기
        var window_scrollTop = $(window).scrollTop(); //브라우저의 스크롤 위치값
        var gap              = 500;                    //간격조정 px
        var isVisible        = false;

        isVisible = ( target - gap <= (window_height) )                    ? true : isVisible;
        isVisible = ( target - gap <= (window_height + window_scrollTop) ) ? true : isVisible;

        return isVisible;
    }
    </script>


    </script>


    


<div class="gif_down-layer" style="display:none;">
    <div class="gif_dimBg"></div>
    <div class="gif_down_layer gif_pop-layer" id="gif_down_layer">
        <div class="gif_down_layer_box" id="gif_down_layer_box">
            <a href="javascript:;" onclick="jfnGifLayerClose()" class="gif_btn-layerClose">X</a>
            <img id="gif_download_img" src="//cdn2.ppomppu.co.kr/images/lazyloading.jpg">
        </div>
    </div>
</div>
<script type="text/javascript">
//-------------------------------------------------------------------
// gif 다운로드 보기 시작
//-------------------------------------------------------------------
function jfnGifInstantView(obj)
{
    var org_src = $(obj).data("org-src");
    $gif = $("<img>").attr("src", org_src).addClass("org_gif");
    $(obj).parent().prepend($gif);
    $(obj).siblings("video").remove();

    $(obj).next().remove();
    $(obj).remove();
    if ( $(".pic_bg video").length <= 1 ) $(".pic_bg .btn_show_org_all").remove();
    jfnGifInstantViewAllInit();
}
function jfnGifInstantViewAll()
{
	$(".btn_show_org").each( function (i) {
        var org_src = $(this).data("org-src");
        $gif = $("<img>").attr("src", org_src).addClass("org_gif");
        $(this).parent().prepend($gif);
        $(this).siblings("video").remove();
        $(this).remove();
	});

	$(".pic_bg .btn_show_org_all").remove();
}
function jfnGifLayerPopupView(imgurl, w, h)
{

    var img_w = w;
    var img_h = h;
    if( $(document).width() < w - 20 )
    {
        img_w = $(document).width() - 20;
        img_h = Math.round((h) * (img_w/w));
    }
    w = img_w; h = img_h;


    $('#gif_down_layer_box').css({width:w, height:h});
    $('#gif_download_img').css({width:w, height:h});

    $("#gif_download_img").attr("src", imgurl);
    jfnGifLayerPopupShow(w,h);
}

function jfnGifLayerPopupShow(w,h){

    var $el = $('#gif_down_layer');        //레이어의 id를 $el 변수에 저장
    var isgif_dim = $el.prev().hasClass('gif_dimBg');   //gif_dimmed 레이어를 감지하기 위한 boolean 변수

    // if( $(document).width() < w - 20) w = $(document).width() - 70;
    $('#gif_down_layer_box').css('width', w+'px');
    $('#gif_down_layer_box').css('height', h);

    isgif_dim ? $('.gif_down-layer').fadeIn() : $el.fadeIn();

    var $elWidth = ~~($el.outerWidth()),
        $elHeight = ~~($el.outerHeight()),
        docWidth = $(document).width(),
        docHeight = $(document).height();

    // 화면의 중앙에 레이어를 띄운다.
    if ($elHeight < docHeight || $elWidth < docWidth) {
        $el.css({
            marginTop: -$elHeight/2,
            marginLeft: -$elWidth/2
        })
    } else {
        $el.css({top: 0, left: 0});
    }
}

function jfnGifLayerClose()
{
    $('.gif_down-layer').fadeOut();

    setTimeout(function(){
        $("#gif_download_img").attr("src", "//cdn2.ppomppu.co.kr/images/lazyloading.jpg");
    }, 400);

    return false;
}

function jfnGifInstantViewAllInit() {

    if ( $(".pic_bg .btn_show_org_all").length == 1 ) $(".pic_bg .btn_show_org_all").remove();

    $(".btn_show_org_all").each( function (i) {
    	if ( $(this).parents('.pic_bg').length == 0 ) $(this).remove();
	});

	$(".pic_bg .btn_show_org_all").each( function (i) {
	    if ( i == 0 || i== ($(".pic_bg .btn_show_org_all").length-1) ) $(this).show();
	    else $(this).hide();
	});
}

// lazyload 실패시 로그
var lazyloadFail = new class {
    constructor(){
        this.isCall = false; // 한페이지에서 한번만 콜
    }

    log(id, no, reg_date){
        if( this.isCall ) return false;
        this.isCall = true;

        no = 1 * no;
        reg_date = 1 * reg_date;
        var min_reg_date = 1657089240; // '2022-07-06 15:34:00'

        if( reg_date > min_reg_date ){
            $.ajax({
                "url":"/zboard/comment_no_image.php?id=" +id+ "&no=" +no+ "&reg_date=" +reg_date,
                "type":"get",
                "dataType":"json",
                "timeout":2000,
            });
        }
    }
};

//-------------------------------------------------------------------
// gif 다운로드 보기 종료
//-------------------------------------------------------------------
</script>
<!-- GIF 다운로드  end -->


<script>
// get SNS oembed
$('.cmt-media-link').on('click', function(e){
    var self = this;
    var $next = $(self).next();
    var url = $(self).data('url');
    var media = $(self).data('media');
    var api_url = '';
    var data = {};

    if( $(self).hasClass('active') ){
        $next.hide();
        $(self).removeClass('active');
        return false;
    }

    if( $next.html() ){
        $next.show();
        $(self).addClass('active');
        return false;
    }

    switch( media )
    {
        case 'instagram' :
            if( url.match(/instagram.com\/p\/|instagram.com\/tv\//) ){
                api_url = "https://graph.facebook.com/v9.0/instagram_oembed?";
                data = {
                    "url" : url,
                    "access_token" : "427974708216137|02c9c2734c062288a55445eb068469d4"
                };
            }else{
                return false;
            }
            break;

        case 'twitter' :
            if( url.match(/\/status\//) ){
                api_url = "https://publish.twitter.com/oembed?callback=?";
                data = {"url" : url};
            }else{
                return false;
            }
            break;

        case 'facebook' :
            // video
            if( url.match(/\/videos\/|\/video\.php\?/) ){
                api_url = "https://graph.facebook.com/v9.0/oembed_video?";
            // post
            }else if( url.match(/\/posts\/|\/activity\/|\/photos\/|\/media\/|\/questions\/|\/notes\/|\/photo.php\?|\/permalink.php/) ){
                api_url = "https://graph.facebook.com/v9.0/oembed_post?";
            // page
            }else{
                api_url = "https://graph.facebook.com/v9.0/oembed_page?";
            }
            data = {
                "url" : url,
                "access_token" : "427974708216137|02c9c2734c062288a55445eb068469d4"
            };
            break;

        case 'youtube' :
            var res = '<iframe width="560px" height="315px" src="https://www.youtube.com/embed/' +url+ '" frameborder="0" allow="accelerometer; encrypted-media; gyroscope; picture-in-picture" allowfullscreen=""></iframe>'
            $next.html(res);
            $(self).addClass('active');
            return false;

        case 'kakao' :
            var res = '<iframe title="" width="640" height="360" src="https://play-tv.kakao.com/embed/player/cliplink/' +url+ '" allowfullscreen frameborder="0" scrolling="no" allow="autoplay"></iframe>'
            $next.html(res);
            $(self).addClass('active');
            return false;

        case 'naver' :
            var res = '<iframe src="https://tv.naver.com/embed/' +url+ '" frameborder="no" scrolling="no" marginwidth="0" marginheight="0" WIDTH="544" HEIGHT="306" allow="autoplay" allowfullscreen></iframe>'
            $next.html(res);
            $(self).addClass('active');
            return false;

        default :
            return false;
    }

    $.getJSON(api_url, data).done(function(res){
        if( typeof res.html != 'undefined' ){
            $next.html(res.html);
            $(self).addClass('active');
        }
    });

    return false;
});

$(document).ready(function(){
    // SNS oembed call after page loaded

    $('.cmt-media-link').trigger('click');
    jfnGifInstantViewAllInit();
});
</script>

</body>
</html>
