            jQuery(document).ready(function($){
				// $('.btn-live-chat a').click(function(e){
				// 	$('.pop-chat').show();
				// 	e.preventDefault();				
				// });
				$('.pop-close').click(function(){
					$('.pop-chat').hide();
					$('.phoneModalWrapper').hide();
				});

				$('.other-phone a').click(function(e){
					$('.phoneModalWrapper').show();
					e.preventDefault();		
				});

                $('.boxes.owl-carousel').owlCarousel({
                    loop:true,
                    margin:10,
                    nav:true,
                    dots:true,
                    responsive:{
                        0:{
                            items:1,

                        },
                        600:{
                            items:1
                        },
                        1000:{
                            items:1
                        }
                    }
                })

                if(jQuery(window).width() <= 640){
                    $('.content-box.owl-carousel').owlCarousel({
                        loop:true,
                        nav:true,
                        dots:true,
                        responsive:{
                            0:{
                                nav:true,
                                dots:true,
                                items:1
                            },
                            600:{
                                items:1
                            },
                            1000:{
                                items:1
                            }
                        }
                    });
                }
            });
        </script>
        <script type="text/javascript">
         function myFunction() {
           var x = document.getElementById("myLinks");
           if (x.style.display === "block") {
             x.style.display = "none";
             jQuery('.mobile-logo-x').hide();
             jQuery('.mobile-logo-o').show();
           } else {
             x.style.display = "block";
             jQuery('.mobile-logo-o').hide();
             jQuery('.mobile-logo-x').show();
           }
         }
         jQuery(document).ready(function(){
            jQuery(this).find('.sub-menu').toggle();
            jQuery('.dropdown').on('click',function(e){
                jQuery(this).find('.sub-menu').toggle();

                if(jQuery(this).hasClass('rotate')){
                    jQuery('.rotate').find('img').css('transform', 'rotate(0)');
                    jQuery(this).removeClass('rotate');
                } else {
                    jQuery(this).addClass('rotate');
                    jQuery('.rotate').find('img').css('transform', 'rotate(180deg)');
                }
                //e.preventDefault();
            });
         });
      </script>

