<script type="text/javascript">
	
		
		
		
			
			
		
	

	function displayFconnectThickBox(accessToken) {
		urlstr='/candidate/fconnect.html?ajax=true&access_token='+accessToken;
		title =	'Login Details';
		childWindow =GB_showCenter(title, urlstr,350,600);
	}   
	
	function displaySocialConnectThickBox(socialProfileId,pId,st,apc) {
		urlstr='/candidate/socialconnect.html?pId='+pId+'&ajax=true&pfId='+socialProfileId;
		if(st){
			urlstr+='&st='+st;
		}
		if(apc){
			urlstr+='&apc='+apc;
		}
		title =	'Login Details';
		childWindow =GB_showCenter(title, urlstr,350,600);
	}

	function displayAlreadyTJThickBox(access_token,emailId){
		urlstr='/candidate/fconnect.html?ajax=true&emailId='+emailId+"&access_token="+access_token;
		title =	'Login Details';
		childWindow =GB_showCenter(title, urlstr,350,600);
	}
	
	function displayAlreadyTJThickBoxSocial(socialProfileId,sctjLoginId,pId,st,apc){
		urlstr='/candidate/socialconnect.html?pId='+pId+'&ajax=true&sctjLoginId='+sctjLoginId+"&pfId="+socialProfileId;
		if(st){
			urlstr+='&st='+st;
		}
		if(apc){
			urlstr+='&apc='+apc;
		}
		title =	'Login Details';
		childWindow =GB_showCenter(title, urlstr,350,600);
	}
</script> 
