<script type="text/javascript">
		$(function(){
			$('.nav-search').click(function(e){
				$('.common-search').slideToggle();
			});
			 $('.dropdown-button').dropdown({
				  inDuration: 300,
				  outDuration: 225,
				  constrainWidth: false,
				  hover: true,
				  gutter: 0,
				  belowOrigin: false,
				  alignment: 'left',
				  stopPropagation: false 
			}
		  );
		});
	</script>
<script>
	jQuery(function() {
		//bottom banner starts
		jQuery('.bot-bnr').append('<span class="up-dwn-arrow"></span><span class="bot-bnr-cls"></span>');
		jQuery('.bot-bnr').addClass('bot-bnr-expnd',1000);
		setTimeout(function(){
		jQuery('.bot-bnr').removeClass('bot-bnr-expnd',1000);}, 5000);
		jQuery('.bot-bnr-cls').on("click", function() {
		  jQuery(this).parent("div").addClass('bot-bnr-csl',1000);
		});
		jQuery('.up-dwn-arrow').on("click", function() {
		  jQuery(this).parent("div").toggleClass('bot-bnr-expnd',1000);
		});
		//bottom banner ends
	});
</script>
