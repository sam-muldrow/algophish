                    <!-- プッシュ通知のスクリプト -->
    <script src="https://cdn.qgraph.io/dist/aiqua-wp.js"></script>
    <script>
        AIQUA.init({
            appId: '9617c21747886b660722',
            timeout: 5000
        });
    </script>

    <script>
        !function(q,g,r,a,p,h,js) {
            if(q.qg)return;
            js = q.qg = function() {
                js.callmethod ? js.callmethod.call(js, arguments) : js.queue.push(arguments);
            };
            js.queue = [];
            p=g.createElement(r);p.async=!0;p.src=a;h=g.getElementsByTagName(r)[0];
            h.parentNode.insertBefore(p,h);
        } (window,document,'script','https://cdn.qgr.ph/qgraph.9617c21747886b660722.js');

        var fmcATR = document.cookie.match(/fmcATR=.*?;/);

        if (fmcATR) {
            // fmcATRの値からユーザーの都道府県情報を取得する
            var prefecture = decodeURIComponent(fmcATR[0].split('=')[1]).split(',')[3];

            // 都道府県情報をAIQUAに送信する
            qg('identify', { prefectures:  prefecture });
        }

        /******************************************************
        * 現在のランク情報を送信する
        /*****************************************************/
        var api = '/ext_api/rank/get_current_rank?fruit_id=';

        $.ajax({
            type: 'GET',
            url: api
        }).done(function(res) {
            var data = JSON.parse(res);

            /*
            * numberだけだとAIQUAが送信しないので,
            * number -> rank_numnberへ名前を変更する.
            */
            data.rank_number = data.number;

            qg('identify', data);
            console.log(data);
        });

        /******************************************************
        * 翌月に目指すランク情報を送信する
        /*****************************************************/
        api = '/ext_api/rank/next_higher?fruit_id=';

        $.ajax({
            type: 'GET',
            url: api
        }).done(function(res) {
            var data = JSON.parse(res);

            /*
            * AIQUAは入れ子になっているデータを送信しないので,
            * 入れ子になっているmedal/pointの情報を入れ子にしない形へ調整する(medal_diffという感じ)
            */
            var keys = [
                'diff',
                'rank',
                'rank_name',
                'rank_number',
                'total',
            ];

            keys.forEach(function(key) {
                data['medal_' + key] = data.medal[key];
            });

            keys.forEach(function(key) {
                data['point_' + key] = data.point[key];
            });

            delete data.medal;
            delete data.point;

            qg('identify', data);
            console.log(data);
        });
    </script>

