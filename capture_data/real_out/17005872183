        getline(".line1")
        getline(".line2")
        getline(".line3")
        getline(".line4")

        function getline(select) {
            var child = $(select).children()
            var sum = 0
            var ind = 0
            var storenode = $(select + " .line-container")

            for (var i = 0; i < child.length; i++) {
                if (sum < 700) {
                    sum = sum + $(select + " .line-container").eq(i).width()

                    if (i == (child.length - 1)) {
                        ind = i
                    }
                } else {
                    ind = i - 1
                    break
                }
            }
            var hiddenwidth = 0
            var lefts = 15
            for (var i = 0; i < ind; i++) {
                hiddenwidth = hiddenwidth + $(select + " .line-container").eq(i).width()
                lefts = lefts + 15
            }
            var width = 776 - hiddenwidth - lefts
            $(select + " .line-container a").eq(ind).css({
                "width": width + "px",
                "white-space": "nowrap",
                "overflow": "hidden",
                "text-overflow": "ellipsis",
            })
            for (var i = ind + 1; i < child.length; i++) {
                storenode.eq(i).remove()
            }
        }
