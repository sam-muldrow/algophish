<script>

loadDW.addEventListener('load', function () {
    $(function() {

        dw_connect_csrf(function (data) {

            var connect_csrf = jQuery('<input/>', {
                name: 'csrf_token',
                value: data.csrf_hash,
                type: 'hidden'
            });

            $('#js-signup').append(connect_csrf);
        });

        $(document).on('change', 'input[name="email"]', function(event) {

            var username = $('input[name="username"]');

            if (username.val().length === 0)
            {
                $.post(
                    '/connect/ajax/profile/username',
                    {
                        email: $(this).val(),
                        csrf_token: csrf_hash
                    },
                    function(data) {
                        if (data.username != '')
                        {
                            username.val(data.username);
                        }
                    }
                );
            }
        });
    

    });
});
</script>
