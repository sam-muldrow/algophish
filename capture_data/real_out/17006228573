            <div style="display:none;" class="toYoutube">
              <a href="https://www.youtube.com/@locondoch/featured" target="_blank" onclick="ga('send', 'event', 'lookbook_link', 'click_lookbook', 'click_lookbook_pc');gtag('event', 'click_event', {'click_category': 'lookbook_link', 'click_label': 'click_lookbook_pc', 'click_action': 'banner'});"><i uk-icon="youtube"></i>YouTube</a>
            </div>
