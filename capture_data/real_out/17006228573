        <script type="text/x-handlebars-template" id="template_pcTopNav_l">
          {{#each this}}
            {{#if (lte @index 2)}}
              <li data-item="{{category.code}}">
                <a href="/shop/category/{{category.code}}">{{category.name}}</a>
              </li>
              {{#each category_child}}
                {{#if (lte @index 7)}}
                  <li data-item="{{code}}" class="-child">
                    <a href="/shop/category/{{code}}">{{name}}</a>
                  </li>
                {{/if}}
              {{/each}}
            {{/if}}
            {{#if (gte @index 3)}}
              <li data-item="{{category.code}}">
                <a href="/shop/category/{{category.code}}">{{category.name}}</a>
              </li>
            {{/if}}
          {{/each}}
        </script>
        <script type="text/x-handlebars-template" id="template_pcTopNav_m">
          {{#each this}}
            {{#if (lte @index 2)}}
              <li data-item="{{category.code}}">
                <a href="/shop/category/{{category.code}}">{{category.name}}</a>
              </li>
              {{#each category_child}}
                {{#if (lte @index 4)}}            
                  <li data-item="{{code}}" class="-child">
                    <a href="/shop/category/{{code}}">{{name}}</a>
                  </li>
                {{/if}}
              {{/each}}
            {{/if}}
            {{#if (gte @index 3)}}
              <li data-item="{{category.code}}">
                <a href="/shop/category/{{category.code}}">{{category.name}}</a>
              </li>
            {{/if}}
          {{/each}}
        </script>
        <script type="text/x-handlebars-template" id="template_pcTopNav_noChild">
          {{#each this}}
            <li data-item="{{category.code}}">
              <a href="/shop/category/{{category.code}}">{{category.name}}</a>
            </li>
          {{/each}}
        </script>
        <script type="text/x-handlebars-template" id="template_pcTopNav_girls_boys">
          {{#each this}}
            {{#if (eq @index 0)}}
              <li data-item="{{category.code}}">
                <a href="/shop/category/{{category.code}}">{{category.name}}</a>
              </li>
              {{#each category_child}}
                {{#if (lte @index 2)}}            
