      <!---------------------------------------------- 
      section-hotBrand 人気ブランド
      ------------------------------------------------>
      <section class="section-hotBrand">
        <div class="section-heading-wrap">
          <h2 class="section-heading">人気ブランド</h2>
        </div>
        <div class="hotBrand-wrap">
          <ul class="hotBrand-list" id="top_hotBrand">
            <li class="loadingImg"><img src="https://media.aws.locondo.jp/contents/img/sp/top/top_loader.gif"></li>
          </ul>
        </div>
        <div class="btn-visibleAll"><span>11〜50位</span></div>
        <!-- handlebarsテンプレート設定 - HOT BRAND -->
        <!-- GA計測イベントのテキスト(括弧など除去) -->
        <script>
          Handlebars.registerHelper('gaString', function(txt) {
            return new Handlebars.SafeString(txt.replace(/('|"|,)/g, ''));
          });
        </script>
        <!-- 価格の区切り -->
        <script>
          Handlebars.registerHelper('toLocaleString', function(str) {
            var number = Number(str);
            return number.toLocaleString();
          });
          Handlebars.registerHelper('encodeURIComponentTwice', function(str) {
            var encode1 = encodeURIComponent(str);
            var encode2 = encodeURIComponent(encode1);
            return encode2;
          });
        </script>
        <script type="text/x-handlebars-template" id="hotBrand_template">
          {{#each brands}}
          <li class="hotBrand-item">
            <span class="badge -round"><em>{{rank}}</em></span>
            <span class="brandName">{{brand_name}}</span>
            <ul class="catalog-2pSet">
              <li class="catalog-item">
                <a href="/shop/commodity/{{commodities.[0].shop_code}}/{{commodities.[0].commodity_code}}" onclick="ga('send', 'event', 'topHotBrand', '{{gaString brand_name}}', '{{commodities.[0].commodity_code}}');gtag('event', 'click_event', {'click_category': 'pcTopHotBrand', 'click_label': '{{gaString brand_name}}', 'click_action': 'banner', 'click_commoditycode': '{{commodities.[0].commodity_code}}'});">
                  <div class="catalog-imgContainer">
                    <img class="catalog-img lazy" data-src="{{commodities.[0].image_link}}">
                  </div>
                  <span class="catalog-commodityName">{{commodities.[0].commodity_name}}</span>
                  {{#if commodities.[0].discount_price}}
                  <p class="catalog-price -discount">&yen;{{toLocaleString commodities.[0].discount_price}}</p>
                  {{else}}
                  <p class="catalog-price">&yen;{{toLocaleString commodities.[0].unit_price}}</p>
                  {{/if}}
                </a>
              </li>
              <li class="catalog-item">
                <a href="/shop/commodity/{{commodities.[1].shop_code}}/{{commodities.[1].commodity_code}}" onclick="ga('send', 'event', 'topHotBrand', '{{gaString brand_name}}', '{{commodities.[1].commodity_code}}');gtag('event', 'click_event', {'click_category': 'pcTopHotBrand', 'click_label': '{{gaString brand_name}}', 'click_action': 'banner', 'click_commoditycode': '{{commodities.[1].commodity_code}}'});">
                  <div class="catalog-imgContainer">
                    <img class="catalog-img lazy" data-src="{{commodities.[1].image_link}}">
                  </div>
                  <span class="catalog-commodityName">{{commodities.[1].commodity_name}}</span>
                  {{#if commodities.[1].discount_price}}
                  <p class="catalog-price -discount">&yen;{{toLocaleString commodities.[1].discount_price}}</p>
                  {{else}}
                  <p class="catalog-price">&yen;{{toLocaleString commodities.[1].unit_price}}</p>
                  {{/if}}
                </a>
              </li>
            </ul>
            <span class="btn-toBrand">
              <a href="/shop/brand/{{encodeURIComponentTwice brand_name}}" onclick="ga('send', 'event', 'topHotBrand', '{{gaString brand_name}}', 'btn_viewAll');gtag('event', 'click_event', {'click_category': 'pcTopHotBrand', 'click_label': '{{gaString brand_name}}_btn_viewAll', 'click_action': 'text'});">すべて見る</a>
            </span>
          </li>
          {{/each}}
        </script>
      </section>

