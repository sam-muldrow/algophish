        <script type="text/x-handlebars-template" id="limitedSku_template">
          <li class="catalog-item {{imageRatioByCommodityCode sku}}">
            <a class="catalog-link" href="{{url}}" target="_self" onclick="ga('send', 'event', 'limited-pc-{{shopCode}}', 'banner');gtag('event', 'click_event', {'click_category': 'topPage', 'click_label': 'limited-pc-{{shopCode}}', 'click_action': 'banner'});">
