        <!-- ※※※※※※ 本番：coordinate40s-category=1817 QA：coordinate40s-category=1812 -->
        <script type="text/javascript">
          function doCoordinate(){
            var domain       = document.domain;
            var regexp       =  /^qa\d/;
            var check_domain = '';
            var jsonUrl      = '';
            if (regexp.test(domain)){
              check_domain = 'qa';
            }
            if ( check_domain == 'qa' ){
              jsonUrl = '/shop/contents/note/wp-json/wp/v2/posts?categories=1812&per_page=15';
            } else {
              jsonUrl = '/shop/contents/note/wp-json/wp/v2/posts?categories=1817&per_page=15';
            }
            console.log('doCoordinate');

            var func_getJSON = function(){
              var def = $.Deferred();
              $.getJSON( jsonUrl, function(results) {
                $.each(results, function(i, item) {
                  var date_data = new Date(item.date);
                  var year      = date_data.getFullYear();
                  var month     = date_data.getMonth() + 1;
                  var day       = date_data.getDate();
                  var date      = year + '.' + month + '.' +day;
                  var link      = item.link;
                  var img       = item.acf['carousel-image'];
                  var title_raw   = item.title.rendered;
                  var title_array = title_raw.split(/（No.*）/);
                  var html 
                    = '<li>' 
                    + '<a href="' + link + '" onClick="ga(\'send\', \'event\', \'coordinate_link\', \'click_coordinate\', \'click_coordinate_pc\');gtag(\'event\', \'click_event\', {\'click_category\': \'coordinate_link\', \'click_label\': \'click_coordinate_pc\', \'click_action\': \'banner\'});" target="_blank">'
                    + '<div class="imgWrap"><img src="' + img + '"></div>'
                    + '<div class="textGroup">'
                    + '<span class="title">' + title_array[1] + '</span>'
                    + '<span class="caption">' + date + '</span>'
                    + '</div>'
                    + '</a></li>';

                  $("#coordinateList").append(html);
                  console.log('append');
                });
                def.resolve(results);
              });
              return def.promise();
            };
            func_getJSON().done(function(){
              $("#coordinateList").slick({
                slidesToShow  : 5,
                slidesToScroll: 5,
                arrows        :true,
                prevArrow     : '<button class="arrow -left"><i uk-icon="chevron-left" class="uk-icon"></button>',
                nextArrow     : '<button class="arrow -right"><i uk-icon="chevron-right" class="uk-icon"></button>',
              });
            });

          }
          ScrollTrigger.create({
            trigger: '.section-coordinate',
            start: 'top bottom',
            end: 'bottom bottom',
            once: true,
            onEnter: () => doCoordinate(),
          });
        </script>
      </section>

