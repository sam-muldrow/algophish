            {
                key: "favouriteListingMap",
                callback: FavouriteData.fetchFavouriteListings
            },
            {
                key: "todoListingMap",
                callback: TodoData.fetchTodoListings
            },
            {
                key: "bookmarkedArticleMap",
                callback: ArticleBookmarkData.fetchBookmarkedArticles
            },
            {
                key: "bookmarkedVideoMap",
                callback: VideoBookmarkData.fetchBookmarkedVideos
            }
        );

        window.freshDaily.sessionLogicList = window.freshDaily.sessionLogicList.concat(sessionLogicList);
    })();
</script>




<script type="text/javascript">
    window.freshDaily.facebookPromise = new Promise(function (resolve, reject) {
        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) return;
            js = d.createElement(s);
            js.id = id;
            js.src = "//connect.facebook.net/en_US/sdk.js";
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));

        window.fbAsyncInit = function () {
            FB.init({
                appId: "100616073343311",
                xfbml: true,
                version: "v2.5"
            });

            resolve();
        }
    });
</script>

