    <script>
      window.googletag = window.googletag || {cmd: []};
      googletag.cmd.push(function() {
      // Refresh targeting keyvalue pairing.
      var REFRESH_KEY = 'refresh';
      var REFRESH_VALUE = 'true';

      // The below ad slots are allowed to refresh, as they contain the targeting keys which have been declared for refresh via setTargeting()
      // googletag.defineSlot('/70671651/blogto/desktop/arts/leaderboard_top', [[970, 250], [970, 90], [728, 90]], 'leaderboard_top').setTargeting(REFRESH_KEY, REFRESH_VALUE).addService(googletag.pubads());
      // googletag.defineSlot('/70671651/blogto/desktop/arts/rec_atf', [[300, 600], [300, 250]], 'rec_atf').setTargeting(REFRESH_KEY, REFRESH_VALUE).addService(googletag.pubads());
      // googletag.defineSlot('/70671651/blogto/desktop/arts/rec_btf', [300, 250], 'rec_btf').setTargeting(REFRESH_KEY, REFRESH_VALUE).addService(googletag.pubads());
      // googletag.defineSlot('/70671651/blogto/desktop/arts/leaderboard_instream', [[970, 90], [728, 90]], 'leaderboard_instream').setTargeting(REFRESH_KEY, REFRESH_VALUE).addService(googletag.pubads());
      // googletag.defineSlot('/70671651/blogto/desktop/arts/billboard_instream', [970, 250], 'billboard_instream').setTargeting(REFRESH_KEY, REFRESH_VALUE).addService(googletag.pubads());
      // googletag.defineOutOfPageSlot('/70671651/blogto/desktop/OOP', 'div-gpt-ad-1665342109022-0').addService(googletag.pubads());

      // Number of seconds to wait before refreshing ad slot with new ad requests, only after the slot becomes viewable. Does not require a page refresh.

        var SECONDS_TO_WAIT_AFTER_VIEWABILITY = 45;

        googletag.pubads().addEventListener('impressionViewable', function(event) {
          var slot = event.slot;
          if (slot.getTargeting(REFRESH_KEY).indexOf(REFRESH_VALUE) > -1) {
            setTimeout(function() {
              googletag.pubads().refresh([slot]);
              console.log('refresh', slot);
            }, SECONDS_TO_WAIT_AFTER_VIEWABILITY * 1000);
          }
        });

        // Lazy loading with GAM.

        googletag.pubads().enableLazyLoad({

          // Fetch slots within 2.5 viewports.
          fetchMarginPercent: 250,

          // Render slots within 1 viewports.
          renderMarginPercent: 100,

          // Double the above values on mobile, where viewports are smaller and users tend to scroll faster.
          mobileScaling: 2.0
        });

        googletag.pubads().enableSingleRequest();
        // googletag.pubads().collapseEmptyDivs();
        googletag.enableServices();

      });
    </script>




