<script type="text/javascript">
	layui.use('index', layui.factory('index'));
	layui.use('search', layui.factory('search'));
</script>
<script>
/*<![CDATA[*/
    var tel = '18888888888'
    ,qq = '478923'
    ,ajaxUrl = '//www.home77.com';
    localStorage.setItem('tel', tel);
    localStorage.setItem('qq', qq);

	
	layui.use(['form'],function(){
		var $         = layui.$
		,form         = layui.form;
		
		$('#startPrice').bind('input propertychange', function(){
			var endPrice=$("#endPrice").val();
			if(endPrice!=''&&$(this).val()!=''){
				$("#showPriceBtn").css("visibility","visible");
			}else{
				$("#showPriceBtn").css("visibility","hidden");
			}
		});
		$('#endPrice').bind('input propertychange', function(){
			var startPrice=$("#startPrice").val();
			if(startPrice!=''&&$(this).val()!=''){
				$("#showPriceBtn").css("visibility","visible");
			}else{
				$("#showPriceBtn").css("visibility","hidden");
			}
		});
		
		//自定义框
		$('#startArea').bind('input propertychange', function(){
			var endArea=$("#endArea").val();
			if(endArea!=''&&$(this).val()!=''){
				$("#showAreaBtn").css("visibility","visible");
			}else{
				$("#showAreaBtn").css("visibility","hidden");
			}
		});
		$('#endArea').bind('input propertychange', function(){
			var startArea=$("#startArea").val();
			if(startArea!=''&&$(this).val()!=''){
				$("#showAreaBtn").css("visibility","visible");
			}else{
				$("#showAreaBtn").css("visibility","hidden");
			}
		});
		
		//点击面积筛选
		$('#showAreaBtn').on('click', function(event) {
			findOfficeByAreaAndPrice();
		});
		
		//点击价格筛选
		$('#showPriceBtn').on('click', function(event) {
			findOfficeByAreaAndPrice();
		});
		
		function findOfficeByAreaAndPrice(){
			var startArea=$("#startArea").val();
			var endArea=$("#endArea").val();
			var startPrice=$("#startPrice").val();
			var endPrice=$("#endPrice").val();
			var param = '';
			if(startArea!=""&&startArea!=undefined){
				if (param.length > 0) param += '&';
				param += 'startArea='+startArea;
			}
			if(endArea!=""&&endArea!=undefined){
				if (param.length > 0) param += '&';
				param += 'endArea='+endArea;
			}
			if(startPrice!=""&&startPrice!=undefined){
				if (param.length > 0) param += '&';
				param += 'startPrice='+startPrice;
			}
			if(endPrice!=""&&endPrice!=undefined){
				if (param.length > 0) param += '&';
				param += 'endPrice='+endPrice;
			}
			window.open('/zulin/___'+startArea+','+endArea+'_'+startPrice+'-'+endPrice+'__1_0_1.html');
			
		}
		
		//地铁线select
        form.on('select(subwaySelectFilter)', function(data){
             var subwaySelectId=data.value;
             window.open(subwaySelectId);
        });
	});
/*]]>*/		
</script>

