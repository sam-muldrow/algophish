    <script>
    
    firstTime = true;
    const languageFromUrl = 'es_ES';
      document.addEventListener('bsLanguageSelectorChanged',(e) => {
        if (firstTime) {
          const languageSelector = document.querySelector('bs-footer').shadowRoot.querySelector("bs-language-selector");
          languageSelector.setAttribute('selected-language',languageFromUrl);
          firstTime = false;
        } else {
          switch (e.detail.language) {
          case 'es_ES':
            if ('es' != 'es') {
              window.location.href = '/bsnacional/es/particulares/';
            }
            break;
          case 'en_GB':
            if ('en' != 'es') {
              window.location.href = '/bsnacional/en/personal/';
            }
            break;
          case 'ca_ES':
            if ('ca' != 'es') {
              window.location.href = '/bsnacional/ca/particulars/';
            }
            break;
          default:
          }
        }
      });
    </script>
	
  
    
            


  


