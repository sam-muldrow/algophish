<div class="coh-container ssa-component coh-component ssa-component-instance-512c0b1e-84e9-4118-b8b0-04942154ab9d coh-component-instance-512c0b1e-84e9-4118-b8b0-04942154ab9d ssa-instance-8bc34e363ba6c500230aef5cd86d6952 coh-ce-cpt_bau_container-b77f3116"> <div class="coh-container ssa-instance-8bc34e363ba6c500230aef5cd86d6952 coh-ce-cpt_bau_container-5261b2c8"> <div class="coh-container ssa-component coh-component ssa-component-instance-86771038-e78b-4f0e-b948-14f22ce7ffb0 coh-component-instance-86771038-e78b-4f0e-b948-14f22ce7ffb0  ssa-instance-1ea236b87c0d615be7e2992098eb2469 coh-ce-cpt_vodacom_heading_responsive-49ce5c2a"> <h2 class="coh-heading ssa-instance-1ea236b87c0d615be7e2992098eb2469 coh-ce-cpt_vodacom_heading_responsive-7d76b287"> Black Friday Month </h2> </div>
<div class="coh-container ssa-component coh-component ssa-component-instance-8f381e17-b77a-49c2-b973-e60e0fcdbea6 coh-component-instance-8f381e17-b77a-49c2-b973-e60e0fcdbea6  ssa-instance-f7ba55416305e1f3659e52edf132ccda coh-ce-cpt_vodacom_heading_responsive-49ce5c2a"> <h3 class="coh-heading ssa-instance-f7ba55416305e1f3659e52edf132ccda coh-ce-cpt_vodacom_heading_responsive-7d76b287"> SIM-only &amp; Home Internet </h3> </div>
<script class="coh-inline-element ssa-component coh-component ssa-component-instance-0f06ad14-87a1-4775-b554-f3790feeb1f4 coh-component-instance-0f06ad14-87a1-4775-b554-f3790feeb1f4">if (!window.hasOwnProperty('blackFridayDealCard')) {
            window.blackFridayDealCard = true;
            var swipeTolerance = 100;
            var prevWindowWidth = window.innerWidth;

            jQuery(document).ready(() => {
                var railClass = '.bau-glass-rail';
                var slideClass = '.bau-glass-slide';
                var cardClass = '.bau-glass-card';
                var leftButtonClass = '.bau-glass-left';
                var rightButtonClass = '.bau-glass-right';
                var paginationClass = '.bau-glass-pagination';
                var dotClass = '.bau-glass-dot';

                function moveSlides(rail) {
                    var slides = rail.querySelectorAll(slideClass);
                    var leftButton = rail.querySelector(leftButtonClass);
                    var rightButton = rail.querySelector(rightButtonClass);
                    var dots = rail.querySelectorAll(dotClass);
                    
                    if (rail.currentSlide <= 0) {
                        rail.currentSlide = 0;
                        leftButton.style.display = 'none';
                    }
                    else {
                        leftButton.style.display = 'block';
                    }
                    if (rail.currentSlide >= slides.length - 1) {
                        rail.currentSlide = slides.length - 1;
                        rightButton.style.display = 'none';
                    }
                    else {
                        rightButton.style.display = 'block';
                    }

                    dots.forEach((dot, index) => {
                        if (index == rail.currentSlide) {
                            dot.classList.add('bau-active');
                        }
                        else {
                            dot.classList.remove('bau-active');
                        }
                    });

                    slides.forEach((slide) => {
                        slide.style.transform = 'translateX(-' + (100 * rail.currentSlide) + '%)';
                    });
                }

                function createSlides(includeInitialised = true) {
                    var rails;
                    if (includeInitialised) {
                        rails = document.querySelectorAll(railClass);
                    }
                    else {
                        rails = document.querySelectorAll(railClass + ':not(.init)');
                    }
                    rails.forEach((rail) => {
                        var cards = rail.querySelectorAll(cardClass);
                        var firstCard = cards[0];
                        var compStyle = window.getComputedStyle(firstCard);
                        var cardsPerSlide = 1;
                        switch(compStyle.width) {
                            case 'calc(25% - 16px)': 
                                { 
                                    cardsPerSlide = 4; 
                                } break;
                            case 'calc(33.333% - 16px)': 
                                { 
                                    cardsPerSlide = 3; 
                                } break;
                            case 'calc(50% - 16px)': 
                                { 
                                    cardsPerSlide = 2; 
                                } break;
                            case 'calc(100% - 16px)': 
                                { 
                                    cardsPerSlide = 1; 
                                } break;
                            default: 
                                { 
                                    cardsPerSlide = Math.floor(firstCard.parentNode.clientWidth / firstCard.clientWidth);
                                }
                        }
                        var pagination = rail.querySelector(paginationClass);
                        var dot = rail.querySelector(dotClass);
                        var oldSlides = rail.querySelectorAll(slideClass);
                        var neededSlides = Math.ceil(cards.length / cardsPerSlide);
                        pagination.replaceChildren();
                        if (neededSlides > 1) {
                            pagination.style.display = 'flex';
                        }
                        for (var i = 0; i < neededSlides; i++) {
                            rail.prepend(oldSlides[0].cloneNode());
                            pagination.prepend(dot.cloneNode());
                        }
                        var slides = rail.querySelectorAll(slideClass);
                        var cardCount = 0;
                        var slideCount = 0;
                        cards.forEach((card) => {
                            if (cardCount >= cardsPerSlide) {
                                cardCount = 0;
                                slideCount++;
                            }
                            slides[slideCount].appendChild(card);
                            cardCount++;
                        });
                        oldSlides.forEach((slide) => {
                            slide.remove();
                        });
                        
                        rail.currentSlide = 0;

                        var dots = rail.querySelectorAll(dotClass);
                        dots.forEach((dot, index) => {
                            dot.addEventListener('click', (e) => {
                                rail.currentSlide = index;
                                moveSlides(rail);
                            });
                            if (index == 0) {
                                dot.classList.add('bau-active');
                                dot.click();
                            }
                            else {
                                dot.classList.remove('bau-active');
                            }
                        });

                        if (!includeInitialised) {
                            rail.classList.add('init');

                            var leftButton = rail.querySelector(leftButtonClass);
                            var rightButton = rail.querySelector(rightButtonClass);

                            leftButton.addEventListener('click', (e) => {
                                rail.currentSlide--;
                                moveSlides(rail);
                            });
                
                            rightButton.addEventListener('click', (e) => {
                                rail.currentSlide++;
                                moveSlides(rail);
                            });

                            rail.addEventListener('touchstart', (e) => {
                                rail.x0 = e.changedTouches[0].clientX;
                            });

                            rail.addEventListener('touchend',(e) => {
                                var x1 = e.changedTouches[0].clientX;
                                var diffX = rail.x0 - x1;
                                    if (diffX > swipeTolerance) {
                                        rail.currentSlide++;
                                        moveSlides(rail);
                                    }
                                    else if (diffX < swipeTolerance * -1){
                                        rail.currentSlide--;
                                        moveSlides(rail);
                                    }
                            });
                        }
                    });
                }

                function initSlides() {
                    createSlides(false);
                }
                
                var resizeTimeout;
                window.addEventListener('resize', () => {
                    if (window.innerWidth !== prevWindowWidth) {
                        prevWindowWidth = window.innerWidth;
                        clearTimeout(resizeTimeout);
                        resizeTimeout = setTimeout(createSlides, 200);
                    }
                });
                initSlides();
            }, false);
        }</script> <style class="coh-inline-element ssa-component coh-component ssa-component-instance-0f06ad14-87a1-4775-b554-f3790feeb1f4 coh-component-instance-0f06ad14-87a1-4775-b554-f3790feeb1f4">@media (min-width : 1921px) {
    .bau-glass-left,
    .bau-glass-right {
        width: 95px;
        height: 95px;
        border-radius: 50%
    }

    .bau-glass-left:after {
        transform: translate(-50%, -50%) rotate(-45deg);
        left: 50%;
    }
    
    .bau-glass-right:after {
        transform: translate(50%, -50%) rotate(45deg);
        right: 50%;
    }
}</style> <div class="coh-container ssa-component coh-component ssa-component-instance-0f06ad14-87a1-4775-b554-f3790feeb1f4 coh-component-instance-0f06ad14-87a1-4775-b554-f3790feeb1f4 bau-glass-rail coh-ce-cpt_bau_glass-4a9d322b"> <div class="coh-container bau-glass-slide coh-ce-cpt_bau_glass-a22692a6"> <div class="coh-container bau-glass-card coh-ce-cpt_bau_glass-faf4a11d1 coh-ce-cpt_bau_glass-faf4a11d"> <p class="coh-paragraph ssa-instance-b3810321b94fe926269caef15b643d56 coh-ce-cpt_bau_glass-8ee4cdae1 coh-ce-cpt_bau_glass-8ee4cdae">SIM ONLY</p> <div class="coh-container coh-ce-cpt_bau_glass-6986e9cf1 coh-ce-cpt_bau_glass-6986e9cf"> <img class="coh-image coh-ce-cpt_bau_glass-1d4ec35a1 coh-ce-cpt_bau_glass-1d4ec35a coh-image-responsive-xl" src="/sites/vodacomcoza/files/2023-11/11_1.webp" title="500GB Anytime data + 400GB Night Owl data" alt="500GB Anytime data + 400GB Night Owl data" /> </div> <div class="coh-container coh-ce-cpt_bau_glass-3f8af86d1 coh-ce-cpt_bau_glass-3f8af86d"> <h5 class="coh-heading coh-ce-cpt_bau_glass-5d91e0b1 coh-ce-cpt_bau_glass-5d91e0b"> </h5> <p class="coh-paragraph coh-ce-cpt_bau_glass-351d06801 coh-ce-cpt_bau_glass-351d0680">500GB Anytime data + 400GB Night Owl data</p> <div class="coh-container coh-ce-cpt_bau_glass-e26cf4a21 coh-ce-cpt_bau_glass-e26cf4a2"> <div class="coh-container coh-ce-cpt_bau_glass-6d7d33c11 coh-ce-cpt_bau_glass-6d7d33c1"> <h6 class="coh-heading coh-ce-cpt_bau_glass-986a66e81 coh-ce-cpt_bau_glass-986a66e8"> R399 PM x 36 </h6> <div class="coh-wysiwyg coh-ce-cpt_bau_glass-f211152c1 coh-ce-cpt_bau_glass-f211152c"> <p id="R1579-PM-X-36-0"><s id="R1579-PM-X-36-1">R1579 PM X 36</s></p>
