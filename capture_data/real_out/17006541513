<div id="block-vodacom-sub-theme-content" data-block-plugin-id="system_main_block" class="block block-system block-system-main-block">
<article data-history-node-id="34001" role="article" id="node--full-34001" about="/vodacom/home">
<div class="field field--name-field-blank-canvas-layout-canvas field--type-cohesion-entity-reference-revisions field--label-hidden field__item" id="field__item--field_blank_canvas_layout_canvas--34001--0"> <style class="coh-inline-element ssa-component coh-component ssa-component-instance-38109a6f-60ca-4878-87c9-663ef8a520b4 coh-component-instance-38109a6f-60ca-4878-87c9-663ef8a520b4">body {
  --vodacom-red: #e60000;
  --vodacom-dark-red: #501a12;
}

@keyframes pulsate {
               0% {
                    opacity: 1;
                    transform: translate(-50%, -50%) scale(1); 
                    
                }
                100% {
                    opacity: 0;
                    transform: translate(-50%, -50%) scale(.75);
                }
            }</style> <div class="coh-container ssa-component coh-component ssa-component-instance-38109a6f-60ca-4878-87c9-663ef8a520b4 coh-component-instance-38109a6f-60ca-4878-87c9-663ef8a520b4 coh-ce-cpt_bau_bf_animated_circles-d661eb82"> <div class="coh-container coh-ce-cpt_bau_bf_animated_circles-56749b11"> </div> <div class="coh-container coh-ce-cpt_bau_bf_animated_circles-7f7c5217"> </div> <div class="coh-container coh-ce-cpt_bau_bf_animated_circles-52e0e8ac"> </div> </div>
<script class="coh-inline-element ssa-component coh-component ssa-component-instance-84f025cf-f9e0-4fab-a40a-87ad96ec3b6c coh-component-instance-84f025cf-f9e0-4fab-a40a-87ad96ec3b6c">if (!window.hasOwnProperty('blackFridayBanner')) {
            window.blackFridayBanner = true;
            var swipeTolerance = 100;
            var prevWindowWidth = window.innerWidth;

            jQuery(document).ready(function() {
                var carousels = document.querySelectorAll('.bau-carousel');

                carousels.forEach((carousel) => {
                    var currentBanner = 0;
                    var currentX = 0;
                    var banners = carousel.querySelectorAll('.bau-banner');
                    var pagination = carousel.querySelector('.bau-carousel-pagination');
                    var leftButton = carousel.querySelector('.bau-carousel-left');
                    var rightButton = carousel.querySelector('.bau-carousel-right');
                    var numberOfBanners = banners.length;

                    leftButton.addEventListener('click', function(e) {
                        currentBanner--;
                        anim();
                    });
                    rightButton.addEventListener('click', function(e) {
                        currentBanner++;
                        anim();
                    });
                    banners.forEach((banner) => {
                        banner.addEventListener('touchstart', (e) => {
                            currentX = e.changedTouches[0].clientX;
                        });
                        banner.addEventListener('touchend', (e) => {
                            var swipeTolerance = 100;
                            var diffX = currentX - e.changedTouches[0].clientX;
                            if (diffX > swipeTolerance) {
                                currentBanner++;
                                anim();
                            }
                            else if (diffX < swipeTolerance * -1) {
                                currentBanner--;
                                anim();
                            }
                        });
                    });

                    function draw() {
                        if (currentBanner <= 0) {
                            currentBanner = 0;
                            leftButton.style.display = 'none';
                        }
                        else {
                            leftButton.style.display = 'block';
                        }
                        if (currentBanner >= numberOfBanners - 1) {
                            currentBanner = numberOfBanners - 1;
                            rightButton.style.display = 'none';
                        }
                        else {
                            rightButton.style.display = 'block';
                        }
                        
                        var dots = pagination.querySelectorAll('.bau-carousel-dot');
                        dots.forEach((dot, index) => {
                            if (index == currentBanner) {
                                dot.classList.add('bau-active');
                            }
                            else {
                                dot.classList.remove('bau-active');
                            }
                        });
                    }

                    function anim() {
                        draw();
                        requestAnimationFrame(function() {
                            banners.forEach((banner) => {
                                banner.style.transform = 'translateX(-' + (currentBanner * 100) + '%';
                            });
                        });
                    }

                    function init() {
                        if (numberOfBanners > 1) {
                            pagination.style.display = 'flex';

                        }
                        else {
                            pagination.style.display = 'none';
                        }
                        var dot = pagination.querySelector('.bau-carousel-dot');

                        for (var i = 1; i < numberOfBanners; i++) {
                            var clonedDot = dot.cloneNode();
                            
                            pagination.appendChild(clonedDot);
                        }

                        var dots = pagination.querySelectorAll('.bau-carousel-dot');
                        dots.forEach((dot, index) => {
                            dot.addEventListener('click', (e) => {
                                e.preventDefault();
                                currentBanner = index;
                                anim();
                            });
                        });
                        draw();
                    }

                    var resizeTimeout;
                    window.addEventListener('resize', () => {
                        if (window.innerWidth !== prevWindowWidth) {
                            prevWindowWidth = window.innerWidth;
                            clearTimeout(resizeTimeout);
                            resizeTimeout = setTimeout(draw, 200);
                        }
                    });

                    init();
                });
            });
        }</script> <style class="coh-inline-element ssa-component coh-component ssa-component-instance-84f025cf-f9e0-4fab-a40a-87ad96ec3b6c coh-component-instance-84f025cf-f9e0-4fab-a40a-87ad96ec3b6c">@media (min-width : 1921px) {
    .bau-carousel-left,
    .bau-carousel-right {
        width: 95px;
        height: 95px;
        border-radius: 50%
    }

    .bau-carousel-left:after {
        transform: translate(-50%, -50%) rotate(-45deg);
        left: 50%;
    }
    
    .bau-carousel-right:after {
        transform: translate(50%, -50%) rotate(45deg);
        right: 50%;
    }
}</style> <div class="coh-container ssa-component coh-component ssa-component-instance-84f025cf-f9e0-4fab-a40a-87ad96ec3b6c coh-component-instance-84f025cf-f9e0-4fab-a40a-87ad96ec3b6c bau-carousel coh-ce-cpt_bau_banner-ce5fb707"> <div class="coh-container bau-banner coh-ce-cpt_bau_banner-a95de6df1 coh-ce-cpt_bau_banner-a95de6df"> <a class="coh-container coh-ce-cpt_bau_banner-f9bf56f21 coh-ce-cpt_bau_banner-f9bf56f2" href="/shopping/deal-details-page/DV3K07DAAG?contractSkus=DV3K07DAAA&amp;contractSkus=DV3K07DAAG&amp;icmp=internal/HPTO/Banner1/november2023/HomeInternet30mbps"> <div class="coh-container coh-ce-cpt_bau_banner-3324c9321 coh-ce-cpt_bau_banner-3324c932"> </div> <img class="coh-image coh-ce-cpt_bau_banner-1e015aae1 coh-ce-cpt_bau_banner-1e015aae coh-image-responsive-xl" src="/sites/vodacomcoza/files/2023-11/b11.webp" title="Get 40% off Home Internet 30Mbps" alt="Get 40% off Home Internet 30Mbps" /> </a> <div class="coh-container coh-ce-cpt_bau_banner-d66842071 coh-ce-cpt_bau_banner-d6684207"> <h1 class="coh-heading coh-ce-cpt_bau_banner-940149391 coh-ce-cpt_bau_banner-94014939"> SKIP THE LINE </h1> <h2 class="coh-heading coh-ce-cpt_bau_banner-f4279e0d1 coh-ce-cpt_bau_banner-f4279e0d"> Get 40% off Home Internet 30Mbps </h2> <p class="coh-paragraph coh-ce-cpt_bau_banner-237aacc11 coh-ce-cpt_bau_banner-237aacc1">Was R499 per month x 36</p> <div class="coh-container coh-ce-cpt_bau_banner-1c68f98c1 coh-ce-cpt_bau_banner-1c68f98c"> <div class="coh-container ssa-instance-4265e5dabebb6e2d3820fc8550b7b782 coh-ce-cpt_bau_banner-52b049531 coh-ce-cpt_bau_banner-52b04953"> <p class="coh-paragraph coh-ce-cpt_bau_banner-5b8ca9901 coh-ce-cpt_bau_banner-5b8ca990">Now only</p> <h3 class="coh-heading coh-ce-cpt_bau_banner-8784e4c71 coh-ce-cpt_bau_banner-8784e4c7"> R299 </h3> <p class="coh-paragraph coh-ce-cpt_bau_banner-db53e5141 coh-ce-cpt_bau_banner-db53e514">Per month x 36</p> </div> <div class="coh-container coh-ce-cpt_bau_banner-dca9d0411 coh-ce-cpt_bau_banner-dca9d041">