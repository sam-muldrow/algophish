function stockSiseRolling() {
var kospiObj = getObj("stockKospi");
var visible = isShow(kospiObj);
setShow(kospiObj, !visible);
setShow(getObj("stockKosdaq"), visible);

setTimeout("stockSiseRolling()",3000);
}


