function insuTrendSubmit() {
if(j$("select[id=insuKind]").val() == "") {
alert("º¸ÇèÁ¾·ù¸¦ ¼±ÅÃÇÏ¼¼¿ä");
}else if(j$("select[id=insuAge]").val() == "") {
