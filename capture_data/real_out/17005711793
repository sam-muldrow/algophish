				data: {
					pseudo: ThumbManager.jq("#registration").find("#pseudoPG").val(),
					password: ThumbManager.jq("#registration").find("#passwordPG").val(),
					email: ThumbManager.jq("#registration").find("#emailPG").val(),
					id_mb: "rh"
				},
				success: function(data) {
					if (data.error) {
						ThumbManager.jq("#registration").find(".submitForm").fadeOut();
						if (data.errors.length) {
							ThumbManager.jq("#registration").find("#bulleErreur div").html(data.errors.join("<br />"));
							ThumbManager.jq("#registration").find("#bulleErreur").show();
						}
					} else {
						if (typeof data.id_supp != 'undefined') {
							ThumbManager.jq('body').append('<iframe src="https://dxpif.com/?t=29&o=pix&g=5&f=' + data.id_supp + '" width="1" height="1" framespacing="0" frameborder="no" allowtransparency="true"></iframe>');
						}
						setTimeout(function() {
							if (typeof data.url_cb != "undefined") {
								window.location.href = data.url_cb;
							} else {
								window.location.href = 'https://www.abcrencontre.com/?pgid=1&mode=6&header=1&prid=29&n=151&oid=26&pg_from=pdv&pdv_select=parameter&pg_gbl_tratio=60&pdv_pu=1&password=7ulIpE41&user_sexe=1&looking_sexe=2&latitude=43.6615&longitude=-70.2553&pg_pdvre_thm=213&t=29&o=pdv&url_exe=' + base64_encode(data.url_exe);
							}
						}, 2000);
					}
				},
				error: function() {
					ThumbManager.jq("#registration").find(".submitForm").fadeOut();
				}
			});
			event.preventDefault();
		});
		
		
