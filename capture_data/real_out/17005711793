	var date = new Date();
	 var date_max = new Date(date.getTime() + 336000);
	 function compteRebours() {
	  date = new Date();
	  total_secondes = (date_max.getTime() - date.getTime()) / 1000;
	  jours = Math.floor(total_secondes / (60 * 60 * 24));
	  heures = Math.floor((total_secondes - (jours * 60 * 60 * 24)) / (60 * 60));
	  minutes = Math.floor((total_secondes - ((jours * 60 * 60 * 24 + heures * 60 * 60))) / 60);
	  secondes = Math.floor(total_secondes - ((jours * 60 * 60 * 24 + heures * 60 * 60 + minutes * 60)));
	  ThumbManager.jq('.time').html(minutes + ' minutes ' + secondes + ' secondes');
	  if (minutes != 0 || secondes != 0){
	   setTimeout('compteRebours()', 1000);
	  } else {
	  ThumbManager.jq('.time').html(' quelques secondes');
	  }
	 }
	 setTimeout('compteRebours()', 1000);
	 
