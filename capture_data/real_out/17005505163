<script type="text/javascript">
   $('.nav-link').click(function(){
     $('.nav-link').removeClass('active');
     $('.tab-pane').removeClass('active');
   
     $(this).addClass('active');
   
     $('#'+$(this).attr('id')+'-panel').addClass('active');
   
   });
   $('.popup_out').mousedown(function(){
     if ($('.popup_out').css('display')!='none') $('.popup_out').css('display','none'); else $('.popup_out').css('display','block');
     if ($('.search-option-box').css('display')=='none') $('.search-option-box').css('display','block'); else $('.search-option-box').css('display','none');
   });
   $('.search-form-button-close').click(function(){
     $('.search-option-box').css('display','none');
     $('.popup_out').css('display','block');
   });

//hide active menus on click
$(document).mouseup(function(e)
{
e.stopPropagation();
var close_tools = $(".free_tools");
var free_tools_click = $("#free_tools_click");
// if the target of the click isn't the container nor a descendant of the container
if (!close_tools.is(e.target) && close_tools.has(e.target).length === 0 && !free_tools_click.is(e.target))
{
$(".free_tools").slideUp("fast");
}
var close_search = $(".search_wrap");
var search_icon = $(".searchicon");
var autosuggest = $(".ui-corner-all");
// if the target of the click isn't the container nor a descendant of the container
if (!close_search.is(e.target) && close_search.has(e.target).length === 0 && !search_icon.is(e.target) && !autosuggest.is(e.target))
{
$("#searchbox").slideUp("fast"); $("#topic_search_link").addClass("searchselectlink").removeClass("searchcatlink"); $("#picture_search_link").addClass("searchcatlink").removeClass("searchselectlink"); $("#author_search_link").addClass("searchcatlink").removeClass("searchselectlink"); $("#blog_search_link").addClass("searchcatlink").removeClass("searchselectlink");
}

$(".close_gen").click(function(){ $("#quote_gen").fadeOut("fast");});
var quote_gen = $("#quote_gen");
                    // if the target of the click isn't the container nor a descendant of the container
                    if (!quote_gen.is(e.target) && quote_gen.has(e.target).length === 0)
                    {
                    $("#quote_gen").fadeOut("fast");
                    }
                    
                    var close_menu = $(".menu_wrap");
                    var menu_icon = $(".menuicon");
                    // if the target of the click isn't the container nor a descendant of the container
                    if (!close_menu.is(e.target) && close_menu.has(e.target).length === 0 && !menu_icon.is(e.target))
                    {
                    $("#menubox").fadeOut("fast");
                    }
});
                                              
                                              
                                              
                                              
