<script type="text/javascript">
    $(document).ready(function(){
    $(".qgen").click(function(){
    $("#quote_gen").slideToggle("fast");
    qids = document.getElementById('quote_ids').value;
    random_quote_gen(qids);
                     });
    });
    
    //generate random quote
    function random_quote_gen(quote_ids){
        $.ajax({
               type: "POST",
               url: "https://www.searchquotes.com/0new/mods/tools/quote_generator.php",
               data: {quote_ids:quote_ids}
               })
        .done(function( result ) {$("#quote_gen").html( result+"<div style='margin-top:-25;font-size:28px;color:#000;display:inline-block;float:right;'><i class='fas fa-times-circle close_gen'></i></div>" );});//done
    }//generate random quote
</script>

