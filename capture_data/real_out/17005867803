    function setupAuthStateHandler() {
        FS.authStateHandler.init(
            function () {
                location.href = JSON.parse("\u0022\u002flogin\u003freturnUrl\u003d\u00252F\u0022");
            }, function () {
                location.href = JSON.parse("\u0022\u002flogout\u003freturnUrl\u003d\u00252F\u0022");
            });
        FS.authStateHandler.reflect(window.authState);
    }
</script>



