<script class="consent-independent" type="application/javascript">

    window['consentDataReadyPromise'] = new Promise(consentDataReadyExecutorFunc);

    function consentDataReadyExecutorFunc(resolve) {

        // The cookieName value must not be changed. The CMP
        // conditional script loader is dependent on this.
        const cookieName = "referrer"

        function getNested(obj, searchString) {
            for (let i = 0; i < obj.length; i++) {
                let dataLayerObj = obj[i]
                if (dataLayerObj[searchString] !== undefined)
                    return dataLayerObj[searchString]
            }
        }

        function setupSnowplow() {
            // SnowPlow can function without cookies if no consent has
            // been given.
            const spHasConsent = cmpHandler.hasConsent("Snowplow", "statistics");
            const referrer = getNested(window.dataLayer, cookieName);
            const sendHeartbeats = false;

            initSnowPlow(spHasConsent, referrer, sendHeartbeats);

            resolve();
        }

        window.consentConfig = {
            elementSelector: ".consent-dependent",
            refresh: {
                strategy: 'cookie',
                cookie: {
                    name: cookieName,
                    requiredConsentVendors: "Jyllands-Posten",
                    requiredConsentPurposes: "statistics"
                }
            },
            eventName: "consent_ready",
            consentDataReadyCallback: setupSnowplow
        }

        window.cmpHandler = new JpmCmp.CmpHandler(consentConfig, window);

        function cmpHandlerCookieBotCallback() {
            
                
                
                

                var c = Cookiebot,
                    d = document,
                    cookies = d.cookie.split(";"),
                    consentCookie = "",
                    consentDate = c.consentUTC,
                    consentExpiry = (!consentDate) ? null : new Date(
                        consentDate.setMonth(consentDate.getMonth() + 12)
                    ),
                    now = new Date();

                for (var i = 0; i < cookies.length; i++) {
                    if (/^ *CookieConsent/g.test(cookies[i])) {
                        consentCookie = cookies[i];
                    }
                }

                if (consentCookie !== "" && consentExpiry && now < consentExpiry) {
                    var expiry = consentExpiry.toUTCString();
                    d.cookie = consentCookie + ";expires=" + expiry + ';path=/';
                }

                

                cmpHandler.cookiebotCallback(Cookiebot.consent, Cookiebot.changed);
            
        }

        var domContentLoaded = new Promise(function(resolve) {
            window.addEventListener("DOMContentLoaded",resolve,false);
        })
        var cookieBotOnConsentReady = new Promise(function(resolve) {
            window.addEventListener("CookiebotOnConsentReady", resolve, false);
        })

        

        
        Promise.all([domContentLoaded, cookieBotOnConsentReady]).then(function(e) {
            cmpHandlerCookieBotCallback()
            
            window.addEventListener('CookiebotOnConsentReady', cmpHandlerCookieBotCallback)
        }, false);
    }
</script>

            

