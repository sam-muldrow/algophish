    <path d="m105.75,1.29h2.33c2.13,0,3.25.96,3.25,3.26v.52c0,2.25-1,3.43-3.23,3.43h-2.35V1.29Zm2.35,6.19c1.39,0,1.94-.59,1.94-2.28v-.66c0-1.77-.69-2.23-2.04-2.23h-1.02v5.17h1.12Z" />
    <path d="m112.91,1.29h1.23v2.98h3.02V1.29h1.23v7.21h-1.23v-3.2h-3.02v3.2h-1.23V1.29Z" />
    <path d="m119.97,5.23v-.66c0-2.23,1.25-3.4,3.07-3.4s3.03,1.09,3.03,3.4v.66c0,2.23-1.25,3.39-3.07,3.39s-3.03-1.09-3.03-3.39Zm4.81.14v-.79c0-1.61-.48-2.42-1.78-2.42-1.18,0-1.74.81-1.74,2.25v.79c0,1.62.5,2.44,1.78,2.44,1.18,0,1.74-.82,1.74-2.27Z" />
    <path d="m127.66,1.29h1.23v6.19h2.97v1.02h-4.2V1.29Z" />
    <path d="m133.26,1.29h2.33c2.13,0,3.25.96,3.25,3.26v.52c0,2.25-1,3.43-3.23,3.43h-2.35V1.29Zm2.35,6.19c1.39,0,1.94-.59,1.94-2.28v-.66c0-1.77-.69-2.23-2.04-2.23h-1.02v5.17h1.12Z" />
</svg>

        </div>
