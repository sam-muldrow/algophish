  */
  function show_VK() {
    $('#FB_tab').hide();
    $('#VK_tab').show();

    $('#fb_groups').hide();
    $('#vk_groups').show();
  }

  function initVK() {
    if ($("#vk_groups").length) {
      VK.Widgets.Group("vk_groups", {mode: 0, width: "240", height: "360"}, 33337763);
    }
  }
</script>


