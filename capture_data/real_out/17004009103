                                            <a class="tile__commentsCount" href="/news/Program+TV%3A+pocz%C4%85tek+listopada+2023.+Najlepsze+filmy+w+ofercie-152686#komentarze" >
                            <span class="tile__commentsCountValue">
                                7
                            </span>&nbsp;komentarzy                        </a>
                                                                
                                                            </div>
                    </div>
    </article>
                </li>
                    
            
                                                                                                                                        
                                                        
                    
                                    
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    
                
            
                            <li class="crs__item">
                        <article class="Tile tile tile--column " >

                    
    
    
        
    
    
        
