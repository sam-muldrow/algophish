                                        IRI._loggedInfo.personalData.firstname + ' ' + IRI._loggedInfo.personalData.surname :
                                        IRI._loggedInfo.name;

                                    const link = '/user/' + IRI._loggedInfo.name;

                                    document.write('<div class="userHeaderTop">' +
                                        '<div id="userHeaderButton"> ' +
                                        '<a href="'+link+'" class="user__avatar user__avatar--' +
                                        (window.innerWidth > 480 ? "large" : "tiny") +
                                        '">' +
                                        '<img class="user__avatarImg" src="' +
                                        IRI._loggedAvatar +
                                        '">' +
                                        "</a>" +
                                        '<a href="' +
                                        link +
                                        '">' +
                                        '<span class="user__name-wrap">' +
                                        '<span class="user__name">' +
                                        name +
                                        "</span>" +
                                        "</span>" +
                                        "</a>" +
                                        '<span id="userMenuToggle" class="user-menu_toggle">' +
                                        '<i class="ico ico--arrowDownThinMedium"></i>' +
                                        '<span id="userCounter" class="user-menu_toggle__notifications hide"></span>' +
                                        "</span>" +
                                        "</div>" +
                                        "</div>")
                                }
                            </script></div><div id="siteMenuWrapper" class="menu "><ul class="list"><li id="filmsMainMenuLink" class="item-top item__with-submenu "><a id="filmwebMainMobile" href="/films" class="item-title"> <span data-i18n="menu.films.main" data-cacheId="iriHeaderNew2">Filmy</span> <i class="ico ico--arrowDownThinLarge"></i> </a><div class="menu__submenu"><ul class="list list--nested"><li class="item-sub "><a href="/films"> <span data-i18n="menu.films.main_page" data-cacheId="iriHeaderNew2"> Strona gł&oacute;wna film&oacute;w</span> </a></li><li class="item-sub "><a href="/premiere"> <span data-i18n="menu.title.premieres" data-cacheId="iriHeaderNew2">Premiery i zapowiedzi</span> </a></li><li class="item-sub item-sub__space-bottom "><a href="/trailers"> <span data-i18n="menu.films.trailers" data-cacheId="iriHeaderNew2">Zwiastuny</span> </a></li><li class="item-sub "><a href="/films/search"> <span data-i18n="menu.films.catalog_films" data-cacheId="iriHeaderNew2">Baza film&oacute;w</span> </a></li><li class="item-sub "><a href="/persons/search"> <span data-i18n="menu.films.catalog_persons" data-cacheId="iriHeaderNew2">Baza os&oacute;b kina</span> </a></li><li class="item-sub "><a href="/characters/search"> <span data-i18n="menu.films.catalog_characters" data-cacheId="iriHeaderNew2">Baza postaci</span> </a></li><li class="item-sub item-sub__space-bottom "><a href="/worlds/search"> <span data-i18n="menu.films.catalog_worlds" data-cacheId="iriHeaderNew2">Baza świat&oacute;w</span> </a></li><li class="item-sub "><a href="/awards"> <span data-i18n="menu.films.awards" data-cacheId="iriHeaderNew2">Nagrody</span> </a></li><li class="item-sub "><a href="/news"> <span data-i18n="menu.title.news" data-cacheId="iriHeaderNew2">Newsy</span> </a></li><li class="item-sub "><a href="/reviews"> <span data-i18n="menu.title.reviews" data-cacheId="iriHeaderNew2">Recenzje</span> </a></li><li class="item-sub item-sub__space-bottom "><a href="/fwm"> <span data-i18n="menu.film