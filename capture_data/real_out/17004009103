    "use strict";

/* we using ['catch'] in this file, because YUI Compressor dont like .catch() methods */
/* globals IRI utils */

// import utils from 'Helpers/utils';

// import utils from "../../../src/js/common/helpers/utils";
// import {UserModule} from "../../../src/js/common/User";

// import waitingModule from "../../../src/js/common/helpers/WaitingModule";

const _typeof =
    typeof Symbol === "function" && typeof Symbol.iterator === "symbol"
        ? function (obj) {
            return typeof obj;
        }
        : function (obj) {
            return obj &&
            typeof Symbol === "function" &&
            obj.constructor === Symbol &&
            obj !== Symbol.prototype
                ? "symbol"
                : typeof obj;
        };

(function () {
    /* DO NOT IMPORT ANYTHING HERE! */

    if (!Array.intersect) {
        Array.prototype.intersect = function () {
            let a = Array.prototype.shift.apply(arguments),
                r = [];
            if (this.length > 0 && a.length > 0) {
                if (this.length >= a.length) {
                    a = a.concat([]);
                    outer: for (let i = this.length - 1; i > -1; i--) {
                        for (let j = a.length - 1; j > -1; j--) {
                            if (this[i] === a[j]) {
                                r.push(a.splice(j, 1)[0]);
                                if (a.length == 0) {
                                    break outer;
                                }
                            }
                        }
                    }
                } else {
                    r = a.intersect(this);
                }
            }
            return arguments.length > 0 ? r.intersect(arguments) : r;
        };
    }

    function WaitingModule() {
        this.loadedParts = [];
        this.toDo = {};
        this.confPerPart = {};
        this.tryRunningAll();
    }

    WaitingModule.prototype = {
        /**
         * Runs through the list of loaded parts and fires actions that have all the needed parts loaded.
         */
        tryRunningAll: function () {
            for (const parts in this.toDo) {
                if (this.toDo.hasOwnProperty(parts)) {
                    this.tryRunningForParts(parts);
                }
            }
        },

        /**
         * Try running actions registered for given set of parts.
         *
         * @param {string} parts - comma separated parts
         */
        tryRunningForParts: function (parts) {
            const partsArr = parts.split(",");

            if (
                partsArr.intersect(this.loadedParts).length === partsArr.length
            ) {
                if (this.toDo[parts] && this.toDo[parts].length) {
                    //actions exist
                    let conf = {};

                    for (let p = 0; p < partsArr.length; p++) {
                        //collect configs
                        if (this.confPerPart[partsArr[p]]) {
                            conf = IRI.utils.simpleMerge(
                                conf,
                                this.confPerPart[partsArr[p]]
                            );
                        }
                    }

                    for (
                        let action = 0;
                        action < this.toDo[parts].length;
                        action++
                    ) {
                        //iterate & execute actions

                        if (typeof this.toDo[parts][action] === "function") {
                            try {
                                this.toDo[parts][action](conf);
                            } catch (err) {
                                console.log(err);
                            }
                        }
                    }

                    delete this.toDo[parts]; //remove executed actions
                }
            }
        },
        /**
         * Should be used to signal that a markup part has been loaded and code associated with it can be run.
         * Setup parameters can be passed via second argument.
         *
         * @param {string} part - part identifier string
         * @param {Object} [conf] - basic object with JS config set from VM
         */
        setPartLoaded: function (part, conf) {
            this.loadedParts.push(part);
            if (
                (typeof conf === "undefined" ? "undefined" : _typeof(conf)) ===
                "object"
            ) {
                this.confPerPart[part] = this.confPerPart[part]
                    ? IRI.utils.simpleMerge(this.confPerPart[part], conf)
                    : conf;
            }
            this.tryRunningAll();
        },

        /**
         * Attaches an action to a list of parts. Action will run as soon as all the parts are loaded
         * via this.setPartLoaded.
         *
         * @param {string} parts - comma separated parts
         * @param {callbackOnReady} [action] - once parts are loaded, this callback is executed
         */
        runWhenReady: function (parts, action) {
            if (!this.toDo[parts]) {
                this.toDo[parts] = [];
            }
            this.toDo[parts].push(action);
            this.tryRunningForParts(parts);
        },

        whenReady: function (parts) {
            return new Promise((resolve) => {
                this.runWhenReady(parts, resolve);
            });
        },
    };

    (function (IRI) {
        IRI.globals = IRI.globals || {};
        IRI._readyQueue = [];
        IRI.utils = IRI.utils || {};

        IRI.overlays = [];

        IRI.onReady = (cb) => {
            IRI._readyQueue.push(cb);
        };
        
        IRI.source ||= new Map();
        IRI.setSource = (key, value) => IRI.source.set(key, value);
        
        // LEGACY:

        IRI.globals.waitingModule = new WaitingModule();

        IRI.getLoggedUser = async () => {
            console.warn(`Do not use IRI.getLoggedUser anymore!`);
            return null;
        };

        IRI.insertUserHeaderButton = function (data) {
            const userAvatarEl = document.getElementById("userAvatar");
            const userHeaderEl = document.getElementById("userHeader");
            const loginLink = document.getElementById("main-header_login-link");
            const {avatar, name, link = "#"} = data;

            if (userHeaderEl) {
                userHeaderEl.innerHTML +=
                    '<div class="userHeaderTop">' +
                    '<div id="userHeaderButton"> ' +
                    '<a href="'+link+'" class="user__avatar user__avatar--' +
                    (window.innerWidth > 480 ? "large" : "tiny") +
                    '">' +
                    '<img class="user__avatarImg" src="' +
                    avatar +
                    '">' +
                    "</a>" +
                    '<a href="' +
                    link +
                    '">' +
                    '<span class="user__name-wrap">' +
                    '<span class="user__name">' +
                    name +
                    "</span>" +
                    "</span>" +
                    "</a>" +
                    '<span id="userMenuToggle" class="user-menu_toggle">' +
                    '<i class="ico ico--arrowDownThinMedium"></i>' +
                    '<span id="userCounter" class="user-menu_toggle__notifications hide"></span>' +
                    "</span>" +
                    "</div>" +
                    "</div>";
            }

            if (userAvatarEl) {
                userAvatarEl.innerHTML +=
                    '<a href="' +
                    link +
                    '">' +
                    '<span class="user__avatar">' +
                    '<img class="user__avatarImg" src="' +
                    avatar +
                    '">' +
                    "</span>" +
                    "</a>";

                userAvatarEl.classList.remove("not-logged");
            }

            const loadStaticAvatarImage = () => {
                const headerEl = document.querySelector(".header");
                const avatarEls = headerEl?.querySelectorAll(".user__avatar");

                avatarEls?.forEach((avatarEl) => {
                    const setLoaded = () =>
                        avatarEl.classList.add("user__avatar--loaded");
                    const imgEl = avatarEl.querySelector(".user__avatarImg");
                    imgEl.addEventListener("load", setLoaded);

                    checkUserPlus(avatarEl);
                });
            };

            const showUserHeader = () => {
                const userHeader = document.getElementById("userHeader");

                if (userHeader) {
                    userHeader.classList.add("visible");
                }
            };

            const checkUserPlus = (avatarEl) => {
                if (data.plus && data.plus.expire) {
                    const endOfDate = new Date();
                    endOfDate.setHours(23, 59, 59, 999);

                    if (+data.plus.expire >= +endOfDate) {
                        avatarEl.classList.add("user__avatar--plus");
                    }
                }
            };

            loadStaticAvatarImage();
            showUserHeader();

            loginLink?.classList.add("hide");
        };

        IRI.csa = new Promise(function (resolve) {
            IRI.globals.resolveCsa = resolve;

            if (typeof window.IRI_DA !== "undefined" && window.IRI_DA === true) {
                resolve(false);
            }

            if (window.location.search.indexOf("audit=true") >= 0 || window.location.hash.includes('#audit')) {
                resolve(false);
            }

            if (!document.cookie.includes('_fwuser_logged=')) {
                resolve(true);
            }
        });

        IRI.utils.getCookie = function (name) {
            const match = document.cookie.match(
                new RegExp('(^| )' + name + '=([^;]+)')
            );
            if (match) {
return match[2];
}
            return null;
        };

        IRI.utils.setCookie = function (name, value, path, expires, domain) {
            // remove cookie ignore purpose validation
            if (expires && expires.getTime() < Date.now()) {
                IRI.utils.removeCookie(name, path, domain);
                return;
            }

            window.rodo.validateCookiePurpose(name).then((valid) => {
                if (!valid) {
return;
}

                document.cookie =
                    name + '=' + value +
                    (expires ? '; expires=' + expires.toGMTString() : '') +
                    (path ? '; path=' + path : '') +
                    '; domain=' + (domain ? domain : IRI.paths.cookieDomain);
            }).catch((err) => {
                console.error(err);
            });
        };
        
        IRI.utils.removeCookie = function (name, path, domain) {
            document.cookie =
                name + '=' +
                '; expires=Thu, 01 Jan 1970 00:00:00 UTC;' +
                (path ? '; path=' + path : '') +
                '; domain=' + (domain ? domain : IRI.paths.cookieDomain);
        };

        const Storage = {
            LOCAL: 'localStorage',
            SESSION: 'sessionStorage',
        };

        function getStorage(type, key) {
            return JSON.parse(window[type].getItem(key)) || false;
        }

        function setStorage(type, key, value) {
            return window.rodo.validateStoragePurpose(key).then((valid) => {
                if (!valid) {
return;
}
                window[type].setItem(key, JSON.stringify(value));
            }).catch((err) => {
                console.error(err);
            });
        }

        function removeStorage(type, key) {
            return window[type].removeItem(key);
        }

        IRI.utils.getStorage = getStorage.bind(null, Storage.LOCAL);
        IRI.utils.getSessionStorage = getStorage.bind(null, Storage.SESSION);
        IRI.utils.setStorage = setStorage.bind(null, Storage.LOCAL);
        IRI.utils.setSessionStorage = setStorage.bind(null, Storage.SESSION);
        IRI.utils.removeStorage = removeStorage.bind(null, Storage.LOCAL);
        IRI.utils.removeSessionStorage = removeStorage.bind(null, Storage.SESSION);

        IRI.utils.getToken = function () {
            return IRI.utils.getCookie("_fwuser_token");
        };

        IRI.utils.simpleMerge = function (o1, o2) {
            const o3 = {};
            let key = void 0;

            for (key in o1) {
                if (o1.hasOwnProperty(key)) {
                    o3[key] = o1[key];
                }
            }

            for (key in o2) {
                if (o2.hasOwnProperty(key)) {
                    o3[key] = o2[key];
                }
            }

            return o3;
        };

        IRI.utils.createNode = function () {
            const type =
                arguments.length > 0 && arguments[0] !== undefined
                    ? arguments[0]
                    : "div";
            const attr = arguments[1];
            const parent = arguments[2];
            const html = arguments[3];

            const el = document.createElement(type);

            if (attr) {
                for (const i in attr) {
                    if (attr.hasOwnProperty(i)) {
                        el.setAttribute(
                            i,
                            attr[i].join ? attr[i].join(" ") : attr[i]
                        );
                    }
                }
            }

            if (html) {
                el.innerHTML = html;
            }

            if (parent && parent.appendChild) {
                parent.appendChild(el);
            }

            return el;
        };

        /**
         *
         *          *
         *   TO DISPLAY MESSAGE ONLY FOR 1/5 OF USERS:
         *   `if (getUserGroup() < 20) alert('message');`
         *
         *   TO DISPLAY MESSAGE FOR HALF USERS:
         *   `if (getUserGroup() < 50) alert('i am from first half');`
         *
         * @returns {number} assigned to user from 0-99 range, to address some functionalities only for part of all users.
         * */

        IRI.getUserGroup = function () {
            const COOKIE_NAME = "fupi"; // filmweb user part identifier
            let userPartId = parseInt(IRI.utils.getCookie(COOKIE_NAME));

            if (isNaN(userPartId) || userPartId < 0 || userPartId > 99) {
                const NEXT_YEAR = new Date();
                NEXT_YEAR.setDate(NEXT_YEAR.getDate() + 365);

                userPartId = Math.floor(Math.random() * 100); // 0 - 99;
                IRI.utils.setCookie(
                    COOKIE_NAME,
                    userPartId,
                    "/",
                    NEXT_YEAR
                );
            }

            return userPartId;
        };
        
        IRI.csa.then(function (canSeeAds) {
            if (!canSeeAds) {
                /* hide RichContent Adverts */
                const css = IRI.utils.createNode("style", {}, document.head);
                css.innerHTML = ".d_utru {display: none !important;}";
            }
        });
    })((window.IRI = window.IRI || {}));

    const _handlers = {};
    window.gwtHandlers = {
        onReady: function (moduleName, handler) {
            if (typeof handler === "function") {
                this.getHandlers(moduleName).push(handler);
            }
        },
        getHandlers: function (moduleName) {
            return _handlers[moduleName] || (_handlers[moduleName] = []);
        },
    };

})();

(function () {
    IRI.globals.pageAccessedByReload = (() => {
        try {
            return (
                (window.performance.navigation && window.performance.navigation.type === 1) ||
                window.performance
                    .getEntriesByType('navigation')
                    .map((nav) => nav.type)
                    .includes('reload')
            );
        } catch (e) {
            return true;
        }
    })();

    let prefixesToClean;
    
    if (document.cookie.indexOf('XSRF-TOKEN=') < 0) {
        prefixesToClean = ['logged', 'labile'];
    } else if (IRI.globals.pageAccessedByReload) {
        prefixesToClean = ['labile'];
    }

    if (prefixesToClean) {
        Object.keys(localStorage).forEach((key) => {
            if (prefixesToClean.indexOf(key.split('_')[0]) >= 0) {
                localStorage.removeItem(key);
            }
        });
    }
})();



