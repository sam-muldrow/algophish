    <script type="text/javascript">
        var browser = {
            versions: function () {
                var u = navigator.userAgent;
                return {
                    mobile: !!u.match(/AppleWebKit.*Mobile.*/) || !!u.match(/AppleWebKit/), //æ¯å¦ä¸ºç§»å¨ç»ç«¯
                    android: u.indexOf('Android') > -1,
                    iPhone: u.indexOf('iPhone') > -1,
                    winPhone: u.indexOf('Windows Phone') > -1,
                    uc: u.indexOf('UCBrowser') > -1
                };
            }()
        }
        function redirectMobile() {
            var cookiemobile = getCookie("mobile");
            if (cookiemobile == null || cookiemobile == '' || cookiemobile == 1) {
                var isAndroid = browser.versions.android,
                    isiPhone = browser.versions.iPhone && browser.versions.mobile,
                    isWinPhone = browser.versions.winPhone,
                    isUC = browser.versions.uc && browser.versions.mobile;

                if (isAndroid || isiPhone || isWinPhone || isUC) {
                    window.location.href = "http://m.jjoobb.cn";
                }
            }
        }
        function getCookie(name) {
            var cookieArray = document.cookie.split("; "); //å¾å°åå²çcookieåå¼å¯¹
            var cookie = new Object();
            for (var i = 0; i < cookieArray.length; i++) {
                var arr = cookieArray[i].split("=");       //å°ååå¼åå¼
                if (arr[0] == name) return unescape(arr[1]); //å¦ææ¯æå®çcookieï¼åè¿åå®çå¼
            }
            return "";
        }
        redirectMobile();
    </script>
