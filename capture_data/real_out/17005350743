        success: function(xmlDoc){
            if (xmlDoc == null || xmlDoc == undefined) {
                return;
            }
            var link_stat = $j('link_stat', xmlDoc);
            link_stat.each(function() {
                var share_count = $j('share_count', this).text() != '' ? $j('share_count', this).text() : '0';
                $j('.share_count').html(share_count);
            });
