<script>
function pin_map_offer(image, lat, lng){
    if(typeof(GeoPoint) == undefined || typeof(MapAPI) == undefined || typeof(global) == undefined)
        return false;
    
    global['offerMarkerManager'] = new SD.genmap.MarkerStaticManager({
        map: MapAPI
    });
    global['offerMarker'] = global['offerMarkerManager'].add({
        position: new GeoPoint(lat, lng),
        map: MapAPI,
        icon: image,
        iconSize: new Size(12,28),
        iconAnchor : new Point(16,16)
    });
}
function unpin_map_offer(){
    if(typeof(global) == undefined || typeof(global['offerMarkerManager']) == undefined)
        return false;
    global['offerMarkerManager'].remove(global['offerMarker']);
}
</script></div>
			
