 <script>
    $(function() {
        $('.ec-newsRole__product_news').each(function() {
            var listLength = $(this).find('.ec-newsRole__product_newsItem').length;
            if (listLength > 5) {
                $(this).find('.ec-newsRole__product_newsItem:gt(4)').each(function() {
                    $(this).hide();
                });
                //$(this).append('<a id="news_readmore" class="ec-inlineBtn--top">more</a>');
                $(this).append('<a id="news_readmore" class="news_mottomiru" style="text-align: center;display: block;margin: 5px 15%;"><svg width="24px" height="24px" viewBox="0 0 24 24"><path d="M7 10l5 5 5-5z"></path><path d="M0 0h24v24H0z" fill="none"></path></svg><div style="bottom: 7px;display: inline-flex;position: relative;"><div style="font-size: .875rem;letter-spacing: .25px;align-self: center;bottom: -1px;display: inline-block;margin-left: 4px;margin-right: 24px;position: relative;">もっと見る</div></div></a>');
                var dispNum = 5;
                $(this).find('#news_readmore').click(function() {
                    dispNum += 5;
                    $(this).parent().find('.ec-newsRole__product_newsItem:lt(' + dispNum + ')').show();
                    if (dispNum >= listLength) {
                        $(this).hide();
                    }
                })
            }
        });
    });

</script>



