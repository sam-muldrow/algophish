            <script>
    // タイマー
    $(document).ready(function () {
        $("#bn_change").each(function (index, target) {
            var startDate = '2022/1/21 18:00'; //1
            var endDate = "2022/4/3 11:00"; //2
            var nowDate = new Date();

            if (startDate) {
                startDate = new Date(startDate);
            } else {
                startDate = nowDate;
            }

            if (endDate) {
                endDate = new Date(endDate);
            }

            //イベント前
            if (startDate >= nowDate) {
                $(this).html(
                    '<a href="https://www.e-earphone.jp/product/search/list/?category_id=12&orderby=1&on_sale_only_display=true"><img src="https://www.e-earphone.jp/html/template/default/assets_time/01_tm2020/banner/202110/used_top.webp"></a>'
                );
            }

            //イベント開始
            if (startDate <= nowDate && (!endDate || nowDate <= endDate)) {
                $(this).html(
                    '<a href="https://www.e-earphone.jp/ex/contents/event/16928/youtube_channel_cp/"><img src="https://www.e-earphone.jp/html/template/default/assets_time/01_tm2020/banner/202201/YouTube_cp_600_687.webp"></a>'
                );
            }

            //イベント終了
            if ((!endDate || nowDate >= endDate)) {
                $(this).html(
                    '<a href="https://www.e-earphone.jp/product/search/list/?category_id=12&orderby=1&on_sale_only_display=true"><img src="https://www.e-earphone.jp/html/template/default/assets_time/01_tm2020/banner/202110/used_top.webp"></a>'
                );
            }

        });
    });

    // バナーランダム表示
    /*
    $(function() {
        const directory = "https://www.e-earphone.jp/html/template/default/assets_time/01_tm2020/banner/202112/review_cp/";
    */

    /* 画像ファイル名 */
    /*
