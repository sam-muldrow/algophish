            <script>
    $(function(){
        var reviewStar = $('.review-list__contents__info .common-rating');
        $('.review-list__contents__info .common-rating-number-sub.displayNone').each(function(){
            if ($(this).text().indexOf('無評価') !== -1) {
                $(this).prev(reviewStar).find('.common-rating__item').css('width', '0');
            } else if ($(this).text().indexOf('0.5') !== -1) {
                $(this).prev(reviewStar).find('.common-rating__item').css('width', '10%');
            } else if ($(this).text().indexOf('1.0') !== -1) {
                $(this).prev(reviewStar).find('.common-rating__item').css('width', '20%');
            } else if ($(this).text().indexOf('1.5') !== -1) {
                $(this).prev(reviewStar).find('.common-rating__item').css('width', '30%');
            } else if ($(this).text().indexOf('2.0') !== -1) {
                $(this).prev(reviewStar).find('.common-rating__item').css('width', '40%');
            } else if ($(this).text().indexOf('2.5') !== -1) {
                $(this).prev(reviewStar).find('.common-rating__item').css('width', '50%');
            } else if ($(this).text().indexOf('3.0') !== -1) {
                $(this).prev(reviewStar).find('.common-rating__item').css('width', '60%');
            } else if ($(this).text().indexOf('3.5') !== -1) {
                $(this).prev(reviewStar).find('.common-rating__item').css('width', '70%');
            } else if ($(this).text().indexOf('4.0') !== -1) {
                $(this).prev(reviewStar).find('.common-rating__item').css('width', '80%');
            } else if ($(this).text().indexOf('4.5') !== -1) {
                $(this).prev(reviewStar).find('.common-rating__item').css('width', '90%');
            } else if ($(this).text().indexOf('5.0') !== -1) {
                $(this).prev(reviewStar).find('.common-rating__item').css('width', '100%');
            }
        });
    });
</script>

