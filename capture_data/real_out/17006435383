<script>
    $(function() {
        // 記事の数に応じてリスト表示幅自動調整
        var wpCount = $('.wordpress_link_wrapper li').length;
        var wpWidth = 310 * wpCount - 20;

        if (window.matchMedia('(min-width: 768px)').matches) {
            $('.wordpress_link_wrapper ul').css('width', `${wpWidth}px`)
        } else if (window.matchMedia('(max-width: 768px)').matches) {
            wpWidth = 270 * wpCount - 20;
            $('.wordpress_link_wrapper ul').css('width', `${wpWidth}px`)
        }

    });

</script>

