            "3ee", "Astell_and_Kern", "audio-technica", "BANG_and_OLUFSEN", "Bowers_and_Wilkins", "Campfire_Audio", "CHORD_and_MAJOR", "DENON", "DEVIALET", "EPOS_JAPAN", "GRADO", "House_of_Marley", "Jabra", "JBL", "Meze_Audio", "MPOW", "NOBUNAGA_Labs", "NUARL", "SONY", "Unique_Melody", "Victor_jvc", "MOONDROP", "Razer", "final"
        ];

        var l = array.length;
