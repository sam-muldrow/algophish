<script>
    $(function () {
        // ブラックフライデー週末特価
        const bstartDate = new Date('2023/11/22 20:03:00');
        const bendDate = new Date('2023/11/24 20:02:59');
        // 中古の日
        const bchukostartDate = new Date('2023/11/14 20:03:00');
        const bchukoendDate = new Date('2023/11/16 10:59:59');
        // シークレットセール
        const bgwstartDate = new Date('2023/11/01 11:00:00');
        const bgwendDate = new Date('2023/11/02 10:59:59');
        // カウントダウンタイマー
        const bkinrostartDate = new Date('2023/10/31 10:00:00');
        const bkinroendDate = new Date('2023/10/31 20:02:59');
        // 中古の日＋文化の日特価
        const bgwkinrostartDate = new Date('2023/11/04 20:03:00');
        const bgwkinroendDate = new Date('2023/11/05 10:59:59');
        // セール(予備)
        const bbftwostartDate = new Date('2023/11/21 20:03:00');
        const bbftwoendDate = new Date('2023/11/22 20:02:59');
        // 今
        const bnowDate = new Date();

        if (bstartDate <= bnowDate && (!bendDate || bnowDate <= bendDate)) {
            $("#rtoaster_weekendsale_banner").html('<div class="sale_bnn"><a href="https://www.e-earphone.jp/product/search/list/?category_id=12&on_sale_only_display=true&orderby=1&sale=766b7653"><img src="https://www.e-earphone.jp/html/template/default/assets_time/01_tm2020/banner/202311/sony_black_friday_02_used_980_100.webp" alt="【第2弾】中古ブラックフライデーセール！"></a></div><div class="sale_bn"><a href="https://www.e-earphone.jp/product/search/list/?category_id=12&on_sale_only_display=true&orderby=1&sale=54c67588"><img src="https://www.e-earphone.jp/html/template/default/assets_time/01_tm2020/banner/202311/sony_black_friday_01_old_980_100.webp" alt="【第1弾 中古ブラックフライデーセール】中古SONY WF-1000XM4均一セール！"></a><a href="https://www.e-earphone.jp/product/search/list/?category_id=12&on_sale_only_display=true&orderby=1&sale=725957b9"><img src="https://www.e-earphone.jp/html/template/default/assets_time/01_tm2020/banner/202311/wintersale_airpodspro_20231106_980_100.webp" alt="【中古】初代AirPodsProウインターセール！"></a></div>');
