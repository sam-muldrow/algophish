<!-- ▼ hgman個別set_timerタイマー (timer-start-date / timer-end-date) ▼ -->
<script>
    $(function () {
        $(".set_timer").each(function (index, target) {
            var timerStartDate = $(this).attr("timer-start-date");
            var timerEndDate = $(this).attr("timer-end-date");
            var timerNowDate = new Date();

            if (timerStartDate) {
                timerStartDate = new Date(timerStartDate);
            } else {
                timerStartDate = timerNowDate;
            }
            if (timerEndDate) {
                timerEndDate = new Date(timerEndDate);
            }

            if (timerStartDate <= timerNowDate && (!timerEndDate || timerNowDate <= timerEndDate)) {
                $(this).show();
            } else {
                $(this).hide();
            }
        });
    });
</script>
<!-- ▲ hgman個別set_timerタイマー (timer-start-date / timer-end-date) ▲ -->




