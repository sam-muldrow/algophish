<script>
    $('#search_type').on('change', function () {
        var searchTypeSelect = document.getElementById("search_type");
        var searchTypeSelectedText = searchTypeSelect.options[searchTypeSelect.selectedIndex].text;
        window.dataLayer = window.dataLayer || [];
        dataLayer.push({
            'event': 'searchTypeChange',
            'searchTypeChangedText': searchTypeSelectedText
        });
    });

    $('#search_type-sp').on('change', function () {
        var searchTypeSelect = document.getElementById("search_type-sp");
        var searchTypeSelectedText = searchTypeSelect.options[searchTypeSelect.selectedIndex].text;
        window.dataLayer = window.dataLayer || [];
        dataLayer.push({
            'event': 'searchTypeChange',
            'searchTypeChangedText': searchTypeSelectedText
        });
    });

    $('#search_type-sp2').on('change', function () {
        var searchTypeSelect = document.getElementById("search_type-sp2");
        var searchTypeSelectedText = searchTypeSelect.options[searchTypeSelect.selectedIndex].text;
        window.dataLayer = window.dataLayer || [];
        dataLayer.push({
            'event': 'searchTypeChange',
            'searchTypeChangedText': searchTypeSelectedText
        });
    });
</script>
