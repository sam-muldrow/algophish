		<script src="https://www.nextag.com/odf/js/odf.js"></script><script>Odf.set('website_base_url', 'https://www.nextag.com/');$(document).ready(function() {Odf.init();});</script>     
		
		<script>
		
			$( document ).ready(function() {
				
				fitty('#logo_text', {
					maxSize: 50
				});
				
				$('#toggle_make_offer').click( function(e) {
								
					e.preventDefault();
					$('html, body').animate({
						scrollTop: $('#make_offer').offset().top
					}, 800);
					
					return true;
					
				});
				
				
				
				$( 'input[type="submit"]' ).click(function() {
					
					if ($('form#contact_form').checkValidity()) {
					
						$('div#preloader').show();
						$('div#preloader').fadeTo( 0, 0.8);
						$('div#preloader #status').show();
					
					}
					
				});
				
				$( '#escrow_pay, #bin_dan' ).click(function() {
					
					$('div#preloader').show();
					$('div#preloader').fadeTo( 0, 0.8);
					$('div#preloader #status').show();
					
				});
				
				$( document ).ajaxComplete(function() {
					
					$('div#preloader').hide();
					$('div#preloader #status').hide();
					
				});
			
			});
		</script> 
		
