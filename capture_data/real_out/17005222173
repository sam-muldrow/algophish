    function init_Promo_Pec(lang) {
        var imgURL, title, subtitle, promo, fromPrice, price, vat, renew, useCode, cta_link, text_cta, close_button, html, css;
        switch (lang) {
            case "it":
                imgURL = 'https://mediacdn.aruba.it/MediaCDNRepository/files/f5/f5c234d6-2e72-42fd-a92f-38d46feb9619.jpg';
                title = 'Preparati al futuro della Fatturazione Elettronica';
                subtitle = 'Dal 1° gennaio 2024 obbligatoria per tutti i forfettari';
                promo = '';
                fromPrice = '';
                price = '1&euro;';
                vat = '<span class="prezzo-color">+ IVA</span><span> / 3 MESI</span>';
                renew = 'al rinnovo 25,00 &euro; + IVA / anno';
                useCode = '';
                text_cta = 'SCOPRI<span class="sr-only">SCOPRI</span>';
                cta_link = 'https://www.pec.it/fatturazione-elettronica-regime-forfettario.aspx?utm_source=pec-it&utm_medium=sticky-bar&utm_campaign=fe-forfettari-0923';
                close_button = 'Chiudi la barra';
            break;
        }
        html = '<div class="promoPEC"><div class="mainContainer-mobile-promoPEC display-flex justify-content-center align-items-center text-align-left"><div class="container-mobile-promoPEC"><div class="container-description-promoPEC display-flex flex-direction-column"><div class="lg-text bold">' + title + '</div><div class="md-text hidden-xs">' + subtitle + '</div></div><div class="container-prezzo-promoPEC display-flex flex-direction-column"><div class="promo xs-text uppercase hidden-xs">' + promo + '</div><div class="fromPrice xs-text bold mr-5">' + fromPrice + '</div><div class="container-prezzo display-flex flex-direction-row"><div class="prezzo prezzo-color bold">' + price + '</div><div class="display-flex flex-direction-column justify-content-center ml-2"><div class="sm-text">' + vat + '</div><div class="xs-text">' + renew + '</div></div></div></div></div><div class="container-button-promoPEC text-center"><div class="md-text bold hidden-xs">' + useCode + '</div><a class="promoPEC-btn action--aruba-sticky-bar-cta" href="' + cta_link + '">' + text_cta + '</a></div></div></div>';
        css = '.visible-xl{display:none }.display-flex{display:flex }.flex-direction-column{flex-direction:column }.flex-direction-row{flex-direction:row }.align-items-center{align-items:center }.justify-content-center{justify-content:center }.uppercase{text-transform:uppercase }.mb-0{margin-bottom:0 }.ml-5{margin-left:5px }.mr-5{margin-right:5px }.promoPEC{box-shadow:0 0 5px 3px rgba(0,0,0,.4);font-family:"Lato",sans-serif;color:#222;background-color:rgb(219,244,255);padding-top:7px;padding-bottom:7px;background: center/cover no-repeat url(' + imgURL + ') rgb(219,244,255) }.container-logo-promoPEC .title{margin-left:7px;font-size:21px;font-weight:bold }.lg-text{font-size:20px }.md-text{font-size:15px }.sm-text{font-size:23px }.xs-text{font-size:11px }.bold{font-weight:bold }.text-align-left{text-align:left }.uppercase{text-transform:uppercase }.container-mobile-promoPEC{display:flex;flex-direction:row;justify-content:center }.container-description-promoPEC, .container-prezzo-promoPEC, .container-button-promoPEC{padding:0 30px }.prezzo{font-size:49px;line-height:48px }.prezzo-color{color:#f5541a }s.strikeout{-moz-text-decoration-line:none;-webkit-text-decoration-line:none;text-decoration-line:none }.strikeout{position:relative }.strikeout::after{content:"";position:absolute;left:0;top:0;width:100%;height:100%;background:linear-gradient(to bottom right,transparent 46%,#e35205 48%,#e35205 50%,#e35205 52%,transparent 54%);transform:skewY(10deg);overflow:hidden }.badge{display:inline-block;padding:.25em .4em;font-size:100%;font-weight:normal;line-height:1;text-align:center;white-space:nowrap;vertical-align:baseline;border-radius:.25rem;transition:color .15s ease-in-out, background-color .15s ease-in-out, border-color .15s ease-in-out, box-shadow .15s ease-in-out }.badge-color{color:#fff;background-color:#28a745;background-image:linear-gradient(123deg,#109006 0%,#469d04 52%,#77a903 100%) }.promoPEC-btn, .promoPEC-btn:hover, .promoPEC-btn:focus{text-decoration:none }.prom