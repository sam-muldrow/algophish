  $(document).ready(function () {
    var imagenes_destacadas = { }
    var numero_elementos_slide = 5;
    var temporizador;
    var semaforo = false;
    var actual = 0;
    imagenes_destacadas["0"] = {
      "id": 'contenedora_imagen_principal_0',
      "duracion": 20000
    }
    imagenes_destacadas["1"] = {
      "id": 'contenedora_imagen_principal_1',
      "duracion": 20000
    }
    imagenes_destacadas["2"] = {
      "id": 'contenedora_imagen_principal_2',
      "duracion": 20000
    }
    imagenes_destacadas["3"] = {
      "id": 'contenedora_imagen_principal_3',
      "duracion": 20000
    }
    imagenes_destacadas["4"] = {
      "id": 'contenedora_imagen_principal_4',
      "duracion": 20000
    }
    function rotacion (posicion, tiempo, numero_elementos)
    {
      if (!semaforo)
      {
        semaforo = true;
        temporizador = setTimeout(function(){
          $('.contenedora_imagen_principal').hide();
          siguiente = (posicion == numero_elementos-1) ? 0 : posicion + 1;
          $('#' + imagenes_destacadas[String(siguiente)]["id"]).fadeIn( "slow", function() { });
          actual = siguiente;
          activarElementoRotacionVertical(siguiente);
          $slide_destacados.trigger('to.owl.carousel', [siguiente-1, 300, true]);
          clearTimeout(temporizador);
          rotacion(siguiente, imagenes_destacadas[String(siguiente)]["duracion"], numero_elementos);
        }, tiempo);
        semaforo = false;
      }
    }
    function activarElementoRotacionVertical(posicion)
    {
      //Paramos los otros videos
      $('.contenedora_imagen_principal .caja_video iframe').each(function(){
        $(this).stopVideo();
      })
      $('.caja_noticia.destacado .caja_foto .caja_video iframe').each(function(){
        $(this).stopVideo();
      });
      $('.elemento_destacado_vertical .fila_oculta').hide();
      $('.fila_oculta_' + posicion).fadeIn( "slow", function() { });
    }
    activarElementoRotacionVertical(actual);
    rotacion(actual, imagenes_destacadas["0"]["duracion"], numero_elementos_slide);
    $slide_destacados = $('.contenedora_portada_destacados_slide_horizontal');
    $slide_destacados.owlCarousel({
      loop: true,
      margin: 0,
      responsiveClass: true,
      nav: true,
      autoplay: false,
      animateOut: 'fadeOut',
      navText: ['<i class="fa fa-caret-left" aria-hidden="true"></i>', '<i class="fa fa-caret-right" aria-hidden="true"></i>'],
      responsive: {
        0: {
          items: 3
        }
      }
    }).on('click', '.owl-item', function () {
      posicion = (($(this).index() - 3) % numero_elementos_slide);
      $('.contenedora_imagen_principal').hide();
      $('#' + imagenes_destacadas[String(posicion)]["id"]).fadeIn( "slow", function() { });
      clearTimeout(temporizador);
    });
    $('.contenedora_imagen_principal, .caja_foto').hover(
      function()
      {
        if (!$(this).find('.imagen').is(":hidden"))
        {
          $(this).find('.fotografo').show();
        }
        else
        {
          $(this).find('.fotografo').hide();
        }
      },
      function(){
        $(this).find('.fotografo').hide();
      }
    );
    $('.contenedora_imagen_principal .imagen').click(
      function()
      {
        clearTimeout(temporizador);
      }
    );
    $('.elemento_destacado_vertical').click(
      function()
      {
        clearTimeout(temporizador);
        posicion = $(this).attr("data-id");
        activarElementoRotacionVertical(posicion);
        $('.contenedora_imagen_principal').hide();
        var imagen = $('#' + imagenes_destacadas[String(posicion)]["id"]);
        if ($(imagen).data('t2v-video-mostrado') == '0')
        {
          $(imagen).find('.imagen').show();
          $(imagen).find('.caja_info').show();
          $(imagen).find('.caja_boton_video').show();
          $(imagen).find('.fotografo2').addClass('fotografo').removeClass('fotografo2');
          $(imagen).find('.caja_video').hide();
        }
        var imagen = $(imagen).fadeIn( "slow", function() { });
      }
    );
    $('.flecha_mostrar_sinopsis').click(
      function()
      {
        clearTimeout(temporizador);
        $(this).hide();
        $(this).parent().find('.flecha_ocultar_sinopsis').show();
        $(this).parent().find('.resumen').show();
      }
    );
    $('.flecha_ocultar_sinopsis').click(
      function()
      {
        clearTimeout(temporizador);
        $(this).hide();
        $(this).parent().find('.flecha_mostrar_sinopsis').show();
        $(this).parent().find('.resumen').hide();
      }
    );
    $('.contenedora_imagen_principal.tiene-video').click(function(){
      clearTimeout(temporizador);
      $(this).find('.imagen').hide();
      $(this).find('.caja_info').hide();
      $(this).find('.fotografo').toggleClass('fotografo2');
      $(this).find('.fotografo2').toggleClass('fotografo');
      $(this).find('.caja_video').show();
      $(this).find('.caja_video iframe').playVideo();
      $(this).data('t2v-video-mostrado', '1');
    });
    $('.contenedora_imagen_principal .caja_boton_video').click(function(){
      clearTimeout(temporizador);
      $(this).parent().find('.imagen').hide();
      $(this).parent().find('.caja_info').hide();
      $(this).parent().find('.fotografo').toggleClass('fotografo2');
      $(this).parent().find('.fotografo2').toggleClass('fotografo');
      $(this).hide();
      $(this).parent().find('.caja_video').show();
      $(this).parent().find('.caja_video iframe').playVideo();
      $(this).parent().data('t2v-video-mostrado', '1');
    });
    $('.caja_noticia.destacado .caja_foto.tiene-video').click(function(){
      clearTimeout(temporizador);
      $(this).find('.enlace_noticia_destacada').hide();
      $(this).find('.caja_texto_destacado').hide();
      $(this).find('.fotografo').toggleClass('fotografo2');
      $(this).find('.fotografo2').toggleClass('fotografo');
      $(this).find('.caja_video').show();
      $(this).find('.caja_video iframe').playVideo();
      $(this).data('t2v-video-mostrado', '1');
    });
    $('.caja_noticia.destacado .caja_foto .caja_boton_video').click(function(){
      clearTimeout(temporizador);
      $(this).parent().find('.enlace_noticia_destacada').hide();
      $(this).parent().find('.caja_texto_destacado').hide();
      $(this).parent().find('.fotografo').toggleClass('fotografo2');
      $(this).parent().find('.fotografo2').toggleClass('fotografo');
      $(this).hide();
      $(this).parent().find('.caja_video').show();
      $(this).parent().find('.caja_video iframe').playVideo();
      $(this).parent().data('t2v-video-mostrado', '1');
    });
    $('.caja_noticia.destacado .caja_foto.tiene-noticia, .contenedora_imagen_principal.tiene-noticia').click(function(){
      clearTimeout(temporizador);
      var url = $(this).data('t2v-url');
      if (typeof(url) !== 'undefined' && url != '')
      {
        location.href = url;
      }
    });
    $('.enlaces a, .enlace_video').on('click', function(event) {
      event.stopPropagation();
    });
  });
  $(document).ready(function () {
    var patrocinadores_destacados = { }
    var actual_patrocinadores = 0;
    var numero_elementos_patrocinadores = 4;
    var temporizador_patrocinadores;
    patrocinadores_destacados["0"] = {
      "id": 'contenedora_patrocinador_0',
      "duracion": 5000 /* Tiempo en milisegundos */
    }
    patrocinadores_destacados["1"] = {
      "id": 'contenedora_patrocinador_1',
      "duracion": 5000 /* Tiempo en milisegundos */
    }
    patrocinadores_destacados["2"] = {
      "id": 'contenedora_patrocinador_2',
      "duracion": 5000 /* Tiempo en milisegundos */
    }
    patrocinadores_destacados["3"] = {
      "id": 'contenedora_patrocinador_3',
      "duracion": 5000 /* Tiempo en milisegundos */
    }
    function rotacionPatrocinadores (posicion, tiempo, numero_elementos_patrocinadores)
    {
      temporizador_patrocinadores = setTimeout(function(){
        $('.contenedora_patrocinador').hide();
        siguiente = (posicion == numero_elementos_patrocinadores-1) ? 0 : posicion + 1;
        actual_patrocinadores = siguiente;
        $('.' + patrocinadores_destacados[String(siguiente)]["id"]).fadeIn( "slow", function() { });
        clearTimeout(temporizador_patrocinadores);
        rotacionPatrocinadores(siguiente, patrocinadores_destacados[String(siguiente)]["duracion"], numero_elementos_patrocinadores);
      }, tiempo);
    }
    if (numero_elementos_patrocinadores > 0)
    {
      rotacionPatrocinadores(actual_patrocinadores, patrocinadores_destacados["0"]["duracion"], numero_elementos_patrocinadores);
    }
});
$('.contenedora_portada_noticias > .fila > .caja_noticia > .caja_foto > .caja_boton_video').click(function(){
  $(this).parent().find('a').hide();
  $(this).parent().find('.caja_info').hide();
  $(this).parent().find('.fotografo').removeClass('fotografo');
  $(this).hide();
  $(this).parent().find('.caja_video').show();
  $(this).parent().find('.caja_video iframe').playVideo();
});
$('.contenedora_portada_noticias > .fila > .caja_noticia a.reproduce_video').click(function(){
  $(this).closest('.caja_noticia').find('.caja_foto a').hide();
  $(this).closest('.caja_noticia').find('.caja_foto .caja_info').hide();
  $(this).closest('.caja_noticia').find('.caja_foto .fotografo').removeClass('fotografo');
  //$(this).hide();
  $(this).closest('.caja_noticia').find('.caja_video').show();
  $(this).closest('.caja_noticia').find('.caja_video iframe').playVideo();
});
$(document).ready(function () {
  $('.contenedora_portada_noticias > .fila .caja_noticia > iframe').closest('.caja_noticia').addClass('kia_en_zona');
});
