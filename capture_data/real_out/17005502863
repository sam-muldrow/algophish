<script async src="https://www.googletagmanager.com/gtag/js?id=G-ER3NBPE9CN"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());
  gtag('config', 'G-ER3NBPE9CN');
</script>


	<script>			
	      function compatibleMobileDevice(){
	        var result = false;
	        if (navigator && navigator.userAgent) {
	          var ios      = navigator.userAgent.match(/iPhone/i) ||
                           navigator.userAgent.match(/iPod/i);
            var android  =  navigator.userAgent.match(/Android\s((4\.[3-9])|([5-9]))/i);	// match Android 4.3+
            if (ios)
              result = 'iOS';
            else if (android)
              result = 'Android';
          }
          return result;
        }
        
        function iPhoneAppMessage() {
          // Only show if iPhone and if not dismessed
          var cmd = compatibleMobileDevice();
          if (cmd && !getCookie('iam')) {
            var msg = 'Worldtime Buddy has a FREE [OS] app, would you like to use it instead?'.replace('[OS]', cmd);
            var url = cmd.toLowerCase() == 'ios'
                ? 'https://itunes.apple.com/app/apple-store/id719765848?pt=5776801&ct=pop&mt=8'
                : 'https://play.google.com/store/apps/details?id=com.helloka.worldtimebuddy';
            var result = confirm(msg);
            if (result) {
              _gaq.push(['_trackEvent','Misc', cmd + ' App Msg', 'Followed']);
              window.location = url;
              setCookie('iam', 1, 30); // 30 days              
            } else {
              // Set cookie to mark that iphone app message has been dismissed for 3 days
              setCookie('iam', 1, 3);
              _gaq.push(['_trackEvent','Misc', cmd + ' App Msg', 'Dismissed']);                                                                       
            } 
            
          }
        };
        
        $(window).load(function() {
          iPhoneAppMessage();
        })
        
				function openFeedbackPopup() {
					
					if (jmpopups.openPopupLayer) {
						jmpopups.openPopupLayer({
				        	name: "feedbackForm",
				        	width: 700,
				        	height: 500,
				        	target: 'feedback-form', 
				        	//url: "/feedback"
					        success: function() {
				              	var feedbackForm = '<div onmousedown="jmpopups.closePopupLayer()" style="cursor: pointer; position: absolute; width: 15px; height: 15px; font-size: 16px; font-family: Verdana; color: #444; font-weight: bold; top: -1px; right: 2px">x</div><iframe style="width: 100%; height: 100%" src="/feedback" frameborder="0" scrolling="no"/>';
				              	$('#popupLayer_feedbackForm')
				              		.css({border: '5px solid #444444', padding: '5px', background: '#ffffff', borderRadius: '5px'})
				              		.html(feedbackForm);
					        }
						});
					} else {
						
						setTimeout(
							function() {
								openFeedbackPopup()
							}, 50);
					}
				}	
				
					
				$('#feedback-bttn, #feedback-link')
					.bind('mousedown.feedback', openFeedbackPopup);

				
        						
	</script>
