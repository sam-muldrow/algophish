<script type="text/javascript">
  let microAdCount = 0;
  let microAdTimer = function(capId) {
    setTimeout(function() {
      if (typeof Treasure === "function") {
        var td = new Treasure({
          host: 'in.treasuredata.com',
          writeKey: '7474/16ad482a465a2ca53a2cbdc35bcb01abfa98e8c5',
          database: 'recruit_lifestyle_dmp_db',
          startInSignedMode: true
        });
        td.set('$global', 'td_global_id', 'td_global_id');
        td.set('access_log', {member_id: capId});
        td.trackPageview('access_log');
      } else {
        if (microAdCount > 10) { return; }
        microAdTimer(capId);
        microAdCount++;
      }
    }, 3000);
  };
  window.addEventListener('load', function() {
    if (/^(www|store|purchase)/.test(location.host) && PM.user.helloInfoJson) {
      let capId = $.cookie('s_cap_uz') || "";
      if (capId) { microAdTimer(capId); }
    }
  },false);
</script>

