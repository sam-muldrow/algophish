<script>
$(window).scroll(function() {
    if ($(this).scrollTop()>900){
        $("#gotop").removeClass("hide");
    }else{
        $("#gotop").addClass("hide");
    };
});
 $(window).scroll(function() {
    if ($(this).scrollTop()>300){
        $(".body_home").addClass("showdiv");
    }else{
        $(".body_home").removeClass("showdiv");
    };
});

$('#gotop').click(function () {
    $('html,body').animate({
        scrollTop : '0px'
    }, 800);
}); 
</script>

