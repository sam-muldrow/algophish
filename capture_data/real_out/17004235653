<script type='text/javascript'>
  function isInAppBrowser() {
    if(isSpJs){
      var ua = navigator.userAgent.toLowerCase();
      var apps = ["appbankapppuzdradb", "appbankmonst", "pazudoraya", "appbanklineage2","appbankfgoapp","appsule"];
      for (var i = 0; i < apps.length; i++) {
        if (ua.indexOf(apps[i]) > 0) {
          return true;
        }
      }
    }
    return false;
  }
  function insertCSS($url){
    (function(w,d,l){
      var s=d.getElementsByTagName(l),f=s[s.length-1].nextSibling,j=d.createElement(l);
      j.rel="stylesheet";
      j.type="text/css";
      j.href=$url;
      f.parentNode.insertBefore(j,f);
    })(window,document,'link');
  }
  (function (){
    if(isSpJs){
      var ua = navigator.userAgent.toLowerCase();
      if(ua.match(ua.match(' appbankfgoapp/') || ua.match('appsule'))){
        insertCSS('/wp-content/themes/appbank4/sp/for_app.css');
      }else if(ua.match("android") && ua.match(" appbankapppuzdradb/")){
        insertCSS('/wp-content/themes/appbank3/sp/for_app_android.css');
      }
    }
  })();
</script>
