<script>
  if(!isSpJs){
    if(adslotsPC.length>0){
      // 外部ファイル(url)の読み込みを待ってから実行させる
      function loadScript(url) {
        return new Promise(function(resolve, reject) {
          var scripttag = document.createElement('script');
          scripttag.type = 'text/javascript';
          scripttag.src = url;
          scripttag.onload = function() {
            resolve();
          };
          scripttag.onerror = function() {
            reject();
          };
          document.getElementById('flux_appbank_TM').appendChild(scripttag);
        });
      }

      loadScript('https://flux-cdn.com/client/appbank/flux_appbank_TM.min.js')
        .then(function() {
          
          var pbjs = window.pbjs ||{};
          pbjs.que = pbjs.que ||[];

          const scripttag = document.createElement('script');
          scripttag.src = "//ads.pubmatic.com/AdServer/js/pwt/160165/3233/pwt.js";
          scripttag.async = true;
          document.getElementById('pwtpc').appendChild(scripttag);

          var amazonBidder = function () {
                ! function (a9, a, p, s, t, A, g) {
                  if (a[a9]) return;
                  function q(c, r) {
                      a[a9]._Q.push([c, r])
                  }
                  a[a9] = {
                    init: function () {
                        q("i", arguments)
                    },
                    fetchBids: function () {
                        q("f", arguments)
                    },
                    setDisplayBids: function () {},
                    targetingKeys: function () {
                        return []
                  },
                    _Q: []
                  };
                  A = p.createElement(s);
                  A.async = !0;
                  A.src = t;
                  g = p.getElementsByTagName(s)[0];
                  g.parentNode.insertBefore(A, g)
                }("apstag", window, document, "script", "//c.amazon-adsystem.com/aax2/apstag.js");
                apstag.init({
                    pubID: '3952',
                    adServer: 'googletag',
                    bidTimeout: 1e3
                });
                apstag.fetchBids({
                slots:adslotsPC
              }, function (bids) {
                  googletag.cmd.push(function () {
                      readyBids.amazon = true;
                      launchAdServer();
                  });
              });
          }
          var headerBiddingCall = function () {
              prebidBidder();
              amazonBidder();
          }
          headerBiddingCall();
          setTimeout(function () {
              requestAdServer();
          }, failSafeTimeout);
          
        })
        .catch(function() {
          console.log('flux_appbank_TMの読み込みに失敗しました');
        });
    }
  }
</script>

