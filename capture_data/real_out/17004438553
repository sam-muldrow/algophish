    <script>

        function savecookie() {

            if (getCookie('litebox') == '') {

                setCookie('litebox', 'save', 0.5);

                return true;

            }

            return false;

        }

        $(document).ready(function () {

            if (savecookie()) {

                setTimeout(function () {

                    $('#popupModal').modal('show');

                    //$('.litebox').fadeIn();

                }, 1000);

            }

        });

    </script>










