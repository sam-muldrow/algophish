<script>
  window._GDPR2 = {
    ESSENTIAL: 'essential',
    ANALYTICS: 'analytics',
    MARKETING: 'marketing',

    essential: [],
    analytics: [],
    marketing: [],

    consent: "",

    registerScript(scriptFunc, level, callback) {
      if (window.CE_GDPR_V2 && window.CE_GDPR_V2.registerScript) {
        window.CE_GDPR_V2.registerScript(scriptFunc, level, callback);
      } else {
        this[level].push({
          script: scriptFunc,
          callback
        });
      }
    },

    updateConsent(consent) {
      this.consent = consent;

      if (window.CE_GDPR_V2) {
        window.CE_GDPR_V2.updateConsent(consent);
      }
    }
  }
</script>

