<script src="https://www.gfk.com/hs-fs/hub/2405078/hub_generated/template_assets/75158491548/1698919706887/GFK_Redesign_2022/js/gfk.min.js" nonce="0fJiboX9TREBwk2fMsr80w=="></script>
<script src="https://www.gfk.com/hs-fs/hub/2405078/hub_generated/template_assets/76314271321/1655220899255/HeaderFooter__Redesign-2022/Header/macros/search-input/search-input.min.js" nonce="0fJiboX9TREBwk2fMsr80w=="></script>

<script type="text/javascript" nonce="0fJiboX9TREBwk2fMsr80w==">
    const slider = document.getElementsByClassName('MO-hero_slider__slider')[0];
    const progressbars = document.querySelectorAll('.MO-hero_slider__progress__bar');
    const slidercount = document.querySelector('.MO-hero_slider__slideIndex');
    const duration = 5000;
    const animationActive = false;
    UIkit.util.ready(function () {
        UIkit.slider(slider, {
            autoplay: false,
            autoplayInterval: duration,
            pauseOnHover: false,
            draggable: true
        });
        
        // init all progressbars with slider duration
        progressbars.forEach(bar => bar.style = 'animation-duration: '+duration+'ms;');
        
        const uiSlider = UIkit.slider(slider);
        if(animationActive){
            UIkit.util.on(slider, 'beforeitemshow', ev => {
                    ev.target.classList.add('animate--init');
                    ev.target.classList.add('animate--start');
            });
        }
        UIkit.util.on(slider, 'itemshown', ev => {
            slider.classList.add('progress--start');
            uiSlider.startAutoplay();
            if (slidercount) {
                slidercount.innerHTML = '' + (ev.detail[0].index + 1);
            }
        });
        UIkit.util.on(slider, 'itemhidden', ev => {
            ev.target.classList.remove('animate--init');
            ev.target.classList.remove('animate--start');
            slider.classList.remove('progress--start');
        });
        
        slider.addEventListener('mouseenter', ev =>  {
            uiSlider.stopAutoplay();
            progressbars.forEach(bar => bar.style = 'animation: none')
        });
        slider.addEventListener('mouseleave', ev =>  {
            uiSlider.startAutoplay();
            progressbars.forEach(bar => bar.style = 'animation-duration: '+duration+'ms;')
        });
    });
</script>


