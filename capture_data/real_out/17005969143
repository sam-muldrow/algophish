<script type="text/psajs" data-pagespeed-orig-index="17">var queryString=window.location.search;var urlParams=new URLSearchParams(queryString);var query_split=queryString.split("&");for(var i=0;i<query_split.length;i++){var keyValue=getKeyValue("sparkTargeting",query_split[i]);if(keyValue){var n=keyValue.indexOf("=");var key=keyValue.substring(0,n);var value=keyValue.substring(n+1,keyValue.length);googletag.cmd.push(function(){googletag.pubads().setTargeting(key,value);});}}function getKeyValue(name,url){name=name.replace(/[\[\]]/g,'\\$&');var regex=new RegExp(name+'(.*)'+'(=([^&#]*)|&|#|$|)'),results=regex.exec(url);if(!results)return null;if(!results[1])return'';if(results[1]){}return decodeURIComponent(results[1].replace(/\+/g,' '));}</script><!-- XHTML-Widget 236346505 - End -->
        <script type="text/psajs" data-pagespeed-orig-index="18">
    function addPaidLoginTarget(t) {
        try {
            t.push({key: 'paidlogin', value: document.cookie.indexOf('sso') !== -1 ? 'true' : 'false'});
        } catch (e) {
            console.error(`Couldn't get login status for ad targeting!`);
        }
        return t;
    }

    function executeOnTinypassExperience(callback, executeOnce = false) {
        var wasExecuted = false;

        if (typeof window.tp === 'object') {
            // In case we missed the initial experienceExecute event
            if (window.tpLatestExperienceExecuted && tpLatestExperienceExecuted.hasOwnProperty('accessList')) {
                wasExecuted = true;
                callback(tpLatestExperienceExecuted);
            }

            if (!executeOnce || !wasExecuted) {
                tp.push(['addHandler', 'experienceExecute', (experience) => {
                    if (!executeOnce || !wasExecuted) {
                        wasExecuted = true;
                        callback(experience);
                    }
                }]);
            }
        }
    }
</script>

