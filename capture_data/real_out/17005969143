                                }, true);
                            } else {
                                
                                                        googletag.cmd.push(function () {
                                                            gptAdSlots[9] = googletag.defineOutOfPageSlot("/39216077/sta_abendblatt/homepage/outofpage", "outofpage_homepage").addService(googletag.pubads()).setTargeting("pos", "outofpage").setCollapseEmptyDiv(true);
                                                        });
                                                    
                            }

                            document.addEventListener("DOMContentLoaded", function(e) {
                                var articleBody = document.getElementsByClassName('article__body')[0];
                                var sideColumn = document.getElementsByClassName('sidebar article__aside')[0];
                                var sideBarColumn;
                                var sideBarColumnChildren = 0;
                                try {
                                    sideBarColumn = sideColumn.getElementsByClassName('sidebar__column')[0];
                                    sideBarColumnChildren = sideBarColumn.childElementCount;
                                } catch (e) {
                                    sideBarColumnChildren = 0;
                                }
                                // remove sidebar column, if there are no children (ads)
                                if (sideBarColumnChildren < 1 && sideColumn !== undefined) {
                                    sideColumn.remove();
                                }

                                // hide side column and set article width 100% to avoid gap on right
                                // todo: only do this, if all ads on right column are hidden, else show it
                                if ((sideBarColumn === false) && articleBody !== undefined) {
                                    articleBody.style.width = '100%';
                                }

                            });

                                

                            var adRefresh = function (slot) {
                                googletag.cmd.push(function () {
                                    // window.console.log("Ads Refresh Desktop");
                                    googletag.pubads().refresh([gptAdSlots[slot]]);
                                });
                            };

                            googletag.cmd.push(function () {
                                googletag.pubads().collapseEmptyDivs(true);

                                

                                googletag.pubads().enableSingleRequest();

                                googletag.pubads().addEventListener("slotRenderEnded", function (event) {
                                    var slotId = event.slot.getSlotElementId(),
                                        isHalfPageSlot = slotId === "oms_gpt_rectangle",
                                        $slot = jQuery("#" + slotId);

                                    if (event.isEmpty || slotId === "oms_gpt_billboard" && typeof (no_billboard) != "undefined" && no_billboard) {
                                        $slot.closest(".ad").hide();
                                    } else {
                                        $slot.closest(".ad").show();
                                    }

                                    if (isHalfPageSlot) {
                                        /* Slot contains a half page ad */
                                        if ($slot.height() > 350) {
                                            jQuery(".hide-on-halfpage-ad").hide();
                                            jQuery(".show-on-halfpage-ad").show();
                                        }
                                        /* Slot contains no ad or is smaller than a half page ad */
                                        else {
                                            jQuery(".hide-on-halfpage-ad").show();
                                            jQuery(".show-on-halfpage-ad").hide();
                                        }
                                    }
                                });

                                googletag.enableServices();
                                googletag.pubads().setTargeting("bundesland", ["HH"]);

                                var omsFormatValues = [];
                                
                                    omsFormatValues.push("no_intext");
                                
                                googletag.pubads().setTargeting("oms_format", omsFormatValues);

                                

                                var targets = addPaidLoginTarget([{"key":"artikeltyp","value":"index"},{"key":"keyword","value":["Hamburg","Nachrichten","News","Nachrichten-Archiv","Politik","Sport","Wirtschaft","Finanzen","Kultur","Reise","Fotostrecke"]},{"key":"lokseite","value":"homepage"},{"key":"loktitel","value":"hao"},{"key":"paid","value":"false"},{"key":"wp","value":"homepage"}]);

                                jQuery.each(targets, function (index, target) {
                                    googletag.pubads().setTargeting(target.key, [target.value]);
                                });
                            });
                            var stroer_targets = [{"key":"artikeltyp","value":"index"},{"key":"keyword","value":["Hamburg","Nachrichten","News","Nachrichten-Archiv","Politik","Sport","Wirtschaft","Finanzen","Kultur","Reise","Fotostrecke"]},{"key":"lokseite","value":"homepage"},{"key":"loktitel","value":"hao"},{"key":"paid","value":"false"},{"key":"wp","value":"homepage"}];
