<source srcset="/aktionen/der-westen-hilft/gemeinsam-gegen-den-hunger/teaser-der-westen-hilft-gemeinsam-fuer-muetter-100~_v-gseaclassicxl.jpg" media="(min-width: 1901px)" />
<source srcset="/aktionen/der-westen-hilft/gemeinsam-gegen-den-hunger/teaser-der-westen-hilft-gemeinsam-fuer-muetter-100~_v-ARDAustauschformat.jpg" media="(min-width: 1010px) and (max-width: 1900px)" />
