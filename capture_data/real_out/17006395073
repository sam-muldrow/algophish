<script type="text/javascript">
    /* <![CDATA[ */
    goog_snippet_vars = function(ident) {
        var w = window;
        w.google_conversion_id = 990576704;
        w.google_conversion_label = (ident && ident.label) ? ident.label : "ddaMCNDLpAYQwICs2AM";
        w.google_conversion_value = 1.00;
        w.google_remarketing_only = false;
    }
    // DO NOT CHANGE THE CODE BELOW.
    goog_report_conversion = function(url, ident) {
        goog_snippet_vars(ident);
        window.google_conversion_format = "3";
        var opt = new Object();
        opt.onload_callback = function() {
            if (typeof(url) != 'undefined') {
                window.location = url;
            }
        }
        var conv_handler = window['google_trackConversion'];
        if (typeof(conv_handler) == 'function') {
            conv_handler(opt);
        }
    }
    /* ]]> */
</script>
