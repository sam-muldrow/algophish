<script type="text/javascript">
	$(function() {
		$('.fn-currency-change-form').on('click', '.fn-currency-change', function(){

			var selected_currency = $(this).data('selected-currency-code') || 'USD';
			$('input[name="selected_currency"]').val(selected_currency);
			
			var $submit_form = $(this).parents('.fn-currency-change-form');
			var currency     = $(this).val();
			var url          = $submit_form.find('.fn-currency-url-' + currency).val();
			if(url){
				$submit_form.attr('action', url);
			}

			$submit_form.submit();
		});

		$('#currency-switch-area').hide();
		$('#currency-switch').on({
			mouseover:function(){
				$('#currency-switch-area').show();
			},
			mouseout:function(){
				$('#currency-switch-area').hide();
			}
		});
		$('#currency-switch-area').on({
			mouseout:function(){
				$('#currency-switch-area').hide();
			}
		});
	});
</script>


							

<script type="text/javascript">
$(document).ready(function() {

	var $menu = $('.dropdown-menu').menuAim({
		activate: activateSubmenu,  // fired on row activation
		deactivate: deactivateSubmenu  // fired on row deactivation
	});


	function activateSubmenu(row) {
			var $row = $(row),
				submenuId = $row.data("submenuId"),
				$submenu = $("#" + submenuId);


			// Show the submenu
			$submenu.show();
		}

		function deactivateSubmenu(row) {
			var $row = $(row),
				submenuId = $row.data("submenuId"),
				$submenu = $("#" + submenuId);

			// Hide the submenu and remove the row's highlighted look
			$submenu.hide();
		}


		// Bootstrap's dropdown menus immediately close on document click.
		// Don't let this event close the menu if a submenu is being clicked.
		// This event propagation control doesn't belong in the menu-aim plugin
		// itself because the plugin is agnostic to bootstrap.
		$(".dropdown-menu li").click(function(e) {
			e.stopPropagation();
		});

		$(document).click(function() {
			// Simply hide the submenu on any click. Again, this is just a hacked
			// together menu/submenu structure to show the use of jQuery-menu-aim.
			$(".popover").css("display", "none");
			$("a.maintainHover").removeClass("maintainHover");
		});

});

</script>

