			<img class="loading-img" src="//cdn.beforward.jp/assets/images/loading_animation.gif?_=64c30c1f"/>
		</div>
		<input type="hidden" id="recently-checked-url" data-recently-checked-url="/recently_checked/getViewedVehicles">
		<script type="text/javascript">
			$(function(){
				var url = $('#recently-checked-url').data('recently-checked-url');
				$.ajaxSetup({ async: true, cache: false });
				$.get(url,
						function (data) {
							$('.fn-recently-checked-loading').remove();
							$('.fn-top-display-lists-recently-checked').replaceWith(data);

							//data内のHTMLにliが存在する(＝履歴が存在する)場合に表示
							if ($('.fn-top-display-lists-recently-checked > li').length > 0) {
								$('#recently_viewed_vehicles').show();
							}
						}
				);
			});
		</script>
	</div>






