<script type="text/javascript">
//<![CDATA[
function topSearch(){
	var $keyword = $('#keyword');

	if ($keyword.attr('title') === $keyword.val()) {
		$keyword.val('');
	}

	return searchFormAction(document.getElementById("schFm"));
}
//]]>
</script>

<script>
$(function () {
	// placeholder for IE8
	var supportsInputAttribute = function (attr) {
		var input = document.createElement('input');
		return attr in input;
	};
	if (!supportsInputAttribute('placeholder')) {
		$('[placeholder]').each(function () {
			var
			input = $(this),
			placeholderText = input.attr('placeholder'),
			placeholderColor = 'GrayText',
			defaultColor = input.css('color');
			input.
			focus(function () {
				if (input.val() === placeholderText) {
				input.val('').css('color', defaultColor);
				}
			}).
			blur(function () {
				if (input.val() === '') {
				input.val(placeholderText).css('color', placeholderColor);
				} else if (input.val() === placeholderText) {
				input.css('color', placeholderColor);
				}
			}).
			blur().
			parents('form').
				submit(function () {
				if (input.val() === placeholderText) {
					input.val('');
				}
				});
		});
	}
  	// NOT REGISTERED YET?
	$('.not-registered-closebutton').on('click', function() {
		// コントローラで設定している期間と同じ12時間後に設定
		var expires = new Date();
		expires.setTime(expires.getTime() + 1000 * 60 * 60 * 12);
		document.cookie = 'wwwbf[account_not_reg_yet]=close;path=/;domain=.beforward.jp;expires='+ expires.toUTCString();
		$('.not-registered').hide();
	});
});
</script>
<!-- /placeholder  -->

