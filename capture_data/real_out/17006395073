<script src="//cdn.beforward.jp/assets/js/jquery.lightbox-2.9.min.js?_=64c30c39"></script>

<script src="//cdn.beforward.jp/assets/js/reviews_and_ratings.js?_=64c30c39"></script>

<script type="text/javascript">
//<![CDATA[
$(function() {
	// keyword_area_action
	$('#anchor-of-search-button').click(function(e){
		$('#dummy-anchor-of-search-button').click();
	});

	// Frequently Asked Questions Accordion
	$('#top-faq-contents dt').on('click', function(){
		var $elm = $(this);
		var $elmIcon = $elm.find('i');
		$elm.toggleClass('is-open');
		$elm.next('dd').slideToggle(100);
		$elmIcon.toggleClass('icon-angle-down icon-angle-up');
	});

	// 在庫国を変更してSearchをかける
	$('.shipping-country .country-sort').on('click', function(){
		$('.shipping-country .country-sort').removeClass("is-active");
		var countryVal = $(this).data('value');
		//ボタンをアクティブ化
		$(this).addClass("is-active");
		
		var $activeTab = $('.tab-active');
		var form = '';

		// アクティブになっているタブによって検索URLを変更
		if($activeTab.hasClass('clearance')) {
			form = '/stocklist/stock_country=' + countryVal + '/icon_clearance=1/sortkey=q';
		}else if($activeTab.hasClass('new-arrivals')) {
			form = '/stocklist/stock_country=' + countryVal + '/sortkey=n';
		}else if($activeTab.hasClass('premium-class')) {
			form = '/stocklist/stock_country=' + countryVal + '/icon_campaign=1/sortkey=n';
		}else if($activeTab.hasClass('third-party')) {
			form = '/stocklist/stock_country=' + countryVal + '/icon_promotion16=1/sortkey=n';
		}

		window.location.href = form;
	});

	// 検索条件から車両数取得
	$(window).load(function() {
		getSearchCount();
	});
});
//]]>
</script>

