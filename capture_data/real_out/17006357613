<!--analitits UTM V-28.04-->
<script>
const params = new URLSearchParams(window.location.search);
params.delete("q");
params.delete("video");
let pam = params.get("pam");

function forms() {

    const links = document.querySelectorAll('form');
    links.forEach(form => {
        let url = form.action;
        if (typeof url !== "string") return false;
        let paramObj = {};
        for (var value of params.keys()) {
            paramObj[value] = params.get(value);
        }
        for (const [key, value] of Object.entries(paramObj)) {
            const hidden = document.createElement("input");
            hidden.type = "hidden";
            hidden.name = key;
            hidden.value = value;
            form.appendChild(hidden);
        }
    });
}

function links() {
    const links = document.querySelectorAll('a');
    links.forEach(link => {
        let oldUrl = link.href;
        let updatedURL = oldUrl + (oldUrl.includes("?") ? "&" : "?") + params.toString();
        link.href = updatedURL;
    });
}

function onloader() {
    if (Array.from(params).length !== 0) {
        links();
        forms();
    }
}
onloader()
document.addEventListener("DOMContentLoaded", onloader);
</script>
