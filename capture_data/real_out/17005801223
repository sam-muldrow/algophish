        function throttle( fn, delay ) {
            var _this = this, timer = null;
            return function() {
                var context = _this, args = arguments;
                clearTimeout( timer );
                timer = setTimeout(function() {
                    fn.apply( context, args );
                }, delay );
            };
        }

