                                var aTime = 1000;
                                var aDelta = 50;
                                var oldOP = 1;

                                var d = oldOP / (aTime / aDelta);

                                setTimeout(function () {
                                    var aInterval = setInterval(function () {
                                        oldOP -= d;
                                        if (oldOP <= 0) {
                                            mainProductScreenshot.style.opacity = 0;
                                            mainProductScreenshot.style.display = "none";
