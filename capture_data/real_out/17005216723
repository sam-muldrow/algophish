var fadeThis = document.getElementById('fade1');

VisSense.VisMon.Builder(VisSense(fadeThis, { fullyvisible: 0,  hidden: 0}))
    .on('fullyvisible', function(monitor) {
     fadeThis.style.opacity = 1;
    })
    .on('hidden', function(monitor) {
      fadeThis.style.opacity = 0;
    })
    .build()
    .start();
</script>
