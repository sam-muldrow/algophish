    })
    .on('hidden', function(monitor) {
      fadeThis3.style.opacity = 0;
    })
    .build()
    .start();
</script>
<script>
var fadeThis5 = document.getElementById('fade5');

VisSense.VisMon.Builder(VisSense(fadeThis5, { fullyvisible: 0.1 }))
    .on('fullyvisible', function(monitor) {
     fadeThis5.style.opacity = 1;
    })
    .on('hidden', function(monitor) {
      fadeThis5.style.opacity = 0;
    })
    .build()
    .start();
</script>

<script>
document.getElementById('one').onclick = function(){
  window.scrollTo({
    behavior: 'smooth',
    top:
      document.querySelector('.section2').getBoundingClientRect().top -
      document.body.getBoundingClientRect().top - 45,
  });
}
document.getElementById('two').onclick = function(){
	$("html, body").animate({ scrollTop: $('.couple_cta').offset().top - ($(window).height()-$('.phone_cta').outerHeight())}, 'easeOutCirc');
}
document.getElementById('three').onclick = function(){
	$("html, body").animate({ scrollTop: $('.phone_cta').offset().top - ($(window).height()-$('.phone_cta').outerHeight() + 10)}, 2000, 'easeOutCirc');
}
document.getElementById('four').onclick = function(){
	$("html, body").animate({ scrollTop: $('.section3').offset().top - 50}, 'easeOutCirc');
};

$('#five').click(function(){
	$('html, body').animate({scrollTop: $('body').offset().top}, 1000, 'easeOutCirc');
});



</script>
<script>

	$(document).ready(function(){
		var i = 0;
		var time = 5000;
		var galleryImage = $('.mobiles').find('img').first();
		var images = [
			'/tour/zxdatingsequence/mobiles/1.webp',
			'/tour/zxdatingsequence/mobiles/2.webp',
			'/tour/zxdatingsequence/mobiles/3.webp',
			'/tour/zxdatingsequence/mobiles/4.webp',
			'/tour/zxdatingsequence/mobiles/5.webp',
		];
	
	
		setInterval(function() {
			i = (i + 1) % images.length;
			var oldImg = $(".mobiles img");
			var img = new Image();
			img.src = images[i];
			var newImg = $(img).hide();
			$(".mobiles").append(img);
			newImg.fadeIn(750);		
			oldImg.stop(true).fadeOut(1500, function() {
				$(this).remove();
			});
			return false;
		}, time);
	
		$('.mobiles').click(function(){
			window.location = "/main.php?a=user.registerfp_mobile&id=noadvert&prg=1&tour=1&ot=&pg=1&niche=w-pg";
		});
	
	})
	
	</script>	
