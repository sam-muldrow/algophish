EbaumComment.prototype.addHandler=function(){if(!this._authenticated)return this.promptLogin(),!1;if(!jQuery("#addComment").hasClass("disabled")&&0==this._processing){this._processing=1;var b=escape(jQuery("#addCommentText").val()).replace(/%u2019/i,"%27");eComment=this;jQuery.post("/comment/add/"+this._id+"/",{comment:b},function(b){if(b.success)clearInterval(f),clearTimeout(g),eComment.fire(CommentEvent.ADD_COMPLETE),jQuery("#addCommentText").val(""),jQuery("#commentMessage").html(b.message).show(),
b=parseInt(jQuery(".commentTitle").html()),jQuery(".commentTitle").html(b+1+" Comments"),eComment._ga.fireGA("User - Media Comment",eComment._mediaType,window.location.href);else{jQuery("#commentError").html(b.error).show();g=b.error.replace(/[^0-9]/g,"");g=b.data;if(0<g){jQuery("#commentTimer").show();jQuery("#addComment").addClass("disabled");var c=g,f=setInterval(function(){jQuery("#commentSeconds").html(--c)},1E3),g=setTimeout(function(){clearInterval(f);jQuery("#commentTimer, #commentError").hide();
