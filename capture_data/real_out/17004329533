<div id="backToTop" class="backToTop" >
	<span class="backToTopArrow"></span>
	<span class="backToTopText">Top</span>
</div>
<script type="text/javascript">
var amountScrolled = jQuery(window).height();
var miniMenuScroll = 80;
jQuery(window).scroll(function() {
    if (jQuery(window).scrollTop() > miniMenuScroll) {
        //jQuery('#miniMenu').fadeIn('slow');
        jQuery('#miniMenu').addClass('slideDown');
    } else {
        jQuery('#miniMenu').removeClass('slideDown');
    }
	if (jQuery(window).scrollTop() > 1 ) {
		jQuery('#backToTop').fadeIn('slow');
		jQuery('.sticky-container').fadeOut('fast');
	} else {
		jQuery('#backToTop').fadeOut('slow');
		jQuery('.sticky-container').fadeIn('fast');
	}
});
jQuery('#backToTop').click(function() {
    var backGA = new EbaumGAEvent();
    backGA.fireGA('Back To Top', 'desktop', window.location.href);
	jQuery('html, body').animate({
		scrollTop: 0
	}, 700);
	return false;
});
</script>        <div id="cmn_wrap">

