              <script>

const boolUserLoggedIn = userLoggedIn && userLoggedIn === 'true';

if (!boolUserLoggedIn) {
	blogherads.adq.push('frame2');
}


window.addEventListener('load', (event) => {
    
	//var ilist = document.images;
var imgs = document.getElementsByClassName("lazysrc");

var link = document.getElementsByTagName("link");

// console.log("imgs[i].src")

for(var i = 0; i < imgs.length; i++) {


  if(imgs[i].src == 'banner.gif') {
	  // found the banner
	  imgs[i].src = 'banner.gif';
	  // console.log(imgs[i])
  }

  // console.log("imgsimgs")
  // console.log(imgs[i].dataset.lazysrc)

  imgs[i].src = imgs[i].dataset.lazysrc;

  // console.log(imgs[i].src)
}

for(var i = 0; i < link.length; i++) {


// console.log("link")
// console.log(link[i].dataset.lazysrc)

link[i].src = link[i].dataset.lazysrc;
link[i].as = "style";
link[i].onload = "this.onload=null;this.rel='stylesheet'";

// console.log(link[i].src)
} 
	
});

</script>

