    	<div id="skm-ad-flexbanner"></div>
<script type="text/javascript">

blogherads.adq.push(function() { 
    let newSlot = blogherads.defineSlot('flexbanner', "skm-ad-flexbanner"); 
    newSlot.display(); 

    const subheaderDivBlock = document.getElementById('subheader_atf_wrapper');
    const followDivBlock = document.getElementsByClassName('followUs emc_sideblock')[0];

    blogherads.addEventListener('gptSlotRenderEnded', function(event) {

        const { domId } = event.slot;
        const [adWidth, adHeight] = event.size;
      

        if (
            domId === 'skm-ad-flexbanner' && 
            adWidth >= 970
        ) {
            subheaderDivBlock.style.display = "flex";
            subheaderDivBlock.style.justifyContent = "center";

            const subheaderAtfDivBlock = document.getElementById('subheader_atf');

            followDivBlock.style.display = "none";

            if (subheaderAtfDivBlock) {
                subheaderAtfDivBlock.style.position = "relative";
                subheaderAtfDivBlock.style.marginBottom = "20px";
            }

            /* console.log(" >= 970"); */
            /* console.log("adWidth: " + adWidth); */
            /* console.log("adHeight: " + adHeight); */
            /* console.log("domId: " + domId); */
        } else if (
            domId === 'skm-ad-flexbanner' && 
            adWidth < 970
        ) {
            var clientWidth = document.getElementById('skm-ad-flexbanner').clientWidth;
            /* console.log(clientWidth + " < 970"); */
            /* console.log("adWidth: " + adWidth); */
            /* console.log("adHeight: " + adHeight); */
            subheaderDivBlock.style.display = "block";
            followDivBlock.style.display = "block";
            /* console.log("domId: " + domId); */
            var clientWidth2 = document.getElementById('subheader_atf').clientWidth;
           /* console.log("clientWidth2 : " + clientWidth2 ); */
        }

    });

});



  setInterval(function(){ 

    var checkSizeW = document.getElementById('skm-ad-flexbanner').clientWidth;
    const subheaderDivBlock = document.getElementById('subheader_atf_wrapper');
    const followDivBlock = document.getElementsByClassName('followUs emc_sideblock')[0];

    if (
            checkSizeW >= 970
        ) {

            subheaderDivBlock.style.display = "flex";
            subheaderDivBlock.style.justifyContent = "center";

            const subheaderAtfDivBlock = document.getElementById('subheader_atf');

            followDivBlock.style.display = "none";

            if (subheaderAtfDivBlock) {
                subheaderAtfDivBlock.style.position = "relative";
                subheaderAtfDivBlock.style.marginBottom = "20px";
            }

            /* console.log(" >= 970 -->Luis"); */
            /* console.log("adWidth: " + checkSizeW); */
    


         } else if (
            checkSizeW < 970
        ) {
            var clientWidth = document.getElementById('skm-ad-flexbanner').clientWidth;
            /* console.log(clientWidth + " < 970 -->Luis"); */
            /* console.log("adWidth: " + checkSizeW); */
            subheaderDivBlock.style.display = "block";
            followDivBlock.style.display = "block";
        }


}, 3000);

</script>    </div>
