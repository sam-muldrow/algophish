					jQuery('.topCommentsPagination .page').click(function() {
					    var info = jQuery(this).attr('id').split('-');
						jQuery('.topCommentsPagination .current').removeClass('current');
						jQuery(this).addClass('current');
						jQuery('.topCommentWrapper').hide();
						jQuery('#topComment-' + info[1]).show();
