<script type="text/javascript">
jQuery(document).ready(function() {
    var odItems = jQuery('.odometer');
    var str = '';
    for (var i = 0; i < odItems.length; i++) {
        var fullId = jQuery(odItems[i]).attr('id');
        var info   = fullId.split('-');

    	var id = {
    		type: info[0],
    		id: info[1]
    	};
    	str += id.id;

    	if (i < odItems.length - 1) {
    		str += ',';
    	}
    }


    var idStr = str;

    });
</script>

