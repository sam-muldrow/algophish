    <script charset="utf-8" src="assets/javascripts/components/nav/component-nav-side.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.min.js" type="text/javascript"></script>

    <script charset="utf-8" src="assets/libs/jquery.easy-autocomplete-min-mod.js"></script>

    <script>
      $.getScript("assets/javascripts/components/component-header.js").done(function(){
        $.getScript("assets/javascripts/components/component-footer.js").done(function(){
          new Vue({
            el: '#app',
            mixins: [mixin_ajax],
            data: function () {
              return {
                ajax: {
                  requests: {
                    shop_campaigns_list: {
                      url: 'assets/data/json/campaigns.json',
                    },
                  },
                  responses: {
                    shop_campaigns_list: {
                      success: false,
                      data: '',
                    },
                  },
                }
              }
            },
            mounted:function() {
              this.getDataByAjax();

              var elem = document.querySelector('.news-scroll-flickity');
              var flkty = new Flickity( elem, {
                cellSelector: '.news-scroll-item',
                cellAlign: 'center',
                autoPlay: 2500,
                wrapAround: true,
                prevNextButtons: true,
                draggable: '>1',
                pageDots: false,
                freeScroll: true,
                on: {
                  ready: function() {
                    document.querySelector('.news-scroll-wrap').style.height = "auto";
                  }
                }
              });
              initSearch();
            },
            updated:function() {

              if(this.ajax.responses.shop_campaigns_list.success){
                var elem = document.querySelector('.campaign-list');
                var flkty = new Flickity( elem, {
                  cellSelector: '.campaign-list-item',
                  cellAlign: 'left',
                  contain: true,
                  autoPlay: false,
                });
              }
            },
          });
        });
      });

    </script>

