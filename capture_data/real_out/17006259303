<script>
/* floater for google ads*/
function floater_google() {
	googletag.pubads().addEventListener('slotRenderEnded', function(event) {
		try {
			var floaterUnits = ['sd_floater', 'sd_floater_mob', 'zd_floater', 'zd_floater_mob', 'sib_floater', 'sib_floater_mob', 'dub_floater', 'dub_floater_mob'];

			var slot = event.slot;
			var slotAdUnit = slot.getAdUnitPath().split('/')[2]; /* /11425286/sd_floater */

			if (floaterUnits.includes(slotAdUnit) ) {
				if (event.isEmpty !== true) {
					console.log('slot.getAdUnitPath event not empty', slotAdUnit);															

					var slotWrapperEl = document.querySelector('.position_floater_top');
					var adFrame = slotWrapperEl.querySelector('iframe');

					if (!slotWrapperEl.classList.contains('dfp-ticker')) {

						slotWrapperEl.classList.add('floater_top_anchor');

						if (adFrame.offsetHeight > 5) {
							setTimeout(function() {
								if (!document.querySelector('.floater_top_close')) {
									adFrame.parentNode.insertAdjacentHTML('afterbegin', '<button type="button" class="floater_top_close"><img src="/templates/site/images/pngs/dfp-close.png" alt="Zatvori" /></button>');

									document.querySelector('.floater_top_close').addEventListener('click', function() {
										slotWrapperEl.parentNode.removeChild(slotWrapperEl);
									});
								}											
							}, 5000);										
						}
					}
				}
			}
		} catch (error) {
			console.error(error);
		} 
	});		
}

function getCookiePrebid(cname) {
	var name = cname + "=";
	var ca = document.cookie.split(';');
	for(var i = 0; i < ca.length; i++) {
		var c = ca[i];
		while (c.charAt(0) == ' ') {
			c = c.substring(1);
		}
		if (c.indexOf(name) == 0) {
			return c.substring(name.length, c.length);
		}
	}
	return "";
}

var trackGaPrebidEvents;
trackGaPrebidEvents = Math.floor((Math.random()*100) + 1) == 1 ? true : false;
function gaPrebidEvents(gaLabel) {
	if (trackGaPrebidEvents) {		
		var gaEventCat = 'Prebid 1/100 PW';	
		var urlPortalHost = new URL(window.location);		
		try {
			ga("send", "event", gaEventCat , urlPortalHost.hostname, gaLabel);
			console.log ("%c GA ::::: " + gaEventCat + ' ' + urlPortalHost.hostname + ' ' + gaLabel, 'color: #006600;' );	
		} catch (e) {
			console.log(e);
		}	
	}
}
gaPrebidEvents('Pageview ALL')	
