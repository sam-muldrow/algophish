				gaPrebidEvents('Pageview Prebid Init')	

				pbjs.setConfig(prebidSettings);
				pbjs.addAdUnits(prebidUnits);
				pbjs.aliasBidder("appnexus", "projectagora", {gvlid: 1032});
				
				// pbjs.enableAnalytics({provider: 'ga', options: {global: 'ga', enableDistribution: false}});
				// pbjs.enableAnalytics([{provider: 'pubwise', options: {site: '458cf2c6-1dfd-4bba-bd7d-d69c79b661c3', endpoint:'https://api.pubwise.io/api/v4/event/add/'}}]);
				pbjs.requestBids({bidsBackHandler: initAdserver, timeout: prebidTimeout});
			});

			gaPrebidEvents('Pageview Prebid Init 2')	
			// prebid
			function initAdserver(blTimeout2 = false) {
				if (!blTimeout2) {
					var gaLabel = pbjs.initAdserverSet ? 'Pageview Prebid Done' : 'Pageview Prebid Timeout1';
					gaPrebidEvents(gaLabel);
					console.log ("%c DFP::::: Prebid Timeout1", 'color: #006600;' );
				}

				if (pbjs.initAdserverSet) return;

				pbjs.initAdserverSet = true;
				googletag.cmd.push(function() {
					pbjs.que.push(function() {
						pbjs.setTargetingForGPTAsync();
                                                                                                        userAuth.done(function (response) {
                                                                if (response.success && typeof response.logged_in !== 'undefined' && response.logged_in && typeof response.adfree !== 'undefined' && response.adfree) {
                                                                        console.log('Ad free');
                                                                } else {
                                                                        googletag.pubads().refresh();
                                                                }
                                                        }).fail(function (response) {
                                                                googletag.pubads().refresh();
                                                        });
                                                						//googletag.pubads().refresh(prebidUnits);
					});
				});
			}

			setTimeout(function() {
				gaPrebidEvents('Pageview Prebid Timeout2')	
				console.log ("%c DFP::::: Prebid Timeout2", 'color: #006600;' );
				initAdserver(true);
			}, prebidTimeoutFailsafe);

