	<script>
    $(function(){
       if($.getUrlParam !== undefined){
            var value = $.getUrlParam('onlyShowFeature') == 'true' ? 375 : 640;
        var phoneScale = parseInt(window.screen.width) / value;
        $('[name=viewport]').attr('content','width=device-width, initial-scale='+phoneScale+', minimum-scale='+phoneScale+', maximum-scale='+phoneScale+', user-scalable=no')
       }
       var pattern
       $('#cardNumber').bind('input propertychange',function(){
           // var op=""
           // // this.value = this.value.replace(/[^\d]/g, '').replace(/(.{4})/g, '$1');
           // var t=$("#cardNumber").val().replace(/\D/g, "");
           // for (var i=0;i<t.length;i++){
           //     if(i%4===0 && i>0) {
           //         op += " " + t.charAt(i);
           //     }else {
           //         op += t.charAt(i);
           //     }
           // }
           // $(this).val(op);
           if (!!window.ActiveXObject || "ActiveXObject" in window){
               const oldLen = this.value.length
               this.value = this.value.replace(/\s/g, '').replace(/(.{4})(?=[^$])/g, "$1 ")
               this.value%4 === 0 ? $("#cardNumber")[0].selectionStart += (this.value.length - oldLen):$("#cardNumber")[0].selectionStart += (this.value.length - 1)
               $("#cardNumber")[0].selectionEnd = $("#cardNumber")[0].selectionStart
           }else{
               this.value = this.value.replace(/\s/g, '').replace(/(.{4})(?=[^$])/g, "$1 ")
           
           }
           pattern = /^\d{14,18}$/;
           if (pattern.test($("#cardNumber").val().replace(/\s*/g,""))) {
               $(".errMsg").html("")
               $(".continue1").addClass("next-step")
           }else{
               $(".continue1").removeClass("next-step")
           }
       })
       
       $("#phoneNumber").bind('input propertychange',function(){
           pattern = /^[1][3,4,5,6,7,8,9][0-9]{9}$/;
           if (pattern.test($("#phoneNumber").val())) {
               $("#otp").removeClass("coutTime")
           }
       })
       $("#optValue").bind('input propertychange',function(){
           if($("#optValue").val().length != 0){
               $(".continue2").addClass("next-step")
           }else{
               $(".continue2").removeClass("next-step")
           }
       })
   })
   function changeRecommendPage() {
    	if($('.recommend_box.first').is(':hidden')){
    		$('.recommend_box.first').show();
    		$('.recommend_box.second').hide();
    	}else {
    		$('.recommend_box.first').hide();
    		$('.recommend_box.second').show();
    	}
    }
   </script>
   
