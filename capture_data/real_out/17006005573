<script>

	try {
		var login2 = new Vue({

			el:"#custom_valiz_noe",
			data:{
				emailSubscribe:"1",
				register_Data:{
					registerType : "THIRDPARTY",
					registrationChannel : "WEB",
					mobile: "",//dynamic
					otpCode : "", //dynamic
					thirdParty: localStorage.getItem('thirdParty'),
					thirdPartyData: localStorage.getItem('thirdPartyData'),
					emailSubscribe: "1"
				},
				regPhone:/^1[0123456789][0-9]{9}$/,
				mobileErr: {
					show: "none",
					msg: ""
				},
				phone:'',
				valid1OtpCode: "",
				otpErr: {
					show: "none",
					msg: ""
				},
				otp_data: {
					otpType: "USER_REGISTRATION",
					mobile: "",
					captchaCode: ""
				},
				send_otpErr: {
					show: "none",
					msg: ""
				},
				send_regErr: {
					show: "none",
					msg: ""
				},
				pwdlogin:"",
				withoutpwdErr:"",
				agree_policy:"0",
				check_agr: false,
				agrErr: {
					show: "none",
					msg: "请同意用户协议",
				},
				registerwithPwd:{
					registerType : "THIRDPARTY",
					registrationChannel : "WEB",
					mobile: localStorage.getItem('mobile'),
					thirdParty: localStorage.getItem('thirdParty'),
					thirdPartyData: localStorage.getItem('thirdPartyData'),
					pwd: "",
					pwdSafeToken: localStorage.getItem('pwdSafeToken'),
					emailSubscribe: "1"
				},

				withoutPwdlogin:{
					registerType : "THIRDPARTY",
					registrationChannel : "WEB",
					mobile: localStorage.getItem('mobile'),
					thirdParty: localStorage.getItem('thirdParty'),
					thirdPartyData: localStorage.getItem('thirdPartyData'),
					pwdSafeToken: localStorage.getItem('pwdSafeToken'),
					resetPwdConsent: "1",
					emailSubscribe: "1"

				}


			},
			created:function() {

				if (localStorage.getItem('login_nc_token_header')) {
					localStorage.removeItem('login_nc_token_header');
				}
				if (localStorage.getItem('login_session_id_header')) {
					localStorage.removeItem('login_session_id_header');
				}
				if (localStorage.getItem('login_sig_header')) {
					localStorage.removeItem('login_sig_header');
				}
				if (localStorage.getItem('register_nc_token_header')) {
					localStorage.removeItem('register_nc_token_header');
				}
				if (localStorage.getItem('register_session_id_header')) {
					localStorage.removeItem('register_session_id_header');
				}
				if (localStorage.getItem('register_sig_header')) {
					localStorage.removeItem('register_sig_header');
				}
				if (localStorage.getItem('third_reg_nc_token_header')) {
					localStorage.removeItem('third_reg_nc_token_header');
				}
				if (localStorage.getItem('third_reg_session_id_header')) {
					localStorage.removeItem('third_reg_session_id_header');
				}
				if (localStorage.getItem('third_reg_sig_header')) {
					localStorage.removeItem('third_reg_sig_header');
				}

			},
			mounted: function() {
				var nc_token3 = ["FFFF0N0000000000A10B", (new Date()).getTime(), Math.random()].join(':');
				var NC_Opt3 = {
					renderTo: "#aliverifyHeader3",
					appkey: "FFFF0N0000000000A10B",
					scene: "nc_message",
					token: nc_token3,
					customWidth: 316,
					trans: {
						"key1": "code0"
					},
					elementID: ["usernameID"],
					is_Opt: 0,
					language: "cn",
					isEnabled: true,
					timeout: 3000,
					times: 5,
					apimap: {
						// 'analyze': '//a.com/nocaptcha/analyze.jsonp',
						// 'get_captcha': '//b.com/get_captcha/ver3',
						// 'get_captcha': '//pin3.aliyun.com/get_captcha/ver3'
						// 'get_img': '//c.com/get_img',
						// 'checkcode': '//d.com/captcha/checkcode.jsonp',
						// 'umid_Url': '//e.com/security/umscript/3.2.1/um.js',
						// 'uab_Url': '//aeu.alicdn.com/js/uac/909.js',
						// 'umid_serUrl': 'https://g.com/service/um.json'
					},
					callback: function (data) {
						//window.console && console.log(nc_token)
						//window.console && console.log(data.csessionid)
						//window.console && console.log(data.sig)
						localStorage.setItem("third_reg_nc_token_header", nc_token3);
						localStorage.setItem("third_reg_session_id_header", data.csessionid);
						localStorage.setItem("third_reg_sig_header", data.sig);
					}
				}
				var nc3 = new noCaptcha(NC_Opt3)
				nc3.upLang('cn', {
					_startTEXT1: "请先按住滑块滑动到最右边，再登陆",
					_yesTEXT: "验证通过",
					_error300: "哎呀，出错了，点击<a href=\"javascript:__nc.reset()\">刷新</a>再来一次",
					_errorNetwork: "网络不给力，请<a href=\"javascript:__nc.reset()\">点击刷新</a>",
				})
			},

			methods:{
				empty_mobile:function() {
					this.phone = ""
				},
				autoSetval1:function(){

					if ($('.checkVal1')[0].checked== true) {
						this.emailSubscribe="1";
					}
					else {
						this.emailSubscribe="0";
					}

				},
				//同意协议
				checkAgr:function() {
					if (this.agree_policy != "1") {
						this.agrErr.show = "block";
						this.check_agr = false
					} else {
						this.agrErr.show = "none";
						this.check_agr = true;
					}
				},
				removeLocalData: function(){
					if(localStorage.getItem('thirdParty')){
						localStorage.removeItem('thirdParty');
					}
					if( localStorage.getItem("pwdSafeToken")){
						localStorage.removeItem("pwdSafeToken");
					}
					if(localStorage.getItem("mobile")){
						localStorage.removeItem("mobile");
					}
					if(localStorage.getItem("thirdPartyData")){
						localStorage.removeItem("thirdPartyData");
					}
				},
				thirdPartyRegister: function(){
					var this11 = this;
					if(this11.agree_policy != "1"){
						this11.agrErr.show = "block";
					}
					else if (this11.phone == "") {
						this11.mobileErr.show = "block";
						this11.mobileErr.msg = "手机号不能为空";
						this11.check_mobile = false;
					}else if (this11.valid1OtpCode == "") {
						this11.otpErr.show = "block";
						this11.otpErr.msg = "请输入验证码";
						this11.check_otp = false;
					}
					else {
						this11.otpErr.show = "none";
						this11.mobileErr.show = "none";
						this11.register_Data.emailSubscribe = this11.emailSubscribe;
						this11.register_Data.otpCode = this11.valid1OtpCode;
						this11.register_Data.thirdParty = localStorage.getItem("thirdParty");
						this11.register_Data.thirdPartyData = localStorage.getItem("thirdPartyData");
						axios({
							method: 'POST',
							url: '/eSolverOmniChannel/account/register.do',
							data: this11.register_Data

						}).then(function (response) {
							localStorage.removeItem('hcm_state') // 重置hcm_state
							if (response.status == "200" && response.data.resultMsg[0].code == "00") {
								localStorage.setItem("access_token", response.data.resultData.access_token);
								localStorage.setItem("userAlias", response.data.resultData.userAlias);
								localStorage.setItem('customerID',response.data.resultData.customerID);
								login2.removeLocalData();
								// hcm上报版本更新
								checkHcmVersion(false).then(() => {
									setHcmState(false).then(() => {
										location.href = location.origin + location.pathname;
									})
								})
							} else if (response.status == "200" && response.data.resultMsg[0].code == "96") {
								localStorage.setItem("pwdSafeToken", response.data.resultData.pwdSafeToken);
								localStorage.setItem("mobile", this11.phone); //Case 2.b call the same api by passing pwdSafeToken and pwd
								localStorage.setItem("emailSubscribe", this11.emailSubscribe);
								$('.valiz_noe').parent('.overlays').hide();
								$('.second_one').parent('.overlays').show();
							} else {

								this11.send_regErr.show = "block";
								this11.send_regErr.msg = response.data.resultMsg[0].message;
							}
						})
								.catch(function (err) {
									console.log("issue in 3rd party registartion");
									//this11.send_regErr.show = "block";
									//this11.send_regErr.msg = "issue in 3rd party registartion";
								});
					}
				},
				checkMobile1:function() {
					if (this.phone == "") {
						this.mobileErr.show = "block";
						this.mobileErr.msg = "手机号不能为空";
						this.check_mobile = false;
					}
					if (!this.regPhone.test(this.phone)) {
						this.mobileErr.show = "block";
						this.mobileErr.msg = "请输入正确的手机号码";
						this.check_mobile = false;

					} else {
						this.register_Data.mobile=this.phone;
						this.mobileErr.show = "none";
						this.mobileErr.msg = "";
						this.check_mobile = true;
					}

				},
				checkOtp:function() {
					if (this.valid1OtpCode == "") {
						this.otpErr.show = "block";
						this.otpErr.msg = "请输入验证码";
						this.check_otp = false
					} else {
						this.otpErr.show = "none";
						this.check_otp = true;
					}
				},
				send_otp1:function() {
					var this12=this;
					if (this12.check_mobile) {
						/*倒计时*/
						var countdown_i = 120;

						function countdown() {
							$('.span_countdown span').html(countdown_i)
							var ttt = setTimeout(function () {
								countdown_i--;
								if (countdown_i < 0) {
									$(".huoqu1").show();
									$(".huoqu2").hide();
									countdown_i = 120;
								} else {
									countdown();
								}
							}, 1000)
						}
						if (this12.phone == "") {
							this12.mobileErr.show = "block";
							this12.mobileErr.msg = "手机号码不能为空";

						} else if (localStorage.third_reg_session_id_header == undefined || localStorage.third_reg_sig_header == undefined || localStorage.third_reg_nc_token_header == undefined) {
							this12.otpErr.show = "block";
							//this.otpErr.msg = "请滑动上方再获取验证码";
							this12.otpErr.msg = "请拖动上方滑块再获取验证码";

						} else {
							$(".huoqu1").hide();
							$(".huoqu2").show();
							countdown();
							this12.otpErr.show = "none";

							var data = {
								"otpType": "USER_REGISTRATION",
								"mobile": this12.phone,
								"sessionId": localStorage.third_reg_session_id_header,
								"sig": localStorage.third_reg_sig_header,
								"token": localStorage.third_reg_nc_token_header,
								"appKey": "FFFF0N0000000000A10B",
								"scene": "nc_message",
							}
							this12.otp_data.mobile = this12.phone;

							axios({
								method: 'POST',
								url: _api.HybrisAPI.sendOTP(),
								data: data

							}).then(function(response) {

								this12.register_Data.otpCode=this12.valid1;

							}).catch(function(err) {

							});
						}



					}else{
						this.mobileErr.show = "block";
						this.mobileErr.msg = "手机号不能为空";
						this.check_mobile = false;
					}
				},
				formatingRUrl1:function() {
					var params = {};
					var parts = location.search.substring(1).split('&');

					for (var i = 0; i < parts.length; i++) {
						var nv = parts[i].split('=');
						if (!nv[0]) continue;
						params[nv[0]] = nv[1] || true;
					}

					var awe = params.code;
					var aeet = params.state;
					var remavabledata = "code=" + awe + "&state=" + aeet;
					var aplj = window.location.href;
					var ret = "";
					if (aplj.indexOf("code") - aplj.indexOf("?") == 1) {
						ret = "?" + remavabledata;
						ret = aplj.replace(ret, '');
					} else {
						ret = "&" + remavabledata;
						ret = aplj.replace(ret, '');
					}
					var redirectUrlD=location.origin + location.pathname;
					setTimeout(function () {
						window.location.href = redirectUrlD;
					}, 500);

				},
				cancleregistration: function(){
					$('.overlays').hide();
					login2.removeLocalData();
					login2.formatingRUrl1();
				},
				/*autoSetval:function(){

                    if ($('.checkVal')[0].checked== true) {
                        this.agree_policy="1";
                    }
                    else {
                        this.agree_policy="0";
                    }

                },*/
				needpwdlogin:function(){

					this.mobileErr.show = "none";
					$('.error_tip5').hide();
					this.registerwithPwd.emailSubscribe=localStorage.getItem("emailSubscribe");
					this.registerwithPwd.mobile=localStorage.getItem("mobile");
					this.registerwithPwd.thirdParty=localStorage.getItem("thirdParty");
					this.registerwithPwd.pwdSafeToken=localStorage.getItem("pwdSafeToken");
					this.registerwithPwd.thirdPartyData=localStorage.getItem("thirdPartyData");
					if(!this.pwdlogin == ""){
						this.registerwithPwd.pwd=encryptPadding(this.pwdlogin);
						axios({
							method:'POST',
							url: '/eSolverOmniChannel/account/register.do',
							data: this.registerwithPwd

						}).then(function(response){
							localStorage.removeItem('hcm_state') // 重置hcm_state
							if (response.status == "200" && response.data.resultMsg[0].code== "00"){
								localStorage.setItem("access_token",response.data.resultData.access_token);
								console.log("Successful login");
								localStorage.setItem('customerID',response.data.resultData.customerID);
								var redirectUrlD=location.origin + location.pathname;
								login2.removeLocalData();
								// hcm上报版本更新
								checkHcmVersion(false).then(() => {
									setHcmState(false).then(() => {
										window.location.href = redirectUrlD;
									})
								})
							}else{
								$('.error_tip5').show();
								$('.error_tip5').html(response.data.resultMsg[0].message);
							}
						})
								.catch(function(err){
									console.log("wrong with password-login");
									//alert()
								});
					}else{
						this.mobileErr.show = "block";
						this.mobileErr.msg = "密码字段不能为空"
					}
				},
				noneedpwd: function(){
					$('.error_tip5').hide();
					this.mobileErr.show = "none";
					this.withoutPwdlogin.emailSubscribe=localStorage.getItem("emailSubscribe");
					this.withoutPwdlogin.mobile=localStorage.getItem("mobile");
					this.withoutPwdlogin.pwdSafeToken=localStorage.getItem("pwdSafeToken");
					this.withoutPwdlogin.thirdPartyData=localStorage.getItem("thirdPartyData");
					this.withoutPwdlogin.thirdParty=localStorage.getItem("thirdParty");
					axios({
						method:'POST',
						url: '/eSolverOmniChannel/account/register.do',
						data: this.withoutPwdlogin

					}).then(function(response){
						localStorage.removeItem('hcm_state') // 重置hcm_state
						if (response.status == "200" && response.data.resultMsg[0].code== "00"){
							localStorage.setItem("access_token",response.data.resultData.access_token);
							localStorage.setItem('customerID',response.data.resultData.customerID);
							$('.second_one').parent('.overlays').hide();
							console.log("Successful login");
							login2.removeLocalData();
							// hcm上报版本更新
							checkHcmVersion(false).then(() => {
								setHcmState(false).then(() => {
									window.location.href = location.origin + location.pathname;
								})
							})
						}else{
							$('.error_tip5').show();
							$('.error_tip5').html(response.data.resultMsg[0].message);
						}
					})
							.catch(function(err){
								console.log("something went wrong with without password login");
							});
				},
				noneedlogin: function(){
					$('.overlays').hide();
					login2.removeLocalData();
					login2.formatingRUrl1();
				}

			}

		})
	} catch (e) {
		console.log('err', e)
	}

</script>
    

    
