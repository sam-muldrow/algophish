<script>
 
    $('.hqyz').click(function(){
        if(sessonid == ""  || sessonid== undefined){
            $(".nctips").show();
        }else{
            $(".huoqu1").hide();
            $(".huoqu2").show();
            countdown();
            $(".message_error").hide();



        }
    })



</script>



