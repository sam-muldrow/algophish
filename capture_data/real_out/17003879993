    <script>
        //è·åå¹¿åä½ä¿¡æ¯
        function showAdInfo(catalogid){
            // å¤æ­å¹¿åå¯¹è±¡å­å¨ï¼åæ¶æ¯å¤§äº0çæ°ç»ææ§è¡å¹¿åä»£ç 
            if(typeof(paiqijson)!="undefined" && paiqijson instanceof Array && paiqijson.length > 0) {
                getAdInfo("http://www.315che.com/",catalogid);
            }
        }

        function getAdInfo(url,catalogid){
            var map = paiqijson[0];
            for(var key in map){
                if(url==key){
                    var list = map[key];
                    for(var i=0;i<list.length;i++){
                        if(list[i].city==catalogid){
                            showAd(list[i]);
                            if(list[i].hasOwnProperty('exposure')){
                                $("#adimgb"+i).attr("src",list[i].exposure)
                            }

                        }else if(list[i].city=="0"){
                            var flag = true;
                            for(var j=0;j<list.length;j++){
                                if(list[i].adtype==list[j].adtype&&list[i].adplace==list[j].adplace&&list[i].city==catalogid){
                                    flag = false;
                                    break;
                                }
                            }
                            if(flag){
                                showAd(list[i]);
                                if(list[i].hasOwnProperty('exposure')){
                                    $("#adimgb"+i).attr("src",list[i].exposure)
                                }
                            }
                        }
                    }
                    break;
                }
            }
        }

        function showAd(list){
            // åå±å¹¿å
            if(list.adtype!=4) {
                var html = '';
                if (list.adimg.substring(list.adimg.length - 3) == "swf") {
                    html += '<embed height="' + list.adheight + '" width="' + list.adwidth + '" src="' + list.adimg + '" wmode="transparent" type="application/x-shockwave-flash"></embed>';
                    html += '<a target="_blank" href="' + list.adurl + '"><div style="position:relative; left:0; z-index:10; width:' + list.adwidth + 'px; height:' + list.adheight + 'px; margin-top:-' + list.adheight + 'px; cursor:pointer;background-image: url(https://www.315che.com/images/ad_bg.gif)"></div></a>';
                } else {
                    html += "<a target='_blank' href='" + list.adurl + "'><img width='" + list.adwidth + "' height='" + list.adheight + "' src='" + list.adimg + "'></a>";
                }
                // adtype:å¹¿åä½ç±»å(1éæ ,2å·¦åéæ ,5å³åéæ ,3æé®å¹¿å,4åå±å¹¿å,6æ¬æµ®å¹¿å)
                $("#showAd" + list.adtype + list.adplace).html(html).show();
                // ä¸çº§divè®¾ç½®æ¾ç¤ºï¼é»è®¤é¦é¡µæ¯ä¸æ¾ç¤ºç
                $("#showAd" + list.adtype + list.adplace).parent().show();
            }
        }

    </script>
