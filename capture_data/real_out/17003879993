        $(function() {

            if ($('.index-focus-box').length > 0) {
                var focusLen = $('.index-focus-slider li').length;
                // è®¾ç½®ç¦ç¹å¤§å¾åç¹å¯¼èªçå­çå·¦è¾¹è·
                $(".index-focus-nav").css({
                    'margin-left': '-' + $(".index-focus-nav").width() / 2 + 'px'
                });

                // éåè®¾ç½®ç¦ç¹å¤§å¾æ é¢çå­çå·¦è¾¹è·
                $(".index-focus-slider li").each(function(index, element) {
                    $(element).find('.title').css({
                        'margin-left': '-' + $(element).find('.title').width() / 2 + 'px'
                    })
                });

                // æå­å¯¼èª
                $(".index-focus-nav li").hover(function(){
                    $(this).addClass("active").siblings().removeClass("active");
                    $(".index-focus-slider li").removeClass("active").eq($(this).index()).addClass("active");
                    $(".index-focus-slider li").eq($(this).index()).find('.title').show();
                }, function() {
                    $(".index-focus-slider li").find('.title').hide();
                });

                // èªå¨åæ¢
                var autoPlayTimer = null,
                    $btnPrev = $('.index-focus-box .btn-prev'),
                    $btnNext = $('.index-focus-box .btn-next'),
                    autoPlay = function() {
                        var curInd = $('.index-focus-nav li[class=active]').index();
                        curInd ++;
                        if (curInd > (focusLen - 1)) {
                            curInd = 0;
                        }
                        $('.index-focus-nav li').removeClass('active').eq(curInd).addClass('active');
                        $('.index-focus-slider li').removeClass("active").eq(curInd).addClass("active");
                    },
                    prev = function() {
                        var curInd = $('.index-focus-nav li[class=active]').index();
                        curInd --;
                        if (curInd < 0) {
                            curInd = (focusLen - 1);
                        }
                        $('.index-focus-nav li').removeClass('active').eq(curInd).addClass('active');
                        $('.index-focus-slider li').removeClass("active").eq(curInd).addClass("active");
                    },
                    hoverControl = function(obj) {
                        obj.hover(function(){
                            // é¼ æ ç»è¿
                            clearInterval(autoPlayTimer);
                        }, function(){
                            // é¼ æ ç¦»å¼
                            autoPlayTimer = setInterval(function() {
                                autoPlay(); //å®æ¶è°ç¨æ¹æ³åæ¢
                            }, 3000);
                        }).trigger("mouseleave");
                    };

                hoverControl($(".index-focus-box"));

                // ä¸ä¸å¼ 
                $btnPrev.click(function() {
                    prev();
                });

                // ä¸ä¸å¼ 
                $btnNext.click(function() {
                    autoPlay();
                });
            // }

                // // æå­å¯¼èª
                // $(".index-focus-nav li").hover(function(){
                //     $(this).addClass("active").siblings().removeClass("active");
                //     $(".index-focus-slider li").removeClass("active").eq($(this).index()).addClass("active");
                // });

                // èªå¨åæ¢
                // var autoPlayTimer;
                // var autoPlay = function() {
