<script data-cfasync="true" src="https://cdn.noticiasaominuto.com/js/app.js?id=d279c6180cb19ca98329"></script>
<script data-cfasync="true" type="text/javascript" src="/js/meteorologia.js?id=8bee5921685e7a1fa10e"></script>
<script>
    function showSmartBanner() {
        if(getCookieValue("_smartbanner") == 1) return;
        $('#nm-smartbanner').html(`
            <a href="https://play.google.com/store/apps/details?id=com.noticiasaominuto.pt" target="_blank">
                <span id="pop-out-close" class="pop-out-close"></span>
                <img src="/img/app/v2/smart-banner-450x90.png" width="100%">
            </a>
        `);
        $('.rich-media-container').css('margin-top', '115px');
        $('#nm-smartbanner').on('click', function(e) {
            if(e.target.className == 'pop-out-close') {
                e.preventDefault();
            }
            $(this).slideUp();
            $('.rich-media-container').css('margin-top', '40px');
            setCookie("_smartbanner", 1, 1);
        })
    }

    function setCookie(cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + exdays * 24 * 60 * 60 * 1000);
        var expires = "expires=" + d.toUTCString();
        document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
    }

    function getCookieValue(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(";");
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == " ") {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    }
    window.addEventListener('beforeinstallprompt', (e) => {
        if(e.platforms && e.platforms[0] === 'play') showSmartBanner();
    });
</script>
