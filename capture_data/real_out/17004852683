	{		
		var submit_button = document.getElementById("submission");
		
		if (document.ibl.backlinkurl.value == "" || document.ibl.backlinkurl.value == "http://www." || document.ibl.backlinkurl.value == "https://www." || document.ibl.backlinkurl.value.includes("https://https://")  || document.ibl.backlinkurl.value.includes(".https://") )
