    display: flex;
    height: 24px;
    justify-content: center;
  }

  .notification-menu-button {
    width: 24px;
    background: none;
    border: 0;
    padding: 0;
  }

  .notification-menu-hidden {
    display: hidden;
  }

  .notification-menu {
    background-color: var(--rj-nexusblack-high-contrast);
    width: 80%;
    display: none;
    font-family: Montserrat;
    font-weight: 500;
    font-size: 13px;
    line-height: 18px;
    z-index: 1;
    opacity: 1;
  }

  .notification-menu-row {
    display: flex;
    padding: 5px 0;
  }

  .notification-menu-row:first-child,
  .notification-menu-row:nth-child(2) {
    padding-top: 10px;
  }

  .notification-menu-row:hover {
    background-color: var(--rj-white-05);
  }

  .notification-mark-as-read,
  .notification-mark-as-unread {
    display: none;
  }

  .notification-wrapper.notification-unread .notification-mark-as-read {
    display: flex;
  }

  .notification-wrapper.notification-read .notification-mark-as-unread {
    display: flex;
  }

  .notification-menu-icon-wrapper {
    padding: 10px 0;
    width: 15%;
  }

  .notification-menu-icon {
    margin-right: 4px;
    width: 30px;
  }

  .notification-menu-text {
    padding: 10px 0;
    background: none;
    border: 0;
  }

  .notification-read .notification {
    opacity: 0.5;
  }

  @media (min-width: 1024px) {

    .notification-category-generic-icon,
