			$(document).ready(function() {
				if(window.innerWidth < 1012) {
					$('.demo-billboard').remove()
				}

				if(window.innerWidth >= 1012 && window.innerWidth < 1460) {
					$('.demo-mrec').remove()
				}
			});
		</script>
