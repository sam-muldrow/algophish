<script>
	if ( areAdsBlocked() === true )
	{
		$('.ads').remove();
		document.write('<div class="i44904b2502"> <a class="i44904b2502-link" href="https://users.nexusmods.com/account/billing/premium"> <img class="i44904b2502-billboard" src="https://www.nexusmods.com/assets/images/premiumbanner/billboard.png" alt="Get your mods and collections faster. No ads. No waiting. Go premium now."> <img class="i44904b2502-mpu" src="https://www.nexusmods.com/assets/images/premiumbanner/mpu.png" alt="Get your mods and collections faster. No ads. No waiting. Go premium now."> </a> </div> <style> .i44904b2502 { height: 260px; padding: 5px 10px; width: 100%; display: flex; justify-content: center; } .i44904b2502-billboard { display: none; } .i44904b2502-mpu { height: 250px; width: 300px; } @media (min-width: 1024px) { .i44904b2502 { } .i44904b2502-mpu { display: none; } .i44904b2502-billboard { display: block; height: 250px; width: 970px; } } </style>');

		$('body').on('click', '.i44904b2502-link', function () {
			window.nexusDataLayer = window.nexusDataLayer || [];
			window.nexusDataLayer.push({
				event: 'premium_banner_click',
			});
		});
	}
	</script>
