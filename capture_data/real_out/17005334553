                $('#backgroundImageHandler').css('background-image', 'url('+$('#academicLinksContentWrapper').data('category-item-image')+')');

                function displayCategoryLinkContent(image,title,desc){
                    $('#academicLinksSection').css('background-image', 'url('+image+')');
                    $('#backgroundImageHandler').stop(true,true).animate({ opacity: 0},150, function () {
                        $(this).css('background-image', 'url('+image+')');
                        $(this).stop(true,true).fadeIn(150);
                        });
                        $('.academic-links-content-wrapper').stop(true,true).animate({ opacity: 0},150, function () {
                        $(this).html('<h4>'+title+'</h3>'+'<p>'+desc+'</p>');
                        $(this).stop(true,true).animate({ opacity: 1},150);
                    });
                }


                function displayChildLinkContent(child){
                    $('#academicLinksSection').css('background-image', 'url('+$(child).data('link-item-image')+')');
                        $('#backgroundImageHandler').stop(true,true).animate({ opacity: 0},150, function () {
                        $(this).css('background-image', 'url('+$(child).data('link-item-image')+')');
                        $(this).stop(true,true).fadeIn(150);
                    });
                    $('.academic-links-content-wrapper').stop(true,true).animate({ opacity: 0},150, function () {
                        $(this).html('<h4>'+$(child).data('link-item-title')+'</h4>'+'<p>'+$(child).data('link-item-desc')+'</p>');
                        $(this).stop(true,true).animate({ opacity: 1},150);
                    });
                }

                $('#linkCategoryChilds li').mouseenter(function(){
                    displayChildLinkContent(this);
                })
                $('#linkCategoryChilds').mouseleave(function(){
                    displayCategoryLinkContent('/images/www/fa/link/academics-link/2023/651d5076b20d7-70.jpg','پذیرش دانشجویی','');
                })
            
jQuery&&jQuery.pjax&&(jQuery.pjax.defaults.maxCacheLength=0);
if (jQuery('#major-list-widget').data('select2')) { jQuery('#major-list-widget').select2('destroy'); }
jQuery.when(jQuery('#major-list-widget').select2(select2_92eca192)).done(initS2Loading('major-list-widget','s2options_d6851687'));


    $('#major-list-widget').on('change', function(e) {
        var data = $('#major-list-widget option:selected').attr('data-value');
        var x = $('input[data-selector="' + data + '"]');
        var info = $('#infoName').val();
        var dataId = x.attr('data-id');
        var dataType = x.attr('data-type');
        if(info !== null){
            window.location.href = 'https://ut.ac.ir/fa/major' + '?' + dataType + '=' + dataId + '&MajorSearch[infoName]=' + info;
        } else{
            window.location.href = 'https://ut.ac.ir/fa/major' + '?' + dataType + '=' + dataId;
        }
     });

if (jQuery('#thesis-list-widget').data('select2')) { jQuery('#thesis-list-widget').select2('destroy'); }
jQuery.when(jQuery('#thesis-list-widget').select2(select2_92eca192)).done(initS2Loading('thesis-list-widget','s2options_d6851687'));


    $('#thesis-list-widget').on('change', function() {
        var data = $('#thesis-list-widget option:selected').attr('data-value');
        var x = $('input[data-selector="' + data + '"]');
        var info = $('#collegeInfoName').val();
        var dataId = x.attr('data-id');
        var dataType = x.attr('data-type');
        if (info !== null) {
            window.location.href = 'https://ut.ac.ir/fa/thesis' + '?' + dataType + '=' + dataId + '&ThesisSearch[title]=' + info;
        } else {
            window.location.href = 'https://ut.ac.ir/fa/thesis' + '?' + dataType + '=' + dataId;
        }
    });


