<script>
// scroll back to top
$('.back-to-top').click(function(){
$('html, body').animate({ scrollTop: 0 }, 'slow');
return false;
});
// display of back to top button
$(window).scroll(function () {
if ($(window).scrollTop() > 100) {
$('.back-to-top').show('slow');
} else {
$('.back-to-top').hide('slow');
}
});
</script>
