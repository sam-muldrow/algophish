<script>
					$(document).ready(function() {
						try {
							var todayOrdinal = new Date().getDay();
							$('.my-store-hours-content').find('div[data-ordinal="' + todayOrdinal + '"]').find('.store-days').text('TODAY');

							// make today's hours the first in the list
							var numDays = $('.my-store-hours-content').find('div.day-wrapper').filter('[data-ordinal]').length;
							for (var i = 0; i < numDays; i++) {
								if ($('.my-store-hours-content').find('div.day-wrapper:first').attr('data-ordinal') != todayOrdinal) {
									$('.my-store-hours-content').find('div.day-wrapper:first').appendTo('.my-store-hours-content');
								}
							}
						} catch(e) {
							console.error(e);
						}
					});
				</script>
