        <script>
            const globalMobileNav = document.querySelector(".js-mobile-global-nav");
            if (globalMobileNav) {
                runMobileNav();
            }

            function runMobileNav() {
                const menuBarMobile = globalMobileNav.querySelector(".js-menu-bar");
                const dropdownContainerMobile = globalMobileNav.querySelector(".js-dropdown-container");
                const toggleMobile = globalMobileNav.querySelector(".js-toggle");
                const menuMobile = globalMobileNav.querySelector(".js-menu");
                const toggleButtonsMobile = globalMobileNav.querySelectorAll('.js-toggle');
                const closeButtonsMobile = globalMobileNav.querySelectorAll('.js-close');
                const searchDropdownListMobile = globalMobileNav.querySelector(
                    '.js-dropdown-list[data-dropdown-id="dropdown-list-search-mobile"]');
                const searchArrowMobile = searchDropdownListMobile.querySelector(".js-input-arrow");

                document.addEventListener("DOMContentLoaded", function () {
                    const href = window.location.href;

                    let path = "";
                    switch (true) {
                        case (window.location.pathname.indexOf("partners") === 1 || window.location.pathname.indexOf("partners") === 4):
                            path = "partners";
                            break;
                        case href.includes("careers"):
                            path = "careers";
                            break;
                        case href.includes("investorcenter"):
                            path = "investorcenter";
                            break;
                        default:
                            path = "";
                    }

                    // Find the menu item for the current path and show its submenu
                    let menuItem = document.querySelector('.js-menu .js-item[data-path="' + path + '"]');
                    if (menuItem) {
                        let submenu = menuItem.querySelector('ul');
                        if (submenu) {
                            submenu.classList.remove('hidden');
                        }
                    }
                });

                toggleButtonsMobile.forEach(button => {
                    button.addEventListener('click', closeAllDropdownMobiles);
                });

                closeButtonsMobile.forEach(button => {
                    button.addEventListener('click', closeAllDropdownMobiles);
                });

                function closeAllDropdownMobiles() {
                    const dropdownListsMobile = globalMobileNav.querySelectorAll('.js-dropdown-list');
                    const dropdownBtnsMobile = globalMobileNav.querySelectorAll('.js-dropdown-btn');

                    // Close all open dropdowns
                    dropdownListsMobile.forEach(list => {
                        list.classList.add('hidden');
                    });

                    // Remove any active styles from the dropdown buttons
                    dropdownBtnsMobile.forEach(btn => {
                        btn.classList.remove('bg-slb-white');
                        btn.querySelector('span').classList.remove('before:text-slb-blue');
                        /* menuBarMobile.classList.remove("border-slb-white"); */
                    });
                }

                const onDropdownClickMobile = (e) => {
                    if (e.target.classList.contains("js-dropdown-btn")) {
                        const dropdownId = e.target.getAttribute("data-dropdown-id");
                        const relevantDropdownList = globalMobileNav.querySelector(
                            `.js-dropdown-list[data-dropdown-id="${dropdownId}"]`
                        );
                        const otherDropdownLists = globalMobileNav.querySelectorAll(
                            `.js-dropdown-list:not([data-dropdown-id="${dropdownId}"])`
                        );

                        const otherDropdownBtns = globalMobileNav.querySelectorAll(
                            `.js-dropdown-btn:not([data-dropdown-id="${dropdownId}"])`
                        );

                        otherDropdownLists.forEach((list) => {
                            list.classList.add("hidden");
                        });

                        otherDropdownBtns.forEach((btn) => {
                            btn.classList.remove("bg-slb-white");
                            btn.querySelector("span").classList.remove("before:text-slb-blue");
                        });

                        relevantDropdownList.classList.toggle("hidden");
                        e.target.classList.toggle("bg-slb-white");
                        e.target.querySelector("span").classList.toggle("before:text-slb-blue");
                        /*
                        if(e.target.classList.contains("bg-slb-white")){
                          menuBarMobile.classList.add("border-slb-white");
                        } else {
                          menuBarMobile.classList.remove("border-slb-white");
                        } */
                    } else {
                        if (!e.target.closest(".js-dropdown-list")) {
                            closeAllDropdowns();
                        }
                    }
                };

                const focusInputMobile = (e) => {
                    if (e.target.classList.contains("js-search-input-mobile")) {
                        searchArrowMobile.classList.toggle("before:text-slb-blue")
                    } else {
                        searchArrowMobile.classList.remove("before:text-slb-blue")
                    }
                }

                /* Toggle mobile menu */
                const toggleMobileMenu = (e) => {
                    if (!e.target.classList.contains("js-toggle")) return;
                    if (menuMobile.classList.contains("hidden")) {
                        showMenu();
                    } else {
                        hideMenu();
                    }
                }

                const showMenu = () => {
                    menuMobile.classList.remove("hidden");
                    dropdownContainerMobile.classList.add("hidden");
                    toggleMobile.innerHTML = `<span class="icon-font-close-white pointer-events-none"></span>`;
                    toggleMobile.classList.remove("border-l");
                }

                const hideMenu = () => {
                    dropdownContainerMobile.classList.remove("hidden");
                    menuMobile.classList.add("hidden");
                    toggleMobile.innerHTML = `<span class="icon-font-menu-hamburger-white pointer-events-none"></span>`;
                    toggleMobile.classList.add("border-l");
                    const openList = menuMobile.querySelectorAll(".js-side-menu");
                    if (menuMobile.querySelectorAll(".w-full").length > 0) {
                        openList.forEach(
                            (openMenu) => {
                                openMenu.classList.remove("w-full");
                                openMenu.classList.add("w-0");
                            }
                        )
                    }
                    document.body.classList.remove("overflow-hidden");
                }

                const sideNav = menuMobile.querySelectorAll(".js-sidenav")

                sideNav.forEach(menu => {
                    menu.addEventListener("click", openSideMenuMobile, false);
                });

                function openSideMenuMobile(e) {
                    if (!e.target.classList.contains("js-sidenav")) return;
                    const sideMenu = e.target.querySelector(".js-side-menu")
                    sideMenu.classList.add("w-full");
                    sideMenu.classList.remove("w-0");

                    document.body.classList.add("overflow-hidden");
                }

                function closeSideMenuMobile(e) {
                    if (!e.target.classList.contains('js-back-btn')) return;
                    e.target.parentElement.classList.remove('w-full');
                    e.target.parentElement.classList.add('w-0');

                    document.body.classList.remove("overflow-hidden");
                }

                function closeMobileMenuonLinkClick(e) {
                    if (!e.target.classList.contains("js-close-mobile-menu-link")) return;
                    hideMenu();
                }

                /* Event Listeners */
                document.addEventListener("click", closeSideMenuMobile, false);
                toggleMobile.addEventListener("click", toggleMobileMenu, false);
                document.addEventListener("click", focusInputMobile, false);
                document.addEventListener("click", closeMobileMenuonLinkClick, false);
            }
        </script>
        <script>
            if (document.querySelector("div#coveo-search-popular-terms-mobile")) {
                const searchArrowImg = document.getElementById('_searchArrowMobile');
                function doSearch(searchTerm) {
                    var inputField = document.getElementById('_searchFieldMobile');
                    var searchUri = (inputField && inputField.getAttribute('data-searchUri')) ? inputField.getAttribute('data-searchUri') : "/search";

                    if (searchTerm.length > 0) {
                        window.location = searchUri + "#q=" + searchTerm;
                    }
                }

                if (searchArrowImg) {
                    searchArrowImg.onclick = function (e) {
                        var inputField = document.getElementById('_searchFieldMobile')
                        var inputTextValue = inputField.value;
                        doSearch(inputTextValue);
                    }
                }


                function keyup(e) {
                    if (e.srcElement.id === "_searchFieldMobile") {
                        var inputTextValue = e.target.value;
                        if (inputTextValue.length > 0) {
                            searchArrowImg.classList.remove("icon-font-arrow-right-grey-4");
                            searchArrowImg.classList.add("icon-font-arrow-right-blue");
                            if (e.keyCode == 13) {
                                doSearch(inputTextValue);
                            }
                        }
                        else {
                            searchArrowImg.classList.remove("icon-font-arrow-right-blue");
                            searchArrowImg.classList.add("icon-font-arrow-right-grey-4");
                        }
                    }
                }

                function populatePopularSearchMobile() {
                    var popularsearchurl = $('#_searchFieldMobile').attr('sitename') == "onesubsea" ? "https://platform.cloud.coveo.com/rest/search/v2/querySuggest?pipeline=SubSeaJV&count=10" : "https://platform.cloud.coveo.com/rest/search/v2/querySuggest?pipeline=default&tab=slbcom&count=10"
                    var origin = window.location.origin;
                    var url = origin + "/coveo/rest/token";
                    fetch(url, {
                        method: 'GET'
                    })
                        .then(function (response) {
                            if (!response.ok) {
                                throw Error(response.statusText);
                            }
                            return response.json();
                        })
                        .then(function (data) {
                            if (data != undefined) {
                                fetch(popularsearchurl, {
                                    method: 'GET',
                                    headers: {
                                        'Authorization': 'Bearer ' + data.token,
                                        'Content-Type': 'application/json;charset=utf-8'
                                    }
                                })
                                    .then(function (response) {
                                        if (!response.ok) {
                                            throw Error(response.statusText);
                                        }
                                        return response.json();
                                    })
                                    .then(function (data) {
                                        if (data != undefined && data.completions.length > 0) {
                                            var list = "";
                                            data.completions.forEach(function (item) {
                                                list += '<li><a href="/search#q=' + item.expression + '">' + item.expression + '</a></li>'
                                            });

                                            var div = document.getElementById('coveo-search-popular-terms-list-mobile');
                                            div.innerHTML += list;
                                        }
                                    })
                                    .catch(error => console.log(error));
                            }
                        })
                        .catch(error => console.log(error));
                }
                window.onkeyup = keyup;
                window.addEventListener ? window.addEventListener("load", populatePopularSearchMobile, false) : window.attachEvent && window.attachEvent("onload", populatePopularSearchMobile);
            }
        </script>
        <!-- End Mobile Section -->
