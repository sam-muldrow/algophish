    <script>
        //renderTitle: () => {
        function renderTitle() {
            const textContainer = document
                .querySelector(".video-banner-content-text");
            const titleText = textContainer.innerHTML;
            const text = titleText.trim();
            const words = text.split(' ');
            const length = words.length;

            textContainer.replaceChildren();

            words.forEach((word, index) => {
                if (word) {
                    const container = document.createElement('div');
                    container.classList.add('relative', 'overflow-hidden');
                    container.style.display = 'inline-flex';

                    const textContent = document.createElement('span');
                    textContent.classList.add('video-banner-text-content', 'block');

                    if (index !== length - 1) {
                        textContent.innerHTML = `${word}&nbsp;`;
                    } else {
                        textContent.innerHTML = word;
                    }

                    container.appendChild(textContent);
                    textContainer.appendChild(container);
                }
            });
            textContainer.classList.remove('hidden');
        }
        window.addEventListener ? window.addEventListener("load", renderTitle, false) : window.attachEvent && window.attachEvent("onload", renderTitle);
    </script>

