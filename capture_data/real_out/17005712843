                <!-- Link List -->
                    <ul class="list-none my-2.5">
                        <!-- List Link -->
                                    <li class="mb-9">
                                        <a class="group" href="https://www.slb.com/driving-energy-innovation/decarbonizing-industry">
                                            <h3 class="font-medium text-slb-black text-[16px] leading-5.5 mb-2.5 pointer-events-none group-hover:text-slb-blue group-hover:underline group-hover:underline-offset-4">
                                                Decarbonizing Industry
                                            </h3>
                                        </a>
                                        <h4 class="text-[14px] leading-5.5 font-extralight text-slb-grey-5">
                                            Working together to abate emissions.
                                        </h4>
                                    </li>
                                    <li class="mb-9">
                                        <a class="group" href="https://www.slb.com/driving-energy-innovation/innovating-in-oil-and-gas">
                                            <h3 class="font-medium text-slb-black text-[16px] leading-5.5 mb-2.5 pointer-events-none group-hover:text-slb-blue group-hover:underline group-hover:underline-offset-4">
                                                Innovating in Oil and Gas
                                            </h3>
                                        </a>
                                        <h4 class="text-[14px] leading-5.5 font-extralight text-slb-grey-5">
                                            Improving performance in the oil and gas industry.
                                        </h4>
                                    </li>
