	<script type="text/javascript">
		$(document).ready(function(){
            // Don't made this with display:none in the code - seo needs this content and will
            // not interpret display:none. We should hide it when dom is ready.
            $('#infobox .infoContentBox .more').hide();

			$('#infobox #infoInfoOpenClose').click(function(){

				// toggle ...
				if($('#infobox .less').css('display') == 'none'){
					$('#infobox .less').css('display', 'inline');
				} else{
					$('#infobox .less').css('display', 'none');
				}

				if ($('#infobox #infoContent').hasClass('infoContentBox')){
					$('#infobox #infoContent').toggleClass('infoContentBoxOpened infoContentBox');
				}

				// toggle link class
				$(this).toggleClass('down up');

				$(this).hasClass('down') ? $(this).html("Mehr lesen...").text() : $(this).html("Schließen").text();
				// toggle content
				$('#infobox #infoContent div.more').slideToggle('500', function(){
					if ($('#infobox #infoContent').hasClass('infoContentBox')){
						$('#infobox #infoContent').toggleClass('infoContentBox infoContentBoxOpened');
					}
				});

				$('html,body').animate({scrollTop: $(this).offset().top }, 400);

				return null;
			});
		});
	</script>

