function adBlockNotDetected() {
jQuery('#block-adb-enabled').hide();
jQuery('#block-adb-not-enabled').show();
}

if(typeof blockAdBlock === 'undefined') {
adBlockDetected();
} else {
blockAdBlock.setOption({ debug: true });
blockAdBlock.onDetected(adBlockDetected).onNotDetected(adBlockNotDetected);
}
</script>

<h5 class="bg-success" id="block-adb-not-enabled" style="display: none;">
  </h5>
<h5 class="bg-danger" id="block-adb-enabled" style="display: none;">
  <div id="szMfdathTz" style="display:block !important;position:fixed;bottom:0;margin-top:10px;width:100%;background:#c44;color:#fff;font-size:25px;z-index:99999"><div style="display:block !important;padding:20px;margin:0 auto;text-align:center;max-width:980px;overflow:hidden"><div style="float:left;display:block !important;">&#x5C4F;&#x853D;&#x5E7F;&#x544A;&#x5C06;&#x4F7F;&#x6211;&#x4EEC;&#x6CA1;&#x6709;&#x8D44;&#x91D1;&#x4E3A;&#x4F60;&#x63D0;&#x4F9B;&#x514D;&#x8D39;&#x7684;&#x670D;&#x52A1;&#x3002;&#x8BF7;&#x60A8;&#x5173;&#x95ED;&#x5E7F;&#x544A;&#x5C4F;&#x853D;&#x7EC4;&#x4EF6;&#x3002;&#x5E7F;&#x544A;&#x662F;&#x6211;&#x4EEC;&#x552F;&#x4E00;&#x7684;&#x6536;&#x5165;&#x6765;&#x6E90;&#x3002;&#x8C22;&#x8C22;&#x60A8;&#x70B9;&#x51FB;&#x5E7F;&#x544A;&#x652F;&#x6301;&#x6211;&#x4EEC;&#x53D1;&#x5C55;&#x3002;</div><div style="float:right;cursor:pointer;font-weight:700;display:block !important;" id="GShtJTYejp"> </div></div></div></h5>


</html>
