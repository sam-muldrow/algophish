<link href="/css/wide/logout_howto.css" media="screen" rel="stylesheet" type="text/css">
<script>
  $(function(){
    let now   = new Date(),
        year  = now.getFullYear();
        co_year = year - 2002;
    $('.year').text(''+year+'');
    $('#year_co').text(''+co_year+'');
    function animeON(){
      $('.top_howto_title, .top_howto_img_wrap').addClass('howto_animation');
    }
    setTimeout(animeON, 900);
    $('#top_howto_exchange_slide').not('.slick-initialized').slick({
      slidesToShow: 7,
      slidesToScroll: 1,
      autoplay: true,
      autoplaySpeed: 0,
      speed: 3000,
      arrows: false,
      cssEase: 'linear'
    });
    function animeON2(){
      $('#howto_feature1').addClass('howto_animation');
    }
    function animeON3(){
      $('#howto_feature2').addClass('howto_animation');
    }
    function animeON4(){
      $('#howto_feature3').addClass('howto_animation');
    }
    function animeON5(){
      $('#howto_feature4').addClass('howto_animation');
    }
    function animeON6(){
      $('#howto_feature5').addClass('howto_animation');
    }
    let Pos1 = $('#howto_feature1').offset().top;
    $(window).scroll(function(){
      let winScrollTop = $(this).scrollTop(),
          winHeight = window.outerHeight;
      if (winScrollTop >= Pos1 - 500) {
        setTimeout(animeON2, 0);
        setTimeout(animeON3, 200);
        setTimeout(animeON4, 400);
        setTimeout(animeON5, 600);
        setTimeout(animeON6, 800);
      }else if(winHeight - 150 >= Pos1){
        setTimeout(animeON2, 0);
        setTimeout(animeON3, 200);
        setTimeout(animeON4, 400);
        setTimeout(animeON5, 600);
        setTimeout(animeON6, 800);
      }
    });
  });
</script>
