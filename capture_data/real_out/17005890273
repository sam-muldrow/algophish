<script>
  const CATEGORY_REGISTER = '登録、申込みで貯める';
  const CATEGORY_TRIAL = 'お試し・サンプル';
  const CATEGORY_SHOPPING = 'お買い物で貯める';
  const CATEGORY_SMP_API = 'スマホAPI';
  $(window).on('load',function(){
    const promise = Ranking1();
    promise.done(function(){
      const RankingItemBox = document.querySelectorAll('.CommonRankingBox');
      RankingItemBox.forEach(function(target) {
        const targetBox = target.querySelectorAll('.CommonRankingBox__itemInner');
        targetBox.forEach(function(target, i) {
          const num = i+1;
          const targetBox = target.querySelector('.CommonRankingBox__rankNum')
          targetBox.textContent = ""+num+"";
          targetBox.classList.add('CommonRankingBox__rankNum--'+num+'');

        });
      });
      $('.CommonRankingBox').each(function(){
        $(this).children('li:not(:lt(10))').remove();
      });
    });


    const RankingNavBtn = document.querySelectorAll(".Ranking__navSlider .CommonPageMenu__Btn");
    RankingNavBtn.forEach(function(target){
      target.addEventListener('click',function(){
        const promise2 = Ranking2();
        promise2.done(function(){
          const RankingItemBox = document.querySelectorAll('.CommonRankingBox');
          RankingItemBox.forEach(function(target) {
            const targetBox = target.querySelectorAll('.CommonRankingBox__itemInner');
            targetBox.forEach(function(target, i) {
              const num = i+1;
              const targetBox = target.querySelector('.CommonRankingBox__rankNum')
              targetBox.textContent = ""+num+"";
              targetBox.classList.add('CommonRankingBox__rankNum--'+num+'');

            });
          });
          $('.CommonRankingBox').each(function(){
            $(this).children('li:not(:lt(10))').remove();
          });
          const RankingLoading = document.querySelectorAll('.RankingLoading');
          RankingLoading.forEach(function(target){
            target.style="display:none;";
          });
        });
      },{once:true});
    });


    function Ranking1(){
      let deferred1 = new $.Deferred();
      let dataAll = {logreco: {response_number: 15 , method_type:2, spot_name: 'SPTOP_ranking'}},
      dataService = {logreco: {response_number: 15 , category1:CATEGORY_REGISTER ,  method_type:2 , spot_name: 'SPTOP_ranking'}},
      dataShopping = {logreco: {response_number: 15 , category1:CATEGORY_SHOPPING, method_type:2, spot_name: 'SPTOP_ranking'}};
      $.get('/logreco/ranking', dataAll, function(html){
        $('#RankingAll').html(html);
        $.get('/logreco/ranking', dataService, function(html){
          $('#RankingService').html(html);
          $.get('/logreco/ranking', dataShopping, function(html){
            $('#RankingShopping').html(html);
            deferred1.resolve();
          });
        });
      });
      return deferred1.promise();
    };

    function Ranking2(){
      let deferred2 = new $.Deferred();
      let dataCreditcard = {logreco: {response_number: 15 , category1:CATEGORY_REGISTER , category2:'クレジットカード',  method_type:2 , spot_name: 'SPTOP_ranking'}},
      dataApply = {logreco: {response_number: 15 , category1:CATEGORY_REGISTER,category2:'会員登録・資料請求', method_type:2, spot_name: 'SPTOP_ranking'}},
      dataBank = {logreco: {response_number: 15 ,  category1:CATEGORY_REGISTER ,category2:'FX・銀行・証券',  method_type:2 , spot_name: 'SPTOP_ranking'}},
      dataBeauty = {logreco: {response_number: 15 ,category1:CATEGORY_REGISTER ,category2:'美容・エステ・出会い',  method_type:2 , spot_name: 'SPTOP_ranking'}},
      dataTry = {logreco: {response_number: 15 , category1:CATEGORY_TRIAL, method_type:2 , spot_name: 'SPTOP_ranking'}},
      dataTravel = {logreco: {response_number: 15 ,category2:'旅行,旅行・サービス', method_type:2 , spot_name: 'SPTOP_ranking'}};
      $.get('/logreco/ranking', dataCreditcard, function(html){
        $('#RankingCard').html(html);
        $.get('/logreco/ranking', dataApply, function(html){
          $('#RankingRegister').html(html);
          $.get('/logreco/ranking',dataBank, function(html){
            $('#RankingOpening').html(html);
            $.get('/logreco/ranking', dataBeauty, function(html){
              $('#RankingBeauty').html(html);
              $.get('/logreco/ranking', dataTry, function(html){
                $('#RankingTrial').html(html);
                $.get('/logreco/ranking', dataTravel, function(html){
                  $('#RankingTravel').html(html);
                  deferred2.resolve();
                });
              });
            });
          });
        });
      });
      return deferred2.promise();
    };
  });
</script>




