<script>

		$(document).ready(function() {
			var style = 'dark';
			$('.uix_styleToggle').click(function() {
				themehouse.styleSwitch.switchStyle(style);
				if (style == 'light') {
					style = 'dark'
				} else {
					style = 'light';
				}
			});
		});
	
