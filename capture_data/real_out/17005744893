                .addSize([783, 0], [[300, 250],[336, 280],"fluid"])
                .addSize([320, 0], [[300, 250],"fluid"])
                .addSize([0, 0], [])
                .build();

                var mobileRectangleSizes = googletag.sizeMapping()
                .addSize([1003, 0], [])
                .addSize([783, 0], [])
                .addSize([320, 0], [[300, 250],"fluid"])
                .addSize([0, 0], [])
                .build();

