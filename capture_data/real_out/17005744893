                slotTopLeaderboard = googletag.defineSlot('/21717805079/Portaldodog.com.br-ROS-Top-Leaderboard', [320,50], 'div-gpt-ad-top-leaderboard')
                .defineSizeMapping(billboardSizes)
                .addService(googletag.pubads());
                
                slotTopLeaderboard2 = googletag.defineSlot('/21717805079/Portaldodog.com.br-ROS-Top-Leaderboard', [320,50], 'div-gpt-ad-top-leaderboard-2')
                .defineSizeMapping(billboardSizes2)
                .addService(googletag.pubads());
                
