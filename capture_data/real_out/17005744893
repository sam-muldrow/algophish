    const myDiv = document.querySelector('.code-block.code-block-8');
    const stickyPosition = myDiv.offsetTop;

    function stickDiv() {
        if (window.innerWidth < activationWidth && window.pageYOffset > stickyPosition) {
            myDiv.classList.add('sticky-top-ad');
