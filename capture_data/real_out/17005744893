            var slotTopLeaderboard2;
            var topSidebar;
            var middleSidebar;
            var bottomSidebar;
            var mobileAd;
            var stickyFooter;

            var SECONDS_TO_WAIT_AFTER_VIEWABILITY = 10; //sets the refresh interval in seconds
            
            googletag.cmd.push(function(){

               //Setting up responsible ads via size mapping
                var billboardSizes = googletag.sizeMapping()
                .addSize([1003, 0], [[970, 250],[970,90],[728, 90],"fluid"])
                .addSize([783, 0], [[728, 90],"fluid"])
                .addSize([320, 0], [[320, 50],[320, 100],"fluid"])
                .addSize([0, 0], [])
