<script type="rocketlazyloadscript">
    document.addEventListener('DOMContentLoaded', function() {
        const storiesItems = document.querySelectorAll('.webstories__item');
        const storyIframe = document.querySelector('.webstories__iframe');
        const storyModal = document.querySelector('.webstories__modal');
        const storyCloseButton = document.querySelector('.webstories__close-button');

        storiesItems.forEach(item => {
            item.addEventListener('click',function(e) {
                e.preventDefault();
                storyIframe.setAttribute('src', e.currentTarget.attributes.href.value);
                storyModal.classList.add('webstories__modal--open');
            });
        });

        storyCloseButton.addEventListener('click', function(e) {
            e.preventDefault();
            storyModal.classList.remove('webstories__modal--open');
            storyIframe.setAttribute('src','');
        });
    })
</script>
<link rel="preload" as="image" href="/cachorros/wp-content/themes/pdd2020/imagens/loading.gif" />
<div class="webstories__modal">
<div class="webstories__close-button">X</div>
<iframe src frameborder="0" class="webstories__iframe" id="webstory-iframe"></iframe>
