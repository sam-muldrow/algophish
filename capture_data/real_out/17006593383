<script type="text/javascript">
	var facebookAuthClient = 'FACEBOOK';
	var googleAuthClient = 'GOOGLE';
	var facebookAppId = '662882871444142';
	var googleAppId = '1052383724492-q5g7i9aqamapqumpdn936e219013roqa.apps.googleusercontent.com';

	var url= '//in.via.com/accounts/service';

	var user = 'null';
	var roleB2c = '0';
	var loginAction = 'VACDL';
	var forgetPwdAction = 'FPA'
	var updatePwdLoginAction = 'UPAL';

	// Load the SDK asynchronously
	$( document ).ready(function() {

		var googleDiv = document.querySelector('#loginGoogle');
		google.accounts.id.initialize({
		client_id: googleAppId,
		cookiepolicy: 'single_host_origin',
		scope: 'profile email',
		callback: function(response){
			if(response!=null && response.credential!= null){
				var clientRequestParameters = [];
				var clientRequestParameters = {
					'accessToken' :  response.credential,
					'authClient' : 'GOOGLE',
					'type': 'V2',
					'role' : '0'
			};
			new LOGIN_UTIL().loginToVia(clientRequestParameters);
			}
		},
	});
	google.accounts.id.renderButton(googleDiv, {
		logo_alignment: 'left',
		theme: 'outline',
		text: "continue_with",
		size: 'large',
	});



	  (function(d, s, id) {
	    var js, fjs = d.getElementsByTagName(s)[0];
	    if (d.getElementById(id)) return;
	    js = d.createElement(s); js.id = id;
	    js.src = "//connect.facebook.net/en_US/sdk.js";
	    fjs.parentNode.insertBefore(js, fjs);
  		}(document, 'script', 'facebook-jssdk'));
	});
</script>


<!-- olark -->

	



<script type="text/javascript">
window.addEventListener('error', function(err) {
  ga('send', 'exception', {
    'exDescription': err.message,
    'exFatal': false
  });
});
// Track AJAX errors (jQuery API)
$(document).ajaxError(function(err, request, settings) {
  ga('send', 'exception', {
    'exDescription': err.message,
    'exFatal': false
  });
});
</script>



	


 
