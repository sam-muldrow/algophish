  $(document).ready(function() {
    $(".c__yumpu-categories--back").on('click', resetChips)
    var words = setupWords(categoryData);
    renderChips(words);
    if ($("#yumpu-navbar").hasClass("yp-navbar-fixed")) {
      scrollToBanner();
    }
  });

  function setupWords(category) {
    var result = [];

    for (const [key, value] of Object.entries(category)) {
      result.push({
        text: value.name,
        link: getLink(key, value),
        clickHandler: function() {
          handleChipClick(key)
        }
      });
    }

    result.sort((a, b) => {
      const nameA = a.text.toLowerCase();
      const nameB = b.text.toLowerCase();
      if (nameA < nameB) {
        return -1;
      } else if (nameA > nameB) {
        return 1;
      }
      return 0;
    });

    return result;
  }

  function renderChips(words) {
    $("#categories-chips__list").empty();

    words.forEach(function(word) {
      var chip = $('<a/>', {
        class: "categories-chips__chip",
        href: word.link,
        html: `<span>${word.text}</span>`,
        click: word.clickHandler
      });
      $("#categories-chips__list").append(chip)
    })
    $('#categories-chips').animate({
      opacity: 1,
    }, 300)
  }

  function getWeight(child) {
    var total = 0;
    if (!child.children) return child.weight;
    for (const [key, value] of Object.entries(child.children)) {
      if (value.children) {
        total += getWeight(value);
      } else {
        total += value.weight
      }
    }
    return total;
  }

  function getLink(id, category) {
    if (category.children) {
      return null;
    }
    return 'news' + category.link + '?utm_source=yn&utm_medium=website&utm_campaign=chips&utm_term=' + id;
  }

  function handleChipClick(categoryID) {
    if (!categoryData[categoryID]?.children) {
      return;
    }

    $(".c__yumpu-categories--maintext").hide();
    $(".c__yumpu-categories--categorytext").fadeIn();
    $(".c__yumpu-categories--back").fadeIn();
    var new_words = setupWords(categoryData[categoryID].children);
    updateChips(new_words);
  }

  function resetChips() {
    $(".c__yumpu-categories--categorytext").hide();
    $(".c__yumpu-categories--back").hide();
    $(".c__yumpu-categories--maintext").fadeIn();
    var new_words = setupWords(categoryData);
    updateChips(new_words)
  }

  function updateChips(content) {
    $('#categories-chips').css({
      opacity: 0,
    })
    renderChips(content);
  }

  function scrollToBanner() {
    $('html, body').animate({
      scrollTop: $(".c__banner").offset().top
    }, 0);

    setTimeout(() => {
      $(document).on("scroll", function() {
        $("#yumpu-navbar").removeClass("yp-navbar-fixed")
        $(document).off("scroll")
      })
    }, 1000)
  }
</script></div>
