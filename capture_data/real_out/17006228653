(function($refs){
const accordionSections = $refs['accordion'].querySelectorAll(".c__accordion--section");
accordionSections[0].classList.add("active");
const firstText = accordionSections[0].querySelector(".c__accordion--text");
firstText.style.height = firstText.scrollHeight + "px";

accordionSections.forEach(function(accordionSection) {
    accordionSection.addEventListener("click", function() {
        accordionSections.forEach(accordion => {
            accordion.classList.remove("active");
            accordion.querySelector(".c__accordion--text").style.height = "0";
        });
        this.classList.toggle("active");
        const text = this.querySelector(".c__accordion--text");
        text.style.height = text.scrollHeight + "px";
    });
})
})((window.vlpbtsFindRefs||function(){return{}})('u99b8589e17c5be3f'));
