    <script type="text/javascript">
        if (CurrentUser.UserId) {
            $.getJSON("//www.fanhuan.com/ajax/messageinfo?callback=?", function (data) {
                if (data.rt == 1) {
                    if (data.JinBi != 0) {
                        if ($.cookie('GoldOverdue') != data.JinBi || !$.cookie('GoldOverdue')) {
                            $('.icon-notice').show();
                        }                        
                    }
                }
                $('#nav li').eq(1).click(function(){
                    if ($('.icon-notice').css('display') == 'block') {
                        $.cookie('GoldOverdue', data.JinBi, { path: "/", expires: 365, domain: "fanhuan.com" })
                    }
                })
            });
        }

        $("#nav_notice").mouseover(function () {
            $(this).css("background", "#1280dd")
        }).mouseleave(function () {
            $(this).css("background", "none")
            $("#sign-in1").css("color", "#fff");
            $(".signwi").css({ 'display': 'block' });
            $(".signwi1").css({ 'display': 'none' });
        })
    </script>
    
