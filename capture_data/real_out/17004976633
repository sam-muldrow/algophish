<script id="loginCheckScript"></script>
<script>
var menuData=false;
var targetUrl="/";
if(targetUrl.replace(/\/$/,"")!=""){
	targetUrl=targetUrl.replace("http://","https://");
}else{
	targetUrl="";		
}
function getMenuData(){
	
	$.ajax({
			type: "GET",
			url: "//www.seoul.co.kr/news/getMenuListAjax.php?type=left_menu",
			dataType: "jsonp",
			success:function(result){
				if(result!=""){
					var tmpHtml="";
					for(var i=0;i<result.content.length;i++){
						if(result.content[i]['href'].search("http")<0){
								result.content[i]['href']=targetUrl+result.content[i]['href'];														
						}

						tmpHtml+="<li id='subMenu_"+i+"' class='sideMenuBar_ul_li'><a href='"+result.content[i]['href']+"' alt='"+result.content[i]['ko_name']+"'>"+result.content[i]['ko_name']+"</a>\n";
						tmpHtml+="<div class='sideMenuBarSub'><ul>\n";
						var tmpArr=result.content[i]["subMenu"]["item"];
						for(var k=0;k<tmpArr.length;k++){	
							if(tmpArr[k]["href"].search("http")<0){
								tmpArr[k]["href"]=targetUrl+tmpArr[k]["href"];	
							}
						    tmpHtml+="<li><a href='"+tmpArr[k]["href"]+"' alt='"+tmpArr[k]["ko_name"]+"'>"+tmpArr[k]["ko_name"]+"</a></li>\n";
						}
						tmpHtml+="</ul></div>\n";
						tmpHtml+="</li>\n";
						
					}

					$(".sideMenuBar_ul").append(tmpHtml);
					
					menuData=true;					
					eventBindMenu();			
				}			
			},
			error:function(){
			//
			}
	});	
}


$(window).on("resize load", function(event){
	if($(".sideMenuBar").height()<$(window).height()){
		$(".sideMenuBar").css({"height":$(window).height()+"px"});
	}
  
    $(".sideMenuBarSub_back").css({"height":$(".sideMenuBar").height()+"px"});

});

var sideMenuToggle=true;

function toggleSideMenu(){
    if(sideMenuToggle){
		if(!menuData){
			getMenuData();			
		}
	    $(".sideMenuWrap").animate({"left":"0px"},300);	  
    }else{
		$(".sideMenuBarSub").css({"display":"none"});		
	    $("#sideMenuBarSub_back").css({"display":"none"})
		$(".sideMenuWrap").animate({"left":"-200px"},300);	  
	}
	sideMenuToggle=!sideMenuToggle;
	
}


function eventBindMenu(){

	$(".sideMenuBar_ul_li").mouseenter(function() {
	
		$(".sideMenuBarSub").css({"display":"none"});		

		if($("#sideMenuBarSub_back").css("display")=="none"){
			$(".sideMenuWrap").css({"width":"340px"});	  
			$("#sideMenuBarSub_back").fadeIn(200);	
			$("#sideMenuBarSub_back").css({"display":"block"});;	
			$("#"+$(this).attr("id")+" > div").fadeIn(300);	
		}else{
			$("#"+$(this).attr("id")+" > div").css({"display":"block"});;	
		}

		//$("#"+$(this).attr("id")+" > div").fadeIn(300);	

		$(".sideMenuBar_ul_li").removeClass("ss");

		$(this).addClass("ss");

	});


	$("#sidMenuArea").mouseleave(function(){
		$(".sideMenuBarSub").css({"display":"none"});
		$("#sideMenuBarSub_back").fadeOut();
		$("#sideMenuBarSub_back").css({"display":"none"});
		$(".sideMenuWrap").css({"width":"200px"});	  
	});

	
}


</script>

