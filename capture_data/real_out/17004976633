	<script>
	// ë¸ëëë´ì¤ show/hidden
	
	function showBrandLayer(){
		if($("#brandLayerBottomWrap").length>0){
			$("#brandArea").clone().attr("id","").appendTo("#brandLayerBottomWrap");	
			$("#brandLayerBottomWrap").show();
		}
	}
	
	var brandLayerToggle=false;
	function toggleBrandLayer(){
		if(brandLayerToggle){
			$("#brandArea").slideUp(200);  
		}else{
			$("#brandArea").slideDown(200);
		}
		brandLayerToggle=!brandLayerToggle;
		
	}
	
	/*top flash news*/
	function getFlashNews(){
		
		if($("#topFlashNewsDiv").length>0){
		
			$.ajax({
				type: "GET",
				url: "//www.seoul.co.kr/news/getFlashNewsAjax.php",
				dataType: "jsonp",
				success:function(result){	
					var tmpHtml="";
					tmpHtml+="<ul>\n";
					for(var i=0;i<result.content.length;i++){
						tmpHtml+="<li class='lineclamp1' style='width:610px;'><a href='/news/newsView.php?id="+result.content[i]["id"]+"' title='ì¤ìê° ìë³´'>"+result.content[i]["title"]+"</a></li>\n";
					}
					tmpHtml+="</ul>";

					$("#slick_topflash").append(tmpHtml);

					$('#slick_topflash ul').slick({	
						arrows:false,   
						slidesToShow:1,
						slidesToScroll:1,   
						vertical:true,
						autoplay: true,
						pauseOnFocus:false,
						autoplaySpeed:4000,
					});	 
					
					
				},
				error:function(){
				   //alert("ë°ì´í° ìì ì ì¤í¨ íììµëë¤.");			
				}
			});	
		}
	}


	$(document).ready(function(){
		getFlashNews();
		showBrandLayer();

		try{
		    document.getElementById("loginCheckScript").src="//www.seoul.co.kr/member/ssl_loginCheckScript_n12.php?ct=";
		}catch(e){

		}
	});


	</script>
