	  <script>
	  var totalCnt=$("#slick_banner > li").length;
	  //var random_num = Math.floor(Math.random() * totalCnt);
	  var random_num = 0; 

	  var $ban_status = $('.bannerPaging');
	  var $ban_slickElement = $('#slick_banner');

	  $ban_slickElement.on('init reInit afterChange', function (event, slick, currentSlide, nextSlide){
	    var i = (currentSlide ? currentSlide : 0) + 1;
	    $ban_status.text(i + '/' + slick.slideCount);
	  });

	  $('#slick_banner').slick({	
         arrows:false,   
         slidesToShow:1,
         slidesToScroll:1,
         autoplay: true,
         pauseOnFocus:false,
		 initialSlide:random_num,
         autoplaySpeed:4000
	  });

	  $("#rollBan_prev").click(function(){
	    $('#slick_banner').slick("slickPrev");
	  });

	  $("#rollBan_next").click(function(){
	    $('#slick_banner').slick("slickNext");
