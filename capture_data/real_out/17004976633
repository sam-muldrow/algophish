  	  <script>
	  var itr_totalCnt=$("#slick_interactive > li").length;
	  var itr_random_num = Math.floor(Math.random() * itr_totalCnt);
	  itr_random_num=0;
	  
	  var $itr_ban_status = $('.interac_paging_num');
	  var $itr_ban_slickElement = $('#slick_interactive');

	  $itr_ban_slickElement.on('init reInit afterChange', function (event, slick, currentSlide, nextSlide){
	    var ii = (currentSlide ? currentSlide : (currentSlide != 0 ? itr_random_num : 0)) + 1;
	    $itr_ban_status.text(ii + '/' + slick.slideCount);
	  });

	  $('#slick_interactive').slick({	
         arrows:false,   
         slidesToShow:1,
         slidesToScroll:1,
         autoplay: false,
         pauseOnFocus:false,
		 initialSlide:itr_random_num,		 
         //autoplaySpeed:4000
	  });

	  $("#interactive_prev").click(function(){
	    $('#slick_interactive').slick("slickPrev");
	  });

	  $("#interactive_next").click(function(){
	    $('#slick_interactive').slick("slickNext");
	  });
