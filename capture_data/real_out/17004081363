    <script>
        $(document).ready(function() {
            function cy_milismall() {
                var _show = null;
                var _hideTimer = null;
                var planItems = $('div.cy_chartlet_box ul li');
                planItems.each(function(index) {

                    var li = $(this);
                    var div = li.find('div.cy_layerbox');

                    div.css("display", "none");
                    li.bind('mouseenter', function() {
                        if (_show != null) {
                            clearTimeout(_show);
                        }
                        _show = setTimeout(function() {
                            $('ul._commuity_nav li div.conbox').hide();

                            div.show('fast');

                        }, 200);
                    });
                    li.bind('mouseleave', function() {
                        if (_show != null) {
                            clearTimeout(_show);
                        }
                        _hideTimer = setTimeout(function() {
                            div.hide('fast');

                        }, 200);
                    });
                });
            };
            cy_milismall();

            function cy_milismall1() {
                var _show = null;
                var _hideTimer = null;
                var planItems = $('div.cy_chartlet_box1 ul li');
                planItems.each(function(index) {

                    var li = $(this);
                    var div = li.find('div.cy_layerbox1');

                    div.css("display", "none");
                    li.bind('mouseenter', function() {
                        if (_show != null) {
                            clearTimeout(_show);
                        }
                        _show = setTimeout(function() {
                            $('ul._commuity_nav li div.conbox').hide();

                            div.show('fast');

                        }, 200);
                    });
                    li.bind('mouseleave', function() {
                        if (_show != null) {
                            clearTimeout(_show);
                        }
                        _hideTimer = setTimeout(function() {
                            div.hide('fast');

                        }, 200);
                    });
                });
            };
            cy_milismall1();
        });
