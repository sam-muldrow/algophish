        $("#leftSwitchHeaderTabs li").click(function() {
            $(this).addClass("current").siblings().removeClass("current");
            $("div[class*='header_style_']").hide();
            $("div[class*='header_style_']").eq($(this).index()).show();
        });
        $("#leftSwitchFooterTabs li").click(function() {
            $(this).addClass("current").siblings().removeClass("current");
            $("div[class*='footer_style_']").hide();
            $(".footer_style_" + ($(this).index() + 1)).show();
        });

        function clickHideArea(index){
            if($('#navbar').height() == '52'){
                //$(".substation").show();
                $(".substation").css('display','flex');
                $("#navbar").css("height", "102px")
                $(".area").find("img").removeClass("arrowDown").addClass("arrowUp");
            }else{
                $(".area").find("img").removeClass("arrowUp").addClass("arrowDown");
                $("#navbar").css("height", "52px")
                $(".substation").hide();
            }

        }

        window.onload = function(){
            showArea('9');
        }

