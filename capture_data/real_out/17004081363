            var abovetop19 = $("#pplm").offset().top - 70;
            var scTop = 0;
            $(document).scroll(function() {
                scTop = $(this).scrollTop();
                if (scTop >= abovetop) {
                    $("#backtop2").addClass("topsticky");
                } else {
                    $("#backtop2").removeClass("topsticky");
                }
                if (scTop >= abovetop1 && scTop < abovetop2) {
                    $("#backtop2 li").removeClass("sel")
                    $("#backtop2 li").eq(0).addClass("sel")
                } else if (scTop >= abovetop2 && scTop < abovetop3) {
                    $("#backtop2 li").removeClass("sel")
                    $("#backtop2 li").eq(1).addClass("sel")
                } else if (scTop >= abovetop3 && scTop < abovetop4) {
                    $("#backtop2 li").removeClass("sel")
                    $("#backtop2 li").eq(2).addClass("sel")
