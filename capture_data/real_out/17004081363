            <script>
              function closeVideo() {
                  $(".video_wrapper").addClass("blockNone");
                  $(".live_wrapper").addClass("blockNone");
                  $(".video_inner_wrapper video").attr('src','');
              }
              function closeLive() {
                  $(".live_wrapper").addClass("blockNone");
                  $(".video_wrapper").addClass("blockNone");
                  $('#liveIframe').attr('src','');
              }
              function openVideo(thisdiv) {
                  let videourl = thisdiv.attr('data-url');//è·åè§é¢å°å
                  videourl=ridhttp(videourl);
                  let normalurl = thisdiv.siblings().children("a").attr("href");
                  normalurl=ridhttp(normalurl);
                  if (videourl){
                    $(".video_inner_wrapper video").attr('src',videourl);//èµå¼è§é¢å°å
                    $(".video_wrapper").removeClass("blockNone");//æå¼è§é¢çªå£
                    $(".live_wrapper").addClass("blockNone");//å³é­ç´æ­çªå£
                    $('#liveIframe').attr('src','');//å³é­ç´æ­çªå£
                  }else{
                     window.open(normalurl);
                  }

              }
              function openLive(thisdiv,a_url) {
                  let liveurl = thisdiv.attr('data-url');//è·åç´æ­å°å
                  liveurl=ridhttp(liveurl);
                  a_url=ridhttp(a_url);
                  let liveStatus = thisdiv.next().attr("class");
                  let ismp4 = /.mp4$/g;
                  if (ismp4.test(liveurl)){
                    $(".video_inner_wrapper video").attr('src',liveurl);//èµå¼è§é¢å°å
                    $(".video_wrapper").removeClass("blockNone");//æå¼è§é¢çªå£
                    $(".live_wrapper").addClass("blockNone");//å³é­ç´æ­çªå£
                    $('#liveIframe').attr('src','');//å³é­ç´æ­çªå£
                  }
                  else {
                    if (liveurl && liveStatus !='txt_yg') { //ç´æ­é¢åçæ¶åæ²¡æå°ååä¸æ­æ¾
                      liveurl = liveurl.concat("?autoPlay=true");
                      $('#liveIframe').attr('src',liveurl);//èµå¼ç´æ­å°å
                      $(".video_wrapper").addClass("blockNone");//å³é­è§é¢çªå£
                      $(".video_inner_wrapper video").attr('src','');//è§é¢æåæ­æ¾
                      $(".live_wrapper").removeClass("blockNone");//æå¼ç´æ­çªå£
                    } else {
                      window.open(a_url);
                    }
                  }

              }

              function ridhttp(httpurl){
                  if(httpurl){
                      httpurl=httpurl.replace(/^https?:\/\//, "//");
                  }
                  return httpurl;
              }

              $(function() {      
                  $('.video_wrapper .close').on('click', function() {
                      closeVideo();
                  });
                  $('.live_wrapper .close').on('click', function() {
                      closeLive();
                  });
                  $('.focus_video').on('click',function(){
                      openVideo($(this))
                  })
                  $('.focus_live').on('click',function(){
                      openLive($(this))
                  })
                  $('.topnews_hotv_video img').on('click',function(){//ç¹å»è§é¢
                      openVideo($(this))
                  })
                  $('.topnews_hotv_video .icon').on('click',function(){//ç¹å»è§é¢æ­æ¾å¾æ 
                      openVideo($(this).siblings("img"))
                  })
                  $('.topnews_hotv_live img').on('click',function(){//ç¹å»ç´æ­
                      let live_yg = $(this).siblings(".txt_yg").children("a").attr("href");
                      let live_replay = $(this).siblings(".txt_replay").children("a").attr("href");
                      let live_ing = $(this).siblings(".txt_ing").children("a").attr("href");
                      let normal_url = $(this).siblings().children("a").attr("href");
                      if(live_yg){
                        openLive($(this),live_yg);
                      }else if(live_replay){
                        openLive($(this),live_replay);
                      }else if(live_ing){
                        openLive($(this),live_ing);
                      }else{
                        window.open(normal_url);
                      }
                  })
                  $('.topnews_hotv_live .playicon').on('click',function(){//ç¹å»ç´æ­æ­æ¾å¾æ 
                      let live_yg = $(this).siblings(".txt_yg").children("a").attr("href");
                      let live_replay = $(this).siblings(".txt_replay").children("a").attr("href");
                      let live_ing = $(this).siblings(".txt_ing").children("a").attr("href");
                      let normal_url = $(this).siblings().children("a").attr("href");
                      if(live_yg){
                        openLive($(this).siblings("img"),live_yg);
                      }else if(live_replay){
                        openLive($(this).siblings("img"),live_replay);
                      }else if(live_ing){
                        openLive($(this).siblings("img"),live_ing);
                      }else{
                        window.open(normal_url);
                      }
                  })
              })

          </script>
