        $(window).on("load", function() {
            $("#backtop2 a").each(function(index, element) {
                $(this).click(function() {
                    //$(this).addClass("sel").siblings().removeClass("sel");
                    if (index == 0) {
                        $('html,body').animate({
                            scrollTop: $(this.hash).offset().top - 30
                        }, 1000);
                    } else {
                        $('html,body').animate({
                            scrollTop: $(this.hash).offset().top - 60
                        }, 1000);
                    }
                })
            });

            var abovetop = $("#backtop2").offset().top;
            var abovetop1 = $("#tt").offset().top - 70;
            var abovetop2 = $("#yw").offset().top - 70;
            var abovetop3 = $("#zt").offset().top - 70;
            var abovetop4 = $("#sp").offset().top - 70;
            var abovetop5 = $("#pic").offset().top - 70;
            var abovetop6 = $("#sz").offset().top - 70;
