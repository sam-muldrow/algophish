        <script>
            function switchPics() {
                var currentShowPicsIndex = 0;
                $(".milicon_box_chartlet_each").each(function(index) {
                    if ($(this).hasClass("current")) {
                        currentShowPicsIndex = index;
                    }
                });
                $(".milicon_box_chartlet_each").removeClass("current");
                if (currentShowPicsIndex + 1 > 2) {
                    currentShowPicsIndex = 0;
                } else {
                    currentShowPicsIndex++;
                }
                $(".milicon_box_chartlet_each").eq(currentShowPicsIndex).addClass("current");
            }
        </script>
