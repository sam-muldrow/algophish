                } else if (scTop >= abovetop7 && scTop < abovetop8) {
                    $("#backtop2 li").removeClass("sel")
                    $("#backtop2 li").eq(6).addClass("sel")
                } else if (scTop >= abovetop8 && scTop < abovetop9) {
                    $("#backtop2 li").removeClass("sel")
                    $("#backtop2 li").eq(7).addClass("sel")
                } else if (scTop >= abovetop9 && scTop < abovetop10) {
                    $("#backtop2 li").removeClass("sel")
                    $("#backtop2 li").eq(8).addClass("sel")

                } else if (scTop >= abovetop10 && scTop < abovetop11) {
                    $("#backtop2 li").removeClass("sel")
                    $("#backtop2 li").eq(9).addClass("sel")

                } else if (scTop >= abovetop11 && scTop < abovetop12) {
                    $("#backtop2 li").removeClass("sel")
                    $("#backtop2 li").eq(10).addClass("sel")

                } else if (scTop >= abovetop12 && scTop < abovetop13) {
