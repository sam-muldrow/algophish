<script>
  function updateTimer() {
    future = new Date('2021-02-28 05:09:00'.replace(/-/g, "/"));
    now = new Date();
    diff = future - now;

    days = Math.floor(diff / (1000 * 60 * 60 * 24));
    hours = Math.floor(diff / (1000 * 60 * 60));
    mins = Math.floor(diff / (1000 * 60));
    secs = Math.floor(diff / 1000);

    d = days;
    h = hours - days * 24;
    m = mins - hours * 60;
    s = secs - mins * 60;

    document.getElementById("new_timer")
        .innerHTML =
        '<div>' + d + '<span>Days</span></div>' +
        '<div>' + h + '<span>Hours</span></div>' +
        '<div>' + m + '<span>Minutes</span></div>' +
        '<div>' + s + '<span>Seconds</span></div>';
}
setInterval('updateTimer()', 1000);
//   Timer With Autohide Function
  function announcement_bar_demo_makeTimer() {
					var endTime = new Date("2021-01-31 05:00:00");
					endTime = (Date.parse(endTime) / 1000);
					var now = new Date();
					now = (Date.parse(now) / 1000);
					var timeLeft = endTime - now;
					var days = Math.floor(timeLeft / 86400);
					var hours = Math.floor((timeLeft - (days * 86400)) / 3600);
					var minutes = Math.floor((timeLeft - (days * 86400) - (hours * 3600 )) / 60);
					var seconds = Math.floor((timeLeft - (days * 86400) - (hours * 3600) - (minutes * 60)));
		
					if (hours < "10") { hours = "0" + hours; }
					if (minutes < "10") { minutes = "0" + minutes; }
					if (seconds < "10") { seconds = "0" + seconds; }
					if (timeLeft <= "0") {
					clearInterval(timeinterval);
					$("#announcement_bar_demo").hide();
				}
					$("#announcement_bar_demo_days").html(days);
					$("#announcement_bar_demo_hours").html(hours);
					$("#announcement_bar_demo_minutes").html(minutes);
					$("#announcement_bar_demo_seconds").html(seconds);
			}
			var timeinterval = setInterval(function() { announcement_bar_demo_makeTimer(); }, 1000);

  
//   //   Close Button Hide Function  
    function closeHide(){
				$(document).ready(function(){
  					$("#announcement_bar_demo_close_button").click(function(){
    					$("#announcement_bar_demo").hide();
  					});
				});
			}
			closeHide();
  
      
            
//     Timer Show/Hide Function
	var buttonTimer = document.getElementById("new_timer");
	var timerButton = true; 
	function timer_hide(){
		 if (timerButton){
		 	buttonTimer.style.display = "";
		 }else{
		 	buttonTimer.style.display = "none";
		 }
	}
  timer_hide();
      
//  Button Shake ON/OFF Function
	var content = document.getElementById("announcement_bar_demo_btn-shake");
	var change = true; 
	function auto_change(){
		 if (change) {
		 	animation.style.animation = "";
		 }
	}
  auto_change();
            

</script>


<script>

// Show the first tab and hide the rest
$('#tabs-nav li:first-child').addClass('active');
$('.tab-content').hide();
$('.tab-content:first').show();

// Click function
$('#tabs-nav li').click(function(){
  $('#tabs-nav li').removeClass('active');
  $(this).addClass('active');
  $('.tab-content').hide();
  
  var activeTab = $(this).find('a').attr('href');
  $(activeTab).fadeIn();
  return false;
});
  
    $('.toggle1').on("click", function() {
    
     if ($(this).hasClass("open1")) {
          $(this).removeClass("open1");
          $(this).siblings(".accordion-content1").slideUp(200);

        } else {

          $(".toggle1").removeClass("open1");
          $(this).addClass("open1");
          $(".accordion-content1").slideUp(200);
          $(this).siblings(".accordion-content1").slideDown(200);
        }
    
    
  });
  
  </script>

