<script type="text/javascript">
        $(document).ready(function () {
            $(".checkout-footer").on("click", ".continue-shopping", function () {
                document.querySelector(".c-icoPanels.s-kt-navigation__cart .c-dropDown__toggler").click();
            });

            var enableSiteSearch = 'True';

            if (enableSiteSearch == 'False')
            {
                $(".s-kt-navigation .c-searchPanel form button[type=submit]").on("click",function (event) {
                    event.preventDefault();
                    var $form = $(this).parents("form");
                    var $input = $form.find("input:visible");
                    var inputVal = $.trim($input.val());                

                    $form.find(".c-searchCard__body .c-tooltip__group").remove();

                    if (!inputVal) {
                        $input.focus();
                        $input.parents(".f-input").addClass("f-input--invalid");
                        return;
                    }
                     
                    if (!$form.valid()) {
                        var tooltip = $('<div class="c-tooltip__group"  style="display: flex;flex-direction: column;"><div class="c-tooltip__message c-tooltip__message--error" style="display: block;">'
                            + $input.attr("data-val-length") + '</div><div class="c-tooltip__arrow"></div></div>');
                        $form.find(".c-searchCard__body").append(tooltip);
                        return;
                    }

                    $input.val(inputVal);
                    var validateAction = $(this).data("validate-action");
                    var validateMethod = $(this).data("validate-method");

                    $.ajax({
                        url: validateAction,
                        type: validateMethod,
                        data: $form.serializeArray(),
                        success: function (data) {
                            $form.find(".c-searchCard__body .c-tooltip__group").remove();
                            if (data) {
                                $form.submit();
                            } else {
                                $form.find(".c-searchCard__body").append($('<div class="c-tooltip__group" style="display: flex;flex-direction: column;"><div class="c-tooltip__message c-tooltip__message--helper" style="display: block;">Your search did not produce any results.</br>Please try different term(s)</div><div class="c-tooltip__arrow"></div></div>'));
                            }
                        }
                    });
                });

                $(".section-navigation .c-searchPanel form input[type=text]").keyup(function() {
                    $(this).parents(".f-input").removeClass("f-input--invalid");
                    if ($(this).parents("form").valid())
                        $(this).parents(".c-searchCard__body").find(".c-tooltip__group").remove();
                });
            }
        });
    </script>
<script type="text/javascript">
        $(function () {
            $(".s-searchConfigurator form button[type=submit]").on("click",function (event) {
                event.preventDefault();
                var $form = $(this).parents("form");
                var $input = $form.find("input:visible");
                var inputVal = $.trim($input.val());

                $form.find(".message-group .c-tooltip__group").remove();

                if (!inputVal) {
                    $input.focus();
                    $input.parents(".f-input").addClass("f-input--invalid");
                    return;
                }

                if (!$form.valid()) {
                    var tooltip = $('<div class="c-tooltip__group"><div class="c-tooltip__message c-tooltip__message--error" style="display: block;">'
                        + $input.attr("data-val-length") + '</div><div class="c-tooltip__arrow"></div></div>');
                    $form.find(".message-group").append(tooltip);
                    return;
                }
                $input.val(inputVal);
                var validateAction = $(this).data("validate-action");
                var validateMethod = $(this).data("validate-method");
                var searchLocation = $(this).data("search-location");
                $.ajax({
                    url: validateAction,
                    type: validateMethod,
                    data: $form.serializeArray(),
                    success: function (data) {
                        $form.find(".message-group .c-tooltip__group").remove();
                        if (data) {
                            var searchResultPageUrl = $form.data("search-result-page-url");
                            searchResultPageUrl = searchResultPageUrl + KCMS.utilities.buildUrlParameter($form.serializeArray());
                            KCMS.EventBus.publish("/siteSearch/searchTerm", { searchTerm: inputVal, searchLocation: searchLocation, searchResultPageUrl: searchResultPageUrl });

                            setTimeout(function () {
                                $form.submit();
                            }, 100);
                        } else {
                            var tooltip = $('<div class="c-tooltip__group"><div class="c-tooltip__message c-tooltip__message--helper" style="display: block;">Your search did not produce any results.</br>Please try different term(s)</div><div class="c-tooltip__arrow"></div></div>');

                            $form.find(".message-group").append(tooltip);
                        }
                    }
                });
            });

            $(".s-searchConfigurator form input[type=text]").on("keyup", function () {
                $(this).parents(".f-input").removeClass("f-input--invalid");
                if ($(this).parents("form").valid())
                    $(this).parents("form").find(".message-group .c-tooltip__group").remove();
            })
        });
    </script>
