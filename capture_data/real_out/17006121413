<script type="text/javascript">
            KCMS.EventBus.subscribe("OnCustomerSigin", function (data) {
                var eventData = {
                    'event': 'login-complete',
                    'userId': data.customerId || undefined,
                    'loggedIn': data.loggedIn || 'yes',
                    'autoLogin': data.autoLogin || false
                };
                window.dataLayer.push(eventData);
            });
        </script>
