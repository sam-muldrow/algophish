        if (window.location.search) {
            var queries = ["utm_campaign"];
            var search = window.location.search.toLowerCase();
            for (var i = 0; i < queries.length; i++) {
                var key = queries[i].toLowerCase();
                if (search.indexOf("?" + key + "=") != -1 || search.indexOf("&" + key + "=") != -1) {
                    var requestQuery = '/request/query' + window.location.search;
                    var xmlHttp = new XMLHttpRequest();
