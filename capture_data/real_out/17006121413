        id: "id",
        ar: "ar-EG",
        latam: "es-ES",
    }

    var nrLang = typeof workContext !== "undefined" ? workContext.lcode : "en";
    switch (nrLang) {
        case "vn":
            nrLang = "vi";
            break;
        case "jp":
            nrLang = "ja";
            break;
        case "latam":
            nrLang = "es";
            break;
    }
    //Correcting language codes
    window._bcvma = window._bcvma || [];
    _bcvma.push(["setAccountID", "564662031747073880"]);
