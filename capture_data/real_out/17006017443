	$('.home_menu_tab2 span').hover(function(){
		if(!$(this).hasClass('active')){
			var tabIndex = $(this).attr('tabIndex');			
			$('.home_menu_tab2 span').removeClass('active');
			$(this).addClass('active');
			$('.menu2_content').hide();
