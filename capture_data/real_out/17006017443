		/**
		 * Corner news display
		 */
				
		/**
		 * Breaking list section
		 */
		var brk_list_width = 0;
		$('.headlines li').each(function(index, element) {
           brk_list_width = brk_list_width + $(this).innerWidth() + 25;
        });
		//alert(total_hl_list_width);
		$('.headlines ul').css('width',brk_list_width);
		
