	$('.home_menu_tab3 span').hover(function(){
		if(!$(this).hasClass('active')){
			var tabIndex = $(this).attr('tabIndex');			
			$('.home_menu_tab3 span').removeClass('active');
			$(this).addClass('active');
			$('.menu3_content').hide();
			$('#' + tabIndex).fadeIn(800);
		}
	});
</script>
