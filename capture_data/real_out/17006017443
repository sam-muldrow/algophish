	#menu_category ul.header_hover_bottom_menu>li>a{
		position:relative; display:block; padding:6px 15px; color:#000; /*text-shadow: 1px 0 #006699*/;
	}
	@media (min-width: 768px) and (max-width: 991px) {
	#menu_category.sticky{width: 720px;}
	}
	@media screen and (min-width: 768px) and (max-width: 1024px) {
	#menu_category ul.header_top_menu>li{font-size:14px;}
	#menu_category ul.header_top_menu>li>a{padding:6px 4px;}
	 #menu_category ul.header_top_menu>li.aro>ul>li>a{ font-size:14px;}
