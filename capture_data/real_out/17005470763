	<script>
		var audit = $('.js_audit');
		var audit_content = $('.js_audit_content');

		audit.click(function(){
			if($(this).hasClass('active')){
				$(this).removeClass('active');
			} else if($(this).not('active')){
				$(this).addClass('active');
			}

			if ($(this).siblings(audit_content).hasClass('visible')){
				$(this).siblings(audit_content).slideToggle(50);
			} else if($(this).siblings(audit_content).not('visible')){
				$(this).siblings(audit_content).addClass('visible');
			}
		});
	</script>
