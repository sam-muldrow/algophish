			};
			(function(){var w=window;var ic=w.Intercom;if(typeof ic==="function")
			{ic('reattach_activator');ic('update',intercomSettings);}else
			{var d=document;var i=function(){i.c(arguments)};i.q=[];i.c=function(args)
			{i.q.push(args)};w.Intercom=i;function l()
			{var s=d.createElement('script');s.type='text/javascript';s.async=true;
			s.src='https://widget.intercom.io/widget/' + w.intercomSettings.app_id;
			var x=d.getElementsByTagName('script')[0];
			x.parentNode.insertBefore(s,x);}if(w.attachEvent){w.attachEvent('onload',l);}
			else{w.addEventListener('load',l,false);}if(window.statistics_async){l();}}})();

			Intercom('onUnreadCountChange', function(count){
				var unreadCount = document.querySelector('.livechat_launcher_count');
				if (unreadCount){
					unreadCount.textContent = count;
					if (count) {
						unreadCount.classList.add('active');
					} else {
						unreadCount.classList.remove('active');
					}
				}
			});
			if (window.show_intercom){
				Intercom('show');
			}
