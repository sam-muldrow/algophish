	<script>
		var slider = $('.js_review-slider');

		slider.each(function(){
		    $(this).collider();

		    
	var cut = $('.js_cut');
	var size = 272;

	
		for(var index = 0; index < cut.length; index++){
			cut.eq(index).addClass('item-' + index);

			var cite = $('.item-' + index);

			cite.each(function(){
				var text = '<span class="preview">' + $(this).html().substring(0, size) + '</span>' + '<span class="tridot">...</span>' + '<span class="cut">' + $(this).html().substring() + '</span>';

				$(this).html(text);
			});
		}
	

	cut.after($('<a class="container-review_slide_expand text__turquoise js_expand" href="javascript:{}">Expand text</a>'));
	

	$('.js_expand').click(function(){
		$(this).siblings(cut).find('.cut').show();
		$(this).siblings(cut).find('.tridot').hide();
		$(this).siblings(cut).find('.preview').hide();
		$(this).css('display', 'none');
	});

	
		$('.ts-prev, .ts-next, .steps a').click(function(){
			cut.find('.cut').hide();
			cut.find('.tridot').show();
			cut.find('.preview').show();
			$('.js_expand').css('display', 'block');
		});

		//prev & next opacity change
		var timeout = null;

		$('.ts-prev, .ts-next').click(function(){
			$(this).css('opacity', '0.6');

			clearTimeout(timeout);
			timeout = setTimeout(function(){ $('.ts-prev, .ts-next').css('opacity', ''); }, 200);
		});

		//add imgs to steps
		var customer_img = $('.container-review_slide_img'),
			step = $('.js_review-steps span'),
			array_src = [];

		customer_img.each(function(index) {
			$(this).attr('slide', index);

			var src = $(this).attr('src');
			array_src.push(src);
		});

		$(window).on('load scroll',function(){
			if(step.visible()){
				for (var i = 0; i < step.length; i++) {
					step.eq(i).css('background-image', 'url(' + array_src[i] + ')');
				}
			}
		});
	

		});

		slider.swipeDetector().on("swipeLeft swipeRight", function(event) {
			if (event.type === "swipeLeft") {
				$(".ts-prev").click();
			} else if (event.type === "swipeRight") {
				$(".ts-next").click();
			}
		});
	</script>
