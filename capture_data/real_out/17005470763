	<script>
		
			
			function load_recaptcha_script(func){
				if (window.grecaptcha){
					func()
				} else {
					var d=document,sn=d.createElement('script');
					sn.type='text/javascript';
					sn.async=true;
					var s=d.getElementsByTagName('script')[0];
					s.parentNode.insertBefore(sn, s);
					sn.onload = func;
					sn.src='https://www.google.com/recaptcha/api.js?render=6LeiRYAaAAAAACjtql5DiW8ly3UGznSNiHYl4r86';
				}
			}

			function recaptcha_exec(e, form, action) {
				e.preventDefault();
				load_recaptcha_script(function(){
					grecaptcha.ready(function(){
						grecaptcha.execute("6LeiRYAaAAAAACjtql5DiW8ly3UGznSNiHYl4r86", {action: action}).then(function(token) {
							input = $("input[name=recaptcha_v3_token]", form);
							input.val(token);
							form[0].submit();
						});
					});
				});
			}
		

		//    
		$('.js_joinnow-form_btn').click(function(e){
			var form = $(this).parents('.js_join_form');

			
				recaptcha_exec(e, form, 'submit');
			
		});

		//      
		$('.js_signup_openid').click(function(e){
			var form = $(this).parents().siblings('.js_openid_hidden').find('.js_openid_signup_form'),
				fieldset = $(this).parents('fieldset'),
				openid_subscribe = form.find('.js_openid_subscribe'),
				subscribe = fieldset.find('.js_subscribe'),
				openid_type = form.find('.js_openid_type'),
				terms = fieldset.find('.js_terms_and_privacy'),
				openid_terms = form.find('.js_openid_terms_and_privacy');

			openid_subscribe.prop('checked', subscribe.is(':checked'));
			openid_type.val($(this).attr('data-openidtype'));
			if (terms && terms.is(':checked')) openid_terms.prop('checked', terms.is(':checked'));

			
				recaptcha_exec(e, form, 'openid_submit');
			
		});
	</script>


			<script>
				//Show popup
				$('.js_show_popup').click(function() {
					var popup_id = $('#' + $(this).attr("rel"));

					$(popup_id).show(100, function(){
						var frame = $(this).find('iframe');
						frame.attr('src', frame.attr('data-src') || frame.attr('src'));
					});
					$('.js_overlay_popup').show();
				});

				$('.js_popup_close').click(function() {
					$('.js_overlay_popup, .js_popup').hide();
				});

				
				if (document.location.hash) {
					var els = $(document.location.hash + '.js_show_popup');
					if (els.length) {
						document.location.hash = '';
						els[0].click();
					}
				}

				//Checkbox action
				$(function(){
					var checkbox_trial = $('.js_checkbox_trial');

					checkbox_trial.on('change', function(){
						if(checkbox_trial.prop('checked')) {
							$('.js_hidden-fields-trial').show();
						} else{
							$('.js_hidden-fields-trial').hide();
						}
					});
				});

				//Validate popup form
				jQuery.validator.addMethod("pocophone", function(phone_nmb, element) {
					return this.optional(element) || phone_nmb.match(/^\+?[0-9\-\(\)\s]*$/);
				}, '* Please enter your phone');

				jQuery.validator.addMethod("url_mod", function(url, element) {
					return this.optional(element) || url.match(/^(https?:\/\/)?(?:\S+(?::\S*)?@)?(?:(?!(?:10|127)(?:\.\d{1,3}){3})(?!(?:169\.254|192\.168)(?:\.\d{1,3}){2})(?!172\.(?:1[6-9]|2\d|3[0-1])(?:\.\d{1,3}){2})(?:[1-9]\d?|1\d\d|2[01]\d|22[0-3])(?:\.(?:1?\d{1,2}|2[0-4]\d|25[0-5])){2}(?:\.(?:[1-9]\d?|1\d\d|2[0-4]\d|25[0-4]))|(?:(?:[a-z\u00a1-\uffff0-9]-*)*[a-z\u00a1-\uffff0-9]+)(?:\.(?:[a-z\u00a1-\uffff0-9]-*)*[a-z\u00a1-\uffff0-9]+)*(?:\.(?:[a-z\u00a1-\uffff]{2,})).?)(?::\d{2,5})?(?:[/?#]\S*)?$/ui);
				}, '* Please enter a valid url');

				$('.js_request_form').each(function() {
					$(this).validate({
						submitHandler: function(form) {
							var rcnt = parseInt($.cookie("rcnt"))||0;
							if(rcnt<2){
								$.ajax({
									type: $(form).attr('method'),
									url: "/contact_me/", //$(form).attr('action'),
									data: $(form).serialize() //, dataType: 'json'
								}).done(function(data){
									//ok msg
									if(data && data.status){
										rcnt += 1;
										$.cookie("rcnt", rcnt, {expires : 1});
									}
									$(form).hide().next().show();
								}).fail(function(){
									//fail msg
									//$(form).hide().next().show();
								});
							} else {
								$(form).hide();
								$(".twice_send").show();
							}
							//event.preventDefault();
							return false;
						},
						rules:{
							name: "required",
							email:		{ required: true, email: true },
							site_url:	{ required: false, minlength: 5, url_mod: true },
							phone:		{ required: false, minlength: 3, pocophone: true }
						},
						messages:{
							name:"* Please enter your name",
							email:{
								required: "* Please enter your email address",
								email: "* Please enter a valid email address"
							},
							site_url:{
								//required: "* Please enter a valid url",
								minlength: "Your text must consist of at least 5 characters"
							},
							phone:{
								//required: "* Please enter your phone",
								minlength: "Your text must consist of at least 3 characters"
							}
						}
					});
				});
			</script>

			<script>
		(function($){
			$.fn.visible = function(){
				var el_top = $(this).offset().top,
					el_bottom = el_top + $(this).outerHeight(),
					visible_top = $(window).scrollTop(),
					visible_bottom = visible_top + $(window).height();

				return el_bottom > visible_top && el_top < visible_bottom;
			};
		})(jQuery);

		$(window).on('load scroll',function(){
			$('.js_lazy_bg').each(function (index, el) {
				if($(this).visible()){
					el.setAttribute("style", "background-image: url(" + el.getAttribute('data-background') + ")");
				};
			});
		});
	</script>

			
			<script src="https://site.wstatic.net/static/js/site/jquery.periodic.987ae0077276.js" type="text/javascript"></script>
			<script>
				function countdown(el_target, targetdate){
					var targetdate = new Date(targetdate);
					var currTime = new Date();
					var timeDiff = (targetdate - currTime) / 1000;

					if (timeDiff <= 0){
						$(el_target).html('Expired');
					} else {
						var oneMinute = 60;
						var oneHour   = 60*60;
						var oneDay    = 60*60*24;
						var days = Math.floor(timeDiff/oneDay);
						var hours = Math.floor((timeDiff-days*oneDay)/oneHour);
						var minutes = Math.floor((timeDiff-days*oneDay-hours*oneHour)/oneMinute)
						var seconds = Math.floor(timeDiff-days*oneDay-hours*oneHour-minutes*oneMinute)

						time = jQuery.map([hours, minutes, seconds], function(value){
							return (value < 10) ? '0' + value.toString() : value.toString();
						});

						$(el_target).html([days, time.join(':')].join((days > 1) ?' <span>days</span> ':' <span>day</span> '));
					}
				}

				$(document).ready(function(){
					if($('.js_countown')){
						$.periodic({period: 1000, decay: 1.0}, function(){
							countdown($('.js_countown'), '2023/11/01 08:00:00 UTC');
						});
					}
				});
			</script>

			
	    	<script>
	    		$(document).ready(function(){
					$('.js_to-top').click(function () {
						$('body,html').animate({
							scrollTop: 0
						}, 800);

						return false;
					});
				});
	    	</script>

		    
	
		    	
