  data-page-section="X-faktor 5 - Relationsvindue">

  <article class="teaser teaser--flex teaser--min-height faux-block-link js-int-links" data-category="x-faktor" data-element-type="relation">
  <div class="article-intro article-intro--bg">

    <div class="article-intro--bg__media" style="background-color:rgba(0,0,0,0.19999999);">
          <figure class="media  ">		
  <div class="media__wrapper media__wrapper--bg media__wrapper--gray" style=''>
    <img class="media__image image--fullwidth   image--backgroundcover  js-lazyload-image lazyload-image"  src='data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==' data-srcset='https://politiken.dk/incoming/img9203829.2mu51w/ALTERNATES/p3x1_960/Stempelkande%20efterar2 960w, https://politiken.dk/incoming/img9203829.2mu51w/ALTERNATES/p3x1_1125/Stempelkande%20efterar2 1125w, https://politiken.dk/incoming/img9203829.2mu51w/ALTERNATES/p3x1_2000/Stempelkande%20efterar2 2000w' data-sizes='(max-width: 960px) 960px, (max-width: 1117px) 1125px, 2000px'/>
    </div>
  </figure>
