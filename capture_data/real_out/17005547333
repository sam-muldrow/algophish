    necessary: 'necessary',
    preferences: 'preferences',
    statistics: 'statistics',
    marketing: 'marketing',
    translateCategories: function (cat) {
      switch (cat) {
        case jppolCMPVendorCategories.necessary:
          return 'nødvendige';
        case jppolCMPVendorCategories.preferences:
          return 'præferencer';
        case jppolCMPVendorCategories.statistics:
          return 'statistik';
        case jppolCMPVendorCategories.marketing:
          return 'marketing';
        default:
          return cat;
      }
    }
  };
