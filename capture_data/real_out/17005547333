        (function () {
          if (window.hasOwnProperty("gptHandler") && window.gptHandler.hasOwnProperty("bannerData")) {
            window.gptHandler.bannerData.push({"targetId":"topscroll","sizes":[[1,2]],"format":["banner"]});
          }

          if (window.hasOwnProperty("jppolPerf") && window.jppolPerf.hasOwnProperty("observeAd")) {
            window.jppolPerf.observeAd("topscroll");
          }
        }());
      </script>

      
    </div>
  

