<script type="text/javascript"> 
var mboxElement = document.getElementById("MB\u002DHOMEPAGE\u002DTOPBANNER");
if (typeof adobe !== "undefined") {
	mboxElement.style.opacity = "0";
	adobe.target.getOffer({  
	  "mbox": "MB\u002DHOMEPAGE\u002DTOPBANNER",
	  "params": {
		  "bladeSequence": 2,
	  },
	  "success": function(offer) {          
	        adobe.target.applyOffer( { 
	           "mbox": "MB\u002DHOMEPAGE\u002DTOPBANNER",
	           "selector": "#"+"MB\u002DHOMEPAGE\u002DTOPBANNER",
	           "offer": offer 
	        } );
	        var updatedMboxElement = document.getElementById("MB\u002DHOMEPAGE\u002DTOPBANNER");
            updatedMboxElement.style.opacity = "1";
	  },  
	  "error": function(status, error) {
		    var updatedMboxElement = document.getElementById("MB\u002DHOMEPAGE\u002DTOPBANNER");
		    updatedMboxElement.style.opacity = "1";
	        console.log('Error', status, error);
	  }
	});
}
</script>





    
    
    




