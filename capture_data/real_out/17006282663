<section class="section" id="content-offers-copy">
	
	
	
	
	
	<div class="offers-slim-image-text
	
	
	" data-component="offers">
		<div class="offers-slim-image-text__container">
			<div class="offers-slim-image-text__img-holder">
				<div id="content-offers-copy-image" class="component-lazy-image" data-component="lazy-image" data-desktop-img-src="/content/dam/bankwest/web-assets/images/offers/slim-image-and-text/offers-siat-claymation-bradley-brittany-bevan-ice-cream-truck.jpg" data-mobile-img-src="/content/dam/bankwest/web-assets/images/offers/slim-image-and-text/offers-siat-claymation-bradley-brittany-bevan-ice-cream-truck.jpg" data-img-type="wide" data-bg-x="center" data-bg-y="middle" data-bg-mob-x="center" data-bg-mob-y="top"></div>
			</div>
			<div class="offers-slim-image-text__content-holder">
				<div class="offers-slim-image-text__header-holder">
					<h1 class="offers-slim-image-text__title">
						
								Variable rate home loan with less fees and less fuss
						
					</h1>
					<h3 class="offers-slim-image-text__sub-title">Bankwest Simple Home Loan</h3>
				</div>
				<div class="offers-slim-image-text__attributes">
					



