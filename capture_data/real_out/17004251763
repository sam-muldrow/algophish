						<% var src=cookbook.Recipes && cookbook.Recipes[i] && cookbook.Recipes[i].PictureUrl ? cookbook.Recipes[i].PictureUrl : 'https://assets.afcdn.com/recipe/20100101/recipe_default_img_blurred_5.jpg';%>
