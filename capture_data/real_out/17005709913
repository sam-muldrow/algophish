        .addSize([750, 0], [[300, 600],[300, 250]])
        .build();
  var skin = googletag.sizeMapping()
        .addSize([0, 0], [])
        .addSize([1100, 0], [1, 1])
        .build();
  var horizontal_banners = googletag.sizeMapping()
        .addSize([0, 0], [[300, 250], [320, 100]])
        .addSize([340,0], [[336,280], [300,250], [320, 100]])
        .addSize([768, 0], [[728, 90]])
        .build();		
      	googletag.defineSlot('/126962324/tlife_Rectangle_Homepage_1', [[300, 250], [300, 600]], 'div-gpt-ad-1592911387542-0').addService(googletag.pubads());
  	googletag.defineSlot('/126962324/tlife_Rectangle_Homepage_2', [[300, 250], [300, 600]], 'div-gpt-ad-1592987796845-0').addService(googletag.pubads());
  	googletag.defineSlot('/126962324/tlife_Horizontal_Homepage', [970, 250], 'div-gpt-ad-1592911353519-0').defineSizeMapping(top_banners).addService(googletag.pubads());
  	googletag.defineSlot('/126962324/tlife_Horizontal_Homepage_2', [970, 250], 'div-gpt-ad-1593073117041-0').defineSizeMapping(top_banners).addService(googletag.pubads());
      
              googletag.pubads().setTargeting('Page', "1077067");
            googletag.pubads().setTargeting('Type', "page");
    
    //googletag.pubads().disableInitialLoad(); 
    googletag.pubads().enableSingleRequest();
    googletag.pubads().enableLazyLoad({
                fetchMarginPercent: 100,  
                renderMarginPercent: 100,  
                mobileScaling: 1.0  
              });
    googletag.enableServices();
  });
</script>
</head>

