                            if(typeof etimesslider != "undefined" && !window.matchMedia("(max-width: 768px)").matches){
                                if($(thisObjDestination).find('li').length > 2){
                                
                                    var oneLiWidth_dest = $(thisObjDestination).find('[data-plugin="appendeddiv"] li:first').outerWidth();
                                    
                                    oneLiWidth = parseInt(oneLiWidth_dest * 7);
                                    
                                    
                                    <!--$(thisObjDestination).outerWidth();-->
                                    etimesslider.init(oneLiWidth,"x",$(thisObjDestination).find('[data-plugin="appendeddiv"]'),$(thisObjDestination).find('[data-plugin="appendeddiv"] li'),$(thisObjDestination).find(".nextbtn"),$(thisObjDestination).find(".prebtn"),"disable",1)
                                }
                            }
                        });
                    }
                    
                    
                    
                })();
            
        </script><div id="destinationpopupOuter" class="destinationpopupOuter"><div class="destinationpop"><div class="destinationHeader"><div class="desclosebtn"><span></span></div><span class="lable"><h3>Top Travel Destinations</h3></span><span class="destinationSearch"><div class="search_box"><input autocomplete="off" class="placeholder" name="" type="text" placeholder="Search" id="search_cities"><a href="javascript://" class="ent_sprite searchIcon2" id="searchCitiesIcon" data-ga="" data-label="" data-action="" name="searchbutton">search</a><a style="display:none;" href="javascript://" class="searchcross" name="crossbutton">close</a></div></span></div><div class="destinationMiddle"><div class="desAlphabetTabs"><ul></ul></div><div class="destinationInner"><ul class="din_ul"><li data-ga="Destinations Widget,Click,Agra" data-cityname="Agra"><a href="/travel/agra/travel-guide/cs31408220.cms"><span class="citytitlename">Agra</span></a></li><li data-ga="Destinations Widget,Click,Amsterdam" data-cityname="Amsterdam"><a href="/travel/amsterdam/travel-guide/cs58006637.cms"><span class="citytitlename">Amsterdam</span></a></li><li data-ga="Destinations Widget,Click,Athens" data-cityname="Athens"><a href="/travel/athens/travel-guide/cs45136173.cms"><span class="citytitlename">Athens</span></a></li><li data-ga="Destinations Widget,Click,Bali" data-cityname="Bali"><a href="/travel/bali/travel-guide/cs47581488.cms"><span class="citytitlename">Bali</span></a></li><li data-ga="Destinations Widget,Click,Bangkok" data-cityname="Bangkok"><a href="/travel/bangkok/travel-guide/cs35618542.cms"><span class="citytitlename">Bangkok</span></a></li><li data-ga="Destinations Widget,Click,Chennai" data-cityname="Chennai"><a href="/travel/chennai/travel-guide/cs24528091.cms"><span class="citytitlename">Chennai</span></a></li><li data-ga="Destinations Widget,Click,Darjeeling" data-cityname="Darjeeling"><a href="/travel/darjeeling/travel-guide/cs37317105.cms"><span class="citytitlename">Darjeeli