      function loadNextItems(api, start) { var br; if ($.browser.msie && $.browser.version == "6.0") { br = "ie6" } else { br = "all" }; $.ajax({ type: "POST", url: "/rotatingbanners.aspx?uri=" + document.location + "&br=" + br, contentType: "application/json; charset=utf-8", success: function (msg) { if (start) { appendulItems(msg) } else { appendItems(api, msg) } } }) } function appendItems(api, items) { var str = "<ul>"; $(items.item).each(function (i) { if (i < 4 || i > 4) { str += "<li class='lih'>" + String(this) + "</li>" } else { str += "</ul>"; api.addItem(str); str = "<ul><li class='lih'>" + String(this) + "</li>" } }); str += "</ul>"; api.addItem(str) } function appendulItems(items) { var str = "<ul>"; $(items.item).each(function (i) { if (i < 4 || i > 4) { str += "<li class='lih'>" + String(this) + "</li>" } else { str += "</ul><ul><li class='lih'>" + String(this) + "</li>" } }); str += "</ul>"; $(".items").append(str); $(".scrollable").scrollable({ easing: "custom", circular: true, keyboard: false, speed: 1500, onSeek: function () { if (this.getSize() < 10 && this.getIndex() > 0) { loadNextItems(this, false) } } }).autoscroll({ autoplay: true, interval: 15000 }) } $.easing.custom = function (x, t, b, c, d) { var s = 1.70158; if ((t /= d / 2) < 1) { return c / 2 * (t * t * (((s *= (1.525)) + 1) * t - s)) + b } return c / 2 * ((t -= 2) * t * (((s *= (1.525)) + 1) * t + s) + 2) + b }; $(function () { loadNextItems(null, true) });
</script>
