    function emailCheck() {
        var txtEmail = document.getElementById('ctl00_Newsletter1_txtEmail');
        if (txtEmail.value == 'سجل بريدك') return false;
        if (!ValidateEmail(txtEmail.value, true)) return false;
        return true;
    }
