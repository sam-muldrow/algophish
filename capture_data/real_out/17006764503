//23.06.06 팝업 
function onload_popup() { //페이지 로드 시 팝업 띄움.
   var cookie= getnull("nullName");
   if (cookie!= "noPopup") { //창 닫기를 체크하지 않았으면 팝업 띄움.
	   window.open('pop/pop230714.htm', '[노컷비즈] 인턴기자 합격자 발표', 'left=50,top=50,width=280,height=469,toolbar=no,menubar=no,status=no,scrollbars=no,resizable=no');
   }
}

function getnull(name) { //쿠키 가져오기
	var Found = false;
	var start, end;
	var i = 0;
	while (i <= document.cookie.length) {
		start = i
		end = start + name.length
		if (document.cookie.substring(start, end) == name) {
			Found = true;
			break;
		}
		i++;
	}
	if (Found == true) {
		start = end + 1;
		end = document.cookie.indexOf(";", start);
		if (end < start) {
			end = document.cookie.length;
		}
		return document.cookie.substring(start, end)
	}
	return "";
}

function printArticle() {

    // 감싸는 태그 class 추가.
    $('.tn_b2').addClass("extra");

    // tn2 => 오른쪽
    // tn1 => 왼쪽
    $('.top2.st2').after(
        $(document.createElement("p")).addClass("snews tn2").append($(document.createElement("a")).attr("href", "/news/5236475").text("[정정 및 반론 보도] 남성교통 주식회사 관련")))
        .after($(document.createElement("p")).addClass("snews tn1").append($(document.createElement("a")).attr("href", "/news/5237311").text("[단독]'불법 대리수술 40차례' 병원 여전히 성업중…손 놓은 보건당국")));
}
