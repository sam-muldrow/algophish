<script type="text/javascript" src="/js/libs/art_template.js"></script>
<script>
    var page = 1;
    var left_type = '';
    var last_id = 0;
    var adv_flag = '0';
    getList(1);

    function getList(page) {
        var url = "/home/getItemsList?page=" + page + "&adv_flag=" + adv_flag + "&left_type=" + left_type + "&last_id=" + last_id;
        $.ajax({
            url: url,
            async: true,
            beforeSend: function () {
                common._loadding('.loadding');
                $(".click-more").hide();
            },
            complete: function () {
                // common._loaddingClose();
            },
            success: function (data) {
                if (data.status == 'success') {
                    //返回的数据补充到剩下的id为left_page的节点中
                    template.defaults.escape = false;
                    var html = "";
                    $.each(data.data.preList, function (i, vals) {
                        var temp_data = {
                            'vals': vals
                        };
                        html += template('pre_tmpl', temp_data);
                    });

                    var pre_page = page - 1;
                    $("#left_" + pre_page).append(html);

                    var append_html = "";
                    $.each(data.data.dataList, function (i, vals) {
                        //循环寻找最小的发布时间,分页时last_id取最小的推送时间
                        for (k = 0; k < vals.length; k++) {
                            if (last_id == 0) {
                                last_id = vals[0].send_at;
                            }

                            if (last_id > vals[k].send_at) {
                                last_id = vals[k].send_at;
                            }
                        }

                        html = "";
                        if (vals.length == 1 && vals[0].shape == '2*2') {
                            var temp_data = {
                                'vals': vals
                            };
                            html += template('a_module', temp_data);
                        } else if (vals.length == 3 && (vals[0].shape == '1*1' && vals[1].shape == '1*1' && vals[2].shape == '2*1' || (vals[0].shape == '2*1' && vals[1].shape == '1*1' && vals[2].shape == '1*1'))) {
                            var temp_data = {
                                'vals': vals
                            };
                            html += template('c_module', temp_data);
                        } else if (vals.length == 2 && vals[0].shape == '2*1' && vals[1].shape == '2*1') {
                            var temp_data = {
                                'vals': vals
                            };
                            html += template('b_module', temp_data);
                        } else if (vals.length == 4 && vals[0].shape == '1*1' && vals[1].shape == '1*1' && vals[2].shape == '1*1' && vals[3].shape == '1*1') {
                            var temp_data = {
                                'vals': vals
                            };
                            html += template('d_module', temp_data);
                        } else if (!!vals && vals.length <= 3 && vals[0].shape == '1*1') {
                            var temp_data = {
                                'vals': vals,
                                'page': page
                            };
                            html += template('f_module', temp_data);
                        } else if (!!vals && vals.length <= 3 && vals[0].shape == '2*1') {
                            var temp_data = {
                                'vals': vals,
                                'page': page
                            };
                            html += template('e_module', temp_data); //f_module,e_module模型是两个未填充满的模型
                        }

                        if (page == 1 && (i == 0 || i == 1)) {
                            $('.home-content-2').append(html);
                        } else if (page == 1 && i == 2) {
                            $('.home-content-3-2').append(html);
                        } else if (page == 1 && i == 3 && adv_flag == 1) {
                            var temp_data = {
                                'vals': vals,
                            };
                            var html = template('pre_tmpl', temp_data);
                            $('.home-content-ad').prepend(html);
                        } else {
                            append_html += html;
                        }
                    });

                    left_type = data.data.left_type;
                    common._loaddingClose();
                    $(".home-content-4").append(append_html);
                    // likeicon();

                    if (data.data.is_last_page == 1) {
                        $(".click-more").hide();
                    } else {
                        $(".click-more").show();
                    }

                }
            },
            error: function () {
                common._loaddingClose();
                $(".click-more").show();
            }
        });
    }

    //点击下一页
    $("#more_page").on('click', function () {
        page = page + 1;
        getList(page);
    });

    function likeicon() {
        if ($('.like-icon')) {
            var items = $('.like-icon');
            items.each(function () {
                // var likeIcon = $(this).find($('i.icobutton'));
                var likeIcon = $(this);
                mo_init(likeIcon);
            })
        }
    }
</script>
