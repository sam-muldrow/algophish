<script>
$(function(){
	$('.header .search-sub').click(function(){
		$('.search').show();
		$('.nav').hide();
		$(this).hide();
	})
	$('.header .search-3').click(function(){
		$('.search').hide();
		$('.nav').show();
		$('.search-sub').show();
	})

})

//将头部的评论，点赞的红点去掉
function removeRedSpots(obj){
	var obj = $(obj);
	var data_type = obj.attr("data_type");
	var url = "/message/reduceRedSpots?data_type="+data_type;
	$.ajax({
        url:url,
        async:false,
        beforeSend:function(){
        },
        complete:function(){
        },
        success:function(data){
        	if(data.status=='success'){
            	obj.children('.dot-icon').remove();
            	if($('#header_message_content .dot-icon').length==0){
               		$('#header_msg1 .bells-spot').remove();
                }
        	}
   		},
    });
}

function toPrivateLetter(){
	var data={
            index:3,
            getdata:'getMessagePrivateLetter'
    }

    sessionStorage.setItem('notificationnav', JSON.stringify(data))
	window.location.href='/message';
}

function toMessage(){
	var data={
            index:0,
            getdata:'getMessageNotice'
    }

   sessionStorage.setItem('notificationnav', JSON.stringify(data))
   window.location.href='/message';
}
</script>
