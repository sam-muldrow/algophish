<script type="text/javascript">
$(document).ready(function(){
if (window.viewportSize.getWidth() > 768) {
if(window.location.hash === '#/findaplan'){
$('#autoLaunchPlanPicker').click();
}
}
var changeBusinessType = function(businessType){
if (businessType == "Business") {
$('.select-business').show();
$('.select-education').hide();
$('.select-nonprofit').hide();
} else if (businessType == "Education") {
$('.select-education').show();
$('.select-business').hide();
$('.select-nonprofit').hide();
} else if (businessType == "Nonprofit") {
$('.select-nonprofit').show();
$('.select-business').hide();
$('.select-education').hide();
} else {
$('.select-business').hide();
$('.select-education').hide();
$('.select-nonprofit').hide();
}
};
$("#businesstype").change(function() {
changeBusinessType($(this).val());
});
changeBusinessType($("#businesstype").val());
});
</script>
