		<script>
	const ch = (r) => {
		const uri = window.location.toString();
		const a = uri.split('//')[1].split('/')[2];

		if (r)
			window.location = '/s/' + a + '/' + r;
		else
			window.location = '/s/' + a;
	};

	$().ready(function () {
		$('#dist').on('change', function (e) {
			e.preventDefault();
			window.location = $(this).val();
		});

		// $('#q').autocomplete({
