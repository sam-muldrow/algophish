		// get the logged in user
		$('.flag').on('click', function (e) {
			e.preventDefault()

			//if (!confirm('Report this listing?')) return;

			var t = $(this)
			var l = t.attr('data-id')

			console.log(t.attr('data-id'))

			$.getJSON('/api/2013-03-11/complaint.php', {id: l}, function (d) {
				console.log(d)
				if (d.deletedIds) {
					console.log(d.deletedIds)
					for (var c = 0; c < d.deletedIds.length; c++)
						$('#' + d.deletedIds[c]).css('background', '#f77').fadeOut()
				}
				else
					t.text(d.reason).delay(10000).fadeOut()
			})
		})
	})

		var pageOptions = {
