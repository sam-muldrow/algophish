function tab_img_change2(t){
	   for(var i = 1; i <= 5; i++) {
$("tab_bbsi+i+").css("background-color","red");

		  eval("document.getElementById('tab_viewi"+i+"')").style.display="none";

		 if ( t == i ) {
$('tab_bbsi2').css("background-color","red");
		  eval("document.getElementById('tab_viewi"+i+"')").style.display="";
		 }         
	   }     
	}
