function insertCookieHome(cookieName,cookieValue,domain) {
CookiePolicy.setCookie(cookieName,cookieValue,domain,365, "lax");
}
function redirect(domain, site, lang){
if(window.location.pathname !== '/vgn-ext-templating/v/index.jsp') {
let default_domain = ".caixabank.es";
let default_site = "PART";
let default_lang = "esp";
redirects = getRedirectsVar();
if (typeof domain === "undefined" || domain === null)
domain = default_domain;
if (typeof site === "undefined" || site === null)
site = default_site;
if (typeof lang === "undefined" || lang === null)
lang = default_lang;
if (redirects[domain] === undefined || redirects[domain][site] == undefined || redirects[domain][site][lang] === undefined) {
domain = default_domain;
site = default_site;
lang = default_lang;
}
let redirectVar = window.location.host.indexOf("pre.") === 0 ? "pre." + redirects[domain][site][lang] : redirects[domain][site][lang]
redirectVar = "//" + redirectVar;
location.replace(redirectVar);
}
}
function seeIdiomaAndDomainAndThenRunTheRedirect() {
let centorno = "";
let cidioma = "";
let newCookie = "";
if (CookiePolicy && CookiePolicy.isAllowedComponent('NeoCampaingsPesonalization')) {
centorno = CookiePolicy.getCookie('prefix');
cidioma = CookiePolicy.getCookie('idioma');
newCookie = CookiePolicy.getCookie('LOCan');
}
let pidioma='';
let query=this.location.search.substring(1);
let params=query.split("&");
let htmlLang=$("html").attr("lang");
let actualDomain = CookiePolicy.getDomain();
for (var i = 0; i < params.length; i++) {var params2=params[i].split("=");if (params2[0]=="id_idioma") pidioma=params2[1];}
let idioma='';
let listIdiomas = {"esp":"esp","es":"esp","ca":"cat","cat":"cat","en":"en","fr":"fr","de":"de","va":"va","eu":"eu","gl":"gl","1":"esp","2":"cat","3":"en","4":"fr","5":"de","6":"va","10":"eu","11":"gl"};
let listNewCookiesEmpresas = ["NFD","NPH","DayOne","AGR","NBE","NEG"];
let listNewCookiesParticulares = ["WEA","PRIV","PRE","HOL","FSE","FAM","MIC","INT","IMA","NOW"];
if(pidioma !== '') {
if(listIdiomas.hasOwnProperty(pidioma))
idioma = listIdiomas[pidioma];
}
if(idioma === '') {
if(cidioma && listIdiomas.hasOwnProperty(cidioma))
idioma = listIdiomas[cidioma];
}
if (idioma === '' && htmlLang)
idioma=listIdiomas[htmlLang];
if(idioma === '') idioma=listIdiomas["es"];
if (newCookie && newCookie !=='') centorno = listNewCookiesEmpresas.indexOf(newCookie) !== -1 ? "EMP" : (listNewCookiesParticulares.indexOf(newCookie) !== -1 ? "PART" :"");
centorno = centorno ? centorno : "PART";
if(cidioma !== idioma) {
insertCookieHome('idioma', idioma,actualDomain);
}
redirect(actualDomain, centorno, idioma);
}
try{
seeIdiomaAndDomainAndThenRunTheRedirect();
}catch(error) {
console.error("Error running the function : ",error);
console.info("Using the default redirect");
redirect(null,null,null);
}
</script>
</div>
