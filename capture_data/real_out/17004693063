                 el: ".swiper-pagination",
                 clickable: true,
             },
             breakpoints: {
                 640: {
                     slidesPerView: 2,
                 },
                 768: {
                     slidesPerView: 4,
                 },
                 1024: {
                     slidesPerView: 6,
                 },
             },
         });
        });
    </script>
