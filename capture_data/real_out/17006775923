<script>
    var slideStarted = false;
  	// 12 seconds
    var timePerSlide = 12000;
    var time = 0;
    var currentSlide = 0;
    var maxSlideLength = $('.custom-solution-carousel__solutions-container').length;
	var intervalRate = 10;
    var tick = setInterval(interval, intervalRate);
  
  	var solutionsContainerClass = 'custom-solution-carousel__solutions-container';
  	var detailsContainerClass = 'custom-solution-carousel__details-container';
    var progressBarClass = 'custom-solution-carousel__progress-bar-fill';
  
  	function interval() {
      	if(time >= timePerSlide) {
          	time = 0;
          	currentSlide += 1;

          	if(currentSlide >= maxSlideLength) {
            	currentSlide = 0;
          	}
        	setSlide(currentSlide);
      	}
    
    	time += intervalRate;
    	var percent = (time / timePerSlide) * 100;
    
    	$( '.' + progressBarClass ).css( 'width', percent + '%' );
  	}
  
  	function setSlide(slide) {
    	$( '.' + solutionsContainerClass ).removeClass( solutionsContainerClass + '--active' );
    	$( $( '.' + solutionsContainerClass )[slide] ).addClass( solutionsContainerClass + '--active' );
    
    	$( '.' + detailsContainerClass ).removeClass( detailsContainerClass + '--active' );
    	$( $( '.' + detailsContainerClass )[slide] ).addClass( detailsContainerClass + '--active' );
  	}
  
  	$( '.' + solutionsContainerClass ).click(function() {
        var index = $( '.' + solutionsContainerClass ).index(this);
        setSlide(index);
        $( '.' + progressBarClass ).css( 'width', '100%' );
        clearInterval(tick);
  	});
  
  
</script>

