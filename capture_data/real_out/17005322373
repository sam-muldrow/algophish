<script>
    var timer;
    var $window = $(window);
    var winHeight, scrollDistance;
    winHeight = window.innerHeight ? window.innerHeight : $window.height(),
            scrollDistance = $window.scrollTop() + winHeight;

    function showZoneDiv($this) {
        var zid = $this.attr("data-zid");
        $(".menu-lv-zone>li").addClass('inactive');
        $(".menu-lv-zone>li").removeClass('active');
        $this.removeClass('inactive');
        $this.addClass('active');
        $(".mega-info-unit").hide();
        $("#zone-" + zid).show();
        showMenuInfo();
    }

    $(document).ready(function () {
        $(".menu-lv-zone>li:not(.item-empty) .column-left, .menu-lv-zone>li:not(.item-empty) .menu-zone-icon").hover(
                function () {
                    $this = $(this).closest("li:not(.item-empty)");
                    timer = setTimeout(function () {
                        showZoneDiv($this);
                    }, 250);
                }
                , function () {
                    clearTimeout(timer);
                }
        );
        $(".menu-mega").hover(function () {
        }, function () {
            hideMenuInfo()
        });
        $(".menu-lv-zone>li.item-empty").hover(function () {
            hideMenuInfo()
        }, function () {
        });

        var _home_timer_tab;
        $(".nav:not(.new-nav-tabs) li a").hover(
                function () {
                    var self = this;
                    _home_timer_tab = setTimeout(function () {
                        $(self).trigger('click')
                    }, 500);
                }, function () {
                    clearTimeout(_home_timer_tab);
                });
    });

    function showMenuInfo() {
        $(".menu-mega-info").show();
    }

    function hideMenuInfo() {
        setTimeout(function () {
            $(".menu-mega-info").hide();
            $(".menu-lv-zone>li").removeClass('inactive');
            $(".menu-lv-zone>li").removeClass('active');
        }, 100);
    }
</script>
