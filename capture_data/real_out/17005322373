<script>
var final_group = 1;
var lotame_has_change_tpl = false;
checkLotameGroup('female', function(group, result){
    temp_val = (result) ? lotame_group_config[group]['cookie_group_id'] : lotame_group_config['default']['cookie_group_id'];
    if ($.cookie('nw_group_id') === null || $.cookie('nw_group_id') === '') {
        $.cookie('nw_group_id', temp_val, {domain: '.price.com.hk', path:'/', expires:
            1
        });
    }
    if(result) {
        final_group = lotame_group_config[group]['cookie_group_id'];
        switchLotameTemplate(group);
    }
});
if(final_group == 1){
    switchLotameTemplate('default');
}
if(lotame_has_change_tpl){
    var tracked_elm = $(".tab-wrapper>div .profile-list-item-list .list-fav-product>ul:visible");
    var marks = calculateMarks();
    $window.off('scroll.scrollDepthImpress');
    $window.on('scroll.scrollDepthImpress', throttle(function() {
        var $window = $(window),
            docHeight = $(document).height(),
            winHeight = window.innerHeight ? window.innerHeight : $window.height(),
            scrollDistance = $window.scrollTop() + winHeight;
        // If all marks already hit, unbind scroll event\
        if (cache.length >= (tracked_elm.find('li[id^="track_"]:visible').find('.p img').length)) {
            $window.off('scroll.scrollDepthImpress');
            pushLastViewCount();
            return;
        }
        checkMarks(marks, scrollDistance);
    }, 500));
}
function checkLotameGroup(group, func){
    if(typeof lotame_group != 'undefined'){
        if(typeof lotame_group[group] != 'undefined'){
            var temp_flag = false;
            $.each(lotame_group[group], function(k, v){
                if($.inArray(v, lotaudsarr) >= 0){
                    temp_flag = true;
                }
            });
                        if(typeof func == 'function'){
                func(group, temp_flag);
            }
        }
    }
}
function switchLotameTemplate(group){
    var temp_obj_a = $('.nw-group[ref="'+group+'"]');
    $.each(temp_obj_a, function(k, v){
        var temp_obj = $(this);
        var temp_parent_obj = $(this).parent();
        var all_group = temp_parent_obj.find('.nw-group');
        var has_multi_group = false;
        var group_name_a = [];
        $.each(all_group, function(){
            var temp_ref = $(this).attr('ref');
            if(typeof temp_ref != 'undefined' && $.inArray(temp_ref, group_name_a) < 0){
                group_name_a.push(temp_ref);
            }
        });
        if(group_name_a.length > 1){
            has_multi_group = true;
        }
        if(has_multi_group){
            lotame_has_change_tpl = true;
            temp_parent_obj.find('.nw-group').hide();
            temp_parent_obj.find('.nw-group[ref="'+group+'"]').show();
        }
    });
}

function sendlotameTrackingImg(){
    var temp_path = "https://tr2.price.com.hk/aud/icon.gif?type=aud&so=w";
    if(typeof lotame_audience != 'undefined' && lotame_audience) {
        try {
            var pid = lotame_audience.getProfileId();
            var nwtc = "655c100db12ca9.56573704";
            var uid = "";
            var audience_a = lotame_audience.getAudiences();
            temp_path += "&pid=" + pid;
            temp_path += "&aud=" + audience_a.join('|');
            temp_path += "&nwtc=" + nwtc;
            if(uid){
                temp_path += "&uid=" + uid;
            }
            if(audience_a.length > 0) {
                var temp_img = new Image();
                temp_img.src = temp_path;
            }
        } catch (e){ }
    }
}
</script>
<script src="https://www.google.com/recaptcha/api.js?render=6LcJzIUUAAAAANHB3krufMWOoMi4_H-fSGq5wLp9"></script>
<script>
    grecaptcha.ready(function() {
        grecaptcha.execute('6LcJzIUUAAAAANHB3krufMWOoMi4_H-fSGq5wLp9', {action: 'homepage'}).then(function(token) {
            checkInvisibleRecaptcha(token);
        });
    });
    function checkInvisibleRecaptcha(token){
        $.ajax({
            url: '/ajax.php?ct=check_invisible_recaptcha',
            data: {
                "response": token
            },
            dataType: 'json',
            type: 'POST',
            beforeSend: function(xhr) {},
            success: function(response) {
                if(typeof response.status.tracking_image != 'undefined' && response.status.tracking_image){
                    var temp_img = new Image();
                    temp_img.src = response.status.tracking_image;
                }
            },
            complete: function(response) {}
        })
    }
</script>

    
