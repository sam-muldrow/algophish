    var cache = [],
            pid = [],
            tracked_elm = $(".tab-wrapper>div .profile-list-item-list .list-fav-product>ul:visible");
    var marks = calculateMarks();

    $window.on('scroll.scrollDepthImpress', throttle(function () {
        var $window = $(window),
                docHeight = $(document).height(),
                winHeight = window.innerHeight ? window.innerHeight : $window.height(),
                scrollDistance = $window.scrollTop() + winHeight;
        // If all marks already hit, unbind scroll event\
        if (cache.length >= (tracked_elm.find('li[id^="track_"]:visible').find('.p img').length)) {
            $window.off('scroll.scrollDepthImpress');
            pushLastViewCount();
            return;
        }
        checkMarks(marks, scrollDistance);
    }, 500));

    // $(window).resize(function() {});

    // Override plugin function
    function throttle(func, wait) {
        var context, args, result;
        var timeout = null;
        var previous = 0;
        var later = function () {
            previous = new Date;
            timeout = null;
            result = func.apply(context, args);
        };
        return function () {
            var now = new Date;
            if (!previous) previous = now;
            var remaining = wait - (now - previous);
            context = this;
            args = arguments;
            if (remaining <= 0) {
                clearTimeout(timeout);
                timeout = null;
                previous = now;
                result = func.apply(context, args);
            } else if (!timeout) {
                timeout = setTimeout(later, remaining);
            }
            return result;
        };
    }

    function calculateMarks() {
        var result = {};
        var last_ck1 = 0;
        $.each($('[id*="track_"]'), function (index, elem) {
            // var temp_pid = $(elem).attr('id').substr(6, 6), h, ck1, ck2;
            if ($(elem).length) {
                h = $(elem).height();
                ck1 = $(elem).offset().top;
                if (last_ck1 == 0)
                    last_ck1 = ck1;
                if (ck1 > last_ck1)
                    last_ck1 = ck1;
                if (ck1 == 0)
                    ck1 = last_ck1;
                if (ck1 < last_ck1)
                    ck1 = last_ck1;
                result[$(elem).attr('id')] = ck1;
            }
        });
        return result;
    }

    function checkMarks(marks, scrollDistance) {
        $.each(marks, function (key, val) {
            if (scrollDistance >= val) {
                pushViewCount(key);
            }
        });
    }

    function pushViewCount(key) {
        var target = tracked_elm.find('#' + key).find('.p img');
        $.each(target, function (i, el) {
            if ($.inArray(key + i, cache) <= -1) {
                el = $(el);
                if (el.attr('src') == undefined && tracked_elm.find('#' + key).closest('div[class^="tab-pane"]').is(":visible")) {
                    var url = el.data('src');
                    el.attr('src', url);
                    el.removeAttr('data-src');

                    if ($.inArray(key + i, cache) <= -1) {
                        cache.push(key + i);
                    }
                }
            }
        });

    }

    function pushLastViewCount() {
        var target = tracked_elm.find('.p img');
        $.each(target, function (i, el) {
            el = $(el);
            if (el.attr('src') == undefined) {
                var url = el.data('src');
                el.attr('src', url);
                el.removeAttr('data-src');
            }
        });
    }

    function switchTabPushViewCount(target_element) {
        var target = $('#' + target_element).find('.profile-list-item-list:visible .p img');
        $.each(target, function (i, el) {
            el = $(el);
            if (el.attr('src') == undefined && el.closest('div[class^="tab-pane"]').is(":visible")) {
                var url = el.data('src');
                el.attr('src', url);
                el.removeAttr('data-src');
            }
        });
    }

    // If all marks already hit, unbind scroll event
    if (cache.length >= (tracked_elm.find('li[id^="track_"]:visible').find('.p img').length)) {
        pushLastViewCount();
    } else {
        checkMarks(marks, scrollDistance);
    }

    $('.tab-product .nav-tabs > li > a').on('shown.bs.tab', function (e) {
        textOverflowEllipsis();
        switchTabPushViewCount($(this).attr('href').replace('#', ''));
    });
</script><script>
    (function() {
        if (typeof $('#home-theme-container') != 'undefined') {
            var temp_html = $('#home-theme-container').html();
            $(temp_html).prependTo('.page-view-body .page-view-respond-control');
        }

        
    })();
</script>


