$(document).ready(function() {
	var timer;
	$("#dlapp, .dlapp-box").mouseleave(function() {
	    timer = setTimeout(qr_fadeout, 200);
	}).mouseenter(function() {
	    clearTimeout(timer);
	    $('.dlapp-box').fadeIn(200);
	});

	function qr_fadeout() {
	    $('.dlapp-box').fadeOut(200);
	}

});

