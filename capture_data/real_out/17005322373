                                                  <div class="tab-news tab-outlet">
                                    <style>
.special-event-icon-text{
color:#eb2550;
}
.special-event-icon-text:hover{
color:#b31436;
}
</style>
<script>
var bannerLoopDuration = 5000;	//5s feature banner auto looping
var videoDuration = 0;
var hoverTimeOut = 500;			//trigger hover after 0.5s
var clickTimeOut = 5000			//click hold for 5s then restart looping
var hoverCounter = 0;			//counter for li tab hover
var bannerHoverCounter = 0;		//counter for banner hover
var hoverTrigger = 0;			//flag for checking effect active by click or hover
var clickTrigger = 0;			//counter for reactiving banner autoloop after click effect
var wrapperHover = 0;			//flag for checking user mouse over on banner wrapper area
var bannerLoopContent;
var has_SR1 = false;
var disable_video = false; 
var allowAutoLoop = true;

function restartBannerLooping(index){
	allowAutoLoop = true;
	$(".new-nav-tabs li a").eq(index).trigger('click');
	$("div.active_bar").finish().css({width: "0%",transition: "none"});
	clearInterval(bannerLoopContent);
	startBannerLooping();
}
function stopBannerLooping(index){
	allowAutoLoop = false;
	$(".new-nav-tabs li a").eq(index).trigger('click');
	$("div.active_bar").finish().css({width: "0%",transition: "none"});
	clearInterval(bannerLoopContent);
	$(".new-nav-tabs").find('.active_bar').eq(index).css({width: "100%",transition: "none"});
}
function appendAdSR1(index,title){
	if ($('#content-ad-24').find('div[display=null]').html() == undefined && has_SR1 == false){
		has_SR1 = true;
		allowAutoLoop = false;
		// $("#ad_div_sr1").children().last().remove();
		$('a[href="#tab-news-"]').addClass("previous");
		
		var last_tab = $(".new-nav-tabs li").last();
		var last_banner = $('.tab-news.tab-outlet .tab-pane').last();
		last_tab.find('.text').text(title);
		last_banner.find('div.ec-banner, div.tab-news-pane-wrapper').html($("#content-ad-24").html());

		$(".new-nav-tabs li").eq(index-1).after(last_tab);
		$('.tab-news.tab-outlet .tab-pane').eq(index-1).after(last_banner);

		$('#content-ad-24').remove();
		stopBannerLooping(index);
	}
}
</script>
<div class="tab-wrapper tab-banner-wrapper" role="tabpanel">
  <!-- Nav tabs -->
  <ul class="nav nav-tabs new-nav-tabs" role="tablist">
		
	<!--new tabs-->
	  			<li role="presentation">
			<a class="tab-title previous" href="#tab-news-" aria-controls="tab-news-" role="tab" data-toggle="tab"><span class="text">情報</span><span class="process_bar"><div class="active_bar"></div></span></a>
			<div class="p"><img data-src="https://tr2.price.com.hk/iconp.gif?type=ad&so=w&aid=news&act=im&pgtype=h&pos=1" /></div>
		</li>
		  	  				<li role="presentation" class="active">
