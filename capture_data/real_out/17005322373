function toggleLangMenu(){
       if ($(".lang-switch-button").hasClass("dropup")){
            $(".lang-switch-dropdown").slideUp("fast")
            $(".lang-switch-button").removeClass("dropup");
            $(document).off('click',closeLangMenu);
        }else{
            $(".lang-switch-dropdown").slideDown("fast",function(){
                $(document).on('click', closeLangMenu);
            });
            $(".lang-switch-button").addClass("dropup");
            var closeLangMenu = function(e){
                var dropdown = $(".lang-switch-dropdown");
                if(!dropdown.is(e.target)){
                    e.stopPropagation();
                    $(document).off('click',closeLangMenu);
                    $(".lang-switch-button").removeClass("dropup");
                    dropdown.slideUp("fast");
                }
            }
        }
};
function focusSearchBar(){
	$('#searchq').select().focus();
}
function callLogJsPhp(query){
	var search_section = 'A';
	var search_type = 'e';
	$.ajax({
		url: "/scripts/log.js.php?type=search&g=&q="+query+"&count=0&time=0&suggest=&s=C&gsacc=0&search_type="+search_type+"&search_section="+search_section+"&search_prod_count=0"
	});
}
</script>

