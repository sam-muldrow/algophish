<script>
var lotaudsarr = [];
if (typeof(Storage) !== "undefined") {
	if (localStorage.getItem("lotauds") != null) {
   		var lotaudsarr = localStorage.getItem("lotauds").split(",");
	}
}

var price_keys = [];
$.each(lotaudsarr, function (i,el){
	el = Number(el);
	price_keys.push(el);
});

$.ajax({
    url: '/ajax.php?ct=store_lotame',
    data: {personal_ids: price_keys},
    dataType: 'json',
    type: 'GET',
    success: function(data) {},
});
</script>
