<script>
function detectIE() {
    var ua = window.navigator.userAgent;

    var msie = ua.indexOf('MSIE ');
    if (msie > 0) {
        // IE 10 or older => return version number
        return parseInt(ua.substring(msie + 5, ua.indexOf('.', msie)), 10);
    }

    var trident = ua.indexOf('Trident/');
    if (trident > 0) {
        // IE 11 => return version number
        var rv = ua.indexOf('rv:');
        return parseInt(ua.substring(rv + 3, ua.indexOf('.', rv)), 10);
    }

    var edge = ua.indexOf('Edge/');
    if (edge > 0) {
       // Edge (IE 12+) => return version number
       return parseInt(ua.substring(edge + 5, ua.indexOf('.', edge)), 10);
    }

    // other browser
    return false;
}

if (detectIE() > 10 || !detectIE()){

var unreadTimeOut = 30000;
var currentPage = 'index';
var im_uid = $.cookie('ui_uid');
var tr_session = null || '';


var IMUrlPrice = 'https://www.price.com.hk/';
var IMUrlPriceCar = 'https://car.price.com.hk/';
var IMUrlSocket = 'wss://im2.price.com.hk/';

var IMUrlApi = 'https://apis.price.com.hk/im2/v1/';
var IMUrlApiUnread = '';
var IMUrlApiUnreadSource = 'https://apis.price.com.hk/im2/v1/chat/{SENDER_ID}/unread';
if (IMUrlApiUnreadSource.length > 0 && $.cookie('ui_uid') > 0){
    IMUrlApiUnread = IMUrlApiUnreadSource.replace('{SENDER_ID}', 'MEM' + $.cookie('ui_uid'));
}
var IMEntryApi = IMUrlPrice + 'im-entry.php';
var IMUrlApiGuestName = 'https://apis.price.com.hk/im2/v1/user/guest/';

var priceQuoteTrackUrl = 'https://tr.price.com.hk/icon.gif';
var priceQuoteTrackImUrl = priceQuoteTrackUrl + "?type=p&v=2&tr_so=w&tr_ref=hcu&tr_ti=0&tr_h=%7BHASH%7D&tr_act=pl&tr_des=i_%7Clgen_0%7Chcupos_";
var priceAjaxUrl = IMUrlPrice + 'ajax.php';


$(document).ready(function(){
    loginCheckInterval = setInterval(function(){
        if (im_uid != $.cookie('ui_uid')){
            reInitIM();
        }
    }, loginCheckTimeOut);

    if (currentPage == 'product'){
        var quoteMerchantIds = [];
        $.each($('.product-list-item-list').find('li'), function(i,el){
            var listMcId = $(el).attr('name');
            if (typeof listMcId != 'undefined' && listMcId.length > 0 && !isNaN(parseInt(listMcId))){
                quoteMerchantIds.push(listMcId);
            }
        });
        getMcImStatus(quoteMerchantIds);
    }
    
    if (typeof merchantPage == 'undefined' || !merchantPage){
        if (currentPage == 'starshop'){
            var pageMcId = 0;
            if (parseInt(pageMcId) > 0){
                getMcImStatus([pageMcId]);
            }
        }
    }

    $("#imImgIframe").fancybox({
        type: "image",
        openEffect	: 'none',
        closeEffect	: 'none',
        helpers:  {
            thumbs : {
                width: 50,
                height: 50
            }
        }
    });
});

}
