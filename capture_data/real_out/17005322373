<script>

var tag = document.createElement('script');
tag.src = "//www.youtube.com/player_api";
var firstScriptTag = document.getElementsByTagName('script')[0];
firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

var player;
var YT_Player;
function onYouTubePlayerAPIReady(){
	YT_Player = new YT.Player('streaming-video', {
    events: {
      // call this function when player is ready to use
      'onReady': onPlayerReady
    }
  });
}
function onPlayerReady(event){
	player = YT_Player;
}
//$(document).ready(function(){
		var width = 0;
	var hoverBuffer;
	var clickBuffer;	
	
	$(".home-wrapper .nav-tabs > li > a").on("shown.bs.tab", function (e) {
		textOverflowEllipsis();
	});
	//flag for ad news
	if (!has_SR1){
				if (allowAutoLoop){
			//start looping when page loaded 
			startBannerLooping();
		}
			}
	
	bannerImpressionGa($(".new-nav-tabs li.active a"), $(".new-nav-tabs li.active a").attr('href'));
	//nav tab onclick
	$(".new-nav-tabs li a").click(
		function(){
			var tmp_id = $(this).attr('href');
			if (!$(tmp_id).hasClass('active')){
				bannerImpressionGa(this,tmp_id);
			}
			var prev = $(".new-nav-tabs li a").removeClass().addClass("tab-title");
			$(this).parent().prevAll().children().addClass("tab-title previous");
			$("div.active_bar").stop();
			//onclick progress bar effect
			if (hoverTrigger == 0){
				if (allowAutoLoop){
					clearInterval(bannerLoopContent);
					bannerLoopContent = null;
				}
				if(!disable_video){
					var tab_type = $(this).data("type");
					if(typeof tab_type !== 'undefined' && tab_type == 'streaming' && reset_video == 0){
					if(player){
						player.playVideo();
					}
				    reset_video = 1;
				  }else if(typeof tab_type === 'undefined' || tab_type != 'streaming'){
						if(player){
							if(player.getPlayerState() == 1){
								player.pauseVideo();
							}
							reset_video = 0;
						}
				  }
				}
				$("div.active_bar").finish().css("width","0%");//clear animation and set loader = 100%
				$(this).find('.active_bar').css("width","100%");
				clickTrigger = 0;
				//restart 5s looping if onclick and leave hover within 0.5s
				if (clickBuffer){
					clearInterval(clickBuffer);
					clickBuffer = null;
				}
				clickBuffer = setInterval(function(){
					if (clickTrigger >= clickTimeOut){
						clearInterval(clickBuffer); 
						clickBuffer = null;
						if (allowAutoLoop){
							clearInterval(bannerLoopContent);
							bannerLoopContent = null;
							$("div.active_bar").css({transition: 'none'});
							if (hoverTrigger == 0)
								startBannerLooping(); // restart looping
						}
					}	
					else {
						clickTrigger=clickTrigger+100;
					}
				}, 100);
			}
	});
	
	//banner wrapper hover
	$('.tab-banner-wrapper').hover(
		function(){
			wrapperHover = 1;
		},
		function(){
			wrapperHover = 0;
			
			if (clickBuffer){
				clearInterval(clickBuffer);
				clickBuffer = null;
			}
			if (allowAutoLoop){
				clearInterval(bannerLoopContent);
				bannerLoopContent = null;
			}
			$("div.active_bar").css({width: "0%",transition: "none"});
						if(!disable_video){
				var tab_type = $(".tab-news li.active").find("a").data("type");
				if(typeof tab_type !== 'undefined' && tab_type == 'streaming'){
					var vid = document.getElementById("streaming-video");
					bannerLoopDuration = 5000;
					duration = bannerLoopDuration + getStreamingVideoRemainTime();
					bannerLoopDuration = duration;
				}
			}
			if (allowAutoLoop){
				startBannerLooping(); // restart looping
			}else{
				$(".tab-wrapper .new-nav-tabs li.active a span div").css("width","100%");
			}
		}
	);
	
	//nav tab hover, freeze auto looping. Looping will be restarted when mouse leaving tab/image
	$(".new-nav-tabs li a").hover(
		function(){
			var self = this;
			// set interval (count on second) ==> time spent (on hover)
			if (hoverBuffer == null){
				hoverBuffer = setInterval(function(){
					if (hoverCounter >= hoverTimeOut){
						hoverTrigger = 1;
						clearInterval(hoverBuffer); 
						hoverBuffer = null;
						if (allowAutoLoop){
							clearInterval(bannerLoopContent);
							bannerLoopContent = null;
						}
						if(!disable_video){
							var tab_type = $(self).data("type");
							if(typeof tab_type !== 'undefined' && tab_type == 'streaming' && reset_video == 0){
								if(player){
									player.playVideo();
								}
						    reset_video = 1;
						  }else if(typeof tab_type === 'undefined' || tab_type != 'streaming'){
						  	if(player){
						  		//vid.pause();
						  		if(player.getPlayerState() == 1){
										player.pauseVideo();
									}
									reset_video = 0;
						  		bannerLoopDuration = 5000;
						  		clearInterval(bannerLoopContent);
									bannerLoopContent = null;
									startBannerLooping();
						  	}
						  }else{
						  	if(player){
						  		if(player.getPlayerState() == 2){
										player.playVideo();
									}
								}
						  }
						}
						$("div.active_bar").finish().css({width: "0%",transition: "none"});//clear animation and set loader = 100%
						$(self).find('.active_bar').css("width","100%");
						$(self).trigger("click");
					}
					else {
						hoverCounter=hoverCounter+100;
					}
				}, 100);
			}
			else {
				return false;
			}
		},
		function(){
			hoverTrigger = 0;
			clearInterval(hoverBuffer);
			hoverBuffer = null;
			if (wrapperHover == 0){
				// hovered 0.5s, restart 5s looping
				if (hoverCounter>=hoverTimeOut){
					if (clickBuffer){
						clearInterval(clickBuffer);
						clickBuffer = null;
					}
					if (allowAutoLoop){
						clearInterval(bannerLoopContent);
						bannerLoopContent = null;
					}
					$("div.active_bar").css({width: "0%",transition: "none"}); //set all loader to 0%
										if (allowAutoLoop){
						startBannerLooping(); // restart looping
					}else{
						$(this).find('.active_bar').css("width","100%");
					}
				}
			}
			hoverCounter = 0; //reset counter 
		}
	);

	//special hover for Banner and news titles
	$("div.ec-banner, div.tab-news-pane-wrapper").hover(
		function(){
			var self = this;
			if (hoverBuffer == null){
				hoverBuffer = setInterval(function(){
					if (bannerHoverCounter >= hoverTimeOut){
						hoverTrigger = 1;
						clearInterval(hoverBuffer); 
						hoverBuffer = null;
						if (allowAutoLoop){
							clearInterval(bannerLoopContent);
							bannerLoopContent = null;
						}
						$("div.active_bar").finish().css("width","0%");//clear animation and set loader = 100%
						$(".tab-news li.active span.process_bar div.active_bar").css("width","100%");
						$(self).trigger("click");
					}	
					else {
						bannerHoverCounter=bannerHoverCounter+100;
					}
				}, 100);
			}
			else{
				return false;
			}
		},
		function(){
			// reset time to 0 when cursor is leaving the area
			hoverTrigger = 0;
			clearInterval(hoverBuffer); 
			hoverBuffer = null;
			if (wrapperHover == 0){
				// hovered 0.5s, restart 5s looping
				if (bannerHoverCounter>=hoverTimeOut){
					if (clickBuffer){
						clearInterval(clickBuffer);
						clickBuffer = null;
					}
					if (allowAutoLoop){
						clearInterval(bannerLoopContent);
						bannerLoopContent = null;
					}
					$("div.active_bar").css({width: "0%",transition: "none"});
										if (allowAutoLoop){
						startBannerLooping(); // restart looping
					}else{
						$(this).find('.active_bar').css("width","100%");
					}
				}
			}
			bannerHoverCounter = 0;
		}
	);
//});
var reset_video = 0;
//auto looping class control and active process bar
//for controling the direction of progress: 
//start progress bar: right = '' ; close progress bar: right:'0'
function startBannerLooping() {
		//start progress bar when function called 
	var duration = bannerLoopDuration;
	if($(".tab-news li.active")[0] == $(".tab-news li:first")[0] && news_triple_time){
		duration = bannerLoopDuration * 3;
		news_triple_count = 1;
	}
	if(!bannerLoopContent && !disable_video){
		var tab_type = $(".tab-news li.active").find("a").data("type");
		if(typeof tab_type !== 'undefined' && tab_type == 'streaming' && reset_video == 0){
			if(player){
				if(player.getPlayerState() == 2 || player.getPlayerState() == 0){
					player.playVideo();
				}
			}
			reset_video = 1;
			bannerLoopDuration = 5000;
			duration = bannerLoopDuration + getStreamingVideoRemainTime();
			bannerLoopDuration = duration;
			var streaming_tab_index = $(".tab-news li.active").find('a').index()+1;
			stopBannerLooping(streaming_tab_index);
			restartBannerLooping(streaming_tab_index);
			return false;
		}else if(typeof tab_type == 'undefined' || tab_type != 'streaming'){
			bannerLoopDuration = 5000;
			if($(".tab-news li.active")[0] == $(".tab-news li:first")[0] && news_triple_time){
				bannerLoopDuration = bannerLoopDuration * 3
			}
			duration = bannerLoopDuration;
		}
	}

	$(".tab-news li.active").prev().find('a').addClass('previous');
	$(".tab-news li.active span.process_bar div.active_bar").css({"right":"","width":"0%"});
	$(".tab-news li.active span.process_bar div.active_bar").animate({width: "100%"}, duration, "linear");
	
	//autolooping content
	clearInterval(bannerLoopContent);
	bannerLoopContent = setInterval(function(){
		var duration = bannerLoopDuration;
		hoverTrigger = 1;
		var next_tab_obj = $(".tab-news li.active").next();
		if(!disable_video){
			var tab_type = next_tab_obj.find('a').data('type');
			if(typeof tab_type !== 'undefined' && tab_type == 'streaming' && reset_video == 0){
				if(player){
					player.playVideo();
				}
				reset_video = 1;
				bannerLoopDuration = 5000;
				duration = bannerLoopDuration + getStreamingVideoRemainTime();
				bannerLoopDuration = duration;
				var streaming_tab_index = next_tab_obj.find('a').index()+1;
				stopBannerLooping(streaming_tab_index);
				restartBannerLooping(streaming_tab_index);
			}else if((typeof tab_type === 'undefined' || tab_type != 'streaming') && bannerLoopDuration != 5000){
				bannerLoopDuration = 5000;
				duration = bannerLoopDuration;
				var streaming_tab_index = next_tab_obj.find('a').index()+1;
				stopBannerLooping(streaming_tab_index);
				restartBannerLooping(streaming_tab_index);
			}else if(typeof tab_type !== 'undefined' && tab_type == 'streaming'){
				if(player){
					if(player.getPlayerState() == 2){
						player.playVideo();
						reset_video = 1;
						bannerLoopDuration = 5000;
						duration = bannerLoopDuration + getStreamingVideoRemainTime();
						bannerLoopDuration = duration;
						var streaming_tab_index = next_tab_obj.find('a').index()+1;
						stopBannerLooping(streaming_tab_index);
						restartBannerLooping(streaming_tab_index);
					}
				}
			}
		}

		if($(".tab-news li.active")[0] == $(".tab-news li:first")[0] && news_triple_count < 3 && news_triple_time){
			news_triple_count = news_triple_count + 1;
		}else{
			//close last progress bar 
			$(".tab-news li.active span.process_bar div.active_bar").css("right","0");
			$(".tab-news li.active span.process_bar div.active_bar").css({width: "0%",transition: "all 0.3s ease"});
			//Find next li tab. If it is the last li, find the first li.
			if(next_tab_obj.length == 0){
				$(".tab-news li:first").find("a").trigger("click");
				if(news_triple_time){
					duration = bannerLoopDuration * 3;
					news_triple_count = 1;
				}
			}else{
				$(next_tab_obj[0]).find("a").trigger("click");
			}
			//start new progress bar 
			$(".tab-news li.active span.process_bar div.active_bar").animate({width: "100%"}, duration, "linear");
			$(".tab-news li.active span.process_bar div.active_bar").css({"right":"",transition:"none"});	
		}
	}, bannerLoopDuration);
}

function bannerImpressionGa(el,tmp_id){
	var tmp_title = $(el).find('.text').html();
	var temp_index = $(el).parent().index();
	var tmp_href = $(tmp_id).find('.banner-path').attr('href');
	tmp_href = (tmp_href)?tmp_href:'""';
	if (temp_index != 0){
		ga_trackEvent(null, 'topbanner-impression', 'feature_banner-'+temp_index+', '+tmp_title+', '+tmp_href);
	}
    var temp_tracking_obj = $(el).next('.p');
    var temp_tracking_img_obj = temp_tracking_obj.find('img');
    if(typeof temp_tracking_img_obj != "undefined"){
        $.each(temp_tracking_img_obj, function(k, v){
            var temp_src = $(this).data('src');
            if(typeof temp_src != 'undefined'){
                var new_img = new Image();
                new_img.src = temp_src;
            }
        });
    }
	delete tmp_title,temp_index,tmp_href;
}
var news_triple_time = false;
var news_triple_count = 0;
function startNewsAdBanner(){
	news_triple_time = true;
	var tmp_clickTimeOut = clickTimeOut;
	clickTimeOut = 0;
	bannerLoopDuration = 5000;
	
		if (!has_SR1){
		$(".new-nav-tabs li a").eq(0).trigger('click');
		$("div.active_bar").finish().css({width: "0%",transition: "none"});
	}
	setTimeout(function(){
		clickTimeOut = tmp_clickTimeOut;
	}, 1000);
	}


function getStreamingVideoRemainTime(){
	return Math.ceil(48 - player.getCurrentTime()) * 1000;
}
function bannerClick(link, tracking_url, category, action){
	if(typeof tracking_url != 'undefined'){
        var new_img = new Image();
        new_img.src = tracking_url;
	}
	return ga_trackEvent(link, category, action);
}
</script>                                </div>
