	<script>

	var SUBSCRIPTION_UPDATED_INDEX = parseInt("3");
	var SERVICE_WORKER_VERSION = parseInt("4");

	// content type
	var contentType = undefined;

	// model ids for tracking
	var modelIds = [];

    
    



    
    



	var host = window.location.hostname;

	if (!window.isBot) { // 

		if (isIOS === undefined || isIOS !== true) {

			if (host === 'www.hdblog.it') {

				if ('serviceWorker' in navigator && 'PushManager' in window) {

					navigator.serviceWorker.register('/serviceworker.js?v=4');

					navigator.serviceWorker.ready.then(function(reg) {

						console.log('Service Worker is registered', reg);

						subscribeUser(reg);
					})
						.catch(function (error) {
							console.error('Service Worker Error', error);
						});

				} else if (isSafari) {

					if (pushn_safari_web_push_id && pushn_safari_web_service_url) {

						subscribeSafari(pushn_safari_web_push_id, pushn_safari_web_service_url);
					}
				}
			}
		} else {
			if ('serviceWorker' in navigator) {

				navigator.serviceWorker.getRegistrations().then(function(registrations) {

					//for(let registration of registrations) {
					for(var i = 0; i < registrations.length; i++) {

						var registration = registrations[i];

						registration.unregister()

					}}).catch(function(err) {

					console.log('Service Worker registration failed: ', err);

				});
			}
		}
	}

</script>

	
	
