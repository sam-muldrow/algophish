<script type="text/javascript">

	function findParentId (el, var_id) {

		if (el.id == var_id) {

			return el;
		}

		while (el.parentNode) {

			el = el.parentNode;

			if (el.id == var_id) {

				return el;
			}
		}

		return null;
	}

	setInterval(function(){buildZonasPositions();}, 300);

</script>


