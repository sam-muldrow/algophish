        <script  type="text/javascript"  >               //<![CDATA[
               $( function () {
                       
$('#switch-contrast-1').click(function(event) { //to high, add link
  event.preventDefault();
  var theme = "-"+$(this).attr('rel');
  addStyleSheet(theme);
  $("html").addClass("high-contrast");
  is_highContra = "1";
  setCookie1("highcontra",is_highContra,1);
});

$('#switch-contrast-2').click(function(event) {
  var theme = "-"+$(this).attr('rel');
  var highContraCSS_select=highContraCSS(theme);

  event.preventDefault();
  $(highContraCSS_select).remove();
  $("html").removeClass("high-contrast");
  is_highContra = "0";
  setCookie1("highcontra",is_highContra,1);
});

// Add id for GA
//Flipboard
function setCtrlIDforFlipboard() {
  $('.slider--flipboard > .slick-prev').attr('id', 'slidePrev');
  $('.slider--flipboard > .slick-next').attr('id', 'slideNext');

  $('.slider--flipboard > .slick-dots-container .slick-dots button').each(function(i) {
    $(this).attr('id', 'flipboard-page-' + (i + 1));
  });
}

setCtrlIDforFlipboard();

$('.slider--flipboard').on('setPosition', function() {
  setCtrlIDforFlipboard();
});

//Poster
$('.slider--poster > .slick-prev').attr('id', 'poster-highlight-prev');
$('.slider--poster > .slick-next').attr('id', 'poster-highlight-next');

//Photo
$('.slider--caphoto .js-slick-pause').attr('id', 'photo-pause');

$(document).on('click', '.slider--caphoto .js-slick-pause', function() {
  $(this).attr('id', 'photo-play');
});

$(document).on('click', '.slider--caphoto .js-slick-play', function() {
  $(this).attr('id', 'photo-pause');
});

$('.slider--caphoto .slick-dots button').each(function(i) {
  $(this).attr('id', 'photo-page-' + (i + 1));
});

//Hero slider
$('.slider--hero .js-slick-pause').attr('id', 'hero-slider-pause');

$(document).on('click', '.slider--hero .js-slick-pause', function() {
  $(this).attr('id', 'hero-slider-play');
});

$(document).on('click', '.slider--hero .js-slick-play', function() {
  $(this).attr('id', 'hero-slider-pause');
});

$('.slider--hero .slick-dots button').each(function(i) {
  $(this).attr('id', 'hero-slider-page-' + (i + 1));
});

// Fixed explore btn according to the screen height
var heroExploreBtn = $('.section-hero__explore');

$('.page-main').on('inview', function(event, isInView) {
  heroExploreBtn.delay(200).fadeIn(300);

  if (isInView) {
    heroExploreBtn.css('position', 'absolute');
  } else {
    heroExploreBtn.css('position', 'fixed');
  }
}).trigger('inview');

// Trigger play/pause when the slider is scrolled into / out of view
if($('.slider--caphoto .slider__item').length > 1) {
	$('.slider--caphoto').on('inview', function(event, isInView) {
		if (isInView) {
			if(!$('.slider--caphoto .js-slick-play').length) {
				$('.slider--caphoto').slick("slickPlay");
			}
		} else {
			$('.slider--caphoto').slick("slickPause");
		}
	}).trigger('inview');
}


    $('.facebook').attr('href',$('.facebook').attr('href')+encodeURIComponent(location.href));
    $('.twitter').attr('href',$('.twitter').attr('href')+encodeURIComponent(location.href));


                       });
