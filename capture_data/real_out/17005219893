    <script type="text/javascript">
        $(function () {
            $(".ban_nav").mouseover(function () {
                $(".menu", this).show()
            })
                .mouseout(function () {
                    $(".menu", this).hide()
                })

            function showMenu() {
                $(".bigdata").fadeIn(1000, function () {
                    $(".media").fadeIn(1000);
                    $(".advertisers").fadeIn(1000);
                    $(".precision").fadeIn(1000);
                });
            }

            function showeye() {

                $(".animate_cube .eye").animate({opacity: "1"}, 1500, function () {
                    $(".animate_cube").animate({
                        width: "400px",
                        height: "400px",
                        marginLeft: "-200px",
                        marginTop: "-250px"
                    }, 1000, showMenu());
                    $(".animate_cube ul").empty();
                })

            }

            function showCube() {
                var cubeLi = $('.animate_cube').find('li');
                var animate_cubeH = $(".animate_cube").height();
                var cubeW = animate_cubeH;
                var imgW = cubeW / 8;
                var marginH = cubeW / 2;
                $(".animate_cube").css({
                    width: cubeW,
                    left: "50%",
                    top: "50%",
                    marginTop: -marginH,
                    marginLeft: -marginH
                });

                cubeLi.filter(":even").css({top: "-479px"})
                cubeLi.filter(":odd").css({top: "479px"})
                cubeLi.css({opacity: "0", display: "block"})
                $(".animate_cube .eye").css({opacity: "0"});

                for (i = 0; i < cubeLi.length; i++) {
                    //alert(cubeW)
                    var oleft = imgW * i;
                    cubeLi.eq(i).css({left: oleft});
                    cubeLi.eq(i).animate({opacity: "1", top: "0"}, 1500)
                }
                setTimeout(function () {
                    showeye()
                }, 1500)

            }

            showCube();
        });

    </script>
