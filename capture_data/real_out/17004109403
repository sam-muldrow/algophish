<script type="text/javascript">
/*jQuery(document).ready(function() {
 setTimeout(function() {
      showinterstitialip();
  }, 1000);

});*/


function showinterstitialip() {
let todayDate = new Date();
var rehpmobcity= '';
if((localStorage.getItem('interstitialip2019') == null) && todayDate.toDateString() == "Sat Dec 14 2019" && rehpmobcity=='Bangalore') {

trigger_home_mobile_popup();

localStorage.setItem('interstitialip2019', true);
let timeleft = 6;
   let downloadTimer = setInterval(function(){
   timeleft--;
   document.getElementById("bannercoutdown").textContent = timeleft;
   if(timeleft == 0){
        document.getElementById('sg_sel_loc_closehome').click();
       jQuery("#sg_sel_loc_closehome").trigger('click');
    }
   if(timeleft <= 0)
       clearInterval(downloadTimer);

   },1000);
}
}


function trigger_home_mobile_popup(){

        var origin = window.location.host;
        var data   = {};
        var url    = "//"+origin+"/ip2/public/index.php/home-page-modal";

        jQuery('#homepagesection').load(url,data,function(result,status,xhr){
            //console.log('result');
            //console.log(result);

            if(result!=''){
                if (status == "success") {
                    setTimeout(function() {
                        jQuery('#mobile_popup').modal({show:true});
                    }, 600);
                }
            }
        });
    }

    function trigger_header_login_content(value,cityid,localityid,pageref){

        var origin = window.location.host;
        var data   = {};

        if(value!=''){
            data.pagefrom = value;

            if(value=='solidgroundspdf'){
                var date = new Date();
                date.setTime(date.getTime()+(1*5*60*1000));
                var expires = "; expires="+date.toGMTString();

                document.cookie = "solidgroundspdfcityref="+cityid+";domain=.indiaproperty.com;"+expires+";path=/";
                document.cookie = "solidgroundspdflocref="+localityid+";domain=.indiaproperty.com;"+expires+";path=/";
                document.cookie = "solidgroundspdfpageref="+pageref+";domain=.indiaproperty.com;"+expires+";path=/";
            }
        }
        var url    = "//"+origin+"/ip2/public/index.php/load-registration-signin-modal";

        jQuery('#signinmodaldynamiccontent').load(url,data,function(result,status,xhr){
            if (status == "success") {
                setTimeout(function() {
                    jQuery('#signin-modal').modal({show:true});
                }, 200);
            }
        });
    }

    function trigger_header_sign_in_verify_mobile(){

        var origin = window.location.host;
        var data   = {};
        var url    = "//"+origin+"/ip2/public/index.php/verify-registration-mobile-no";

        jQuery('#verifyregistrationmobileno').load(url,data,function(result,status,xhr){
            console.log('result');
            console.log(result);

            if(result!=''){
                if (status == "success") {
                    setTimeout(function() {
                        jQuery('#verifyregistrationmobileno-modal').modal({show:true});
                    }, 200);
                }
            }
        });
    }

    function loadscriptfileviajs(url, callback){
        
        var script = document.createElement("script")
        script.type = "text/javascript";

        if (script.readyState){  //IE
            script.onreadystatechange = function(){
                if (script.readyState == "loaded" || script.readyState == "complete"){
                    script.onreadystatechange = null;
                    callback();
                }
            };
        } else {  //Others
            script.onload = function(){
                callback();
            };
        }

        script.src = url;
        document.getElementsByTagName("head")[0].appendChild(script);
    }

    function getUrlVars()
    {
        var vars = [], hash;
        var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
        for(var i = 0; i < hashes.length; i++)
        {
            hash = hashes[i].split('=');
            vars.push(hash[0]);
            vars[hash[0]] = hash[1];
        }
        return vars;
    }

    var author = getUrlVars();

    if( author['registrationverifymobile']!= undefined && author['registrationverifymobile']!= 'undefined' ){
        setTimeout(function(){ trigger_header_sign_in_verify_mobile(); }, 4000);
    }
</script>

