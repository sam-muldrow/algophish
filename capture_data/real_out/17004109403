<script type="text/javascript" src="//static.ipstaticcnt.com/ip2/js/jquery.min.js"></script>
<script type="text/javascript" src="//static.ipstaticcnt.com/ip2/js/lib/jquery-ui-1.11.2.min.js"></script>
<script type="text/javascript" src="//static.ipstaticcnt.com/ip2/js/jquery.cookie.js"></script>
<script>
$( document ).ready(function() {
	var goptions = {
	  enableHighAccuracy: true,
	  timeout: 60000,
	  maximumAge: 0
	};
	function gsuccess(pos) {
	  var crd = pos.coords;
	  var geolat = crd.latitude;
	  var geolng = crd.longitude;
	  var geolatlng = crd.latitude+","+crd.longitude;
	  var geolatlngurl = "citypage/locationbasedcity.php"; 
	  $.ajax({
	 	url: geolatlngurl,
	 	data: "ll="+geolatlng, 
	 	success: function(result){
        	var parsedData = jQuery.parseJSON(result);
        	var cityid = parsedData['CityId'];
        	var cityname = parsedData['CityName'];
        	document.cookie = "geolatlng="+geolatlng;
        	document.cookie = "cityid="+cityid;
        	document.cookie = "cityname="+cityname;
        	if(result!='' && $.cookie("reloadcookie")!=1){
        		document.cookie = "reloadcookie=1";
        		location.reload();
        	}
    	}
	  });
	}
	function gerror(err) {
	  if(err!='' && $.cookie("reloadcookie")!=2){
      	document.cookie = "reloadcookie=2";
	  	console.warn(`ERROR(${err.code}): ${err.message}`);
      	location.reload();
      }
	}
	navigator.geolocation.getCurrentPosition(gsuccess, gerror, goptions);
});	


</script>


