            function startHomePlayer(StreamURL, playerBlock)
            {
                var checkm3u8=null ;
                let self = this;
                let isAddPlaying = false;
                var isStreamError = false;
                var isPause = false;
                var isBuffering = false;
                var playerSetup = jwplayer(playerBlock).setup({
                    playlist: [{
                        file: StreamURL,
                        //file: "https://yuppindott.akamaized.net/hls/live/2014935/abnottind/abnottind/playlist.m3u8?dw=60&hdnts=st=1594037131~exp=1594051531~acl=!*/2014935/abnottind/*!/payload/karbonn-minus_5_1422906_NA_IN_NA_karbonn-minus_1030/*~hmac=8d59cf9b65bfb0ad785fa629953b7616db98014d78218005e8359897201f365e&ud=1030_karbonn-minus_1422906_live_121.244.63.135_-1&b=100-2500",
                        provider: checkm3u8 == null ? '':"http://static.aka.yupp.yuppcdn.net/staticstorage/jwplayer/jw_7_4_4/3.10/AkamaiAdvancedJWStreamProvider.swf",
                        type: checkm3u8 == null ? '' : 'flv',
                        tracks: [{
                            file: '',
                            kind: "thumbnails"
                        }]
                    }],
                    visualplaylist: false,
                    advertising: this.jwAdvertising,
                    primary: 'html5',
                    abouttext: "YuppTV.com",
                    aboutlink: "http://www.yupptv.com",
                    width: "100%",
                    autostart: true,
                    preload: "auto",
                    related: {
                        displayMode: "none"
                    },
                    mute: false,
                    repeat: false,
                    nextUpDisplay: false,
                    aspectratio: '16:9',
                    events: {
                        adPlay: (data) => {

                            onAdPlay(data);
                        },
                        adSkipped: (data) => {
                            onAdSkipped(data);
                        },
                        adComplete: (data) => {

                            onAdCompleted(data);
                        },
                        adError: () => {

                            onAdError();
                        },
                        adClick: () => {

                            onAdClick();
                        },
                        time: () => {
                            jwplayerProgramchange()
                        },
                        firstFrame: () => {
                            initialSetBitRate();
                        },
                        levelsChanged: (data) => {
                            jwplayerSetBitRate(data)
                        },
                        play: (data) => {

                            onPlayStarted(data);
                        },

                        pause: (data) => {
                            ((!!isBuffering) ? self.playerSetup.play() : onPauseEvent());
                        },
                        buffer: (data) => {
                            OnBufferEvent(data);
                        }
                        ,
                        seek: (data) => {
                            onSeekEvent(data);
                        },
                        complete: () => {
                            onCompleteEvent();
                        },

                        error: (data) => {
                            OnErrorEvent(data);
                            if (data.code == 232404 || data.code == 230001 || data.code == 232001 || data.code == 230000) {
                                !!this.playerSetup && this.playerSetup.stop() && this.playerSetup.remove();
                                this.isStreamError = true;
                                this.errorMessage = data.message;
                            }
                        },
                        setupError: (data) => {
                            console.log("setuperror")
                        },
                    }

                });

                function onAdClick() {

                }
                function onAdError() {
                    console.log("ad error")
                }
                function onAdCompleted(data) {
                    self.isAdPlaying = false;
                    self.playerState = "adend";
                    self.plugin.handleAdCompleted({ adType: self.keyAdd[data.adposition] });
                }
                function onAdPlay(data) {
                    if (self.isBuffering) {
                        (self.plugin) != undefined && self.plugin != '' && self.plugin.handleBufferEnd();
                        self.isBuffering = false;
                    }

                    if (!self.isAdPlaying) {
                        self.isVideoPlaying = false;
                        self.isAdPlaying = true;
                        self.playerState = "adplaying";
                        self.plugin.handleAdStarted({ adType: self.keyAdd[data.adposition] });
                    }
                }
                function onAdSkipped(data) {

                    self.playerState = "adskipped";
                    (self.plugin) != undefined && self.plugin != '' && self.plugin.handleAdSkipped({ adType: self.keyAdd[data.adposition] });

                }
                function onPlayStarted(data) {
                    if (!self.isVideoPlaying) {

                        self.isVideoPlaying = true;
                        self.playerState = "playing";
                        (self.plugin) != undefined && self.plugin != '' && self.plugin.handlePlayStarted();

                    }
                    if (!!self.isVideoPlaying && self.isBuffering) {
                        self.isBuffering = false;
                        self.playerState = "playing";
                        (self.plugin) != undefined && self.plugin != '' && self.plugin.handleBufferEnd();

                    }
                    if (self.isPause) {
                        self.isPause = false;
                        self.playerState = "playing";
                        (self.plugin) != undefined && self.plugin != '' && self.plugin.handleResume();
                    }
                    initialSetBitRate();
                }


                function onSeekEvent(data) {
                    if (!!self.isVideoPlaying) {
                        (data.offset == 0) && (data.offset == 1)
                        self.playerState = "seeking";
                        self.plugin.handleSeek({ STPosition: Math.floor((data.position) * 1000), ETPosition: Math.floor((data.offset) * 1000) });

                    }

                }


                function onPauseEvent() {
                    if (!self.isPause) {
                        self.isPause = true;
                        self.playerState = "paused";
                        (self.plugin) != undefined && self.plugin != '' && self.plugin.handlePause();

                    }
                }

                function OnBufferEvent(data) {
                    if (!!self.isVideoPlaying && !self.isBuffering) {
                        self.isBuffering = true;
                        self.playerState = "buffering";
                        (self.plugin) != undefined && self.plugin != '' && self.plugin.handleBufferStart();

                    }
                }
                function OnErrorEvent(data) {
                    if ((self.plugin) != undefined && self.plugin != '') {
                        self.playerState = "error";
                        self.plugin.thumbnailVideoClick();
                        self.plugin.handlePlayerLoad();
                        self.plugin.handleError({ errorMsg: (!!data && !!data.message) ? data.message : "network error" });
                    }
                }
                function onCompleteEvent() //ENDEVENT
                {

                    self.playerState = "idle"
                    self.plugin.handlePlayCompleted();
                    contentPlayed(preProgramName, progGenre, progId, 'DD INDIA', 5521, 'English', '', 'home');

                }
                //  this.isEndedByUser = true;

                function jwplayerProgramchange()
                {
                    window.playerPosition = jwplayer('mediaspace').getPosition();
                    if (window.playerState == "seeking") {
                        window.playerState = jwplayer('mediaspace').getState();
                        console.log(window.playerState)
                    }

                    var currentDateTime = Date.now();
                    var programEndTime = 0

                    var currentTime = Date.now();
                    try{
                        programEndTime = 1700623800000;

                    }catch(e){

                    }
                    if ((currentTime >= programEndTime) && self.epgFlag) {
                        self.epgFlag = false;
                        isAutoplay = true;
                        //self.plugin.handlePlayCompleted();
                        //self.plugin.handlePlayStarted();
                        //self.getProgramDetails(String(5521));
                    }



                }
                function jwplayerSetBitRate(data) {
                    if (jwplayer('mediaspace').getVisualQuality() != null) {
                        var vBitrateObj = {
                            // bitRate: (data.levels[data.currentQuality].bitrate / 1000)
                            bitRate: (jwplayer('mediaspace').getVisualQuality().level.bitrate / 1000)
                        }
                        self.plugin.setBitRate(vBitrateObj);
                        self.plugin.handleBitRateChange(vBitrateObj);
                    }

                }
                function initialSetBitRate() {
                    //let bw = jwplayer('mediaspace').getQualityLevels();
                    //var levels = jwplayer('mediaspace').getQualityLevels().map(function (o) { return o.bitrate; })
                    //var level = levels[jwplayer('mediaspace').getCurrentQuality()];
                    if(jwplayer('mediaspace').getVisualQuality()!=null)
                    {
                        var level=jwplayer('mediaspace').getVisualQuality().level.bitrate;
                        var vBitrateObj = {
                            bitRate: (level / 1000)
                        }
                        self.plugin.setBitRate(vBitrateObj);
                    }
                }
                if(jwplayer('mediaspace').getVisualQuality()!=null)
                {
                    var vBitrateObj = {
                        bitRate: (jwplayer('mediaspace').getVisualQuality().level.bitrate/1000)
                    }
                    self.plugin.setBitRate(vBitrateObj);
                }
            }

            function playerEvents(ad) {
                var self = this;
                // Setting adType for Events
                var playerEventObj = {
                    adType: String("0")
                }

                this.player.ads.addEventListener('adbegin', function (e) {  // this.AD_STARTED = 3;
                    self.playerState = "adbegin";
                    self.plugin.handleAdStarted(playerEventObj);
                });

                this.player.ads.addEventListener('adskip', function (e) {  // this.AD_SKIPPED = 4;
                    self.playerState = "adskipped";
                    self.plugin.handleAdSkipped(playerEventObj);
                });

                this.player.ads.addEventListener('adend', function (e) {  // this.AD_COMPLETED = 6;
                    self.playerState = "adended";
                    self.plugin.handleAdCompleted(playerEventObj);
                });

                this.player.addEventListener('canplay', function (e) {  // this.VIDEO_STARTED = 7;
                    //self.plugin.thumbnailVideoClick();
                    //self.plugin.handlePlayerLoad();
                    //self.playerState = "playing";
                    self.plugin.handlePlayStarted();
                });

                this.player.addEventListener('ended', function (e) {   //  this.VIDEO_COMPLETED = 9;
                    self.playerState = "idle"
                    self.plugin.handlePlayCompleted();
                    contentPlayed(preProgramName, progGenre, progId, 'DD INDIA', 5521, 'English', '', 'home');
                });

                this.player.addEventListener('pause', function (e) {   //  this.VIDEO_PAUSE = 13;
                    self.playerState = "paused"
                    self.plugin.handlePause();
                });

                this.player.addEventListener('play', function (e) {   //  this.VIDEO_RESUME = 14;
                    self.plugin.handleResume();
                    self.playerState = "playing"
                })

                this.player.addEventListener('error', function (e) {   //  this.ERROR_MSGS = 16;
                    self.plugin.handleError({ errorMsg: 'Got Error Please Try After Some Time' });
                    self.playerState = "error"
                });

                this.player.addEventListener('waiting', function (e) {
                    self.playerState = "buffering";
                    self.plugin.handleBufferStart();
                });

                this.player.addEventListener('playing', function (e) {
                    self.playerState = "playing";
                    self.plugin.handleBufferEnd();
                    //changeActiveTrack();
                });
                player.network.addResponseInterceptor((response) => {
                    // If the response comes from a specific URL
                    if (response.url.includes("m3u8") && response.status == "404") {
                        self.playerState = "error";
                        self.plugin.handleError({ errorMsg: 'error 404' });
                    }
                    if (response.url.includes("m3u8") && response.status == "403") {
                        self.playerState = "error";
                        self.plugin.handleError({ errorMsg: 'error 403' });
                    }
                    if (response.url.includes("m3u8") && response.status == "402") {
                        self.playerState = "error";
                        self.plugin.handleError({ errorMsg: 'error 402' });
                    }
                });
            }

            

            function prepAdObj(adObject) {
                var obj = [];
                var self = this;

                function setAd(ad) {
                    if (ad.adType == '0') {
                        self.laterObj['0'] = 'https://pubads.g.doubleclick.net/gampad/live/ads?sz=640x480&iu=/28418187/YuppTV.com_Pre_Telugu&impl=s&gdfp_req=1&env=vp&output=vast&unviewed_position_start=1&url=__referrer__&description_url=http%3A%2F%2Fyupptv.com&correlator=__timestamp__'
                    }
                    else if (ad.adType == '1') {
                        var key = ad.duration1;
                        self.laterObj[key] = ad.adUrl;

                        if (!!ad.duration2) {
                            for (var i = ad.duration1 + ad.duration2; i <= 452; i = i + ad.duration2) {
                                self.laterObj[i] = ad.adUrl
                            }
                        }
                    }
                    else if (ad.adType == '2') {
                        var key = ad.duration1;
                        self.laterObj[key] = ad.adUrl;

                        if (!!ad.duration2) {
                            for (var i = ad.duration1 + ad.duration2; i <= 452; i = i + ad.duration2) {
                                self.laterObj[i] = ad.adUrl;
                            }
                        }
                    }
                    else if (ad.adType == '3') {
                        var key = ad.duration1;
                        self.laterObj[key] = ad.adUrl;
                    }
                }

                for (var i = 0; i < adObject.length; i++) {
                    adObject[i].adtype = "0";
                    setAd(adObject[i]);
                }
                return obj
            }

            

