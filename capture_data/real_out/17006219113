<div id="userAcceptanceModal" class="modal fade vc-modal" role="dialog" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog modal-lg vc-modal-dialog">
        <div class="modal-content">
            <div class="modal-body">
