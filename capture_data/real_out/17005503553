  <script type="text/javascript">
(function(){
function redirectToBigNoMember(){
const is_big_member = document.cookie.split('; ').find(row => row.startsWith('is_member'));
  if(!is_big_member || !is_big_member.split("=")[1]){
        window.location.replace('/big/index.html'+ document.location.search);
  }
}
function redirectToBigMember(){
const is_big_member = document.cookie.split('; ').find(row => row.startsWith('is_member'));
  if(is_big_member && is_big_member.split("=")[1]){
        window.location.replace('/big/member.html'+ document.location.search);
  }
}
function redirectToFMLanding(){
  const is_fp_visited = document.cookie.split('; ').find(row => row.startsWith('is_fp_visited'));
  const params = new URLSearchParams(document.location.search.substring(1));
  const cp = params.get("cp");
  const token = params.get("TOKEN");
    if((!is_fp_visited || !is_fp_visited.split("=")[1]) && cp == "fp" && token ){
        document.cookie = "is_fp_visited=1; max-age=31536000; path=/";
        window.location.replace('/landing/famipay/index.html'+ document.location.search);
  }
  	 
}
function redirectToWinnerMember(){
const is_winner_member = document.cookie.split('; ').find(row => row.startsWith('is_winner_member'));
  if(is_winner_member && is_winner_member.split("=")[1]){
        window.location.replace('/winner/member.html'+ document.location.search);
  }
}
function redirectToWinnerNoMember(){
const is_winner_member = document.cookie.split('; ').find(row => row.startsWith('is_winner_member'));
  if(!is_winner_member || !is_big_member.split("=")[1]){
        window.location.replace('/winner/index.html'+ document.location.search);
  }
}

function topRedirect(){
  if(document.location.pathname == "/big/member.html" 
	|| document.location.pathname == "/big/index.html"
	|| document.location.pathname == "/winner/member.html"
	|| document.location.pathname == "/winner/index.html"
	|| document.location.pathname == "/toto/index.html" ){
    redirectToFMLanding();
  }

  const params = new URLSearchParams(document.location.search.substring(1));
  const support = params.get("support");
  if(support != "xxx"){
		if(document.location.pathname == "/big/member.html" ){
		  redirectToBigNoMember();
		}
		if(document.location.pathname == "/big/index.html" || document.location.pathname == "/big/"  ){
		  redirectToBigMember();
		}
		if(document.location.pathname == "/winner/member.html" ){
		  redirectToWinnerNoMember();
		}
		if(document.location.pathname == "/winner/index.html" || document.location.pathname == "/winner/"  ){
		  redirectToWinnerMember();  
		}
  }
}
topRedirect();
}());
  </script>
