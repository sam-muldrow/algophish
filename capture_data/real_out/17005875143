<script>
var method="mouseenter";
if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
method="click";
}
//var h = 0;
var val = 0;
jQuery(document).ready(function(){
jQuery('.click').on(''+method+'',function() {
   if (jQuery('.pn-ProductNav_Contents ul#nav').hasClass('show')) {
    jQuery('.pn-ProductNav_Contents ul#nav').removeClass('show');
    jQuery('.pn-ProductNav_Contents ul#nav li.menu-item').removeClass('drop')
    return;
  } 
  val = -6;
  jQuery('.pn-ProductNav_Contents ul#nav li.menu-item').each(function() {
    var of = jQuery(this).offset().top - jQuery('#nav').offset().top;
	
    if ( of > 20) {
      jQuery(this).addClass('drop');	
      var menu_height=jQuery(this).height()+1; 
      jQuery(this).css('top', 'calc(100% + ' + val + 'px)');
      val += menu_height;
    }
    jQuery('.pn-ProductNav_Contents ul#nav').addClass('show');
    });
  });

jQuery('.pn-ProductNav_Contents').on('mouseleave',function() {
   if (jQuery('.pn-ProductNav_Contents ul#nav').hasClass('show')) {
   jQuery('.pn-ProductNav_Contents ul#nav').removeClass('show');
    jQuery('.pn-ProductNav_Contents ul#nav li.menu-item').removeClass('drop')
    return;
  } 
  })
  
});
</script>


