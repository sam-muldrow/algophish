            var NSC_pt_cy = (new Date).getFullYear();
            var NSC_pt_cm = (new Date).getMonth();
            var NSC_pt_cd = (new Date).getDate();
            var NSC_pt_cs = (new Date).getSeconds();
            var NCS_fajar_time   = new Date(NSC_pt_cy,(NSC_pt_cm+1),NSC_pt_cd,"05","20",NSC_pt_cs); //24 Hour Based
            var NCS_duhar_time   = new Date(NSC_pt_cy,(NSC_pt_cm+1),NSC_pt_cd,"12","07",NSC_pt_cs); //24 Hour Based
            var NCS_asr_time     = new Date(NSC_pt_cy,(NSC_pt_cm+1),NSC_pt_cd,"15","09",NSC_pt_cs); //24 Hour Based
            var NCS_maghrib_time = new Date(NSC_pt_cy,(NSC_pt_cm+1),NSC_pt_cd,"17","33",NSC_pt_cs); //24 Hour Based
            var NCS_isha_time    = new Date(NSC_pt_cy,(NSC_pt_cm+1),NSC_pt_cd,"18","50",NSC_pt_cs); //24 Hour Based

            RunClientPrayerTimes(1);
        </script>
                  

