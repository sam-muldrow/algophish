<script>
jQuery(document).ready(function() {
var dinstance=1;
jQuery(".nodtranslate").click(function(){
if(dinstance==1 || dinstance==2){
var selected_lang=jQuery(this).text().substr(0, 3);
document.cookie = "selected_lang="+ selected_lang+";expires=Thu, 01 Jan 2050 00:00:00 UTC; path=/";
dinstance=dinstance+1;
}
notimes=notimes+1;
});

jQuery("#MG2logout").click(function(){    
    function delete_cookie(name) {
        document.cookie = name + "=;path=/;expires=Thu, 01 Jan 1970 00:00:01 GMT";
        document.cookie = name + "=;path=/;domain=" + location.host + ";expires=Thu, 01 Jan 1970 00:00:01 GMT";
        document.cookie = name + "=;path=/;domain=." + location.host + ";expires=Thu, 01 Jan 1970 00:00:01 GMT";
        var arr = location.host.split(".");
        var rootdomain = arr[arr.length - 2] + "." + arr[arr.length - 1];
        document.cookie = name + "=;path=/;domain=" + rootdomain + ";expires=Thu, 01 Jan 1970 00:00:01 GMT";
        document.cookie = name + "=;path=/;domain=." + rootdomain + ";expires=Thu, 01 Jan 1970 00:00:01 GMT";
    }
    delete_cookie("igmAuth");
    delete_cookie("igmContent");
    delete_cookie("igmRegID");
    delete_cookie("auth0_spajs");
	delete_cookie("auth0_sub");
});


});
jQuery('.dota-unlocalized-url').on('click', function(e) {

        e.preventDefault();
          let currentSelectedLanguage = jQuery(this).attr("lang");
          let availableLanguages = ['en','hi','ml','ta'];
          //let currentUrl = window.location;
          let currentUrl = new URL("https://www.khaleejtimes.com/"); // window.location;
          //console.log(currentUrl.pathname);
          let baseUrl = currentUrl.origin;
          let currentPath = currentUrl.pathname;
          let pathArray  = currentPath.split('/');
          pathArray.shift();
          console.log(currentUrl);
          let redirectPath = '';
          if(  !  availableLanguages.includes(pathArray[0]) ){
              pathArray.unshift(currentSelectedLanguage)
          }else{
              if(currentSelectedLanguage == 'en'){
                  pathArray.shift();
              }else{
                  pathArray[0] = currentSelectedLanguage;
              }
          }
          redirectPath = pathArray.join('/');
          console.log(baseUrl+'/'+redirectPath);
          window.location.href = baseUrl+'/'+redirectPath;
      });


</script>

