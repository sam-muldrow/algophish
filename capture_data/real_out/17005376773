                  });
                }
        	
        
      }
    window.callTree = true;
    var snizeinterval = setInterval(function () {
    //console.log('count-->');
      if($("#searchspring-content, .template-product, .searchspring-recommendations-home").length > 0){
     // console.log('asd');
        	$("#searchspring-content, .template-product, .searchspring-recommendations-home").bind("DOMSubtreeModified", function() {
              if(window.callTree){
                window.callTree = false;
                setTimeout(function(){
                  funtag();
                  window.callTree = true;
                }, 1000);
              }
      		});
        setTimeout(function(){
                  funtag();
                }, 1000);
        clearInterval(snizeinterval);
      }
    }, 1000);
  });
</script>
