  //  console.log("tagbrands=>",tagbrands);
     var brands = [];
  $('.clearBoth a').each(function(){
      brands.push($(this).text());
  //     console.log("brands=>",brands);
    })
    var difference = [];
    jQuery.grep(tagbrands, function(el) {
      if (jQuery.inArray(el, brands) == -1) difference.push(el);
    });
  //   console.log("difference=>",difference);
  </script>
-->


 <script>
   $(window).on('popstate', function() {
     setTimeout(function () {
       if($('#searchspring-content').is(':empty')){
          location.reload(true);
       }
       }, 2000);
   });
</script> 
