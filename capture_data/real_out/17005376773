                    if(text == tag_text && $(this).hasClass(product_tag_text)){ 
                      	$(this).addClass('show');
                    }
                  });
                }
        	
                product_tag_text = 'is-returnableis_returnable';
        		tag_text= 'Extra 15% Off in Cart';
                startdate = new Date('2023-10-24 00:00:00'.replace(" ", "T")+ "-04:00");
                enddate = new Date('2023-10-31 23:59:00'.replace(" ", "T")+ "-04:00");
                if(startdate <= nowddate && nowddate <= enddate){
                  $(".ss__badge").each(function( index ) {
                    let text= $(this).text();
              //console.log('text=>',text);
                    if(text == tag_text && $(this).hasClass(product_tag_text)){ 
                      	$(this).addClass('show');
                    }
                  });
                }
        	
                product_tag_text = 'is-returnabletrue';
        		tag_text= 'Extra 15% Off in Cart';
                startdate = new Date('2023-08-31 00:00:00'.replace(" ", "T")+ "-04:00");
                enddate = new Date('2023-09-04 23:59:00'.replace(" ", "T")+ "-04:00");
                if(startdate <= nowddate && nowddate <= enddate){
                  $(".ss__badge").each(function( index ) {
                    let text= $(this).text();
              //console.log('text=>',text);
                    if(text == tag_text && $(this).hasClass(product_tag_text)){ 
                      	$(this).addClass('show');
                    }
                  });
                }
        	
                product_tag_text = 'is-returnableis_returnable';
        		tag_text= 'Extra 15% Off in Cart';
                startdate = new Date('2023-08-31 00:00:00'.replace(" ", "T")+ "-04:00");
                enddate = new Date('2023-09-04 23:59:00'.replace(" ", "T")+ "-04:00");
                if(startdate <= nowddate && nowddate <= enddate){
                  $(".ss__badge").each(function( index ) {
                    let text= $(this).text();
              //console.log('text=>',text);
                    if(text == tag_text && $(this).hasClass(product_tag_text)){ 
                      	$(this).addClass('show');
                    }
                  });
                }
        	
                product_tag_text = 'Returnable';
        		tag_text= 'Extra 15% Off in Cart';
                startdate = new Date('2023-06-29 00:00:00'.replace(" ", "T")+ "-04:00");
                enddate = new Date('2023-07-04 23:59:00'.replace(" ", "T")+ "-04:00");
                if(startdate <= nowddate && nowddate <= enddate){
