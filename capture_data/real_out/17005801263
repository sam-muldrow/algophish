<script>
  if (!window.HEADER_SEARCH_CONTENT) {
    window.HEADER_SEARCH_CONTENT = {
      placeholder: 'What are you looking for?',
      searchAction: 'https://www.ramseysolutions.com/search/results',
      engineName: 'search-ramseysolutions-com',
      searchKey: 'search-jre4u12hqsb9r5bsixw584he',
      endpointBase: 'https://ramseysolutions.ent.us-east-1.aws.found.io'
    };
  }

 const toggleMastheadSearch = () => {
    const searchMasthead = document.getElementById('rs_mastheadSearch_box');

   searchMasthead.classList.toggle('u-visuallyHidden');
    document.getElementsByTagName('body')[0].classList.toggle('rs-Search-open');
    document
      .getElementById('rs_search_overlay')
      .classList.toggle('u-visuallyHidden');

    if (searchMasthead.classList.contains('u-visuallyHidden')) {
      document.getElementById('rs_search_button').blur();
    } else {
      if (document.getElementById('rs_mastheadSearch_input'))
        document.getElementById('rs_mastheadSearch_input').focus();
    }
  };
</script>

