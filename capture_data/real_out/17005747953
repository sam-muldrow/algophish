    <section class="part3-box part5-box-detail" itemscope="" itemtype="http://schema.org/CreativeWork" data-element-name="FRONT_D1">
      <header class="header-section brdbtm">
        <h4 class="white-text">
                      گفت‌و‌گو و گزارش                  </h4>
      </header>
            <div class="content-part3 container">
        
      <div class="part3-col1e" data-element-name="FRONT_E1">
      <ul class="part3-col1-list">
                              <li class="container">
              <div class="part3-col1-image">
