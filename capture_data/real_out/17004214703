}
if (window.document.location.pathname != '/') {
gamObject.pageTemplate = ''
gamObject.categorieTemplate = ''
gamObject.pageId = '';
gamObject.pageResult = gamObject.pageTemplate || gamObject.categorieTemplate;
if (!gamObject.pageResult) {
// Verifica si el pathname contiene '/juegos-mentales'
if (window.document.location.pathname.includes('/juegos-mentales')) {
