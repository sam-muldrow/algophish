<script>
// Set GAM Object
window.gamObject = {
// Google Ad Manager CLIENT ID
GPT_CLIENT_ID: '',
// Device name (desktop, tablet, mobile)
GPT_DEVICE_NAME: '',
// List of slots configured
SLOTS: [],
// List of slots that display() ran
slotsLoaded: [],
// YML encode to json
configYamlToJson: [],
// page attrs
pageTemplate: '', // type of resource e.g default, gallery, etc
// Page id only in resources has data
pageId: '',
// Category name
categorieTemplate: '',
// Page resolve of pageTemplate + categorieTemplate
pageResult: '',
// Display ad and add to slots loaded
displayAd: function(i, d, t, m) {
googletag.cmd.push(function() {
if (window.googletag && googletag.apiReady) {
var config;
switch (gamObject.GPT_DEVICE_NAME) {
case "desktop": {
config = gamObject.SLOTS.filter(function (item) {
return item.name == d || new RegExp(d).test(item.name)
})
} break;
case "tablet": {
config = gamObject.SLOTS.filter(function (item) {
return item.name == t || new RegExp(t).test(item.name)
})
} break;
case "mobile": {
config = gamObject.SLOTS.filter(function (item) {
return item.name == m || new RegExp(m).test(item.name)
})
} break;
}
config = config.shift();
if (typeof config !== "undefined" && !gamObject.slotsLoaded.includes(config.name)) {
let tag_google = document.createElement("div");
let { tagId, name } = config;
tag_google.id = tagId;
// Creat element
document.getElementById(i).appendChild(tag_google);
googletag.display(tagId);
// Filter
let slot = googletag.pubads().getSlots().filter(slot => {
return slot.getSlotId().getDomId() === tagId
});
// Refresh slot
if (
slot &&
slot.length > 0 &&
slot.shift().getTargeting(REFRESH_KEY).indexOf(REFRESH_VALUE) > -1
) {
googletag.pubads().refresh([slot.shift()]);
}
gamObject.slotsLoaded.push(name);
}
}
})
},
// Call displayAd with event DOMContentLoaded or with setTimeout, some validations too
display: function(idDiv, unit_desktop, unit_tablet, unit_mobile, gpt=true){
(async () => {
try {
return new Promise((resolve, reject) => {
setTimeout(() => {
if (idDiv)
{
if (
typeof gamObject.displayAd === "function" &&
gpt
) {
gamObject.displayAd(
idDiv,
unit_desktop,
unit_tablet,
unit_mobile
);
return resolve();
}
if (typeof googletag !== "undefined" && !gpt) {
googletag.cmd.push(() => {
if (window.googletag && googletag.apiReady) {
googletag.display(idDiv);
}
});
return resolve();
}
}
return reject();
}, 800);
});
} catch (e) {
console.warn({
details: `${idDiv ? idDiv : "N/A"} - ${unit_desktop ? unit_desktop : "N/A"} - ${unit_mobile ? unit_mobile : "N/A"}`,
error: e
});
}
})();
}
};
// List of slots to refresh in machine learning board (home) - @var Global
window.machineLearningBoardRefreshSlots = [];
// GPT initialization - @var Global
window.googletag = window.googletag || { cmd: [] }; // If no slots found define anyway! to avoid googletag no defined
// Prebid initialization - @var Global
window.pbjs = window.pbjs || {};
pbjs.que = pbjs.que || [];
window.PREBID_TIMEOUT = 3000;
window.DISPLAYSURE_TIMEOUT = 4000;
window.DISPLAYAFTERSURE_TIMEOUT = 5000;
window.TIMER_NONBLOCK = 0;
window.REFRESH_KEY = 'refresh';
window.REFRESH_VALUE = 'true';
