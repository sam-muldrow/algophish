<script async type="text/javascript">

    window.onload = function (event) {
        var footer_js_w = window.innerWidth;
        var footer_js_h = window.innerHeight;
        //document.getElementById("demo").innerHTML = "Width: " + w + "<br>Height: " + h;
        if (footer_js_w < 640) {
            //close_div_d1();
            //open_div_d2();
        } else {

            //close_div_d2();
            //open_div_d1();
        }
    };

    $(document).ready(function () {

        $(window).resize(function () {


            var footer_js_w = window.innerWidth;
            var footer_js_h = window.innerHeight;
            //document.getElementById("demo").innerHTML = "Width: " + w + "<br>Height: " + h;
            if (footer_js_w < 640) {

                //close_div_d1();
                //open_div_d2();
            } else {

                //close_div_d2();
                //open_div_d1();
            }
        });
    });


    function close_div_d1() {
        document.getElementById("footer_js_d1").style.display = "none";
    }

    function close_div_d2() {
        document.getElementById("footer_js_d2").style.display = "none";
    }

    function open_div_d1() {
        document.getElementById("footer_js_d1").style.display = "block";
    }

    function open_div_d2() {
        document.getElementById("footer_js_d2").style.display = "block";
    }
</script>

