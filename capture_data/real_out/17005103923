<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

<script src="https://www.aftabir.com/assets/common/sweetalert/sweetalert2.min.js"></script>
<script>
    $(document).ready(function () {
        $("#newsletter").keyup(function (e) {
            var code = e.which;
            if (code == 13) {
                if ($("#newsletter").val() === "") {
                    Swal.fire({
                        title: 'خطا!',
                        text: 'لطفا ایمیل را وارد کنید',
                        icon: 'error',
                        confirmButtonText: 'باشه'
                    })
                    return;
                }
                e.preventDefault();
                $.ajax({
                    /* the route pointing to the post function */
                    url: "http://www.aftabir.com/newsletter",
                    type: 'POST',
                    /* send the csrf-token and the input to the controller */
                    data: {
                        "_token": "2wc0JJD4VEcYTdlGwxyZcy754tIwZ55TA2A4gaUB",
                        "key": $("#newsletter").val()
                    },
                    dataType: 'JSON',
                    /* remind that 'data' is the response of the AjaxController */

                    statusCode: {
                        200: function () {
                            Swal.fire({
                                title: 'موفق!',
                                text: ' شما با موفقیت عضو خبرنامه شدید!',
                                icon: 'ok',
                                confirmButtonText: 'باشه'
                            })
                            $("#newsletter").val('');
                        },
                        400: function () {
                            Swal.fire({
                                title: 'خطا!',
                                text: ' ایمیل تکراریست',
                                icon: 'error',
                                confirmButtonText: 'باشه'
                            })
                        }
                        ,
                        422: function () {
                            Swal.fire({
                                title: 'خطا!',
                                text: ' ایمیل اشتباه است',
                                icon: 'error',
                                confirmButtonText: 'باشه'
                            })
                        }
                    }

                });
            }
            ;
        });
    });

    $('#register').on('click', function (e) {
        if ($("#newsletter").val() === "") {
            Swal.fire({
                title: ":)",
                text: "لطفا ایمیل را وارد کنید",
                button: "باشه"
            });
            return;
        }
        e.preventDefault();
        $.ajax({
            /* the route pointing to the post function */
            url: "http://www.aftabir.com/newsletter",
            type: 'POST',
            /* send the csrf-token and the input to the controller */
            data: {
                "_token": "2wc0JJD4VEcYTdlGwxyZcy754tIwZ55TA2A4gaUB",
                "key": $("#newsletter").val()
            },
            dataType: 'JSON',
            /* remind that 'data' is the response of the AjaxController */

            statusCode: {
                200: function () {
                    // alert('با موفقیت ثبت شد');
                    // swal("تبریک", "ایمیل شما با موفقیت ثبت شد", "success");
                     Swal.fire({
                        title: ":)",
                        text: "ایمیل شما با موفقیت ثبت شد",
                        icon: "success",
                        button: "ممنون",
                    });
                    $("#newsletter").val('');
                },
                400: function () {
                    // alert('ایمیل تکراری می باشد');.
                    // swal(":(", "ایمیل شما تکراری می باشد", "warning");
                    Swal.fire({
                        title: ":(",
                        text: "ایمیل شما تکراری می باشد",
                        icon: "warning",
                        button: "باشه",
                    });
                },
                422: function () {
                    // alert('فرمت ایمیل نا معتبر است');
                     Swal.fire({
                        title: ":|",
                        text: "فرمت ایمیل نامعتبر است",
                        icon: "error",
                        button: "باشه",
                    });
                }

            }

        });
    });
</script>

</body>
</html>

