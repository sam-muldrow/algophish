			html .index-tabs .index-tabs-data.index-tabs-mobile-full-data table.data-table tr:nth-child(1) td.market-currency-ice.mobile-grid ,
			html .index-tabs .index-tabs-data.index-tabs-mobile-full-data table.data-table tr:nth-child(2) td.market-currency-ice.mobile-grid {
				display:block !important;
			}
			.index-tabs .index-tabs-data.index-tabs-mobile-full-data table.index-tabs-table tbody tr td:nth-child(3),
			.index-tabs .index-tabs-data.index-tabs-mobile-full-data table.index-tabs-table thead tr th:nth-child(3) {
				display: block !important;
			}
		}

