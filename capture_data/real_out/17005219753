<script>
	$('.side-menu-languages-btn').click(function(event) {
		event.preventDefault();
		$(".top-menu-languages-btn").click();
	});
	
	
    $(document).on('click', '.convert-mobile-acc', function(event) {
		event.preventDefault();
		$('.convert-mobile-acc').removeClass('convert-acc-active');
		$(this).addClass('convert-acc-active');
    });
    
    $(document).on('click', '.sana-acc-mobile table tr th', function(event) {
		event.preventDefault();
 		$('.sana-acc-mobile table').find('tr').addClass('acc-hide-tr');
 		$(this).parent().removeClass('acc-hide-tr');
    });
    
    
    	
	
</script>
