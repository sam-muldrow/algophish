  <script type="text/javascript">
    var jurl = '/weixinShare?url=' + encodeURIComponent(location.href),
      mxhr = new XMLHttpRequest(),
       shareTitle = '勇士令状焕新登场 阿哈利姆新迷宫等你破解',
      shareInfo =  '卓尔游侠至宝，米拉娜身心，森海飞霞声望，不朽珍藏，现已到来',
      shareURL =
            'https://img.dota2.com.cn/dota2static/file/2149b077-68d2-4ed8-af07-aaa6cd8f5a05.png'

    mxhr.onreadystatechange = function() {
      if (mxhr.readyState == 4 && mxhr.status == 200) {
        mkWxShare(JSON.parse(mxhr.responseText))
      }
    }
    mxhr.open('GET', jurl, true)
    mxhr.send();

    function wxready() {
      wx.ready(function() {
        //分享给朋友
        wx.onMenuShareAppMessage({
          title: shareTitle, // 分享标题
          desc: shareInfo, // 分享描述
          link: location.href, // 分享链接
          imgUrl: shareURL, // 分享图标
          type: '', // 分享类型,music、video或link，不填默认为link
          success: function() {
            // 用户确认分享后执行的回调函数
          },
          cancel: function() {}
        });

        //分享到朋友圈
        wx.onMenuShareTimeline({
          title: shareTitle, // 分享标题
          link: location.href, // 分享链接
          imgUrl: shareURL, // 分享图标
          success: function() {
            // 用户确认分享后执行的回调函数
          },
          cancel: function() {}
        });

        //分享到QQ
        wx.onMenuShareQQ({
          title: shareTitle, // 分享标题
          desc: shareInfo, // 分享描述
          link: location.href, // 分享链接
          imgUrl: shareURL, // 分享图标
          success: function() {
            // 用户确认分享后执行的回调函数
          },
          cancel: function() {
            // 用户取消分享后执行的回调函数
          }
        });
      })
    }

    function mkWxShare(cbObj) {
      if (cbObj.status == 'failed') {
        return;
      }
      var data = cbObj.data
      wx.config({
        debug: false,
        appId: data.appId,
        timestamp: data.timestamp,
        nonceStr: data.nonceStr,
        signature: data.signature,
        jsApiList: ['onMenuShareTimeline', 'onMenuShareAppMessage', 'onMenuShareQQ']
      });
      wxready();
    }
  </script>
  <script>
    $(function() {
      //奖金
      $('.open-link').click(function() {
        window.open('https://www.dota2.com.cn/outlanders/')
      });
      var is_mobile = (function() {
        var userAgentInfo = navigator.userAgent
        var Agents = new Array(
          'Android',
          'iPhone',
          'SymbianOS',
          'Windows Phone',
          'iPad',
          'iPod',
          'Windows CE',
          'BlackBerry'
        )
        var flag = false
        for (var v = 0; v < Agents.length; v++) {
          if (userAgentInfo.indexOf(Agents[v]) > 0) {
            flag = true
            break
          }
        }
        return flag
      })();
      if (is_mobile) {
        $('video').remove();
        $('body').addClass('m')
      }
      var J_header_video = $('#videoDialog');
      J_header_video.on('click', '.close', function() {
        J_header_video.find('.video-content').html('')
        $(this).closest('.mak').hide();
      })
      $('.J_player').on('click', function(e) {
       /*  if (is_mobile) {
          return;
        } */
        e.preventDefault();
        J_header_video.find('.video-content').append('<video controls="controls" autoplay=""  width="1000" id="dunVideo" loop=""><source type="video/mp4" src="https://img.dota2.com.cn/dota2static/file/2a5a8475-8166-4169-9213-07ca5c348a91.mp4"></video>')
        J_header_video.show();
      })

      function closeVideo() {
        $('#videoDialog').hide().find('video')[0].pause()
      }
      // fixed cookie top
      var done = true
      $(window).scroll(function() {
        var scrollTop = $(window).scrollTop()
        if (scrollTop >= 1200) {
          if (done) {
            done = false
            $('.ruins-map').addClass('ani-show')
            setTimeout(function() {
              $('.ruins-map').addClass('ani-hide')
            }, 1600)
          }
        }
        if ($('.public_bd_cookie_top').length) {
          if ($(window).scrollTop() >= 71) {
            $('body>.header').css({
              position: 'fixed',
              top: '0'
            })
          } else {
            $('body>.header').css({
              position: 'absolute',
              top: '60px'
            })
            $('body').css({
              'padding-top': '60px'
            })
          }
        }
      })
      $('body').on('click', '.public_hg_ok a', function() {
        $('body>.header').css({
          position: 'fixed',
          top: '0'
        })
      })
      //英雄列表
      $.get('/datafeed/heroList?task=herolist').then(function(res) {
        if (res.status === 'success') {
          var heroes = res.result.heroes
          for (var i = 0; i < 5; i++) {
            if (i == 0) {
              randHeroesDom(heroes.splice(0, 25), i)
            } else {
              randHeroesDom(heroes.splice(0, 24), i)
            }
          }
        }

        function randHeroesDom(data, row) {
          var new_data = JSON.parse(JSON.stringify(data));
          for (var i = 0; i < data.length; i++) {
            new_data.push(data[i])
          }
          for (var j = 0, hero_html = ''; j < new_data.length; j++) {
            hero_html += '<a target="_blank" class="homepage_HeroIcon" href="/hero/' + new_data[j].name.replace('npc_dota_hero_', '') + '" style="background-image: url(' + new_data[j].index_img + ')">' +
              '<div class="homepage_HeroNameContainer">' +
              '<span class="hero_icon homepage_icon' + new_data[j].primary_attr + '"></span>' +
              '<div class="homepage_HeroName" >' + new_data[j].name_loc + '</div>' +
              '</div>' +
              '<div class="homepage_FadeContainer">' +
              '<div class="fade_FadeContainer fade_Bottom">' +
              '<div class="fade_Fade_1keus" style = "background: linear-gradient(rgba(0, 0, 0, 0) 50%, rgba(0, 0, 0, 0.733) 75%, rgb(0, 0, 0) 100%);">' +
              '</div>' +
              '</div>' +
              '</div>' +
              '</a>'
          }
          $('.Row' + row).html(hero_html)
        }
      })
    })
  </script>
