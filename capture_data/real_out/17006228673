<script>
	function fn_spo_ent(id){
		location.href	=	'/view/?id='+ id;
	}
	
	(function(){
	  randomN = Math.floor(Math.random()*3);
	  if(randomN == 0){
		  document.getElementById("cont1").style.display = "block";
		  document.getElementById("cont2").style.display = "none";
		  document.getElementById("cont3").style.display = "none";
		  document.getElementById("spoent_tab_1").classList.add("on");
		  document.getElementById("spoent_tab_2").classList.remove("on");
		  document.getElementById("spoent_tab_3").classList.remove("on");
	  }else if(randomN == 1){
		  document.getElementById("cont2").style.display = "block";
		  document.getElementById("cont1").style.display = "none";
		  document.getElementById("cont3").style.display = "none";
		  document.getElementById("spoent_tab_2").classList.add("on");
		  document.getElementById("spoent_tab_1").classList.remove("on");
		  document.getElementById("spoent_tab_3").classList.remove("on");
	  }else{
		  document.getElementById("cont3").style.display = "block";
		  document.getElementById("cont1").style.display = "none";
		  document.getElementById("cont2").style.display = "none";
		  document.getElementById("spoent_tab_3").classList.add("on");
		  document.getElementById("spoent_tab_1").classList.remove("on");
		  document.getElementById("spoent_tab_2").classList.remove("on");
	  }
	})();
	
	Master.imgCropEvt('.imgRoll','img.cropcls11','490','306');

</script>
