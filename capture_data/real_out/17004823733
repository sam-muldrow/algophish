<script>
    document.addEventListener("DOMContentLoaded", function () {
        swapUrlOnResize();

        function swapUrlOnResize(windowWidth) {
            var v = document.getElementById("homepageHero_video");

            var windowWidth = window.innerWidth;
            if (windowWidth > 975.98) {
                v.src =
                    "//s7d9.scene7.com/is/content/MMGS7/Assets/Images/VideosEcom/2023_GC_Video_BlackFriday_Desktop.mp4";
            } else if (windowWidth < 975.98 && windowWidth > 667.98) {
                v.src =
                    "//s7d9.scene7.com/is/content/MMGS7/Assets/Images/VideosEcom/2023_GC_Video_BlackFriday_Tablet.mp4";
            } else if (windowWidth < 667.98) {
                v.src =
                    "//s7d9.scene7.com/is/content/MMGS7/Assets/Images/VideosEcom/2023_GC_Video_BlackFriday_Mobile.mp4";
            }
        }

        window.addEventListener("resize", swapUrlOnResize);
    });
</script>


