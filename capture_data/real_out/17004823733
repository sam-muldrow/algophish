 <!-- calling lazy loading on plp pages for thumbnail temp fix in Default Global Header -->
<script>

var cList = document.body.classList;
window.GCI = window.GCI ||{
        modules: {},
        NodeModules: {}
};
if (
    cList.contains('category') ||
    cList.contains('searchresults') ||
    cList.contains('gcbrand') ){


    function updateImgLazyLoadOnWindow() {
        var lazyLoadInstanceWindow = new LazyLoad({
            data_src: "original"
        });
    lazyLoadInstanceWindow.update()
    }


    window.addEventListener('load',updateImgLazyLoadOnWindow)
    function listenForNewPlpThumbs () {

    
        var llPlpThumbs = new LazyLoad({
          data_src: "original"
          // Assign the callbacks defined above
        });
          $(document).on('gridRendered', function() {
console.log('thumbs rendered load images');
              llPlpThumbs.update() 
          })
      }
      
      if (GCI.externalResourcesLoaded) {
          listenForNewPlpThumbs();
          } else {
          document.addEventListener(
              'externalResourcesLoaded',
              listenForNewPlpThumbs,
              false
          );
      }
}

document.addEventListener('keydown', function(e){
    if( $(e.target).is( "#header-search #typeAheadSearchInput") && e.key === "Enter" ){
        e.stopImmediatePropagation();
        e.preventDefault();
        $(e.target).trigger({type:"manualFire", key:"Enter"});
    }
});
</script><div id="main" tabindex="-1">
<!-- Layout - Tag -->
