                function swapUrlOnResize(windowWidth) {
                var vid02 = document.getElementById("newArrivalPromo_video");
  
                var windowWidth = window.innerWidth;
                // var windowWidth = document.body.clientWidth;
                if (windowWidth > 975.98) {
                    vid02.src =
                        "//s7d9.scene7.com/is/content/MMGS7/Assets/Images/VideosEcom/2023_GC_Video_BFBlackOut_Desktop.mp4";
                } else if (windowWidth < 975.98 && windowWidth > 667.98) {
                    vid02.src =
                        "//s7d9.scene7.com/is/content/MMGS7/Assets/Images/VideosEcom/2023_GC_Video_BFBlackOut_Tablet.mp4";
                } else if (windowWidth < 667.98) {
                    vid02.src =
                        "//s7d9.scene7.com/is/content/MMGS7/Assets/Images/VideosEcom/2023_GC_Video_BFBlackOut_Mobile.mp4";
                }
            }
            window.addEventListener("resize", swapUrlOnResize);
        }
        else {
            var vid02 = document.getElementById("newArrivalPromo_video");
