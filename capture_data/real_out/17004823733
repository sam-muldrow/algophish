<script id="schedule-list-openDayList" type="text/html">
{{ set DAYS =  {
MON: 'Monday',
TUE: 'Tuesday',
WED: 'Wednesday',
THR: 'Thursday',
FRI: 'Friday',
SAT: 'Saturday',
SUN: 'Sunday'
}
}}
{{set openDayList = $data}}
{{each openDayList}}
{{if $value.weekDays}}
{{set storeTimeDetail = $value}}
{{each storeTimeDetail.weekDays}}
{{set day = DAYS[$value]}}
{{set entDateDetail = $imports.getEndTime(openDayList)}}
{{set transformDayKey = DAYS[entDateDetail.dayKey]}}
<li
class="day-details {{transformDayKey === day ? 'text-primaryGreen' : null}}"
>
<span class="day-name">{{day}}</span>
<span>
{{storeTimeDetail.startTime}} {{ storeTimeDetail.startMoment}} - {{storeTimeDetail.endTime}} {{ storeTimeDetail.endMoment}}
</span>
</li>
{{/each}}
{{/if}}
{{/each}}
</script>
