<script>
    const hrChatBtnElBottom = document.querySelector('#hrChatNow');
    if (hrChatBtnElBottom) {
        hrChatBtnElBottom.addEventListener('click', function(e){
            const interstitialBtnElBottom = document.querySelector('.interstitialHelpButton');
            const interstitialChatBtnElBottom = document.querySelector('.interstitial-support-menu_buttons_chat');
            interstitialBtnElBottom.click();
            interstitialChatBtnElBottom.click();
        });
    }

    const hrChatBtnElModule = document.querySelector('#hpModuleChat');
    if (hrChatBtnElModule) {
        hrChatBtnElModule.addEventListener('click', function(e){
            const interstitialBtnElBottom = document.querySelector('.interstitialHelpButton');
            const interstitialChatBtnElBottom = document.querySelector('.interstitial-support-menu_buttons_chat');
            interstitialBtnElBottom.click();
            interstitialChatBtnElBottom.click();
        });
    }
</script>
<script>
    //define global namespace if not available
    window.GCI = window.GCI || {}; 
    GCI = window.GCI;
        
    //create waitForJquery function for javascript that needs jquery
    function hpCallWaitForJquery () {

        

        $(document).on('refreshedMiniCart', function(){
            var jsHpNumberSync = pageData.callCenterNumber;
            $(document).find('.js-phone-number-hp').html('<a href="tel:' + jsHpNumberSync + '" class="cta-link red">' + jsHpNumberSync + '</a>');

            var jsHpNumberSync = pageData.callCenterNumber;
            $(document).find('.js-phone-number-module').html('<a href="tel:' + jsHpNumberSync + '" class=""><div class="call-cta"><svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 20 20" fill="none"><path d="M3.69494 3.57484H7.23042L8.36998 6.42375L6.53932 7.64419C6.32027 7.79023 6.18869 8.03609 6.18869 8.29937C6.19069 8.37309 6.18869 8.30016 6.18869 8.30016L6.18869 8.30098L6.18869 8.30274L6.18872 8.30671L6.18883 8.31643C6.18895 8.32369 6.18915 8.3325 6.18948 8.34283C6.19015 8.36347 6.19135 8.39016 6.19351 8.42249C6.19782 8.48713 6.20594 8.57452 6.22121 8.6814C6.25172 8.89502 6.31096 9.18767 6.42595 9.53265C6.65685 10.2253 7.11054 11.1222 7.99416 12.0058C8.87777 12.8895 9.77465 13.3432 10.4674 13.5741C10.8123 13.689 11.105 13.7483 11.3186 13.7788C11.4255 13.7941 11.5129 13.8022 11.5775 13.8065C11.6098 13.8087 11.6365 13.8099 11.6572 13.8105C11.6675 13.8109 11.6763 13.8111 11.6836 13.8112L11.6933 13.8113L11.6973 13.8113L11.699 13.8113L11.6998 13.8113C11.6998 13.8113 11.7877 13.8065 11.7006 13.8113C11.9989 13.8113 12.2715 13.6428 12.4049 13.376L12.9323 12.3214L16.4252 12.9035V16.3051C14.7628 16.5455 10.2734 16.7824 6.74548 13.2545C3.2176 9.72664 3.45454 5.23716 3.69494 3.57484ZM7.82152 8.68212L9.24355 7.7341C9.8632 7.321 10.1088 6.53033 9.83219 5.83887L8.69262 2.98996C8.45346 2.39206 7.87438 2 7.23042 2H3.6538C2.93842 2 2.26504 2.49699 2.1476 3.27318C1.88042 5.03905 1.51675 10.253 5.6319 14.3681C9.74704 18.4833 14.9609 18.1196 16.7268 17.8524C17.503 17.735 18 17.0616 18 16.3462V12.9035C18 12.1337 17.4434 11.4767 16.6841 11.3501L13.1912 10.768C12.5092 10.6543 11.8329 10.9987 11.5237 11.6171L11.2508 12.1628C11.1651 12.1417 11.0694 12.1147 10.9654 12.08C10.4769 11.9172 9.79897 11.5835 9.10774 10.8923C8.41651 10.201 8.08279 9.52307 7.91998 9.03464C7.87598 8.90266 7.84431 8.78398 7.82152 8.68212Z" fill="#E7131A"/></svg>' + jsHpNumberSync + '</div></a>');
        }); 





    }
    //check if globalHeaderResourcesLoaded (jquery) has been set  
    if (GCI.externalResourcesLoaded) {
        hpCallWaitForJquery();
    } else {
    //if it is not set wait for it before firing javascript using jquery
        document.addEventListener(
            'externalResourcesLoaded',
            hpCallWaitForJquery,
            false
        );
    }
</script>




