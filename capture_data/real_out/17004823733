<!-- Layout - Tag -->
<div id="bodyContentContainer" ><!-- Layout - Tag -->
<!-- Function - Html Content -->
<style>
.pencil-banner-wrap {
	display: block;
	background: #000;
}
.pencil-banner-wrap .text-wrap {
	-webkit-box-sizing: border-box;
	   -moz-box-sizing: border-box;
			box-sizing: border-box;
	display: block;
	padding: 0.8rem;
	text-align: center;
	width: 100%;
}
.pencil-banner-wrap .text-wrap span,
.pencil-banner-wrap .text-wrap a {
	color: #fff;
	font-family: montserratregular, Arial, Helvetica, sans-serif;
	font-size: 0.9rem;
	font-weight: bold;
}
.pencil-banner-wrap .text-wrap a {
	text-decoration: underline;
}

