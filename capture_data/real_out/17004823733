<div aria-hidden="true" itemscope itemtype="//schema.org/WebSite"><meta itemprop="url" content="//www.guitarcenter.com/"/><form itemprop="potentialAction" itemscope itemtype="//schema.org/SearchAction"><meta itemprop="target" content="//www.guitarcenter.com/search?Ntt={search_string}"/><label for="search_string_query"><span class="screen-reader-only">Search String Query (Required):</span><input itemprop="query-input" type="text" id="search_string_query" name="search_string" required/></label><input type="submit"/></form></div></div><!-- Layout - Tag -->
