<script type="text/javascript">
  function fireMagicSpanLinks () {
        magicSpanLinks();
        if (
            cList.contains('category') ||
            cList.contains('searchresults')||
            cList.contains('gcbrand')) {
        var llPlpThumbsBackup = new LazyLoad({
            elements_selector: "[loading=lazy]",
            use_native: true,
            data_src: "original"
            // Assign the callbacks defined above
        });
    }
  }
    if (GCI.externalResourcesLoaded) {
        fireMagicSpanLinks();
    } else {
        document.addEventListener(
            'externalResourcesLoaded',
            fireMagicSpanLinks,
            false
        );
    }


  </script>


