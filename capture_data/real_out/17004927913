<script type="text/javascript">
$(document).ready(function() {

$("#query").bind('input', function() {
  var c = this.selectionStart,
      r = /[^a-z0-9 . ?]/gi,
      v = $(this).val();
  if(r.test(v)) {
    $(this).val(v.replace(r, ''));
    c--;
  }
  this.setSelectionRange(c, c);
});
});

</script>
