    setNavigation();
    // navigation starts here
$(".level-a").on("click", function (event) {      
var portalURL = Liferay.ThemeDisplay.getPortalURL();
var main_url = $(this).attr('data-target')      
if ($(window).width() > 767) {
location.href = portalURL+main_url;
}

});

$('.dropdown-menu a.dropdown-toggle').on('click', function(e) {
if (!$(this).next().hasClass('show')) {    
$(this).parents('.dropdown-menu').first().find('.show').removeClass("show");
}
var $subMenu = $(this).next(".dropdown-menu");
$subMenu.toggleClass('show');
$(this).parents('li.nav-item.dropdown.show').on('hidden.bs.dropdown', function(e) {
$('.dropdown-submenu .show').removeClass("show");
});
return false;
}); 
$(function () {
setNavigation();
});
function setNavigation() {
var win_path = window.location.pathname

var path_split =win_path.split('/');
var path;
//when click on more info button under calculator

if(path_split.length>5){
path_split.pop();

path = path_split.join('/')

} else{
path = win_path;
}

if(path.indexOf("/hi/") !== -1){
path = path.replace("/hi/", "/");
}




// var  path_length =$('.level-main a[data-name="' + win_path + '"]');
if(win_path == '/'){
$('#navbarCollapse').find('.level-main').find('.intro[data-nav="0"]').addClass('dis_table').removeClass('dis_none');
// $('#navbarCollapse').find('.level-main').find('.intro').find('li').first().addClass("menu_highlight" );
$('#navbarCollapse').find('.level-main').find('#0').addClass('active_menu');  
} else{
if( $('.level-main a[data-name*="' + path + '"]').length>0){
$('.level-main a[data-name~="' + path + '"]').parents('.sub_nav_menu').addClass('menu_highlight');
var viewportScroll = $(window).width();
if (viewportScroll > 767) {
$('.level-main a[data-name*="' + path + '"]').parents('.level-main').addClass('show');
$('.level-main a[data-name*="' + path + '"]').parents('.level-main').find('.level-a').addClass('active_menu');
$('.level-main a[data-name*="' + path + '"]').parents('.level-main').find('.intro').addClass('dis_table');
}

} else{
$('#navbarCollapse').find('.level-main').find('.intro[data-nav="0"]').addClass('dis_table').removeClass('dis_none');
// $('#navbarCollapse').find('.level-main').find('.intro').find('li').first().addClass("menu_highlight" );
$('#navbarCollapse').find('.level-main').find('#0').addClass('active_menu');  
}
}
}
// end of navigation
$(document).ready(function(){
        $(".show-more").click(function () {
        if($(".text").hasClass("show-more-height")) {
            $(this).text("(Show Less)");
        } else {
            $(this).text("(Show More)");
        }
        $(".text").toggleClass("show-more-height");
    });
	
	if ($(window).width() < 600) 
{
	$("#atmlocmodal_new").addClass("newclassproceed");
}
	
});


