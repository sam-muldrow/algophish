<script>
	$(document).ready(function() {
		if ($(window).width() <= 960) {	   
			$('.otherserv').find('.card-header').addClass('collapsed');

			$('.otherserv').find('.collapse ').removeClass('show')
			$('#sbi-other-service').removeClass('show')
			$('.sbi_announce').find('.card-header').addClass('collapsed')
			$('#sbi-announce').removeClass('show')	
			

			
		}

	
	
	$(".services_text:contains(RBI Kehta Hai)").addClass("rbikahatahai");
	$('img[alt="RBI Kehta Hai"]').addClass("rbikahatahai_img")

	
	$(".rbikahatahai").click(function(){		
		var r =window.confirm("You are about to leave SBI Website and go to an external link. Please click OK to continue to the link or CANCEL to return to the previous page.");
		if (r==true){				
		window.location.href = "https://www.rbi.org.in/commonperson/English/Scripts/rbikehtahai.aspx";
		}
		else{
			return false;
		}
	});
	$(".rbikahatahai_img").click(function(){		
		var r =window.confirm("You are about to leave SBI Website and go to an external link. Please click OK to continue to the link or CANCEL to return to the previous page.");
		if (r==true){				
		window.location.href = "https://www.rbi.org.in/commonperson/English/Scripts/rbikehtahai.aspx";
		}
		else{
			return false;
		}
	});
	
	//------- For LINKING OF PAN AND ADHAAR ---------------
	
	$(".services_text:contains(LINKING OF PAN AND ADHAAR)").addClass("LINKINGPANADHAAR");
	$('img[alt="LINKING OF PAN AND ADHAAR"]').addClass("LINKINGPANADHAAR_img");
	
	
	$(".LINKINGPANADHAAR").click(function(){		
		var r =window.confirm("You are about to leave SBI Website and go to an external link. Please click OK to continue to the link or CANCEL to return to the previous page.");
		if (r==true){				
		window.location.href = "https://eportal.incometax.gov.in/iec/foservices/#/pre-login/bl-link-aadhaar";
		}
		else{
			return false;
		}
	});
	$(".LINKINGPANADHAAR_img").click(function(){		
		var r =window.confirm("You are about to leave SBI Website and go to an external link. Please click OK to continue to the link or CANCEL to return to the previous page.");
		if (r==true){				
		window.location.href = "https://eportal.incometax.gov.in/iec/foservices/#/pre-login/bl-link-aadhaar";
		}
		else{
			return false;
		}
	});
	
	//-------------------------Sachet-----------------------------------


	$(".services_text:contains(RBI Sachet)").addClass("sachet");
	$('img[alt="RBI Sachet"]').addClass("sachet_jpg");
	
	$(".services_text:contains(आर बी आई  सचेत)").addClass("sachet");
	$('img[alt="आर बी आई  सचेत"]').addClass("sachet_jpg");
	
	$(".sachet").click(function(){		
		var r =window.confirm("You are about to leave SBI Website and go to an external link. Please click OK to continue to the link or CANCEL to return to the previous page.");
		if (r==true){				
		window.location.href = "https://sachet.rbi.org.in/";
		}
		else{
			return false;
		}
	});
	$(".sachet_jpg").click(function(){		
		var r =window.confirm("You are about to leave SBI Website and go to an external link. Please click OK to continue to the link or CANCEL to return to the previous page.");
		if (r==true){				
		window.location.href = "https://sachet.rbi.org.in/";
		}
		else{
			return false;
		}
	});
	
	
	
	

});
</script>
