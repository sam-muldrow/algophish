<script>
	var chat_id = "";
	var msg_counter = "";

	var intentName = "";

	var initialResponse = false;

	jQuery(document)
			.ready(
					function() {

						jQuery(".chat_bot_button").click(function() {
							$('.feedbackView').addClass('d-none')
							$('.chatView').removeClass('d-none')
							$('.chat_close_icon').addClass('d-none');
							$('.close_icon').removeClass("d-none");
							$('.sucessView').addClass('d-none');
							
							// Clear Form Data
							$('#isCustomer').prop('checked', false);
							$('.btn-feedback').removeClass('selected');
							$("#mobileNumber").val('');
							$("#score").val('');
							$("#accountNumber").val('');
							jQuery("#mobileNumber").attr('type', 'number');
							jQuery("#mobileNumber").attr('min', '0');
							$("#account_div").addClass("d-none");
							setTimeout(function() {
								setInitialResponse();
							}, 1000);
						});

						$('#query')
								.keyup(
										function() {

											var yourInput = $(this).val();
											re = /[`~!@#$%^&*()_|+\-=;:<>\{\}\[\]\\\/]/gi;
											var isSplChar = re.test(yourInput);
											if (isSplChar) {
												var no_spl_char = yourInput
														.replace(
																/[`~!@#$%^&*()_|+\-=;:<>\{\}\[\]\\\/]/gi,
																'');
												$(this).val(no_spl_char);
											}
										});

						$('.close_icon').on('click', function() {
							$('#query').val("");
							jQuery("#query_counter").html("300");

							if ($('.feedbackView').hasClass('d-none')) {
								$('.feedbackView').removeClass('d-none')
								$('.chatView').addClass('d-none')
								$('.chat_close_icon').removeClass('d-none');
								$('.close_icon').addClass("d-none");
								return;
							}

						});

						$("#submitBtn").on('click', function() {
							submitData("SUBMIT", "SUBMIT");
						
						});
						
						$("#skipBtn").on('click', function() {
							submitData("SKIP", "SKIP_BUTTON");
						});
					});
	
	function checkBoxChange()  {
		if ($("#isCustomer").is(":checked")) {
			$("#account_div").removeClass("d-none");
		} else {
			$("#account_div").addClass("d-none");
		}
		
	}
	function isNumber(num) {
		if(num.match(/^-?\d+$/)){
		 	return true;
		} else{
			 return false;
		}
	}
	
	function submitData(actionType, actionSubType) {
		var url = "https://sbi.co.in/web/personal-banking/home?p_p_id=ChatBotPortlet&p_p_lifecycle=2&p_p_state=normal&p_p_mode=view&p_p_resource_id=submitForm&p_p_cacheability=cacheLevelPage";
		var mobileNumber = $("#mobileNumber").val();
		$("#mobile_error").html("");
		$("#account_error").html("");
		$("#score_error").html("");
		if (actionType == "SUBMIT") {
			var errors = false;
			if ($.trim (mobileNumber) == "" || $.trim(mobileNumber).length != 10 || !isNumber(mobileNumber)) {
				$("#mobile_error").html("Please enter a vaid mobile number");
				errors = true;
			}
			var accountNumber = $("#accountNumber").val();
			if ($("#isCustomer").is(":checked")) {
				
				if ($.trim (accountNumber) == "" || $.trim(accountNumber).length != 4 || !isNumber(accountNumber)) {
					$("#account_error").html("Please enter last 4 digits of your Account Number");
					errors = true;
				}
				
			}
			
			var rating = $("#score").val();
			if ($.trim(rating) == "") {
				$("#score_error").html("Please select a feedback");
				errors = true;
			}
			
			if (errors) {
				errors = false;
				return;
			}
			var isSbiCustomer = $("#isCustomer").val();	
			
			  var salt = CryptoJS.lib.WordArray.random(128/8);
			  var iv = CryptoJS.lib.WordArray.random(128/8);
			  var key = "ef7f44f5e7f5f7bb";
			  var key128Bits1000Iterations = CryptoJS.PBKDF2(key, salt, { keySize: 128/32, iterations: 10000 });
			
			  var encryptedMob = CryptoJS.AES.encrypt(mobileNumber, key128Bits1000Iterations, { iv: iv, mode: CryptoJS.mode.CBC, padding: CryptoJS.pad.Pkcs7  });
			  var finalMobString = encryptedMob + "^" +salt + "^"+iv;
			  
			  jQuery("#mobileNumber").attr('type', 'password');
			  jQuery("#mobileNumber").val(finalMobString);
		}
		jQuery.ajax({
			url : url,
			type : "post",
			dataType : "json",
			data : {
				"_ChatBotPortlet_rating" : rating,
				"_ChatBotPortlet_mobileNumber" : finalMobString,
				"_ChatBotPortlet_actionType" : actionType,
				"_ChatBotPortlet_actionSubType" : actionSubType,
				"_ChatBotPortlet_isSBICustomer" : isSbiCustomer,
				"_ChatBotPortlet_accountNumber" : accountNumber,
			},
			success : function(data) {
				console.log("Success " + data);
				$('.feedbackView').addClass('d-none');
				$('.chatView').addClass('d-none');
				$('.sucessView').removeClass('d-none');
			}, finally: function() {
				$('.feedbackView').addClass('d-none');
				$('.chatView').addClass('d-none');
				$('.sucessView').removeClass('d-none');
			}
		});
	}

	function setInitialResponse() {

		var conversationID = "TFFfITjFTfjFETTfEITfTFFGIEffTEfI";
		chat_id = conversationID;
		msg_counter = 0;
		var url = 'https://sbi.co.in/web/personal-banking/home?p_p_id=ChatBotPortlet&p_p_lifecycle=2&p_p_state=normal&p_p_mode=view&p_p_resource_id=chat&p_p_cacheability=cacheLevelPage';

		jQuery
				.ajax({
					url : url,
					type : "post",
					dataType : "json",
					async : false,
					crossDomain : true,
					data : {
						"_ChatBotPortlet_userInputMessage" : "hi",
						"_ChatBotPortlet_isConfirmation" : "false",
						"_ChatBotPortlet_botResponseMessage2" : "",
						"_ChatBotPortlet_defaultIntentName" : "root_WhatYono",
						"_ChatBotPortlet_botResponseMessage1" : "",
						"_ChatBotPortlet_intentName1" : "",
						"_ChatBotPortlet_botResponseMessage" : "",
						"_ChatBotPortlet_intentName2" : "",
						"_ChatBotPortlet_conversationID" : conversationID,
						"_ChatBotPortlet_speechBotResponseMessage" : "",
						"_ChatBotPortlet_intentName0" : "",
						"_ChatBotPortlet_suggestions" : [],
						"_ChatBotPortlet_speechID" : "",
						"_ChatBotPortlet_recommendations" : [],
						"_ChatBotPortlet_messageCounter" : "0",
						"_ChatBotPortlet_intentName" : intentName,
						"_ChatBotPortlet_channel" : "MB"
					},
					success : function(data) {
						var resposne = data.botResponseMessage;

						jQuery(".typing").hide();
						initialResponse = true;
						jQuery("#query").removeAttr('disabled');

						if (typeof resposne === "undefined") {
							msg_counter = msg_counter + 1;
							intentName = data.intentName;
							var text = '<div class="chat_message"><span><img src="/o/SBI-Theme/images/custom/chat_sia.png" alt=""></span>';
							text = text
									+ ' <div class="text_message">SIA is Currently Not available. Please try after some time</div><h6>'
									+ getTime()
									+ '</h6><div class="chat_grid_a">';
							text = text + '</div></div>';
							jQuery("#chat_box_overflow").html("");
							jQuery("#chat_box_overflow").append(text);
							jQuery('#chat_box_overflow').scrollTop(
									jQuery('#chat_box_overflow').height()
											* jQuery('#chat_box_overflow')
													.height());
						} else {
							resposne = resposne.replace(/<style.*<\/style>/g,
									'');
							msg_counter = msg_counter + 1;
							intentName = data.intentName;
							var text = '<div class="chat_message"><span><img src="/o/SBI-Theme/images/custom/chat_sia.png" alt=""></span>';
							text = text + ' <div class="text_message">'
									+ resposne + '</div><h6>' + getTime()
									+ '</h6><div class="chat_grid_a">';
							var suggestions = data.suggestions;
							for (var i = 0; i < suggestions.length; i++) {
								text = text
										+ '<button class="chat_grid_list_a">'
										+ suggestions[i] + '</button>';
							}

							text = text + '</div></div>';
							jQuery("#chat_box_overflow").html("");
							jQuery("#chat_box_overflow").append(text);
							jQuery('#chat_box_overflow').scrollTop(
									jQuery('#chat_box_overflow').height()
											* jQuery('#chat_box_overflow')
													.height());
						}
					}
				});
	}
	function fetchQueryResponse(query) {
		var counter = msg_counter;
		var chatId = chat_id;
		var intent = intentName;

		jQuery("#query").attr('disabled', 'disabled');

		jQuery("#query").val("");

		query = encodeURI(query);

		var url = 'https://sbi.co.in/web/personal-banking/home?p_p_id=ChatBotPortlet&p_p_lifecycle=2&p_p_state=normal&p_p_mode=view&p_p_resource_id=chat&p_p_cacheability=cacheLevelPage';
		jQuery
				.ajax({
					url : url,
					type : "post",
					dataType : "json",
					crossDomain : true,
					data : {
						"_ChatBotPortlet_userInputMessage" : query,
						"_ChatBotPortlet_isConfirmation" : "false",
						"_ChatBotPortlet_botResponseMessage2" : "",
						"_ChatBotPortlet_defaultIntentName" : "root_WhatYono",
						"_ChatBotPortlet_botResponseMessage1" : "",
						"_ChatBotPortlet_intentName1" : "",
						"_ChatBotPortlet_botResponseMessage" : "",
						"_ChatBotPortlet_intentName2" : "",
						"_ChatBotPortlet_conversationID" : chatId,
						"_ChatBotPortlet_speechBotResponseMessage" : "",
						"_ChatBotPortlet_intentName0" : "",
						"_ChatBotPortlet_suggestions" : [],
						"_ChatBotPortlet_speechID" : "",
						"_ChatBotPortlet_recommendations" : [],
						"_ChatBotPortlet_messageCounter" : counter,
						"_ChatBotPortlet_intentName" : intent,
						"_ChatBotPortlet_channel" : "MB"
					},
					success : function(data) {

						jQuery(".typing").hide();
						initialResponse = true;
						jQuery("#query").removeAttr('disabled');

						var resposne = data.botResponseMessage;
						if (typeof resposne === "undefined") {
							msg_counter = msg_counter + 1;
							intentName = data.intentName;
							var text = '<div class="chat_message"><span><img src="/o/SBI-Theme/images/custom/chat_sia.png" alt=""></span>';
							text = text
									+ ' <div class="text_message">SIA is Currently Not available. Please try after some time</div><h6>'
									+ getTime()
									+ '</h6><div class="chat_grid_a">';
							text = text + '</div></div>';
							jQuery("#chat_box_overflow").html("");
							jQuery("#chat_box_overflow").append(text);
							$('#chat_box_overflow').animate(
									{
										scrollTop : $('#chat_box_overflow')
												.prop("scrollHeight")
									});
						} else {
							resposne = resposne.replace(/<style.*<\/style>/g,
									'');
							msg_counter = msg_counter + 1;
							intentName = data.intentName;
							var text = '<div class="chat_message"><span><img src="/o/SBI-Theme/images/custom/chat_sia.png" alt=""></span>';
							text = text + ' <div class="text_message">'
									+ resposne + '</div><h6>' + getTime()
									+ '</h6><div class="chat_grid_a">';
							var suggestions = data.suggestions;
							for (var i = 0; i < suggestions.length; i++) {
								text = text
										+ '<button class="chat_grid_list_a">'
										+ suggestions[i] + '</button>';
							}
							text = text + '</div></div>';
							jQuery("#chat_box_overflow").append(text);
							jQuery('#chat_box_overflow').scrollTop(
									jQuery('#chat_box_overflow').height()
											* jQuery('#chat_box_overflow')
													.height());
							jQuery("#chat_likes").css("display", "block");

						}
					}
				});
	}

	function setUserResponse(sentence) {

		var text = '<div class="chat_message_right"><span><img src="/o/SBI-Theme/images/custom/default_profile.jpg" alt=""></span><div class="text_message"><p>'
				+ sentence
				+ '</p></div> <h6>'
				+ getTime()
				+ '</h6></div><div class="clearfix"></div>';
		jQuery('#chat_box_overflow').scrollTop(
				jQuery('#chat_box_overflow').height()
						* jQuery('#chat_box_overflow').height());
		jQuery("#chat_box_overflow").append(text);
	}

	function callAlt(text, e) {

		var isDisabled = $('#query').prop('disabled');
		if (!isDisabled) {
			var query = $(e).text();
			var newStr = text.replace(/%20/g, " ");

			jQuery(".typing").show();
			setUserResponse(query);
			fetchQueryResponse(newStr);
		}

	}

	jQuery('#query').keypress(function(e) {

		var key = (event.keyCode ? event.keyCode : event.which);
		if (key == 13) {
			jQuery("#query_counter").html("300");
			var sentence = jQuery("#query").val();
			sentence = jQuery(jQuery.parseHTML(sentence)).text();
			if (sentence.length == 0) {
				jQuery("#query").val("");

				return;
			}
			var query = jQuery("#query").val();
			jQuery("#query").attr('disabled', true);

			$(".typing").show();

			setUserResponse(sentence);
			fetchQueryResponse(query);

		}

	});

	jQuery("#query_img").on(
			'click',
			function() {

				jQuery("#query_counter").html("300");
				if ((jQuery.trim(jQuery('#query').val()) != '')
						&& (jQuery("#query").val()).length < 300) {
					var sentence = jQuery("#query").val();
					sentence = jQuery(jQuery.parseHTML(sentence)).text();
					if (sentence.length == 0) {
						jQuery("#query").val("");
						return;
					}

					var query = jQuery("#query").val();

					jQuery(".typing").show();

					setUserResponse(sentence);
					fetchQueryResponse(query);

				}
			});

	function scroll() {
		$('#chat_box_overflow').scrollTop(
				$('#chat_box_overflow').height()
						* $('#chat_box_overflow').height());
	}

	jQuery('#chat_box').on('click', '.feedback', function() {
		var id = $(this).attr('id');
		jQuery("#feedback" + id).addClass('img_opc');
		if (id == "0") {
			if ($("#feedback1").hasClass('img_opc')) {
				jQuery("#feedback1").removeClass('img_opc')
			}
		}
		if (id == "1") {
			if (jQuery("#feedback0").hasClass('img_opc')) {
				jQuery("#feedback0").removeClass('img_opc')
			}
		}
		feedback(id);
	});

	function feedback(code) {

		var chatId = chat_id;
		var url = 'https://sbi.co.in/web/personal-banking/home?p_p_id=ChatBotPortlet&p_p_lifecycle=2&p_p_state=normal&p_p_mode=view&p_p_resource_id=feedback&p_p_cacheability=cacheLevelPage';
		jQuery.ajax({
			url : url,
			type : "post",
			dataType : "json",
			crossDomain : true,
			data : {
				"conversationID" : chatId,
				"messageCounter" : "0",
				"feedBackStatus" : code,
				"feedBackNo" : "0"
			},
			success : function(data) {
			}
		});
	}

	jQuery('#chat_box_overflow').on('click', '.chat_grid_list_a', function() {

		var isDisabled = jQuery('#query').prop('disabled');
		if (!isDisabled) {
			jQuery(this).attr('disabled', true);
			var query = jQuery(this).text();

			jQuery(".typing").show();

			setUserResponse(query);

			fetchQueryResponse(query);

		}
	});

	jQuery('#chat_box').on('click', '.refresh_chat', function() {
		jQuery("#chat_box_overflow").empty();
		jQuery("#query_counter").html("300");
		jQuery("#query").val("");
		msg_counter = "";
		intentName = "";
		initialResponse = false;
		setInitialResponse();
	});

	function getTime() {
		var d = new Date();
		var hours = d.getHours().toString();
		var minutes = d.getMinutes().toString();
		var timeFlag = "AM";
		if (parseInt(hours) > 12) {
			hours = hours - 12;
			timeFlag = "PM";
		}
		if (hours.length == 1) {
			hours = "0" + hours;
		}
		if (minutes.length == 1) {
			minutes = "0" + minutes;
		}

		var time = hours + ":" + minutes + " " + timeFlag;
		return time;
	}
</script>


