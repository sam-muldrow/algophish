function sortSizes() {
    // Define the order of sizes
    const sizeOrder = ['XS', 'S', 'M', 'L', 'XL', '2XL', '3XL', '4XL'];

    // Get the list of size elements
    const sizeElements = document.querySelectorAll('.Size .form-option-wrapper');
    
    if (sizeElements) {
      // Convert the size elements to an array and sort them based on their position in the sizeOrder array
      const sortedSizeElements = Array.from(sizeElements).sort((a, b) => {
        const aIndex = sizeOrder.indexOf(a.querySelector('.form-option-variant').textContent.trim().toUpperCase());
        const bIndex = sizeOrder.indexOf(b.querySelector('.form-option-variant').textContent.trim().toUpperCase());
        return aIndex - bIndex;
      });

      // Move the sorted size elements inside the parent with class ".Size"
      const sizeParent = document.querySelector('.Size');
      sortedSizeElements.forEach(element => {
        sizeParent.appendChild(element);
      });
    }
}
    
function titleReplace() {
    const titleSelectors = ['.productView-title', '.breadcrumb-label span', '.cart-item-name__label', '.product-title', '.definitionList-key', '[data-test-id="product-set-widget-name"] a'];

    // Function to replace titles
    const replaceTitles = (elements) => {
        elements.forEach((titleElement) => {
            let title = titleElement.textContent.trim();
            title = title.replace(/\[[^\]]*\]/g, '').trim();
            titleElement.textContent = title;
        });
    };

    // Initial replacement for existing elements
    titleSelectors.forEach((selector) => {
        const titleElements = document.querySelectorAll(selector);
        if (titleElements.length) {
            replaceTitles(titleElements);
        }
    });

    // Observer for changes in the DOM
    const observer = new MutationObserver((mutations) => {
        mutations.forEach((mutation) => {
            if (mutation.addedNodes.length) {
                titleSelectors.forEach((selector) => {
                    mutation.addedNodes.forEach((node) => {
                        if (node.nodeType === Node.ELEMENT_NODE) {
                            const titleElements = node.querySelectorAll(selector);
                            if (titleElements.length) {
                                replaceTitles(titleElements);
                            }
                        }
                    });
                });
            }
        });
    });

    // Configuration of the observer
    const config = { childList: true, subtree: true };

    // Start observing the document body for added nodes
    observer.observe(document.body, config);
}
    
sortSizes();
titleReplace()
</script><script data-bc-cookie-consent="2" type="text/plain">
    function removeActive() {
       document.querySelectorAll('.web-view-tabs .tab-content.is-active').forEach(element => element.classList.remove('is-active'));
   }

   if (document.querySelectorAll('[href="#tab-reviews"]').length) document.querySelectorAll('[href="#tab-reviews"]')[0].addEventListener('click', removeActive);
   if (document.querySelectorAll('[href="#tab-qa"]').length) document.querySelectorAll('[href="#tab-qa"]')[0].addEventListener('click', removeActive);
