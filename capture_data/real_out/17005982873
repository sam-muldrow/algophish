<script type="text/javascript" async=true>
    _WF = window._WF || {};

    const cg = function (key, defaultResult) {
        var cacheValue = _WF.cache.getItem(key);
        return cacheValue !== null ? cacheValue : defaultResult;
    };

    const validNumber = function(value) {
        return value && value !== 0;
    }

    const validString = function(value) {
        return value && value !== "";
    }

    const adUnitPathToDiv = function (adUnitPath) {
        const slot = esme.enabledSlots.find((slot) => slot.placementName == adUnitPath);
        if(slot) return slot.slotId;
        return adUnitPath;
    }

    const buildBidPayload = function (bid, winner) {
        let highBidder = "";
        if(validString(bid.sourceBidder)) {
            highBidder = bid.sourceBidder;
        } else if(validString(bid.bidderCode)) {
            highBidder = bid.bidderCode;
        } else if(validString(bid.bidder)) {
            highBidder = bid.bidder;
        }

        let placement = "";
        if(bid.adUnitCode !== "") {
            placement = adUnitPathToDiv(bid.adUnitCode);
        } else {
            placement = adUnitPathToDiv(bid.rawBidDetails.gpt.slot.getAdUnitPath());
        }

        let timeToRespond = -1;
        if(bid.rawBidDetails.prebid) {
            timeToRespond = validNumber(bid.rawBidDetails.prebid.timeToRespond) ? bid.rawBidDetails.prebid.timeToRespond : -1;
        }

        let requestTimestamp = -1;
        if(bid.rawBidDetails.prebid) {
            requestTimestamp = validNumber(bid.rawBidDetails.prebid.requestTimestamp) ? bid.rawBidDetails.prebid.requestTimestamp : -1;
        }

        let mediaType = "";
        if(bid.rawBidDetails.prebid) {
            mediaType = validString(bid.rawBidDetails.prebid.mediaType) ? bid.rawBidDetails.prebid.mediaType : "";
        }

        var payload = {
            adomain: validString(bid.adDomain) ? bid.adDomain : "",
            adId: bid.adId,
            bidder: bid.bidder,
            bidmarkupfactor: 1,
            cpm: bid.cpm,
            creativeId: validString(bid.creativeId) ? bid.creativeId : "",
            encodedbid: validString(bid.subBidder) ? bid.subBidder : "",
            floorgroup: 'FL_0',
            highBidder: highBidder,
            iscached: !!bid.isCachedBid,
            isfallback: "",
            iswinner: winner == "prebid",
            latency: timeToRespond,
            placement: placement,
            pricebucket: validNumber(bid.priceBucket) ? bid.priceBucket : 0,
            requestid: validString(bid.auctionId) ? bid.auctionId : bid.requestId,
            sourceadunit: "",
            timestamp: requestTimestamp,
            unfilled: !!bid.isUnfilled,
            mediaType: mediaType
        };

        return payload;
    }

    const sendAuctionMessage = function (payload) {
        var message = {
            name: "pbjs_auction_result",
            payload: {
                browser: cg(_WF.BROWSERINFO.NAME),
                bucketid: cg(_WF.KEYS.BUCKETID, ""),
                countrycode: cg(_WF.KEYS.LOCATION, {}).country_code || "",
                deviceos: _WF.utils.getDeviceOS() || "",
                devicetype: cg(_WF.KEYS.DEVICETYPE),
                fingerprint: cg(_WF.KEYS.UUID),
                isrefreshauction: false,
                messagetimestamp: Date.now(),
                name: cg(_WF.KEYS.BLOGNAME),
                pbjsversion: pbjs.version || "",
                pstdatehour: cg(_WF.KEYS.PSTDATEHOUR),
                sessiondepth: cg(_WF.KEYS.SESSIONDEPTH, -1),
                sessiontoken: cg(_WF.KEYS.SESSIONTOKEN),
                subid: cg(_WF.KEYS.SUBIDRAW),
                utmcampaign: cg(_WF.KEYS.UTMCAMPAIGN, ""),
                utmcontent: cg(_WF.KEYS.UTMCONTENT, ""),
                utmmedium: cg(_WF.KEYS.UTMMEDIUM, ""),
                utmsource: cg(_WF.KEYS.UTMSOURCE, ""),
                pagetoken: _WF.utils.getPageToken()
            }
        }
        Object.assign(message.payload, payload);
        _WF.utils.sendTracking(message);
    };

    const sendAuctionInitMessage = function (auction) {
        const msg = {
            name: 'pbjs_auction_init',
            payload: {
                browser: _WF.cache.getItem(_WF.BROWSERINFO.NAME),
                devicetype: _WF.cache.getItem(_WF.KEYS.DEVICETYPE),
                fingerprint: _WF.cache.getItem(_WF.KEYS.UUID),
                isssl: document.location.protocol === 'https:',
                requestid: auction.auctionId,
                sessiondepth: _WF.cache.getItem(_WF.KEYS.SESSIONDEPTH) || -1,
                sessiontoken: _WF.cache.getItem(_WF.KEYS.SESSIONTOKEN),
                sitename: _WF.cache.getItem(_WF.KEYS.BLOGNAME),
                subid: _WF.cache.getItem(_WF.KEYS.SUBIDRAW),
                timestamp: auction.timestamp,
                useragent: navigator.userAgent.substr(0, 255),
                utmsource: _WF.cache.getItem(_WF.KEYS.UTMSOURCE),
                pagetoken: _WF.utils.getPageToken()
            }
        };

        _WF.utils.sendTracking(msg);
    }

    _WF.q.push(function () {
        _WF.on(_WF.EVENTS.SETUP, _WF.EVENTS.PLACEMENTPROVIDERS, _WF.EVENTS.ANCHORYREADY, function () {
            window.esme = window.esme || {};
            window.esme.queue = window.esme.queue || [];

            if (!_WF.utils.isTesting('esme_ab')) {
                return;
            }

            let host = `${window.location.host}/esme`;
            if('www.iwastesomuchtime.com/esme'.indexOf('HOST_NAME') == -1) {
                host = 'www.iwastesomuchtime.com/esme';
                window.esme.hostOverride = 'www.iwastesomuchtime.com/esme';
            }

            let stage = 'production/latest';
            if('production/latest'.indexOf('STAGE') == -1) {
                stage = 'production/latest';
            }

            const esmeVersionKey = 'esme-override-version';
            let qsOverride = cg(_WF.KEYS.QUERYSTRING, {})['esmev'];
            if (!qsOverride) {
                qsOverride = cg(esmeVersionKey);
            }
            if (qsOverride) {
                _WF.cache.setItem(esmeVersionKey, qsOverride).asPersistent();
                stage = qsOverride;
            }

            if('www.iwastesomuchtime.com'.indexOf('CONFIG') == -1) {
                window.esme.configOverride = 'www.iwastesomuchtime.com';
            }

            
            _WF.utils.loadScript('https://' + host + '/client/' + stage + '/boot.bundle.js');

            const dfpDependencies = _WF.cache.getItem(_WF.KEYS.DFPDEPENDENCIES);
            const providers = _WF.cache.getItem(_WF.KEYS.PROVIDERS);

            const enabledSlots = [];

            for(let providerName in providers) {
                if(providers[providerName] !== 'prebidv2') continue;
                let dfpDefinitions;
                if(_WF.utils.isMobile()) {
                    dfpDefinitions = dfpDependencies.find(dep => dep.name == 'mobile slots');
                } else {
                    dfpDefinitions = dfpDependencies.find(dep => dep.name == 'desktop slots');
                }

                const slotDefinition = dfpDefinitions.slots.find(slot => slot.name == providerName);

                if(!slotDefinition) continue;

                enabledSlots.push({
                    id: slotDefinition.div,
                    gam: slotDefinition.adUnitPath.split('/')[2],
                    size: slotDefinition.sizes[0].join('x')
                });
            }

            enabledSlots.forEach(slotToUpdate => {
                _WF.utils.log(`esme > updating ad unit: ${slotToUpdate.id}`, slotToUpdate);
                const slot = document.getElementById(slotToUpdate.id);
                if(!slot) {
                    _WF.utils.log(`esme > slot not found on page, skipping: ${slotToUpdate.id}`, slotToUpdate);
                    return;
                }
                slot.setAttribute('data-esme-ad', slotToUpdate.gam);
                slot.setAttribute('data-esme-size', slotToUpdate.size);
            });

            _WF.on(_WF.EVENTS.DATACOLLECTION, _WF.EVENTS.CONSENTLOADED, _WF.EVENTS.ESMEBOOT, function () {
                googletag.destroySlots();
                esme.queue.push(() => {
                    window.esme.registerEvent('auction.targeting', (data) => {
                        _WF.emit(_WF.EVENTS.DFPTARGETING);
                    });
                });
                esme.queue.push(() => {
                    window.esme.registerEvent('auction.init', (data) => {
                        sendAuctionInitMessage(data.pbjsAuction);
                    })
                })
                esme.queue.push(() => {
                    window.esme.registerEvent('auction.complete', (data) => {
                        _WF.emit(_WF.EVENTS.ALLDFPSLOTSRENDERED);
                        if (!data) return;
                        if (!data.winningBids) return;

                        Object.values(data.winningBids).forEach(slot => {
                            var payload = buildBidPayload(slot.bid, slot.winner);
                            sendAuctionMessage(payload);
                        });
                    });
                });

                esme.queue.push(() => {
                    _WF.emit(_WF.EVENTS.PREBIDLOADED);
                    _WF.emit(_WF.EVENTS.PREBIDINITIALIZED);
                    window.esme.refresh();
                });
            });
        });
    });
</script>
