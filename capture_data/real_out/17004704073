    (function(win) {
        win.addEventListener && win.addEventListener("DOMContentLoaded", function () {
            var s = win.document.createElement("script");
            s.async = true;
            s.src = "https://s.yimg.com/aaq/pv/perf-vitals_2.1.1.js";
            win.document.body.appendChild(s);
        });
    })(window);</script>
