	.fooldali-dobozok .block-heading-wrap {
		text-align: left;
		background-color:#fff;
		margin-bottom:0;
		padding:10px;
	}
	.fooldali-dobozok .block-heading {
		font-size: 220%;
		font-weight: 100;
		font-family: Poppins;
		text-align: left;
		color: #ec76be;
	}
	.fooldali-dobozok .sitka-post-grid-wrap.cols-2{
		display: block;
	}
</style>
