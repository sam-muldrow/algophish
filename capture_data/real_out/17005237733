<script>
$(function(){
function getRaceFlashTop(_type, _show_id)
{
var def= $.Deferred();
var _data={};
_data.pid='api_get_race_flash_'+_type+'_top';
_data.display='Main';
_data.input='UTF-8';
_data.output='jsonp';
$.ajax({
type:'GET',
url:_raceapi_action_api_url,
data:_data,
dataType:_data.output,
timeout:3e3,
success:function(data){
var tpl = data;
var message = '';
if (_type!='abroad') {
tpl = data.tpl;
message = data.message;
}
if (tpl.length < 1) {
def.resolve(false);
}
if (_type=='jra') {
$('ul#TopTicker').prepend(message);
$('#'+_show_id).prepend(tpl);
} else if (_type=='nar') {
$('ul#TopTicker').append(message);
$('#'+_show_id).append(tpl);
} else {
$('#'+_show_id).html(tpl);
}
def.resolve(true);
},
error: function(XHR, textStatus, errorThrown){
def.resolve(false);
}
});
return def.promise();
}
$.when(
getRaceFlashTop('jra','RaceFlashList'),
getRaceFlashTop('nar','RaceFlashList'),
getRaceFlashTop('abroad','RaceFlashAbroadMain')
).then(function(is_jra, is_nar, is_abr) {
if (is_jra) $('#JraList').show();
if (is_nar) $('#NarList').show();
if (is_jra || is_nar) {
if ( is_abr ) {
$('#RaceFlash').addClass('AbrUnder');
}
if ($('ul#TopTicker li').length < 1) {
$('ul#TopTicker').html('<li><p>Ä¾Á°¾ðÊó¡¦ÆþÀþ¡¢·ë²Ì¾ðÊó¤òÂ®Êó¤Ç¤ªÅÁ¤¨¤·¤Þ¤¹¡£</p></li>');
}
$('#RaceFlash').show();
} else {
$('#RaceFlash').remove();
}
setTimeout(function() {
$('ul#TopTicker li').show();
$('ul#TopTicker').liScroll({travelocity: 0.06});
}, 2500);
});
});
</script>
