                        create_box_tttt_slide("box-bai-pr3", "container-box-bai-pr3", "vertical", parseInt(gl_sl_tin_box_ttdn_cot_phai), '4000');
                    });
                }
               
                window['myLazyLoad'].update();
                <!--@@end_code_js_slide_footer@@-->

                window.addEventListener('load',function(){
                    if (typeof(bnBAER_173_15s) != 'undefined') {
                        if (bnBAER_173_15s.aNodes.length>0) {
                            if (docwidth>1200) {
                                $("body").append('<div class="banner_ballon" id="bn_balloon_ctn"></div>');
                                try { banner_tostring(bnBAER_173_15s, "bn_balloon_ctn") } catch (e) {}
                            }
                        }
                    }
                });
                setTimeout(function(){
                    if (typeof(bnBAER_173_15s) != 'undefined') {
                        if (bnBAER_173_15s.aNodes.length>0) {
                            if (document.getElementById('bnBAER_173_15s_0')) {
                                var v_iframe_balloon = document.getElementById('bnBAER_173_15s_0').getElementsByTagName('iframe').item(0);
                                if (v_iframe_balloon) {
                                    var div_before_sticky = $('#bnsubRight').prev();
                                    if (div_before_sticky[0].nodeName != 'SECTION' && div_before_sticky[0].nodeName != 'DIV') {
                                        for(i=0;i<5;i++) {
                                            div_before_sticky = div_before_sticky.prev();
                                            if (div_before_sticky[0].nodeName == 'SECTION' || div_before_sticky[0].nodeName != 'DIV') {
                                                break;
                                            }
                                        }
                                    }
                                    var div_before_sticky_height = div_before_sticky.outerHeight();
                                    var is_expand_balloon = false;
                                    var v_iframe_balloon_content = v_iframe_balloon.contentWindow;
                                    $(window).on('scroll', function () {
                                        if ($(window).scrollTop() >= div_before_sticky.offset().top + div_before_sticky_height / 2) {
                                            if (is_expand_balloon == false) {
                                                is_expand_balloon = true;
                                                sendMessage('banner_balloon', v_iframe_balloon_content, '', 'expand', '', '');
                                            }
                                        } else {
                                            if (is_expand_balloon == true) {
                                                is_expand_balloon = false;
                                                sendMessage('banner_balloon', v_iframe_balloon_content, '', 'collapse', '', '');
                                            }
                                        }
                                    })
                                }
                            }
                        }
                    }
                },5000);
                // Xử lý ẩn hiện phân trang slide theo cấu hình
                let v_div_slide = document.getElementsByClassName("on_off_dots_slide");
                if (v_div_slide.length > 0) {
                    if (v_on_off_slide_dots == 'false') {
                        $('.on_off_dots_slide').hide();
                    }
                }
                        
        </script>        
