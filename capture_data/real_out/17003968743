                    <script>
                    $(function () {
                        if (!('webp' in $.Tpl.helpers)) {
                            var webp = VIPSHOP.Detect.webp();
                            $.Tpl.helper('webp', function (img) {
                                if (!webp) {return img;}
                                if (img.indexOf('//a.appsimg.com') > -1) {return img.replace(/a.appsimg.com\/[a-z0-9\/_-]+\.(jpg|jpeg|png)$/i, '$&.webp');
                                } else {return img.replace(/[acd].vpimg[1-4].com\/([a-z0-9\/_-]+\.(jpg|jpeg|png))$/i, 'a.appsimg.com/$1.webp');}
                            });
                        }
                        var setTime = function (time, wrap) {
                            var endTime = Date.parse(time), now = (new Date()).getTime(), deffer, date = {},
                                day_1 = $('#j-day-1'), day_2 = $('#j-day-2'), hour_1 = $('#j-hour-1'), hour_2 = $('#j-hour-2'),
                                min_1 = $('#j-min-1'), min_2 = $('#j-min-2'),
                                sec_1 = $('#j-sec-1'), sec_2 = $('#j-sec-2'),
                                index = 0, dayEL = $('#j-time-day');
                            var countFn = function () {
                                now = (new Date()).getTime();
                                deffer = endTime - now;
                                if (deffer < 0) {clearInterval(countId); return;}
                                date['day'] = parseInt(deffer / (24 * 60 * 60 * 1000));
                                date['hour'] = parseInt((deffer - date['day'] * 24 * 60 * 60 * 1000) / (1000 * 60 * 60));
                                date['min'] = parseInt((deffer - date['day'] * 24 * 60 * 60 * 1000 - date['hour'] * 60 * 60 * 1000) / (1000 * 60));
                                date['sec'] = parseInt((deffer - date['day'] * 24 * 60 * 60 * 1000 - date['hour'] * 60 * 60 * 1000 - date['min'] * 60 * 1000) / (1000));
                                setCount();
                            };
                            function setCount() {
                                index++;
                                date['day'] = date['day'] < 10 ? '0' + date['day'] : date['day'];
                                date['hour'] = date['hour'] < 10 ? '0' + date['hour'] : date['hour'];
                                date['min'] = date['min'] < 10 ? '0' + date['min'] : date['min'];
                                date['sec'] = date['sec'] < 10 ? '0' + date['sec'] : date['sec'];
                                day_1.text(date['day'].toString().substr(0, 1));
                                day_2.text(date['day'].toString().substr(1, 1));
                                hour_1.text(date['hour'].toString().substr(0, 1));
                                hour_2.text(date['hour'].toString().substr(1, 1));
                                min_1.text(date['min'].toString().substr(0, 1));
                                min_2.text(date['min'].toString().substr(1, 1));
                                sec_1.text(date['sec'].toString().substr(0, 1));
                                sec_2.text(date['sec'].toString().substr(1, 1));
                                if (index <= 1) {if (date['day'] < 1) {dayEL.hide();} wrap.show();}
                            }
                            countFn();
                            var countId = setInterval(countFn, 1000);
                        };
                        var operationWrap = $('#operation-wrap'), adCode = ['ADSU3XLQ', 'ADSY2DWE'], list = new Array(2), number = 0, hasOperationFlag = false;
                        if (typeof window.vipshopAds !== "object") {window.vipshopAds = {};}
                        $.each(adCode, function (i, code) {
                            window.vipshopAds[code] = function (re) {
                                if(re.items && re.items.length){hasOperationFlag = true;operationWrap.parents('.index-cms').addClass('has-operation');}
                                list[i] = re.items;
                                number++;
                                if (number >= adCode.length) {
                                    if(hasOperationFlag){parseOperation();}
                                    else{$.Listeners.pub('hasNoOperationNow').success();}
                                }
                            };
                        });
                        function parseOperation() {
                            var listNew = [];
                            $.each(list, function (index, value) {
                                $.each(value, function(i, v){v.use = 0;});
                            });
                            $.each(list, function (index, value) {
                                if (!value || value.length <= 0) {return;}
                                var o = {}, count = 0;
                                o.bg = '';
                                o.data = [];
                                o.groupBg = '';
                                o.className = '';
                                o.groupFlag = 0;
                                $.each(value, function (i, v) {
                                    if(v.use){return;}
                                    if(v['name']){ v['name'] = $.trim(v['name']);}
                                    var name = v['name'];
                                    if (name.indexOf('col-time') >= 0) {
                                        var text = name.split('_');
                                        o.timeLink = v.link;
                                        o.timeBg = v.img;
                                        o.timeText = text[1];
                                        o.time = text[2];
                                        o.timeColor = '';
                                        if (text[3]) { o.timeColor = 'j-time-wrap-' + text[3];}
                                        o.timeCount = count;
                                        v.use = 1;
                                        count++;
                                    } else if (name.indexOf('col-bg') >= 0) {
                                        o.className = "operation-inner-bg";//飞出
                                        o.bigBg = v.img;
                                        v.use = 1;
                                        count++;
                                    } else if (name.indexOf('col-big-bg') >= 0) {
                                        o.className = "operation-inner-big-bg";//全屏
                                        o.bigBg = v.img;
                                        v.use = 1;
                                        count++;
                                    } else if (name.indexOf('col-group-bg') >= 0) {
                                        var g = {};
                                        g.groupBg = v.img;
                                        g.groupIndex = count;
                                        g.groupCount = parseInt(name.split('_')[1]);
                                        var start = g.groupIndex + 1;
                                        var end = g.groupCount + start;
                                        g.data = value.slice(start, end);
                                        count++;
                                        for(var i = start;  i< end; i++){
                                            if( value[i] && name.indexOf('col-time') < 0 && name.indexOf('col-bg') && name.indexOf('col-big-bg')){
                                                value[i].use = 1;count++;
                                            }
                                            value[i].marsTracker = '{"business":"www","activity_type":"tap","activity_data":{"advertisement_set":{"zone_id":"'+ value[i].id+'","campaign_id":"'+ value[i].aid+'","campaign_name":"'+ value[i].name+'","banner_id":"'+ value[i].id+'","url":"'+ value[i].link+'","unid":"'+ value[i].id+'","frame":"'+ i +'"}}}';
                                        }
                                        o.data.push(g);
                                    } else {
                                        v.marsTracker = '{"business":"www","activity_type":"tap","activity_data":{"advertisement_set":{"zone_id":"'+ v.id+'","campaign_id":"'+ v.aid+'","campaign_name":"'+ v.name+'","banner_id":"'+ v.id+'","url":"'+ v.link+'","unid":"'+ v.id+'","frame":"'+ i +'"}}}';
                                        o.data.push(v);
                                        v.use = 1;
                                        count++;
                                    }
                                });
                                listNew.push(o);
                            });
                            $('#operation-wrap').html($.Tpl('operationWrap', listNew));
                            $('.operation-group-wrap>img').removeClass('lazy').attr('src', function () {return this.getAttribute('data-original');});
                            $('.operation-item img.lazy').removeClass('lazy').attr('src', function () {return this.getAttribute('data-original');});
                            var operationInner = operationWrap.find('.operation-inner'), timeFlag = false;
                            $.each(listNew, function (index, value) {
                                var operationInnerCurrent = operationInner.eq(index);
                                var item = operationInnerCurrent.find('.operation');
                                if (value.time && !timeFlag){
                                    if (item.length > 0) {
                                        if (value.timeCount == 0) {item.eq(0).before($.Tpl('tpl-time', value));}
                                        else {item.after($.Tpl('tpl-time', value));}
                                    } else {operationInnerCurrent.find('.operation-w').html($.Tpl('tpl-time', value));}
                                    var timeJWrap = operationInnerCurrent.find('#j-time-wrap');
                                    if (value.timeLink != '') {timeJWrap.wrapInner('<a class="j-time-wrap-inner" href="' + value.timeLink + '" target="_blank" mars_sead="home_operation_time"></a>');
                                    } else {timeJWrap.wrapInner('<div class="j-time-wrap-inner"></div>');}
                                    setTime(value.time, timeJWrap);
                                    timeFlag = true;
                                }
                            });
                        }
                    });
                </script>
