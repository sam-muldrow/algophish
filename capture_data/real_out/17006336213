var ww = screen ? screen.width : 0;
var mblviewport = document.getElementById('mbl-viewport');
if (! ww) ww = window.innerWidth;
var is_landscape = (Math.abs(window.orientation) === 90);
if (ww > 599 && ww < 680)
mblviewport.setAttribute('content', 'width=700,initial-scale=0.92');
if (ww > 960 && ww < 1099)
mblviewport.setAttribute('content', 'width=1100,initial-scale=0.92');
if (ww == 768 && is_landscape)
mblviewport.setAttribute('content', 'width=1100,initial-scale=0.92');
