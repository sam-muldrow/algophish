//<![CDATA[
var arrCurrency = new Array( );
arrCurrency[0]=new MakeItem("ISK", "1.0");
arrCurrency[1]=new MakeItem("USD", "139.21");
arrCurrency[2]=new MakeItem("EUR", "152.5");
arrCurrency[3]=new MakeItem("GBP", "174.63");
arrCurrency[4]=new MakeItem("CAD", "101.47");
arrCurrency[5]=new MakeItem("DKK", "20.454");
arrCurrency[6]=new MakeItem("NOK", "13.068");
arrCurrency[7]=new MakeItem("SEK", "13.373");
arrCurrency[8]=new MakeItem("CHF", "157.66");
arrCurrency[9]=new MakeItem("JPY", "0.9436");
init( 0 );
/* When selected remove the current value */
$('document').ready(function() {
$('.currency-converter input').focus(function(e) {
$(this).select();
e.preventDefault();
});
});
document.addEventListener('DOMContentLoaded', function() {
let inCur1st = document.getElementById("_value_0");
inCur1st.value = "1000";
inCur1st.onkeyup();
}, false);
