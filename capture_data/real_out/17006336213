<script type="text/javascript">
function setCookie(name, value, days) {
var expires = "";
if (days) {
var date = new Date();
date.setTime(date.getTime() + (days*24*60*60*1000));
expires = "; expires=" + date.toUTCString();
}
document.cookie = name + "=" + (value || "")  + expires + "; path=/";
}
function getCookie(name) {
var nameEQ = name + "=";
var ca = document.cookie.split(';');
for(var i=0;i < ca.length;i++) {
var c = ca[i];
while (c.charAt(0)==' ') c = c.substring(1,c.length);
if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
}
return null;
}
function cookiesAccepted(e) {
e.preventDefault();
setCookie('cookie_consent_agreement', true, 5000);
var element, name, arr;
element = document.getElementById('cookie-consent');
name = 'd-none';
arr = element.className.split(' ');
if (arr.indexOf(name) == -1) {
element.className += ' ' + name;
}
}
</script>
<style>
#cookie-consent {
position: fixed;
bottom: 1em;
left: 1em;
width: 350px;
height: 160px;
background-color: rgb(237, 239, 245);
z-index: 99999;
font-size: 17px;
font-family: Georgia, Palatino, Times, "Times New Roman", serif;
}
#cookie-consent span {
margin: 2em 2em 2em;
display: block;
}
@media (max-width: 600px) {
#cookie-consent {
width: 100%;
left: 0;
bottom: 0;
}
#cookie-consent span {
margin: 1.2em 2em 1.2em;
}
}
#cookie-consent div a {
position: absolute;
bottom: 0;
padding: 10px;
text-align: center;
width: 100%;
background-color:#007;
color: white;
text-decoration: none;
}
#cookie-consent div a:hover {
text-decoration: none;
}
</style>
<div id="cookie-consent" style="display: none;">
<span>
Þessi síða notar vefkökur (cookies)
til að bæta upplifun notenda.
<a href="https://www.mbl.is/frettir/personuupplysingar/" target="_blank">Sjá nánar</a>
