    <script>
        (function () {
            'use strict';
            var couponInfo = document.querySelectorAll('.js_coupon-code--info'),
                remove = function (el, time) {
                    setTimeout(function () {
                        el.removeAttribute('data-hint');
                        el.classList.remove('hint--left');
                        el.classList.remove('hint--right');
                    }, time * 1000);
                },
                initOnClick = function (el) {
                    var couponCopy = el.querySelector('.js_coupon-code--copy'),
                        hintWidth = 70;

                    if (el.dataset.init === true) {
                        return;
                    }

                    el.dataset.init = true;

                    if (!!couponCopy) {
                        couponCopy.onclick = function () {
                            var tempInput = document.createElement("input");
                            tempInput.style = "position: absolute; left: -1000px; top: -1000px";
                            tempInput.value = couponCopy.dataset.code;
                            document.body.appendChild(tempInput);
                            tempInput.select();
                            document.execCommand("copy");
                            if (couponCopy.offsetLeft < hintWidth) { couponCopy.classList.add('hint--right'); }
                            else if (couponCopy.offsetLeft > window.innerWidth - hintWidth) { couponCopy.classList.add('hint--left'); }
                            couponCopy.setAttribute('data-hint', 'Skopiowano');
                            remove(couponCopy, 2);
                            document.body.removeChild(tempInput);
                        };
                    }
                };

            for (var i = 0, len = couponInfo.length; i < len; ++i) {
                initOnClick(couponInfo[i]);
            }
        })();
    </script>

                </div>
                <div class="js_content_paragraph lnd_col-10">
                                



