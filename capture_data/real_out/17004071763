                            <div class="lnd_content">
<script>
function changeHrefForContainers() {
    var barbieContainers = document.querySelectorAll('.barbie');
    var fisherContainers = document.querySelectorAll('.fisher');
    var wheelsContainers = document.querySelectorAll('.wheels');

    function changeHref(elements, newHref) {
        elements.forEach(function(element) {
            var lndButtons = element.querySelectorAll('a.lnd_button-action');
            lndButtons.forEach(function(button) {
                button.setAttribute('href', newHref);
            });
        });
    }

    changeHref(barbieContainers, '/producenci/barbie/Zabawki;basket.htm');
    changeHref(fisherContainers, '/producenci/fisher-price/Zabawki;basket.htm');
    changeHref(wheelsContainers, '/producenci/hot_wheels/Zabawki;basket.htm');
}

window.addEventListener('load', changeHrefForContainers);
</script>                            </div>
                            <div class="lnd_action">
                            </div>
                        </div>
                </div>
        </div>
    </div>
</div>




                            

