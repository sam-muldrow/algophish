<div id="5613b004-4b36-48dd-9497-b11a01035a0b" class="js_lpContent landing-page-content  " style="float:none" data-id="5613b004-4b36-48dd-9497-b11a01035a0b">
    <script type="text/javascript">
        (function(){ 
 var data = {
            "couponSelector": ".coupon-action .lnd_coupon-item",
            "valueSelector": ".coupon-action .lnd_coupon-item__value",
            "titleSelector": ".coupon-action .lnd_coupon-item__title",
            "bodySelector": ".coupon-action .lnd_coupon-item__body",
            "descriptionSelector": ".lnd_coupon-item__description",
            "title": "w kategorii %s oznaczone %b%Kup Teraz%/b%",
            "titleBuy": "na produkty oznaczone %b%Kup Teraz%/b%",
            "titlePayments": "Tylko szybkie płatności.",
            "titleDelivery": "oraz dostawy do %b%15 zł%/b%",
            "titleBuyNow": "Ważny na produkty oznaczone %b%Kup Teraz%/b%",
            "coponActions": ".coupon-action .lnd_coupon-item__actions",
}
 // skrypt zmieniający tytuł oraz wartość kuponu

var coupon = document.querySelectorAll(data.couponSelector);
        
if (coupon.length > 0) {
            coupon.forEach(function(item) {
                var value = item.querySelector(data.valueSelector);
                var title = item.querySelector(data.titleSelector);
                var actions = item.querySelector(data.coponActions);
                var description = item.querySelector(data.descriptionSelector);
                var body = item.querySelector(data.bodySelector);
                var parentContainer = item.closest('.lnd_container');
                var expiration = item.querySelector('.lnd_coupon-item__expiration');

                if (value) {
                    if (value.innerText.indexOf('zł') >= 0 || value.innerText.indexOf('%') >= 0) {
                        value.innerHTML = "Kupon -" + value.innerText;
                    }

                    if (value.innerText.indexOf('Darmowa dostawa') >= 0) {
                        value.innerHTML = "Wysyłka 0 zł";
                    }
                }

                if (title) {
                    if (title.innerHTML.indexOf('cały asortyment') >= 0){
                        title.innerHTML = data.titleBuy;
                    }
                    else if (title.innerHTML.indexOf('kategorii') >= 0) {
                        var cat = title.innerText.split('kategorii')[1].split('oznaczone')[0];
                        title.setAttribute('data-cat', cat);
                    }
                    else if(title.innerHTML.indexOf('kategoriach') >= 0) {
                        var cat = title.innerText.split(',')[0].split('kategoriach')[1].substring(1);
                        var catTitle = data.title.replace('%s', '<b>'+cat+'</b>').replace('%b%', '<b>').replace('%/b%', '</b>');
                        title.setAttribute('data-cat', cat);
                        title.innerHTML = catTitle;
                    }
                }
                else {
                    const title = document.createElement('div');
                    title.classList.add('lnd_coupon-item__title');
                    title.innerHTML = data.titleBuyNow.replace('%b%', '<b>').replace('%/b%', '</b>');
                    body.prepend(title);
                }

                if (description && parentContainer) {
                    if (parentContainer.classList.contains('coupon-action--free')) {
                        var delivery = document.createElement('div');
                        delivery.classList.add('delivery-cost');
                        delivery.innerHTML = data.titleDelivery.replace('%b%', '<b>').replace('%/b%', '</b>');
    
                        description.append(delivery);
                    }
                }

                if (actions && expiration) {
                    var fastPayments = document.createElement('div');
                    fastPayments.classList.add('fast-payments');
                    fastPayments.innerHTML = data.titlePayments;

                    expiration.before(fastPayments);
                }
            });
        } 
  })();
    </script>
</div>




            
<div id="1716a631-f492-45d0-b202-f9d093e5f4c6" class="js_lpContent landing-page-content  " style="float:none" data-id="1716a631-f492-45d0-b202-f9d093e5f4c6">
    <script type="text/javascript">
        (function(){ 
 var data = {
            "valueSelector": ".coupon-action--hot .lnd_coupon-item__value", 
            "titleSelector": ".coupon-action--hot .lnd_coupon-item__title",
            "target": ".coupon-action--hot .card-outer-header h2",
            "title": " w kategorii \"%s\""
 }
 // skrypt kopiujący tytuł kuponu do nagłówka

        var title = document.querySelector(data.titleSelector);
        var target = document.querySelector(data.target);
        var value = document.querySelector(data.valueSelector);
        
        if (value && target && title) {
            var cat = title.getAttribute('data-cat');
            if (cat) {
                var titleDescription = document.createElement('b');
                titleDescription.classList.add('title-description');
                titleDescription.append(value.innerHTML);
                titleDescription.append(data.title.replace('%s', cat));
                target.append(titleDescription);
            }
        } 
  })();
    </script>
</div>




            
<div id="56861815-901a-4c85-a543-b3ad5d788eef" class="js_lpContent landing-page-content  " style="float:none" data-id="56861815-901a-4c85-a543-b3ad5d788eef">
    <script type="text/javascript">
        (function(){ 
 var data = {
            "selector": ".coupon-action.coupon-action--hot .lnd_container-grid > div:last-child .lnd_list",
            "dateAndTimeSelector": ".lnd_object-title h2"
}
 // skrypt pokazujący elementy w określonym czasie określonym w tytule listy

        var elements = document.querySelectorAll(data.selector);
        var elementsArray = [];
        var dateNow = new Date();

        function sortByDateAndTime(elements) {
            elements.sort(function(a, b) {
                return new Date(a.dateAndTime) - new Date(b.dateAndTime);
            });
        }

        elements.forEach(function(element) {

            var dateAndTime = element.querySelector(data.dateAndTimeSelector); // przykład: 2023-02-28T15:00:00

            if (!dateAndTime) {
                return;
            }

            elementsArray.push({
                element: element,
                dateAndTime: dateAndTime.innerHTML
            })
        });

        if (elementsArray.length === 0) {
            return;
        }

        sortByDateAndTime(elementsArray);

        var currentIndex = elementsArray.findIndex(function(element) {
            return dateNow < new Date(element.dateAndTime);
        });

        elementsArray.forEach(function(element, index) {
            if (index === currentIndex) {
                return;
            }
            element.element.remove();
        }); 
  })();
    </script>
</div>




            
<div id="11ef8d22-2d21-4d5d-aff5-33f19ec15c44" class="js_lpContent landing-page-content  " style="float:none" data-id="11ef8d22-2d21-4d5d-aff5-33f19ec15c44">
    <script type="text/javascript">
        (function(){ 
 var data = {
  "container": ".coupon-action.coupon-action--hot",
            "items": [
                {
                    "selector": ".lnd_coupon-list__code",
                    "attr": "data-coupon",
                },
                {
                    "selector": ".js_content_paragraph:last-child .lnd_list",
                    "attr": "data-list",
                }
            ]
}
 // skrypt sprawdzający czy elementy istnieją i w zależności od tego ustawia atrybuty

        var container = document.querySelector(data.container);
        var items = data.items;

        if (container) {
            items.forEach(function(item) {
                var element = container.querySelector(item.selector);
                if (element) {
                    container.setAttribute(item.attr, 'true');
                }
                else {
                    container.setAttribute(item.attr, 'false');
                }
            });
        } 
  })();
    </script>
</div>




            
<div id="f327dddd-1bef-4ad4-86e3-a72004d6606b" class="js_lpContent landing-page-content  " style="float:none" data-id="f327dddd-1bef-4ad4-86e3-a72004d6606b">
    <script type="text/javascript">
        (function(){ 
 var data = {
            "selector": ".coupon-action .lnd_coupon-item__actions .lnd_button-action",
 }
         // skrypt usuwający z urla do produktów parametr minimalnej ceny (m100;)

        var elements = document.querySelectorAll(data.selector);

        elements.forEach(function(element) {
            var url = element.getAttribute('href');
            if (url) {
                element.setAttribute('href', url.replace(/m\d+;/, ''));
            }
        }); 
  })();
    </script>
</div>




            

