var trackers = __gaTracker.getAll();
trackers.forEach(function (tracker) {
var originalSendHitTask = tracker.get('sendHitTask');
tracker.set('sendHitTask',
function (model) {
var payload = model.get('hitPayload');
var currentPayloadSize = payload.length;
var trackerId = model.get('trackingId');
var trackerName = model.get('name');
if (currentPayloadSize > maxPayloadSize) {
var payloadExceededInfo = getExceededPayloadCustomDimensionValue(currentPayloadSize, trackerName);
