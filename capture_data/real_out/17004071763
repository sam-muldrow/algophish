<div id="d873a771-ae0c-48eb-a289-4ac597633261" class="lnd_object lnd_matrix   lnd_text-only    lnd_right
      lnd_bg-middle-center  lnd_h-auto">

    <div class="lnd_tint">
    </div>

    <div class="lnd_wrapper">
        <div class="lnd_grid-items">
                <div class="js_content_paragraph lnd_card lnd_text-only lnd_col-12 ">
                                            <div class="lnd_text lnd_col-8">
                            <div class="lnd_content">
Rotator - skrypt
<script>
var Rotator = function(){
    return {
        init: function(config){
             require(["jquery"], function($) { // require
                $.extend(config, {
                    interval: null,
                    start: 0,
                    end: 0 + config.range - 1
                });
                var $target = $(config.target),

                    render = function(){
                        $rotator.html('');
                        config.data.forEach(function(dataEl){ 
                            $box = $('<a>',{
                                rel: dataEl.nofollow ? "nofollow" : "",
                                class: "rotator__item",
                                style: "width:calc(100% / " + config.boxInRow + ")",
                                href: dataEl.href,
                            })
                            $img = $('<img>',{
                                src: dataEl.photo
                            })
                            $rotator.append($box);
                            $box.append($img);
                        })
                    }

                    show = function(config){
                        $rotator__items = $(config.target).find('.rotator__item'); 
                        $rotator__items.addClass('disabled');
                        $rotator__items.each(function(index, el){
                            if(index >= config.start && index <= config.end){
                                $(el).removeClass('disabled');
                            }
                        })
                    }

                    goNext = function(config){
                        config.start + config.range >= config.data.length ? config.start = 0 : config.start = config.start + config.range;
        
                        config.end = config.start + config.range - 1;
                        config.range < config.data.length ? show(config) : null;
                        refreshLazyLoad($rotator.find('.js_art-img-lazy'));
                    }

                    goPrev = function(config){
                        config.start <= 0 ? config.start = config.data.length - config.range : config.start = config.start - config.range;
        
                        config.end = config.start + config.range - 1;
        
                        config.range < config.data.length ? show(config) : null;
                        refreshLazyLoad($rotator.find('.js_art-img-lazy'));
                    },

                    refreshLazyLoad = function($el){
                        $el.trigger('appear');
                    },

                    setSlideShow = function(config){
                        if (config.autoskip){
                            config.interval = setInterval(function(){goNext(config)}, config.time);
                        }
                    },

                    stopSlideShow = function(config){
                        clearInterval(config.interval)
                    }

                    $rotator = $('<div>', {
                        class: 'rotator__container',
                        mouseover : function(){ 
                            stopSlideShow(config)
                        },
                        mouseleave : function(){
                            setSlideShow(config);
                        },
                    }),

                    $rotatorNav = $('<div>', {
                        class: 'rotator-navigation'
                    });

                    $rotatorNavNext = $('<div>', {
                        class: 'rotator-navigation__right',
                        mousedown : function(){
                            goNext(config);
                            stopSlideShow(config);
                        }
                    })
                    
                    $rotatorNavPrev = $('<div>', {
                        class: 'rotator-navigation__left',
                        mousedown : function(){
                            goPrev(config);
                            stopSlideShow(config);
                        }
                    }), 

                    dataSort = function(config) {
                        config.data.sort(function(){return Math.round(Math.random()) - 0.5;})
                        config.data.forEach(function(dataEl,i){
                            if (dataEl.position != null) {
                                dataElTemp = config.data[dataEl.position];
                                config.data[dataEl.position] = dataEl;
                                config.data[i] = dataElTemp;
                            }
                        })
                    },

                $target.addClass('rotator');
                config.mobileInline ? $target.addClass('mobile-inline') : $target.addClass('mobile-grid');

                $target.append($rotator);
                $target.append($rotatorNav);
                $rotatorNav.append($rotatorNavPrev);
                $rotatorNav.append($rotatorNavNext);
                
                dataSort(config);
                render();
                show(config);
                setSlideShow(config);
                
                if( config.range == config.data.length ) {
                    $rotatorNavNext.addClass('disabled');
                    $rotatorNavPrev.addClass('disabled');
                }
             }) // require
        }
    };
}
rotator = new Rotator();
// v 1.1.0
</script>                            </div>
                            <div class="lnd_action">
                            </div>
                        </div>
                </div>
        </div>
    </div>
</div>




                            

