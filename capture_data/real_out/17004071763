tor .rotator-navigation__right.disabled{display:none}.rotator .rotator-navigation__left:after{border-left:2px solid #0078d4;border-top:2px solid #0078d4;left:2px}.rotator .rotator-navigation__right:after{border-right:2px solid #0078d4;border-bottom:2px solid #0078d4;left:-2px}@media screen and (max-width: 720px){.rotator.mobile-grid .rotator-navigation__left{opacity:0.8}.rotator.mobile-grid .rotator-navigation__right{opacity:0.8}}.rotator:hover .rotator-navigation__left{opacity:0.8}.rotator:hover .rotator-navigation__right{opacity:0.8}

