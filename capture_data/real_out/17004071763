} else {
__gaTracker('send', 'pageview', App.variables.ga.virtualUrl);
__gaTracker('gaMain.send', 'pageview', App.variables.ga.virtualUrl);
window.dataLayer.push({ 'page': App.variables.ga.virtualUrl });
}
}
//family variant events
if (universalAnalyticsConfig.variantEvents != null && universalAnalyticsConfig.variantEvents.length > 0) {
for (var i = 0; i < universalAnalyticsConfig.variantEvents.length; i++) {
__gaTracker('send', 'event',
universalAnalyticsConfig.variantEvents[i].Name,
universalAnalyticsConfig.variantEvents[i].Value);
window.dataLayer.push({
'event': universalAnalyticsConfig.variantEvents[i].Name+'_sgtm',
'eventCategory': universalAnalyticsConfig.variantEvents[i].Value
});
}
}
</script>
<script async src='//www.google-analytics.com/analytics.js'></script>
<!-- End Google Universal Analytics -->






 

