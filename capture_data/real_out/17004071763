<style>
.page-body {
background-color: #F2F5F8!important;
}
.inspiration__rotator .lnd_wrapper {
overflow: initial;
}
</style>

<script>
window.onload = function(){
  const target = document.querySelector('.inspiration__rotator');

  var html = document.getElementsByTagName('html'),
      body = document.getElementsByTagName('body'),
      pagebody = document.getElementsByClassName('page-body');
      page = document.getElementsByClassName('page');


  function handleIntersection(entries) {
    entries.map((entry) => {
      if (entry.isIntersecting) {
        html.length > 0 ? html[0].style["overflow"] = "initial" : "";
        body.length > 0 ? body[0].style["overflow"] = "initial" : "";
        pagebody.length > 0 ? pagebody[0].style["overflow"] = "initial" : "";
        page.length > 0 ? page[0].style["overflow"] = "initial" : "";
      }
    });
  }

  const observer = new IntersectionObserver(handleIntersection);
  if(target){
    observer.observe(target);
  }
}
</script>                            </div>
