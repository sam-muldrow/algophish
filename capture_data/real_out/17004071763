_input,.lnd_article-7da6e .lnd_black textarea.lnd_textarea{color:#FFFFFF;border-color:#e3e7eb;background-color:#000000}.lnd_article-7da6e .lnd_black input.lnd_input::-webkit-input-placeholder,.lnd_article-7da6e .lnd_black textarea.lnd_textarea::-webkit-input-placeholder{color:#e3e7eb}.lnd_article-7da6e .lnd_black input.lnd_input:-moz-placeholder,.lnd_article-7da6e .lnd_black textarea.lnd_textarea:-moz-placeholder{color:#e3e7eb}.lnd_article-7da6e .lnd_black input.lnd_input::-moz-placeholder,.lnd_article-7da6e .lnd_black textarea.lnd_textarea::-moz-placeholder{color:#e3e7eb}.lnd_article-7da6e .lnd_black input.lnd_input:-ms-input-placeholder,.lnd_article-7da6e .lnd_black textarea.lnd_textarea:-ms-input-placeholder{color:#e3e7eb}.lnd_article-7da6e .lnd_accent .lnd_content,.lnd_article-7da6e .lnd_accent a,.lnd_article-7da6e .lnd_accent h1,.lnd_article-7da6e .lnd_accent h2,.lnd_article-7da6e .lnd_accent h3,.lnd_article-7da6e .lnd_accent h4,.lnd_article-7da6e .lnd_accent h5,.lnd_article-7da6e .lnd_accent h6,.lnd_article-7da6e .lnd_accent label,.lnd_article-7da6e .lnd_primary .lnd_content,.lnd_article-7da6e .lnd_primary a,.lnd_article-7da6e .lnd_primary h1,.lnd_article-7da6e .lnd_primary h2,.lnd_article-7da6e .lnd_primary h3,.lnd_article-7da6e .lnd_primary h4,.lnd_article-7da6e .lnd_primary h5,.lnd_article-7da6e .lnd_primary h6,.lnd_article-7da6e .lnd_primary label{color:#F9F9F9}.lnd_article-7da6e .lnd_accent a.lnd_link,.lnd_article-7da6e .lnd_primary a.lnd_link{color:#F9F9F9!important;border-color:#F9F9F9}.lnd_article-7da6e .lnd_accent .lnd_checkbox,.lnd_article-7da6e .lnd_primary .lnd_checkbox{color:#F9F9F9}.lnd_article-7da6e .lnd_accent input.lnd_input,.lnd_article-7da6e .lnd_accent textarea.lnd_textarea,.lnd_article-7da6e .lnd_primary input.lnd_input,.lnd_article-7da6e .lnd_primary textarea.lnd_textarea{border-color:#C7C7C7;color:#F9F9F9}.lnd_article-7da6e .lnd_accent input.lnd_input.lnd_error,.lnd_article-7da6e .lnd_accent textarea.lnd_textarea.lnd_error,.lnd_article-7