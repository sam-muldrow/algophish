    <script type="text/javascript">
        (function(){ 
 var data = {
    items: [
        {
            selector:'.coupon-action--hot.coupon-action__main', 
            category:'CouponCopy',
            action:'hotCoupon'
        },
        {
            selector:'.coupon-action.coupon-action--free', 
            category:'CouponCopy',
            action:'freeCoupon'
        }
        
    ]
}
 // skrypt dodający analitykę do kuponów

    data.items.forEach(function(item){
        var element = document.querySelector(item.selector);
        if(element){
            var copy = element.querySelector('.js_coupon-code--copy');
            if(copy){
                copy.classList.add('js_gtm_copy');
                copy.setAttribute('data-category', item.category);
                copy.setAttribute('data-action', item.action);
            }
        }
    })
 
  })();
    </script>
</div>




            

