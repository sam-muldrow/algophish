<script type="text/javascript">

//JS変数を生成
var mayya_info = {
  jiki_rank: $('#mayya_info .jiki_rank_mayya').text(),
  jiki_rank_price: $('#mayya_info .jiki_rank_price_mayya').text(),
  kikannai_sales_price: Number($('#mayya_info .kikannai_sales_price_mayya').text().replace(',', ''))
};

window.onload = function() {

  //KARTEイベントタグを送信
  try {
    krt("send", "next_nplus_promo", {jiki_rank_name: mayya_info.jiki_rank,jiki_rank_kingaku: mayya_info.jiki_rank_price,kikannai_sales_price: mayya_info.kikannai_sales_price});
  } catch(e) {
    if ('krt' in window) {
      krt('send', '_error', {error_message: e.message});
    }
  }

};

</script>



