<script>
$(function(){
var recommendParams = JSON.parse('{ "recommendParams": [] }').recommendParams;
var basicItemCd = "";
var categoryCd = "";
if (recommendParams.length > 0) {
  basicItemCd = recommendParams.map(function(item) { return item.basicItemCd; }).join("|");
  categoryCd = recommendParams.map(function(item) { return item.categoryCd; }).join("|");
}
else {
  basicItemCd = window.itemcode;
  categoryCd = window.itemCategoryId;
}
if ("NR-18-007" == "NR-18-007") {
  nissenReadHistoryPromise.then(function(data) {
    if (!data || !data.itL || data.itL.length <= 0) {
      return;
    }
    nissen_recommend_build_area(".recommend_NR-18-007", "NR-18-007", { basicItemCd: data.itL[0].itD.item.itCd, categoryCd: data.itL[0].itD.item.cId, titleString: "最近チェックした商品からのおすすめ"  }).then(function() {
      var target = $(".recommend_NR-18-007")
      target.find('.switch-btn-on').on("click", function () {
        var offset = 90;
        $('html,body').animate({ scrollTop: target.offset().top - offset }, 0);
      });
    });
  });
}
else {
  nissen_recommend_build_area(".recommend_NR-18-007", "NR-18-007", { basicItemCd: basicItemCd, categoryCd: categoryCd, titleString: "最近チェックした商品からのおすすめ"  }).then(function() {
    var target = $(".recommend_NR-18-007")
    target.find('.switch-btn-on').on("click", function () {
      var offset = 90;
      if ("NR-18-007" == "NR-18-003") {
        offset = 0;
      }
      $('html,body').animate({ scrollTop: target.offset().top - offset }, 0);
    });
  });
}
});
</script>

