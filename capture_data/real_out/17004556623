<script>
    var queryParam = "so=RCMD&limit=10&fr=%E6%9A%96%E3%83%91%E3%83%B3";
    axios.post("https://psnalize.nissen.co.jp/search-sort", {
        "search_param": queryParam,
        "user_id": visitorId,
        "sort_flg": "1"
    }).then(
        function (_data) {
            var lazyImgClassName = 'trend_lazy_img';
            var template = $("#js-item-parts").html();
            var _applyingTemplate = _.template(template.trim());
            var ids = [];
            var innerHtml = '';
            var elm = $('#trend_keyword_row');
            _.each(_data.data.itemSearchInfoXML.itemSearchInfo.itL, function (e) {
                var item = e.it;
                item._id = item.itCd;
                item.saFlg = true;
                item.img_url = item.img;
                item.trialFlag = item.dispTagList.find(function (dispTag) {return dispTag.dispTag.id == 'DC02'});
                item.lazy_img = lazyImgClassName;
                item.ct_url = "/item/" + item.itCd + "?uri=twt&2nd=ntop_sprisekw_001";
                item.title = item.itNm;
                item.price_string = "<span class='item-price-out-tax'>&yen;" + item.spwl.toLocaleString() + "</span><span class='item-price-in-tax'>(税込&yen;" + item.spl.toLocaleString() + ")" + (item.spl === item.sph ? "" : "～") + "</span>";
                ids.push(item.itCd);
                innerHtml += _applyingTemplate(item);
            });
            if (ids.length > 5) {
                var moreHtml = "";
                moreHtml += '<div class="trend_keyword_more_div"><a class="trend_keyword_more_btn trend_keyword_link" href="/search?so=RCMD&amp;fr=%E6%9A%96%E3%83%91%E3%83%B3&amp;2nd=ntop_risekw_101&sf=15">暖パンをもっと見る</a></div>';
                moreHtml += '<div class="item_btn_wrap">';
                moreHtml += '    <a class="switch-btn-off tk_off">もっと見る</a>';
                moreHtml += '    <a class="switch-btn-on tk_on">閉じる</a>';
                moreHtml += '</div>';
                $(moreHtml).appendTo(elm.parent());
            }
            $(innerHtml).appendTo(elm);
            if (ids.length > 0) {
                $(elm).parent().show();
                LazyImageIntersectionObserver(lazyImgClassName);
                // レビュー情報を取得
                reviewModule.appendReviewScore(ids, function (itemCode, aveDecimal, aveHalfStep, count) {
                    var $targetItem = elm.find("div[data-prod=" + itemCode + "]");
                    var reviewInfo = '<span class="m-item_list_reviewstars rank' + aveHalfStep + '"></span><span class="m-item_list_reviewscore">' + aveDecimal + '</span><span class="m-item_list_reviewcount">(' + count + '件)</span>';
                    // リンク付加
                    var link = $targetItem.find("a").attr("href");
                    if (link) {
                        if (link.match(/\?/)) {
                            reviewInfo = '<a href="' + link + '&qDisp=custom&temp=reviewlist">' + reviewInfo + '</a>';
                        } else {
                            reviewInfo = '<a href="' + link + '?&qDisp=custom&temp=reviewlist">' + reviewInfo + '</a>';
                        }
                    }
                    if ($.trim($targetItem.find('.js-item-review').html()) === '') {
                        $targetItem.find('.js-item-review').append(reviewInfo);
                    }
                });
                // 初期で非表示のアイテムにクラスを付与
                var expandItem = elm.find(".js-item-list:nth-child(n+6), .dv_rc_items:nth-child(n+6)").addClass("expand-item");
                // 閉じるボタン
                var closeButton = elm.parent().find(".switch-btn-on");
                // もっと見るボタン
                var openButton = elm.parent().find(".switch-btn-off");
                expandItem.hide();
                elm.parent().addClass("switch-off");
                closeButton.on("click", function () {
                    elm.parent().addClass("switch-off").removeClass("switch-on");
                    expandItem.hide();
                    $('html,body').animate({ scrollTop: elm.parent().offset().top }, 0);
                });
                openButton.on("click", function () {
                    elm.parent().addClass("switch-on").removeClass("switch-off");
                    expandItem.show();
                });
                elm.parent().find(".item_btn_wrap").show();
            }
        },
        function (_data) {
            // console.log('失敗の場合：'+_data);
        }
    );
</script>
