<script type="text/javascript">
  $(function() {
    // お気に入りアイテム
    var actk = 'OlW6-XD4';
    var cart = false;
    var limit = 15;
    var appendedParam = { "uri": "fav_top", "2nd": "ntop_fav_001"};
    var callback = function(json) {
      viewFavoriteItem(actk, cart, appendedParam)(json);
      LazyImageIntersectionObserver("fav_lazy_img");
      var $favBuildSelector = $('#favorite-block');
      var $favItems = $favBuildSelector.find('.js-item-list');
      if ($favItems.length <= 5) {
        $('#favorite-item-btn').hide();
      } else {
        $('#favorite-item-btn').show();
      }
    }
    favModule.displayFavoritedItem(1, limit, actk, callback);
  });
</script>


