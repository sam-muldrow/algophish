<script>
    // 未読数を更新
    function setUnreadNotifCount(unreadCount) {
        if (unreadCount < 0) {
            $('li.hdr_nav_news, li.hdr-nav_news').hide();
        } else {
            $('li.hdr_nav_news, li.hdr-nav_news').show();
            $('#news_num').toggle(unreadCount > 0);
            if (unreadCount < 99) {
                $('#news_num').text(unreadCount);
                return;
            }
            $('#news_num').text(99);
        }
    }

    function updateNotificationCount() {
        // Flamingo対策
        if ($('li.hdr_nav_news, li.hdr-nav_news').length == 0) {
            return;
        }

        // common.jsをとれていないユーザーは除外
        if (typeof nolGetURLParam != 'function') {
            $('li.hdr_nav_news, li.hdr-nav_news').hide();
            return;
        }

        // 前回アクセス時の未読数を取得
        var unreadNotifCount = localStorage.getItem("unread_number_of_notifications");
        // 特定の流入の場合は処理を中断
        var utmSorce = nolGetURLParam("utm_source");
        var newsFlag = nolGetURLParam("news_flag");
        if (newsFlag != 1 && (utmSorce == "line" || utmSorce == "app_search")) {
            setUnreadNotifCount(unreadNotifCount);
            if (location.href.match(/userinfo/)) {
                initNews();
            }
            return;
        }

        // 前回アクセス日時を取得
        var notifLastHour = localStorage.getItem("unread_number_of_notifications_last_hour");
        // 現在日時を取得
        var now = new Date();
        var curentHour = "" + now.getDate() + now.getHours();
        // 前回アクセス時のお気に入りAPI取得結果を取得
        var favRes = localStorage.getItem("favorite_result");

        // 各種条件がTRUEならお気に入りリストを取得
        if (notifLastHour != curentHour ||
            unreadNotifCount === null ||
            favRes != "200" ||
            location.href.match(/userinfo/)
        ) {
            //getFavorite------------ start
            var favItemList = [];
            $.ajax({
                url: "/member/api/favorite-item?op=getItem&time=" + curentHour,
                data: {},
                dataType: "json",
                type: "GET",
                timeout: 7000,
                cache: true
            }).then(function (data) {
                    var statusCode = data.statusCode;
                    localStorage.setItem('favorite_result', statusCode);
                    if (statusCode != "200") {
                        return;
                    }
                    var itemList = data.data["list"];
                    if (itemList != null && itemList.length > 0) {
                        for (var i = 0; i < itemList.length; i++) {
                            if (itemList[i].skuCode != null) {
                                var addFavItem = {
                                    "itemName": itemList[i].itemName,
                                    "skuCode": itemList[i].skuCode,
                                    "atRegSalesPriceL": (itemList[i].atRegSalesPriceL) ? itemList[i].atRegSalesPriceL : '',
                                    "atRegSalesPriceH": (itemList[i].atRegSalesPriceH) ? itemList[i].atRegSalesPriceH : '',
                                    "atRegSalesPriceInTaxL": (itemList[i].atRegSalesPriceInTaxL) ? itemList[i].atRegSalesPriceInTaxL : '',
                                    "atRegSalesPriceInTaxH": (itemList[i].atRegSalesPriceInTaxH) ? itemList[i].atRegSalesPriceInTaxH : '',
                                    "minSalesPriceWithoutTax": (itemList[i].minSalesPriceWithoutTax) ? itemList[i].minSalesPriceWithoutTax : '',
                                    "maxSalesPriceWithoutTax": (itemList[i].maxSalesPriceWithoutTax) ? itemList[i].maxSalesPriceWithoutTax : '',
                                    "minSalesPrice": (itemList[i].minSalesPrice) ? itemList[i].minSalesPrice : '',
                                    "maxSalesPrice": (itemList[i].maxSalesPrice) ? itemList[i].maxSalesPrice : '',
                                    "detailUrl": itemList[i].detailUrl,
                                    "registerDateTime": itemList[i].registerDateTime
                                };
                                favItemList.push(addFavItem);
                            }
                        }
                    }
                    var path = "";
                    if (location.hostname === "www.nissen.co.jp") {
                        path = "https://notif.nissen.co.jp/create/";
                    } else if (location.hostname === "efs-pre.nissen.co.jp" ||
                               location.hostname === "efs-test.nissen.co.jp" ||
                               location.hostname === "efs-test-origin.nissen.co.jp" ||
                               location.hostname === "efs-edu.nissen.co.jp" ||
                               location.hostname === "obd-pre.nissen.co.jp" ||
                               location.hostname === "obd-test.nissen.co.jp" ||
                               location.hostname === "obd-edu.nissen.co.jp" ||
                               location.hostname === "nnn-origin.nissen.co.jp" ||
                               location.hostname === "localhost") {
                        path = "https://notif.nissen.co.jp/create/test";
                    }
                    axios.post(path, {
                            "tan_id": sessionModule.getDuid(),
                            "user_id": visitorId,
                            "login_flg": !customerId ? "0" : "1",
                            "favorite": favItemList,
                            "rank": customerRankId
                        }, {
                            headers: {
                                "x-api-key": "38tuzE992S7d6eiJM8k7V5XIAyWmIDr85zkvcA8x"
                            }
                        })
                        .then(function (resp) {
                            try {
                                var overview = resp.data.overview;
                                if (overview.service_status == "0") {
                                    setUnreadNotifCount(-1);
                                    if (location.href.match(/userinfo/)) {
                                        window.location.href = '/info/maintenance/versionup.html';
                                    }
                                    return;
                                }
                                localStorage.setItem("unread_number_of_notifications_last_hour", curentHour);
                                if (overview.service_status == 1) {
                                    if (overview.result_process == 0) {
                                        //未読件数初期化
                                        setUnreadNotifCount(0);
                                        if (!location.href.match(/userinfo/)) {
                                            //countAPI ---------- start
                                            var countPath = "https://notif.nissen.co.jp/count/";
                                            if (location.hostname === "efs-pre.nissen.co.jp" || 
                                                location.hostname === "efs-test.nissen.co.jp" ||
                                                location.hostname === "efs-test-origin.nissen.co.jp" ||
                                                location.hostname === "efs-edu.nissen.co.jp" ||
                                                location.hostname === "obd-pre.nissen.co.jp" ||
                                                location.hostname === "obd-test.nissen.co.jp" ||
                                                location.hostname === "obd-edu.nissen.co.jp" ||
                                                location.hostname === "nnn-origin.nissen.co.jp" ||
                                                location.hostname === "localhost") {
                                                countPath = "https://notif.nissen.co.jp/count/test";
                                            }
                                            axios.post(countPath, {
                                                    "tan_id": sessionModule.getDuid(),
                                                    "user_id": visitorId,
                                                    "login_flg": !customerId ? "0" : "1"
                                                }, {
                                                    headers: {
                                                        "x-api-key": "38tuzE992S7d6eiJM8k7V5XIAyWmIDr85zkvcA8x"
                                                    }
                                                })
                                                .then(function (resp) {
                                                    localStorage.setItem("unread_number_of_notifications",
                                                        resp
                                                        .data
                                                        .detail.unread_count_item);
                                                    setUnreadNotifCount(resp.data.detail.unread_count_item);
                                                }).catch(function (error) {
                                                    console.error("updateNotificationCount:COUNTAPI:" +
                                                        error);
                                                })
                                            //countAPI ---------- end
                                        }
                                        return;
                                    } else {
                                        console.error("updateNotificationCount:result_process:" + JSON
                                            .stringify(resp.data));
                                    }
                                } else {
                                    console.error("updateNotificationCount:service_status:" + JSON
                                        .stringify(resp.data));
                                }
                            } catch (error) {
                                console.error("updateNotificationCount:" + error + JSON.stringify(resp.data));
                            }
                        }).catch(function (error) {
                            localStorage.setItem("unread_number_of_notifications_last_hour", curentHour);
                            console.error("updateNotificationCount:API:" + error);
                        }).then(function () {
                            if (location.href.match(/userinfo/)) {
                                initNews();
                            }
                        });
                },
                function () {
                    localStorage.setItem('favorite_result', false);
                }
            );
            //getFavorite------------ end
        } else {
            setUnreadNotifCount(unreadNotifCount);
        }
    }

    $('li.hdr_nav_news, li.hdr-nav_news').show();
    $(document).ready(updateNotificationCount);
</script>


