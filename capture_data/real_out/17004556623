<script type="text/javascript">
$(document).ready(function() {
    var count = 9;
    var promise = nissenCategoryHistoryApiModule.getCategoryHistory(count);
    var html = '';
    var countVal = 1;
    promise.then(function (data) {
      if (data.length > 0) {
          html += '<div class="ch-temp-card item_attr_sec"><h2>最近チェックしたカテゴリ</h2>';
          html += '<div id="itemArea"><ul>';
          for (var i=0; i<data.length; i++) {
              html += '<li><a href="' + data[i].url + '?2nd=ntop_caterireki_001&uri=cateread_ntop_1_0' + countVal + '">';
              html += '<span class="text">' +  data[i].shop + ' – <span>' + data[i].category + '</span></span></a></li>';
              countVal++;
          }
          html += '</ul></div></div>';
          $('#category_history_area').replaceWith(html);
      }
    });
});
</script>
<section id="category_history_area">
</section>


