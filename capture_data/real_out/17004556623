        <script>
              $(function(){
                var soaring_cntup = function(){
                  $('.hdr .hdr_main .hdr_main_inr .hdr_surgeword ul').addClass('active');
                  var container_width = $('.hdr_surgeword').width() - $('.hdr_surgeword p').outerWidth();
                  if (container_width) {
                    var width=0;
                    $('.hdr_surgeword li').each(function(){
                    width += $(this).outerWidth(true);
                    if (width >= container_width) { $(this).addClass('remove'); }
                    });
                  }
                }
                soaring_cntup();			
              });
        </script>

