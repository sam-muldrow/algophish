<script>
$(function(){
var option = {"qsa" : "uri=read_ntop&2nd=ntop_rireki_001"};
option.templateString = $('#js-item-parts').html().trim();
nissenReadHistoryPromise = history_build_area('.js-readhistory-placeholder', 15, 5, option).then(function(data) {
  var target = $(".js-readhistory-placeholder")
  target.find('.js-switch-btn-off').on("click", function () {
    $('html,body').animate({ scrollTop: target.offset().top - 0 }, 0);
  });
  return data;
});
});
</script>


    

