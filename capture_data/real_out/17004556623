<script>
  function openMailModal() {
    if ($(".top-mail_wrap").length == 0) {
      var mailModalString = "";
      mailModalString += '<div class="m-modal_wrap_top-mail top-mail_wrap clx modal-off">';
      mailModalString += '  <div class="m-modal_top-mail">';
      mailModalString += '    <div class="m-modal_inr top-mail">';
      mailModalString +=
        '      <div class="top-mail-window-close m-modal_close" onclick="utilModule.modal(\'.top-mail_wrap\')"></div>';
      mailModalString += '        <div class="top-mail-window">';
      mailModalString += '            <h2>メールマガジンの登録・解除</h2>';
      mailModalString += '            <div class="top-mail-window-area">';
      mailModalString += '                <p>ニッセン会員の方</p>';
      mailModalString += '                <div class="top-mail-window-area-btn">';
      mailModalString += '                  <a href="/member/incoming-mail-list" class="m-btn-next">ログイン</a>';
      mailModalString += '                </div>';
      mailModalString += '            </div>';
      mailModalString += '            <div class="top-mail-window-area">';
      mailModalString += '                <p>ゲストの方</p>';
      mailModalString += '                <div class="top-mail-window-area-btn">';
      mailModalString += '                    <a href="/mail/register" class="m-btn-prev">メールマガジンの登録</a>';
      mailModalString += '                </div>';
      mailModalString += '                <div class="top-mail-window-area-btn">';
      mailModalString += '                    <a href="/mail/cancel" class="m-btn-prev">メールマガジンの解除</a>';
      mailModalString += '                </div>';
      mailModalString += '                <div class="top-mail-window-area-btn">';
      mailModalString +=
        '                    <a href="/mail/arrival-item?op=cancelForm" class="m-btn-prev">入荷お知らせメールの解除</a>';
      mailModalString += '                </div>';
      mailModalString += '            </div>';
      mailModalString += '        </div>';
      mailModalString += '    </div>';
      mailModalString += '  </div>';
      mailModalString += '</div>';
      mailModalString += '<style>';
      mailModalString += '.top-mail_wrap .top-mail {';
      mailModalString += '  width: 590px !important;';
      mailModalString += '  margin: -265px auto 0 auto !important;}';
      mailModalString += '.top-mail_wrap .top-mail .top-mail-window h2 {';
      mailModalString += '  font-size: 16px;';
      mailModalString += '  color: #000;';
      mailModalString += '  font-weight: normal;';
      mailModalString += '  padding-bottom: 10px;';
      mailModalString += '  border-bottom: 1px solid #d1d1d1;}';
      mailModalString += '.top-mail_wrap .top-mail .top-mail-window p {';
      mailModalString += '  font-size: 16px;';
      mailModalString += '  color: #000;';
      mailModalString += '  margin: 30px auto 18px;}';
      mailModalString += '.top-mail_wrap .top-mail .top-mail-window .top-mail-window-area-btn {';
      mailModalString += '  width: 285px;';
      mailModalString += '  margin: 10px auto 0;}';
      mailModalString += '.top-mail_wrap .top-mail .top-mail-window .top-mail-window-close {';
      mailModalString += '  content: "";';
      mailModalString += '  width: 46px;';
      mailModalString += '  height: 46px;';
      mailModalString += '  border-radius: 2px;';
      mailModalString += '  background: #000 url("/res/p/img/common/m-modal_close.png") no-repeat;';
      mailModalString += '  cursor: pointer;';
      mailModalString += '  position: absolute;';
      mailModalString += '  top: -23px;';
      mailModalString += '  right: -23px;}';
      mailModalString += '</style>';
      $("footer").append(mailModalString);
    }
    utilModule.modal('.top-mail_wrap')
  }
</script>
