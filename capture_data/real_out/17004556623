<style>
  .letroUgcsetModalActive .letroUgcsetPostRelatedItemsInner .letroUgcsetPostRelatedItem {
    opacity: 0 !important;
  }

  .letroUgcsetModal[data-contents=letro-ugcset],
  .letroUgcsetModal[data-contents=letro-ugcset] * {
    display: inline-block !important;
  }
</style>

<script>
  var visumoInitializer = function () {
    // 監視対象を取得
    var preview_content_target = document.getElementsByTagName('body')[0];

    // 監視対象が存在しないなら再実行
    if (preview_content_target) {
    } else {
      setTimeout(visumoInitializer, 300);
      return;
    }

    // observerの作成
    var preview_content_observer = new MutationObserver(function (records) {
      records.forEach(function (record, index, array) {
        if (record.type === 'childList') {
          if (record.addedNodes && record.addedNodes.length > 0) {
            Array.prototype.slice.call(record.addedNodes, 0).forEach(function (node, index, array) {
              // Element以外は飛ばす
              if (node.nodeType != 1) {
                return;
              }

              var itemElements = node.getElementsByClassName('letroUgcsetPostRelatedItem');
              if (itemElements && itemElements.length > 0) {
                Array.prototype.slice.call(itemElements, 0).forEach(function (ele, index, array) {
                  // Element以外は飛ばす
                  if (ele.nodeType != 1) {
                    return;
                  }

                  var ankenNumber = '2nd=ntop_insta';
                  var productCode = ele.dataset.href.slice(-12);
                  var itemListInner = document.getElementsByClassName('letroUgcsetModalActive')[0].getElementsByClassName('letroUgcsetPostRelatedItemsInner')[0];

                  ele.insertAdjacentHTML('beforeend', '<div class="itemPrice"></div>');

                  var itemPrice = document.getElementsByClassName('itemPrice')[index];

                  if (productCode) {
                    $.ajax({
                      url: 'https://' + document.domain + '/api/v1/item/detail/' + productCode + '/json',
                      type: 'GET',
                    }).done(function (resp) {
                      // 正常処理
                      if (resp.itemInfoXML.rcd == 200) {
                        // 画像の変更
                        ele.getElementsByClassName('letroUgcsetPostRelatedItemImg')[0].firstElementChild.setAttribute('src', resp.itemInfoXML.item.mainImgList[0].imgInfo.path.replace('_l_', '_m_'));

                        // リンクの設定
                        itemListInner.getElementsByTagName('a')[index].setAttribute('data-new-window', '0');
                        itemListInner.getElementsByTagName('a')[index].setAttribute('href', '/item/' + productCode + '?' + ankenNumber);

                        // 商品名の変更
                        ele.getElementsByClassName('letroUgcsetPostRelatedItemTitle')[0].innerHTML = resp.itemInfoXML.item.itNm;

                        // 価格（価格カンマ入り）
                        var itemPriceSet = '<span class="item-price-out-tax">￥' + String(resp.itemInfoXML.item.spwl).replace(/(\d)(?=(\d\d\d)+(?!\d))/g, '$1,') + '</span><span class="item-price-in-tax">(税込￥' + String(resp.itemInfoXML.item.spl).replace(/(\d)(?=(\d\d\d)+(?!\d))/g, '$1,') + ')';
                        if (resp.itemInfoXML.item.spl < resp.itemInfoXML.item.sph) {
                          itemPrice.innerHTML = itemPriceSet + '〜</span>';
                        } else {
                          itemPrice.innerHTML = itemPriceSet + '</span>';
                        }
                        ele.setAttribute('style', 'opacity: 1 !important;transition: 1s .5s ease-in !important;');
                      }
                      // 異常処理（商品データなし）
                      else {
                        // 要素自体を削除
                        ele.remove();
                        console.log('letro:(rcd == 404) 商品情報が有りません。/ itemCode: ' + productCode);
                      }
                    }).fail(function (error) {
                      // 要素自体を削除
                      ele.remove();
                      console.log('letro:(responseError) 商品情報の取得に失敗しました。/ itemCode: ' + productCode);
                    });
                  }
                });
              }
            });
          }
        }
      });
    });

    // 監視オプションの作成
    var preview_content_options = {
      childList: true,
    };

    // 監視の開始
    preview_content_observer_added = true;
    preview_content_observer.observe(preview_content_target, preview_content_options);
  };

  setTimeout(visumoInitializer, 300);
</script>



