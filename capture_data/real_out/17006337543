    function downloadJSAndCSSAtOnload() {
      var element = document.createElement("script");
      element.src = "/assets/provider_scripts-aa060d1e611202d3f1f6e12ee2f72b538a5332c6e7502b3231c6cf98982790e1.js?app=irishcentral";
      document.body.appendChild(element);
