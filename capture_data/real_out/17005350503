<script type="text/javascript">
        function getDay() {
            var a = new Date();
            var weekdays = new Array(7);
            weekdays[0] = "pazar";
            weekdays[1] = "pazartesi";
            weekdays[2] = "salı";
            weekdays[3] = "çarşamba";
            weekdays[4] = "perşembe";
            weekdays[5] = "cuma";
            weekdays[6] = "cumartesi";
            var r = weekdays[a.getDay()];

            return r;
        }

        function getMonth() {
            var a = new Date();

            var monthNames = ["ocak", "şubat", "mart", "nisan", "mayıs", "haziran",
                "temmuz", "ağustos", "eylül", "ekim", "kasım", "aralık"
            ];

            return monthNames[a.getMonth()];
        }

        var d = new Date();
        var h = d.getHours();

        window.bk_async = function() {
            bk_addPageCtx('page_category', "Anasayfa");
            //bk_addPageCtx('page_cat_1', "subcategory1");
            //bk_addPageCtx('page_cat_2', "subcategory2");
            //bk_addPageCtx('page_cat_2', "subcategory3");
            bk_addPageCtx('saat', h);
            bk_addPageCtx('gun', getDay());
            bk_addPageCtx('ay', getMonth());

            //bk_addPageCtx('keywords', "");
            BKTAG.doTag(65994, 4); };
