$(document).ready(function() {
    $('#nidbox_email').focus();

    $('#nidbox_email').change(function(){
        if ($('#nidbox_email').val()!='')
        $('#hintemail').hide();
    }).keydown(function(){
        $('#hintemail').hide();
    }).change();

    $('#nidbox_password').change(function(){
        if ($('#nidbox_password').val()!='')
        $('#hintpassword').hide();
    }).keydown(function(){
        $('#hintpassword').hide();
    }).change();

    $('#hintemail').click(function(){
    $('#hintemail').html('');
    $('#nidbox_email').focus(); return false;
    });

    $('#hintpassword').click(function(){
    $('#hintpassword').html('');
    $('#nidbox_password').focus(); return false;
    });

});
</script>


