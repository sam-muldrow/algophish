	<script>
		document.addEventListener('DOMContentLoaded', () => {
			let top = 0;
			$('#scroll-top').on('click', function() {
                let position = $(window).scrollTop();
                if (top && !position) {
                    $('html,body').animate({scrollTop: top}, 300);
                    top = 0;
                } else {
                    $('html,body').animate({scrollTop: 0}, 300);
					top = position;			
                }
            });

            $(window).on('scroll', function() {
                let btn      = $('#scroll-top');
                    position = $(this).scrollTop();

                btn.toggleClass('go-bottom', position == 0 && top > 0);
                btn.toggleClass('active-btn', position >= 1000 || top > 0);
            });
          });
     
        </script> 
		
		
			<noscript>
			<img alt="" height="1" width="1" style="display:none" src="https://www.facebook.com/tr?id=1035107587296605&ev=PageView&noscript=1"/>
			</noscript>
		
