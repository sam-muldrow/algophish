<script>// <![CDATA[
; function GEO_parseUrlParams() {
  var winSearch = window.location.search,
    search = winSearch.substring(1, winSearch.length).split('&'),
    urlWithCleanParams = window.location.href.split('?')[0],
    newSearch = new Array(),
    rxRemove = /^(geo_c|geo_r|language|georedirect|cs_redirect)=.*/;

  window.REDIRECT = {};
  window.REDIRECT.search = '';

  if (search === '' || search[0].length === 0) {
    return urlWithCleanParams;
  }

  for (var i = 0; i < search.length; i++) {
    if (rxRemove.test(search[i]) === false) {
      newSearch.push(search[i]);
    }
    else {
      if (search[i] === 'cs_redirect=1') {
        window.REDIRECTED = true;
        document.querySelector('html')['classList'] += ' REDIRECTED ';
      }
      else if (search[i].indexOf('geo_c=') === 0) {
        window.REDIRECT.country = search[i].split('=')[1].toLowerCase();
        window.REDIRECT.search = '?cs_redirect=1';
      }
      else if (search[i].indexOf('geo_r=') === 0) {
        window.REDIRECT.subdivision = search[i].split('=')[1].toLowerCase();
      }
    }
  }

  if (newSearch.length > 0) {
    urlWithCleanParams = urlWithCleanParams + '?' + newSearch.join('&');
    window.REDIRECT.search += (window.REDIRECT.search === '') ? '?' : '&';
    window.REDIRECT.search += newSearch.join('&') + window.location.hash;
  }
  return urlWithCleanParams + window.location.hash;
}

var cleanUrl = GEO_parseUrlParams();

if(cleanUrl !== window.location.href) {
  history.replaceState(null, null, cleanUrl);
}// ]]></script>
<script>// <![CDATA[
; 
if (window.REDIRECT.country) {
  var redirectPath = '/',
    country = {},
    toCheck = [window.REDIRECT.country, window.REDIRECT.country + '-' + window.REDIRECT.subdivision];

  for (var i = 0; i < toCheck.length; i++) {
    if (window.REDIRECT_TARGETS.hasOwnProperty(toCheck[i])) {
      country = window.REDIRECT_TARGETS[toCheck[i]];
      break;
    }
  }

  switch (Object.keys(country).length) {
    case 0: break;
    case 1: redirectPath = country[Object.keys(country)[0]]; break;
    default:
      redirectPath = country[Object.keys(country)[0]];
      for (var lang of window.navigator.languages) {
        var lng = lang.split('-')[0];
        if (country.hasOwnProperty(lng)) {
          redirectPath = country[lng];
          break;
        }
      }
      break;
  }
  if(redirectPath !== '/') {
    window.location.href = redirectPath + window.REDIRECT.search;
  }
}
