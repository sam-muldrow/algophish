    <script defer>
        (function() {
            function preLoad() {
                var i = document.createElement('iframe');
                i.style.display = 'none';
                i.src = window.doccloud.Commerce.preloadurl;
                i.id = 'ecommPreload';
                document.body.appendChild(i);
            }

            setTimeout(preLoad, 3000);
        })();
