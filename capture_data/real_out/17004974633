                document.addEventListener("DOMContentLoaded", () => {
                    const popularTabs = document.querySelectorAll('#nav-popular > a');
                    const nextPan = document.querySelector('#vod-popular-panes > .tab-pane-next');
                    const prevPan = document.querySelector('#vod-popular-panes > .tab-pane-prev');

                    nextPan.addEventListener('click', e => {
                        let popularTabActive = document.querySelector('#nav-popular > a.active');
                        popularTabActive.nextElementSibling ? popularTabActive.nextElementSibling.click() : popularTabs[0].click();
                    });

                    prevPan.addEventListener('click', e => {
                        let popularTabActive = document.querySelector('#nav-popular > a.active');
                        popularTabActive.previousElementSibling ? popularTabActive.previousElementSibling.click() : popularTabs[popularTabs.length-1].click();
                    });
                });
