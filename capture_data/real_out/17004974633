    <script>
        document.addEventListener("DOMContentLoaded", () => {
            // Construct featuredCarousel
            const featuredCarousel = new Swiper('#vod-featured-carousel', {
                init: true,
                //slidesPerView: 2,
                loopedSlides: 3,
                loop: true,
                watchSlidesProgress: true,
                watchSlidesVisibility: true,
                centeredSlides: true,
                autoHeight: false,
                rebuildOnUpdate: true,
                preventClicks: true,
                allowTouchMove: false,
                preloadImages: true,
                updateOnImagesReady: true,
                breakpoints: {
                    640: {
                        slidesPerView: 1,
                        spaceBetween: 0
                    },
                    1028: {
                        slidesPerView: 2,
                        spaceBetween: 30
                    },
                },
                on: {
                    init: function () {
                        console.log('featuredCarousel initialized');
                        // this.el.classList.add('loading');
                    },
                    imagesReady: function () {
                        this.el.classList.remove('loading');
                        //this.el.querySelector('.slide-loader').classList.add('d-none');
                        //this.el.querySelector('.swiper-wrapper').classList.remove('d-none');
                        console.log('featuredCarousel images ready');
                    },
                    resize: function () {
                        console.log('featuredCarousel resized');
                    },
                    slideChange: function () {
                        const slides = this.slides;

                        slides.forEach((element, index) => {
                            if (index == this.activeIndex) {
                                element.style.cursor = 'pointer';
                                element.addEventListener('click', onceLink);
                            } else {
                                element.style.cursor = 'inherit';
                            }
                        });
                    },
                },
                navigation: {
                    nextEl: '.button-next',
                    prevEl: '.button-prev',
                },
            });

            // Update carousels when Sidebar toogle
            window.addEventListener('sidebarToggle', () => {
                featuredCarousel.update();
            });
            
            function onceLink(e) {
                e.currentTarget.removeEventListener('click', onceLink);
                // console.log(this.dataset.href);
                window.location = e.currentTarget.dataset.href;
            }
        });
    </script>
