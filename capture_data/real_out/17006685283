<script type="text/javascript" data-cfasync="false">

            window.addEventListener('load', (function () {
                var loaded = false, storageKey = 'canLoadWidget';

                window.startConversation = startConversation;

                if (localStorage.getItem(storageKey) === '1') {
                    loadWidget();
                    loaded = true;
                }

                function loadWidget(cb) {
                    var url = "https://chat-widget.thulium.com/app/chat-loader.js?hash=morelenet-834bb66a-f3c8-4583-8255-730a8556b60a";
                    var l = document.createElement("script");
                        l.async = true;
                    (document.getElementsByTagName("script")[0]).parentNode.appendChild(l);

                    window._tc = window._tc || function () {
                        (_tc.q = _tc.q || []).push(arguments);
                    };

                    l.onload = function () {
                        
                        window._tc.event_listener(function(event) {
                            const toggleClass = (force) => {
                                const wrapper = document.querySelector('.thulium-chat-wrapper.thulium-mobile-chat')
                                if (wrapper) wrapper.classList.toggle('thulium-mobile-chat-opened', force)
                            }
                            if (event.name === "closed") {
                                localStorage.setItem(storageKey, '0');
                                toggleClass(false);
                            }
                            if (event.name === "opened") {
                                localStorage.setItem(storageKey, '1');
                                toggleClass(true);
                            }
                        });

                        cb && cb();
                    }


                    l.src = url;
                }

                function startConversation(event) {
                    if(event) event.preventDefault();

                    if (!loaded) {
                        loadWidget(openChat);
                        loaded = true;
                    } else {
                        openChat();
                    }
                }

                function openChat() {
                    window._tc.open_chat();
                    localStorage.setItem(storageKey, '1');
                }
                
            }));
        </script>
<div id="thulium-container"></div>
