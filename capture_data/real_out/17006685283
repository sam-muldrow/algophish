<script data-cfasync="false" type="text/javascript">
    window.gaCode = function() {
                                        
        // przykładowe użycie trackingData
        var pageType = 'home page';
        var type = 'Strona główna';

        var userId = '';

        var dataLayer = window.dataLayerLogger ? window.dataLayerLogger(window.dataLayer || []) : window.dataLayer || [];

                    dataLayer.push({
                'ab_test': 'paynow_test=none, reco_slider_test=none, sample_test=none, search_engine_fifty_fifty_test=none'
            })
        
        function isInStandaloneMode() {
            return Boolean(
                (window.matchMedia && window.matchMedia('(display-mode: standalone)').matches)
                || window.navigator.standalone, // Fallback for iOS
            );
        }

                    if(isInStandaloneMode()) {
                dataLayer.push({
                'TWAapp': '1'
            })
            } else {
                dataLayer.push({
                    'TWAapp': '0'
                })
            }
         
        // zbierz produkty w koszyku
        var products = [];
        
        
        /* ==========================================================================
                ========================================================================== */
        
            dataLayer.push({
                'PageType': pageType,
                'type': type,
                'userId': userId ? userId : undefined,
            });

                        dataLayer.push({
                'PageType': pageType,
                'type': type,
                'userId': userId ? userId : undefined,
                'ecommerce': {
                    'impressions': window.impressions || [],
                },
                'event': 'dlReadyImpressions'
            });

                        document.addEventListener('pushImpressionsData', function(e) {
                var impression = e.detail;
                dataLayer.push({
                    'PageType': pageType,
                    'type': type,
                    'userId': userId ? userId : undefined,
                    'ecommerce': {
                        'impressions': impression ? impression.slice(0,30) : [],
                    },
                    'event': 'dlReadyImpressions'
                });
            });

                        dataLayer.push({
                event: 'dlPromotionView',
                ecommerce: {
                    promoView: {
                        promotions: window.firstPromotionData ? [window.firstPromotionData] : [],
                    }
                }
            });

                        document.addEventListener('pushPromotionData', function(e) {
                var promotion = e.detail;
                dataLayer.push({
                    event: 'dlPromotionView',
                    ecommerce: {
                        promoView: {
                            promotions: promotion ? [promotion] : [],
                        }
                    }
                });
            });

                        document.body.addEventListener('click', function(e) {
                var target = e.target;
                var slide = target.closest('.ps-window-slide');
                if(slide) {
                    dataLayer.push({
                        event: 'dlPromotionClick',
                        ecommerce: {
                            promoClick: {
                                promotions: [{
                                    id: slide.getAttribute('data-id'),
                                    name: slide.getAttribute('data-name'),
                                    creative: 'home-promotion-slider',
                                    position: 'slide_' + (Number(slide.getAttribute('data-index')) + 1),
                                }]
                            }
                        }
                    });
                }
            });

                        document.body.addEventListener('click', function(e) {
                var target = e.target;
                var slide = target.closest('.news-slide');
                if(slide) {
                    dataLayer.push({
                        event: 'dlPromotionClick',
                        ecommerce: {
                            promoClick: {
                                promotions: [{
                                    id: slide.getAttribute('data-id') || slide.getAttribute('data-element-id'),
                                    name: slide.getAttribute('data-name') || slide.getAttribute('data-element-name'),
                                    creative: 'aktualnosci',
                                    position: 'slide_' + (Number(slide.getAttribute('data-index')) + 1),
                                }]
                            }
                        }
                    });
                }
            });

                        document.body.addEventListener('click', function(e) {
                var target = e.target;
                var product = target.closest('.product-slide');
                if(product) {
                    var parent = target.closest('[data-box-type]');
                    var list = parent.querySelector('[data-box-title]');
                    var productName = product.getAttribute('data-element-name');

                    dataLayer.push({
                        'ecommerce': {
                            'click': {
                                'actionField': {
                                    'list': list.getAttribute('data-box-title'),
                                },
                                'products': [{
                                    'name': productName && productName.trim(),
                                    'id': product.getAttribute('data-element-id'),
                                    'price': product.getAttribute('data-element-price'),
                                    'category': product.getAttribute('data-element-category'),
                                    'brand': product.getAttribute('data-element-brand'),
                                    'position': product.getAttribute('data-element-position'),
                                }]
                            }
                        }, 'event':'dlClickReady'
                    });
                }
            });

        /* ==========================================================================
                ========================================================================== */
        
        /* ==========================================================================
                ========================================================================== */
        
    };
</script>

