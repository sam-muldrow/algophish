     <script>
    // Variables para el Slider 0
    var dataTrackId = 'Supercuenta Cookie - Personas Home';
    var linkHref = 'https://www.santander.com.ar/personas/cuentas-y-paquetes/super-cuenta';
    var imgTitle = 'Sacá tu tarjeta de débito GRATIS. Abrí tu SuperCuenta 100% online y empezá a ahorrar.';
    var imgDesktop = '/banco/wcm/connect/0a5cc47e-78b6-4056-b86e-51135189d143/Slide-SCno+clientes-Es+muy+facil-DESK.jpg?MOD=AJPERES';
    var imgMobile = '/banco/wcm/connect/6ee84e4c-8c7c-47a8-8e96-421728f146ce/Slide-SCno+clientes-Esmuyfacil-MOB.jpg?MOD=AJPERES';
    var dataGact = 'personas';
    var dataGaid = 'Supercuenta Cookie - Personas Home';
    // Variables para el Slider 1
    var dataTrackId1 = 'Paquetes Cookie - Personas Home';
    var linkHref1 = 'https://www.santander.com.ar/personas/tarjetas/saca-tu-tarjeta-ideal';
    var imgTitle1 = 'Pedí la del Santander, acá tu tarjeta GRATIS. ¡Además! Te regalamos hasta $15.000 de ahorro en supermercados y combustible.';
    var imgDesktop1 = '/banco/wcm/connect/a2055918-31ca-466d-b658-93d1de81dd0c/Carrousel-Slider-Home_Tarjeta-Paquetes_2023.jpg?MOD=AJPERES';
    var imgMobile1 = '/banco/wcm/connect/e8ec7f26-b954-4b08-b7d7-afb3302fd4a4/Carrousel-Slider-Home_Tarjeta-Paquetes_2023_Mobile.jpg?MOD=AJPERES';
    var dataGact1 = 'personas';
    var dataGaid1 = 'Paquetes Cookie - Personas Home';
	
    // HTML del Slider
    var liHtml = '<li  data-track-id="' + dataTrackId + '" id="cookie-slide"><a href="' + linkHref + '"  class="item " data-gact="' + dataGact + '" data-gaid="' + dataGaid + '"><div class="imageContainer"><img src="' + imgDesktop + '" title="' + imgTitle + '"alt="" class="desktop"/><img class="mobile" src="' + imgMobile + '" title="' + imgTitle + '" draggable="false"></div><div class="detailContainer"><div class="table"><div class="cell middle"></div></div></div></a></li>';

    // HTML del Slider 1
    var liHtml1 = '<li  data-track-id="' + dataTrackId1 + '" id="cookie-slide"><a href="' + linkHref1 + '"  class="item " data-gact="' + dataGact1 + '" data-gaid="' + dataGaid1 + '"><div class="imageContainer"><img src="' + imgDesktop1 + '" title="' + imgTitle1 + '"alt="" class="desktop"/><img class="mobile" src="' + imgMobile1 + '" title="' + imgTitle1 + '" draggable="false"></div><div class="detailContainer"><div class="table"><div class="cell middle"></div></div></div></a></li>';


    // Add Slider function
    var addSlideIfDefined = function () {
        if (typeof $('.flexslider').data('flexslider') != 'undefined' && $('.flexslider').data('flexslider') != null) {
            $('.flexslider').data('flexslider').addSlide(liHtml, 0);
        } else {
            setTimeout(addSlideIfDefined, 200);
        }
    };
	
    // Add Slider function1
    var addSlideIfDefined1 = function () {
        if (typeof $('.flexslider').data('flexslider') != 'undefined' && $('.flexslider').data('flexslider') != null) {
            $('.flexslider').data('flexslider').addSlide(liHtml1, 1);
        } else {
            setTimeout(addSlideIfDefined1, 200);
        }
    };
    // Medicion
    var addViewTag = function () {
        if (typeof ga != "undefined" && typeof ga.getAll != "undefined") {
            utag.link({
                event: 'trackEvent',
                eCategory: 'Portal - Promociones',
                eAction: 'view',
                eLabel: 'Supercuenta Cookie - Personas Home'
            });
        } else {
            setTimeout(addViewTag, 200);
        };
    };

    var addClickTag = function () {
        if (typeof ga != "undefined" && typeof ga.getAll != "undefined") {
            $('#cookie-slide').click(function () {
                utag.link({
                    event: 'trackEvent',
                    eCategory: 'Portal - Promociones',
                    eAction: 'click',
                    eLabel: 'Supercuenta Cookie - Personas Home'
                });
            });
        } else {
            setTimeout(addClickTag, 200);
        };
    };
    // Medicion 1
    var addViewTag1 = function () {
        if (typeof ga != "undefined" && typeof ga.getAll != "undefined") {
            utag.link({
                event: 'trackEvent',
                eCategory: 'Portal - Promociones',
                eAction: 'view',
                eLabel: 'Paquetes Cookie - Personas Home'
            });
        } else {
            setTimeout(addViewTag1, 200);
        };
    };

    var addClickTag1 = function () {
        if (typeof ga != "undefined" && typeof ga.getAll != "undefined") {
            $('#cookie-slide').click(function () {
                utag.link({
                    event: 'trackEvent',
                    eCategory: 'Portal - Promociones',
                    eAction: 'click',
                    eLabel: 'Paquetes Cookie - Personas Home'
                });
            });
        } else {
            setTimeout(addClickTag1, 200);
        };
    };
	
    // Cookie Santander
    let santanderCookie = document.cookie.match(/^(.*;)?\s*ga_sr_uid\s*=\s*[^;]+(.*)?$/);

    // Si no esta la cookie Santander, addSlider
    if (santanderCookie == null) {
        addSlideIfDefined();
        addViewTag();
        addClickTag();
        addSlideIfDefined1();
        addViewTag1();
        addClickTag1();
	};
</script>


