<!-- Script -->

<script type="text/javascript">   
     
    var url_formulario = 'https://banco.santander.com.ar/exec/form_esp_women/index.jsp';

    var isEmptyIdImagen = true;
    var isEmptyNombreImagen = true;
    var isEmptyCampania = true;

        

    

        

    var idImagen = ''; 
    var nombreImagen = '';
    var campania = '';

    if(!isEmptyIdImagen && !isEmptyNombreImagen && !isEmptyCampania){
      url_formulario = url_formulario + '?id=' + idImagen + '&nombre=' + nombreImagen + '&campania=' + campania;
    } else if (!isEmptyIdImagen && !isEmptyNombreImagen && isEmptyCampania) {
      url_formulario = url_formulario + '?id=' + idImagen + '&nombre=' + nombreImagen;
    } else if (!isEmptyIdImagen && isEmptyNombreImagen && !isEmptyCampania) {
      url_formulario = url_formulario + '?id=' + idImagen + '&campania=' + campania;
    } else if (isEmptyIdImagen && !isEmptyNombreImagen && !isEmptyCampania) {
      url_formulario = url_formulario + '?nombre=' + nombreImagen + '&campania=' + campania;
    } else if (!isEmptyIdImagen && isEmptyNombreImagen && isEmptyCampania){
      url_formulario = url_formulario + '?id=' + idImagen;
    } else if (isEmptyIdImagen && !isEmptyNombreImagen && isEmptyCampania){
     url_formulario = url_formulario + '?nombre=' + nombreImagen;
   } else if (isEmptyIdImagen && isEmptyNombreImagen && !isEmptyCampania) {
     url_formulario = url_formulario + '?campania=' + campania;
   }

   document.addEventListener('generalesReady', function (e) { 
      var query_cookie = [];
 
      if (getCookie('query_medio')) {
          query_cookie.push('medio=' + getCookie('query_medio'));
      }
    
      if (query_cookie.length) {
          url_formulario = url_formulario.indexOf('?') == -1 ? url_formulario + '?' + query_cookie.join('&') : url_formulario + '&' + query_cookie.join('&');
      }
     }, false);

  	var stack_formulario = new Stack({
  	    id: '9af07743-b3ad-47f7-8037-202bb5663f2b'
  	});

    $('[data-comp="openStack"]').on('click', function(e) {
        e.preventDefault();
        stack_formulario.add({
            iframe: url_formulario, // URL ACA
            onReady: function(data) {
                console.log("ready");
            },
            onRemove: function(data) {
                console.log("remove");
            }
        });
    });    
  
</script>
<!-- /Script -->
        
        
    

    
        





        







    
