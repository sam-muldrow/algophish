<script type="text/javascript">
    let _offers = []
    let _nup = 0

    $(document).ready(function () {

        $body.addClass('personasHomePage');
        $(".site-footer .ayuda h2").html("Nuestra misión es contribuir<br>al progreso de las personas<br>y las empresas.");

    });

    // Add Slider function
    var addSlide = function (liHtml, order) {
        // if (typeof $('.flexslider').data('flexslider') != 'undefined' && $('.flexslider').data('flexslider') != null) {
        // console.log("insertando slide en el orden: " + order)
        // console.log("total de slides: " + $('.flexslider').data('flexslider').last)
        $('.flexslider').data('flexslider').addSlide(liHtml, order);
        // } else {
        //     setTimeout(addSlideIfDefined(liHtml, order), 200);
        // }
    };
    var removeSlide = function (order) {
        $('.flexslider').data('flexslider').removeSlide(order);
    };
    var getHtmlSlide = function (order) {
        return $('.flexslider').data('flexslider').slides[order];
    };


    console.log("cliente")


    let elemento

    $('.slides').on("click", 'a', function (e) {
        console.log(e.target)
        console.log("indexId: ", $(e.target).attr('indexId'))
        let _indexId = $(e.target).attr('indexId')
        if (_indexId !== undefined) {
            e.preventDefault();
            console.log("Llamando cookie-slide-cliente")

            let dataSend = {
                action: "CLICKED",
                channel: "PORTAL",
                codOffer: _offers[_indexId].codOffer,
                idContent: _offers[_indexId].idContent,
                idOffer: _offers[_indexId].idOffer,
                nup: _nup,
                section: _offers[_indexId].section
            }
            //let segmento = window.location.href.includes("personas") ? "PERSONAS" : "SELECT";
            let segmento = "CARRUSEL";
            $.get(
                "https://banco.santander.com.ar/exec/genericoAjax/llamadaXSell.jsp?id=" + _nup + "&accion=fb" + "&segmento=CARRUSEL"
                + "&" + $.param(dataSend),
                function (res) {
                    console.log(res)
                }
            )
            // console.log(elemento)
            // console.log("yendo a la URL: ")
            // console.log(_offers[_indexId].content.link)
            window.location.href = _offers[_indexId].content.goto
        }
    });

    function buildCarrouselOffer(offer, dataTrackId, index) {
        let offert = offer.content;
        let linkHref = offert.goto;
        let imgTitle = offert.title;

        let order = offert.order

        // console.log("Cantidad de imágenes en carrousel: " + cantSliders)
        // console.log("order nuevo: " + order)

        let dataGact = 'personas';
        let dataGaid = 'Supercuenta Cookie cliente - Personas Home';

        let imagenOfer = offert.image;

        console.log("detectando si es mobile")
        let isMobile = window.matchMedia("only screen and (max-width: 760px)").matches;

        if (isMobile) {
            // console.log("es mobile")
            imagenOfer = offert.image_mobile;
        } else {
            // console.log("no es mobile")
        }

        let liHtml = '<li class=".cookie-slide-cliente" data-track-id="' + dataTrackId + '">'
            + '<a href="javascript:;"  class="item " data-gact="' + dataGact + '" data-gaid="' + dataGaid + '">'
            + '<div class="imageContainer">'
            + '<img src="' + imagenOfer + '" indexId="' + index + '" title="' + imgTitle + '"alt="" class="desktop"/>'
            + '<img class="mobile" src="' + imagenOfer + '" indexId="' + order + '" title="' + imgTitle + '" draggable="false">'
            + '</div><div class="detailContainer">'
            + '<div class="table"><div class="cell middle"></div></div></div></a></li>';

        // console.log("liHtml", liHtml)
        // console.log("enviando orden:", order)

        // addSlideIfDefined(liHtml, index);
        let cantSliders = $('.flexslider').data('flexslider').last

        if (cantSliders < order) {
            //WA al insertar un slide al final, se inserta en blanco, por eso esto:
            order = cantSliders;
            let htmlLastSlide = getHtmlSlide(cantSliders)
            addSlide(liHtml, order);
            console.log("cantidad", $('.flexslider').data('flexslider').last)
            removeSlide(order + 1)
            console.log("cantidad", $('.flexslider').data('flexslider').last)
            addSlide(htmlLastSlide, order);

        } else {
            order = offer.order - 1;
            addSlide(liHtml, order);
        }
    }

    $('.flexslider li .flex-active-slide').ready(function () {
        var santanderCookie = document.cookie.match(/(.*ga_sr_uid=)(\d*)\;(.*)/);
        if (santanderCookie !== undefined) {

            _nup = santanderCookie[2];
            let segmento = window.location.href.includes("personas") ? "PERSONAS" : "SELECT";


            $.get("https://banco.santander.com.ar/exec/genericoAjax/llamadaXSell.jsp?id=" + _nup + "&segmento=CARRUSEL" + "&accion=bp"
                , function (data) {
                    console.log("data", data)
                    if (data !== undefined && data.offers.length > 0) {
                        let dataTrackId = data.description;
                        _offers = data.offers;

                        console.log("antes _offers", _offers)
                        _offers = _offers.sort((a, b) => (a.order > b.order ? 1 : -1))
                        console.log("después _offers", _offers)

                        index = 0;
                        data.offers.forEach(offer => {
                            buildCarrouselOffer(offer, dataTrackId, index);
                            index++
                        });
                    }
                });
        }

    })
</script>

<script>
$(document).ready(function(){

const $cuentas = $("[title='Cuentas y Paquetes']").first().parent();
const $tarjetas = $("[title='Tarjetas']").first().parent();

// Cuentas
$cuentas.find("[title='Inicio']").attr("data-GAID", "Personas>Cuentas>Inicio-Menú");
$cuentas.find("[title='Inicio']").attr("data-GACT", "/banco/online/personas");

$cuentas.find("[title='Conocé tu Cuenta Ideal']").attr("data-GAID", "Personas>Cuentas>Conocé tu cuenta ideal-Menú");
$cuentas.find("[title='Conocé tu Cuenta Ideal']").attr("data-GACT", "/banco/online/personas");

$cuentas.find("[title='Caja de ahorro en pesos']").attr("data-GAID", "Personas>Cuentas>Caja de ahorro en pesos-Menú");
$cuentas.find("[title='Caja de ahorro en pesos']").attr("data-GACT", "/banco/online/personas");

$cuentas.find("[title='SuperCuenta']").attr("data-GAID", "Personas>Cuentas>SC-Menú");
$cuentas.find("[title='SuperCuenta']").attr("data-GACT", "/banco/online/personas");

$cuentas.find("[title='SuperCuenta 3']").attr("data-GAID", "Personas>Cuentas>SC3-Menú");
$cuentas.find("[title='SuperCuenta 3']").attr("data-GACT", "/banco/online/personas");

