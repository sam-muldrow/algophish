    /**
     * reorder the blocks of the Chosen articles
     * @author Amr
     */
    reorderBlocks(window.innerWidth);

    /**
     * reorder the blocks according to the base
     * number every screen can get
     * e.g. Desktop can get 4 blocks per-row
     * @param width
     * @author Amr
     */
    function reorderBlocks(width) {
        let container = document.getElementById("appendto");
        let items = [].slice.call(container.children);
        let itemsLength = items.length;
        /**
         * re show the blocks to make the coming operation
         * for precise
         * @author Amr
         */
        items.forEach(function (el) {
            el.style.setProperty('display', 'block');
        })
        if (parseInt(width) >= 773) {
            let reset = items.length % 4;
            items.slice(itemsLength - reset, itemsLength).forEach(function (el) {
                el.style.setProperty('display', 'none');
            })
            return;
        }
        items.forEach(function (el) {
            el.style.setProperty('display', 'block');
        })
    }

    /**
     * handle screen resizing
     * @author Amr
     */
    window.addEventListener("resize", function (e) {
        reorderBlocks(window.innerWidth);
    });

    function load_more2() {
        var loader = document.getElementById("loader");
        var element2 = document.getElementById("word-submit");
        loader.classList.remove("d-none");
        element2.classList.add("d-none");
        page_id = page_id + 1;
        const queryString = window.location.search;
        let ajax_url = '?ajax=true&page=';

        ajax_url = '?ajax=true&page=';

