                    document.getElementById("col_3").innerHTML += data.html.col_3;
                    if (client == 'sa2eh') {
                        document.getElementById("col_4").innerHTML += data.html.col_4;

                    }
                    element2.classList.remove("d-none");
                    addMouseClass();
                }
                /**
                 * reorder the blocks of the Chosen articles
                 * @author Amr
                 */
                reorderBlocks(window.innerWidth);
            });

        //   x.innerHTML + JSON.parse(xhttp.responseText);
    }

    var page_id = 1

    function load_more() {
        var loader = document.getElementById("loader");
        var element2 = document.getElementById("word-submit");
        loader.classList.remove("d-none");
        element2.classList.add("d-none");
        page_id = page_id + 1;
        const queryString = window.location.search;
        let ajax_url = '?ajax=true&page=';

        ajax_url = '?ajax=true&page=';

        getJSON(ajax_url + page_id,
            function (err, data) {
                if (err !== null) {

                } else {
                    //loader.classList.add("d-none");
                    document.getElementById("appendto").innerHTML += data.html;
                    element2.classList.remove("d-none");
                    addMouseClass();
                }
                /**
                 * reorder the blocks of the Chosen articles
                 * @author Amr
                 */
                reorderBlocks(window.innerWidth);
            });

        //   x.innerHTML + JSON.parse(xhttp.responseText);
    }

    var getJSON = function (url, callback) {
        var xhr = new XMLHttpRequest();
        xhr.open('GET', url, true);
        xhr.responseType = 'json';
        xhr.onload = function () {
            var status = xhr.status;
            if (status === 200) {
                callback(null, xhr.response);
            } else {
                callback(status, xhr.response);
            }
        };
        xhr.send();
    };

    function subscribe() {
        try {
            subscribe_form = document.getElementById("subscribe_form_id");
            subscribe_error = document.getElementById("subscribe_caption");
            subscribe_form.classList.add("d-none");
            subscribe_error.innerHTML = '<p  class="caption ">شكراً لاشتراكك، ستصل آخر المقالات قريباً إلى بريدك الإلكتروني</p>';

            const node = document.createElement('<p  class="caption ">شكراً لاشتراكك، ستصل آخر المقالات قريباً إلى بريدك الإلكتروني</p>');
            const {value: email} = document.getElementById("user_email");
            if (email !== "" && email !== null && email !== undefined) {
                axios.post('/newsletter/subscribe', {
                    username: 'صديقة موقع ليالينا',
                    email: email,
                    nl_source: 'layalina_hp_footer_nl',
                    nl_utm_campaign: ''
                }).then(() => document.getElementById("subscribe_caption").appendChild(node))
                    .catch((e) => console.error('Subscribe failed'));
            }
        } catch (e) {
            console.error('Subscribe failed');
        }


    }

    function subscribe_on_carousel() {
        try {
            const subscribe_form = document.getElementById("subscribe_form_carousel");
            subscribe_form.classList.add("d-none");
            const {value: email} = document.getElementById("user_email");
            const subscribe_error = document.getElementById("subscribe_form_error");
            axios.post('/newsletter/subscribe', {
                username: 'صديقة موقع ليالينا',
                email,
                nl_source: 'layalina_hp_footer_nl',
                nl_utm_campaign: ''
            })
                .then(() => {
                    subscribe_error.classList.remove("d-none");
                })
                .catch(function () {
                    subscribe_error.classList.remove("d-none");
                    console.error('subscribe failed');
                });
        }catch (e) {
            console.error('Subscribe failed');
        }

    }


