    // Fetch all the forms we want to apply custom Bootstrap validation styles to
    var forms = document.querySelectorAll('.needs-validation')

    // Loop over them and prevent submission
    Array.prototype.slice.call(forms)
        .forEach(function (form) {
            form.addEventListener('submit', function (event) {
                try {
                    event.preventDefault()
                    event.stopPropagation()

                    if (!form.checkValidity()) throw new Error('Email is not valid');

                    let subscribe_form = document.getElementById("subscribe_form_id");
                    let subscribe_form_on_carousel = document.getElementById("subscribe_form_carousel");
                    if (typeof subscribe_on_carousel !== 'undefined' && subscribe_on_carousel !== null && subscribe_form_on_carousel && form.id === 'subscribe_form_carousel') {
                        subscribe_on_carousel();
                    }
                    if (typeof subscribe !== 'undefined' && subscribe !== null && subscribe_form && form.id === 'subscribe_form') {
                        subscribe();
                    }

                } catch (e) {
                    console.error(e.message);
                    event.preventDefault()
                    event.stopPropagation()
                    form.classList.add('was-validated')
                }

            }, false)
        });

    function get_event_logo() {
        try {
            const imglogo_sponsor = document?.querySelector('#imglogo_sponsor');
            const mainLogo_anim = document?.querySelector('.imglogo.mainLogo');
            link = '/editorial_calendar/editorial-calendar-logo/';
            axios.get(link)
                .then((response) => {
                    if (response.data) {
                        arr = response;
                        if (response?.data?.logoType && response?.data?.html)
                            switch (response?.data?.logoType) {
                                case 1:
                                case 2:
                                    if (!mainLogo_anim) break;
                                    // get all script tags from response.data.html
                                    // and append them to the header DOM
                                    let scripts = response.data.html.match(/<script[^>]*>([\s\S]*?)<\/script>/gi);
                                    if (scripts) {
                                        for (let i = 0; i < scripts.length; i++) {
                                            // remove script tags from html and take out only the script content
                                            scripts[i] = scripts[i].replace(/<script[^>]*>([\s\S]*?)<\/script>/, '$1');
                                            // create a new script tag and append it to the head
                                            const node = document.createElement('script');
                                            node.type = 'text/javascript';
                                            node.async = false;
                                            node.innerHTML = scripts[i];
                                            document.head.appendChild(node);
                                        }
                                    }
                                    // then append the rest of the HTML to the body
                                    mainLogo_anim.innerHTML = response.data.html.replace(/<script[^>]*>([\s\S]*?)<\/script>/gi, '');
                                    break;
                                case 3:
                                    if (imglogo_sponsor) imglogo_sponsor.innerHTML = response.data.html;
                                    break;
                            }
                    }
                }).then(() => {
                init();
            });
        }catch (e){
            console.error(e);
        }
    }

    var auto_load_count = 0;
    window.addEventListener('scroll', () => {
        var isLoading = document.getElementById('word-submit');
        if (
            window.scrollY + window.innerHeight >= document.body.offsetHeight - 500
            && auto_load_count < 3
            && !isLoading.classList.contains('d-none')
        ) {
            auto_load_count += 1;
            load_more();
        }
    });
</script>
