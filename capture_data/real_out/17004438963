(function() {
    LMDA.device = LMDA.device || {};
    var root = document.documentElement,
        classList = [],
        classListAdd = [],
        classListRemove = [];

    LMDA.device.is_high_quality_image = (
        LMDA.device.network != "slow" &&
        (
            ( root.clientWidth < 992 && window.devicePixelRatio >= 3 ) ||
            ( root.clientWidth >= 992 )
        )
    );

    LMDA.device.high_quality_image = function (imgSrc) {
        if (LMDA.device.is_high_quality_image) {
            return imgSrc.replace('img389x562','product').replace('236x341','389x562').replace('46x66','135x194');
        }
        return imgSrc;
    };

    if (LMDA.device.is_high_quality_image) {
        LMDA.device.isRetina = true;
        classListAdd.push("html_retina")
    } else {
        LMDA.device.isRetina = false;
    }

    if ('ontouchstart' in root) {
        LMDA.device.isTouch = true;
        LMDA.isTouch = true; 
        classListAdd.push("html_touch", "touch"); 
    } else {
        LMDA.device.isTouch = false;
        classListAdd.push("html_not-touch", "not-touch"); 
    }

    LMDA.device.network = "fast";
    var tcpInfo = "52673, 62127, 36455";
    if (tcpInfo) {
        var rtt = parseInt(tcpInfo.split(", ")[2]);
    }
    if (rtt > 75000) {
        LMDA.device.network = "slow";
    } else if (rtt > 25000 ) {
        LMDA.device.network = "normal";
    }
    classListAdd.push('html_network-' + LMDA.device.network);

    function recalc () {

        var orientation = (window.innerHeight > window.innerWidth) ? "portrait" : "landscape";
        if (LMDA.device.orientation && LMDA.device.orientation != orientation) {
            classListRemove.push('html_' + LMDA.device.orientation);
        }
        LMDA.device.orientation = orientation;
        classListAdd.push('html_' + orientation);

        var size  = "desktop",
            width = window.innerWidth;
        if (width < 768) {
            size = "phone";
        } else if (width < 992) {
            size = "tablet";
        } else if (width > 1245) {
            size = "wide";
        }
        if (LMDA.device.size && LMDA.device.size != size) {
            classListRemove.push('html_' + LMDA.device.size);
            
            if (LMDA.device.size == "wide") {
                classListRemove.push("widescreen");
                LMDA.isWidescreen = false;
            }
        }
        LMDA.device.size = size;
        classListAdd.push('html_' + size);
        
        if (size == "wide") {
            classListAdd.push("widescreen");
            LMDA.isWidescreen = true;
        }

        classList = root.className.split(" ");

        var removeLen = classListRemove.length;
        for(i = 0; i < removeLen; i++){
            var index = classList.indexOf(classListRemove[i]);
            if (index != -1) {
                classList.splice(index, 1);
            }
        }
        classListRemove = [];

        var addLen = classListAdd.length;
        for(i = 0; i < addLen; i++){
            if (classList.indexOf(classListAdd[i]) == -1) {
                classList.push(classListAdd[i]);
            }
        }
        classListAdd = [];

        root.className = classList.sort().join(" ");

        LMDA.platform != 'mobile' && LMDA.Events && LMDA.Events.trigger
            && LMDA.Events.trigger(LMDA.Events.SCREEN_CHANGE, LMDA.device.size);
    }

    recalc();

    window.addEventListener("resize", resizeThrottler, false);
    window.addEventListener("orientationchange", resizeThrottler, false);
    var resizeTimeout;
    function resizeThrottler() {
        if ( !resizeTimeout ) {
            resizeTimeout = setTimeout(function() {
                resizeTimeout = null;
                recalc();
            }, 300);
        }
    }

    window.addEventListener('beforeunload', function() {
        if (!LMDA.cookie) return;
        if (LMDA.device.is_high_quality_image) {
            LMDA.cookie.set("retina", "x2");
        } else {
            LMDA.cookie.drop("retina");
        }
    });

}());
