<script type="text/javascript">
    (function ($) {
    $(function ($) {
        
   // Set the date we're counting down to
        let promoStart = new Date('Nov 20, 2023 17:00:00 GMT-0700').getTime();
        let cyberStart = new Date('Nov 26, 2023 17:00:00 GMT-0700').getTime();
        let lastChance = new Date('Dec 3, 2023 17:00:00 GMT-0700').getTime();
        let promoEnds = new Date('Dec 6, 2023 00:00:00 GMT-0700').getTime();

        // Get today's date and time
        let now = new Date().getTime();
        if (promoStart > now) {
            $(".xf-page-promo").hide();
        } else if (cyberStart < now && now < lastChance){
            $('.promo-template').addClass('second');
        } else if (lastChance < now && now < promoEnds){
            $('.promo-template').addClass('third');
        } else if (promoEnds < now){
            $(".xf-page-promo").hide();
        }
    });
})(jQuery);
  </script>
</div>
