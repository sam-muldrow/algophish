<script type="text/javascript">
    const closePromo = jQuery('.close-promo');
const promo = closePromo.closest('.promo-template');

closePromo.on('click', () => {
    console.log("close button");
    promo.slideUp(100);
    Utils.cookies.setCookie('promo_closed', 'true', window.location.hostname, '/', 7);
});

const promoCookie = Utils.cookies.getCookie('promo_closed');
if(promoCookie) {
promo.hide();
} else {
}
  </script>
