<script>
    function showLogin() {
        document.getElementById('main_login_modal').style.display = 'block';
        document.getElementById('login-button-open').ariaExpanded = 'true';
        $("button[data-id='login1Select']").focus();
        return false;
    }

    function hideLogin() {
        document.getElementById('main_login_modal').style.display = 'none';
        document.getElementById('login-button-open').ariaExpanded = 'false';
        $("#login-button-open").focus();
    }
</script>
