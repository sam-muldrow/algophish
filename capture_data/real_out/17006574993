<script>
    function loginChanged(cls) {
        if ($('.login-main:visible').length > 0 /*!= $('.login-main').length*/) {
            $('.' + cls + '.login-content').hide().filter($('.' + cls + '.' + $('#' + cls + 'Select option:selected').val())).show();
        }
        else {
            $('.' + cls + '.login-content').hide().filter($('.' + cls + '.' + $('#' + cls + 'SelectIE option:selected').val())).show();
        }
    }

    // IE support
    if (window.document.documentMode) {
        $('.login-main').hide();
        $('.ie-only').show();

        // IE mobile - tablets I guess?
        $('#login1SelectIE option:eq(0)').prop('option:selected', true)
        $("select option").addClass("changeOptionBackground").on('change', function () {
            var optionIndex = $(this).find('option:selected').index();
            $(this).toggleClass('changeOptionBackground', optionIndex == 0);
        });
    }

    loginChanged('login1');
</script>

