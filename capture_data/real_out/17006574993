        <script>
            function cbAlertsResize() {
                var mastheadHeight = $('#masthead').outerHeight();
                var alertHeight = $('#SiteAlerts').outerHeight();

                $('#masthead').css('top', alertHeight);
                $('#Body').css('top', alertHeight);
                $('.nav-wrapper').css('margin-top', mastheadHeight + alertHeight);

                var browserWidth = $(window).width();
                if (browserWidth >= 992) {
                    $('.nav-flyout-first').css('margin-top', '0px');
                    $('.nav-flyout-second').css('margin-top', '0px');

                    if ("False" === "True") {
                        $('.location-finder_ribbon').css('top', '0px');
                        $('.map-full').css('top', '0px');
                    }
                }
                else {

                    if ("False" === "True") {
                        var locationFinderMenuHeight = $('.location-finder_ribbon').outerHeight();
                        $('.location-finder_ribbon').css('top', mastheadHeight + alertHeight);
                        $('.map-full').css('top', mastheadHeight + alertHeight + locationFinderMenuHeight);
                    }

                    $('.nav-flyout-first').css('margin-top', mastheadHeight + alertHeight);
                    $('.nav-flyout-second').css('margin-top', mastheadHeight + alertHeight);
                }
            }

            function ctcAlertsResize() {
                var alertHeight = $('#SiteAlerts').outerHeight();
                $('#wrapper').css('top', alertHeight);
            }

            function cfAlertsResize() {
                var alertHeight = $('#SiteAlerts').outerHeight();
                $('#SiteAlerts').css('top', '0px');
                $('.body-wrapper').css('margin-top', alertHeight);
                $('.overlay').css('margin-top', alertHeight);
            }

            function chcAlertsResize() {
                var alertHeight = $('#SiteAlerts').outerHeight();
                $('#wrapper').css('top', alertHeight);

                var browserWidth = $(window).width();
                if (browserWidth >= 992) {
                    $('#header').css('top', alertHeight);
                }
                else
                {
                    $('#header').css('top', '0px');
                }
            }

            function alertsResize() {

                        cbAlertsResize();

            }

            document.addEventListener('DOMContentLoaded', alertsResize, false);
            window.addEventListener('resize', alertsResize, false);
        </script>
