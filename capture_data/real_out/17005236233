	requirejs(['jquery','bootstrap','custom'], function ($) {
     /* Mobile Menu Start */
/* Mobile Menu Start */
	$(".hamburger-mobile-show").click(function(){
		$("#navbarNavAltMarkup").toggle();
		$("body").toggleClass("stop-scroll");
		$("#header-bar-mobile").toggleClass("open-mob-menu");
		$(".hamburger-mobile-hide").css({"display":"inline"});
		$(".header-bar").css({"position":"sticky"});
		$('.hamburger-mobile-show').hide();
		$('.readinapp-mob').hide();
		$('.secondary-menu .secondary-menu-list').addClass('col-11');
		$('.menu-align-buttons-noclick').hide();$('.menu-align-buttons-onclick').show();
		//$("#header-bar-mobile").css({"height":"200px"});
	});
	$(".hamburger-mobile-hide").click(function(){
		$(".header-bar").css({"position":"unset"});
		$("#header-bar-mobile").toggleClass("open-mob-menu");
		$("#navbarNavAltMarkup").toggle();
		$("body").toggleClass("stop-scroll");
		$('.readinapp-mob').show();
		$('.secondary-menu .secondary-menu-list').removeClass('col-11');
		$(".hamburger-mobile-show").css({"display":"inline"});$('.hamburger-mobile-hide').hide();
		$('.menu-align-buttons-noclick').show();$('.menu-align-buttons-onclick').hide();
		    if($('.menu-dropdown-list .title .menu-dropdown-heading').next('.menu-dropdown-sub-list').hasClass('opened')){
				$('.menu-dropdown-list .title .menu-dropdown-heading').next('.menu-dropdown-sub-list').removeClass('opened');
				$('.menu-dropdown-list .title .menu-dropdown-heading').removeClass("arrowactive");
				$('.menu-dropdown-list .title').css({"background-color":"transparent"});
			}
			if($('.mob-see-all').hasClass('opened')){
				$('.mob-see-all').html('See All');$(".mob-see-all").css({"text-decoration":"none"});
				$('.menu-see-less').toggleClass('hidden');$('.menu-see-all .last').toggleClass('uline');
				$('.mob-see-all').toggleClass('opened');return false;
			}
	});
/* Mobile Menu End */

   setTimeout(
	   function(){
		$('.menu-dropdown-list .title .menu-dropdown-heading .menu-arrow-mob').click(function(){
			$('.menu-dropdown-list .title').css({"background-color":"transparent"});
			if($(this).parent().next('.menu-dropdown-sub-list').hasClass('opened')){
				$('.menu-dropdown-list .title .menu-dropdown-heading').toggleClass("active");
				$('.menu-dropdown-list .title .menu-dropdown-heading').removeClass('arrowactive');
				$(this).parent().next('.menu-dropdown-sub-list').removeClass('opened');
				$(this).parent().find('.footer-arrow').toggleClass("active");
				return false;
			}
			else{
				$('.menu-dropdown-list .title .footer-arrow').toggleClass("active");
			}
			$('.menu-dropdown-list .title .menu-dropdown-sub-list').removeClass('opened');
			$('.menu-dropdown-list .title .menu-dropdown-heading').removeClass('arrowactive');
			$(this).parent().next('.menu-dropdown-sub-list').toggleClass('opened');
			$(this).parent().toggleClass("arrowactive");
			$('.arrowactive').parents(".title").css({"background-color":"#e7e7e7"});
		});
	$('.see-all-action').click(function(){
		if($('.mob-see-all').hasClass('opened')){
		$('.mob-see-all').html('See All');$(".mob-see-all").css({"text-decoration":"none"});
		$('.menu-see-less').toggleClass('hidden');$('.menu-see-all .last').toggleClass('uline');
		$('.mob-see-all').toggleClass('opened');return false;
		}
		else{
		$('.mob-see-all').html('See Less');$(".mob-see-all").css({"text-decoration":"underline"});
		$('.menu-see-less').toggleClass('hidden');$('.menu-see-all .last').toggleClass('uline');
		$('.mob-see-all').toggleClass('opened');return false;
		}
	});
	   }, 2000
   )

   $(".hamburger-show").click(function(){
		$(".desktop-popup-menu").css({"display":"flex"});
		$('.desktop-popup-menu').toggleClass('popup-menu-open');
		$(".hamburger-hide").css({"display":"inline"});
		$('.hamburger-show').hide();
		$(".bl-header").addClass('bl-menu-active');
		//$(".bl-logo-lg .bl-nav-item").css({"display":"none"});

		//$(".popup-menu-open .header-search").css({"display":"block"});
	});
	$(".hamburger-hide").click(function(){
		$(".desktop-popup-menu").css({"display":"none"});
		$('.desktop-popup-menu').toggleClass('popup-menu-open');
		$(".hamburger-show").css({"display":"inline"});$('.hamburger-hide').hide();
		$(".bl-header").removeClass('bl-menu-active');
		//$(".bl-logo-lg .bl-nav-item").css({"display":"block"});
	});
		$('.header-search #newsd, .header-search #newspop, input#newsd').click(function () {
		window.location.href = '/search/';
	});

    });
