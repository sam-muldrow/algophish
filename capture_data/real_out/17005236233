   $(document).on("click", ".thgsignin", function () {
        if (typeof tp.pianoId != "undefined") {
            tp.pianoId.show({
                loggedIn: function (data) {
                    location.reload();
                },
                loggedOut: function () {
                    location.reload();
                }
            });
        }
    });
});
     tp = window["tp"] || [];
		tp.push(['init', function () {
			tp.pianoId.init({
			loggedIn: function (data) {
			thgloggedIn(data)
			},
			loggedOut: function () {
			location.reload();
			}
			});
		}
		]);
		function thgloggedIn(data) {
			if(document.getElementsByTagName("header").length > 0) {
			let headers = document.querySelectorAll("header");
        	[...headers].forEach(el => {
				el.classList.remove("logout");
				el.classList.add("login");
		    });
			}
		}
		function thgLogout() {
		typeof __thg_event != "undefined" ?  __thg_event.triggerLogOut(tp.pianoId.getUser()): null;
		tp.push(["init", function () {
				tp = window.tp || [];
				tp.pianoId.logout()
				location.reload();
		}]);
		}
	userIdentify.then(function() {
		var list = document.querySelectorAll(".menuuserdetect");
			for (var i = 0; i < list.length; ++i) {
				list[i].style.display = "inline-block" ;
			}
	});
