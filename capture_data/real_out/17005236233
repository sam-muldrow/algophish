require(['jquery'], function($) {
  //navigations box
      $(document).on("click", ".navigations-icon", function () {
          $(".navigations").toggleClass("show-navigations");
      });
      $(document).on("click", ".navigations-box .navigations-close, .navigations-overlay", function () {
          $(".navigations").removeClass("show-navigations");
      });
      if($(window).width() < 571){ /* desktop */
      $(document).on("click", ".navigations .navigations-list li a", function () { 
          $(".navigations").removeClass("show-navigations");
      });
      }
      $(document).mouseup(function(e){
        var container = $(".navigations-icon");
        if (!container.is(e.target) && container.has(e.target).length === 0) 
        {   
            $(".navigations-icon").removeClass("opened");
        }    
      });
      $(document).on("click", ".purch-sub", function (event) {
          document.getElementById("plan-section-top").scrollIntoView({
          behavior: 'smooth'
        });
      });
      $(document).on("click", ".more-btn", function (event) {
        document.getElementById("plan-section-top").scrollIntoView({
          behavior: 'smooth'
        });
      });
      $(document).on("click", ".opinionckick", function (event) {
         event.preventDefault();
        document.getElementById("opinionview").scrollIntoView({
         behavior: 'smooth'
       });
       $(".navigations").toggleClass("show-navigations");
     });
     $(document).on("click", ".todayspollckick", function (event) {
       event.preventDefault();
        document.getElementById("todayspollview").scrollIntoView({
         behavior: 'smooth'
       });
       $(".navigations").toggleClass("show-navigations");
     });
      $(document).on("click", ".navigations-box .navigations-list li a", function (event) {
        //event.preventDefault();
                var prevScrollnew = window.pageYOffset;
                var target = "#" + this.getAttribute('data-target');
                var currentScrollnew = $(target).offset().top - 10;
                // alert(currentScrollnew+" "+prevScrollnew);
                if (prevScrollnew > currentScrollnew) {
                    $('html, body').animate({
                            scrollTop: $(target).offset().top - 100
                    }, 300);
                }
                else{
                    $('html, body').animate({
                            scrollTop: $(target).offset().top - 10
                    }, 300);
                }
                
                if(this.getAttribute('data-state') == "nobar"){
                    $(".navigations").removeClass("show-navigations");
                    $(".navigations-icon").removeClass("opened");
                }else{
                    $(".navigations-icon").addClass("opened");
                }
                prevScrollnew = currentScrollnew;
  });
});
</script><script type="text/javascript">
require(["jquery"], function ($) {
	$(this).scroll(function() {
		if ($(this).scrollTop() > 300) {
			if($( window ).width() > 1200){
				$("#left-bottom-sticky-container").css("bottom", "30px");
				return 0;
			}
			if($( window ).width() > 1000 && $( window ).width() < 1200){
				$("#left-bottom-sticky-container").css("bottom", "12px");
				return 0;
			}
			if($( window ).width() > 600 && $( window ).width() < 1000){
				$("#left-bottom-sticky-container").css("bottom", "0px");
				return 0;
			}
			if($( window ).width() <= 600){
				if($(".flooting-ad").css("display") == "block"){
					
					$("#left-bottom-sticky-container").css("bottom", "95px");
					return 0;
				}
				$("#left-bottom-sticky-container").css("bottom", "0px");
				return 0;
			}
			$("#left-bottom-sticky-container").css("bottom", "30px");
		}
		else{
			$("#left-bottom-sticky-container").css("bottom", "-100%");
		}

	});
});
