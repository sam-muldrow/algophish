</script>            <script>
                window.addEventListener("load", function () {
                    if (imgListJson != undefined) {
                        for (const key in imgListJson) {
                            if (key.startsWith('src') || key.startsWith('id') || key.startsWith('class')) {
                                var $img = $('img[' + key + ']');
                                if ($img.length != 0) {
                                    $img.attr('alt', imgListJson[key]);
                                }
                            }
                        }
                    }
                });
            </script>
