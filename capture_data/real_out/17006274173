<script>
document.addEventListener('DOMContentLoaded', function () {
(function($) {
    $(function() {
        let $alert_str = '<div class="cd-popup all-Announcement-Popup" role="alert">\
        <div class="cd-popup-container">\
            <h1>\
                <p>IE瀏覽器停止支援通知</p>\
            </h1>\
            <p>親愛的顧客您好：<br>\
                   因應微軟IE瀏覽器即將停止支援，玉山銀行官方網站已於2022年2月16日起停止支援IE瀏覽器。<br>\
                  為持續提供您最優質的服務，建議您使用其他瀏覽器，<br>\
                  詳細資訊可參考\
<a style="text-decoration: underline;color:#00a19b;" href="https://www.esunbank.com.tw/bank/about/announcement/announcement?i=eYxNfOekAkKLtZwoUwDqaw&amp;p=9WLFRe74B0ikiWlxA46w8Q&amp;d=HA4k102TJkG2QuRj6K5Wtg" target="_blank"> 本行瀏覽器建議公告 </a>，謝謝。</p>\
            <div class="pop-btn-wrap">\
                <ul class="cd-buttons">\
                    <li class="onlyBtn"><li class="onlyBtn"><a href="#" class="confirm">確認</a>\
                    </li>\
                </ul>\
            </div>\
            <a href="#" class="cd-popup-close img-replace">Close</a>\
        </div>\
    </div>';
        $('body').append($alert_str).append('<link href="/bank/css/esunbank_m/wealth/fund/style-mfund-search.css?v=3" rel="stylesheet" />');
        var funcObj = {
            getCookie: function(c_name) {
                var i, x, y, EachCookies = document.cookie.split(";");
                for (i = 0; i < EachCookies.length; i++) {
                    x = EachCookies[i].substr(0, EachCookies[i].indexOf("="));
                    y = EachCookies[i].substr(EachCookies[i].indexOf("=") + 1);
                    x = x.replace(/^\s+|\s+$/g, "").replace(/<[^>]*>/g, "").replace(/([\('\)])/g, "\\$1").replace(/ /g, "%20");
                    y = y.replace(/^\s+|\s+$/g, "").replace(/<[^>]*>/g, "").replace(/([\('\)])/g, "\\$1").replace(/ /g, "%20");
                    if (x == c_name) {
                        return unescape(y);
                    }
                }
            },
            setCookie: function(c_name, value, exdays) {
                var exdate = new Date();
                if (exdays == 0) {
                    exdate.setHours(23, 59, 59, 999);
                }
                exdate.setDate(exdate.getDate() + exdays);
                var c_value = escape(value) + ((exdays == null) ? "" : "; expires=" + exdate.toUTCString());
                document.cookie = c_name + "=" + c_value + ";path=/;";
            },
            isIE: function() { // Detecting IE
                return (!!window.ActiveXObject || "ActiveXObject" in window);
            },
            browserDetectForIECookieKey: "ie_check"
        }
        if (funcObj.isIE() && funcObj.getCookie(funcObj.browserDetectForIECookieKey) == null) {
            $('.cd-popup.all-Announcement-Popup').addClass('is-visible');
            funcObj.setCookie(funcObj.browserDetectForIECookieKey, 'IE_T', 0);
        }
        //close popup
        $('.cd-popup').on('click', function(event) {
            if ($(event.target).is('.cd-popup-close')) {
                event.preventDefault();
                $(this).removeClass('is-visible');
            }
        });
        //close popup when clicking the esc keyboard button
        $(document).keyup(function(event) {
            if (event.which == '27') {
                $('.cd-popup').removeClass('is-visible');
            }
        });
        //close popup
        $('.cd-popup .confirm').on('click', function(event) {
            event.preventDefault();
            $('.cd-popup').removeClass('is-visible');
        });
    });
})(jQuery);
});
</script>

