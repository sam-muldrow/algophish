<script>
function ForumsCategory(CategoryId, forumId, ddid) {
ddid = ddid || 'ForumId';
if (forumId === 0) forumId = null;
$.ajax({
type: "get",
url: "/home/getforumsbycategoryid",
data: {
cid: CategoryId,
fid: forumId
},
datatype: "json",
traditional: true,
success: function (data) {
var form = "<select>";
var arr = $.map(data, function (el) { return el; });
for (var i = 0; i < arr.length; i++) {
form = form + '<option value= ' + arr[i].ForumId + ' > ' + arr[i].ForumName + '</option> ';
}
form = form + '</select>';
$('#' + ddid).html(form);
}
});
}
</script>

