	drawPieChart();
	*/
	//jsChartBarScrl();
	/*
	// main Banner
	$('.event-slide').each(function()
	{
		var $slide = $(this);
		var mainBanner = new Swiper('.event-slide .swiper-container', 
		{
			autoplay:{ delay:5000, },
			slidesPerView: 1,
			spaceBetween: 0,
