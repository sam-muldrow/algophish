<script>
function NoticePopupClose(){
	KBCARD_GLOBAL.closePopup('#NoticePopup0')
}

function NoticePopupNeverOpen() {
    var date = new Date();
	var today = date.getFullYear().toString()+date.getMonth().toString()+date.getDate().toString();
    SetCookie("NoticePopup0", today, 1);
    NoticePopupClose();
}    

$(function() {
	var date = new Date();
	var today = date.getFullYear().toString()+date.getMonth().toString()+date.getDate().toString();
	if(GetCookie("NoticePopup0") != today) {
		//KBCARD_GLOBAL.openPopup('#NoticePopup0');	
	}
})

</script>
