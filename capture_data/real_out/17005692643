<script>
function DebugOut(input)
{
	try
	{
		//console.log(JSON.stringify(input));
	}
	catch(e)
	{
		//console.log(e);
	}
}

function GetCardType(input)
{
	var Result = "KB국민";
	var CardType = NVL(input,""); 
	if("1" == CardType) Result = "KB국민";
	else if("2" == CardType) Result = "KB국민,국민BC";
	else if("3" == CardType) Result = "KB국민,KB국민체크";
	else if("4" == CardType) Result = "다원화";
	else if("5" == CardType) Result = "다원화(소유자별)";
	else if("6" == CardType) Result = "KB국민,국민BC,제휴체크";
	else if("7" == CardType) Result = "KB국민,KB국민체크,제휴체크";
	else if("8" == CardType) Result = "KB국민(일반),KB국민(특정)";
	else if("9" == CardType) Result = "KB국민(일반),KB국민(특정),제휴체크";
	else if("A" == CardType) Result = "다원화";
	else if("B" == CardType) Result = "KB국민(일반),KB국민(특정)";
	return Result;
}

$(document).ready(function() 
{
	Initialize();
});

function Initialize()
{
	InitializeBanner();
	
	CommonPopup(); //CommonPopup.jsp 호출.
	InitializeProduct();
	
}

function InitializeProduct()
{
	//추천상품 처음탭선택하기.	
	try
	{
		$(".recom-card .tabs .tabs__menu > li").removeClass();
		$(".recom-card .tabs .tabs__menu > li:first-child").addClass('on').find('a').attr('title', '선택됨');
	}
	catch(e)
	{		
	}
}

var g_Ref = 0;
var g_Data = new Object();
function LoadingShow()
{
	g_Ref++;
	//showProgress(true);
}

function LoadingHide()
{
	g_Ref--;
	if(g_Ref <= 0)
	{
		g_Ref = 0;
		//showProgress(false);	
		fnComplete();
	}	
}

function InitializeLoginData()
{
	showProgress(true);
	
	LoadingShow();
	$.ajax({
        type : 'POST',
        url:"/CMN/API/HCAA0001",
        dataType : 'json',
        contentType : 'application/x-www-form-urlencoded;charset=UTF-8',
        data : "Section=1",
        success: fnSuccess,
        error:fnFail,
        timeout:30000
    });
	
	LoadingShow();
	$.ajax({
        type : 'POST',
        url:"/CMN/API/HCAA0001",
        dataType : 'json',
        contentType : 'application/x-www-form-urlencoded;charset=UTF-8',
        data : "Section=2",
        success: fnSuccess,
        error:fnFail,
        timeout:30000
    });
	
	LoadingShow();
	$.ajax({
        type : 'POST',
        url:"/CMN/API/HCAA0001",
        dataType : 'json',
        contentType : 'application/x-www-form-urlencoded;charset=UTF-8',
        data : "Section=3",
        success: fnSuccess,
        error:fnFail,
        timeout:30000
    });
	LoadingShow();
	$.ajax({
        type : 'POST',
        url:"/CMN/API/HCAA0001",
        dataType : 'json',
        contentType : 'application/x-www-form-urlencoded;charset=UTF-8',
        data : "Section=4",
        success: fnSuccess,
        error:fnFail,
        timeout:30000
    });	
	LoadingShow();
	$.ajax({
        type : 'POST',
        url:"/CMN/API/HCAA0001",
        dataType : 'json',
        contentType : 'application/x-www-form-urlencoded;charset=UTF-8',
        data : "Section=5",
        success: fnSuccess,
        error:fnFail,
        timeout:30000
    });	
}

function fnSuccess(Response)
{	
	//DebugOut("Success...");
	//DebugOut(Response);
	//DebugOut(Response.RESULT.CODE);
	if(Response.RESULT != null && Response.RESULT.CODE != null && Response.RESULT.CODE == "0000")
	{
		if(Response.Section == "1")	
		{
			g_Data.loginData = Response.RESULT.loginData;			
			try
			{
				SectionLogin(g_Data);
			}
			catch(e)
			{				
			}
			try
			{
				SectionSettle(g_Data);
			}
			catch(e)
			{				
			}
			try
			{
				SectionLimit(g_Data);
			}
			catch(e)
			{				
			}
			
			g_Data.billsData = Response.RESULT.billsData;
			try
			{
				SectionBills(g_Data);
			}
			catch(e)
			{				
			}
			
			g_Data.reportBusiness = Response.RESULT.reportBusiness;
			g_Data.reportLocate = Response.RESULT.reportLocate;
			
			try
			{
				SectionReport(g_Data);			
			}
			catch(e)
			{				
			}
			
			g_Data.foundCardData = Response.RESULT.foundCardData;
			
			try
			{
				SectionFoundCard(g_Data);
			}
			catch(e)
			{				
			}
			
			g_Data.ownCardData = Response.RESULT.ownCardData;
			g_Data.suggestCardData = Response.RESULT.suggestCardData;
			try
			{
				SectionCard(g_Data);
			}
			catch(e)
			{				
			}			
			showProgress(false);	
		}
		else if(Response.Section == "2")	
		{
			g_Data.pointData = Response.RESULT.pointData;
			try
			{
				SectionPoint(g_Data);
			}
			catch(e)
			{				
			}
			
			g_Data.primeData = Response.RESULT.primeData;
			g_Data.vipLoungeData = Response.RESULT.vipLoungeData;		
		}
		else if(Response.Section == "3")	
		{
			g_Data.shortLoanData = Response.RESULT.shortLoanData;	
		}
		else if(Response.Section == "4")	
		{
			g_Data.loanData = Response.RESULT.loanData;
		}
		else if(Response.Section == "5")	
		{
			g_Data.revolvingData = Response.RESULT.revolvingData;		
		}
	}
	LoadingHide();
	/*
	if(Response.RESULT.CODE == "0000")
	{
		SectionLogin(Response.RESULT);
		SectionVipLounge(Response.RESULT);
		SectionSettle(Response.RESULT);
		SectionLimit(Response.RESULT);
		SectionPoint(Response.RESULT);
		SectionFoundCard(Response.RESULT);
		SectionShortLoan(Response.RESULT);
		SectionLoan(Response.RESULT);
		SectionRevolving(Response.RESULT);
		SectionReport(Response.RESULT);
		SectionCard(Response.RESULT);		
		InitializeChart();
	}
	*/
}

function fnFail(xhr, ajaxOptions, thrownError)
{
	LoadingHide();
	DebugOut("Fail....");
	//DebugOut(obj);
}

function fnComplete()
{
	//DebugOut(g_Data);
	try
	{
		SectionVipLounge(g_Data);
	}
	catch(e)
	{		
	}
	try
	{
		SectionShortLoan(g_Data);
	}
	catch(e)
	{		
	}
	try
	{
		SectionLoan(g_Data);
	}
	catch(e)
	{		
	}
	try
	{
		SectionRevolving(g_Data);
	}
	catch(e)
	{		
	}
	/*
	SectionLogin(g_Data);
	SectionVipLounge(g_Data);
	SectionSettle(g_Data);
	SectionLimit(g_Data);
	SectionPoint(g_Data);
	SectionFoundCard(g_Data);
	SectionShortLoan(g_Data);
	SectionLoan(g_Data);
	SectionRevolving(g_Data);
	SectionReport(g_Data);
	SectionCard(g_Data);
	*/
	InitializeChart();
}

/**
 * 로그인 섹션
 */
function SectionLogin(Response)
{
	if(Response.loginData == null) return;	
	//DebugOut("Response.loginData");
	//DebugOut(Response.loginData);	
	var CreditAddrType = Response.loginData.CreditAddrType;
    var CreditAddrName = Response.loginData.CreditAddrName;
    var CheckAddrType = Response.loginData.CheckAddrType;
    var CheckAddrName = Response.loginData.CheckAddrName;    
    var SettleType = Response.loginData.SettleType;    
    var Settle1 = Response.loginData.Settle1;
    var Settle2 = Response.loginData.Settle2;
    var Settle3 = Response.loginData.Settle3;    
    var SmsType = Response.loginData.SmsType;    
    var vipYn = Response.loginData.vipYn;
    var userGrade = Response.loginData.userGrade;        
    var lifeFeeCnt = 0;  
    if(Settle1 > 0) lifeFeeCnt++;
    if(Settle2 > 0) lifeFeeCnt++;
    if(Settle3 > 0) lifeFeeCnt++;
    $("#CreditAddrType").html(CreditAddrType);	
	if(SmsType == "") $("#SmsType").html("미등록");
	else $("#SmsType").html(SmsType);	
	$("#lifeFeeCnt").html(lifeFeeCnt+"건 이용중");	
	if(userGrade != "")
	{
		$(".user-info__class").html(userGrade);
		$(".user-info__class").show();
	}
}

/**
 * VIP 라운지 데이터
 */
function SectionVipLounge(Response)
{
	if(Response.vipLoungeData == null || Response.ownCardData == null || Response.ownCardData.vipCardList == null || Response.ownCardData.vipCardList.length == 0)
	{
		$(".lounge-vip").hide();
		return;
	}
	//DebugOut("Response.vipLoungeData");
	//DebugOut(Response.vipLoungeData);	
	//DebugOut(Response.vipLoungeData.vipServiceAmount);                       
	//DebugOut(Response.vipLoungeData.vipCouponCount);	
	$("#vipServiceAmount").html(comma(NVL(Response.vipLoungeData.vipServiceAmount,"0"))+"<em class=\"vip-cont__unit\">원</em>");	
	if(NVL(Response.vipLoungeData.corpVipCouponCount,"0") != "0") {
		$("#usableCoupon").text("개인_신청가능 쿠폰");
		$("#usedCoupon").text("기업_신청가능 쿠폰");
		$("#vipCouponCount").html(NVL(Response.vipLoungeData.vipCouponCount,"0")+"<em class=\"vip-cont__unit\">장</em>");	
		$("#usedVipCouponCount").html(NVL(Response.vipLoungeData.corpVipCouponCount,"0")+"<em class=\"vip-cont__unit\">장</em>");
		$("#usedVipCouponCount").attr("onClick", "location.href='/BON/DVIEW/HBDM0000?corpYn=Y'");
	} else {
		$("#vipCouponCount").html(NVL(Response.vipLoungeData.vipCouponCount,"0")+"<em class=\"vip-cont__unit\">장</em>");	
		$("#usedVipCouponCount").html(NVL(Response.vipLoungeData.usedVipCouponCount,"0")+"<em class=\"vip-cont__unit\">장</em>");	
	}
	for(i = 0 ; i < Response.ownCardData.vipCardList.length ; i++)
	{
		var currentCard = Response.ownCardData.vipCardList[i];
		var AppendHtml = "";
		var productURL = "/CRD/DVIEW/HCAMCXPRICAC0076?mainCC=a&cooperationcode=" + currentCard.allianceCD + "&solicitorcode=7030024780";
		AppendHtml += "<div class=\"swiper-slide\">";
		AppendHtml += "	<a href=\"javascript:location.href='"+productURL+"';\" class=\"vip-cont__link\">";
		AppendHtml += "		<img class=\"imgs\" src=\""+currentCard.cardImg+"\" alt=\""+currentCard.productName+"\">";
		AppendHtml += "		<div class=\"vip-cont__inner\">";
		AppendHtml += "			<p class=\"vip-cont__tit\">나의 VIP 카드"+(i+1)+"</p>";
		AppendHtml += "			<p class=\"vip-cont__txt\">"+currentCard.productName+"</p>";
		AppendHtml += "		</div>";
		AppendHtml += "	</a>";
		AppendHtml += "</div>";	
		$("#vipCardList").append(AppendHtml);		
	}
	
	$("#TabVip").show();
	$("#TabPrime").hide();
	if(Response.primeData != null)
	{
		if(Response.primeData.custSvcYn == "Y")
		{
			$("#custSvcCdNm").html(Response.primeData.custSvcCdNm+"<em class=\"vip-cont__unit\"></em>");
			$("#accuMile").html("<a href=\"/BON/DVIEW/HBCMCXPRISVS0039\">"+comma(Response.primeData.accuMile)+"<em class=\"vip-cont__unit\">M</em></a>");
						
			var currentDate = new Date();
			var currentYear = currentDate.getFullYear();		
			var expireDate = new Date(currentYear+"/01/31");	
			var StartDate = formatDate(Response.primeData.custRegDt, ".");

			var startDate = new Date(StartDate);			
			var InsertHtml = "";
			
			
			if(currentDate > expireDate) InsertHtml = StartDate + "~" + (currentYear+1) + ".01.31";
			else InsertHtml = StartDate + "~" + currentYear + ".01.31";
			
			$("#primeDate").html(InsertHtml);			
			$("#TabPrime").show();			
		}		
	}
	$(".lounge-vip").show();
	
	//vip카드
	var vipSwiper = new Swiper('.swiper-wrap--vip', 
	{
		slidesPerView: 1,
		spaceBetween: 32,
		pagination: { el: '.swiper-pagination', type: 'fraction', },
		navigation: { prevEl: '.swiper-wrap--vip .swiper-button-prev', nextEl: '.swiper-wrap--vip .swiper-button-next', },
	});
}

/**
 * MYKB 결제예정금액
 */
function SectionSettle(Response)
{
	if(Response.loginData == null) return;	
	//DebugOut("Response.loginData");
	//DebugOut(Response.loginData);	
	$("#SettleTab1").hide();
	$("#SettleTab2").hide();
	$("#SettleTab3").hide();
	$("#SettleTab4").hide();	
	
	var cardGb = Response.loginData.cardGb; 
	
	var Credit = true;
	var Check1 = true;
	var Check2 = true;
	
	if(Response.loginData.CreditSettleAccount == "" && Response.loginData.CreditSettleDate == "") Credit = false;
	if(Response.loginData.CheckSettleAccount1 == "" && Response.loginData.CheckSettleDate1 == "") Check1 = false;
	if(Response.loginData.CheckSettleAccount2 == "" && Response.loginData.CheckSettleDate2 == "") Check2 = false;
	
	var AppentHtml = "";
	
	//신용카드 결제예정금액
	$("#CreditSettleList").html("");
	if(Response.loginData.CreditSettleDate != null && Response.loginData.CreditSettleDate != "")
	{
		AppentHtml = "<li class=\"on\"><a href=\"javascript:void(0)\">"+Response.loginData.CreditSettleDate+"<em class=\"payment-day\">"+Response.loginData.CreditSettleDay+"</em></a></li>";
		$("#CreditSettleList").append(AppentHtml);
		$("#CreditSettleAccount").html("결제계좌 : "+Response.loginData.CreditSettleOrg+ " " +Response.loginData.CreditSettleAccount);
		$("#CreditSettleAmount").html(comma(Response.loginData.CreditSettleAmount)+"<em class=\"unit\">원</em>");		
	}	
	if(cardGb == "4")
	{
		if(Response.loginData.cardGb4settleList != null)
		{
			for(i = 0 ; i < Response.loginData.cardGb4settleList.length ; i++)
			{				
				AppentHtml = "<li id=\"CreditSettleNextDate\"><a href=\"javascript:void(0)\">"+Response.loginData.cardGb4settleList[i].SettleNextDate+"<em class=\"payment-day\">"+Response.loginData.cardGb4settleList[i].SettleNextDay+"</em></a></li>";
				$("#CreditSettleList").append(AppentHtml);		
				$("#CreditSettleNextAccount").html("결제계좌 : "+Response.loginData.CreditSettleOrg+ " " +Response.loginData.CreditSettleAccount);
				$("#CreditSettleNextAmount").html(comma(Response.loginData.cardGb4settleList[i].SettleNextAmount)+"<em class=\"unit\">원</em>");
				break;
			}			
		}
	}
	else
	{		
		if(Response.loginData.CreditSettleNextDate != null && Response.loginData.CreditSettleNextDate != "")
		{
			AppentHtml = "<li id=\"CreditSettleNextDate\"><a href=\"javascript:void(0)\">"+Response.loginData.CreditSettleNextDate+"<em class=\"payment-day\">"+Response.loginData.CreditSettleNextDay+"</em></a></li>";
			$("#CreditSettleList").append(AppentHtml);		
			$("#CreditSettleNextAccount").html("결제계좌 : "+Response.loginData.CreditSettleOrg+ " " +Response.loginData.CreditSettleAccount);
			$("#CreditSettleNextAmount").html(comma(Response.loginData.CreditSettleNextAmount)+"<em class=\"unit\">원</em>");
		}	
	}
	
	//체크카드 
	if(cardGb == "2") 
	{
		//이원화고객
		$("#CheckSettleList0").html("");
		AppentHtml = "<li class=\"on\"><a href=\"javascript:void(0)\">"+Response.loginData.CheckSettleDate1+"<em class=\"payment-day\">"+Response.loginData.CheckSettleDay1+"</em></a></li>";
		$("#CheckSettleList0").append(AppentHtml);		
		$("#CheckSettleAccount0").html("결제계좌 : "+Response.loginData.CheckSettleOrg1+ " " +Response.loginData.CheckSettleAccount1);
		$("#CheckSettleAmount0").html(comma(Response.loginData.CheckSettleAmount1)+"<em class=\"unit\">원</em>");
		
		if(Response.loginData.CheckSettleNextDate != null && Response.loginData.CheckSettleNextDate != "")
		{
			AppentHtml = "<li><a href=\"javascript:void(0)\">"+Response.loginData.CheckSettleNextDate+"<em class=\"payment-day\">"+Response.loginData.CheckSettleNextDay+"</em></a></li>";
			$("#CheckSettleList0").append(AppentHtml);		
			$("#CheckSettleNextAccount0").html("결제계좌 : "+Response.loginData.CheckSettleOrg1+ " " +Response.loginData.CheckSettleAccount1);
			$("#CheckSettleNextAmount0").html(comma(Response.loginData.CheckSettleNextAmount)+"<em class=\"unit\">원</em>");
		}	
	}
	else
	{
		//일반고객
		$("#CheckSettleList1").html("");
		AppentHtml = "<li class=\"on\"><a href=\"javascript:void(0)\">"+Response.loginData.CheckSettleDate1+"<em class=\"payment-day\">"+Response.loginData.CheckSettleDay1+"</em></a></li>";
		$("#CheckSettleList1").append(AppentHtml);		
		$("#CheckSettleAccount1").html("결제계좌 : "+Response.loginData.CheckSettleOrg1+ " " +Response.loginData.CheckSettleAccount1);
		$("#CheckSettleAmount1").html(comma(Response.loginData.CheckSettleAmount1)+"<em class=\"unit\">원</em>");	
		
		if(Response.loginData.CheckSettleNextDate != null && Response.loginData.CheckSettleNextDate != "")
		{
			AppentHtml = "<li><a href=\"javascript:void(0)\">"+Response.loginData.CheckSettleNextDate+"<em class=\"payment-day\">"+Response.loginData.CheckSettleNextDay+"</em></a></li>";
			$("#CheckSettleList1").append(AppentHtml);		
			$("#CheckSettleNextAccount1").html("결제계좌 : "+Response.loginData.CheckSettleOrg1+ " " +Response.loginData.CheckSettleAccount1);
			$("#CheckSettleNextAmount1").html(comma(Response.loginData.CheckSettleNextAmount)+"<em class=\"unit\">원</em>");
		}
	}
	
	$("#CheckSettleDate2").html("<a href=\"javascript:void(0)\">"+Response.loginData.CheckSettleDate2+"<em class=\"payment-day\">"+Response.loginData.CheckSettleDay2+"</em></a>");
	$("#CheckSettleAccount2").html("결제계좌 : "+Response.loginData.CheckSettleOrg2+ " " +Response.loginData.CheckSettleAccount2);
	$("#CheckSettleAmount2").html(comma(Response.loginData.CheckSettleAmount2)+"<em class=\"unit\">원</em>");
	
	if(Credit) $("#SettleTab1").show();
	else $("#SettleTab1").hide();
	
	if(cardGb == "2") 
	{
		if(Check1) $("#SettleTab2").show();
		else $("#SettleTab2").hide();
	}
	else
	{
		if(Check1) $("#SettleTab3").show();
		else $("#SettleTab3").hide();
	}
	if(Credit == false && Check1 == false)
	{
		//제휴체크카드만 있을때 보여준다.
		if(Check2) $("#SettleTab4").show();
		else $("#SettleTab4").hide();
	}
	
	if(cardGb == "2") 
	{
		$("#SettleTab1Title").html("KB");
		$("#SettleTab2Title").html("BC");
		$("#SettleTab3Title").html("체크");
		$("#SettleTab4Title").html("제휴체크");
	}
	else if(cardGb == "B")
	{
		$("#SettleTab1Title").html("일반");
		$("#SettleTab2Title").html("BC");
		$("#SettleTab3Title").html("특정");
		$("#SettleTab4Title").html("제휴체크");
	}
	else
	{
		$("#SettleTab1Title").html("신용");
		$("#SettleTab2Title").html("BC");
		$("#SettleTab3Title").html("체크");
		$("#SettleTab4Title").html("제휴체크");
	}
}

/**
 * MYKB 이용한도 
 */
function SectionLimit(Response)
{
	if(Response.loginData == null) return;	
	
	var cardGb = Response.loginData.cardGb;
	var AllTotLimit = Response.loginData.AllTotLimit;
	var AllTotRest = Response.loginData.AllTotRest;
	var AllCashLimit = Response.loginData.AllCashLimit;
	var AllCashRest = Response.loginData.AllCashRest;
	var KmcTotLimit = Response.loginData.KmcTotLimit;
	var KmcTotRest = Response.loginData.KmcTotRest;
	var KmcCashLimit = Response.loginData.KmcCashLimit;
	var KmcCashRest = Response.loginData.KmcCashRest;
	var KbcTotLimit = Response.loginData.KbcTotLimit;
	var KbcTotRest = Response.loginData.KbcTotRest;
	var KbcTotCashLimit = Response.loginData.KbcTotCashLimit;
	var KbcTotCashRest = Response.loginData.KbcTotCashRest;
	var Check1NLimit = Response.loginData.Check1NLimit;
	var Check1DLimit = Response.loginData.Check1DLimit;
	var Check1MLimit = Response.loginData.Check1MLimit;
	
	DebugOut("CardType="+GetCardType(cardGb));
	
	if("2" == cardGb)
	{
		//KB국민,국민BC;
		SectionLimitTab1(KmcTotLimit,KmcTotRest);
		SectionLimitTab2(KbcTotLimit,KbcTotRest);		
	}
	else if("3" == cardGb)
	{
		//KB국민,KB국민체크;
		SectionLimitTab1(AllTotLimit,AllTotRest);
		SectionLimitTab3(Check1NLimit,Check1DLimit,Check1MLimit);
	}
	else if("4" == cardGb || "5" == cardGb)
	{
		//다원화;
		SectionLimitTab1(AllTotLimit,AllTotRest);
		SectionLimitTab3(Check1NLimit,Check1DLimit,Check1MLimit);
	}
	else if("6" == cardGb)
	{
		//KB국민,국민BC,제휴체크;
		SectionLimitTab1(KmcTotLimit,KmcTotRest);
		SectionLimitTab2(KbcTotLimit,KbcTotRest);
		SectionLimitTab4(Check1NLimit,Check1DLimit,Check1MLimit);
	}
	else if("7" == cardGb)
	{
		//KB국민,KB국민체크,제휴체크;
		SectionLimitTab1(AllTotLimit,AllTotRest);
		SectionLimitTab3(Check1NLimit,Check1DLimit,Check1MLimit);
	}
	else if("8" == cardGb || "9" == cardGb || "B" == cardGb)
	{
		//8 : KB국민(일반),KB국민(특정) , 9 : KB국민(일반),KB국민(특정),제휴체크;
		SectionLimitTab1(KmcTotLimit,KmcTotRest);
		SectionLimitTab2(KbcTotLimit,KbcTotRest);
		SectionLimitTab4(Check1NLimit,Check1DLimit,Check1MLimit);
	}
	else if("A" == cardGb)
	{
		//다원화 | 다원화,제휴체크
		SectionLimitTab1(AllTotLimit,AllTotRest);
		SectionLimitTab3(Check1NLimit,Check1DLimit,Check1MLimit);
	}
	else
	{	
		//KB국민
		SectionLimitTab1(AllTotLimit,AllTotRest);
		SectionLimitTab3(Check1NLimit,Check1DLimit,Check1MLimit);
	}
	
	if("2" == cardGb)
	{
		 //KB국민,국민BC;
		$("#LimitTabTitle1").html("KB");
		$("#LimitTabTitle2").html("BC");
		$("#LimitTabTitle3").html("체크");
		$("#LimitTabTitle4").html("KB체크");
		$("#LimitTab1").show();
		$("#LimitTab2").show();
		$("#LimitTab3").hide();
		$("#LimitTab4").hide();			
		$("#LimitTab1").attr("class","on");
		$("#LimitTab2").attr("class","");
		$("#LimitTab3").attr("class","");
		$("#LimitTab4").attr("class","");
	}
	else if("3" == cardGb)
	{
		 //KB국민,KB국민체크;
		$("#LimitTabTitle1").html("신용");
		$("#LimitTabTitle2").html("BC");
		$("#LimitTabTitle3").html("체크");
		$("#LimitTabTitle4").html("KB체크");
		$("#LimitTab1").show();
		$("#LimitTab2").hide();
		$("#LimitTab3").show();
		$("#LimitTab4").hide();			
		$("#LimitTab1").attr("class","on");
		$("#LimitTab2").attr("class","");
		$("#LimitTab3").attr("class","");
		$("#LimitTab4").attr("class","");
	}
	else if("4" == cardGb || "5" == cardGb)
	{
		 //제휴체크;
		$("#LimitTabTitle1").html("신용");
		$("#LimitTabTitle2").html("BC");
		$("#LimitTabTitle3").html("체크");
		$("#LimitTabTitle4").html("제휴체크");
		$("#LimitTab1").show();
		$("#LimitTab2").hide();
		$("#LimitTab3").show();
		$("#LimitTab4").hide();			
		$("#LimitTab1").attr("class","on");
		$("#LimitTab2").attr("class","");
		$("#LimitTab3").attr("class","");
		$("#LimitTab4").attr("class","");
	}
	else if("6" == cardGb)
	{
		 //KB국민,국민BC,제휴체크;
		$("#LimitTabTitle1").html("KB");
		$("#LimitTabTitle2").html("BC");
		$("#LimitTabTitle3").html("체크");
		$("#LimitTabTitle4").html("제휴체크");
		$("#LimitTab1").show();
		$("#LimitTab2").show();
		$("#LimitTab3").hide();
		$("#LimitTab4").hide();			
		$("#LimitTab1").attr("class","on");
		$("#LimitTab2").attr("class","");
		$("#LimitTab3").attr("class","");
		$("#LimitTab4").attr("class","");
	}
	else if("7" == cardGb )
	{
		 //KB국민,KB국민체크,제휴체크;
		$("#LimitTabTitle1").html("KB");
		$("#LimitTabTitle2").html("BC");
		$("#LimitTabTitle3").html("체크");
		$("#LimitTabTitle4").html("제휴체크");
		$("#LimitTab1").show();
		$("#LimitTab2").hide();
		$("#LimitTab3").show();
		$("#LimitTab4").hide();			
		$("#LimitTab1").attr("class","on");
		$("#LimitTab2").attr("class","");
		$("#LimitTab3").attr("class","");
		$("#LimitTab4").attr("class","");
	}
	else if("8" == cardGb || "9" == cardGb || "B" == cardGb)
	{
		//8 : KB국민(일반),KB국민(특정) , 9 : KB국민(일반),KB국민(특정),제휴체크;
		$("#LimitTabTitle1").html("일반");
		$("#LimitTabTitle2").html("특정");
		$("#LimitTabTitle3").html("체크");
		$("#LimitTabTitle4").html("제휴체크");
		$("#LimitTab1").show();
		$("#LimitTab2").show();
		$("#LimitTab3").hide();
		$("#LimitTab4").hide();			
		$("#LimitTab1").attr("class","on");
		$("#LimitTab2").attr("class","");
		$("#LimitTab3").attr("class","");
		$("#LimitTab4").attr("class","");
	}
	else if("A" == cardGb)
	{
		//다원화
		$("#LimitTabTitle1").html("신용");
		$("#LimitTabTitle2").html("특정");
		$("#LimitTabTitle3").html("체크");
		$("#LimitTabTitle4").html("제휴체크");
		$("#LimitTab1").show();
		$("#LimitTab2").hide();
		$("#LimitTab3").show();
		$("#LimitTab4").hide();			
		$("#LimitTab1").attr("class","on");
		$("#LimitTab2").attr("class","");
		$("#LimitTab3").attr("class","");
		$("#LimitTab4").attr("class","");
	}	
	else
	{	
		//KB국민
		$("#LimitTabTitle1").html("신용");
		$("#LimitTabTitle2").html("특정");
		$("#LimitTabTitle3").html("체크");
		$("#LimitTabTitle4").html("KB체크");
		$("#LimitTab1").show();
		$("#LimitTab2").hide();
		$("#LimitTab3").show();
		$("#LimitTab4").hide();				
		$("#LimitTab1").attr("class","on");
		$("#LimitTab2").attr("class","");
		$("#LimitTab3").attr("class","");
		$("#LimitTab4").attr("class","");
	}	
}

function SectionLimitTab1(TotalLimit,TotalRest)
{
	$("#LimitTab1TotAmount").html("총 이용한도 "+comma(TotalLimit)+"원");
	$("#LimitTab1TotRest").html("<a href=\"/MKB/DVIEW/HMBMCXPRIMYS0013\">"+comma(TotalRest)+"<em class=\"payment-info__unit\">원</em></a>");
	var usedAmount = Number(TotalLimit) - Number(TotalRest);
	var usedRate = Math.round((usedAmount / Number(TotalLimit)) * 100);
	if(isNaN(usedRate) == true) usedRate = 0;
	$("#LimitTab1UsedRate").html(usedRate+"% 사용");
	$("#LimitTab1UsedRate").attr("style","width:"+usedRate+"%");
}

function SectionLimitTab2(TotalLimit,TotalRest)
{
	$("#LimitTab2TotAmount").html("총 이용한도 "+comma(TotalLimit)+"원");
	$("#LimitTab2TotRest").html(comma(TotalRest)+"<em class=\"payment-info__unit\">원</em>");
	var usedAmount = Number(TotalLimit) - Number(TotalRest);
	var usedRate = Math.round((usedAmount / Number(TotalLimit)) * 100);
	if(isNaN(usedRate) == true) usedRate = 0;
	$("#LimitTab2UsedRate").html(usedRate+"%");
	$("#LimitTab2UsedRate").attr("style","width:"+usedRate+"%");
}

function SectionLimitTab3(Limit1N,Limit1D,Limit1M)
{
	$("#LimitTab3Check1NLimit").html("<a href=\"/MKB/DVIEW/HMBMCXPRIMYS0013\">"+comma(Limit1N)+"<em class=\"payment-info__unit\">원</em></a>");
	$("#LimitTab3Check1DLimit").html("1일 한도 "+comma(Limit1D)+"원");
	$("#LimitTab3Check1MLimit").html("월간 한도 "+comma(Limit1M)+"원");
}

function SectionLimitTab4(Limit1N,Limit1D,Limit1M)
{
	$("#LimitTab4Check1NLimit").html("<a href=\"/MKB/DVIEW/HMBMCXPRIMYS0013\">"+comma(Limit1N)+"<em class=\"payment-info__unit\">원</em></a>");
	$("#LimitTab4Check1DLimit").html("1일 한도 "+comma(Limit1D)+"원");
	$("#LimitTab4Check1MLimit").html("월간 한도 "+comma(Limit1M)+"원");	
}

/**
 * MYKB 이번달 혜택
 */
function SectionPoint(Response)
{
	if(Response.pointData == null && Response.pointData.bnfAmount == null) 
	{
		$("#bnfAmount").html("0<em class=\"month-benefit__unit\">원</em>");	
		$("#pointRy").html("0<em class=\"month-benefit__unit\">P</em>");
		return;	
	}
	//DebugOut("Response.pointData");
	//DebugOut(Response.pointData);	
	if(NVL(Response.pointData.gbnYearBnfPnt,"") == "1")
	{
		$("#bnfEdYm").html(NVL(Response.pointData.bnfEdYm));
	}
	$("#bnfAmount").html(comma(NVL(Response.pointData.bnfAmount,"0"))+"<em class=\"month-benefit__unit\">원</em>");	
	$("#pointRy").html(comma(NVL(Response.pointData.pointRy,"0"))+"<em class=\"month-benefit__unit\">P</em>");
}

/**
 * 이용실적
 */
function SectionFoundCard(Response)
{	
	if(Response.foundCardData == null || Response.foundCardData.benefitCardList == null || Response.foundCardData.benefitCardCount == null || Response.foundCardData.benefitCardCount == 0 || Response.foundCardData.benefitCardList.length == 0) 
	{
		AppendHtml = "<div class=\"swiper-slide\">";
		AppendHtml += "<div class=\"no-data\">";
		AppendHtml += "<p>이용실적&middot;혜택 대상카드가 없습니다.<br>지금 나에게 딱 맞는 카드를 찾아 보세요.</p>";
		AppendHtml += "<a href=\"javascript:location.href='/CRD/DVIEW/HCAMCXPRICACR0001?mainCC=a'\">맞춤카드</a>";
		AppendHtml += "</div>";
		AppendHtml += "</div>";	
		$("#BenefitCardList").append(AppendHtml);	
	}
	else
	{
		//DebugOut("Response.foundCardData");
		//DebugOut(Response.foundCardData);
		var ownerCardCount = 0;
		for(i = 0 ; i < Response.foundCardData.benefitCardList.length ; i++)
		{
			var currentCard = Response.foundCardData.benefitCardList[i];
			//currentCard.thisMonthAmt; //실적인정금액
			//currentCard.thisMonthPcnt2; //이번달 혜택 % To-be
			//currentCard.thisMonthSec; //이번달 혜택 구간
			//currentCard.lastMonthPcnt; //지난달 혜택 %
			//currentCard.lastMonthSec; //지난달 도달 구간 
			//currentCard.nextMonthPcnt; //다음달 혜택 %
			//currentCard.cardArsltRangMxamt; //추가금액
			//currentCard.nextBoonSectNo; //추가 금액 사용시 도달할수 있는 구간		
			//currentCard.cardSectCnt; //카드혜택구간갯수
			//currentCard.gathering_yn; //회수여부
			//currentCard.cstzBoonYn; // 혜택없는 카드여부
			if(currentCard.isOwner == "1" ||  currentCard.isOwner == "2")
			{			
				ownerCardCount++;
				var CardSectionCount = Number(currentCard.cardSectCnt);
				var percentWidth = Math.round(100 / (CardSectionCount-1));
				var lastMonthSec = Number(currentCard.lastMonthSec); 
				var thisMonthSec = Number(currentCard.thisMonthSec);
				var thisMonthPcnt2 = Number(currentCard.thisMonthPcnt2);
				
				var AppendHtml = "";			
				AppendHtml += "<div class=\"swiper-slide\">";
				AppendHtml += "	<div class=\"usage-card\">";
				AppendHtml += "		<span class=\"usage-card__img\"><img src=\""+currentCard.cardImg+"\" alt=\"\"></span>";
				AppendHtml += "		<span class=\"usage-card__title\">"+currentCard.productName+"</span>";
				AppendHtml += "	</div>";
				AppendHtml += "	<div class=\"chart-stick\">";
				AppendHtml += "		<div class=\"chart-stick__rail\">";
				AppendHtml += "			<div class=\"chart-stick__bar\" style=\"width:"+thisMonthPcnt2+"%;\"></div>";
				AppendHtml += "			<div class=\"chart-stick__circle\"></div>	";
				AppendHtml += "			<div class=\"chart-stick__prev\">지난달 달성</div>";
				AppendHtml += "		</div>";
				AppendHtml += "		<div class=\"stick-list\">";
				
				for(z = 0 ; z < CardSectionCount ; z++)
				{
					var ClassValue = "stick-list__item";
					if(lastMonthSec == z) ClassValue += " prev";
					if(thisMonthSec == z) ClassValue += " on";
					var titleValue = '';
					if(thisMonthSec == z) titleValue = 'title="현재"';
					if(z == (CardSectionCount-1)) AppendHtml += "<div class=\""+ClassValue+"\"><span class=\"unit\" "+titleValue+">"+z+"<em class=\"txt\">구간</em></span></div>"; 
					else AppendHtml += "<div class=\""+ClassValue+"\" style=\"width: "+percentWidth+"%;\"><span class=\"unit\" "+titleValue+">"+z+"<em class=\"txt\">구간</em></span></div>";				
				}	
				AppendHtml += "		</div>";
				AppendHtml += "	</div>";
				
				AppendHtml += "	<div class=\"notice-box\">";
				if(currentCard.cstzBoonYn == "N") 
				{
					AppendHtml += "		<span class=\"badge-noti\">실적미달성</span>";
				}
				else 
				{
					if(currentCard.thisMonthSec == "0") 
					{
						AppendHtml += "		<span class=\"badge-noti \">실적미달성</span>";
					}
					else 
					{
						AppendHtml += "		<span class=\"badge-noti type\">실적달성</span>";
					}				
					if(currentCard.thisMonthPcnt2 == "100.0") 
					{
						AppendHtml += "		<span class=\"notice-box__txt\">다음달 "+(CardSectionCount-1)+"구간 혜택 확정! 모든 구간 달성을 축하드려요!";
					}
					else 
					{
						var cardArsltRangMxamt = comma(Math.floor(Number(NVL(currentCard.cardArsltRangMxamt,"0")))+"");			
						AppendHtml += "		<span class=\"notice-box__txt\">다음달 "+currentCard.nextBoonSectNo+"구간 혜택까지 "+cardArsltRangMxamt+"원 남았어요!</span>";
						AppendHtml += "		<div class=\"tooltip-box\">"; 
						AppendHtml += "			<div class=\"tooltip-box__txt\">실적달성 <button type=\"button\" class=\"tooltip\">i<span class=\"blind\">도움말</span></button></div>";
						AppendHtml += "			<div class=\"tooltip__view w-400\">";
						AppendHtml += "				<div class=\"tooltip-box__head\">";
						AppendHtml += "					<h1 class=\"tit-dep5 mt0 hidden\">제목</h1>";
						AppendHtml += "					<a href=\"javascript:void(0)\" class=\"btn-close\">닫기</a>";
						AppendHtml += "				</div>";
						AppendHtml += "				<div class=\"tooltip-box__cont\">";
						AppendHtml += "					<p>해당 카드는 "+(CardSectionCount-1)+"구간의 실적 구간이 있습니다.<br>더 많은 카드 혜택을 받아보세요.</p>";
						AppendHtml += "				</div>";
						AppendHtml += "			</div>";
						AppendHtml += "		</div>";
					}
				}
				AppendHtml += "	</div>";			
				$("#BenefitCardList").append(AppendHtml);
			}
		}
		if(ownerCardCount == 0)
		{		
			var AppendHtml = "";
			AppendHtml = "<div class=\"swiper-slide\">";
			AppendHtml += "<div class=\"no-data\">";
			AppendHtml += "<p>이용실적&middot;혜택 대상카드가 없습니다.<br>지금 나에게 딱 맞는 카드를 찾아 보세요.</p>";
			AppendHtml += "<a href=\"javascript:location.href='/CRD/DVIEW/HCAMCXPRICACR0001?mainCC=a'\">맞춤카드</a>";
			AppendHtml += "</div>";
			AppendHtml += "</div>";	
			$("#BenefitCardList").append(AppendHtml);
		}			
	}
	
	jsCartStick();//메인 스틱 차트(초기화)
	jsCartStickMove();//메인 스틱 차트(모션)
	
    //이용실적·혜택
	var swiper = new Swiper('.swiper-wrap--num .swiper-container', 
	{
		slidesPerView: 1,
		loop: true,
		pagination: { el: '.swiper-pagination', type: 'fraction', },
		navigation: { prevEl: '.swiper-button-prev', nextEl: '.swiper-button-next', },
		on: 
		{
			slideChangeTransitionEnd: function()
			{
				jsCartStickMove();
			},
		},
	});
}

/**
 * 단기대출
 */
function SectionShortLoan(Response)
{	
	if(Response.shortLoanData == null) return;
	if(Response.ownCardData == null) return;
	if(Response.loginData == null) return;
	
	var isCaDeniedCust = NVL(Response.shortLoanData.isCaDeniedCust,"N");
	var isCaUseCust = NVL(Response.shortLoanData.isCaUseCust,"N");
	var DUALUSERYN = NVL(Response.shortLoanData.DUALUSERYN,"N"); //이원화고객 : Y
	
	//이자율
	var kbDispApplyRate = NVL(Response.shortLoanData.kbDispApplyRate, "0.0"); //할인 후 통합(이원화 고객일시 KB) 이자율
	var dispChargeRateKB = NVL(Response.shortLoanData.dispChargeRateKB, "0.0"); //할인 전 통합(이원화 고객일시 KB) 이자율
	var bcDispApplyRate = NVL(Response.shortLoanData.bcDispApplyRate, "0.0"); //(이원화 고객만 존재) 할인 후 BC 이자율
	var dispChargeRateBC = NVL(Response.shortLoanData.dispChargeRateBC, "0.0"); //(이원화 고객만 존재) 할인 전 BC 이자율
	
	//단기카드대출 한도/잔여한도
	var KB_USELIMITMONEY = NVL(Response.shortLoanData.KB_USELIMITMONEY,"0"); //회원통합(이원화 고객일시 KB) 현금서비스한도  
  	var KB_REMAINMONEY = NVL(Response.shortLoanData.KB_REMAINMONEY,"0"); //회원통합(이원화 고객일시 KB) 현금서비스잔여한도
  	var KBBC_USELIMITMONEY = NVL(Response.shortLoanData.KBBC_USELIMITMONEY,"0"); //(이원화 고객만 존재) BC이용현금서비스한도
  	var KBBC_REMAINMONEY = NVL(Response.shortLoanData.KBBC_REMAINMONEY,"0"); //(이원화 고객만 존재) BC이용현금서비스잔여한도 
	var TOTAL_USELIMITMONEY = NVL(Response.shortLoanData.TOTAL_USELIMITMONEY,"0"); //총 한도
	var TOTAL_REMAINMONEY = NVL(Response.shortLoanData.TOTAL_REMAINMONEY,"0");
		
	var TOTAL_REMAINMONEY_FMT = TOTAL_REMAINMONEY;
	if(TOTAL_REMAINMONEY > 10000) TOTAL_REMAINMONEY_FMT = Number(TOTAL_REMAINMONEY) / 10000;
	if(isNaN(TOTAL_REMAINMONEY_FMT)) TOTAL_REMAINMONEY_FMT = 0;
	
	var KB_REMAINMONEY_FMT = KB_REMAINMONEY;
	if(KB_REMAINMONEY > 10000) KB_REMAINMONEY_FMT = Number(KB_REMAINMONEY) / 10000;
	if(isNaN(KB_REMAINMONEY_FMT)) KB_REMAINMONEY_FMT = 0;
	
	var KBBC_REMAINMONEY_FMT = KBBC_REMAINMONEY;
	if(KBBC_REMAINMONEY > 10000) KBBC_REMAINMONEY_FMT = Number(KBBC_REMAINMONEY) / 10000;
	if(isNaN(KBBC_REMAINMONEY_FMT)) KBBC_REMAINMONEY_FMT = 0;
		
	//2020.01.22 JHR - 단기카드대출 프로세스 개선(증액데이터)
	var strRsultDstic = NVL(Response.shortLoanData.strRsultDstic, "02");
	var strImaAbilYn = NVL(Response.shortLoanData.strImaAbilYn, "");
	var strImaAbilCadvLimt = NVL(Response.shortLoanData.strImaAbilCadvLimt, "0");

	//한도 증액 가능 조건
	//한도 증액 가능 조건이 달라지면 여기 if만 수정 하단에선 canCaLimtUpYn로 판단하면 됨.	
	var canCaLimtUpYn = "N";
	var strImaAbilCadvLimtFmt = "";
	if(strRsultDstic != "02" && (strImaAbilYn == "1" || strImaAbilYn == "4")) 
	{
		canCaLimtUpYn = "Y"; 
		strImaAbilCadvLimtFmt = Number(strImaAbilCadvLimt) / 10000;
	}
	//DebugOut("TOTAL_REMAINMONEY_FMT:"+NVL(Response.shortLoanData.TOTAL_REMAINMONEY,"0"));
	//DebugOut("TOTAL_REMAINMONEY_FMT:"+TOTAL_REMAINMONEY_FMT);
	var SettleType = NVL(Response.loginData.SettleType,"1");
	var CreditUse = NVL(Response.loginData.CreditUse,"1");
	var AppendHtml = "";
	
	if(CreditUse != "1" && TOTAL_REMAINMONEY == "0")
	{
		//신용카드사용고객이아니고 이용가능금액이 없는경우.
		return;
	}
	if(isCaDeniedCust == "Y")
	{
		//이용동의거부고객
		AppendHtml += "<li class=\"breakdown-card__item\">";
		AppendHtml += "<h3>단기카드대출</h3>";
		AppendHtml += "<a href=\"javascript:location.href='/FNC/DVIEW/HFBMCXPRIFISD0005';\" class=\"btn btn--primary btn--s\">동의</a>";
		AppendHtml += "<p class=\"breakdown-card__desc\">현금이 필요할 때, 24시간 365일 1만원부터</p>";
		AppendHtml += "<ul class=\"breakdown-card__group\">";
		AppendHtml += "<li>";
		AppendHtml += "<p class=\"breakdown-card__tit\">가능금액</p>";
		AppendHtml += "<p class=\"breakdown-card__point\"><em class=\"breakdown-card__unit\">최대&nbsp;</em>800<em class=\"breakdown-card__unit\">만원</em></p>";
		AppendHtml += "</li>";
		AppendHtml += "<li>";
		AppendHtml += "<p class=\"breakdown-card__tit\">연 이자율</p>";
		AppendHtml += "<p class=\"breakdown-card__point\">5.90~19.95<em class=\"breakdown-card__unit\">%</em></p>";
		AppendHtml += "</li>";
		AppendHtml += "</ul>";
		//AppendHtml += "<a href=\"javascript:;\" class=\"btn-limit\"><span>6,000만원까지 한도증액가능</span></a>";
		AppendHtml += "</li>";		
		$("#FinanceText").html("이용동의 거절 중인 고객은 동의 신청 후 가능금액+이율이 출력됩니다.");
		
	}
	else 
	{
		$("#FinanceText").html("연 이자율은 개인신용평점 등에 따라 차등 적용됩니다.");
		if(isCaUseCust == "Y") 
		{
			if(DUALUSERYN == "Y")
			{
				//단기대출 이용중고객,이원화고객
				AppendHtml += "<li class=\"breakdown-card__item\">";
				AppendHtml += "<h3>단기카드대출</h3>";
				AppendHtml += "<a href=\"javascript:location.href='/FNC/DVIEW/HFBMCXPRIMYS0044';\" class=\"btn btn--primary btn--s\">이용내역</a>";
				AppendHtml += "<span class=\"badge-noti\">총 한도&nbsp;"+comma(TOTAL_USELIMITMONEY)+"원</span>";				
				AppendHtml += "<ul class=\"breakdown-card__group\">";
				AppendHtml += "<li>";
				AppendHtml += "<p class=\"breakdown-card__tit\">이용가능금액</p>";
				AppendHtml += "<p class=\"breakdown-card__point\"><em class=\"breakdown-card__unit\">BC&nbsp;</em>"+comma(MakeMan(KBBC_REMAINMONEY))+"<em class=\"breakdown-card__unit\">만원</em></p>";
				AppendHtml += "<p class=\"breakdown-card__point\"><em class=\"breakdown-card__unit\">KB&nbsp;</em>"+comma(MakeMan(KB_REMAINMONEY))+"<em class=\"breakdown-card__unit\">만원</em></p>";
				AppendHtml += "</li>";
				AppendHtml += "<li>";
				AppendHtml += "<p class=\"breakdown-card__tit\">연 이자율</p>";
				if (dispChargeRateBC == bcDispApplyRate || dispChargeRateBC == "0.0") 
				{
					AppendHtml += "<p class=\"breakdown-card__point\">"+bcDispApplyRate+"<em class=\"breakdown-card__unit\">%</em></p>";
				}
				else
				{
					AppendHtml += "<p class=\"breakdown-card__point\"><em class=\"breakdown-card__del\">"+dispChargeRateBC+"</em>&nbsp;"+bcDispApplyRate+"<em class=\"breakdown-card__unit\">%</em></p>";
				}			
				if (dispChargeRateKB == kbDispApplyRate || dispChargeRateKB == "0.0") 
				{
					AppendHtml += "<p class=\"breakdown-card__point\">"+kbDispApplyRate+"<em class=\"breakdown-card__unit\">%</em></p>";
				}
				else
				{ 
					AppendHtml += "<p class=\"breakdown-card__point\"><em class=\"breakdown-card__del\">"+dispChargeRateKB+"</em>&nbsp;"+kbDispApplyRate+"<em class=\"breakdown-card__unit\">%</em></p>";
				}	
				//AppendHtml += "<p class=\"breakdown-card__point\">"+bcDispApplyRate+"<em class=\"breakdown-card__unit\">%</em></p>";
				//AppendHtml += "<p class=\"breakdown-card__point\">"+kbDispApplyRate+"<em class=\"breakdown-card__unit\">%</em></p>";
				AppendHtml += "</li>";
				AppendHtml += "</ul>";
				AppendHtml += "</li>";
			}
			else
			{
				//단기대출 이용중고객,일반고객
				AppendHtml += "<li class=\"breakdown-card__item\">";
				AppendHtml += "<h3>단기카드대출</h3>";
				AppendHtml += "<a href=\"javascript:location.href='/FNC/DVIEW/HFBMCXPRIMYS0044';\" class=\"btn btn--primary btn--s\">이용내역</a>";
				AppendHtml += "<span class=\"badge-noti\">총 한도&nbsp;"+comma(TOTAL_USELIMITMONEY)+"원</span>";
				AppendHtml += "<ul class=\"breakdown-card__group\">";
				AppendHtml += "<li>";
				AppendHtml += "<p class=\"breakdown-card__tit\">이용가능금액</p>";
				AppendHtml += "<p class=\"breakdown-card__point\"><em class=\"breakdown-card__unit\"></em>"+comma(MakeMan(TOTAL_REMAINMONEY))+"<em class=\"breakdown-card__unit\">만원</em></p>";
				AppendHtml += "</li>";
				AppendHtml += "<li>";
				AppendHtml += "<p class=\"breakdown-card__tit\">연 이자율</p>";
				if (dispChargeRateKB == kbDispApplyRate || dispChargeRateKB == "0.0") 
				{
					AppendHtml += "<p class=\"breakdown-card__point\">"+kbDispApplyRate+"<em class=\"breakdown-card__unit\">%</em></p>";
				}
				else
				{ 
					AppendHtml += "<p class=\"breakdown-card__point\"><em class=\"breakdown-card__del\">"+dispChargeRateKB+"</em>&nbsp;"+kbDispApplyRate+"<em class=\"breakdown-card__unit\">%</em></p>";
				}
				//AppendHtml += "<p class=\"breakdown-card__point\">"+kbDispApplyRate+"<em class=\"breakdown-card__unit\">%</em></p>";
				AppendHtml += "</li>";
				AppendHtml += "</ul>";
				AppendHtml += "</li>";
			}
		}
		else
		{
			if(DUALUSERYN == "Y")
			{
				//단기대출 미사용고객,이원화
				AppendHtml += "<li class=\"breakdown-card__item\">";
				AppendHtml += "<h3>단기카드대출</h3>";
				AppendHtml += "<a href=\"javascript:location.href='/FNC/DVIEW/HFBMCXPRIFIC0096';\" class=\"btn btn--primary btn--s\">신청</a>";
				AppendHtml += "<p class=\"breakdown-card__desc\">현금이 필요할 때, 24시간 365일 1만원부터</p>";
				AppendHtml += "<ul class=\"breakdown-card__group\">";
				AppendHtml += "<li>";
				AppendHtml += "<p class=\"breakdown-card__tit\">가능금액</p>";
				AppendHtml += "<p class=\"breakdown-card__point\"><em class=\"breakdown-card__unit\">BC&nbsp;</em>"+comma(MakeMan(KBBC_REMAINMONEY_FMT))+"<em class=\"breakdown-card__unit\">만원</em></p>";
				AppendHtml += "<p class=\"breakdown-card__point\"><em class=\"breakdown-card__unit\">KB&nbsp;</em>"+comma(MakeMan(KB_REMAINMONEY_FMT))+"<em class=\"breakdown-card__unit\">만원</em></p>";
				AppendHtml += "</li>";
				AppendHtml += "<li>";
				AppendHtml += "<p class=\"breakdown-card__tit\">연 이자율</p>";
				if (dispChargeRateBC == bcDispApplyRate || dispChargeRateBC == "0.0") 
				{
					AppendHtml += "<p class=\"breakdown-card__point\">"+bcDispApplyRate+"<em class=\"breakdown-card__unit\">%</em></p>";
				}
				else
				{
					AppendHtml += "<p class=\"breakdown-card__point\"><em class=\"breakdown-card__del\">"+dispChargeRateBC+"</em>&nbsp;"+bcDispApplyRate+"<em class=\"breakdown-card__unit\">%</em></p>";
				}			
				if (dispChargeRateKB == kbDispApplyRate || dispChargeRateKB == "0.0") 
				{
					AppendHtml += "<p class=\"breakdown-card__point\">"+kbDispApplyRate+"<em class=\"breakdown-card__unit\">%</em></p>";
				}
				else
				{ 
					AppendHtml += "<p class=\"breakdown-card__point\"><em class=\"breakdown-card__del\">"+dispChargeRateKB+"</em>&nbsp;"+kbDispApplyRate+"<em class=\"breakdown-card__unit\">%</em></p>";
				}			
				AppendHtml += "</li>";
				AppendHtml += "</ul>";
				if(canCaLimtUpYn == "Y") AppendHtml += "<a href=\"javascript:location.href='/MKB/DVIEW/HMBMCXPRIMYS0013';\" class=\"btn-limit\"><span>"+comma(strImaAbilCadvLimtFmt)+"만원까지 한도증액가능</span></a>";
				AppendHtml += "</li>";
			}
			else
			{
				//단기대출 미사용고객,일반
				AppendHtml += "<li class=\"breakdown-card__item\">";
				AppendHtml += "<h3>단기카드대출</h3>";
				AppendHtml += "<a href=\"javascript:location.href='/FNC/DVIEW/HFBMCXPRIFIC0096';\" class=\"btn btn--primary btn--s\">신청</a>";
				AppendHtml += "<p class=\"breakdown-card__desc\">현금이 필요할 때, 24시간 365일 1만원부터</p>";
				AppendHtml += "<ul class=\"breakdown-card__group\">";
				AppendHtml += "<li>";
				AppendHtml += "<p class=\"breakdown-card__tit\">가능금액</p>";
				AppendHtml += "<p class=\"breakdown-card__point\"><em class=\"breakdown-card__unit\">최대&nbsp;</em>"+comma(MakeMan(TOTAL_REMAINMONEY_FMT))+"<em class=\"breakdown-card__unit\">만원</em></p>";
				AppendHtml += "</li>";
				AppendHtml += "<li>";
				AppendHtml += "<p class=\"breakdown-card__tit\">연 이자율</p>";
				if (dispChargeRateKB == kbDispApplyRate || dispChargeRateKB == "0.0") 
				{
					AppendHtml += "<p class=\"breakdown-card__point\">"+kbDispApplyRate+"<em class=\"breakdown-card__unit\">%</em></p>";
				}
				else
				{ 
					AppendHtml += "<p class=\"breakdown-card__point\"><em class=\"breakdown-card__del\">"+dispChargeRateKB+"</em>&nbsp;"+kbDispApplyRate+"<em class=\"breakdown-card__unit\">%</em></p>";
				}
				AppendHtml += "</li>";
				AppendHtml += "</ul>";
				if(canCaLimtUpYn == "Y") AppendHtml += "<a href=\"javascript:location.href='/MKB/DVIEW/HMBMCXPRIMYS0013';\" class=\"btn-limit\"><span>"+comma(strImaAbilCadvLimtFmt)+"만원까지 한도증액가능</span></a>";
				AppendHtml += "</li>";
			}
		}
	}
	$("#FinanceList").append(AppendHtml);
}

function SectionLoan(Response)
{
	if(Response.loanData == null) return ;
	
	var cardloanAgreedYn = NVL(Response.loanData.cardloanAgreedYn,"N");
	var cardloanInUseYn = NVL(Response.loanData.cardloanInUseYn,"N");
	var askAmtSum = NVL(Response.loanData.askAmtSum,"0");
	var totalLoanRemainAmt = NVL(Response.loanData.totalLoanRemainAmt,"0");	 
	var totalLoanAmt = NVL(Response.loanData.totalLoanAmt,"0");	 
	var maxAblamtBaseAmt = NVL(Response.loanData.maxAblamtBaseAmt,"0");
	var maxAblamtBaseAmtInt = 0;
	if(maxAblamtBaseAmt > 10000) maxAblamtBaseAmtInt = Number(maxAblamtBaseAmt) / 10000;	
	//DebugOut(maxAblamtBaseAmtInt);
	var maxAblamtBaseIrt = NVL(Response.loanData.maxAblamtBaseIrt,"0");
	var maxAblamtBaseDscntIrt = NVL(Response.loanData.maxAblamtBaseDscntIrt,"0");
	var minInlnBaseAmt = NVL(Response.loanData.minInlnBaseAmt,"0"); //최소이자율기준 금액
	var minInlnBaseIrt = NVL(Response.loanData.minInlnBaseIrt,"0"); //최소이자율기준 정상이율
	var minInlnBaseDscntIrt = NVL(Response.loanData.minInlnBaseDscntIrt,"0"); //(할인후)최소이자율기준 할인후적용금리

	var AppendHtml = "";
	
	if(cardloanAgreedYn == "Y" && totalLoanAmt > 0)
	{
		//이미대출한사람
		AppendHtml += "<li class=\"breakdown-card__item\">";
		AppendHtml += "   <h3>장기카드대출</h3>";
		AppendHtml += "<a href=\"/FNC/DVIEW/HFAMCXPRIFIC0099\" class=\"btn btn--primary btn--s\">대출관리</a>";
		AppendHtml += "<span class=\"badge-noti\">결제예정금액&nbsp;"+comma(askAmtSum)+"원</span>";
		AppendHtml += "<p class=\"breakdown-card__tit\">대출 잔액</p>";
		AppendHtml += "<p class=\"breakdown-card__point\">"+comma(totalLoanRemainAmt)+"<em class=\"breakdown-card__unit\">원</em></p>";
		AppendHtml += "</li>";		
	}
	else if(cardloanAgreedYn == "Y" && totalLoanAmt <= 0 && maxAblamtBaseDscntIrt > 0)
	{
		//아직 안한사람
		AppendHtml += "<li class=\"breakdown-card__item\">";
		AppendHtml += "<h3>장기카드대출</h3>";
		AppendHtml += "<a href=\"/FNC/DVIEW/HFAMCXPRIFIC0100\" class=\"btn btn--primary btn--s\">신청</a>";
		AppendHtml += "<p class=\"breakdown-card__desc\">목돈이 필요할 때 중도상환수수료 없이 신청</p>";
		AppendHtml += "<ul class=\"breakdown-card__group\">";
		AppendHtml += "<li>";
		AppendHtml += "<p class=\"breakdown-card__tit\">가능금액</p>";
		AppendHtml += "<p class=\"breakdown-card__point\"><em class=\"breakdown-card__unit\">최대</em>&nbsp;"+comma(""+maxAblamtBaseAmtInt)+"<em class=\"breakdown-card__unit\">만원</em></p>";
		AppendHtml += "</li>";
		AppendHtml += "<li>";
		AppendHtml += "<p class=\"breakdown-card__tit\">연 이자율</p>";
		AppendHtml += "<p class=\"breakdown-card__point\">"+maxAblamtBaseDscntIrt+"<em class=\"breakdown-card__unit\">%</em></p>";
		AppendHtml += "</li>";
		AppendHtml += "</ul>";
		AppendHtml += "</li>";			
	}
	// 장기카드대출 미동의 고객
	else if (cardloanAgreedYn == "N") {
		AppendHtml += "<li class=\"breakdown-card__item\">";
		AppendHtml += "<h3>장기카드대출</h3>";
		AppendHtml += "<a href=\"/FNC/DVIEW/HFAMCXPRIFIS0023\" class=\"btn btn--primary btn--s\">동의</a>";
		AppendHtml += "<p class=\"breakdown-card__desc\">목돈이 필요할 때 중도상환수수료 없이 신청</p>";
		AppendHtml += "<ul class=\"breakdown-card__group\">";
		AppendHtml += "<li>";
		AppendHtml += "<p class=\"breakdown-card__tit\">가능금액</p>";
		AppendHtml += "<p class=\"breakdown-card__point\"><em class=\"breakdown-card__unit\">최대&nbsp;</em>5,000<em class=\"breakdown-card__unit\">만원</em></p>";
		AppendHtml += "</li>";
		AppendHtml += "<li>";
		AppendHtml += "<p class=\"breakdown-card__tit\">연 이자율</p>";
		AppendHtml += "<p class=\"breakdown-card__point\">3.90~19.90<em class=\"breakdown-card__unit\">%</em></p>";
		AppendHtml += "</li>";
		AppendHtml += "</ul>";
		AppendHtml += "</li>";
		$("#FinanceText").html("이용동의 거절 중인 고객은 동의 신청 후 가능금액+이율이 출력됩니다.");
	}
	else
	{
		//고정문구
		AppendHtml += "<li class=\"breakdown-card__item\">";
		AppendHtml += "<h3>장기카드대출</h3>";
		AppendHtml += "<a href=\"/FNC/DVIEW/HFAMCXPRIFIC0100\" class=\"btn btn--primary btn--s\">신청</a>";
		AppendHtml += "<p class=\"breakdown-card__desc\">목돈이 필요할 때 중도상환수수료 없이 신청</p>";
		AppendHtml += "<ul class=\"breakdown-card__group\">";
		AppendHtml += "<li>";
		AppendHtml += "<p class=\"breakdown-card__tit\">가능금액</p>";
		AppendHtml += "<p class=\"breakdown-card__point\"><em class=\"breakdown-card__unit\">최대&nbsp;</em>5,000<em class=\"breakdown-card__unit\">만원</em></p>";
		AppendHtml += "</li>";
		AppendHtml += "<li>";
		AppendHtml += "<p class=\"breakdown-card__tit\">연 이자율</p>";
		AppendHtml += "<p class=\"breakdown-card__point\">3.90~19.90<em class=\"breakdown-card__unit\">%</em></p>";
		AppendHtml += "</li>";
		AppendHtml += "</ul>";
		AppendHtml += "</li>";	
	}
	$("#FinanceList").append(AppendHtml);
}

function SectionRevolving(Response)
{
	if(Response.revolvingData == null) return ;
	var availableServiceYn = NVL(Response.revolvingData.availableServiceYn,"");
	var hopeRate = NVL(Response.revolvingData.hopeRate, "0");
	var minHopeRate = NVL(Response.revolvingData.minHopeRate, "0");
	var rlsRate = NVL(Response.revolvingData.rlsRate, "0");
	var hopePayAmt = NVL(Response.revolvingData.hopePayAmt, "0");

	var minPayAmt = NVL(Response.revolvingData.minPayAmt, "0");
	
	if(minHopeRate.indexOf(".") > 0)
	{
		var idx = minHopeRate.indexOf(".");
		minHopeRate = minHopeRate.substring(0, idx);
	}
	var AppendHtml = "";
	if(availableServiceYn == "A")
	{
		AppendHtml += "<li class=\"breakdown-card__item\">";
		AppendHtml += "<h3>일부결제금액이월약정</h3>";
		AppendHtml += "<a href=\"/FNC/DVIEW/HFCMCXPRIFIS0018\" class=\"btn btn--primary btn--s\">변경</a>";
		AppendHtml += "<span class=\"badge-noti\">결제예정금액 <a href=\"/MKB/DVIEW/HMCMCXPRIMYS0041\" class=\"btn_link\">최소결제 "+comma(minPayAmt)+"원</a></span>";
		AppendHtml += "<ul class=\"breakdown-card__group\">";
		AppendHtml += "<li>";
		AppendHtml += "<p class=\"breakdown-card__tit\">결제비율</p>";
		AppendHtml += "<p class=\"breakdown-card__point\">"+hopeRate+"<em class=\"breakdown-card__unit\">%</em></p>";
		AppendHtml += "</li>";
		AppendHtml += "<li>";
		AppendHtml += "<p class=\"breakdown-card__tit\">연 이자율</p>";
		AppendHtml += "<p class=\"breakdown-card__point\">"+rlsRate+"<em class=\"breakdown-card__unit\">%</em></p>";
		AppendHtml += "</li>";
		AppendHtml += "</ul>";
		AppendHtml += "</li>";
	}
	else
	{
		if(availableServiceYn == "Y")
		{
			AppendHtml += "<li class=\"breakdown-card__item\">";
			AppendHtml += "<h3>일부결제금액이월약정</h3>";
			AppendHtml += "<a href=\"/FNC/DVIEW/HFCMCXPRIFIS0017\" class=\"btn btn--primary btn--s\">신청</a>";
			AppendHtml += "<p class=\"breakdown-card__desc\">결제금액이 부담될 때 최소결제</p>";
			AppendHtml += "<ul class=\"breakdown-card__group\">";
			AppendHtml += "<li>";
			AppendHtml += "<p class=\"breakdown-card__tit\">결제비율</p>";
			AppendHtml += "<p class=\"breakdown-card__point\">"+hopeRate+"<em class=\"breakdown-card__unit\">%</em></p>";
			AppendHtml += "</li>";
			AppendHtml += "<li>";
			AppendHtml += "<p class=\"breakdown-card__tit\">연 이자율</p>";
			AppendHtml += "<p class=\"breakdown-card__point\">"+rlsRate+"<em class=\"breakdown-card__unit\">%</em></p>";
			AppendHtml += "</li>";
			AppendHtml += "</ul>";
			AppendHtml += "</li>";
		}
		else
		{
			AppendHtml += "<li class=\"breakdown-card__item\">";
			AppendHtml += "<h3>일부결제금액이월약정</h3>";
			AppendHtml += "<a href=\"/FNC/DVIEW/HFCMCXPRIFIS0017\" class=\"btn btn--primary btn--s\">신청</a>";
			AppendHtml += "<p class=\"breakdown-card__desc\">결제금액이 부담될 때 최소결제</p>";
			AppendHtml += "<ul class=\"breakdown-card__group\">";
			AppendHtml += "<li>";
			AppendHtml += "<p class=\"breakdown-card__tit\">연 이자율</p>";
			AppendHtml += "<p class=\"breakdown-card__point\">5.60~23.60<em class=\"breakdown-card__unit\">%</em></p>";
			AppendHtml += "</li>";
			AppendHtml += "</ul>";
			AppendHtml += "</li>";
		}		
	}
	$("#FinanceList").append(AppendHtml);
}

/**
 * 이용대금 명세서 
 */
function SectionBills(Response)
{
	if(Response.loginData == null) 
	{
		SectionBillsClose(); //이용대금명세서가 없습니다.		                               
		return;
	}
	if(Response.billsData == null)
	{
		SectionBillsClose(); //이용대금명세서가 없습니다.                               
		return;
	}
	if(Response.billsData.Settle1 == null && Response.billsData.Settle2 == null) 
	{
		SectionBillsClose(); //이용대금명세서가 없습니다.
		return;
	}
	var cardGb = Response.loginData.cardGb;
	DebugOut("CardType="+GetCardType(cardGb));
	
	var Settle1Visible = false;
	var Settle2Visible = false;
	var Settle1Date1 = "";
	var Settle1Date2 = "";
	var Settle1Amount = "";	
	var Settle2Date1 = "";
	var Settle2Date2 = "";
	var Settle2Amount = "";
	if(Response.billsData.Settle1 != null)
	{
		Settle1Date1 = NVL(Response.billsData.Settle1.SettleDate1,"");
		Settle1Date2 = NVL(Response.billsData.Settle1.SettleDate2,"");
		Settle1Amount = NVL(Response.billsData.Settle1.SettleAmount,"");		
		if(Settle1Date1 != "" && Settle1Date2 != "" && Settle1Amount != "") Settle1Visible = true;
	}
	if(Response.billsData.Settle2 != null)
	{
		Settle2Date1 = NVL(Response.billsData.Settle2.SettleDate1,"");
		Settle2Date2 = NVL(Response.billsData.Settle2.SettleDate2,"");
		Settle2Amount = NVL(Response.billsData.Settle2.SettleAmount,"");
		if(Settle2Date1 != "" && Settle2Date2 != "" && Settle2Amount != "") Settle2Visible = true;
	}
	
	if(Settle1Visible == false && Settle2Visible == false)
	{
		SectionBillsClose(); //이용대금명세서가 없습니다.
		return ;
	}
	SectionBillsTab1(Settle1Date1,Settle1Date2,Settle1Amount);
	SectionBillsTab2(Settle2Date1,Settle2Date2,Settle2Amount);				
	
	$("#ReportSettleTab1").attr("class","");
	$("#ReportSettleTab2").attr("class","");
	$("#ReportSettleTab1Content").hide();
	$("#ReportSettleTab2Content").hide();
	
	if("2" == cardGb)
	{
		//KB국민,국민BC;
		$("#ReportSettleTab1Title").html("KB");
		$("#ReportSettleTab2Title").html("BC");		
		if(Settle1Visible) $("#ReportSettleTab1").show();
		else $("#ReportSettleTab1").hide();
		if(Settle2Visible) $("#ReportSettleTab2").show();
		else $("#ReportSettleTab2").hide();
		if(Settle1Visible) 
		{
			$("#ReportSettleTab1").attr("class","on");
			$("#ReportSettleTab1Content").show();
		}
		else
		{
			$("#ReportSettleTab2").attr("class","on");
			$("#ReportSettleTab2Content").show();
		}				
	}
	else if("3" == cardGb)
	{
		//KB국민,KB국민체크;	
		$("#ReportSettleTab1Title").html("신용");
		$("#ReportSettleTab2Title").html("체크");		
		if(Settle1Visible) $("#ReportSettleTab1").show();
		else $("#ReportSettleTab1").hide();
		if(Settle2Visible) $("#ReportSettleTab2").show();
		else $("#ReportSettleTab2").hide();
		if(Settle1Visible) $("#ReportSettleTab1").attr("class","on");
		else $("#ReportSettleTab2").attr("class","on");
		if(Settle1Visible) 
		{
			$("#ReportSettleTab1").attr("class","on");
			$("#ReportSettleTab1Content").show();
		}
		else
		{
			$("#ReportSettleTab2").attr("class","on");
			$("#ReportSettleTab2Content").show();
		}
	}
	else if("4" == cardGb || "5" == cardGb)
	{
		//다원화;		
		$("#ReportSettleTab1Title").html("신용");
		$("#ReportSettleTab2Title").html("");		
		if(Settle1Visible) $("#ReportSettleTab1").show();
		else $("#ReportSettleTab1").hide();
		$("#ReportSettleTab2").hide();		
		$("#ReportSettleTab1").attr("class","on");		
		$("#ReportSettleTab1Content").show();
	}	
	else if("6" == cardGb)
	{
		//KB국민,국민BC,제휴체크;
		$("#ReportSettleTab1Title").html("KB");
		$("#ReportSettleTab2Title").html("BC");		
		if(Settle1Visible) $("#ReportSettleTab1").show();
		else $("#ReportSettleTab1").hide();
		if(Settle2Visible) $("#ReportSettleTab2").show();
		else $("#ReportSettleTab2").hide();
		if(Settle1Visible) $("#ReportSettleTab1").attr("class","on");
		else $("#ReportSettleTab2").attr("class","on");
		if(Settle1Visible) 
		{
			$("#ReportSettleTab1").attr("class","on");
			$("#ReportSettleTab1Content").show();
		}
		else
		{
			$("#ReportSettleTab2").attr("class","on");
			$("#ReportSettleTab2Content").show();
		}
	}
	else if("7" == cardGb)
	{
		//KB국민,KB국민체크,제휴체크;
		$("#ReportSettleTab1Title").html("신용");
		$("#ReportSettleTab2Title").html("체크");		
		if(Settle1Visible) $("#ReportSettleTab1").show();
		else $("#ReportSettleTab1").hide();
		if(Settle2Visible) $("#ReportSettleTab2").show();
		else $("#ReportSettleTab2").hide();
		if(Settle1Visible) $("#ReportSettleTab1").attr("class","on");
		else $("#ReportSettleTab2").attr("class","on");
		if(Settle1Visible) 
		{
			$("#ReportSettleTab1").attr("class","on");
			$("#ReportSettleTab1Content").show();
		}
		else
		{
			$("#ReportSettleTab2").attr("class","on");
			$("#ReportSettleTab2Content").show();
		}
	}
	else if("8" == cardGb || "9" == cardGb  || "B" == cardGb)
	{
		//8 : KB국민(일반),KB국민(특정) , 9 : KB국민(일반),KB국민(특정),제휴체크;
		$("#ReportSettleTab1Title").html("일반");
		$("#ReportSettleTab2Title").html("특정");		
		if(Settle1Visible) $("#ReportSettleTab1").show();
		else $("#ReportSettleTab1").hide();
		if(Settle2Visible) $("#ReportSettleTab2").show();
		else $("#ReportSettleTab2").hide();
		if(Settle1Visible) $("#ReportSettleTab1").attr("class","on");
		else $("#ReportSettleTab2").attr("class","on");
		if(Settle1Visible) 
		{
			$("#ReportSettleTab1").attr("class","on");
			$("#ReportSettleTab1Content").show();
		}
		else
		{
			$("#ReportSettleTab2").attr("class","on");
			$("#ReportSettleTab2Content").show();
		}
	}
	else if("A" == cardGb)
	{
		//다원화 | 다원화,제휴체크
		$("#ReportSettleTab1Title").html("신용");
		$("#ReportSettleTab2Title").html("");		
		if(Settle1Visible) $("#ReportSettleTab1").show();
		else $("#ReportSettleTab1").hide();
		$("#ReportSettleTab2").hide();		
		$("#ReportSettleTab1").attr("class","on");
		$("#ReportSettleTab1Content").show();
	}
	else
	{	
		//KB국민
		$("#ReportSettleTab1Title").html("신용");
		$("#ReportSettleTab2Title").html("");		
		if(Settle1Visible) $("#ReportSettleTab1").show();
		else $("#ReportSettleTab1").hide();
		$("#ReportSettleTab2").hide();		
		$("#ReportSettleTab1").attr("class","on");
		$("#ReportSettleTab1Content").show();
	}
}

function SectionBillsTab1(SettleDate1,SettleDate2,SettleAmount)
{
	$("#ReportSettleTab1Date").html(getMonth(SettleDate1)+"월 이용대금명세서(작성기준일 "+formatDate(SettleDate2,".")+")");
	$("#ReportSettleTab1Amount").html(comma(SettleAmount)+"<em class=\"usage-fee__unit\">원</em>");	
}

function SectionBillsTab2(SettleDate1,SettleDate2,SettleAmount)
{
	$("#ReportSettleTab2Date").html(getMonth(SettleDate1)+"월 이용대금명세서(작성기준일 "+formatDate(SettleDate2,".")+")");
	$("#ReportSettleTab2Amount").html(comma(SettleAmount)+"<em class=\"usage-fee__unit\">원</em>");	
}
 
function SectionBillsClose()
{
	$("#ReportSettleTab1Date").html("");
	$("#ReportSettleTab1Amount").html("<em class=\"usage-fee__noting\">이용대금명세서가 없습니다.</em>");		
	$("#ReportSettleTab1Title").html("");		
	$("#ReportSettleTab2Title").html("");
	$("ReportSettleTab1").hide();
	$("ReportSettleTab2").hide();
	$("ReportSettleTab1").attr("class","");
	$("ReportSettleTab2").attr("class","");	
	return;
}

/**
 * 소비리포트 
 */
function SectionReport(Response)
{	
	var AppendHtml = "";
	if(Response.reportBusiness == null || Response.reportBusiness.length <= 0)
	{
		AppendHtml = "<div class=\"swiper-slide\">";
		AppendHtml += "	<div class=\"usage-report__tit\">업종별<span>(최근 3개월간)</span></div>";
		AppendHtml += "	<div class=\"no-data\">";
		AppendHtml += "		<p>최근 3개월 이용이 없어요.</p>";
		AppendHtml += "	</div>";
		AppendHtml += "</div>";
		$("#ReportList").append(AppendHtml);
	}
	else
	{
		//소비리포트		
		AppendHtml = "<div class=\"swiper-slide\">";
		AppendHtml += "	<div class=\"usage-report__tit\">업종별<span>(최근 3개월간)</span></div>";
	    AppendHtml += "	<div class=\"circle-box circle-box--flex\">";
	    AppendHtml += "		<div class=\"circle-svg\" aria-hidden=\"true\">";
	    AppendHtml += "			<svg></svg>";
	   	AppendHtml += "		</div>";
		AppendHtml += "		<ul class=\"circle-info\">";
		//for(i = 0 ; i < Response.reportBusiness.length ; i++)
		//{
		//	if(i > 2) break; 
		//	AppendHtml += "			<li>";
		//	AppendHtml += "				<em class=\"circle-info__pct\"><span class=\"circle-info__p"+(i+1)+"\"></span>"+Response.reportBusiness[i].RATE+"%</em>";
		//	AppendHtml += "				<strong class=\"circle-info__tit\">"+Response.reportBusiness[i].NAME+"</strong>";
		//	AppendHtml += "			</li>";			
		//}		
		AppendHtml += "		</ul>";
		AppendHtml += "	</div>";   
		AppendHtml += "</div>";
		$("#ReportList").append(AppendHtml);
		var pieChartData = [];		
		for(i = 0 ; i < Response.reportBusiness.length ; i++)
		{			
			if(i > 2) break;
			pieChartData.push({"name":Response.reportBusiness[i].NAME,"value":Response.reportBusiness[i].AMT, "ratio":Response.reportBusiness[i].RATE});
		}
		chart.pie.init(); 
		chart.pie.$target[0].pieChartSetup(pieChartData);
		chart.pie.$target[0].pieChartStart();		
	}
	
	if(Response.reportLocate == null || Response.reportLocate.LOCAL1 == null)
	{
		AppendHtml = "<div class=\"swiper-slide\">";
		AppendHtml += "<div class=\"usage-report__tit\">지역별<span>(최근 1개월간)</span>";
		AppendHtml += "<div class=\"tooltip-box\">";
		AppendHtml += "<div class=\"tooltip-box__txt\"><button type=\"button\" class=\"tooltip\">i</button></div>";
		AppendHtml += "<div class=\"tooltip__view w-300\">";
		AppendHtml += "<div class=\"tooltip-box__head\">";
		AppendHtml += "<h1 class=\"tit-dep5 mt0 hidden\">지역별 실정기준 및 제외업종</h1>";
		AppendHtml += "<a href=\"javascript:void(0)\" class=\"btn-close\">닫기</a>";
		AppendHtml += "</div>";
		AppendHtml += "<div class=\"tooltip-box__cont\">";
		AppendHtml += "<dl>";
		AppendHtml += "<dt>실적기준 :&nbsp;</dt>";
		AppendHtml += "<dd>매월 5일</dd>";
		AppendHtml += "</dl>";
		AppendHtml += "<dl>";
		AppendHtml += "<dt>제외업종 :&nbsp;</dt>";
		AppendHtml += "<dd>전자상거래, 교통, 세금<br>생활요금 관련 업종</dd>";
		AppendHtml += "</dl>";
		AppendHtml += "</div>";
		AppendHtml += "</div>";
		AppendHtml += "</div>";
		AppendHtml += "</div>";    	
		AppendHtml += "<div class=\"no-data\">";
		AppendHtml += "<p>최근 1개월 사용 현황이 없습니다.</p>";
		AppendHtml += "</div>";    
		AppendHtml += "</div>";
		$("#ReportList").append(AppendHtml);
	}
	else
	{
		AppendHtml = "<div class=\"swiper-slide\">";
		AppendHtml += "<div class=\"usage-report__tit\">지역별<span>(최근 1개월간)</span>";
		AppendHtml += "<div class=\"tooltip-box\">";
		AppendHtml += "<div class=\"tooltip-box__txt\"><button type=\"button\" class=\"tooltip\">i</button></div>";
		AppendHtml += "<div class=\"tooltip__view w-300\">";
		AppendHtml += "<div class=\"tooltip-box__head\">";
		AppendHtml += "<h1 class=\"tit-dep5 mt0 hidden\">지역별 실정기준 및 제외업종</h1>";
		AppendHtml += "<a href=\"javascript:void(0)\" class=\"btn-close\">닫기</a>";
		AppendHtml += "</div>";
		AppendHtml += "<div class=\"tooltip-box__cont\">";
		AppendHtml += "<dl>";
		AppendHtml += "<dt>실적기준 :&nbsp;</dt>";
		AppendHtml += "<dd>매월 5일</dd>";
		AppendHtml += "</dl>";
		AppendHtml += "<dl>";
		AppendHtml += "<dt>제외업종 :&nbsp;</dt>";
		AppendHtml += "<dd>전자상거래, 교통, 세금<br>생활요금 관련 업종</dd>";
		AppendHtml += "</dl>";
		AppendHtml += "</div>";
		AppendHtml += "</div>";
		AppendHtml += "</div>";
		AppendHtml += "</div>";								
		AppendHtml += "<div class=\"bar-graph bar-graph--horizontal bar-graph--flex\">";
		AppendHtml += "<ul class=\"bar-graph__list\">";
		if(Response.reportLocate.LOCAL1 != null && Response.reportLocate.LOCAL1 != "")
		{
			AppendHtml += "<li>";
			AppendHtml += "<div class=\"bar-graph__grh\">";
			AppendHtml += "<p class=\"bar-graph__bar bar-graph__bar--blue\">62</p>";
			AppendHtml += "</div>";
			AppendHtml += "<div class=\"bar-graph__info\">";
			AppendHtml += "<span class=\"bar-graph__percent\"></span>";
			AppendHtml += "<strong class=\"bar-graph__txt\">"+Response.reportLocate.LOCAL1+"</strong>";
			AppendHtml += "</div>";
			AppendHtml += "</li>";
		}
		if(Response.reportLocate.LOCAL2 != null && Response.reportLocate.LOCAL2 != "")
		{
			AppendHtml += "<li>";
			AppendHtml += "<div class=\"bar-graph__grh\">";
			AppendHtml += "<p class=\"bar-graph__bar bar-graph__bar--purple\">33</p>";
			AppendHtml += "</div>";
			AppendHtml += "<div class=\"bar-graph__info\">";
			AppendHtml += "<span class=\"bar-graph__percent\"></span>";
			AppendHtml += "<strong class=\"bar-graph__txt\">"+Response.reportLocate.LOCAL2+"</strong>";
			AppendHtml += "</div>";
			AppendHtml += "</li>";
		}
		if(Response.reportLocate.LOCAL3 != null && Response.reportLocate.LOCAL3 != "")
		{
			AppendHtml += "<li>";
			AppendHtml += "<div class=\"bar-graph__grh\">";
			AppendHtml += "<p class=\"bar-graph__bar bar-graph__bar--yellow\">17</p>";
			AppendHtml += "</div>";
			AppendHtml += "<div class=\"bar-graph__info\">";
			AppendHtml += "<span class=\"bar-graph__percent\"></span>";
			AppendHtml += "<strong class=\"bar-graph__txt\">"+Response.reportLocate.LOCAL3+"</strong>";
			AppendHtml += "</div>";
			AppendHtml += "</li>";
		}
		AppendHtml += "</ul>";
		AppendHtml += "</div>";
		AppendHtml += "</div>";
		$("#ReportList").append(AppendHtml);
	}	
	jsChartBarScrl();	
	var reportSwiper = new Swiper('.usage-report .swiper-container', 
	{
		slidesPerView: 1,
		loop: true,
		pagination: { el: '.swiper-pagination', type: 'fraction', },
		navigation: { prevEl: '.swiper-button-prev', nextEl: '.swiper-button-next', },
		on: 
		{
			slideChangeTransitionEnd: function()
			{
				if(this.activeIndex == 1) 
				{
					try
					{
						chart.pie.$target[0].pieChartStart();
					}
					catch(e)
					{						
					}				
				}
				if(this.activeIndex == 2) jsChartBarScrl();
			},
		},
	});
	$(document).on('click', '.tooltip', function(e)
	{
		var $this = $(this).parent().next('.tooltip__view');
		var $tg = $this.prev('.tooltip-box__txt');
		var $btnClose = $this.find('.btn-close');
		var thWidth = $(this).parent().width();
		var oHeight = $tg.height(); 
		var secHt = $tg.closest('section').children().outerHeight() + 48;
		var secTop = $tg.closest('section').offset().top;
		var tgTop = $tg.offset().top;
		var tipLt = secHt - (tgTop - secTop) - 18;
		var thisHt = $this.height();		
		if($this.hasClass('tooltip__view--rt'))
		{
			if(thisHt > tipLt) 
			{
				$this.show().css({ 'top' : tipLt - thisHt, 'left' : thWidth + 10 });
			}
			else 
			{
				$this.show().css({ 'top' : oHeight, 'left' : thWidth + 10 });
			}
		}
		else 
		{
			$this.show().css({ 'top' : oHeight, 'left' : 10 });
		}
		$btnClose.on('click', function()
		{
			$this.hide();
		});
	});
}

/**
 * 소유카드와 추천카드
 */
function SectionCard(Response)
{
	if(Response.ownCardData == null) return;
	//DebugOut("Response.ownCardData");
	//DebugOut(Response.ownCardData);
	
	var ownerCardCount = 0;
	if(Response.ownCardData.ownCardList != null)
	{
		for(i = 0 ; i < Response.ownCardData.ownCardList.length ; i++)
		{
			var AppendHtml = "";
			if(Response.ownCardData.ownCardList[i].isOwner == "1")
			{
				ownerCardCount++;
				var CardNo = Response.ownCardData.ownCardList[i].maskedCardNo;
				var CardNoHtml = " "+CardNo.substring(0, 7) + "<em class=\"mark\">●●</em>-<em class=\"mark\">●●●●</em>-" + CardNo.substring(15, 19);
				var productURL = "/CRD/DVIEW/HCAMCXPRICAC0076?mainCC=a&cooperationcode="+Response.ownCardData.ownCardList[i].allianceCD + "&solicitorcode=7030024780";			
				AppendHtml += "<div class=\"swiper-slide\">";
				AppendHtml += "<div class=\"card-intro__inner\">";
				AppendHtml += "<a href=\"javascript:location.href='"+productURL+"';\" class=\"card-intro__link\">";
				AppendHtml += "<span class=\"card-intro__img\"><img src=\""+Response.ownCardData.ownCardList[i].cardImg+"\"></span>";
				AppendHtml += "<div class=\"card-intro__text\">";
				AppendHtml += "<p class=\"card-intro__title\">"+Response.ownCardData.ownCardList[i].productName+"</p>";
				AppendHtml += "<span class=\"card-intro__number\"><img src=\""+Response.ownCardData.ownCardList[i].brandImgPath+"\">"+CardNoHtml+"</span>";
				AppendHtml += "</div>";
				AppendHtml += "</a>";
				AppendHtml += "</div>";
				AppendHtml += "</div>";
				$("#OwnCardList").append(AppendHtml);
			}
		}		
	}
	if(ownerCardCount == 0)
	{
		var AppendHtml = "";
		AppendHtml += "<div class=\"swiper-slide\">";
		AppendHtml += "<div class=\"card-intro__inner no-data\">";
		AppendHtml += "<div class=\"card-intro__text\">";
		AppendHtml += "<p>보유하고 있는카드가 없습니다.<br>지금 나에게 딱 맞는 카드를 찾아 보세요.</p>";
		AppendHtml += "<button type=\"button\" class=\"btn btn--gray2 btn--s\" onclick=\"javascript:location.href='/CRD/DVIEW/HCAMCXPRICACR0001?mainCC=a'\">맞춤카드</button>";
		AppendHtml += "</div>";
		AppendHtml += "</div>";
		AppendHtml += "</div>";
		$("#OwnCardList").append(AppendHtml);
	}

	if(Response.suggestCardData != null)
	{
		var selectList = null;
		if(Response.suggestCardData.suggestCardList1 != null && Response.suggestCardData.suggestCardList1.length > 0)
		{
			selectList = Response.suggestCardData.suggestCardList1;
		}
		else if(Response.suggestCardData.suggestCardList2 != null && Response.suggestCardData.suggestCardList2.length > 0)
		{
			selectList = Response.suggestCardData.suggestCardList2;
		}
		else if(Response.suggestCardData.suggestCardList3 != null && Response.suggestCardData.suggestCardList3.length > 0)
		{
			selectList = Response.suggestCardData.suggestCardList3;
		}
		if(selectList != null)
		{
			for(i = 0 ; i < selectList.length ; i++)
			{			
				var AppendHtml = "";
				var productCode = selectList[i].productCode;
				var productAllianceCD = selectList[i].productAllianceCD;
				var productName = selectList[i].productName;
				var productType = selectList[i].productType;
				var productText = selectList[i].productText;
				var productImgPath = selectList[i].productImgPath;
				var productRow	= selectList[i].productRow;
				var productURL = "/CRD/DVIEW/HCAMCXPRICAC0076?mainCC=a&cooperationcode="+productAllianceCD+ "&solicitorcode=7030024780";
				var productDescription = NVL(selectList[i].productDescription,"");			
				var tagDescription = "";
				if(productDescription != "")
				{
					var productDescriptList = productDescription.split(",");				
					for(z = 0 ; z < productDescriptList.length ; z++)
					{
						tagDescription += "#" + productDescriptList[z].trim() + " ";
					}
				}	
				AppendHtml += "<div class=\"swiper-slide\">";			
				AppendHtml += "<div class=\"card-intro__inner type\">";
				AppendHtml += "<a href=\"javascript:location.href='"+productURL+"';\" class=\"card-intro__link\">";
				if(productRow == "01"){
					AppendHtml += "<span class=\"card-intro__img row\"><img src=\""+productImgPath+"\"></span>";
				}else{
					AppendHtml += "<span class=\"card-intro__img\"><img src=\""+productImgPath+"\"></span>";
				}
				AppendHtml += "<div class=\"card-intro__text\">";
				if(tagDescription != "") AppendHtml += "<span class=\"badge-noti\">"+tagDescription+"</span>";
				AppendHtml += "<p class=\"card-intro__title\">"+productName+"</p>";
				AppendHtml += "<span class=\"card-intro__desc\">"+productText+"</span>";
				AppendHtml += "</div>";
				AppendHtml += "</a>";
				AppendHtml += "</div>";
				AppendHtml += "</div>";
				$("#SuggestCardList").append(AppendHtml);	
			}
		}		
	}
	
	var ownCard = new Swiper('.swiper-wrap__card .swiper-container', 
	{
		slidesPerView: 1,
		loop: true,
		pagination: { el: '.swiper-pagination', type: 'fraction', },
		navigation: { prevEl: '.swiper-button-prev', nextEl: '.swiper-button-next', },
	});
}

function InitializeBanner()
{
	if($('.setting-banner.pc-main').length > 0) {
		// main Banner
		var $slide = $('.setting-banner.pc-main'),
			autoCheck = true;
		
		var mainBanner = new Swiper('.setting-banner.pc-main .swiper-container', 
		{
			autoplay:{ delay:5000, },
			slidesPerView: 1,
			spaceBetween: 0,
			loop: true,
			pagination: { el: '.swiper-pagination', type: 'fraction', },
			navigation: { prevEl: '.setting-banner.pc-main .swiper-button-prev', nextEl: '.setting-banner.pc-main .swiper-button-next', },
			on: 
			{
				init: function() {
					//웹접근성 추가
					$slide.find('.swiper-slide').not('.swiper-slide-active').find('a').attr('tabindex', '-1');
				},				
				slideChangeTransitionEnd: function(e)
				{
					$slide.find('.swiper-slide').removeClass('mov').find('a').attr('tabindex', '-1');
					$slide.find('.swiper-slide-active').addClass('mov').find('a').removeAttr('tabindex');

					if(autoCheck == false) {
						$slide.find('.swiper-slide-active').find('a').focus();
					}
				},
			},
		});
		
		$('.swiper-button-next, .swiper-button-prev').on('click', function() {
			mainBanner.autoplay.stop();
			autoCheck = false;
		});
		
		$('.setting-banner.pc-main .swiper-wrap__btn').on('click', function()
		{
			if($(this).hasClass('on'))
			{
				$(this).removeClass('on');
				$(this).text('정지');
				mainBanner.autoplay.start();
				autoCheck = true;
			}
			else
			{
				$(this).addClass('on');
				$(this).text('재생 ');
				mainBanner.autoplay.stop();
				autoCheck = false;
			}
		});

		$('.swiper-slide a').on('focus', function() {
			mainBanner.autoplay.stop();
			autoCheck = false;
			$('.setting-banner.pc-main .swiper-wrap__btn').addClass('on').text('재생');
		});
	}
	
	// popup Banner
    $('.setting-banner.pop-slip').each(function(){
        var $slide = $(this);
        var mainBanner = new Swiper('.setting-banner.pop-slip .swiper-container', {
            autoplay:{
                delay:5000,
            },
            slidesPerView: 1,
            spaceBetween: 0,
            loop: true,
            pagination: {
                el: '.swiper-pagination',
            },
            navigation: {
            	prevEl: '.setting-banner.pop-slip .swiper-button-prev',
            	nextEl: '.setting-banner.pop-slip .swiper-button-next',                
            },
        });
    });
    
}

function InitializeChart()
{
	/*
	var len = $('.circle-svg').length;
