<script>
function CommonPopup()
{
	CommonPopupOpen();
}


function CommonPopupOpen()
{


	var PopupLength = $("#admin-popup").children().length;	
	if(PopupLength > 0)
	{
		//var popupSerialNo = $("#CommonPopup0").attr("popupSerialNo");

		var popupSerialNo = $("#admin-popup").attr("popupSerialNo");
		var key           = "CommonPopup"+popupSerialNo;
		var expire        = localStorage.getItem(key);
		var now           = Date.now();
		
		if(utlf_IsNullOrEmpty(expire) || (expire != "N" && now >= expire)) {
			KBCARD_GLOBAL.openPopup('#admin-popup');
			localStorage.removeItem(key);
		}
	}

}

function CommonPopupClose()
{
	var PopupLength = $("#admin-popup").children().length;	
	if(PopupLength > 0)
	{


		KBCARD_GLOBAL.closePopup("#admin-popup");
	
	}
}

function CommonPopupNeverOpen(popupSerialNo, popupExpire)
{
	//SetCookie("CommonPopup"+popupSerialNo,"E",365);	
	var expire = "";
	var date   = new Date();
	
	if(popupExpire == 999)
		expire = "N";
	else if(popupExpire > 0 && popupExpire < 31)
		expire = date.setDate(date.getDate()+parseInt(popupExpire));
		
	localStorage.setItem("CommonPopup"+popupSerialNo, expire);	
	CommonPopupClose();
}

function SetCookie(Name,Value,ExpireDays)
{
	var todayDate = new Date();
	todayDate.setDate(todayDate.getDate() + ExpireDays);
	document.cookie = Name + '=' + escape(Value) + '; path=/; expires=' + todayDate.toGMTString() + ';'
	// document.cookie = name + '=' + escape(value) + '; path=/;';
}

function GetCookie(Name)
{
	var search = Name + "=";
	var name = "";
	var cookiee = document.cookie;
	var tmpVal = -1;
	var offset = cookiee.indexOf(search);
	var end = -1;
	if (cookiee.length) 
	{
		tmpVal = cookiee.indexOf(";", offset);
		if (offset > -1) 
		{
			offset += search.length;
			end = (tmpVal < 0) ? cookiee.length : tmpVal;
		}
	}
	name = document.cookie.substring(offset, end) || '';
	return name;
}

 
</script>
		<!-- 공통팝업처리추가 sangman.park 20220325 Added. -->
