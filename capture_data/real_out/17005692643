//막대형 차트(bar chart)
function jsChartBAr()
{
	$('.bar-graph').each(function()
	{
		var $this = $(this);
		var $item = $(this).find('.bar-graph__bar');
		var itemAry = $item.map(function()
		{
			return $(this).text();
		});
		var moreNum = Math.max.apply(Math, itemAry);
		$item.each(function()
		{
			var txt = $(this).text();
			var vlu = (txt / moreNum) * 100;
			var spd = 3;
			var delay = 3;
			if($(this).closest('.bar-graph').hasClass('bar-graph--horizontal'))
			{
				if($this.hasClass('on'))
				{
					$(this).css({'width':vlu + '%', 'transition':'width .' + spd + 's', 'transition-delay':'.' + delay + 's'});
				}
				else
				{
					$(this).css({'width':'0%', 'transition':'inherit'});
				}
			}
			else
			{
				if($this.hasClass('on'))
				{
					$(this).css({'height':vlu + '%', 'transition':'height .' + spd + 's', 'transition-delay':'.' + delay + 's'});
				}
				else
				{
					$(this).css({'height':'0%', 'transition':'inherit'});
				}
			}
		});
	});
}

function jsChartBarScrl()
{
	var winTop = $(window).scrollTop();
	var winHei = $(window).height();
	$('.bar-graph').each(function()
	{
		var top = $(this).offset().top;
		var hei = $(this).outerHeight();
		if(winTop + winHei > top)
		{
			if(winTop > top + hei)
			{
				$(this).removeClass('on');
			}
			else
			{
				if(winTop + winHei > top + (hei / 2))
				{
					$(this).addClass('on');
				}
				
			}
		}
		else
		{
			$(this).removeClass('on');
		}
	});
	jsChartBAr();
}
    
function doViewNews(articleSerial) 
{
    $('#newsForm :input[name=ARTICLE_SERIAL]').val(articleSerial);
    $('#newsForm').submit();
}

function comma(str) 
{
	if(str == null || str == "") return;
    var parts = str.toString().split(".");
    parts[0] = parts[0].replace(/\B(?=(\d{3})+(?!\d))/g, ",");
    return parts.join(".");
}
function formatDate(dateString, seperator)
{
    var temp = String(dateString).trim();
    if(temp == "undefined" || temp == "")
    {
        return "";
    }
    else
    {
        var year = temp.substring(0, 4);
        var month = temp.substring(4, 6);
        var day = temp.substring(6, 8);

        if(seperator == "undefined" || seperator == null || seperator == "" || seperator == "add")
        {
            return year + "-" + month + "-" + day;
        }
        else
        {
            return year + seperator + month + seperator + day;
        }
    }
}
function getMonth(dateString)
{
    var temp = String(dateString).trim();
    if(temp == "undefined" || temp == "") return "";
    else
    {
        var month = temp.substring(4, 6);
        return "" + Number(month);
    }
}
function NVL(input,defVal)
{
	if(input == null || input == "undefined" || input == "") return defVal;
	else return input;
}

function MakeMan(input)
{
	if(input == null || input == "") return input;
	if(input.length > 4) return input.substring(0, input.length-4);
	else return input;
}
function MakeMoney(val)
{
	if (val == null || val == "") 
	{
		//obj = obj.replace(/[\D]/g, "");
		return val;
	}
	else 
	{
		if (val.substr(0, 1) == "-") 
		{
			val = val.substr(1, val.length);
			if (val.length > 3) 
			{
				var mod = val.length % 3;
				var retval = (mod > 0 ? (val.substring(0, mod)) : "");
				for ( var i = 0; i < Math.floor(val.length / 3); i++) 
				{
					if ((mod == 0) && (i == 0)) retval += val.substring(mod + 3 * i, mod + 3 * i + 3);
					else retval += "," + val.substring(mod + 3 * i, mod + 3 * i 	+ 3);
				}
				return "-" + retval;
			}
			return "-" + val;
		}
		else 
		{
			val = val.toString().replace(/[\D]/g, "");
			if (val.length > 3) 
			{
				var mod = val.length % 3;
				var retval = (mod > 0 ? (val.substring(0, mod)) : "");
				for ( var i = 0; i < Math.floor(val.length / 3); i++) 
				{
					if ((mod == 0) && (i == 0)) retval += val.substring(mod + 3 * i, mod + 3 * i + 3);
					else retval += "," + val.substring(mod + 3 * i, mod + 3 * i + 3);
				}
				return retval;
			}
			return val;
		}
	}
}
</script>

