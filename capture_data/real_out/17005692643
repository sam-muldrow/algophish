}

function ahnlabInit(clbc){
	//console.log("%cahnlabInit start, clbc: "+clbc, "color:blue");
	secuKeyInitCallback = ahnlabInitCallback;//ahnlab init callback function
	if ( kcmn.isFunc(clbc) ) {
		ahnlabInitAfterClbc = clbc;
	}
	secuKeyProc();
}

function ahnlabInitCallback(){
	$("#loginPwd").prop("disabled", false);
	$("#vPerkeypwd").prop("disabled", false);
	
	if ( kcmn.isFunc(ahnlabInitAfterClbc) ) {
		kcmn.execFunc(ahnlabInitAfterClbc);
		ahnlabInitAfterClbc = null;//호출 후 초기화
	}
}

