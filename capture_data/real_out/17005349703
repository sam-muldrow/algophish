    $('.lazy').lazy();

    $('.box-ads').each(function() {
      if ($(this).height() <= 25) {
        $(this).addClass('d-none');
      }
    });

    const stickyOffset = $('#nav-sticky').offset().top;
    window.onscroll = function() {
      if (window.pageYOffset > stickyOffset) {
        $('#nav-sticky').addClass('position-fixed').addClass('top-0').removeClass('position-relative');
        $('.sky-left').addClass('sky-left-scroll');
        $('.sky-right').addClass('sky-right-scroll');
      } else {
        $('#nav-sticky').removeClass('position-fixed').removeClass('top-0').addClass('position-relative');
        $('.sky-left').removeClass('sky-left-scroll');
        $('.sky-right').removeClass('sky-right-scroll');
      }
    }
        $('#b1Search').on('submit', function(e) {
      e.preventDefault();
      let param = $('#b1Search input[name="search"]').val();
      param = encodeURIComponent(param);
      param = param.split('%20').join('+');
      window.location.href = '/search/' + param.toLowerCase();
    });
    $('#b1BtnSearch').click(function() {
      if ($('#b1Search input[name="search"]').val() != '') {
        $('#b1Search').submit();
      }
    });

