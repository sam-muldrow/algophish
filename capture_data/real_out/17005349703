<script type="text/javascript">
  let widel = false;
  const idel = document.querySelector('#investor');
  window.addEventListener('scroll', function() {
    const idelpos = idel.getBoundingClientRect();
    if(idelpos.top < window.innerHeight && idelpos.bottom >= 0 && widel == false) {
      writeID();
    }
  });

  function writeID() {
    widel = true;
    const idifr = document.createElement('iframe');
    idifr.setAttribute('src','https://investor.id/widget/investor');
    idifr.setAttribute('height','345');
    idifr.setAttribute('scrolling','no');
    idifr.setAttribute('frameborder','no');
    idifr.setAttribute('class','overflow-hidden w-100 mb-2');
    idel.appendChild(idifr);
  }
</script><div id="jakartaglobe"></div>
<script type="text/javascript">
  let wjgel = false;
  const jgel = document.querySelector('#jakartaglobe');
  window.addEventListener('scroll', function() {
    const jgelpos = jgel.getBoundingClientRect();
    if(jgelpos.top < window.innerHeight && jgelpos.bottom >= 0 && wjgel == false) {
      writeJG();
    }
  });

  function writeJG() {
    wjgel = true;
    const jgifr = document.createElement('iframe');
    jgifr.setAttribute('src','https://jakartaglobe.id/widget/jakartaglobe');
    jgifr.setAttribute('height','345');
    jgifr.setAttribute('scrolling','no');
    jgifr.setAttribute('frameborder','no');
    jgifr.setAttribute('class','overflow-hidden w-100 mb-2');
    jgel.appendChild(jgifr);
  }
</script>

