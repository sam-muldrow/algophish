                            var template = $('#breaking-items').html();
                            Mustache.parse(template);   // optional, speeds up future uses
                            var rendered = Mustache.render(template, data);
                            if(breaking_news_wrapper.data('visible') == 0) {
                                $('.breaking-news-wrapper').slideDown(1000);
                                breaking_news_wrapper.data('visible', 1);
                            } else {
                                clearTimeout(timeoutHandle);
                            }
