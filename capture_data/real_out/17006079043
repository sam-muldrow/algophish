            function breakingInit() {
                $.getJSON( 'https://www.almanar.com.lb/ajax/breaking_news.php', function( data ) {
                    if(data.breaking_news[0] != null) {
                        if(data.last_id != breaking_news_wrapper.data('last_id')) {
