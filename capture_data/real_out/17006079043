                                var tab_buttons = tabs_container.find('.clickable-tab');
                tab_buttons.on('click', function() {
                    var $this = $(this);
                    if($this.data('loaded') == 0) {
                                                var tab_pane_container = $($this.attr('href'));
                        var sports_template = document.getElementById('tab-items-sport_11103').innerHTML;
                        $.getJSON( $this.data('url'), function( data ) {
                            Mustache.parse(sports_template);   // optional, speeds up future uses
