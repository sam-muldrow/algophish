<script>
    document.addEventListener("DOMContentLoaded",() => {
        // Is Sandbox
        var isSandboxMode = "no";
        

        // Section Name
        var sectionName;
        

        if (sectionName == undefined) {
            if (window.location.pathname === '/') {
                sectionName = "Home"
            }
            else {
                var pathname = window.location.pathname.split('/')
                sectionName = pathname.slice(-1)[0]
            }
        }

        // isHome
        var isHome = "no";
        if (window.location.pathname === '/') {
            isHome = "yes"
        }

        window.htlbid = window.htlbid || {};
        htlbid.cmd = htlbid.cmd || [];
        htlbid.cmd.push(function() {
            htlbid.layout('universal'); // Leave as 'universal' or add custom layout
            htlbid.setTargeting("is_testing", isSandboxMode); // Set to "no" for production
            htlbid.setTargeting("is_home", isHome); // Set to "yes" on the homepage
            htlbid.setTargeting("category", sectionName); // dynamically pass page category into this function
            htlbid.setTargeting("post_id", "0000017e-e975-d1e5-a1fe-ed756f1d0000"); // dynamically pass unique post/page id into this function
            htlbid.setTargeting("entry_group",);
            htlbid.setTargeting("entry_id", "");
            htlbid.setTargeting("entry_type", "");
            htlbid.setTargeting("page_type", "home_page");
            htlbid.setTargeting("hub_pages", "");
            htlbid.setTargeting("keywords", "homepage");
        });
    })
</script>
