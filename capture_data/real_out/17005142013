<script>
    (function () {
        var storeKey = "IBICN_DBP_0_INDEX";
        var storeVal = JSON.parse(sessionStorage.getItem(storeKey)) || {};

        // 检测元素
        function checkRect(elId, data) {
            var el = document.querySelector('#' + elId);
            if (checkElementInView(el) && !storeVal[elId]) {
                postTraceData(data);
                storeVal[elId] = true;
                sessionStorage.setItem(storeKey, JSON.stringify(storeVal));
            }
        }

        // 是否曝光
        function checkElementInView(el) {
            var rect = el.getBoundingClientRect();
            return (rect.top >= 0 && rect.left >= 0 && rect.bottom <= (window.innerHeight || document.documentElement.clientHeight) && rect.right <= (window.innerWidth || document.documentElement.clientWidth))
        }

        // 发送数据
        function postTraceData(data) {
            var xhr = new XMLHttpRequest();
            xhr.open('POST', 'https://tongji.ibicn.com/dbp/trace/show.jsp', true);
            // 为表单数据 POST 请求添加所需的 HTTP 头
            xhr.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
            xhr.withCredentials = true; //支持跨域发送cookies
            var formData;
            formData = new FormData();
            formData.append('entityId', data.entityId);
            formData.append('page', data.page);
            formData.append('area', data.id);
            var dataArr = [];
            formData.forEach(function (v, k) {
                dataArr.push(k + '=' + v)
            });

            xhr.onreadystatechange = function (response) {
                if (xhr.readyState == 4 && xhr.status == 200 && xhr.responseText != "") {
                    // console.log('response', response)
                }
            };
            xhr.send(dataArr.join('&'));
        }

        function start() {
            
            checkRect('header', {"elementId":"header","entityId":"0","id":"10001","page":"INDEX","parentId":"0"});
            
            checkRect('floor3', {"elementId":"floor3","entityId":"0","id":"10019","page":"INDEX","parentId":"0"});
            
            checkRect('floor6', {"elementId":"floor6","entityId":"0","id":"10022","page":"INDEX","parentId":"0"});
            
            checkRect('floor9', {"elementId":"floor9","entityId":"0","id":"10025","page":"INDEX","parentId":"0"});
            
            checkRect('floor12', {"elementId":"floor12","entityId":"0","id":"10028","page":"INDEX","parentId":"0"});
            
        }

        // 监听滚动
        window.addEventListener('scroll', function (e) {
            start()
        });

        start()
    })();

    (function () {
        document.querySelectorAll("[data-dbp-anchor]").forEach(function (el) {
            el.addEventListener("click", function () {
                var data = JSON.parse((el.getAttribute("data-dbp-anchor"))) || {};
                data.target = data.target || el.title || el.innerHTML || '';
                data.url = data.url || el.href || '';
                postClickData(data);
            })
        });
        document.querySelector("[data-dbp-header-search]").addEventListener("click", function () {
            var data = JSON.parse((this.getAttribute("data-dbp-header-search"))) || {};
            var target = document.getElementById("head_sou").value || '';
            var entity = document.getElementById("con_from").querySelector("ul.gg a.hover").innerHTML;
            data.target = entity + '{}' + target;
            postClickData(data);
        });

        // 发送数据
        function postClickData(data) {
            var xhr = new XMLHttpRequest();
            xhr.open('POST', 'https://tongji.ibicn.com/dbp/trace/click.jsp', true);
            // 为表单数据 POST 请求添加所需的 HTTP 头
            xhr.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
            xhr.withCredentials = true; //支持跨域发送cookies
            var dataArr = [];
            for (var k in data) {
                dataArr.push(k + '=' + data[k])
            }

            xhr.onreadystatechange = function (response) {
                if (xhr.readyState == 4 && xhr.status == 200 && xhr.responseText != "") {
                    // console.log('response', response)
                }
            };
            xhr.send(dataArr.join('&'));
        }
    })();
</script>


