<script type="text/javascript" src="//file.ibicn.com/www/v4/global/libs/layui-v2.4.3/layui/layui.all.js"></script>
<script>
    $(document).ready(function () {
        $("#complaintMobile").change(function () {
            checkComplaintMobile()
        });
        $("#complaintEmail").change(function () {
            checkComplaintEmail()
        });
        $("#accusationMobile").change(function () {
            checkAccusationMobile()
        });
        $("#accusationEmail").change(function () {
            checkAccusationEmail()
        });
    });

    function submit_complaint() {
        if ($("#complaintTypeId").val() == "") {
            layer.alert("请选择投诉类型！", {icon: 5});
            return false;
        }
        if ($("#complaintTitle").val() == "") {
            layer.alert("请输入投诉内容！", {icon: 5});
            return false;
        }
        if ($("#complaintDescription").val() == "") {
            layer.alert("请输入投诉详情！", {icon: 5});
            return false;
        } else if ($("#complaintDescription").val().length > 250) {
            layer.alert("投诉详情长度不能超过250！", {icon: 5});
            return false;
        }
        if ($("#complaintName").val() == "") {
            layer.alert("请输入投诉人姓名！", {icon: 5});
            return false;
        }
        if ($("#complaintMobile").val() == '') {
            layer.alert("请输入联系电话！", {icon: 5});
            return false;
        }
        if ($("#complaintEmail").val() == '') {
            layer.alert("请输入邮箱！", {icon: 5});
            return false;
        }
        if ($("#complaint_code").val() == "") {
            layer.alert("请输入验证码！", {icon: 5});
            return false;
        }
        if (!checkComplaintMobile()) {
            return false;
        }
        if (!checkComplaintEmail()) {
            return false;
        }

        $.ajax({
            type: "post",
            url: 'https://api.ibicn.com/ibicn/feed/complaint.jsp',
            xhrFields: {
                withCredentials: true
            },
            data: $("#complaintForm").serialize(),
            dataType: 'json',
            success: function (res) {
                if (res.code == 0) {
                    alert(res.message);
                    $(".global_popup").hide();
                    $(".global_mask").hide();
                    $("#complaintForm")[0].reset();
                } else {
                    alert(res.message);
                }
            },
            error: function (err) {
                alert('系统错误，请稍候重试！');
            }
        });
        return false;
    }

    function submit_accusation() {
        if ($("#accusationTypeId").val() == "") {
            layer.alert("请选择举报类型！", {icon: 5});
            return false;
        }
        if ($("#accusationTitle").val() == "") {
            layer.alert("请输入举报内容！", {icon: 5});
            return false;
        }
        if ($("#accusationDescription").val() == "") {
            layer.alert("请输入举报详情！", {icon: 5});
            return false;
        } else if ($("#accusationDescription").val().length > 250) {
            layer.alert("举报详情长度不能超过250！", {icon: 5});
            return false;
        }
        if ($("#accusationName").val() == "") {
            layer.alert("请输入举报人姓名！", {icon: 5});
            return false;
        }
        if ($("#accusationMobile").val() == '') {
            layer.alert("请输入联系电话！", {icon: 5});
            return false;
        }
        if ($("#accusationEmail").val() == '') {
            layer.alert("请输入邮箱！", {icon: 5});
            return false;
        }
        if ($("#accusation_code").val() == "") {
            layer.alert("请输入验证码！", {icon: 5});
            return false;
        }
        if (!checkAccusationMobile()) {
            return false;
        }
        if (!checkAccusationEmail()) {
            return false;
        }

        $.ajax({
            type: "post",
            url: 'https://api.ibicn.com/ibicn/feed/accusation.jsp',
            xhrFields: {
                withCredentials: true
            },
            data: $("#accusationForm").serialize(),
            dataType: 'json',
            success: function (res) {
                if (res.code == 0) {
                    alert(res.message);
                    $(".global_popup").hide();
                    $(".global_mask").hide();
                    $("#accusationForm")[0].reset();
                } else {
                    alert(res.message);
                }
            },
            error: function (err) {
                alert('系统错误，请稍候重试！');
            }
        });
        return false;
    }

    function checkComplaintMobile() {
        var check = /^1[\d]{10}$/;
        if (check.test($("#complaintMobile").val())) {
            return true;
        } else {
            layer.alert("手机号格式错误！", {icon: 5});
            return false;
        }
    }

    function checkComplaintEmail() {
        var check = /\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*/;
        if (check.test($("#complaintEmail").val())) {
            return true;
        } else {
            layer.alert("邮箱格式错误！", {icon: 5});
            return false;
        }
    }

    function checkAccusationMobile() {
        var check = /^1[\d]{10}$/;
        if (check.test($("#accusationMobile").val())) {
            return true;
        } else {
            layer.alert("手机号格式错误！", {icon: 5});
            return false;
        }
    }

    function checkAccusationEmail() {
        var check = /\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*/;
        if (check.test($("#accusationEmail").val())) {
            return true;
        } else {
            layer.alert("邮箱格式错误！", {icon: 5});
            return false;
        }
    }
</script>
<script src="//file.ibicn.com/www/v4/global/js/jquery.cookie.js"></script>
<script type="text/javascript">
    $(function(){
        $("#con_from .gg li a").click(function(){
            $("#con_from").attr("action",$(this).parent().attr("hf"));
            $("#con_from .gg li a").removeClass("hover");
            $(this).addClass("hover");
        });
    });

    $(function(){
        $('#btnCloseBodytopHappyyear').click(function() {
            $('.body_top_wrap').removeClass('body_top_wrap');
            $(this).remove();
            $('#body_top_ggw').css('display','block');
        });

        var html='<span style="position: absolute;font-size: 10px;color: #fff;background: rgba(0,0,0,0.6);line-height: 1;padding: 2px;right: 0; bottom: 0;"><a href="https://about.ibicn.com/market.html" style="color:#fff" target="_blank">广告</a></span>';
        $("iframe[onload^='___baidu_union_ca']").parent().parent().css({position:"relative"});
        $("iframe[onload^='___baidu_union_ca']").parent().parent().append(html)
    });

    $(function(){
        if(!$.cookie('useCookie')){
            $('.use-cookie-wrap .inner').animate({height:'80px'}, 'slow', 'swing');
        }
    });
    function doUseCookie(){
        $('.use-cookie-wrap .inner').animate({height:'0'}, 'fast', 'swing');
        $.cookie('useCookie', 'true', {expires: 365});
    }


    $(function(){
        var beginDate = '2020-01-20';
        var closeDate = '2020-03-31';
        if(closeDate===''){
            closeDate = new Date(beginDate).getTime() + 86399999;
        }
        var beginDateTimestamp = new Date(beginDate).getTime();
        var closeDateTimestamp = new Date(closeDate).getTime();
        var nowTimestamp = new Date().getTime();
        if(nowTimestamp>=beginDateTimestamp && nowTimestamp<=closeDateTimestamp){
            $('.header_top_banner').show();
        }else{
            $('.header_top_banner').hide();
        }
    });

    $(function () {
        $(".global_feedback_btn").on("click", function () {
            $(".global_mask").show();
            $(".global_popup").show();
        });
        $(".global_mask").on("click", function () {
            $(this).hide();
            $(".global_popup").hide();
        });
        $(".global_popup_tab .global_popup_tab_item").on("click", function () {
            var index = $(this).index();

            $(".global_popup_tab .global_popup_tab_item").removeClass('active').eq(index).addClass('active');
            $(".global_popup .global_popup_body_item ").hide().eq(index).show();
        })
    });

    $(function () {
        $('#header .header_bottom .header_bottom_right .input').click("click", function () {
            var entityId = $("#header .header_bottom .header_bottom_right .gg a[class='hover']").attr('id');
            window.open("https://so.ibicn.com/?ac=" + entityId);
            return false
        });
    });
</script>
