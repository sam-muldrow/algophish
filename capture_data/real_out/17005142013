<script>
    function showComplaintCode(){
        var obj = document.getElementById('complaintCaptchaImg');
        if(obj.style.display=='none'){
            obj.src='https://api.ibicn.com/include/func/captcha.jsp?rand='+Math.random();
            obj.style.display='';
        }
    }

    function showAccusationCode(){
        var obj = document.getElementById('accusationCaptchaImg');
        if(obj.style.display=='none'){
            obj.src='https://api.ibicn.com/include/func/captcha.jsp?rand='+Math.random();
            obj.style.display='';
        }
    }
</script>
