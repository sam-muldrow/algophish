    justify-content: center;
}

.placeholder-text::before {
    content: "Advertisement";
    text-align: center;
    width: 200px;
    font-family: Open Sans;
    font-style: normal;
    font-weight: normal;
    font-size: 10px;
    line-height: 20px;
    color: #999999;
}
</style>



