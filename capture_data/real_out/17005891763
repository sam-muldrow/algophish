	const locale = matches && matches[1];
	const getLink = l => document.querySelector("link[rel=alternate][data-redir~='"+l+"' i]")
	const link = locale && (getLink(locale) || getLink('en-'+locale.split("-")[1]) ||
		getLink(locale.split("-")[0]) || getLink('en'));
	const redirectTarget = link && !link.hasAttribute('data-nr') && link.href;
	if (redirectTarget) {
		const location = document.location;
		if (sessionStorage) {
			sessionStorage.setItem('ni-analytics.redirecting-page', location.href);
			sessionStorage.setItem('ni-analytics.original-referrer', document.referrer);
		}
		location.replace(new URL(redirectTarget).pathname + location.search + location.hash);
	}
}
