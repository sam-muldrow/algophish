									<span></span>
					<p class="campaignDateText">
						現在の価格有効期限：2023年11月23日(木) 17時まで<br class="sp-only">(<span class="js-trial3-countdown-box"></span>)
					</p>
					<script>
					window.addEventListener('load', function() {
						var end = 1700726400000;
						var $count = $('.js-trial3-countdown-box');
						(trial3Countdown = function(){
							var _d = new Date();
							var now = _d.getTime();
							if(end > now) {
								var time_text = '残り';
								var diff = end - now;
								var num = 24 * 60 * 60 * 1000;
								var day = Math.floor(diff / num);
								if(day) time_text += day + '日';
								diff = diff - (day * num);
								var num = 60 * 60 * 1000;
								var hour = Math.floor(diff / num);
								if(hour || day) time_text += hour + '時間';
								diff = diff - (hour * num);
								var num = 60 * 1000;
								var minute = Math.floor(diff / num);
								if(day || hour || minute) time_text += (minute < 10 && (day || hour)) ? '0' + minute + '分' : minute + '分';
								diff = diff - (minute * num);
								var num = 1000;
								var second = Math.floor(diff / num);
								time_text += (second < 10	&& (day || hour || minute)) ? '0' + second + '秒' : second + '秒';
								diff = diff - (second * num);
								var csecond = Math.floor(diff / 10);
								time_text += (csecond < 10) ? '0' + csecond : csecond;
								$count.text(time_text);
								setTimeout("trial3Countdown()", 10);
							} else {
								$count.text('終了しました。');
							}
						})();
					});
					</script>
				
