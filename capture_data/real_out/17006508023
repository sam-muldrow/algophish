				<script>
				window.addEventListener('load', function() {
					var swiper_selector = '#topMvTldSlider';
					var $tld_tab = $('.js-tld-tab');
					var $tld_list = $('.js-tld-list');
					$(swiper_selector).after('<div id="topMvTldSliderNavi" class="popularTLDSlider_navi" style="display: none;"><span class="popularTLDSlider_prev" id="topMvTldSliderNaviPrev"></span><span class="popularTLDSlider_next" id="topMvTldSliderNaviNext"></span></div>');
					
					var swiper = new Swiper(swiper_selector, {
						init: false,
						loop: true,
						navigation: {
							nextEl: '#topMvTldSliderNaviNext',
							prevEl: '#topMvTldSliderNaviPrev'
						}
					});
					swiper.on('init', function() {
						if(swiper.slides.length <= 1) {
							swiper.destroy();
						} else {
							$('#topMvTldSliderNavi').show();
						}
					});

					$tld_tab.on('click', function() {
						$tld_tab.removeClass('is-active');
						$(this).addClass('is-active');
						$tld_list.toggleClass('tld-update');
					});

					swiper.init();
				});
				</script>
			</div>
			
