<script>
	function validateEmail(email) {
		var re = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i;
		return re.test(email);
	}
	
	$(document).ready(function() {
		var month = [], day = [], year = [];
		var m;
		var d;
		for (var i = 1; i <= 12; i++) {
			month.push(i);
		}
		for (var i = 1; i <= 31; i++) {
			day.push(i);
		}
		for (var i = 1900; i <= (new Date().getFullYear() - 18); i++) {
			year.push(i);
		}
		$.each(day, function(index, d) {
			$("#day").append("<option value="+d+">" + d + "</option>");
		});
		$.each(month, function(index, m) {
			$("#month").append("<option value="+m+">" + m + "</option>");
		});
		$.each(year, function(index, y) {
			$("#year").append("<option value="+y+">" + y + "</option>");
		});
		
		$(".pre-inscription").on("click", function() {
			var name = $("#name").val();
			var firstname = $("#firstname").val();
			var day = $("#day").val();
			var month = $("#month").val();
			var year = $("#year").val();
			var email = $("#email").val();
			var civility = $('input[name=civility]:checked').val();
			var confirm = $('input[name=confirm]:checked').val();
			
			var date = null;
			if (day != "Jour" && month != "Mois" && year != "Année") {
				date = day+"/"+month+"/"+year;
			}

			var url = "https://www.genybet.fr/affp?pf=match&affid=geny.com&siteid=geny.com&lid=q937ZeAZN9f8X2V97ayeT2ve3VhzHy9D&u=H&v=2&t=A&campid=static&srv=Enr"
			if (email != undefined && email != null && validateEmail(email) && confirm == "on") {
				url += "?email="+email;
				
				if (name != undefined && name != null && name != "") {
					url+= "&name="+name;
				}
				if (firstname != undefined && firstname != null && firstname != "") {
					url+= "&firstname="+firstname;
				}
				if (date != null) {
					url+= "&date="+date;
				}
				if (civility != undefined && civility != null && civility != "") {
					url+= "&civility="+civility;
				}
			}
			
			window.open(url, "_blank");
		});
	});
</script>
</div>
