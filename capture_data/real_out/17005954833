            <script type="text/javascript">
                var div = document.getElementById('telex-content');
                var wrapper = document.getElementById('telex');
                var num = 6 // nombre de news
                function successHandler(o) {
                    var root = o.responseXML.documentElement;
                    var dates = root.getElementsByTagName("pubDate");
                    var titles = root.getElementsByTagName("title");
                    var links = root.getElementsByTagName("link");
                    for (var i = 0; i < num; i++) {
                        if (dates[i + 2]) {
                            wrapper.style.display = "block";
                        }
                        var lnk = document.createElement('a');
                        lnk.href = links[i + 2].firstChild.nodeValue;
                        var d = new Date(dates[i + 2].firstChild.nodeValue);
                        var hr = d.getHours();
                        var mn = d.getMinutes();
                        hr = (hr < 10) ? '0' + hr : hr;
                        mn = (mn < 10) ? '0' + mn : mn;
                        var x = document.createTextNode(hr + ':' + mn + ' ' + titles[i + 2].firstChild.nodeValue);
                        lnk.appendChild(x);
                        div.appendChild(lnk);
                        if (i < num - 1) {
                            var br = document.createElement('div');
                            div.appendChild(br);
                        }
                    }
                }

                function failureHandler(o) {
                    div.innerHTML = o.status + " " + o.statusText;
                }

                YAHOO.util.Connect.asyncRequest('GET', 'https://rss.geny.com/web/media/rss.xml', {
                    success: successHandler,
                    failure: failureHandler
                });
            </script>

