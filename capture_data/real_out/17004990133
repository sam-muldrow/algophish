    <script src="https://use.typekit.net/uhb4nat.js"></script>
<script>
  (function () {
    Typekit.load();

    var loader = new tk.FontLoader();
    var fonts = null || [];
    var group = [];
    fonts.forEach(function (font) {
      loader.loadFont(font.css_name, font.opaque_id, font.fvd, font.unicode_range, font.feature_settings, font.dynamic);
      if (tk.GlobalFontEvents && typeof tk.GlobalFontEvents.isFontLoaded == 'function') {
        group.push(tk.GlobalFontEvents.isFontLoaded(font.css_name, font.fvd));
      }
    });

    // TODO: This is only used for the Twitter cards. Once
    // the twitter cards are rewritten, we can remove this.
    // Only call this if Knockout is present.
    if (typeof(ko) !== "undefined") {
      ko.computed(function () {
        return group.every(function (isLoaded) {
          return isLoaded() === true;
        });
      }).subscribe(function (loaded) {
        if (loaded && tk.GlobalFontEvents.active) {
          tk.GlobalFontEvents.active();
        }
      });
    }
  }());
</script>


