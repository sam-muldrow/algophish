	    <script type="text/javascript">

        if (typeof calculateHeight !== "function") {
            function calculateHeight(index, range, media_admin_height, key, loaded) {
                console.log(index, range);
                let media_container = $('[data-container="media-listing"]');
                let indexes = [index];

                var fl = 1;
                while (fl <= range) {
                    indexes.push(index + fl);
                    fl++;
                }

                var height = 0;
                let media_top_height = 134;
                let media_bottom_height = 137;

                let media_list = media_container.find('.media-element');

                media_list.each(function (i) {
                    if (indexes.indexOf(i) > -1) {
                        if (loaded) {
                            height += $(this).height();
                        } else {
                            let img_height = parseInt($(this).data('height'));
                            if (img_height > 0) {
                                height += img_height + media_top_height + media_bottom_height + media_admin_height;
                            } else {
                                height += $(this).find('.figure').height();
                            }
                        }
                    }
                })

                console.log(height)

                let box_margin = 30 * (indexes.length - 1);

                if (box_margin > 0) {
                    height += box_margin;
                }

                $('.media-rectangle-right-wrapper-' + key).css('height', height + 'px');
                console.log(height)
            }
        }

        window.addEventListener('DOMContentLoaded', (event) => {
            calculateHeight(1, 0, 0, 2080, false);

            let ad = $('[data-id="media-rectangle-right-2080"]');
            ad.show();

            let margin = 80;
            ad.stick_in_parent({
                offset_top: margin
            });
        });
    </script>
