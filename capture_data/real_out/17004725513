function getDataToTrack(){
return {
cc: HOME.countryCode,
ch: HOME.channel,
pr: HOME.currentProduct,
fl: HOME.upaTrackerData.fl,
url: HOME.upaTrackerData.url,
ab: HOME.abString || "",
userid: HOME.upaTrackerData.userid,
homeType: HOME.homeType,
travelHistory: HOME.homeTopicData.travel_history,
tripStatus: HOME.homeTopicData.trip_status,
companyId: HOME.upaTrackerData.company_id,
device: HOME.device
}
}
window.addEventListener('DOMContentLoaded', function(event) {
window.customTimmings.trackMainFeature(window.getDataToTrack());
if (window.newrelic) {
window.newrelic.addPageAction("times", {
pageLoaded: window.performance.now() / 1000,
});
}
function _trackWebVital(metric) {
// CLS valor custom entre 0.1 y 0.25 (cambio repentino de posicion/tamaño de elementos)
// FID,FCP,LCP y TTFB vienen en milisegundos, con decimales
var webVitalsMetric = {
metricName: metric.name,
// customTimmsings uses integers
time: metric.name === 'CLS'? Math.round(metric.value * 1000) : Math.round(metric.value)
};
window.customTimmings.trackTimmings([webVitalsMetric], window.getDataToTrack(), HOME.upaTrackerData.xuowHeader, HOME.upaTrackerData.pageview_id);
}
webVitals.getCLS((metric) => _trackWebVital(metric),{reportAllChanges:true});
webVitals.getFID((metric) => _trackWebVital(metric),{reportAllChanges:true});
webVitals.getFCP((metric) => _trackWebVital(metric),{reportAllChanges:true});
webVitals.getLCP((metric) => _trackWebVital(metric),{reportAllChanges:true});
webVitals.getTTFB((metric) => _trackWebVital(metric),{reportAllChanges:true});
});
window.addEventListener('beforeunload', () => {
// TODO add condition to track bounce
window.customTimmings.trackBounce();
});
</script>
