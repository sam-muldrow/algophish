<script>
function generateUuidv4() {
return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function(c) {
var r = Math.random() * 16 | 0, v = c == 'x' ? r : (r & 0x3 | 0x8);
return v.toString(16);
});
}
function generatePageviewIdForDefaultHome() {
if (!HOME.upaTrackerData.pageview_id && !HOME.upaAsyncTrackerData.pageview_id) {
var newPageviewID = HOME.pageviewIdTemplate.replace('%s', generateUuidv4);
HOME.upaTrackerData.pageview_id = newPageviewID;
HOME.upaAsyncTrackerData.pageview_id = newPageviewID;
}
}
</script>
