							$( '.comment-count' ).each( function() {
								var index = null;
								for ( var i = 0; i < counts.data.length; i++ ) {
									if ( parseInt( counts.data[ i ].articleId ) === $( this ).data( 'postid' ) ) {
										index = i;
										break;
									}
								}

								if ( ! index ) {
									$( this ).parents( '.comment-bubble' ).remove();
									return;
								}

								var count = counts.data[ index ].commentCount;
								counts.data[ index ].emotes.forEach( function( emote ) {
									count += emote;
								} );

								if ( count > 0 ) {
									var output = '';
									if ( 1 === count ) {
										output = '1 Reaction';
									} else {
										output = parseInt( count ) + ' Reactions';
									}
									$( this ).html( output );
								} else {
									$( this ).parents( '.comment-bubble' ).remove();
								}
							});
						})(jQuery);
				} );

