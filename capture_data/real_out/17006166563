.ss-create-list .ss-left, .ss-create-list .ss-right { position: absolute; font-size: 40px;display: none;height:calc(100% - 20px); align-items: center; padding:1px; z-index: 1001;}
.ss-create-list .ss-left{-webkit-transform: rotate(180deg);transform: rotate(180deg);left:0;box-shadow: -10px 0px 11px -4px rgb(0 0 0 / 9%);}
.ss-create-list .ss-right{-webkit-transform: rotate(0deg);transform: rotate(0deg);right:0;box-shadow: -10px 0px 11px -4px rgb(0 0 0 / 9%);}
@media (max-width:1200px) {
  .create-secion2 .ss-create-list li{min-width: 130px;padding: 10px; margin: 10px;}
}
@media (max-width:992px) {
  .create-secion2 .ss-create-list{overflow-x:scroll;margin: 0 33px;}
  .create-secion2 .ss-create-list::-webkit-scrollbar{width:0px;}
  .ss-create-list .ss-left, .ss-create-list .ss-right{display:flex;}
  .create-secion2 .ss-create-list li{padding: 10px 2px;font-size:12px;}
  .create-secion2 .ss-create-list img{max-width: 66%;}
}
@keyframes pulse {
  0% {transform: scale(1, 1);}
  50% {transform: scale(1.1);}
  100% {transform: scale(1, 1);}
}
