      document.body.appendChild(script2);
    }
	
    window.addEventListener('load', (event) => {
      setTimeout(getcommonMethod(), 3000);
    });
    function getcommonMethod()
    {
      scripts1();
	  	  scripts2();
	        WindowLoad();
      windowOnScroll();
      popup();
	  setTimeout(nnGTPScript(), 2000);
      setTimeout(nnGTMScript(), 2000);
