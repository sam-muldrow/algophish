      script1.src = 'https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js';
      script1.async = true;
      script1.onload = () => {
      };
      script1.onerror = () => {
        console.log('Error occurred while loading script1');
      };
      document.body.appendChild(script1);
    }
    function scripts2()
    {
      const script2 = document.createElement('script');
      script2.src = 'https://static.newsnationtv.com/nntv-web/js/v1/poll-widget.min.js';
      script2.async = true;
      script2.onload = () => {
