    $("#searchBtn").click(function () {
        var q = $('#searchTitle').val().replace("?", "").replace("ï¼", "");
        var id = $('input[name="nsid"]').val();
        if (q == '') {
            return false;
        }
        if (id == 1) {
            window.open("http://s.chuandong.com/cse/search?nsid=1&q=" + q + "&s=2844918584110574840");
        }
        if (id == 2) {
            window.open("/company/list_q" + q + ".html");
        }
        if (id == 3) {
            window.open("/news/search_q" + q + ".html");
        }
        if (id == 4) {
            window.open("/product/list_q" + q + ".html");
        }
        if (id == 5) {
            window.open("/tech/list_q" + q + ".html");
        }
        if (id == 6) {
            window.open("/biz/list_st1_q" + q + ".html");
        }
        if (id == 7) {
            window.open("/brand/search_k" + q + ".html");
        }
        if (id == 8) {
            window.open("/exhibition/list_k" + q + ".html");
        }
        if (id == 9) {
            window.open("/topic/list_q" + q + ".html");
        }
        if (id == 10) {
            window.open("/video/list_q" + q + ".html");
        }
        if (id == 11) {
            window.open("/download/list_q" + q + ".html");
        }
        if (id == 12) {
            window.open("http://s.chuandong.com/cse/search?q=" + q + "&s=2844918584110574840&nsid=7");
        }
    });
    $('#searchTitle').on('keydown', function (event) {
        if (event.keyCode == 13) {
            var q = $('#searchTitle').val().replace("?", "").replace("ï¼", "");
            var id = $('input[name="nsid"]').val();
            if (q == '') {
                return false;
            }
            if (id == 1) {
                window.open("http://s.chuandong.com/cse/search?nsid=1&q=" + q + "&s=2844918584110574840");
            }
            if (id == 2) {
                window.open("/company/list_q" + q + ".html");
            }
            if (id == 3) {
                window.open("/news/search_q" + q + ".html");
            }
            if (id == 4) {
                window.open("/product/list_q" + q + ".html");
            }
            if (id == 5) {
                window.open("/tech/list_q" + q + ".html");
            }
            if (id == 6) {
                window.open("/biz/list_st1_q" + q + ".html");
            }
            if (id == 7) {
                window.open("/brand/search_k" + q + ".html");
            }
            if (id == 8) {
                window.open("/exhibition/list_k" + q + ".html");
            }
            if (id == 9) {
                window.open("/topic/list_q" + q + ".html");
            }
            if (id == 10) {
                window.open("/video/list_q" + q + ".html");
            }
            if (id == 11) {
                window.open("/download/list_q" + q + ".html");
            }
            if (id == 12) {
                window.open("http://s.chuandong.com/cse/search?q=" + q + "&s=2844918584110574840&nsid=7");
            }
        }
    });
    var barDop = $("#cheader").offset().top;
    var barDop_1 = $("#header").offset().top;
    $(window).scroll(function () {
        // è·åæµè§å¨çªå£çé¡¶é¨é«åº¦
        var scrollTop = $(window).scrollTop();
        var scrollTop_1 = $(window).scrollTop();

        // å¤æ­æ»å¨æ¡ç¦»æµè§å¨çªå£é¡¶é¨çè·ç¦»ï¼å®ç°æ·¡å¥æ·¡åºçç¹æåè½
        if (scrollTop_1 >= barDop_1 + 100) {
            $("#header").css({ "width": "100%", "position": "fixed", "top": 0, "z-index": "9999999" });
        } else {
            $("#header").removeAttr("style");
        }

        if (scrollTop >= barDop + 100) {
            $("#cheader").css({ "width": "100%", "position": "fixed", "top": "28px", "z-index": "9999999", "background": "#fef7f8" });
        } else {
            $("#cheader").removeAttr("style");
        }
    });

