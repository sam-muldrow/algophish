    $(window).scroll(function () {
        var winScrollHeight = $(window).scrollTop();
        var winHeight = $(window).height();
        for (var i = 0; i < $('.cfloatNav').find('li.li-nav').length; i++) {
            //console.log('dsadsa', i)
            var itemOffsetTop = $('.main-part-' + i + ' .lab').offset().top;
            var itemOuterHeight = $('.main-part-' + i + ' .lab').outerHeight(true);

            if (!(winScrollHeight > itemOffsetTop + itemOuterHeight) && !(winScrollHeight < itemOffsetTop - winHeight)) {
                //console.log("åºç°äº")
                $('.cfloatNav').find('li').eq(i).find('a').addClass('bg-color')
            } else {
                //console.log("æ¶å¤±äº")
                $('.cfloatNav').find('li').eq(i).find('a').removeClass('bg-color')
            }

        }
    });


