<script type="application/javascript">
	$(document).ready(function() {
		var drmenu = $('#folder_drmenu');

		// клик по шапке списка
		drmenu.find('.drmenu-single').on('click', function() {
			if (drmenu.hasClass('activated')){
				drmenu.find('.drmenu-drop').hide();
				drmenu.removeClass('activated');
			} else{
				drmenu.find('.drmenu-drop').show();
				drmenu.addClass('activated');
			}
		});

		// клик по элементам списка
		drmenu.on('click', 'li', function() {

//			if ($(this).hasClass('item')){
				// показываем выбранный пункт и ззаписываем его  в инпут
				drmenu.find('#folder_id').val($(this).attr('id'));
				drmenu.find('.drmenu-single span').html($(this).attr('data-name'));

				// отмечаем выбранный пункт
				drmenu.find('li').removeClass('checked');
				$(this).addClass('checked');

				drmenu.find('.drmenu-drop').hide();
				drmenu.removeClass('activated');
//			}
		});

		// создание новой папки
		drmenu.find('#add-folder-btn').on('click', function() {
			var folderName = $('#add-folder-name').val();
			if (folderName == ''){
				$('#add-folder-name').addClass('err-text');
			} else {
				$('.green-plus-loading').show();
				$('#add-folder-btn').hide();

				var $post = $.ajax({
					url: '/user/folders/ajaxEdit',
					type: 'POST',
					dataType: 'json',
					data: {
						oper : "add",
						parent_id :  null,
						position : null,
						can_be_copied: 0,
						name : folderName
					}
				});
				$post.done(function(r){
					if(r.id) {
						//$(data.rslt.obj).attr('data-id', r.id);
						drmenu.find('ul.drmenu-results').append('<li class = "item" id="'+r.id+'" data-name="'+folderName+'">'+folderName+'<span class="new-mark">new</span></li>');
						$('#add-folder-name').removeClass('err-text');
						$('.green-plus-loading').hide();
						$('#add-folder-btn').show();
					} else {
						alert('Server Error');
					}
				});
				$post.fail(function(){
					alert('Server Error');
				});
			}
		});
	});
</script>


