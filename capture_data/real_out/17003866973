<script type="text/javascript">
  let bvSetup = new Promise((resolve, reject) => {
    pageLoadSeconds = 0;
    const bvRatingsContainer = document.getElementById('bvRatingsContainer').children[0];
    let readyStateInterval = setInterval(() => {      
      if (document.readyState === "complete" && bvRatingsContainer.children[0] !== undefined) {     
        resolve("Page setup completed in time for BV setup!");
        clearInterval(readyStateInterval);
      } else {
        pageLoadSeconds++;
        if (pageLoadSeconds === 50) {
          reject("Page failed to load in time for BV setup!");
          clearInterval(readyStateInterval);
        } 
      }
    }, 500)
  });
  bvSetup.then((message) => {   
    const bvStars = document.querySelector(".bv_stars_button_container").innerHTML;
    const mintPencilBvStars = document.getElementById('mintPencilBvStars');
    mintPencilBvStars.innerHTML = bvStars;    
  }).catch((error) => { 
    const mintPencilBvStars = document.getElementById('mintPencilBvStars');
    mintPencilBvStars.style.display = 'none';
    console.log(error);
  }) 
</script></div>
              </div>
            </div>
          
        </li>
        
      </ul>
      
