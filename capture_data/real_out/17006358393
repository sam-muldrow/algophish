    display: inline;
    white-space: normal;
}
.ellipsis-box p:not(:first-of-type):before, .ellipsis-box p:after {
    content: '\a';
    white-space: pre;
}
.ellipsis-box button:after {
    content: attr(data-label-more);
}
.ellipsis-box.expanded .ellipsis-box-inner {
    display: block;
}
.ellipsis-box.expanded button:after {
    content: attr(data-label-less);
}
