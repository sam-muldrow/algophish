<script>
    var ellipBoxes = document.querySelectorAll('[data-ellipsis]');
    for (var i = 0; i < ellipBoxes.length; i++) {
        var ellipBox = ellipBoxes[i];
        var actionBtn = ellipBox.querySelector('button');
        actionBtn.setAttribute("aria-expanded","false")
        actionBtn.addEventListener('click', function(e) {
            ellipBox.classList.toggle('expanded');
            actionBtn.setAttribute("aria-expanded", ellipBox.classList.contains('expanded'));
        });
    }
</script>
