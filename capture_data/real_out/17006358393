<script>
      document.addEventListener('DOMContentLoaded', function(event) { 
        // Giving message line break for mobile
        var gvMsg = document.querySelector(".gv-msg .c-rich-text__content h2");
        if (gvMsg) {
          var html = gvMsg.innerHTML;
          var newHtml = html.replace(',',',<br class="u-hidden@md-and-up"/>');
          gvMsg.innerHTML = newHtml;
        };
        
        // Animate link class assignment
        var ccpaLinks = document.querySelectorAll(".perks a");
        for (var i = 0; i < ccpaLinks.length; i++) {
          var cLink = ccpaLinks[i].classList.add("link-animate");
        };
      })

      //ADA script
      setTimeout(() => {document.querySelectorAll('[id ^= "pr-category-snippets"]').forEach( el => {el.removeAttribute('aria-labelledby')})}, 1000);

      //ThredUP script
      document.addEventListener("DOMContentLoaded", (event) => {
const storyLinks = document.querySelectorAll(".story-slider a[href='/#']");
const fanFaves = document.querySelector("[data-cid='redesign-footer-fan-favorites-links']");
const fanFavesParent = fanFaves.parentElement;
const fanFavesButton = fanFaves.querySelector("button");
const elPreloved = document.querySelector("#footer-preloved");
if (storyLinks.length && elPreloved) {
  storyLinks.forEach((el) => {
    el.addEventListener("click", (e) => {
      e.preventDefault();
      if (!fanFavesParent.classList.contains("is--expanded")) {
        fanFavesButton.click();
      }
      elPreloved.click();
      const elActive = el.closest(".c-content-tile");
      if (elActive) {
        setTimeout(() => {elActive.classList.remove("is--active");}, 0);
      }
    }, false);
  });
}
});
      </script>
