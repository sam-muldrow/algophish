        <div id="auth-sync-session"></div>
        <script>
            function cookiesEnabled() {
                if (navigator.cookieEnabled) {
                    return true;
                }
                document.cookie = "cookiesEnabled=1";
                var cookieExists = document.cookie.indexOf("cookiesEnabled=") != -1;
                // Delete cookie
                document.cookie = "cookiesEnabled=1; expires=Thu, 01-Jan-1970 00:00:01 GMT";
                return cookieExists;
            }

            function addIframes() {
                if (cookiesEnabled()) {
                    var syncDiv = document.getElementById('auth-sync-session');
                    syncDiv.innerHTML = "<iframe style='display: none' src='https://accounts.ocado.com/auth-service/sso/check-session' id='js-opframe'></iframe>" +
                            "<iframe style='display: none' src='/webshop/sso/client-check-session.go?referrer=%2Fwebshop%2FstartWebshop.do'></iframe>";
                }
            }

            addIframes();
        </script>
    



    
