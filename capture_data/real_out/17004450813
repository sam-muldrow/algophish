			var isIE11 = false;
			var userAgent = navigator.userAgent;
			var appName = navigator.appName;
			if (appName == "Microsoft Internet Explorer") {
				var re = new RegExp("MSIE ([0-9]{1,}[\\.0-9]{0,})");
