  /* Read more Buttons*/
  const moreButton = function(ele, btnLabel, classname){
    const parentElement = document.querySelector(ele);
    if(! classname){
      classname = 'btn';
    }
    if(parentElement){
      const headlines = parentElement.querySelectorAll('.widget__headline-text');
      if(headlines.length > 0){
        headlines.forEach(hlElem=>{
          const elBody = hlElem.closest('.widget__body');
          const elUrl = hlElem.href;
          const elLabel = btnLabel || 'Read More';
          const elText = hlElem.innerText;
          const elBtn = `<a class="${classname}" aria-label="${elLabel} about ${elText}" href="${elUrl}">${elLabel}</a>`;
          const hasbtn = elBody.querySelector('.btn:last-child');

          if(! hasbtn ){
            elBody.insertAdjacentHTML( 'beforeend', elBtn );
          }
        });
      }
    }
  }
  const copylinkBtn = function(parentEle){
    const parentElement = document.querySelector(parentEle);
    if(parentElement){
      const headlines = parentElement.querySelectorAll('.widget__headline-text');
      if(headlines.length > 0){
        headlines.forEach(hlElem=>{
          const elBody = hlElem.closest('.widget__body');
          const elUrl = hlElem.href;
          const copyLink = `<a href="javascript:;" data-clipboard-text="${elUrl}" class="share-copy" aria-label="Copy article link address" target="_self" rel="noopener noreferrer"><i aria-hidden="true" class="icons-share fa fa-link"></i></a>`;
          const sharingBtns = elBody.querySelector('.widget__shares');
          if(! sharingBtns.querySelector('.share-copy')){
            sharingBtns.insertAdjacentHTML('beforeend',copyLink );
          }
        });
      }
    }
  }

