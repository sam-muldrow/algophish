  var forceMobileSiteOnClick = function($link, path) {
    $link.click(function(event) {
      event.preventDefault();
      DailyKos.viewMobileSwitch.viewMobileSite(path);
    });
  }

  forceMobileSiteOnClick($("#mobile_login_link"), "/login");
  forceMobileSiteOnClick($("#mobile_signup_link"), "/users/signup");
