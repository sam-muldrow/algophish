<script type='text/javascript'>
  (function() {
    var _sf_startpt=(new Date()).getTime();
    var _sf_async_config = window._sf_async_config = (window._sf_async_config || {});
    _sf_async_config.uid = 2709;
    _sf_async_config.domain = String('dailykos.com');
    _sf_async_config.flickerControl = false;
    _sf_async_config.useCanonical = true;
    _sf_async_config.useCanonicalDomain = true;

    if(String("").toLowerCase() === "true") {
      _sf_async_config.sections = 'dk-action';
      _sf_async_config.authors = 'dk-activism';
    } else {
      if (typeof DailyKos.currentStoryID != 'undefined') {
        _sf_async_config.sections = DailyKos.currentStoryTags.join(',');
        _sf_async_config.authors = DailyKos.currentStoryAuthor;
      }
    }

    function loadChartbeat() {
      var e = document.createElement('script');
      var n = document.getElementsByTagName('script')[0];
      e.type = 'text/javascript';
      e.async = true;
      e.src = '//static.chartbeat.com/js/chartbeat.js';
      n.parentNode.insertBefore(e, n);
    }

    loadChartbeat();
  })();
</script>


    <div class="page-top">
      <script type="text/javascript">
  let browserOutdated = false;
  let dismissedAt = null;

  window.addEventListener('load', function() {
    const docGrid = document.body.style.grid;
    const closeBtn = document.querySelector('.ui-banner__close');

    if(docGrid === undefined) {
      browserOutdated = true;
      dismissedAt = window.localStorage.getItem('outdatedBrowserDismissedAt');
    }

    if(browserOutdated) {
      (!dismissedAt || dismissExpired(dismissedAt)) && document.querySelector('.ui-banner').classList.remove('hidden');
    }

    var dismissExpired = function(val) {
      if(!val) return false;

      var sevenDaysAgo = new Date();
      sevenDaysAgo.setDate(sevenDaysAgo.getDate() - 7);

      return (val < sevenDaysAgo);
    };

    closeBtn.addEventListener('click', function(e) {
      dismissedAt = window.localStorage.setItem('outdatedBrowserDismissedAt', Date.now());
      document.querySelector('.ui-banner').classList.add('hidden');
    })
  })
</script>

<div class="ui-banner ui-banner--outdated-browser hidden">
  <span>
    <span class="bold">Unfortunately you are using an outdated web browser.</span>
    Please upgrade your browser to improve your experience and security.
  </span>
  <span class="ui-banner__close"></span>
</div>


        <script>
  var htlbid = htlbid || {};
  htlbid.cmd = htlbid.cmd || [];
  (function() {
    var src = 'https://htlbid.com/v3/dailykos.com/htlbid.js';
    document.write('<scr' + 'ipt src="' + src + '"></scr' + 'ipt>');
  })();
</script>

        <script>
  if (DailyKos.Ads && DailyKos.Ads.isAdsPage()) {
    htlbid.cmd.push(function() {
      if (!!DailyKos.currentStoryID) {
        htlbid.setTargeting('story_id', DailyKos.currentStoryID);
        htlbid.layout();
        htlbid.setTargeting('groups', DailyKos.currentStoryGroupNames);
        htlbid.layout();
        htlbid.setTargeting('tags', DailyKos.currentStoryTags);
        htlbid.layout();
        htlbid.setTargeting('home', 'false');
        htlbid.layout();
      } else {
        htlbid.setTargeting('home', 'true');
        htlbid.layout();
      }
      htlbid.setTargeting('domain', window.location.hostname);
      htlbid.layout();
      htlbid.setTargeting('referrer', document.referrer);
      htlbid.layout();
      htlbid.setTargeting('pathname', window.location.pathname);
      htlbid.layout();
      htlbid.setTargeting('signed_petitions', DailyKos.Ads.getSignedPetitions());
      htlbid.layout();
      htlbid.setTargeting('last_splashed_datetime', DailyKos.Ads.getLastSplashed());
      htlbid.layout();
      htlbid.setTargeting('page_type', DailyKos.pageType || "");
      htlbid.layout();
    });
  }
</script>

          <div id='splash-ad'></div>
  <!-- NOTE: The #top-mobile-placeholder id is needed by the HTL API. -->
  <button type="button" id="responsiveSplash-remove"></button>
  <div id="top-mobile-placeholder" class="story-splash-ad-placeholder"></div>
  <script>
    if (DailyKos.showsAds()) {
      const splashAd = {
        altUnitInnerHTML: `<div class="htlad-MOB_Adhesive"></div>`,
        altUnitLocId: 'splash-ad',
        cookieName: 'splashad',
        placeholderClass: 'story-splash-ad-placeholder',
        splashGamCode: '/1095769/Splash',
        splashHourInterval: 1,
        splashUnitInnerHTML: `<div class="htl-ad" data-unit="Splash" data-sizes="0x0:1x1|760x0:1x1" data-prebid="0x0:|768x0:" data-refresh="none"></div>`,
        splashUnitLocId: 'splash-ad',

        getCookie: function (name) {
          const nameEQ = name + '=';
          const ca = document.cookie.split(';');
          for (let i=0; i < ca.length; i++) {
            let c = ca[i];
            while (c.charAt(0) === ' ') c = c.substring(1, c.length);
            if (c.indexOf(nameEQ) === 0) return c.substring(nameEQ.length, c.length);
          }
          return null;
        },

        insertAd: function (parentId, innerHTML) {
          const el = document.getElementById(parentId);
          if (el) { el.innerHTML = innerHTML; }
        },

        removePlaceholder: function() {
          const el = document.getElementById('top-mobile-placeholder');
          if (el && el.parentNode) { el.parentNode.removeChild(el); }
        },

        setCookie: function (name, value, hours) {
          let expires = '';
          if (hours) {
            const date = new Date();
            date.setTime(date.getTime() + (hours * 60 * 60 * 1000));
            expires = "; expires=" + date.toUTCString();
          }
          document.cookie = name + '=' + (value || '')  + expires + '; path=/';
        },

        start: function () {
          document.getElementById('responsiveSplash-remove').addEventListener("click", () => {
            document.getElementById('responsiveSplash-remove').classList.remove('show');
            window.splashclose();
          });
          if (splashAd.getCookie(splashAd.cookieName)) {
            splashAd.insertAd(splashAd.splashUnitLocId, splashAd.altUnitInnerHTML);
            splashAd.removePlaceholder();
          } else {
            document.getElementById('responsiveSplash-remove').classList.add('show');
            splashAd.insertAd(splashAd.splashUnitLocId, splashAd.splashUnitInnerHTML);
            splashAd.setCookie(splashAd.cookieName, '1', splashAd.splashHourInterval);
            //googletag event listener for unfilled
            window.googletag = window.googletag || {};
            window.googletag.cmd = window.googletag.cmd || [];
            window.googletag.cmd.push(function() {
              window.googletag.pubads().addEventListener('slotRenderEnded', (event) => {
                if (event.slot.getAdUnitPath() === splashAd.splashGamCode && event.isEmpty) {
                  splashAd.insertAd(splashAd.splashUnitLocId, splashAd.altUnitInnerHTML); //insert anchor on unfilled
                  splashAd.removePlaceholder();
                }
              });
            });
          }
        },
      };

      splashAd.start();
    };
  </script>


