    <script type="text/javascript">
  const helpBtn = document.getElementById('toggle-helpscout');
  const hamburgerHelpBtn = document.getElementById('toggle-helpscout-hamburger-nav');
  let hasInitialized = false;

  var initializeHelpScout = function() {
    if (!hasInitialized) {
      !function(e,t,n){function a(){var e=t.getElementsByTagName("script")[0],n=t.createElement("script");n.type="text/javascript",n.async=!0,n.src="https://beacon-v2.helpscout.net",e.parentNode.insertBefore(n,e)}if(e.Beacon=n=function(t,n,a){e.Beacon.readyQueue.push({method:t,options:n,data:a})},n.readyQueue=[],"complete"===t.readyState)return a();e.attachEvent?e.attachEvent("onload",a):e.addEventListener("load",a,!1)}(window,document,window.Beacon||function(){});
        Beacon('init', "123b268c-9e2e-46e3-b29c-6f0cb8e6b942");
        if (DailyKos.isLoggedIn()) {
          Beacon('identify', {
            name: DailyKos.currentUserNickname(),
            email: DailyKos.currentUserEmail(),
            signature: DailyKos.helpScoutSignature(),
            'isSubscriber': DailyKos.isSubscriber(),
            'roles': DailyKos.currentUserRoles(),
            'showsAds': DailyKos.showsAds(),
            'AdminLink': window.location.host + '/administration/users/' + DailyKos.currentUserId(),
          });
        } else {
          Beacon('logout');
        }
        Beacon('suggest', ['5ee3ae9b2c7d3a10cba8d5bb', '5edfd4d004286306f804e76d']);
        Beacon('config', {
          mode: 'selfService',
          docsEnabled: true,
          messaging: {
            chatEnabled: true,
            contactForm: {
              showGetInTouch: true,
              allowAttachments: true
            },
          },
          labels: {
            noTimeToWaitAround: "We usually respond by the next business day.",
            messageConfirmationText: "We usually respond by the next business day.",
            responseTime: "We usually respond by the next business day",
            escalationTalkText: "Send us a message",
            whatMethodWorks: "Send us a message."
          }
        })
        hasInitialized = true;
    }
  };

  helpBtn.addEventListener('mouseenter', function() {
    initializeHelpScout();
  });
  hamburgerHelpBtn.addEventListener('mouseenter', function() {
    initializeHelpScout();
  });
</script>

