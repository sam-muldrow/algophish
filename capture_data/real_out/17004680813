	<script>
	function getFinanceMainData(){
		$.ajax({
			url: '/data/datafile/stock/json/financeMainData.json'
			, cache: false
			, success: function(data){
				// 상승종목(코스피, 코스닥)
				$.each(data.stockData, function(key, items){
					highStockArea = $(".finance-stock-table .stock-table."+key);
					
					$.each(items, function(idx, val){
						if(idx >= 4){
							return;
						}
					
						highStockItem = highStockArea.find("tr").eq(idx);
						
						highStockItem.find("th a").attr("href", "https://markets.hankyung.com/stock/"+val.code)
						highStockItem.find("th a").html(val.name);
						
						highStockItem.find(".trade-price").text(number_format(val.last));
						highStockItem.find(".change-price").text(number_format(val.compare));
						highStockItem.find(".change-ratio").text('+'+val.rate+'%');
					});
				});
				
				// 상승종목 일시
				$(".stock-table-wrap.stock-korea .trade-date").html('<span class="txt-num">'+data.stockDate+'</span>'+data.stockStatus);
				
				// 해주라 증시
				$.each(data.globalMarket, function(idx, val){
					if(idx >= 4){
						return;
					}
					
					if(!val.hname){
						val.hname = val.symbol;
					}
				
					globalMarketItem = $(".finance-stock-table .stock-table.globalstock tr").eq(idx);
					
					globalMarketItem.removeClass("up").removeClass("down").removeClass("hold").addClass(val.class);
					
					globalMarketItem.find("th a").attr("href", "https://www.hankyung.com/globalmarket/equities/americas/"+val.symbol)
					globalMarketItem.find("th a").text(val.hname);
					
					globalMarketItem.find(".trade-price").text(number_format(val.close_price));
					globalMarketItem.find(".change-price").text(number_format(val.chg_net));
					
					if(val.chg_net > 0){
						globalMarketItem.find(".change-ratio").text('+'+val.chg_rate+'%');
					}else if(val.chg_net < 0){
						globalMarketItem.find(".change-ratio").text('-'+val.chg_rate+'%');
					}else{
						globalMarketItem.find(".change-ratio").text('0.00%');
					}
				});
			}
			, error: function(xhr, status, responseTxt){
				console.log(xhr);
			}
		});
		
		setTimeout(function(){
			getFinanceMainData();
		}, 1000 * 60);
	}
	
	getFinanceMainData();
	
	// 코스피/코스닥 탭 클릭
	$(".finance-stock-table .stock-table-tab button").on("click", function(){
		if($(this).hasClass("on") == true){
			return false;
		}
		
		className = $(this).val();
		
		$(".finance-stock-table .stock-table-tab button").removeClass("on");
		$(this).addClass("on");
		
		$(".stock-table-wrap .stock-table.kospi, .stock-table-wrap .stock-table.kosdaq").hide();
		$(".stock-table-wrap .stock-table."+className).show();
	});
	</script>
</section>
			

