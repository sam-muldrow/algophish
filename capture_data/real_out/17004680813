    <script>
    $(document).ready(function(){
        const descriptionHeight = 
            $(".special-reports .special-reports-cont").height() // 전체
            - $(".special-reports .special-reports-cont .section-name").outerHeight(true) // 섹션 타이틀
            - $(".special-reports .special-reports-cont .reports-name").outerHeight(true) // 탑섹션 명
            - $(".special-reports .special-reports-cont .reports-desc").outerHeight(true) // 탑섹션 설명
            - $(".special-reports .special-reports-cont .reports-news").outerHeight(true); // 탑섹션 뉴스

            const newsShow = Math.floor(descriptionHeight / 60) - 1;

            $(".special-reports .special-reports-cont .reports-news li:gt("+newsShow+")").remove();
            $(".special-reports .special-reports-cont .reports-news li").show();
    });
    </script>
