	<main id="contents">
		<!-- 마이 모듈(2022-07-26 추가) -->
		<section class="mymodul-wrap" style="display:none;">
			<div class="mymodul-inner">
				<!-- 로그인 전 -->
				<div class="mymodul-before-login">
					<p class="info-msg">
						<a href="https://id.hankyung.com/login/joinPage.do" class="txt-underline"><strong>한국경제 회원가입</strong></a>하고 회원에게만 제공하는 콘텐츠와 특별한 투자정보 등의 해택을 받으세요.
					</p>
					<button type="button" class="btn-close-mymodul">
						<span class="blind">하루동안 보지 않기</span>
					</button>
				</div>

				<script>
				// 쿠키값 없으면 보여주기
				if($.cookie('myModuleLogin') != 'Y'){
					$('.mymodul-wrap').show();
				}

				// 하루동안 보지 않기 클릭
				$(".mymodul-before-login .btn-close-mymodul").on("click", function(){
					const myToday = new Date();
					const myExpires = new Date(myToday.getFullYear(), myToday.getMonth(), myToday.getDate(), 23, 59, 59);

					$.cookie('myModuleLogin', 'Y', {expires: myExpires});
					$('.mymodul-wrap').slideUp();
				});

				// 5분뒤에 자동으로 보지 않음
				setTimeout(function(){
					if($('.mymodul-wrap').is(":visible") == true){
						$(".mymodul-before-login .btn-close-mymodul").trigger("click");
					}
				}, 1000 * 60 * 5);
				</script>
			</div>
		</section>
		<!-- // 마이 모듈 -->
		
