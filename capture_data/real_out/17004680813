<script>
var coinMark = {
	'1'		: {sign: '+', classname: 'up'}
	, '5'	: {sign: '-', classname: 'down'}
	, '3'	: {sign: '', classname: 'hold'}
};

var setCoinData = function(){
	$.get("/data/service/coin.hankyung.com/coin_data/investing_master.json", function(data){
		$.each(data, function(idx, val){
			area = $(".coin-list li."+val.code);
			
			mark = coinMark[val.fluc];
			
			// 신호 클래스
			area.find("span.status").removeClass("up");
			area.find("span.status").removeClass("down");
			area.find("span.status").removeClass("hold");
			
			area.find("span.status").addClass(mark.classname);
			
			// 값
			area.find("span.status span.num").text(number_format(val.last_krw));
			// 등락률
			area.find("span.status span.rate em").text(mark.sign+val.chg_pct_krw.toFixed(2)+'%');
		});
	});
	
	setTimeout(function() {
        setCoinData();
    }, 1000 * 60);
}

setCoinData();
</script>
			

