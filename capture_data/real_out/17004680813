	<script>
	// 빈 ul 삭제
	$('.main-headline ul').each(function(index,element){
		if($(element).find('li').length == 0){
			$(element).remove();
		}
	});

	// 위로가기 버튼
	$('.btn-top').on("click", function(){
		$('html, body').animate( { scrollTop : 0 }, 300 );
		return false;
	});

	$(function (){
		// [2021.07.02] 하위메뉴 레이어 키보드 접근성 추가
		$('.gnb-dropdown').keyup(function() {
			$(this).addClass('focus');
		});
		$('.gnb-dropdown-menu li:last-child a').blur(function() {
			$(this).parents('.gnb-dropdown').removeClass('focus');
		});

		$(document).on('mouseup',function(e){
			if (!$(e.target).is('.gnb-dropdown-menu') && !$(e.target).parents().is('.gnb-dropdown-menu')){
				$('.gnb-dropdown').removeClass('focus');
			};
		});
	});
	</script>

	<!-- 팝업 -->
    <script type="text/javascript">
var today = new Date();

var year  = today.getFullYear().toString();    //년
var month = (today.getMonth()+1).toString();   //월
var day   = today.getDate().toString();        //일
var hour  = today.getHours().toString();       //시
if(month.length==1) month="0"+month;
if(day.length==1) day="0"+day;
if(hour.length==1) hour="0" + hour;

var now = year+month+day;
var now2 = year+month+day+hour;

var specs = 'toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbas=no,resizable=no,width=300,height=434,left=10,top=10';

// 저소득 한부모 가정 구독료 지원 팝업
