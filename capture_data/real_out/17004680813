<script>
var setRankingNews = function(){
	$.get("/data/datafile/hankyung/json/realtimeNews.json", function(data){
		$(".ranking-cont ol").html('');
	
		$.each(data.list, function(idx, val){
			if(idx < 15){
				ord = Math.floor(idx/5);
				
				$(".ranking-cont ol").eq(ord).append('\
					<li>\
						<em class="num">'+(idx+1)+'.</em>\
						<h2 class="news-tit"><a href="'+val.url+'" data-pm="'+val.payment+'">'+val.title+'</a></h2>\
					</li>\
				');
			}
		});
	});

	markPayment();
}

setRankingNews();
</script>
			

