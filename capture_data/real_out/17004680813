<script>
const taglastupdate = $(".section-tagnews .tag-category div.item").map(function(){
	return parseInt($(this).data("lastupdate"));
}).get();

// 슬라이드
$('.section-tagnews .tag-category').slick({
	slidesToShow: 7
	, slidesToScroll: 1
	, dots: false
	, infinite: false
	, variableWidth: true
});

// 항목 선택
$(".section-tagnews .tag-category div.item").on("click", function(){
	tagid = $(this).data("tagid");
	
	$(".section-tagnews .tag-category div.item").removeClass("on");
	$(this).addClass("on");
	
	$(".section-tagnews .tag-news .major-thumb-news").hide();
	$(".section-tagnews .tag-news .major-thumb-news[data-tagid="+tagid+"]").show();
});

$(".section-tagnews .tag-category div.item").eq(taglastupdate.indexOf(Math.max.apply(null, taglastupdate))).trigger("click");

// 더보기
$(".section-tagnews .btn-more-tagnews").on("click", function(){
	tagPage = $(this).data("page");
	tagid = $(this).data("tagid");
	slug = $(this).data("slug");
	
	tagPage++;
	
	$(this).data("page", tagPage);
	
	if(tagPage >= 4){
		location.href = "/tag/"+encodeURI(slug);
		return false;
	}
	
	$(".section-tagnews .tag-news .major-thumb-news[data-tagid="+tagid+"] ul li:lt("+(tagPage*4)+")").show();
	
	if(tagPage >= 3){
		$(this).html('전체보기<span class="icon-plus"></span>');
	}
});
</script>
			

			<section class="main-component politics">
	<div class="layout-inner">
		<h3 class="main-tit"><a href="https://www.hankyung.com/politics">정치</a></h3>
		<article class="news-tmp nt-thumb-bundle">
			<ul>
				<li>
					<div class="news-item">
						<div class="thumb">
							<a href="https://www.hankyung.com/article/2023112099037">
								<img src="https://img.hankyung.com/photo/202311/ZN.35083367.3.jpg" alt="민주당, 급기야 '尹 탄핵' 언급…與 &quot;개딸 얻고 국민 잃을 것&quot;">
							</a> 
						</div>
						<div class="txt-cont">
							<h2 class="news-tit"><a href="https://www.hankyung.com/article/2023112099037" data-pm="N">민주당, 급기야 '尹 탄핵' 언급…與 "개딸 얻고 국민 잃을 것"</a></h2>
						</div>
