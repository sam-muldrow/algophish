<script>
// 지수 데이터 호출 
$.ajax({
	url: '/data/datafile/ext/json/kapiData.json'
	, success: function(data){
		if(data.status == 'success'){
			$(".kapi-cont .status").removeClass("up").removeClass("down").removeClass("hold");
			
			if(data.differ > 0){
				$(".kapi-cont .status").addClass("up");
				$(".kapi-cont .status .rate").html("+"+data.differ+"<em>+"+data.rate+"%</em>");
			}else if(data.differ < 0){
				$(".kapi-cont .status").addClass("down");
				$(".kapi-cont .status .rate").html(data.differ+"<em>"+data.rate+"%</em>");
			}else{
				$(".kapi-cont .status").addClass("hold");
				$(".kapi-cont .status .rate").html(data.differ+"<em>"+data.rate+"%</em>");
			}
			
			$(".kapi-cont .status .num").text(number_format(data.kapi));
			$(".kapi-cont .date span").text(data.date);
		}
	}
	, error: function(xhr, status, responseTxt){
		console.log(xhr);
	}
});

// 차트 데이터 호출
$.ajax({
	url: '/data/datafile/ext/json/kapiChart.json'
	, success: function(data){
		drawKapiChart(data);
	}
	, error: function(xhr, status, responseTxt){
		console.log(xhr);
	}
});

function drawKapiChart(data){
	new Highcharts.StockChart({
		// 차트 영역 옵션
		chart: {
			renderTo: 'kapiChartArea'
			, height: 240
		}
		// 내비게이터
		, navigator: {
			enabled: false
		}
		, rangeSelector: {
			enabled: false
		}
		// 하단 스크롤바
		, scrollbar: {
            enabled: false
        }
		// X축
		, xAxis: {
            gridLineWidth: null
            , type: 'datetime'
            , dateTimeLabelFormats: {day: '%m월 %d일', week: '%m월 %d일'}
        }
		// Y축
		, yAxis: {
			gridLineWidth: null
            , opposite: false
			, min: data.min
			, max: data.max
        }
		, tooltip: {
            valueDecimals: 2,
            type: 'datetime',
            useHTML: true,
            formatter: function () {
				var topDateFm = Highcharts.dateFormat('%m월 %d일', this.x);
				
				var s = '';
				
                $.each(this.points, function (i, point) {
					s += '<b>'+Highcharts.numberFormat(point.y, 2, '.', ',')+'</b>'+'<span style="margin-left:10px;">' + topDateFm + '</span>';
                });
				
                return s;
            },
            shared: true
        }
		, series: [
			{
				type: "area"
				, id: "tic"
				, valueDecimals: 2
				, data: data.data
				, color: 'rgb(247, 111, 111)'
				, fillOpacity: 0.1
				, negativeColor: '#3c74ff'
				, dataGrouping: {
					units: [['day', [1]]]
				}
			}
		]
	});
}
</script>
			

