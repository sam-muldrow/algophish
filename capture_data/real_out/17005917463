        <script>
            function onChangeLocale(selectItem) {
                //var selectedLanguage = selectItem[selectItem.selectedIndex].value;
                var selectedLanguage = selectItem.value;
                //alert("selectedLanguage = "+selectedLanguage);
                document.getElementById(selectedLanguage).selected = "true";
                //document.getElementById("selectLocale").selected="true";
                if (selectedLanguage != "en-US") {
                    setCookie("LN_C1_Lang", selectedLanguage);
                    window.top.location.href = "https://mylang.unionbankonline.co.in?lang=hindi";
                } else {
                    setCookie("LN_C1_Lang", selectedLanguage);
                    window.top.location.href = "https://www.unionbankonline.co.in?lang=eng";
                }
                //location.reload(true);
                //reload();
            }

            function setCookie(cname, cvalue) {
                document.cookie = cname + "=" + cvalue + "; " + "; " + "path=/;domain=.unionbankonline.co.in";
            }
        </script>

