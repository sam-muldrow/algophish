  $(function () {
    var finished = true;
    var page = 2;
    var url = "//a.jiemian.com/index.php?m=index&a=indexAjax";
    var lasttime = '';
    $(window).scroll(function () {
      if (($(document).height() - $(window).height() - $(document).scrollTop()) <= $(window).height() + 800 &&
        finished) {
        finished = false;
        $.get(url.replace('http://', '//'), {
          page: page,
          lasttime: lasttime
        }, function (data) {
          var len = data.length - 1;
          $.each(data, function (i, n) {
            // 注：逻辑，第二页加载上部左中右，对应在reportAjax，center，videoAjax等返回格式[{id:''},{},{}]
            // 现逻辑，原第一二页数据，合并为第一页数据。第二页为科技
            if (typeof (n.type) != 'undefined' && n.type == 'before') {
              $('#' + n.id).before(n.rst);
            } else {
              $('#' + n.id).append(n.rst);
            }
            $("img[data-lazy-img]").lazyload();
            if (i == len && page < 3) {
              page++;
              finished = true;
            } else if (page == 3) {
              getJMediaList(1, true);
            }
          });
        }, 'jsonp');
      }
    });
    $('#index-right-ad1').each(function () {
      var _this = this;
      var st = setTimeout(function () {
        var scriptStr = $(_this).find('script').html();
        if (scriptStr != 'undefined') {
          scriptStr = scriptStr.replace('document.write', '$(#index-right-ad1).html');
          eval(scriptStr);
          clearTimeout(st);
        }
      }, 100);
    });
    if ($(".blxxjb").length > 0) {
      $('.blxxjb').bxSlider({
        auto: true,
        mode: 'vertical',
        speed: 500,
        slideWidth: 320,
        pagerAutoWidth: true,
        autoHover: true,
        pause: 4000,
        pager: false,
        controls: false
      });
    }

    // 界面号图片预加载
    function showImg (index) {
      $('.media-list img').each(function (index, item) {
        $(item).attr('src', $(item).attr('data-img'));
        $(item).removeAttr('data-img');
      });
    }



    function getJMediaList (page, flag) {
      $.ajax({
        type: "post",
        url: "https://a.jiemian.com/index.php?m=index&a=indexAjaxJmedia&page=" + page,
        dataType: "jsonp",
        jsonp: "callback",
        success: function (res) {
          $('.media-content .iconfont').css('display', 'block');
          var resData = res[0] || res.rst;
          // 底部media轮播图
          var mediaSwiper = new Swiper('.media-content .swiper-container', {
            loop: false,
            navigation: {
              nextEl: '.nextpage_icon',
              prevEl: '.previouspage_icon',
            },
            pagination: {
              el: '.media-pagination',
              clickable: true,
              bulletActiveClass: 'default',
              renderBullet: function (index, className) {
                var html = '', htmlArr = [];
                if (resData.pageCount > 1 && flag) {
                  for (var i = 0; i < resData.pageCount; i++) {
                    html = '<li class="' + className + '" pageindex="' + (i + 1) + '">' + (i + 1) + '</li>';
                    htmlArr.push(html);
                  }
                  return (htmlArr.join('')) ;
                }
              },
            },
          });
          $('.media-list').empty().append('<li slide-index=' + page + '>' + resData.rst + '</li>');
          $('#media-content').attr('page', page);
          $(".hover-tips").fsrTip();
          showImg(page);
          $('.swiper-button li').removeClass('bullet-active');
          $('.swiper-button li[pageindex =' + page + ' ]').addClass('bullet-active');
          var item = $('.media-content .swiper-pagination-bullet');
          item.on('click', function () {
            if ($(this).hasClass('bullet-active')) return;
            item.removeClass('bullet-active');
            $(this).addClass('bullet-active');
            getJMediaList($(this).attr('pageindex'), true);
          })  
        },
        error: function (err) { 
        }
      });
    }

   // 界面号前一页
    var currentPage = 0,
      rqeuestPage = 0;
    $('.swiper-button .previouspage_icon').click(function () {
      currentPage = Number($('#media-content').attr('page'));
      rqeuestPage = currentPage - 1;
      if(rqeuestPage > 0){
        setTimeout(function(){
          getJMediaList(rqeuestPage, true);
        }, 500)
      }
    })
    // 后一页点击事件
    $('.swiper-button .nextpage_icon').click(function () {
      rqeuestPage = Number($('#media-content').attr('page')) + 1;
      if(rqeuestPage <= $('.swiper-button ul li').length ) {
        setTimeout(function(){
          getJMediaList(rqeuestPage, true);
        }, 500)
      }
    })

  });

