<script>
$(document).ready(function(){
	$(".txt-search-kw").keyup(function(e){
    var code = e.which; // recommended to use e.which, it's normalized across browsers
    if(code==13){
    	if($(".sel-search-subkind").val()){
    		searchData();
    		$('.searchfixed').hide();
    	}else{
	    	e.preventDefault();
	    	location.href = "/search/new/"+$('.txt-search-kw').val();
    	}
    }
	});
});
</script>
