        var loadE = [];
        let scrollSectionXTemplate13 = (classItem = '.lazySections')=>{
            const items = document.querySelectorAll(classItem);
            items.forEach(item=>{
                if (item.children[0] && ((item.children[0].nodeName == "SCRIPT" && item.children[0].type == "x-template") || item.getAttribute('data-target13'))){
                    let event = item.getAttribute('data-event') ? item.getAttribute('data-event').split("::") : "";
                    let dataId = item.getAttribute('data-idSection') ? item.getAttribute('data-idSection') : false;
                    switch (event[0]) {
                        case 'scroll':
                            new IntersectionObserver(
                                entries=>{entries.forEach(
                                    entry =>{
                                        if (entry.isIntersecting) {
                                            if(dataId){
                                                addDependencies13(dataId);
                                                const scrollVal = setInterval(()=>{
                                                    if (loadE.indexOf(dataId) != -1 && !entry.target.classList.contains('activeSection')){
                                                        entry.target.innerHTML = entry.target.children[0].innerHTML;
                                                        entry.target.classList.add('activeSection');
                                                        if (!event[1]){
                                                            clearInterval(scrollVal);
                                                        }
                                                    }
                                                    if (event[1] && entry.target.classList.contains('activeSection')){
                                                        window[event[1]]();
                                                        clearInterval(scrollVal);
                                                    }
                                                },100);
                                            }else {
                                                if (!entry.target.classList.contains('activeSection')){
                                                    entry.target.innerHTML = entry.target.children[0].innerHTML;
                                                    entry.target.classList.add('activeSection');
                                                }
                                                const scrollVal = setInterval(()=> {
                                                    if (event[1] && entry.target.classList.contains('activeSection') &&  window[event[1]]) {
                                                        window[event[1]]();
                                                        clearInterval(scrollVal);
                                                    }
                                                });
                                            }
                                        }
                                    });
                                },
                                {
                                    threshold: 0.2
                                }
                            ).observe(item);
                            break;
                        default:
                            console.log(`Event not found`);
                    }
                }
            });
        };
        document.addEventListener("DOMContentLoaded", function() {
            scrollSectionXTemplate13();

        });


