					function pixelView(click)
					{
                    if (scradb || !(typeof adblockV1 == 'undefined')) {
    						if (typeof click !== 'undefined') {
    							var url = 'https://g.cda.pl/g.php?pixel&vi=NWVjYzIwZTgyYTQzYjcyZWFlMGJiNjUzYjgyMmJmZTA_NzEwOA&pl=clickTracking&ct=[timestamp]';
    						} else {
    							var url = 'https://g.cda.pl/g.php?pixel&vi=NWVjYzIwZTgyYTQzYjcyZWFlMGJiNjUzYjgyMmJmZTA_NzEwOA&pl=impression&ct=[timestamp]';
    						}                        
						
						url = url.replace('[timestamp]', (new Date().getTime()));
						
						var pixel = new Image(1,1);
						pixel.onload = function() {}
						pixel.src = url;
                        }
						
						return true;
					}
					pixelView();
