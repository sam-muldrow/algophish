<script>
    function showPodcast(url) {
        document.querySelector('#podcast_player iframe').setAttribute('src', url);
    }

    jQuery(document).ready(function($) {
        jQuery('.list-podcast').on('click', function() {
            var url = jQuery(this).data('podcast');
            showPodcast(url);
        });
    });
