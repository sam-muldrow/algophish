<script>
checkIEBrowser = function () {
        let e = !1;
        return (navigator.userAgent.match(/msie/i) || navigator.userAgent.match(/trident/i)) && (e = !0), e
    }


equalheight = function(container){

var currentTallest = 0,
     currentRowStart = 0,
     rowDivs = new Array(),
     $el,
     topPosition = 0;
 jQuery(container).each(function() {

   $el = jQuery(this);
   $($el).height('auto')
   topPostion = $el.position().top;

   if (currentRowStart != topPostion) {
     for (currentDiv = 0 ; currentDiv < rowDivs.length ; currentDiv++) {
       rowDivs[currentDiv].height(currentTallest);
     }
     rowDivs.length = 0; // empty the array
     currentRowStart = topPostion;
     currentTallest = $el.height();
     rowDivs.push($el);
   } else {
     rowDivs.push($el);
     currentTallest = (currentTallest < $el.height()) ? ($el.height()) : (currentTallest);
  }
   for (currentDiv = 0 ; currentDiv < rowDivs.length ; currentDiv++) {
     rowDivs[currentDiv].height(currentTallest);
   }
 });
}

jQuery(window).load(function() {

  equalheight('.small');
equalheight('.large');
});


jQuery(window).resize(function(){
  equalheight('.sales .small');
equalheight('.sales .large');
});


jQuery(window).ready(function(){

jQuery('.acba#signup-popup-show .button, .acba#thankyou-message .button').text('Join Now');
var terms1 = jQuery('#signup-popup-show .acba-terms').text().replace('CONTINUE SHOPPING', 'JOIN NOW');
jQuery('#signup-popup-show .acba-terms').text(terms1);
var terms2 = jQuery('#thankyou-message .register-thank-you .acba-terms').text().replace('CONTINUE SHOPPING', 'JOIN NOW');
jQuery('#thankyou-message .register-thank-you .acba-terms').text(terms2);
jQuery('.acba-message p strong').text('JOIN NOW');



jQuery(".promo_text").each(function(){
promoText = jQuery(this).text();
if(promoText.indexOf("Bold") != -1){
 promoText.replace("Extra 20% OFF (Bold for set date range)", "");
 jQuery(this).text(promoText);
}


promoHtml = jQuery(this).html();
if(promoHtml != ''){
 
}


})

 if(jQuery('#listcoupons') && !("string"==typeof localStorage.coupon_data)) {
            jQuery('#listcoupons').find('ul li:contains("BF10")').hide();
            jQuery('#listcoupons').find('ul li:contains("BF20")').hide();
            jQuery('#listcoupons').find('ul li:contains("BF30")').hide();
            jQuery('#listcoupons').find('ul li:contains("BF50")').hide();
        }
       
        if(jQuery('#voucher-list') && !("string"==typeof localStorage.coupon_data)) {
            jQuery('#voucher-list').bind('DOMSubtreeModified', function(){
                jQuery(this).find('li:contains("BF10")').hide();
                jQuery(this).find('li:contains("BF20")').hide();
                jQuery(this).find('li:contains("BF30")').hide();
                jQuery(this).find('li:contains("BF50")').hide();
            });
        }
      


});
</script>
