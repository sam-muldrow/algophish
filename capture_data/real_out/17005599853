<script type="text/javascript" defer>
//  <![CDATA[
    jQuery(document).ready(function () {

        jQuery('.countdown').each(function () {
            var obj = jQuery(this);
            endDate = jQuery(this).data('countdown');
            var finalDate = moment.tz(endDate, "Europe/London");

            obj.countdown(finalDate.toDate(), function (event) {

                if (event.strftime('%D') == 1) {
                    timeLeftDays = event.strftime('%-D Jour');
                } else if (event.strftime('%D') == 0) {
                    timeLeftDays = 'Aujourd\'hui';
                } else {
                    timeLeftDays = event.strftime('%-D Jours');
                }

                timeLeftHrs = event.strftime('%H:%M:%S');
                timeLeft = '<strong>' + timeLeftDays + '</strong> ' + timeLeftHrs;
                obj.next('.counter').html(timeLeft);
            });
        });

        jQuery(".item-holder").stop().mouseenter(function () {
            jQuery(this).find(".description_text").stop().animate(
                    {opacity: 1}, 400);
        }).stop().mouseleave(function () {
            jQuery(this).find(".description_text").stop().animate({opacity: 0}, 400);
        });

        jQuery(".home_slider").owlCarousel({
            autoplay: true,
            autoplayTimeout: 3000,
            autoplayHoverPause: true,
            loop: true,
            items: 1,
            lazyLoad:true,
            responsive: false,
            dots: true,
            animateOut: 'fadeOut'
        });
    });
    //]]>
</script>
</div>
