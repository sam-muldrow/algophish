				var redirectIEUrl = "https://www.brandalley.fr/browser-not-supported";
				if(window.location != redirectIEUrl) {
					window.location.replace(redirectIEUrl);
				}
			}
    </script> <script type="text/javascript">
        var popupFlag = 1;
        var $j = jQuery.noConflict();
        var achicaData = window.universal_variable.user.achica_data || [];
        var chsterLoginPopup =  window.universal_variable.cocosa_referrer  +  window.universal_variable.mysale_referrer;   

        $j(function () {
            var login_box_is_open = false;
            
            if (typeof (window.universal_variable.afterAuthUrl) !== 'undefined') {
                jQuery("#afterAuthUrl").val(window.universal_variable.afterAuthUrl);
            }

            var joinnow_box = jQuery('#signup-popup-show');
            var login_box = jQuery('#newLogin');
            var cmsPageCheck = document.body.className.match(/cms-page-view/gi);


            if(chsterLoginPopup > 0 && !cmsPageCheck ){
                joinnow_box.show();
            }

            jQuery('#trigger-login-popup').on('click', function (e) {
                if (login_box_is_open == false) {
                    e.preventDefault();
                    login_box_is_open = true;

                    var reCustomerValidate = Mage.Cookies.get('returning_customer');
                    //alert(reCustomerValidate);
                    if (typeof reCustomerValidate != 'undefined' && reCustomerValidate != null) {
                        login_box.show(0, function () {
                            containerH = jQuery(this).children('.login-container').height();
                            checkHeight();

                        });
                         if(!checkIEBrowser()){
                jQuery('#login-email').focus();
                   }
                    } else {
                        joinnow_box.show(0, function () {
                            containerH = jQuery(this).children('.login-container').height();
                            checkHeight();

                        });
                         if(!checkIEBrowser()){
                    jQuery('#email_address').focus();
                   }
                    }
                    //return false;
                }
            });

                if(!window.universal_variable.tk && !window.universal_variable.user.logged_in){
               $j("a").not(".brand-directory-main a, .login-overlay a, .brand-products-top .breadcrumbs a, .brand-description-content a, .brand-share a, .brand-product-list .item a, .view-more-brand-list a, .allowLink a, .social-links a, #footer a, #showJoinBox, #showLoginBox, .new-login a, .logo a, li.blog a, a.main-cat, #login-form a, .customer-account-forgotpassword #form-validate a, .customer-account-create #form-validate a, .cms-page-view .main-container a, .contacts-index-index .main-container a, .closeBtn, .contact-link a").unbind('click').on('click', function () {
                    if (popupFlag == '1') {
                        $j(this).attr('href', 'javascript:void(0)');
                        $j('#trigger-login-popup').trigger('click');
                        allowScroll();
                        return false;
                    }
                });

                $j("#search1, .quickbuy-behavesimple").unbind('click').on('click', function () {
                    if (popupFlag == '1') {
                        $j('#trigger-login-popup').trigger('click');
                        allowScroll();
                        return false;
                    }
                });
                $j("#search1").on('keypress', function () {
                    if (popupFlag == '1') {
                        $j('#trigger-login-popup').trigger('click');
                        allowScroll();
                        return false;
                    }
                });
            }
    

            jQuery('#forgotPassword').click(function () {
                jQuery('#newLogin').addClass('forgot-password');
                jQuery('#login-form-pop').hide();
                jQuery('#new-password-form').show();
                jQuery('.ba-message h2').text('Récupérer votre mot de passe');
                jQuery('.login-terms-cookies, .acba .acba-terms, .signUpTrigger').hide();
                allowScroll();
            });
            jQuery('#showPLoginPart').click(function () {
                jQuery('#newLogin').removeClass('forgot-password');
                jQuery('#new-password-form').hide();
                jQuery('#login-form-pop').show();
                jQuery('.ba-message h2').text('Heureux de vous revoir');
                 if(!checkIEBrowser()){
                jQuery('#login-email').focus();
                   }
                jQuery('.login-terms-cookies, .acba .acba-terms, .signUpTrigger').show();
                allowScroll();
            });
            jQuery('.signUpTrigger').click(function () {
                jQuery('#newLogin').removeClass('forgot-password');
                jQuery('#newLogin').hide();
                jQuery('.ba-message h2').text("Join the UK's best flash sale website today!");
                jQuery('#signup-popup-show').show();
                allowScroll();
                 if(!checkIEBrowser()){
                    jQuery('#email_address').focus();
                   }
            });


            jQuery('.login-triger').click(function () {
                var matchText = "There is already an account with this email address."
                var emailMatchText = jQuery('#signup-popup-show .validation-advice').text();
                var signupEmail = jQuery('#email_address').val();
                var loginEmail = jQuery('#login-email');


                jQuery('#newLogin').removeClass('forgot-password');
                if (emailMatchText == matchText) {
                    loginEmail.val(signupEmail);
                }

                jQuery('#signup-popup-show').hide();
                jQuery('.ba-message h2').text('Heureux de vous revoir');
                jQuery('#newLogin').show();
                allowScroll();
                 if(!checkIEBrowser()){
                jQuery('#login-email').focus();
                   }
            });

            jQuery('#showLoginBox').click(function () {
                jQuery('#newLogin').removeClass('forgot-password');
                jQuery(this).attr('href', 'javascript:void(0)');
                jQuery('#newLogin').show(0, function () {
                    containerH = jQuery(this).children('.login-container').height();
                    checkHeight();

                });
                allowScroll();
                 if(!checkIEBrowser()){
                jQuery('#login-email').focus();
                   }


                var trustpilotWidget = jQuery('.trustpilot-message');
                if(trustpilotWidget.length !== 0) {
                    setTimeout(function(){
                        trustpilotWidget.find('.fa-spinner').remove();
                    }, 2000);
                }
            });
            jQuery('#showJoinBox').click(function () {
                jQuery('#newLogin').removeClass('forgot-password');
                jQuery(this).attr('href', 'javascript:void(0)');
                jQuery('#signup-popup-show').show(0, function () {
                    containerH = jQuery(this).children('.login-container').height();
                    checkHeight();

                });
                allowScroll();
                 if(!checkIEBrowser()){
                    jQuery('#email_address').focus();
                   }
                //jQuery('#newLogin').show();
            });
            jQuery('.ba-logo .fa-close').click(function () {
                jQuery('.login-overlay').hide();
            });

            if(achicaData.hasOwnProperty('achica_user_found') && window.universal_variable.user.logged_in != true){
                if(achicaData.achica_user_found == true && achicaData.ba_user_found == true){
                   //User exists in Achica and BA both platforms, not logged in
                    $j('#login-email').val(achicaData.achica_email);
                        login_box.show(0, function () {
                            containerH = $j('#trigger-login-popup').children('.login-container').height();
                            checkHeight();

                        });
                         if(!checkIEBrowser()){
                jQuery('#login-email').focus();
                   }
                    $j('#newLogin').addClass('acba');
                    allowScroll();
                } else if(achicaData.achica_user_found == false && achicaData.ba_user_found == false){
                    //User is not Achica or in Achica but we can't authenticate, not logged in
                    if(achicaData.achica_email != null){
                        $j("#email_address").val(achicaData.achica_email);
                    } else {
                        $j("#email_address").val('');
                    }
                    $j('#signup-popup-show').show();
                    $j('#signup-popup-show').addClass('acba');
                }

            } else if(achicaData.hasOwnProperty('achica_guest') && achicaData.achica_guest == true && window.universal_variable.user.logged_in != true) {
                jQuery('.login-overlay').addClass('acba-no-user');
                jQuery('#signup-popup-show').show();
                allowScroll();
                 if(!checkIEBrowser()){
                    jQuery('#email_address').focus();
                   }
            }            

            if(window.universal_variable.cocosa_referrer == '1' || window.universal_variable.mysale_referrer == '2' || window.universal_variable.ba_ac == '1'){
                $j('.ba-logo').addClass('ba-logos');
            } else {
                $j('.ba-logo').removeClass('ba-logos');
            }        
  
            if(window.universal_variable.ba_ac == '1'){
                $j('.acba-message').show();
                $j('.achica-logo').addClass('show-logo');
            }else{
                $j('.acba-message').hide();
                $j('.achica-logo').removeClass('show-logo');
            }


            if(window.universal_variable.cocosa_referrer == '1'){
                $j('.cocosa-logo').addClass('show-logo');

                $j('.chester-type').html("We're delighted to announce that Cocosa is now part of the BrandAlley family.<br><a class='underline' href=\"https://www.brandalley.fr/cocosa-brandalley/\" target='_blank'>More Information</a>");
            }else{
                $j('.chester-logo').removeClass('show-logo');
            }   
            
            if(window.universal_variable.mysale_referrer == '2'){
                $j('.chester-logo').addClass('show-logo');
                $j('.chester-type').text("We're delighted to announce that Mysale is now part of the BrandAlley family.");
            }else{
                $j('.chester-logo').removeClass('show-logo');
            } 
           
            if(window.universal_variable.cocosa_referrer == '1'|| window.universal_variable.mysale_referrer == '2'){
                $j('.chester-message').show();
                $j('.ba-message').hide();
            }else{
                $j('.chester-message').hide();
                $j('.ba-message').show();
            }
        });


    </script>
