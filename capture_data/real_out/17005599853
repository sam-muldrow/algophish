<script type="text/javascript">
     //<![CDATA[
     var dataValidateFormMain = new VarienForm('form-validate', true);
     var urlToValidate = 'https://www.brandalley.fr/flashsales/index/validateemail/';
     urlToValidate = urlToValidate.replace('https:', '');
     urlToValidate = urlToValidate.replace('http:', '');
     var emailval ='';

    jQuery(document).ready(function(){

        function getUrlParameter(name) {
            name = name.replace(/[\[]/, '\[').replace(/[\]]/, '\]');
            var regex = new RegExp('[\?&]' + name + '=([^&#]*)');
            var results = regex.exec(location.search);
            return results === null ? '' : decodeURIComponent(results[1].replace(/\+/g, ' '));
        };

        jQuery('.acba #join-now-btn-email, .acba .join-now-btn-pass, .acba .button.shop-now').text('Continue Shopping');
        if (jQuery('.acba').length > 0) {
            jQuery('.brandalley-messaging').hide();
        }


        jQuery("#join-now-btn-email").click(function(){
            emailval =  jQuery('#email_address').val();
            if(dataValidateFormMain.validator.validate()){
                validateEmailHost(urlToValidate, emailval, 'join-now-btn-email', 'message_signup');
                jQuery("#password").focus();
            } else{
                jQuery(".error_message").remove();
            }
            jQuery('#signup-error').parent('li').hide(500);
            return false;
        });   //validate enter

        jQuery('#email_address').keydown(function(event) {
            jQuery('.error_message').hide(100);
            emailval = jQuery('#email_address').val();
            if (event.which == 13 && !event.shiftKey && emailval != 'undefined') {
                if(Validation.validate('email_address')){
                    validateEmailHost(urlToValidate, emailval, 'join-now-btn-email', 'message_signup');
                } else {
                    jQuery(".error_message").remove();
                }
                return false;
            }
        });

        jQuery("#password").on('change keydown paste input', function(){
        var passtoconf =  jQuery('#password').val();
            jQuery('#confirmation').val(passtoconf);
        });

        jQuery('.step2 .join-now-unsubscribed').on('click', function() {
            if (!dataValidateFormMain.validator.validate()) {
                return false;
            } else {
                jQuery('#pure360_daily_emails').val('0');
                registerAjax(jQuery(this));
            }
        });
        jQuery('.step2 .join-now-subscribed').on('click', function() {
            jQuery('#pure360_daily_emails').val('1');
            registerAjax(jQuery(this));
        });

        jQuery('.pslogin-button-link.facebook').on('click', function(e) {
            e.preventDefault();
            jQuery('.step1, .step2').hide();
            jQuery('.step-fb').show();
        });

        jQuery('.facebook .pslogin-button-link').on('click', function(e) {
            jQuery('#pure360_daily_emails').val('1');
        });

        jQuery('.fake-fb').on('click', function(e) {
            var date = new Date();
            date.setTime(date.getTime() + (1*24*60*60*1000));
            expires = "; expires=" + date.toUTCString();
            document.cookie = "remove_dm=" + "1"  + expires + "; path=/";
            jQuery('#pure360_daily_emails').val('0');
        });
        
        jQuery('.pslogin-button-auto').on('click', function(e) {
            var date = new Date(); //Create an date object
            date.setTime(date.getTime() - (1000*60*60*24)); 
            expires = "; expires=" + date.toUTCString();
            document.cookie = "remove_dm=" + "1"  + expires + "; path=/";
        });        
});

/* ================ Ajax register start ================== */

    function registerAjax(button) {
        var postUrl = 'https://www.brandalley.fr/customer/account/createpostajax/';
        postUrl = postUrl.replace('https:', '');
        postUrl = postUrl.replace('http:', '');
        if (!dataValidateFormMain.validator.validate()) {
            return false;
        } else {
            jQuery('#email_address').val(emailval);
            button.prop("disabled", true);
            button.text('Loading...');
            jQuery('#signup-error').parent('li').hide(500);
            jQuery.ajax({
                url: postUrl,
                type: "POST",
                data: jQuery('#form-validate').serialize(true),
                success: function(result) {
                    var successUrl = 'https://www.brandalley.fr/flashsales/index/thankyoupopup/';
                    successUrl = successUrl.replace('https:', '');
                    successUrl = successUrl.replace('http:', '');
                    var signupStatus = result;
                        jQuery.get(successUrl, function(data) {
                        var responseData = jQuery.parseJSON(data);
                        if(responseData['flag'] == 1){
                            popupFlag = 0;

                            if(achicaData.achica_user_found == false && achicaData.ba_user_found == false){

                                jQuery('#newLogin').removeClass('acba');
                                jQuery('#newLogin').hide();
                                // jQuery('#thankyou-message').show();
                                // jQuery('#thankyou-message').addClass('acba');
                                // jQuery('#thankyou-message.acba .acba-message.register-thank-you').show();

                            } else {
                                jQuery('#signup-popup-show').hide();
                                //jQuery('#thankyou-message').show();
                            }
                            window.universal_variable.user.user_id = responseData['user_id'];
                            window.dataLayer = window.dataLayer || [];
                            window.dataLayer.push({ 'event': 'thanks-for-registering' });
                            location.reload();
                        } else {
                            jQuery('.join-btn-text').html('Create Account');
                            var result = jQuery.parseJSON(signupStatus);
                            if (result['flag'] == 0) {
                                var errorMsg = result['error'].toString().split(',');
                                var errorHtml = '';
                                jQuery.each(errorMsg, function (n, message) {
                                    if (errorHtml.length > 0) {
                                        errorHtml = errorHtml + '<span>' + message + '</span>';
                                    } else {
                                        errorHtml = '<span>' + message + '</span>'
                                    }
                                });
                                jQuery('#signup-error').html(errorHtml);
                                jQuery('.joinNowFormPass').hide();
                                jQuery('.joinNowFormEmaill').show();
                                jQuery('#signup-error').parent('li').show(500);
                            }
                        }
                    });
                }
            }).done(function () {
                jQuery('.join-now-btn-pass').prop("disabled", false);
            });
        }
    }

    jQuery("#form-validate").submit(function(e) {
        e.preventDefault();
        e.stopImmediatePropagation();
        registerAjax();
    });
    //]]>
</script>
</div>
</div>
</div>
</div>
</div>
<script type="text/javascript">
        //<![CDATA[
        var loginForm = new VarienForm('login-form-pop');
        var urlToValidate = 'https://www.brandalley.fr/flashsales/index/validateemail/';
        urlToValidate = urlToValidate.replace('https:', '');
        urlToValidate = urlToValidate.replace('http:', '');

        /* ================== New Ajax login script start ===================== */
        var loginPostUrl = 'https://www.brandalley.fr/customer/account/loginpostajax/';
        loginPostUrl = loginPostUrl.replace('https:', '');
        loginPostUrl = loginPostUrl.replace('http:', '');
        function loginAjax() {
            jQuery('#popup-error').parent('li').hide();
            var errorHtml = '';
            if (!loginForm.validator.validate()) {
                return false;
            } else {
                jQuery('#send2').prop("disabled", true);
                jQuery('#login-btn-text').html('Chargement...');
                jQuery.ajax({
                    url: loginPostUrl,
                    type: "POST",
                    data: jQuery('#login-form-pop').serialize(true),
                    success: function (result) {
                        var result = jQuery.parseJSON(result);
                        if (result['flag']) {

                            //User exists in Achica and BA both platforms,after successful login
                            if(achicaData.hasOwnProperty('achica_user_found') && achicaData.achica_user_found == true && achicaData.ba_user_found == true){
                               jQuery('#newLogin').removeClass('acba');
                               jQuery('#newLogin').hide();
                               setTimeout(function(){ window.location.reload() }, 5000);
                            }else{

                               if(result['flag'] == 2 || result['flag'] == 3){
                                   //Direct login from popup and customer exists in BA OR direct login from popup and customer didn't exist in BA earlier
                                    jQuery('#newLogin').removeClass('acba');
                                    jQuery('#newLogin').hide();
                                    setTimeout(function(){ window.location.reload() }, 5000);
                               }else {
                                    window.location.reload();
                                }
                            }

                        } else {
                            var errorMsg = result['error'].toString().split(',');

                            if (typeof errorMsg  !== "undefined" && errorMsg!= ''){
                                jQuery.each(errorMsg, function (n, message) {
                                    if (errorHtml.length > 0) {
                                        errorHtml = errorHtml + '<span>' + message + '</span>';
                                    } else {
                                        errorHtml = '<span>' + message + '</span>';
                                    }
                                });
                                jQuery('#popup-error').html(errorHtml);
                                jQuery('#popup-error').parent('li').show();
                            }
                        }
                    }, error: function(XMLHttpRequest, textStatus, errorThrown) {
                            errorHtml = '<span>' + errorThrown + '</span>';
                            jQuery('#popup-error').html(errorHtml);
                            jQuery('#popup-error').parent('li').show();
                        }
                }).done(function () {
                    jQuery('#login-btn-text').html('Connectez-vous');
                    jQuery('#send2').prop("disabled", false);
                });
            }
        }
        jQuery("#login-form-pop").submit(function (e) {
            e.preventDefault();
            e.stopImmediatePropagation();
            loginAjax();
        });

        var resetForm = new VarienForm('new-password-form');
        //]]>

        function showBox(eventTrigger) {
            if (eventTrigger == 'login') {
                jQuery('#signup-popup-show').hide();
                jQuery('#newLogin').show(0, function () {
                    containerH = jQuery(this).children('.login-container').height();
                    checkHeight();

                });
                allowScroll();
                 if(!checkIEBrowser()){
                jQuery('#login-email').focus();
                   }
            } else if (eventTrigger == 'signup') {
                jQuery('#newLogin').hide();
                jQuery('#signup-popup-show').show(0, function () {
                    containerH = jQuery(this).children('.login-container').height();
                    checkHeight();

                });
                allowScroll();
                 if(!checkIEBrowser()){
                    jQuery('#email_address').focus();
                   }
            }
        }


    jQuery("#register-thanks-button").unbind('click').on('click', function () {
        if(  achicaData.hasOwnProperty('achica_user_found') && achicaData.achica_user_found == true){
            jQuery(this).html('Loading...');
            var registerThanksContinueUrl =    'https://www.brandalley.fr/achicalanding/incoming/continue/';
            jQuery.get(registerThanksContinueUrl, function(result) {
                var result = jQuery.parseJSON(result);
                if (result['flag'] == 1 && result['user_id'] > 0) {
                      popupFlag = 0;
                      window.universal_variable.user.user_id = result['user_id'];
                      window.dataLayer = window.dataLayer || [];
                      window.dataLayer.push({ 'event': 'thanks-for-registering' });
                }
                window.location.reload();
            })
        } else {
            window.location.reload();
        }
    })

function showPopup(){
    $j('#trigger-login-popup').trigger('click');
    allowScroll();
    return false;
}
    </script>

