                        <div role="tooltip" id="" class="tooltip" aria-hidden="true">
                            <button class="fi-nav-close" aria-hidden="true" tabindex="-1"></button>
                            <p class="tooltip-text" data-product-sku="{{featureSku}}" data-display-name="{{productName}}">{{productName}} Does not include the feature </p>
                        </div>
                        {{/ifeq}}
                    </div>
                    {{/if}}
                {{/each}}
                {{/if}}
            </div>
        </li>
        {{/each}}
    {{/if}}
    </ul>
</script>

<script id="disclaimer-template" type="text/x-handlebars-template">
    <ul>
    {{#if features}}
        {{#each features}}
        <li class="disclaimer-item">
            {{{featureDisclaimer}}}
        </li>
        {{/each}}
    {{/if}}
    </ul>
</script>


    

