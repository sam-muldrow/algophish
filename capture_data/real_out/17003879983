                                <div class="main-price hidden {{handleAssociatedProduct product.associatedSecondaryPremium.inCart 'remove'}}">{{formatPrice product.associatedPremium.pricing.salePriceWithQuantity}}</div>
                            </div>
                        </div>
                        {{/if}}
                    </div>
                    {{/if}}
                    {{/if}}

                </div><!--/* end right-part */-->
            </div><!--/* end anon div */-->

            {{#if product.bundledProduct}}
            {{#each product.associatedBundledProducts}}
            {{#ifeq product.productType 'PHYSICAL'}}
            {{#if product.thumbnailImage}}
            <figure data-line-type="subordinate"><img src="{{product.thumbnailImage}}" /></figure>
            {{else}}
            <div class="left-part"></div>
            {{/if}}
            {{/ifeq}}
            {{#ifnoteq product.productType 'PHYSICAL'}}
            <div class="left-part"></div>
            {{/ifnoteq}}
            <div>
                <div class="right-part">
                    <!--/* Bundled Products */-->
                    <div class="info-price-wrapper associated-product{{#if ../product.bundledProduct.inCart}} product-in-cart{{/if}}"
                        data-line-type="subordinate"
                        data-bundle-line-id="{{../product.bundledProduct.bundleID}}"
                        data-fpp-line-id="{{../product.associatedProtectionPlan.lineItemId}}"
                        data-sku="{{../product.bundledProduct.bundleSkus}}">
                        <div class="primary-product bundled"
                            data-sku="{{sku}}" >
                            <div class="product-info content" data-product-type="bundle">
                                <span class="line-item--name">{{{ displayName }}}</span>, {{#if pricing.salePriceWithQuantity}}{{formatPrice pricing.salePriceWithQuantity}}{{else}}{{formatPrice pricing.listPriceWithQuantity}}{{/if}}
                                {{#ifnoteq product.productType 'OTHER' }}
                                <div class="stock available">{{ formatInventoryMessage ../parent.inventoryMessages product ../parent.authoring.inStockLabel }}</div>
                                {{/ifnoteq}}
                                <div class="pro-content">
                                    {{#if cartBullets}}

                                    <div class="in-box" data-plan-price="{{formatPrice pricing.listPriceWithQuantity}}">{{#ifeq product.productType 'PHYSICAL'}}What&#39;s Included{{/ifeq}}{{#ifnoteq product.productType 'PHYSICAL'}}See Details{{/ifnoteq}}</div>
                                    <ul class="box-content" hidden>
                                        {{#each cartBullets}}
