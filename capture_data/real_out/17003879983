    <script id="fitbit-pricing-partial" type="text/x-handlebars-template">
        {{#if parent.fhslineItem}}
            <div class="offer-price">{{formatPrice parent.pricing.salePriceWithQuantity }}</div>
            <div class="main-price in-offer"
                 data-price="{{getFhsBasePrice parent.customAttributes false}}">
                {{getFhsBasePrice parent.customAttributes true}}
            </div>
        {{else}}
            {{#isOnSale parent.pricing.salePriceWithQuantity parent.pricing.listPriceWithQuantity }}
                {{#hasReferencePrice parent.pricing.referencePriceWithQuantity parent.pricing.salePriceWithQuantity}}
                    {{#isReferencePriceLower parent.pricing.referencePriceWithQuantity parent.pricing.salePriceWithQuantity}}
                        <div class="main-price"
                             data-price="{{parent.pricing.salePriceWithQuantity.value}}">
                            {{formatPrice parent.pricing.salePriceWithQuantity }}
                        </div>
                    {{else}}
                        <div class="offer-price">{{formatPrice parent.pricing.salePriceWithQuantity }}</div>
                        <div class="main-price in-offer"
                             data-price="{{parent.pricing.referencePriceWithQuantity.value}}">
                            {{formatPrice parent.pricing.referencePriceWithQuantity }}
                        </div>
                    {{/isReferencePriceLower}}
                {{else}}
                    <div class="offer-price">{{formatPrice parent.pricing.salePriceWithQuantity }}</div>
                    <div class="main-price in-offer"
                         data-price="{{parent.pricing.listPriceWithQuantity.value}}">
                        {{formatPrice parent.pricing.listPriceWithQuantity }}
                    </div>
                {{/hasReferencePrice}}
            {{else}}
                <div class="main-price"
                     data-price="{{parent.pricing.listPriceWithQuantity.value}}">
                    {{formatPrice parent.pricing.listPriceWithQuantity }}
                </div>
            {{/isOnSale}}
        {{/if}}
    </script>

    
