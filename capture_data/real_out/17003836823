<script>
  window.addEventListener('load', () => {
    const tosKey = 'digg-tos-2023-10';
    const userAgreed = localStorage.getItem(tosKey);
    const tosDialog = document.querySelector(`#${tosKey}`);

    if (tosDialog) {
      if (!userAgreed) {
        const acceptButton = tosDialog.querySelector('.accept');
        tosDialog.classList.remove('hidden');
        acceptButton.addEventListener('click', (e) => {
          localStorage.setItem(tosKey, '1');
          tosDialog.remove();
        });
      }
    }
  });
</script>

