    <script>
window.googletag.cmd.push(function() {
  if(window.client_data.page_siteLayout !== 'Mobile') return;
  googletag.pubads().addEventListener('slotRenderEnded', function(e) {
    var slot = e.slot,
      res = slot.getResponseInformation();
    if (res && res.advertiserId == 4645279873) {
      BOOMR.hit('adhesion:rendered');
    }
  });
  googletag.pubads().addEventListener('slotOnload', function(e) {
    var slot = e.slot,
        res = slot.getResponseInformation(),
        closed = false;
    if (res && res.advertiserId == 4645279873) {
      var consumable = 0;
      function adhesionMonitor() {
        consumable++;
        var f = $('.cnsmbl-audio-320x50-slider iframe')[0];
        if (!f && !closed) {
          closed = true;
          BOOMR.hit('adhesion:closed');
        }
        if (f && consumable === 10) {
          BOOMR.plugins.FrameTiming.sendBeaconForFrame(
            f.contentWindow,
            'adhesion'
          );
        }
        if(consumable <= 600)
            setTimeout(adhesionMonitor, 1000);
      }
      setTimeout(adhesionMonitor, 1000);
    }
  });
});
</script>
 
     


