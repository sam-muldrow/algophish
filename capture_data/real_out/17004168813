            function reportABTests() {
                var ab_test = "";

                // Quazi-automatically add newer experiments (true=readonly, false=set variation if not set)
                const reportableExperiments={'upsell-premium-gifting-remove':true, 'PB_checkout':true, 'unlimited_guest_51_to_750': true, 'exp-q3_2023_invitation_pricing': true};
                for (const name in reportableExperiments) {
                    const value = window.evite.experiments.getOrSetVariation(name, null, reportableExperiments[name]);
                    if (value) ab_test += "|" + name + "_" + value;
                }
                console.log("AB_TEST", ab_test);
                window.dataLayer.push({ ab_test: ab_test });
            }
            reportABTests();
        </script>
    
    
