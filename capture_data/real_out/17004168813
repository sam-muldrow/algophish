    <script>
window.BOOMR = window.BOOMR || {};
if(!window.BOOMR.hit) window.BOOMR.hit = function() {};

// disable segment checking
client_data.segments_checked = new Date().getTime();
window.CLIENT_TRACKING_TIMEOUT = 600000;

evite.when('evite').then(function(evite) {
  Raven.setDataCallback(function(data) {
    if (evite && evite.dfp && evite.dfp.renderedSlots) {
      data.tags = data.tags || {};
      for (var i = 0; i < evite.dfp.renderedSlots.length; i++) {
        var slot = evite.dfp.renderedSlots[i];
        var req = slot.getResponseInformation();
        if (!req) continue;
        data.tags['advertiser_' + slot.getSlotElementId()] = req.advertiserId;
      }
      data.tags.mobile = evite.detects.isMobile + '';
      if (window.client_data) {
        data.tags.event_id = window.client_data.event_id;
        data.tags.pagegroup = window.client_data.pagegroup;
      }
    }
  });
  Raven.setBreadcrumbCallback(function(c) {
    return c.category === 'xhr' ? false : c;
  });
  Raven.setShouldSendCallback(function(c) {
    if (c.tags && c.tags.pagegroup && c.tags.pagegroup.indexOf('create') > -1)
      return true;
    if (c.culprit) return false;
    try {
      return (
        c.culprit.indexOf('evitecdn.com') > -1 ||
        c.culprit.indexOf('www.evite.com') > -1 ||
        c.culprit.indexOf('evite-services-prod') > -1 ||
        c.culprit.indexOf('evitelabs.com') > -1
      );
    } catch (e) {
      evite.warn('culprit not found');
      return false;
    }
  });
});
evite.when('dfp').then(function(dfp) {
  googletag.cmd.push(function() {
    googletag.pubads().addEventListener('slotRenderEnded', function(event) {
      var slot = event.slot;
      if (!slot) return;
      var response = slot.getResponseInformation();
      response &&
        Raven.captureBreadcrumb({
          message: slot.getSlotElementId(),
          category: 'ads',
          data: response,
        });
    });
  });
});
</script>
