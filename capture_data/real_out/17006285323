<script>
    document.addEventListener("DOMContentLoaded", function(event) {
        let urlCurrent = $(location).attr("href"),
            pattSkyHome = new RegExp("^http[s]?://[wwwcms]+[-devhml]*.sky.com.br[/]?[ptweb/]*[/sky]*[/]?$"), //Site SKY
            res = pattSkyHome.test(urlCurrent);
        if(res){
            require_acc(['home-cards'])
        }else{
            require_acc(['modal-pre-page'])
        }
    })
