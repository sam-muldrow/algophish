<script>
    var signatureType = 'Pós Pago',
        cardsGroupId = '',
        cardsFolderId = '',
        BucketUrl = '';
    document.addEventListener("DOMContentLoaded", function(event) {         
            cardsGroupId = folderInfos.groupId;
            switch(signatureType) {
                case 'Pré Pago':
                    cardsFolderId = folderInfos.pre;
                    BucketUrl = cardsPreUrl;
                    break;
                case 'Pós Pago':
                    cardsFolderId = folderInfos.pos;
                    BucketUrl = cardsPosUrl;
                    break;
                case 'Only Banda Larga':
                case 'Bundle Banda Larga':
                    cardsFolderId = folderInfos.banda;
                    BucketUrl = cardsBdlUrl;
                    break;
                default:
                    cardsFolderId = folderInfos.pre;
                    BucketUrl = cardsPreUrl;
                    break;
            }

        function ativadorSubmenu(){
            $('#home-cards .filter-cards').find('a').map(function(){
                var dataAtivador = $(this).data(),
                    ativador = dataAtivador.ativador ? dataAtivador.ativador : '/';
                if(ativador != ''){
                    if(location.origin.indexOf('cms') != -1){
                        if(location.pathname.replace('/web/sky','') == "" && ativador == '/'){
                            $('#home-cards .filter-cards').find('a').removeClass('active');
                            $(this).addClass('active');
                        }else if(location.pathname.replace('/web/sky','').indexOf(ativador) != -1){
                            $('#home-cards .filter-cards').find('a').removeClass('active');
                            $(this).addClass('active');
                        }
                    }else{
                        if(location.pathname.indexOf(ativador) != -1){
                            $('#home-cards .filter-cards').find('a').removeClass('active');
                            $(this).addClass('active');
                        }
                    }
                }

            })
        }       
        ativadorSubmenu();

        require_acc(['home-cards']);
    })   
</script>
