<script>
	//temporary to leave the homepage container version
	document.addEventListener('DOMContentLoaded', function () {

		var leaveContainerLinks = Array.prototype.slice.call(
			document.querySelectorAll('.leave-container-links')
		);


		leaveContainerLinks.forEach(function (el) {
			el.addEventListener('click', function (e) {
				e.preventDefault();
				Cookie.deleteCookie('use_container');
				window.location.reload();
			});
		});

	});

</script>




