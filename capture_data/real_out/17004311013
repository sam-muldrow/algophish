<script src="https://cdn5.vscdns.com/assets/min/js/dm_app_2023_07_18_01.js"></script>


<script>

window.DMbillingPageLink = "https://hotchatdate.com/click-track.php?product=vip&amp;dhd_page=FullPay&amp;model_id=0&amp;pa_id=0&amp;pg=0&amp;sitekey=whitelabel&amp;test_group=GROUP_A";

</script>
<script type="text/javascript">
    

        var launchBtns = document.querySelectorAll('.dm-launch');
        launchBtns = Array.prototype.slice.call(launchBtns);

        //  If there were buttons to launch the app on the same page as the app, the 'mode' is 'onpage'.
        //  If no buttons, the mode is 'popout'.

        if(launchBtns.length !== 0) {

            //  'ONPAGE' mode:
            //  only user_type 'customer' can have an onpage mode.
            //  In 'onpage' mode, the app gets initialzed on
            //  page load, because we need to establish config vars.
            //  But then:
            //  if we are in a room, set loading state and hold off until the room is
            //  finished and done firguring the connection out.
            //  If not in room, proceed with on page load, same as the 'popout' mode.


            //  On page load, need to decide whether or not to start the DM app,
            //  based on user's preference for their online status
            document.addEventListener(
                'DOMContentLoaded',
                function() {
                    //  Init only once per session - we need to init on page load because we need to establish the config vars right away to make a decision
                    //  whether or not to connect after page load
                    if(!DMMessenger.initialized) {

                        DMMessenger.init({
                            'is_dev': false,
                            'is_internal': 0,
                            'mode': 'onpage',
                            'user_type': 'customer',
                            'customer_type': 'guest',
                            'user_platform': 'computer',
                            'screen_name': '',
                            'dm_key': '',
                            'in_chat': 'true',
                            'online_status': 'offline',
                                                        'cdn': 'https://cdn5.vscdns.com'                        });
                    }

                    //  We do not always launch the app right from the start

                    if(DMMessenger.config.in_chat) {


                        //  In chat, we want to wait until we re connected to a room,
                        //  or decided to show a model offine view before launching the DM messenger, so subscribe to 2 events for those:
                        DOM.addEventListener(
                            document,
                            CHAT_EVENT.ROOM_CONNECTED,
                            function(ev) {

                                DMOnPageModeController.onChatRoomSwitched(ev.detail);
                            }
                        );

                        DOM.addEventListener(
                            document,
                            CHAT_EVENT.ROOM_NOT_CONNECTED_OFFLINE,
                            function(ev) {

                                DMOnPageModeController.onChatRoomSwitched(ev.detail);
                            }
                        );

                        //  In the case when the chat room loads on a homepage (no specific room),
                        //  we need to know about that, but we cannot rely on an event from chat cod ein this case because that would be dispatched before we got to this point and got a chance to subscribe to it, so check for 'model=' in the url instead. If that isn't found, there won't be a room

                        var modelNameParam = DMOnPageModeController.isModelInUrl();

                        //  If there is no 'model=' in the url on page load, then we are on the homepage
                        //  and don't need to wait for the above 2 events from the chat room to launch the app
                        //  (but we first check if user wants the app to launch on page load)
                        if(!modelNameParam) {
                            DMOnPageModeController.decideWhetherToLaunchInChat();
                        }


                    } else {

                        DMOnPageModeController.decideWhetherToLaunchOutsideChat();
                    }

                }
            );


            //  Add event listener for clicking on the launch buttons
            launchBtns.forEach(function(launchBtn) {

                launchBtn.addEventListener(
                    'click',
                    DMOnPageModeController.onLaunchBtnClicked.bind(DMOnPageModeController)
                );
            });

        } else {

        //  DM Messenger launched in 'POPOUT' mode:
        //  Will always init and start on page load.

            document.addEventListener(
                'DOMContentLoaded',
                function() {

                    DMMessenger.init({
                        'is_dev': false,
                        'is_internal': false,
                        'mode': 'popout',
                        'customer_type': 'guest',
                        'user_platform': 'computer',
                        'cdn': 'https://cdn5.vscdns.com'                    });

                    window.atThisSpotInDm = true;

                    //  If for whatever reason, the app never initialized (e.g. usertype is 'customer' and
                    //  customer type is Guest or Basic), then don't start the app'
                    if(DMMessenger.initialized) {

                        DMMessenger.launch();
                    }
                }
            );

        }

</script>





