    //	Save the current value
    var windowOnLoad	= window.onload;

    //	Otherwise, build a new function that encompases the old value
    //	and adds the new function call.
    window.onload	= function() {

        if(windowOnLoad) {

            windowOnLoad();

        }

        fnct();
    }
}





