<script>
    $(document).ready(function() {
        //$('input[type=file]').bootstrapFileInput();
        $('#advoptn_c').click(function() {
            $('#advoptn_c').hide('fast');
            $('#advoptn').slideDown('slow');
        });
        
        $('#show').click(function(e) {
            e.preventDefault();
           $(this).slideUp('fast');
            $('#guestinfo').slideDown('slow');            $('#uploader').slideDown('slow');
        });
        
    });
</script>
<script type="text/javascript">
firstUp = false;
created = [];
numFiles = 0;
finished = [];

function updateHtml(id, data) {
    if (created.indexOf(id) === -1) {
        created.push(id);
        $('#progress').append('<div id="file_' + id + '"><strong>' + data.file_name + '</strong> - <span class="text-success">' + data.file_state + '</span></div>');
        $('#progress_row').slideDown('slow');
    } else {
        $('#file_' + id + ' strong').text(data.file_name);
        if (typeof data.progress !== "undefined") {
            $('#file_' + id + ' span').text(data.file_state + ': ' + data.progress + '%');
        } else if(typeof data.url !== "undefined") {
            $('#file_' + id + ' span').html(data.file_state + '! Watch it <a href="' + data.url + '">here</a>');
        } else {
            $('#file_' + id + ' span').text(data.file_state);
        }
    }
}

function updateProgress() {
     $.get('http://www.videowood.tv/poll-%253D%253D', function (response) {
        if (response.status === 'ok') {
            if(response.finished === 1) allFinished = true;
            $.each(response.data, function (index, value) {
                if(finished.indexOf(index) === -1) {
                    updateHtml(index, value);
                    if (value.file_state === 'Ready') finished.push(index);
                }
            });
        }
    });

    if(finished.length < numFiles) setTimeout(updateProgress, 2000);
}

$(document).ready(function() {

$(function() {
    $("#uploader").pluploadQueue({
        // General settings
        runtimes : 'html5,flash',
        url : 'http://upl.videowood.tv',
        max_file_size : '5G',
        //chunk_size: '1mb',
        resize : {
            width : 200,
            height : 200,
            quality : 90,
            crop: true // crop to exact dimensions
        },
        filters : {
            mime_types: [
                {title : "Video Files", extensions : "avi,rmvb,mkv,flv,mp4,wmv,mpeg,mpg,mov"},
                {title : "Subtitle Files", extensions : "srt"}
            ],
            prevent_duplicates: true
        },
        rename: true,
        sortable: true,
        dragdrop: true,
        views: {
            list: true,
            thumbs: true, // Show thumbs
            active: 'thumbs'
        },
        flash_swf_url : 'http://rawgithub.com/moxiecode/moxie/master/bin/flash/Moxie.cdn.swf',
        multipart_params : {
            'email' : $('#email').val(),
            'upload_id': '%3D%3D'
        },
        init: {
            StateChanged: function(up) {
                numFiles = up.files.length;
                $('#upload_lower').slideUp('slow');
            },
            FileUploaded: function(up, file, info) {
                if(info.status === 200 && !firstUp) {
                    firstUp = true;
                    setTimeout(updateProgress, 1000);
                }
            },
            UploadComplete: function(up) {
                $('#uploader').delay(30000).slideUp('slow');
            }
        }
    });
});

   $('#submitremote').click(function(e) {
      e.preventDefault();
      
      $.post('http://www.videowood.tv/remote', $('#remoteuploadform').serialize(), function(response) {
          if(response.status === 'error') {
              $('#remoteerror').text(response.msg);
              $('#remoteerrordiv').slideDown('slow').delay(10000).slideUp('slow');
          } else {
            $('#submitremote').prop('disabled', true);
            $('#remotestext').prop('disabled', true);
            $('#remotestext').val('');
              
              split = $('#remotestext').text().split("\n");   
              numFiles += split.length;
              
              $('#remotesuccess').text('Links have been added! Status may take up to 10 seconds to appear.');
              $('#remotesuccessdiv').slideDown('slow').delay(10000).slideUp('slow');
              
              if(!firstUp) {
                  firstUp = true;
                  setTimeout(updateProgress, 1000);
              }
          }
      });
   }); 
});
</script>
