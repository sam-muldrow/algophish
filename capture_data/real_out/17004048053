    jQuery(function () {
        jQuery(".barrier_free").click(function (e) {
            var agt = navigator.userAgent.toLowerCase();
            AssistIe = ((agt.indexOf('compatible') > -1 && agt.indexOf('msie') > -1) || (agt.indexOf("trident") > -1 && agt.indexOf("v:11.0") > -1));
            var browserCheck = new BrowserCheck();
            browserCheck.showWin();
