<script type="text/javascript">
    var canvas = document.createElement('canvas');
    var gl = canvas.getContext('experimental-webgl');
    var ig_flg = 0;//éææ¾å¡æ è¯
    if (gl != null) {
        var debugInfo = gl.getExtension('WEBGL_debug_renderer_info');
        gl.getParameter(debugInfo.UNMASKED_VENDOR_WEBGL);
        var glname = gl.getParameter(debugInfo.UNMASKED_RENDERER_WEBGL);
        //glname = "Basic Render Driver";
        if (glname.indexOf("Basic Render Driver") != -1) {
            ig_flg = 1;
            //jQuery(".m_banner_video").html("");
            //jQuery("#videodiv").css("display", "none");
            jQuery("#videodiv").remove();
            jQuery("#imgdiv").css("display", "block");
        }
    }
    function show() {
        //å¦ææ¯éææ¾å¡ï¼å³é­è§é¢æ­æ¾
        if (ig_flg == 1) {
            //jQuery(".m_banner_video").html("");
            //jQuery("#videodiv").css("display", "none");
            jQuery("#videodiv").remove();
            jQuery("#imgdiv").css("display", "block");
        }
        else {
            var myVideo = document.getElementById("video1");
            myVideo.play();
            jQuery("#videodiv").css("display", "block");
            jQuery("#imgdiv").css("display", "none");
        }
    }

</script>


    
