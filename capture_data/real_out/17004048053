        jQuery(".barrier_free").attr('id', "assist-open")
        jQuery(".nav a").hover(function (e) {
            /*å»æä¿¡ç¨å¡ææ*/
            //if (jQuery(this).index() == 2) {
            //    return;
            //}
            jQuery('.nav_bar_container .nav_menu_collapse').eq(jQuery(this).index()).show();
            jQuery(".nav a").eq(jQuery(this).index()).addClass('active');
        }, function (e) {
            jQuery('.nav_bar_container .nav_menu_collapse').eq(jQuery(this).index()).hide();
            jQuery(".nav a").eq(jQuery(this).index()).removeClass('active');
