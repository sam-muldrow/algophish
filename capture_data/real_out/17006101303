    <script>
        $j(function () {
            var items = $j(".right_big_banner .swiper-wrapper .swiper-slide");

            if (items.length > 1) {
                try {
                    items.sort(() => Math.random() - 0.5);
                    $j(".right_big_banner .swiper-wrapper").html(items);
                }
                catch (e) {
                    $j(".right_big_banner .swiper-wrapper").html(items);
                }
            }

            swiper_init(".right_big_banner", {
                loop: true,
                grabCursor: true,
                pagination: {
                    el: ".swiper-pagination",
                    type: "fraction",
                },
                navigation: {
                    nextEl: ".swiper-button-next",
                    prevEl: ".swiper-button-prev",
                },
                effect: "creative",
                creativeEffect: {
                    prev: {
                        shadow: false,
                        translate: ["0%", 0, -1],
                    },
                    next: {
                        translate: ["100%", 0, 0],
                    },
                },
            });
        });
    </script>
