        //jQuery(function () {
        //웰컴 탑 배너
        var bntabswiper = swiper_init(".welcome_top_tab", {
            slidesPerView: 9,
            watchSlidesProgress: true,
            speed: 100,
        });

        var topbnswiper = swiper_init(".welcome_top_bn", {
            //loop: true,
            thumbs: {
                swiper: bntabswiper
            },
            speed: 500,
            autoplay: {
                delay: 5000,
                disableOnInteraction: false
            },
            //grabCursor: true,
            effect: "creative",
            creativeEffect: {
                prev: {
                    shadow: false,
                    translate: ["0%", 0, -1],
                },
                next: {
                    translate: ["100%", 0, 0],
                },
            },
        });

        $j('.welcome_top_tab .swiper-slide').mouseover(function () {
            topbnswiper.slideTo($j(this).index());
        });

        $j('.welcome_top_bn, .welcome_top_tab').mouseover(function () {
            topbnswiper.autoplay.stop();
        });
        $j('.welcome_top_bn, .welcome_top_tab').mouseout(function () {
            topbnswiper.autoplay.start();
        });
        //});
    </script>
