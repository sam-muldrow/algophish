<script>
    var bottomswiper = swiper_init("#w_eventbanner", {
        slidesPerView: 3,
        slidesPerGroup: 3,
        spaceBetween: 11,
        loop: true,
        navigation: {
            nextEl: ".w_eventbanner_next",
            prevEl: ".w_eventbanner_prev",
        },
        on: {
            init: function (s) {
                if (s.slides.length < 4) {
                    $j(".w_eventbanner_next").hide();
                }
            }
        },
    });  
    bottomswiper.on('slideChange', function () {
        $j('.w_eventbanner_prev').addClass('active');
    });
</script>



