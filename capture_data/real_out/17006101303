	function headerTopReFormBanner(event) { reFormBanner("Wa_header1_headerTop"); }
	Event.observe(window, "load", headerTopReFormBanner);
	Event.observe(window, "resize", headerTopReFormBanner);

	try { setHeaderLayer(); }
