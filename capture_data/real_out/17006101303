<script>
    var midswiper = swiper_init("#w_adbanner", {
        slidesPerView: 3,
        slidesPerGroup:3,
        spaceBetween: 11,
        loop: true,
        navigation: {
            nextEl: ".w_adbanner_next",
            prevEl: ".w_adbanner_prev",
        },
        on: {
            init: function (s) {
                if (s.slides.length < 4) {
                    $j(".w_adbanner_next").hide();
                }
            },
        },
    });  
    midswiper.on('slideChange', function () {
        $j('.w_adbanner_prev').addClass('active');
    });
</script>


