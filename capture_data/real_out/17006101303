<script type="text/javascript">
    function fn_JinySwiperInit() {
        jinySwiper = swiper_init("#welcomeJiny", {
            slidesPerView: 5,
            slidesPerGroup: 5,
            spaceBetween: 30,
            loop: true,
            observer:true,
            navigation: {
                nextEl: ".w_jinylist_next",
                prevEl: ".w_jinylist_prev",
            },
        });
        jinySwiper.on('slideChange', function () {
            $j('.w_jinylist_prev').addClass('active');
            $j('.w_jinylist_next').addClass('active');
        });
    }
</script>

                
            
