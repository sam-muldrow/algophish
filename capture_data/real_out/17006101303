    if (getCookie("ala_w_t_event") != "hide")
        $j("#w_t_event").show();
    
    if (getCookie("ala_w_u_event") != "hide")
        $j("#w_u_event").show();
    
    function fn_hide_top_event(id)
    {
        setCookie('ala_'+id, "hide" , 1);
        $j("#"+id).hide();
    }
</script>

