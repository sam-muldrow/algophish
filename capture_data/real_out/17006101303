            $j('.editor_tab a').hover(function () {
                $j('.editor_tab a').removeClass('on')
                $j(this).addClass('on');
                $j('.editor_list').hide();
                $j('.editor_list').eq($j(this).index()).show();
                $j('.editor_btn div').hide();
                $j('.editor_btn div').eq($j(this).index()).show();
