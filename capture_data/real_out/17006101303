        <div class="search">
			

    <script type="text/javascript">
    
        $j(document).ready(function() {
        
            $j('#txtFooterSearchKeyword').keyup(function(e) {
                if (e.keyCode == 13) 
                    FooterSearchTool_Search();
            });
        
        });
    
       function FooterSearchTool_Search(){
            var txtFooterSearchKeyword = $j('#txtFooterSearchKeyword');

            var keyword = $j.trim(txtFooterSearchKeyword.val());

            if (keyword == "")
            {
                alert('검색어를 입력해주세요');
                txtFooterSearchKeyword.focus();
                return;
            }
       
            var FooterSearchTool1 = $j('.FooterSearchTool1:checked');
            
            if (FooterSearchTool1.length == 0)
                return;
       
            var url = '';
       
            if (FooterSearchTool1.val() == '1')
            {
                url = '/cs_center/wcs_faq_search_result.aspx?SearchWord=' + escape(keyword);
            }
            else if (FooterSearchTool1.val() == '2')
            {
                url = '/search/wsearchresult.aspx?SearchTarget=Talktalk&CustNoSearch=' + escape('알라딘고객센터')  + '@' + '738117195' + '&Keyword=' + escape(keyword);
            }
            else if (FooterSearchTool1.val() == '3')
            {
                url = '/search/wsearchresult.aspx?SearchTarget=Talktalk&CustNoSearch=' + escape('알라딘 이벤트 당첨자 발표')  + '@' + '763745136' + '&Keyword=' + escape(keyword);
            }
            else if (FooterSearchTool1.val() == '4')
            {
                url = '/search/wsearchresult.aspx?SearchTarget=Talktalk&CustNoSearch=' + escape('알라딘 서비스님의 서재')  + '@' + '716550157' + '&Keyword=' + escape(keyword);
            }
       
            if (url != '')
                location.href = url;
       
            
       
            /*
           var urlSearch = '/search/wsearchresult.aspx?SearchTarget=Talktalk&CustNoSearch=' + escape($('blog_search_blogname').value)  + '@' + $('blog_search_cno').value + '&Keyword=' + escape(keyword);
           location.href = urlSearch;
           */
           
           //javascript:Target_Set_Blog('공지사항', '공지사항' ,'738117195')
           
        }
    
    </script>

    
    <h3>검색</h3>
    
    <div class="field">
        <input type="text" id="txtFooterSearchKeyword" />
		<laebl for="txtFooterSearchKeyword" class="hidden">검색</laebl>
        <button type="button" class="search_btn" onclick="FooterSearchTool_Search();">검색</button>
    </div>
    <div class="option" style="display:block;">
        <label><input type="radio" value="1" name="FooterSearchTool1" id="FooterSearchTool1_1" class="FooterSearchTool1" checked="checked"><span>FAQ</span></label>
        <label><input type="radio" value="2" name="FooterSearchTool1" id="FooterSearchTool1_2" class="FooterSearchTool1"><span>공지사항</span></label>
        <label><input type="radio" value="3" name="FooterSearchTool1" id="FooterSearchTool1_3" class="FooterSearchTool1"><span>당첨자발표</span></label>
        <label><input type="radio" value="4" name="FooterSearchTool1" id="FooterSearchTool1_4" class="FooterSearchTool1"><span>서비스 안내</span></label>
    </div>
