	<div class="wide_bn_tab"><a href="#"><img src="https://image.aladin.co.kr/img/bn/book/2023/04/231115_november_layer_tab.gif" style="cursor:pointer" id="#w75" /></a><a href="#"><img src="https://image.aladin.co.kr/img/bn/book/2023/04/2023_vote_layer_tab.gif" style="cursor:pointer" id="#w74" /></a><a href="#"><img src="https://image.aladin.co.kr/img/bn/book/2023/04/231120_nature_layer_tab.gif" style="cursor:pointer" id="#w73" /></a><a href="#"><img src="https://image.aladin.co.kr/img/bn/book/2023/04/231120_faker_layer_tab.gif" style="cursor:pointer" id="#w72" /></a></div>    
<div id="wa_main_welcome1_wa_welcome_top1_wa_welcome_layer1__ctl0_pn_javascript">
	
<script type="text/javascript">
    var $j = jQuery.noConflict();
    var pop = $j("#WelcomePopLayer");
    var setTime = null;

    //$j(document).ready(function () {
        //pop = $j("#WelcomePopLayer");

    if (getCookie("ala_pop_layer") != "checked") {
        var bannerArray = $j(".wide_bn_tab img");
        var currDate = new Date();
        var startDate = new Date('2020-01-09 11:00:00');
        var endDate = new Date('2020-01-09 18:00:00');
        if (bannerArray.length > 0) {
            //console.log((endDate.getTime() - startDate.getTime()) / 60000);
            // 특정시간 동안 노출조건
            if (currDate.getTime() >= startDate.getTime() && currDate.getTime() < endDate.getTime()) {
                fn_show_w_pop($j(bannerArray.eq(3).attr("id")), false);
            }
            else {
                var rnd = Math.floor(Math.random() * (bannerArray.length));
                fn_show_w_pop($j(bannerArray.eq(rnd).attr("id")), false);
            }

        }
    }
    //});
    
    $j(document).ready(function () {
        $j(".wide_bn_tab img").click(function () {
            var banner = $j($j(this).attr("id"));

            if(pop.hasClass("show")) {
                if(banner.is(":hidden")) {
                    clearTimeout(setTime);
                    $j(".welcome_pop_img").hide();
                    banner.show();
                    setTime = setTimeout(fn_hide_w_pop,5000);
                } else {
                    fn_hide_w_pop();
                }
            }
            else {      
                
                fn_show_w_pop(banner, true);
                
            }
        });
    });
    
    function fn_show_w_pop(banner, IsAnimation) {
        clearTimeout(setTime);
        banner.show();
        if(IsAnimation)
            pop.addClass("show").show("slow");
        else
            pop.addClass("show").show();

        setTime = setTimeout(fn_hide_w_pop,5000);
    }
    
    function fn_hide_w_pop()
    {
        clearTimeout(setTime);
        pop.removeClass("show").hide("slow", function() {$j(".welcome_pop_img").hide();});
    }
    
    function ToggleChkPop()
    {
        setCookie( 'ala_pop_layer', "checked" , 1);
        fn_hide_w_pop();
    }
    
</script>

</div>
	
</div>


