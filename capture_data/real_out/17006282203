<script>
  Vue.component('regdialog', {
    template: "#regdialog",
    name: "regdialog",
    data: function () {
      return {
        dialogVisible: false,
        vercodetxt: "验证码",
        checked: false,
        verifCode: ms.base + "/api/code?t=" + new Date().getTime(),
        checked: false,
        puIcon: null,
        regform: {
          // 用户名
          peopleName: '',
          // 密码
          peoplePassword: '',
          puIcon: null,
          imPasswordAnswer:'',
          imPasswordQuestion:'',
          imPasswordQuestionOther:'',
        },
        optionsList:[
            {
                value: '我手机号码的后6位？',
                label: '我手机号码的后6位？'
            },
            {
                value: '我母亲的生日？',
                label: '我母亲的生日？'
            },
            {
                value: '我父亲的生日？',
                label: '我父亲的生日？'
            },
            {
                value: '我最好朋友的生日？',
                label: '我最好朋友的生日？'
            },
            {
                value: '我儿时居住地的地址？',
                label: '我儿时居住地的地址？'
            },
            {
                value: '我小学校名全称？',
                label: '我小学校名全称？'
            },
            {
                value: '我中学校名全称？',
                label: '我中学校名全称？'
            },
            {
                value: '离我家最近的医院全称？',
                label: '离我家最近的医院全称？'
            },
            {
                value: '离我家最近的公园全称？',
                label: '离我家最近的公园全称？'
            },
            {
                value: '我的座右铭是？',
                label: '我的座右铭是？'
            },
            {
                value: '我最喜爱的电影？',
                label: '我最喜爱的电影？'
            },
            {
                value: '我最喜爱的歌曲？',
                label: '我最喜爱的歌曲？'
            },
            {
                value: '我最喜爱的食物？',
                label: '我最喜爱的食物？'
            },
            {
                value: '我最大的爱好？',
                label: '我最大的爱好？'
            },
            {
                value: '我最喜欢的小说？',
                label: '我最喜欢的小说？'
            },
            {
                value: '我最喜欢的运动队？',
                label: '我最喜欢的运动队？'
            },
            {
                value: '9',
                label: '其他'
            }
        ]
      }
    },        
    computed: {            
        rules:function() {
            const that = this
            return{
                rePeoplePassword: [{
                    required: true,
                    message: '确认密码不能为空',
                    trigger: 'blur'
                },
                {
                    min: 6,
                    max: 30,
                    message: '长度在 6 到 30 个字符',
                    trigger: 'blur'
                },
                ],
                // 登录名
                peopleName: [{
                    "required": true,
                    "message": "登录名不能为空"
                },  {
                    "min": 2,
                    "max": 16,
                    "message": "登录名长度必须为2-16"
                }],
                // 登录密码
                peoplePassword: [{
                    "required": true,
                    "message": "登录密码不能为空"
                },  {
                    "min": 6,
                    "max": 16,
                    "message": "登录密码长度必须为6-16"
                }],

                imPasswordQuestion: [{
                    required: true,
                    message: '请选择问题',
                    trigger: 'blur'
                }],

                imPasswordAnswer: [{
                    required: true,
                    message: '请输入答案',
                    trigger: 'blur'
                }],

                imPasswordQuestionOther: [{ validator: function(rule, value, callback) {
                    if(that.regform.imPasswordQuestion==='9'){
                        if(!value){
                            return callback(new Error('请输入问题'))
                        }
                    }
                    callback()
                }, trigger: 'blur' }],
                
                rand_code: [
                    { required: true, message: '请输入验证码', trigger: 'blur' },
                    { min: 1, max: 4, message: '长度不能超过4个字符', trigger: 'change' }
                ],
            }
        } 
    },
    methods: {
      preview () {
        this.dialogVisible = true;
      },
      handleClose () {
        this.dialogVisible = false;
      },
      getcode () {
        this.verifCode = ms.base + "/api/code?t=" + new Date().getTime();
      },
      clickLogin () {
        this.$emit('showlogindialog');
      },
      clickReg () {
        if (!this.checked) {
          this.$notify({
            title: '提示',
            message: '请阅读并勾选《中国山东网网络服务使用协议》',
            type: 'warning'
          });
          return;
        }
        let that = this;
        that.$refs.form.validate((valid) => {
          if (valid) {
            var data = JSON.parse(JSON.stringify(that.regform));
            data.puIcon = that.puIcon;
            data.puRealName = that.regform.peopleName;
            if(data.imPasswordQuestion==='9'){
                data.imPasswordQuestion = data.imPasswordQuestionOther
            }
            ms.http.post(ms.base + "/api/register.do", data).then(function (res) {
              if (res.result != true) {
                that.saveDisabled = false;
                that.$notify({
                  title: '失败',
                  message: res.msg,
                  type: 'warning'
                });
              } else {
                that.$notify({
                  title: '成功',
                  message: '注册成功！请等待管理员审核',
                  type: 'success',
                  duration: '1000',
                  onClose: function () {
                    location.href = "https://www.sdchina.com/mdiyPage/sdong/login.do";
                  }
                });
              }
            }).catch(function (err) {
              console.log(err);
            });

          }
        });
      }
    },
    created: function () {

    },
    mounted: function () {

    },
    beforeDestroy: function () {

    }
  })
</script><script type="text/x-template" id="sd-header">
