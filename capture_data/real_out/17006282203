<script>
    Vue.component('sd-header', {
        template: "#sd-header",
        name: "sd-header",
        data: function () {
            return {
                isLogin: false,
                dataInfo:{}
            }
        },
        computed: {

        },
        methods: {
            clickLogin (showlogin) {
                if (showlogin) {
                    // this.$refs.logindialog.preview();
                    window.location.href = "https://www.sdchina.com//mdiyPage/sdong/login.do";
                } else {
                    // this.$refs.regdialog.preview();
                    window.location.href = "https://www.sdchina.com//mdiyPage/sdong/login.do";
                }
            },
            showregdialog () {
                this.$refs.regdialog.preview();
                this.$refs.logindialog.handleClose();
            },
            showlogindialog () {
                this.$refs.logindialog.preview();
                this.$refs.regdialog.handleClose();
            },
            requestInfo(){
                let that = this;
                ms.http.get(ms.base + "/api/people/user/info.do").then(function (res) {
                    if (res.data) {
                        that.dataInfo = res.data;
                    } else {
                        that.isLogin = false;
                    }
                }).catch(function (err) {
                    that.isLogin = false;
                });
            },
            loginout(){
                let that = this;
                ms.http.post('/api/loginOut.do',).then(function (data) {
                    that.isLogin = false;
                    window.location.reload()
                })
            },
            searchClick(){
                console.log('点击搜索');
                window.location.href = "https://www.sdchina.com/list/1631114318977486849.html";
               
            }
        },
        created: function () {

        },
        mounted: function () {
            gIsLogin(res => {
                this.isLogin = res
                if (res) {
                    this.requestInfo();
                }
            })
        },
        beforeDestroy: function () {

        }
    })
</script>
    <link rel="stylesheet" href="/template/1/sdong/css/index.css">
    <link rel="stylesheet" href="/static/plugins/swiper/4.5.3/swiper.min.css">
</head>

