<script src="/template/1/sdong/js/tab.js"></script>
<script src="/static/plugins/swiper/4.5.3/swiper.min.js"></script>
<script src="/static/plugins/jquery/3.2.1/jquery.min.js"></script>
<script>
    var app = new Vue({
        el: '#app',
        component() {},
        data: {
            searchForm: {
                keyword: ""
            },
            hotSearchKeywords: [],
            contents:[],
            pageSize:8,
            pageCur:3,
            liveList:[],
            liveList1:[],
            liveList2:[],
            liveList3:[],
            eleindex: 1,
            moreStr:'点击加载，了解更多资讯',
            loopData0: [
                { lanhuimage0: 'https://lanhu.oss-cn-beijing.aliyuncs.com/6891c035fe2a2e6d26e86a052001a9d2' },
                { lanhuimage0: 'https://lanhu.oss-cn-beijing.aliyuncs.com/2c741afbf0af225d247e9fe2ab645fcb' },
                { lanhuimage0: 'https://lanhu.oss-cn-beijing.aliyuncs.com/314dc133ff7d6249f7efb8a5532b7bab' }
            ],
        },
        watch: {},
        mounted () {
            g_registerTab("#tab_live_livebox", function () {
                g_tabCallback.livebox = {
                    tit: 'live',
                    box: 'livebox',
                    len: 2,
                    onchange: onTabHomeChange
                }
            });
            g_registerTab("#tab_lang_langbox", function () {
                g_tabCallback.langbox = {
                    tit: 'lang',
                    box: 'langbox',
                    len: 4,
                    onchange: onTabHomeChange
                }
            });
            g_registerTab("#tab_train_trainbox", function () {
                g_tabCallback.trainbox = {
                    tit: 'train',
                    box: 'trainbox',
                    len: 3,
                    onchange: onTabHomeChange
                }
            });
            g_registerTab("#tab_car_carbox", function () {
                g_tabCallback.carbox = {
                    tit: 'car',
                    box: 'carbox',
                    len: 4,
                    onchange: onTabHomeChange
                }
            });
            this.initSwiper();
            new RecCarousel({
                wrap:$('.channel-recommend-h-box'),
                stepTime:4000
            });
           // setAndGetLocale();
           checkSetLocale('https://www.sdchina.com/');
           
        },

        methods: {
          getListLinkUrl(item){
                if(item.type === 1){
                    if(item.outLink && item.outLink.length > 0){
                        return  item.outLink
                    }else {
                        return  '/list/1611278290396684290.html?liveId='+item.liveId
                    }

                } else if(item.type === 2){
                    if(item.outLink && item.outLink.length > 0){
                        return  item.outLink
                    }else {
                        return  '/list/1611278029427089409.html?liveId='+item.liveId
                    }
                }else if(item.type === 3){
                    if(item.outLink && item.outLink.length > 0){
                        return  item.outLink
                    }else {
                        return  '/list/1611278518084476930.html?liveId='+item.liveId
                    }
                }
            },
          enter: function (i) {
            console.log(i);
            this.eleindex = i;
            // if (this.eleindex === i) {
            //   this.eleindex = -1
            // } else {
            //   this.eleindex = i
            // }
          },
          getMulImgUrl(data) {
                let res = data ? JSON.parse(data) : [];
                return res.map((item) =>  item.img169 ||item.url);
            },
          getLiveList(){
                let that = this
                ms.http.post(ms.base +'/api/live/video/list.do', {
                    categoryId:'1611268152650670081',
                    pageSize: 10,
                    pageNo: 1,
                    orderType: 3,
                    contentType:'1C'
                }).then(function(data) {
                    if (data.data) {
                        that.liveList = data.data.rows||[]
                        that.$forceUpdate();
                    }

                })
                ms.http.post(ms.base +'/api/live/video/list.do', {
                    categoryId:'1611268152650670081',
                    pageSize: 10,
                    pageNo: 1,
                    orderType: 3,
                    contentType:'shouyezhibotuptuijian01'
                }).then(function(data) {
                    if (data.data) {
                        that.liveList1 = data.data.rows||[]
                        that.$forceUpdate();
                    }

                })
                ms.http.post(ms.base +'/api/live/video/list.do', {
                    categoryId:'1611268152650670081',
                    pageSize: 10,
                    pageNo: 1,
                    orderType: 3,
                    contentType:'shouyezhibotuptuijian02'
                }).then(function(data) {
                    if (data.data) {
                        that.liveList2 = data.data.rows||[]
                        that.$forceUpdate();
                    }

                })
                ms.http.post(ms.base +'/api/live/video/list.do', {
                    categoryId:'1611268152650670081',
                    pageSize: 10,
                    pageNo: 1,
                    orderType: 3,
                    contentType:'shouyezhibotuptuijian03'
                }).then(function(data) {
                    if (data.data) {
                        that.liveList3 = data.data.rows||[]
                        that.$forceUpdate();
                    }

                })
            },
            loadMore(){
                let that = this
                ms.http.post(ms.base +'/api/co/cms/content/list.do', {
                    typeid: '1605493032704892929',
                    content_type: "1C",
                    pageSize: that.pageSize,
                    pageNo: that.pageCur
                }).then(function(data) {
                    
                    if (data.data) {
                        const rows = data.data.rows||[]
                        rows.forEach(element => {
                            element.litpic = eval(element.litpic||'[]')
                        });
                        const filters = rows.filter(item=>!(that.contents.map(itemi=>itemi.id).includes(item.id)))
                        that.contents.push(...filters)
                        that.pageCur = (filters.length && filters.length >= that.pageSize) ? that.pageCur + 1 : that.pageCur
                        if (filters.length <= 0 || filters.length < that.pageSize) {
                            that.moreStr = "暂无更多资讯，稍后再来哦~";
                        }
                    }

                })
            },
            initSwiper () {
                var mySwiper = new Swiper('.swiper-container', {
                    loop: true,
                    autoplay: {
                    disableOnInteraction: false,
                    },
                    pagination: {
                    el: '.swiper-pagination',
                    renderBullet: function (index, className) {
                        let title = $('.swiper-slide').eq(index+1).data('title')
                        let link =  $('.swiper-slide').eq(index+1).data('link')
                        return '<li class="item ' + className + '">' + '<div><a href="'+link+'" target="_blank" class="g-ellipsis-two">' + title + '</a></div>' + '</li>';
                    },
                    }
                });

                //hover模仿轮播效果
                $(".swiper-pagination-bullet").hover(function () {
                    mySwiper.autoplay.stop()
                    mySwiper.slideToLoop($(this).index());
                }, function () {
                    mySwiper.autoplay.start(); //鼠标移出轮播开启
                })
            }
        },
        computed() {},
        created() {
          this.getLiveList();

          checkHostSite('https://www.sdchina.com/').then(function (res) {
            redictToh5('https://h5.sdchina.com/', {
                  suffixPath: '/pages/index/index',
                  param: res
            })
          });

        }
    })
</script>
