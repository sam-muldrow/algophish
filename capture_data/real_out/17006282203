<script>
  Vue.component('logindialog', {
    template: "#logindialog",
    name: "logindialog",
    data: function () {
      return {
        codeurl: '',
        WX_APPID: '',
        WX_REDIRECT_URI: '',
        WX_STATE: '',
        dialogVisible: false,
        // verifCode: ms.base + "/api/code?t=" + new Date().getTime()+ "&deviceId="+this.deviceId ,
        verifCode:'',
        deviceId:'',
        loginway: "account",
        checked: false,
        agree: false,
        loginform: {
          // 验证码
          rand_code: '',
          // 登录名
          peopleName: '',
          // 登录密码
          peoplePassword: '',
        },
        rules: {
          peopleName: [
            {
              "required": true,
              "message": "登录名不能为空"
            },
            {
              "min": 2,
              "max": 16,
              "message": "登录名长度必须为2-16"
            }],
          // 登录密码
          peoplePassword: [{
            "required": true,
            "message": "登录密码不能为空"
          }],

          rand_code: [
            { required: true, message: '请输入验证码', trigger: 'blur' },
            { min: 1, max: 4, message: '长度不能超过4个字符', trigger: 'change' }
          ],
        }
      }
    },
    computed: {

    },
    methods: {
      getWechatImg () {
        ms.http.post(ms.base + "/api/weChat/webWXLink").then(function (res) {
          console.log(res);
        }).catch(function (err) {
          that.loading = false;
          that.$notify({
            title: '失败',
            message: "系统错误",
            type: 'error'
          });

        });
      },
      getWechatCodeParm () {
        let that = this;
        ms.http.get(ms.base + "/api/mcms/wxQCodeParam",{"deviceId":that.deviceId}).then(function (res) {
          console.log(res);
          that.WX_APPID = res.data.appId;
          that.WX_REDIRECT_URI = res.data.redirectUri;
          that.WX_STATE = res.data.state;
        }).catch(function (err) {
          that.loading = false;
          that.$notify({
            title: '失败',
            message: "系统错误",
            type: 'error'
          });
        });
      },

      preview () {
        this.dialogVisible = true;
        //this.getWechatImg();
        this.getWechatCodeParm();
      },
      handleClose () {
        this.dialogVisible = false;
      },
      clickLoginway (way) {
        this.loginway = way;
        if (this.loginway === 'code') {
          this.$nextTick(() => {
/*            let obj = new WxLogin({
              self_redirect: false,  //值为true时，直接在二维码的弹窗中进行页面跳转及其余操作,
              id: "login_container",       //需要承载二维码的容器id
              appid: "wx3e1ca3d4edd55ea0",
              scope: "snsapi_login",     //网页授权，目前网页只有这一种方式，静默授权
              redirect_uri: encodeURIComponent('http://sdchina-test.ihicon-wh.com/weChat/webWXLogin:8443'),   //回调域名(地址前部分是项目域名，后部分loginSuccess是微信需要跳转的路径（vue中的路由名称）)
              state: Math.random()+'',
              style: "black",
              href: "data:text/css;base64,LmltcG93ZXJCb3ggLnRpdGxlIHsNCiAgZGlzcGxheTogbm9uZTsNCn0NCi5pbXBvd2VyQm94IC5zdGF0dXMuc3RhdHVzX2Jyb3dzZXIgew0KICBkaXNwbGF5OiBub25lOw0KfQ0KLmltcG93ZXJCb3ggLnFyY29kZSB7DQogIGJvcmRlcjogbm9uZTsNCiAgd2lkdGg6IDIwMHB4Ow0KICBoZWlnaHQ6IDIwMHB4Ow0KfQ0KLmltcG93ZXJCb3ggLnN0YXR1c3sNCiAgZGlzcGxheTogbm9uZQ0KfQ==",
            });*/

            var that = this;
            console.log("打印appid:"+that.WX_APPID+"--rediurl:"+that.WX_REDIRECT_URI);
            let obj = new WxLogin({
              self_redirect: false,  //值为true时，直接在二维码的弹窗中进行页面跳转及其余操作,
              id: "login_container",       //需要承载二维码的容器id
              appid:  that.WX_APPID,
              scope: "snsapi_login",     //网页授权，目前网页只有这一种方式，静默授权
              redirect_uri: encodeURIComponent(that.WX_REDIRECT_URI),     //回调域名(地址前部分是项目域名，后部分loginSuccess是微信需要跳转的路径（vue中的路由名称）)
              state: that.WX_STATE,
              style: "black",
              href: "data:text/css;base64,LmltcG93ZXJCb3ggLnRpdGxlIHsNCiAgZGlzcGxheTogbm9uZTsNCn0NCi5pbXBvd2VyQm94IC5zdGF0dXMuc3RhdHVzX2Jyb3dzZXIgew0KICBkaXNwbGF5OiBub25lOw0KfQ0KLmltcG93ZXJCb3ggLnFyY29kZSB7DQogIGJvcmRlcjogbm9uZTsNCiAgd2lkdGg6IDIwMHB4Ow0KICBoZWlnaHQ6IDIwMHB4Ow0KfQ0KLmltcG93ZXJCb3ggLnN0YXR1c3sNCiAgZGlzcGxheTogbm9uZQ0KfQ==",
            });

          });
        } else {
          document.querySelectorAll('iframe').forEach(
            function (elem) {
              elem.parentNode.removeChild(elem);
            });
        }
      },
      clickReg () {
        this.$emit('showregdialog');
      },
      //获取验证码
      getcode () {
        this.verifCode = ms.base + "/api/code?t=" + new Date().getTime() + "&deviceId="+this.deviceId ;
      },
      clickLogin () {
        let that = this;
        that.$refs.form.validate(function (valid) {
          if (valid) {
            that.loading = true;
            ms.http.post(ms.base + "/api/checkLogin.do", that.loginform).then(function (res) {
              if (res.result) {
                that.$notify({
                  title: '成功',
                  message: '登录成功',
                  type: 'success'
                });
                window.location.reload()
                that.handleClose();
              } else {
                that.$notify({
                  title: '失败',
                  message: res.msg,
                  type: 'error'
                });
                that.getcode();
              }
            }).catch(function (err) {
              that.loading = false;
              that.$notify({
                title: '失败',
                message: "系统错误",
                type: 'error'
              });
              console.log(err);
            });
          }
        });
      }
    },
    watch: { // 简单监听
      deviceId(newVal, oldVal) {
        this.verifCode = ms.base + "/api/code?t=" + new Date().getTime() + "&deviceId="+ newVal ;
      },
    },
    created: function () {
      getDeviceId().then((res)=> {
        this.deviceId = res
      })
    },
    mounted: function () {

    },
    beforeDestroy: function () {

    }
  })
</script><link rel="stylesheet" href="/template/1/sdong/css/regdialog.css">
<script type="text/x-template" id="regdialog">
  <div class="logindialog">
