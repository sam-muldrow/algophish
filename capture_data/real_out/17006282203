                <div class="line" v-if="loginway == 'account'"></div>
                <div class="noline" v-else></div>
              </div>
              <div :class="loginway == 'code' ? 'tabselectitem' : 'tabitem'" @click="clickLoginway('code')">
                <div class="txt">微信扫码</div>
                <div class="line" v-if="loginway == 'code'"></div>
                <div class="noline" v-else></div>
              </div>
            </div>
            <div class="accountway" v-if="loginway == 'account'">
              <div class="formbox">
                <el-form ref="form" :model="loginform" :rules="rules" label-position="top" size="large">
                  <el-form-item prop="peopleName" class="input">
                    <div class="inputbox">
                      <div class="txt">用户名</div>
                      <div class="inputcontent marginT16">
                        <input autocomplete="off" v-model="loginform.peopleName" type="text" name="username" placeholder="请输入用户名" />
                      </div>
                    </div>
                  </el-form-item>
                  <el-form-item prop="peoplePassword" class="input">
                    <div class="inputbox">
                      <div class="txt">密码</div>
                      <div class="inputcontent marginT16">
                        <input autocomplete="new-password" 	v-model="loginform.peoplePassword" type="password" name="password" placeholder="请输入密码" />
                      </div>
                    </div>
                  </el-form-item>
                  <el-form-item prop="rand_code" class="input">
                    <div class="verbox">
                      <div class="txt">验证码</div>
                      <div class="vercontent">
                        <div class="inputver marginT16">
                          <input autocomplete="off" 	v-model="loginform.rand_code" type="text" name="password" placeholder="请输入验证码" />
