<script type="text/javascript">//Bilaketa url-a sortzeko
$(document).ready(function() {
	var pathurl = '/eusko-jaurlaritza/tramiteak-zerbitzuak/';
	$("form.egoitza").submit(function() {
		var keyword = $('#home_bilaketa').val();
		/* Replace multi spaces with a single space */
		keyword = keyword.replace(/\s{2,}/g,' ');
		var edukimota = 'ayuda_subvencion,anuncio_contratacion,premio_concurso,autorizacion,registro,inspeccion,carnet,arbitraje_denuncia_reclamacion,multa_sancion,canon_tasa_tributo,certificacion,procedimiento_iniciado_administracion,interaccion_ciudadania,peticion_servicios,comunicacion';
		//Defektuzko katalogazioa: Eusko Jaurlaritza eta hauen erakundeak
		var katalogazioa = ';cO:r01epd0122e4ed314423e0db04c97a47b5baa317f,r01epd0122e4edf39923e0db0b11fff216b637726';
		var metadatuak=';m:procedureCollection.EQ.0,documentLanguage.EQ.'+r01gLang;
		if (keyword !=""){
		keyword='fullText.LIKE.'+keyword;
		metadatuak=metadatuak+','+keyword;
		}
		var bilaketaurl = pathurl+'?r01kQry=tT:'+edukimota+katalogazioa+metadatuak+';pp:r01PageSize.20;p:Inter,Inter_portal';
		
		window.top.location.href = encodeURI(bilaketaurl);
		return false;  // Prevent default button behaviour
	});

//Bilatzaileko labela erakutsi eta ezkutatzeko 
$("#home_bilaketa").focus(function() { 
	$(".label_bilaketa").addClass("inv");
	});
	
$("#home_bilaketa").focusout(function() { 	
	if ($("#home_bilaketa").val() == "")
	{
	$(".label_bilaketa").removeClass("inv");
	}
	
	});

});</script>
</section>
