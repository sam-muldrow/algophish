 $('.navto-search-m').on('click', function() {
  if($("body").hasClass("search-on")) {
   $("body").removeClass('search-on');
   $('.navto-search-m i').removeClass("fa-remove");
  } else {
   $("body").addClass("search-on");
   $('.navto-search-m i').addClass("fa-remove");
  }
 })
