	$(function () { 
	$.ajax({
        url: "https://appapi.doit.com.cn/index.php?m=Home&c=Classes&a=doitonedayvideo",
        type: "get",
        dataType: 'json',
        success: function(obj) {
            console.log(obj);
            var v1 =  JSON.stringify(obj).toString();
            var a = JSON.parse(v1);
            var arr = [];
            for(var i=0;i<a['onedayvideo'].length;i++){
                arr[i] = a['onedayvideo'][i];
            }
            var htm = '';
            for(var k=0;k<arr.length;k++){
                htm+='<div><a target="_blank" href=" '+arr[k]['videoUrl']+'"><ul><li class="edu_title"><img src="'+arr[k]["thumb"]+'" alt=""><div class="edu_title_rg"><p>'+arr[k]["title"]+'</p></div></li></ul></ a></div>';
               
            }
            $('#day_class').html(htm);
            console.log(htm); 
        }

    })
	
	});
    
