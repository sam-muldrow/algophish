        var chat = $.connection.newsHub;
        chat.client.addNewMessageToPage = function (name, message) {
            message.section = message.categoryUrlTitle;//'cat_' + message.categoryId;
            angular.element('#liveNews').scope().pushArticle(name, message, false );
