<script type="text/javascript">
    var partIndex = 1;
    var allCount = 50;

    var partIndex = 1;
    var allCount = 50;
    //切换轮播图
    $(function () {
        var width = window.screen.width
        if (width > 1200) { //pc端
            $('#marquee').bxSlider({
                displaySlideQty: 1, //显示li的个数
                moveSlideQty: 1, //移动li的个数
                captions: true, //自动控制
                auto: true,
                controls: true, //显示左右按钮
                nextText: '>',
                prevText: '<',
                onSliderLoad: function (index) {
                    $('#marquee li:nth-child(2)').find("div").css("left", "1rem")
                },
                onSlideAfter: function (index) {
                    $('#marquee li').find("div").css("left", "-17rem");
                    $(index).find("div").css({
                        "left": "1rem",
                        "transition": "left 0.2s ease"
                    })
                }
            })

        } else {
            $('.carousel').carousel()
        }
        $(window).scroll(function () {
            //当滚动的高度 = 文档的总高度 - 当前窗口的高度时,就以为着已经滚动了一个窗口的高度，及已经到当前窗口的底部
            //alert($(document).scrollTop() + "，，，" + $(document).height() + ",,," + $(window).height());
            if ($(document).scrollTop() >= ($(document).height() - $(window).height())-250) { //判断是否已经滚动到页面底部
                partIndex += 1;
                $("#loadingImg").show();
                $("#part" + partIndex).show();
                $("#part" + partIndex).find("img").each(function () {
                    $(this).attr("src", $(this).attr("data_src"));
                    $(this).removeAttr("data_src");
                })
                getHeight();
                if (partIndex == 5 || (partIndex * 10) >= allCount) {
                    $("#loading,#loadingImg").hide();
                    $("#pagination").show();
                }
            }
        });
        $(".scouselLi").hover(
            function () {
                $(".scouselTitle").show();
            }, function () {
                $(".scouselTitle").hide();
            }
        );
    });
</script>



