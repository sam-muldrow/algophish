<script src="https://d1szzzrpbwp6l2.cloudfront.net/public/js/intersection-observer.js"></script>

<script>

    var observer = new IntersectionObserver(
        function (entries) {
            entries.forEach(function (entry) {
                if (entry.intersectionRatio > 0 && entry.isIntersecting) {
                    entry.target.src = entry.target.dataset.src;
                    observer.unobserve(entry.target);
                }
            });
        },
        {
            threshold: [0.1]
        }
    );

    function query(selector) {
        if (!Array.from) {
            Array.from = function (el) {
                return Array.apply(this, el);
            }
        }
        return Array.from(document.querySelectorAll(selector));
    }

    function showToolTip(ele) {
        $(ele).siblings('i').show();
        setTimeout(function () {
            $(ele).siblings('i').hide();
        }, 1500);
    }

    $(document).ready(function () {
        new ClipboardJS('.deals_coupon');

        query('.js-lazy-image').forEach(function (item) {
            observer.observe(item);
        });
    });
    $('.fav_store_list').on('click', '.quickRemoveFavorite', function () {
        var $this = $(this);
        $this.parent().remove();
        quickChangeFavorite($this.data('id'), $this.data('lang'));
    });
    $('#favoritesAdderButton').click(function (e) {
        e.preventDefault();
        $(this).hide();
        $('#favoritesAdderWrapper').show();
        $('#favoritesAdderWrapper .search-autocomplete').show();
        $('#favoritesAdder').focus();
    });

    $('#favoritesAdder').blur(function () {
        setTimeout(function () {
            $('#favoritesAdderButton').show();
            $('#favoritesAdderWrapper').hide();
        }, 300);
    });

    $('#favoritesAdder').bind('input propertychange', function () {
        var $this = $(this),
            autoCompleteDiv = $this.parents(".seemore").find(".search-autocomplete"),
            dataDiv = $this.parents(".seemore").find(".tt-dataset-data"),
            searchKey = $this.val(),
            searchType = 'merchants';

        requestUrl = "/en/search/autocomplete?data_type=" + searchType + "&q=" + searchKey;
        if (searchKey != "") {
            $.ajax({
                url: requestUrl,
                method: "GET",
                timeout: 10000,
                dataType: 'json',
                success: function (data) {
                    appendHtml = '';
                    if (data) {
                        autoCompleteDiv.show();
                        $.each(data, function (i, n) {
                            appendHtml += '<div class="tt-suggestion tt-selectable"><span data-id=' + n["id"] + ' data-lang=' + n["lang"] + ' class="result merchants fav-merchant favoriteStar">' + n["name"] + '</span>' + '</div>';
                        });
                    }
                    dataDiv.empty();
                    dataDiv.append(appendHtml);

                }
            });
        } else {
            dataDiv.empty();
        }
    });


    var deal_like = "";

    
    function laud(ele, id) {
        var flag1 = 0;
        if (typeof $.cookie('deal_like') != "undefined" && deal_like == "") {
            deal_like = $.cookie('deal_like');
        }
        var like_number = parseInt($(ele).siblings().text());
        var userId = 0;
        if ($(ele).hasClass('isLike')) {
            flag1 = 1;
            deal_like = deal_like.replace(new RegExp(id + "-1,", 'g'), "");
            deal_like = deal_like + id + '-0,';
            $(ele).removeClass("isLike");
            $(ele).siblings().text(like_number - 1);
            $.post(
                "/api-deals.php?method=DealsCallBackForLike",
                {articleId: id, uid: userId, status: flag1}
            );
        } else {
            $(ele).addClass("isLike");
            $(ele).siblings().text(like_number + 1);
            deal_like = deal_like.replace(new RegExp(id + "-0,", 'g'), "");
            deal_like = deal_like + id + '-1,';
            $.post(
                "/api-deals.php?method=DealsCallBackForLike",
                {articleId: id, uid: userId, status: flag1}
            );
        }
        $.cookie("deal_like", deal_like, {"expires": null, "path": "/"});
    }

    $(".fav-merchant").click(function () {
        var action = window.location.pathname;
        var is_login = 0;
        if (!is_login) {
            window.location.href = "/users/login?return=" + encodeURIComponent(action);
        }
    });

    $(".favoriteStar").click(function () {
        var action = window.location.pathname;
        var is_login = 0;
        if (!is_login) {
            window.location.href = "/users/login?return=" + encodeURIComponent(action);
        }
    });

    $('.ebtopbanner .close').click(function () {
       $('.ebtopbanner').remove();
    });

    

</script>
