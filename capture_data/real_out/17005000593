<script>
    
    $(document).on("click", ".moreorless", function() {
        if($(this).prev(".mols").hasClass('hidden')){
            $(this).text("« less");
            $(this).attr("title", "less");
            $(this).prev(".mols").removeClass("hidden");
        }else{
            $(this).text("... »");
            $(this).attr("title", "See more");
            $(this).prev(".mols").addClass("hidden");
        }
    });

</script>
