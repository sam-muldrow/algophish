							id: ids,
							fetch_live_sessions: fetchLiveSessions,
						}),

						signal: controller.signal,
					}
				);

				clearTimeout(rti);

				if (res.status !== 200) {
					return;
				}

				const { stats } = await res.json();

				for (const stat of stats) {
