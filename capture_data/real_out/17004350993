                    <script>
                        var pad = function(n) {
                            return ('00' + n).slice(-2);
                        };

                        var countdown = function(to, selector) {
                            var timerElements = document.querySelectorAll(selector);

                            var timerInterval = setInterval(function() {
                                var diff = Math.max(0, to - Date.now());
                                var days = Math.floor((diff / 1000 / 60 / 60) / 24);
                                var hours = Math.floor((diff / 1000 / 60 / 60) % 24);
                                var minutes = Math.floor((diff / 1000 / 60) % 60);
                                var seconds = Math.floor((diff / 1000) % 60);

                                if (diff <= 0) {
                                    clearInterval(timerInterval);
                                }

                                for (var t = 0; t < timerElements.length; t++) {
                                    timerElements[t].innerHTML = 
                                        (days > 0 ? ('<div><span>' + days + '</span><span class="label">day' + (days > 1 ? 's' : '') + '</span></div>') : '') +
                                        '<div><span>' + pad(hours) + '</span><span class="label">hour' + (hours !== 1 ? 's' : '&nbsp;') + '</span></div>' +
                                        '<div><span>' + pad(minutes) + '</span><span class="label">minute' + (minutes !== 1 ? 's' : '&nbsp;') + '</span></div>' +
                                        '<div><span>' + pad(seconds) + '</span><span class="label">second' + (seconds !== 1 ? 's' : '&nbsp;') + '</span></div>';
                                }
                            }, 1000);
                        };

                        var nextDealTime = new Date(1700456400000);
                        countdown(nextDealTime, '#footer .next-deal-timer');
                    </script>
                
                </div>
            </section>
        
        
