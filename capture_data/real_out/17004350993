<script>
    var sideDealCountdown = document.getElementById('sidedeal-countdown');

    if (sideDealCountdown) {    
        var sideDealCountdownInterval = setInterval(function() {
            var diff = sideDealCountdownDate - Date.now();
            var hours = Math.max(Math.floor((diff / 1000 / 60) / 60 % 24), 0);
            var minutes = Math.max(Math.floor((diff / 1000 / 60) % 60), 0);
            var seconds = Math.max(Math.floor((diff / 1000) % 60), 0);

            sideDealCountdown.innerHTML = hours + ' hour' + (hours === 1 ? '' : 's') + ', ' + minutes + ' minute' + (minutes === 1 ? '' : 's') + ', ' + seconds + ' second' + (seconds === 1 ? '' : 's');
        }, 1000);
    }
</script>
