<script>
  window.addEventListener("DOMContentLoaded", function() {

  let tabs = document.querySelectorAll(".info-box-recipe .tab-content .nav .nav-item a");
const dropbox = document.querySelector(".dropdown-menu");

document.querySelector(".tabli-kartlar h3").after(document.querySelector(".form-group-select"));
 document.querySelector(".dropdown-toggle").innerText = tabs[0].innerText;
tabs.forEach((tab, index) => {
  let dropLi = document.createElement("li");
  dropLi.innerText = tab.innerText;
  index===0&&dropLi.classList.add("d-none");
  dropLi.classList.add("dropdown-item");
  dropbox.appendChild(dropLi);
  tab.style.display = "none";
  
  dropLi.addEventListener("click", function() {
    tabs.forEach(tab => tab.closest(".tab-pane").classList.remove("show", "active"));
    tabs[index].closest(".tab-pane").classList.add("show", "active");
    document.querySelector(".dropdown-toggle").innerText= dropLi.innerText;
    document.querySelectorAll(".dropdown-item").forEach(item=>item.classList.remove("d-none"))
    dropLi.classList.add("d-none");
  });
});
document.querySelectorAll(".tabli-kartlar .info-box-recipe .tab-content .tab-pane >.collapse:not(.show)").forEach(collapse=>{collapse.classList.add("show")});
});
</script>
<script>
  const dropdownn = document.querySelector('.dropdown');
const dropdownMenuu = document.querySelector('.dropdown-menu');

dropdownn.addEventListener('click', function() {
  dropdownMenuu.classList.toggle('show');
    document.querySelector(".dropdown-toggle").classList.toggle("dt");
});
let dropdownItems = document.querySelectorAll('.dropdown-item');
let dropdownContents = document.querySelectorAll('.dropdown-content');

for(let i=0; i<dropdownItems.length; i++){
  dropdownItems[i].addEventListener('click', function(){
    let target = this.getAttribute('data-target');
    for(let j=0; j<dropdownContents.length; j++){
      dropdownContents[j].style.display = 'none';
    }
    document.querySelector(target).style.display = 'block';
  });
}
const dropdown = document.querySelector('.dropdown');
const dropdownToggle = dropdown.querySelector('.dropdown-toggle');

// Her bir dropdown item'ına tıklandığında
dropdownItems.forEach(item => {
  item.addEventListener('click', () => {
    // Tüm aktif sınıflarını kaldır
    dropdownItems.forEach(item => item.classList.remove('active'));

    // Tıklanan elemente aktif sınıfını ekle
    item.classList.add('active');

    // Dropdown toggle'ına seçilen elementin değerini yazdır
    dropdownToggle.textContent = item.textContent;
  });
});
document.querySelector(".dropdown").addEventListener("click", function () {
    if (document.querySelector(".dropdown-item.d-none:nth-child(1)") && document.querySelector(".dropdown-item:not(.d-none):nth-child(2)") && document.querySelector(".dropdown-item:not(.d-none):nth-child(3)")) {
      document.querySelector(".dropdown-item:not(.d-none):nth-child(2)").style.backgroundColor = "#f1f3f7";
      document.querySelector(".dropdown-item:not(.d-none):nth-child(3)").style.backgroundColor = "unset";
    }
    else if (document.querySelector(".dropdown-item:not(.d-none):nth-child(1)") && 
             document.querySelector(".dropdown-item.d-none:nth-child(2)") &&
             document.querySelector(".dropdown-item:not(.d-none):nth-child(3)")) {
      document.querySelector(".dropdown-item:not(.d-none):nth-child(1)").style.backgroundColor = "#f1f3f7";
      document.querySelector(".dropdown-item:not(.d-none):nth-child(3)").style.backgroundColor = "unset";
    }
    else if (document.querySelector(".dropdown-item:not(.d-none):nth-child(1)") && 
             document.querySelector(".dropdown-item:not(.d-none):nth-child(2)") &&
             document.querySelector(".dropdown-item.d-none:nth-child(3)")) {
      document.querySelector(".dropdown-item:not(.d-none):nth-child(1)").style.backgroundColor = "#f1f3f7";
      document.querySelector(".dropdown-item:not(.d-none):nth-child(2)").style.backgroundColor = "unset";
    }
  });
</script></DIV>
