  <script>
  var previousPosDS = "";
  
  document.querySelector('.swiper.slider-cards').addEventListener('touchmove',function() {
  
      try {
          if (previousPosDS != document.querySelector('.swiper.slider-cards').getAttribute('style')) {
              document.querySelector('body').style.overflow = "hidden";
          } else {
              document.querySelector('body').style.overflow = "scroll";
          }
      } catch {
  
      }
  
      previousPosDS = document.querySelector('.swiper.slider-cards').getAttribute('style');
  
  });
