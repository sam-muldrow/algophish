  <script>
    var swiper = new Swiper(".slider-cards", {
        loopedSlides: 4,
        spaceBetween: window.innerWidth >= 1200 ? 8 : 6.18,
        // loop: true,
        slideToClickedSlide: true,
        speed: 200,
        width: window.innerWidth >= 1200 ? 170 : 131,
        doubleClick: event,
        enabled: true,
        grabCursor: true,
        longSwipesRatio: 0.5,
        keyboard: {
            enabled: true,
        },
        scrollbar: {
            el: ".swiper-scrollbar",
            hide: 0,
            draggable: true,
            dragSize: window.innerWidth >= 1200 ? 120 : 75,
            height: window.innerWidth >= 1200 ? 10 : 3,
        },
            navigation: {
            nextEl: ".swiper-button-next",
            prevEl: ".swiper-button-prev",
            },
        on: {
            slideChange: function () {
                var activeIndex = swiper.activeIndex;
                var minicards = document.querySelectorAll('.slider-cards .swiper-slide');
                var sliders = document.querySelectorAll('.slider-content .slider-detail');
  
                var minicardsActive = minicards[activeIndex].getAttribute('data-value');

                document.querySelector(".sub-slider").setAttribute('data-value', minicardsActive);
  
                sliders.forEach(slider => {
                    if (slider.getAttribute('data-value') === minicardsActive) {
                        slider.classList.add('active');
                    } else {
                        slider.classList.remove('active');
                    }
                });
  
            },
        },
    });
    document.addEventListener("DOMContentLoaded", function () {
        var apps = document.querySelectorAll(".detail-apps");
        apps.forEach(appsbrow => {
            if (/iPad|iPhone|iPod/i.test(navigator.userAgent)) {
                appsbrow.querySelectorAll("a")[0].style.display = 'block';
            } else if (/Android/i.test(navigator.userAgent)) {
                appsbrow.querySelectorAll("a")[1].style.display = 'block';
            }
        });
    var resetButton = document.querySelector('.to-left-btn');
    resetButton.addEventListener('click', function () {
    swiper.slideTo(0); 
  });
    });
  </script>
