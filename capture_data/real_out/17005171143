</div>

<script>

    let t_px = 0;
    let suggestedCards = document.querySelectorAll('#suggestDigitalService .card.v2');
    let paramSlider = suggestedCards.length - 3;

    function checkSuggestedArrows() {
        paramSlider == 0 ? document.querySelector('#next-suggested-button').classList.add('d-none') : (paramSlider == suggestedCards.length - 3) ? document.querySelector('#previous-suggested-button').classList.add('d-none') : document.querySelector('#next-suggested-button').classList.remove('d-none') & (document.querySelector('#previous-suggested-button').classList.remove('d-none'));
        suggestedCards.length <= 3 ? document.querySelector('#next-suggested-button').classList.add('d-none') & document.querySelector('#previous-suggested-button').classList.add('d-none') : "";

    }


    let dyn_px = parseInt(getComputedStyle(document.querySelector('#suggestDigitalService .card.v2')).width) + parseInt(getComputedStyle(document.querySelector('#suggestDigitalService .cards')).gap)



    function slider(direction) {

        if (direction == 1 && paramSlider > 0) {
            t_px -= dyn_px;
            suggestedCards.forEach(x => {
                x.style.transform = "translate(" + t_px.toString() + "px)";
            })
            paramSlider = paramSlider - 1;

        } else if (direction == 0 && paramSlider < (suggestedCards.length - 3)) {
            t_px += dyn_px;
            suggestedCards.forEach(x => {
                x.style.transform = "translate(" + t_px.toString() + "px)";
            })
            paramSlider = paramSlider + 1;
        }
        checkSuggestedArrows();
    }
    checkSuggestedArrows();
</script>

