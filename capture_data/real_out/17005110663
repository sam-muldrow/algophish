<script type="text/javascript">
	/*
	function openLayerPopup_allMenu( returnObj ){
		var option = {
			url : "/kor/CMS/SiteMap/SiteMapLayer.do"
			, title : "종합물류기업 (주)한진 전체메뉴"
			, width : "960"
			, height : "600"
			, returnObj : returnObj
			, parameter : {}
			, close_button_yn					: true
			, reload_button_yn					: false
			, minmaxsize_button_yn				: false
		};
		$( "body" ).layerPopup( option );
	} 
	*/
	$sitemapPlaying = false;
	function viewSiteAllMenu( returnObj ) {
		if ( !$sitemapPlaying ) {
			$sitemapPlaying = true;
			$siteMapCore = $(".customSitemap-wr");
			$siteMapCore.scrollTop(0);
			$siteMapCore.addClass("active");
			$("body").addClass("scrnone");
			var delayTime = 0;
			$(".sitemap-list .depth-wr", $siteMapCore ).each(function( index ) {
				var $this = $(this);
				function delayFnc() {
					if ( (index+1) == $(".sitemap-list  .depth-wr", $siteMapCore ).length ) {
						$sitemapPlaying = false;
					}
					$this.addClass("stable");
				}
				setTimeout( delayFnc, delayTime );
				delayTime += 150;
			});
		}
		return false;
	}
	function viewSiteCloseMenu() {
		if ( !$sitemapPlaying ) {
			$sitemapPlaying = true;
			$siteMapCore = $(".customSitemap-wr");
			$siteMapCore.addClass("release");
			$("body").removeClass("scrnone");
			setTimeout( function() {
				$siteMapCore.removeClass("active release")
				$(".sitemap-list .depth-wr", $siteMapCore ).removeClass("stable");
				$sitemapPlaying = false;
			}, 300 );
		}
	}
</script>

<!-- 여기서 onclick 이벤트만 걸어주시면 됩니다. -->
