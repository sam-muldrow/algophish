<script>
	function WayBillSch(obj){
		
		if($("#wblnumText").val() == ""){
			alert("운송장번호를 입력해주세요.");
			$("#wblnumText").focus();
			return false;
		}else{
			  var matchStr = new RegExp("[^0-9]", "gi");
			  var val = $("#wblnumText").val();
			  if(matchStr.test(val) == true){
				alert("숫자로만 입력해주세요.");
				$("#wblnumText").focus();
				$("#wblnumText").val(val.replace(matchStr, ""));
				return false;
			  }else{
					/*var rsa = new RSAKey();
					rsa.setPublic($('#RSAModulus').val(),$('#RSAExponent').val());
					$("#wblnum").val(rsa.encrypt($("#wblnumText").val()));*/
					if(val.length==12 || val.length==14){
						$("#wblnum").val($("#wblnumText").val())
						$("#wblnumText").val("");
						return true;
					}else{
						alert("잘못된 운송장번호 입니다.");
						return false;
					}
			  }
			 
		}
	}
</script>
