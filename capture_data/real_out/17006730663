<script type="text/javascript">
				/*  */
				
				function Zmen_chart(id){
					var nazev_id = '#' + id
					var prefix = id.substr(0, 2)
					var charts_prefix = new Array('a_','m_')
					var charts_group = new Array('a_','m_')
					charts_group['a_'] = new Array('1', '2', '3', '4', '5') // do jake skupiny patri graf dle sveho id
					charts_group['m_'] = new Array('1', '1', '1', '2', '2') // do jake skupiny patri graf dle sveho id
					if (id.indexOf(prefix + 'chart_') != -1) {
						var id_a = id.replace(prefix + 'chart_','')
						var inp = $('#' + prefix + 'chart_focus').val()
						
						// odeslani formulare pro jinou skupinu
						if (charts_group[prefix][(inp - 1)] != charts_group[prefix][(id_a - 1)]) { 
							$('#' + prefix + 'chart_focus').val(id_a)
							$('#' + prefix + 'chart_form').submit()
							return true 
						} 
						if (id_a != inp) {
							var chart_id = '#' + prefix + 'chart_one_' + id_a
							if ($(chart_id)) {
								if (inp) { 
									var lost_focus_chart_id = '#' + prefix + 'chart_one_' + inp
									var lost_focus_img_id = '#' + prefix + 'chart_one_img_' + inp
									if ($(lost_focus_chart_id)) {
										$(lost_focus_chart_id).hide()
										$(nazev_id).attr('class', $(nazev_id).attr('class').replace('chart_nazev_one_focus','chart_nazev_one')) // oprava obcasne chyby
										$(nazev_id).attr('class', $(nazev_id).attr('class').replace('chart_nazev_one','chart_nazev_one_focus'))
										$(nazev_id).attr('title', '')
										var lost_focus_nazev_id = '#' + prefix + 'chart_' + inp
										$(lost_focus_nazev_id).attr('class', $(lost_focus_nazev_id).attr('class').replace('chart_nazev_one_focus', 'chart_nazev_one'))
										$(lost_focus_nazev_id).attr('title', $(lost_focus_img_id).attr('alt'))
										$(chart_id).show()
										$('#' + prefix + 'chart_focus').val(id_a)
										for (var i = 0; i != 6; i++) { 
											if (charts_prefix[i] != prefix) {
												try {
													$('#' + charts_prefix[i] + 'chart_' + prefix + 'focus').val(id_a)
												}
												catch(e){}
											}
										}
									}
								}
							}	
						}
					}
					return false
				}
				
				function odesli(name,value) {
					$("[name=" + name + "]").val(value)
					if (name=='ap') $('#a_chart_form').submit()
					if (name=='mp') $('#m_chart_form').submit()
				}
				
				$('#a_chart_form').submit(function() { 
					  
					return true; })
				$('#m_chart_form').submit(function() {
					  
					return true; })
				
				
				/*  */
		</script>

