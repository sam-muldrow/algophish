            function announceIEDeprecation() {
              var box = document.createElement('div');
              box.className = 'box__browser-nudging';

              var innerBox = document.createElement('div');
              innerBox.className = 'box__inner';
              box.appendChild(innerBox);

              var p = document.createElement('p');
              p.className = 'sprite__homemain--before text__nudging';
              p.innerText = '인터넷 익스플로러는 더 이상 지원되지 않습니다.';
              innerBox.appendChild(p);

              var strong = document.createElement('strong');
              strong.className = 'text__strong';
              strong.innerText = '안전하고 빠른 브라우저로 업데이트 하세요!';
              p.appendChild(strong);

              var linkBox = document.createElement('div');
              linkBox.className = 'box__browser-link';
              innerBox.appendChild(linkBox);

              var linkPrefab = document.createElement('a');
              linkPrefab.className = 'link__browser';
              linkPrefab.target = '_blank';
              linkPrefab.appendChild(document.createElement('span'));
              linkPrefab.children[0].className = 'sprite__homemain--before';

              var chromeLink = linkPrefab.cloneNode(true);
              chromeLink.href = 'https://www.google.com/chrome/';
              chromeLink.children[0].className += ' text__chrome';
              chromeLink.children[0].innerText = '크롬 다운로드';
