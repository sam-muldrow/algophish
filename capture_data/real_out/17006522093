        $(document).ready(function () {
            $("#solTitle a").click(function () {
                var id = $(this).attr("data-id");
                var audioObj = $("audio[data-id='" + id + "']");
                if (audioObj.paused == false) {
                    audioObj.get(0).pause();
                } else {
                    audioObj.get(0).play();
                }
            })
        });
