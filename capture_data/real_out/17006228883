<script type="text/javascript">
    $('input[type=image]').each(function() {
        // Пропускаем желтые кнопочки
        if ($(this).attr('src').indexOf('enter') > -1) {
            return
        }

        $(this).data('src', $(this).attr('src'));

        var src =
            'https://st.nnn.ru/img/ru/'+
                $(this).attr('src').substring($(this).attr('src').lastIndexOf('/'));

        // Заменим картинку
        src = src.replace(/save2.png/g, 'save.png');

        $(this).attr('src', src);
        $(this).error(function() {
            $(this).attr('src', $(this).data('src'));
        });
    });
</script>
