<script type="text/javascript">

               $( ".livestream-vision" ).removeClass("disabled");

               //close stream live
                function onCloseLivestream(){
                     $( ".livestream-vision" ).removeClass("disabled");
                     $('#live-stream-box').remove();
                }

            $(document).ready(function(){
                var htmlContent = '<div id="live-stream-box">'+
                 '<a id="livestream-close" onclick="onCloseLivestream()" class="btn btn-danger livestream-close"><i class="fa fa-times" aria-hidden="true"></i></a>'+
                 '<a id="livestream-draggable" class="btn btn-info"><i class="fa fa-hand-paper-o" aria-hidden="true"></i></a> <div></div><iframe width="560" height="315" src="https://www.youtube.com/embed/YFXerSe2bvo?autoplay=1" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe></div>';
                $('.livestream-vision').click(function(){
                        if($("#live-stream-box" )){
                            $('.livestream-vision').addClass( "disabled" );
                            $( ".header" ).append( htmlContent);
                            $( "#live-stream-box" ).draggable();
                        }
                });
            });



</script>
