        selector: '.footer-section #menu-sidebar-socials-nav li:nth-child(1)',
        markup: '<li class="menu-item menu-item-type-post_type menu-item-object-page"><a class="ccpa-click">Do Not Sell My Info</a></li>'
      }
      ];

      function getMatch() {
        var site;
        for (var i = 0; i < sites.length; i++) {
          if (!!document.querySelector(sites[i].fingerprint)) {
            site = sites[i];
            break
          }
        }
        return site;
      }

      function inject(site) {
        var footerMenuItem = document.querySelector(site.selector);
        var CCPAMenuItem = _WF.utils.createElement(site.markup);
        CCPAMenuItem.style.cursor = 'pointer';
        footerMenuItem.parentElement.insertBefore(CCPAMenuItem, footerMenuItem);
        CCPAMenuItem.addEventListener('click', function () {
          __uspapi("displayUspUi");
        });
      }

      function createFooterLink() {
        var site = getMatch();
        if (site) {
          inject(site);
        }
      }

      function fireWhenReady() {
        var created = false;
        if (document.readyState !== "loading") {
          if (!created) {
            created = true;
            createFooterLink();
          }
        } else {
          document.onreadystatechange = function () {
            if (document.readyState !== 'loading') {
              if (!created) {
                created = true;
                createFooterLink();
              }
            }
          }
        }
      }

      function initCCPACMP() {
        console.log("DEBUG - (CCPA-CMP) init");
        fireWhenReady();
      }

      if (_WF.utils.isCCPARegion() && !_WF.utils.isCrawler()) {
        initCCPACMP();
      }

      _WF.emit(_WF.EVENTS.CONSENTLOADED);
    });
  });
</script>
