<script type="text/javascript">
    $(function(){

        var startDateStr='10.11.2018 08:55';
        var endDateStr='10.11.2018 09:15';
        
        var date = startDateStr.split(" "),
            dateformat = date[0].split("."),
            dateformatT = date[1],
            pastDateD = dateformat[0],
            pastDateM = dateformat[1],
            pastDateY = dateformat[2];

        var enddate = endDateStr.split(" "),
            enddateformat = enddate [0].split("."),
            enddateformatT = enddate [1],
            endpastDateD = enddateformat [0],
            endpastDateM = enddateformat [1],
            endpastDateY = enddateformat [2];

        
        var startDateFormat = new Date(pastDateM + "-" + pastDateD + "-" + pastDateY + " " +dateformatT);
        var endDateFormat = new Date(endpastDateM + "-" + endpastDateD + "-" + endpastDateY + " " +enddateformatT);
        
        var nowDate = new Date();
        if(nowDate >= startDateFormat && nowDate <= endDateFormat && $.cookie("homeVideo") != 1){
            setTimeout(function(){
                openModal("https://medianova/0_nu6fbnik", "iframe", 940, 615);
                $.cookie("homeVideo", 1, { expires : 365 });
                setTimeout(function(){
                    $('.modal').dialog('close');
                }, 80000);
            },2000);
        }
        
    });
	
</script>
