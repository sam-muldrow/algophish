  // AdBlockerモーダル表示処理
  window.showAdBlockerModal = function() {
    SgkUtil.Modal.openModal(
      {
        // headerHtmlStr: null,
        modalOverLayProps: null,
        contentHtmlStr:`
<div style="font-size: 18px;width: 90vw;max-width: 600px;">
    <div style="text-align: center;">
        <img loading="lazy" width="207" height="38" src="https://www.news-postseven.com/wp-content/themes/nps2020_2_sp/images/logo_horizontal_lead_sp_menu.svg" alt="NEWSポストセブン">
    </div>
    <br>
    <div style="text-align: center;font-weight: bold;">
        広告表示の許可をお願いします。
    </div>
    <br>
    <div style="text-align: center;font-size: 12px;font-weight: bold;">
    広告ブロッカーを利用されているようです。<br>
    当サイトでは広告表示の収益で記事を作成しております。<br>
    </div>
</div>
        `,
        footerHtmlStr: '<div style="padding-bottom: 2em"></div>'
      },
      {
        onShow: function(m, el, e) {},
        onClose: function(m, el, e) {
          setTimeout(function() {
            if (SgkUtil.Ad.isAdBlockUser()) {
              showAdBlockerModal();
            }
          }, 5000);
        },
        deleteClose: true,
        disableScroll: true,
        disableFocus: true
      }
    );
  };
</script>
<script>
  try {
    setTimeout(() => {
      SgkUtil.Logger.sendTdClientProcessLog(1, SgkUtil.Ad.isAdBlockUser() ? 'true' : 'false', 'AD_BLOCKER_USER',  {
        deviceType: document.body.dataset.deviceType,
        userAgent: navigator.userAgent
      });
    }, 2000);
  } catch(e) {
    console.error(e);
  }
  try {
    const logName = 'AD_BLOCKER_USER_CHECK_INTERVAL_230712_0940';
    const uuid = SgkUtil.getUuid();
    SgkUtil.Ad.checkAdBlockUserInterval(() => {
      console.log('useAdblock');
      SgkUtil.Logger.sendTdClientProcessLog(1, 'true', logName, {
        deviceType: document.body.dataset.deviceType,
        userAgent: navigator.userAgent
      });
      fetch('/ad_blocker.txt?uuid=' + uuid + '&is_user=true&log_name=' + logName);
      if(!false) showAdBlockerModal();
    }, () => {
      console.log('notUseAdblock');
      SgkUtil.Logger.sendTdClientProcessLog(1, 'false', logName,  {
        deviceType: document.body.dataset.deviceType,
        userAgent: navigator.userAgent
      });
      fetch('/ad_blocker.txt?uuid=' + uuid + '&is_user=false&log_name=' + logName);
    });
  } catch(e) {
    console.error(e);
  }
</script>
