	function clickOpenModalFaqWhats() {         var x = document.getElementById("close-modal-whats-faq");         if (x.style.display === "none") {           x.style.display = "block";         } else {           x.style.display = "none";         }       }      function clickCloseModalFaqWhats() {     var element = document.getElementById("close-modal-whats-faq");       element.classList.add("hide");     } </script></section></section>

