//fnc dec start----------------------------------------------------------------------------------

function strtHandTalkActions() {

     //original $('body .hovered-content').html(function() {
     //$('body .hovered-content, .tenderSection') //$('body .hovered-content, div') // $('body').html(function() {
     // best 1 $('body .hovered-content, .m-text-center').html(function() {
     // $('body .hovered-content, .m-text-center, .companyDetails').html(function() {

        $('body .hovered-content').html(function() {
         var cont = [];
         //console.log(" the text now = " + $(this).text() );
         return '<span class="wr">' + $(this).text().split(" ").join('</span> <span class="wr">') + '</span>';
     }).on("mouseover", "span.wr.dsl", function() {
         currentItem = $(this);
         var hoveredWord = $(this).text();
         if (onProcess)
             return false;
         onProcess = true;
         setTimeout(function() {
             hoveredWord = currentItem.text().trim();
             console.log(hoveredWord);
             //process shortened Job Title
             //hoveredWord.endsWith("...");

             anim(hoveredWord, "anim");
         }, 1000);
         return true;
     });
     $(document).on("mouseout", "span.wr.dsl", function() {
         isStopped = true;
     });


} //function strtHandTalkActions() {

//fnc dec end  ----------------------------------------------------------------------------------

/* new two end */

