				</div>
			</div>
		</div>
        

    <script>
    function go_more_btn_V1()
	{

		    $('.more-jobs-btn').on('click', function () {        
            $(this).find('i').toggleClass('icon-sort-down icon-sort-up');
            if ($(this).find('i').hasClass("icon-sort-up")) {
                //$(this).parent(".hotJobsCompany.allHotJobs").addClass('zoom').find('.allJobs').addClass('active');                
                $(this).parent(".hotJobsCompany.allHotJobs").addClass('zoom').find('.allJobs').addClass('active');                
                $(this).parent(".hotJobsCompany.allHotJobs.zoom").attr("tabindex",-1).focus();
                $(this).attr( 'aria-expanded', 'true');                
            } else {
                $(this).parent(".hotJobsCompany.allHotJobs").removeClass('zoom').find('.allJobs').removeClass('active');
                $(this).parent(".hotJobsCompany.allHotJobs").removeAttr("tabindex",-1).focus();
                $(this).attr( 'aria-expanded', 'false');
            }        
        });


	}//go_more_btn_V1()

	/*
	var foo = function() {
	// Code to handle some kind of event
	};
	
	$( "p" ).bind( "click", foo ); // ... Now foo will be called when paragraphs are clicked ...
	
	$( "p" ).unbind( "click", foo ); // ... foo will no longer be called.
	*/


	var functionbindClickEvent = function () {        
            $(this).find('i').toggleClass('icon-sort-down icon-sort-up');
            if ($(this).find('i').hasClass("icon-sort-up")) {
                //$(this).parent(".hotJobsCompany.allHotJobs").addClass('zoom').find('.allJobs').addClass('active');                
                $(this).parent(".hotJobsCompany.allHotJobs").addClass('zoom').find('.allJobs').addClass('active');                
                $(this).parent(".hotJobsCompany.allHotJobs.zoom").attr("tabindex",-1).focus();
                $(this).attr( 'aria-expanded', 'true');                
            } else {
                $(this).parent(".hotJobsCompany.allHotJobs").removeClass('zoom').find('.allJobs').removeClass('active');
                $(this).parent(".hotJobsCompany.allHotJobs").removeAttr("tabindex",-1).focus();
                $(this).attr( 'aria-expanded', 'false');
            }        
        } // functionbindClickEvent

	function go_more_btn()
	{
		   // $('.more-jobs-btn').on('click', );
		    $('.more-jobs-btn').unbind( "click", functionbindClickEvent );
		    $('.more-jobs-btn').bind( "click", functionbindClickEvent );
	}//go_more_btn()

	go_more_btn();
 	// sI_go_more_btn = this.setInterval("go_more_btn()",2000);

/* tender section more jobs button ----------Start----------------------------------------------------  */



	var functionbindClickEvent_tenderCardExpanded = function () {        

			$(this).find('i').toggleClass('icon-sort-down icon-sort-up');

			if ($(this).find('i').hasClass("icon-sort-up")) {
				$(this).parent(".tender-card.expanded").addClass('zoom auto-height').find('.hidden-items').addClass('active');
				$(this).parent(".tender-card.expanded.zoom.auto-height").attr("tabindex",-1).focus();
				$(this).attr( 'aria-expanded', 'true');
			} else {
				$(this).parent(".tender-card.expanded").removeClass('zoom auto-height').find('.hidden-items').removeClass('active');
				$(this).parent(".tender-card.expanded").removeAttr("tabindex",-1).focus();
				$(this).attr( 'aria-expanded', 'false');
			}//else
                   
        } // functionbindClickEvent_tenderCardExpanded
		
	function go_more_btn_tenderCardExpanded()
	{
		   // $('.more-jobs-btn').on('click', );
		    //$('.more-jobs-btn').unbind( "click", functionbindClickEvent_tenderCardExpanded );
		    
		    
			$('.tender-container .tender-company-list .more-jobs-btn').unbind( "click", functionbindClickEvent );

			$('.tender-container .tender-company-list .more-jobs-btn').unbind( "click", functionbindClickEvent_tenderCardExpanded );
		    $('.tender-container .tender-company-list .more-jobs-btn').bind( "click", functionbindClickEvent_tenderCardExpanded );

	}//go_more_btn_tenderCardExpanded()

	go_more_btn_tenderCardExpanded();
 	// sI_go_more_btn_tenderCardExpanded = this.setInterval("go_more_btn_tenderCardExpanded()",2000);

/* tender section more jobs button ----------End----------------------------------------------------  */

    </script>
		

