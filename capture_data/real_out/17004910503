 $.fn.htashow = function() {
     if($(".hndtk-panel").hasClass("open")) {
         $(".hndtk-panel").removeClass("open");  
     } else {
         $(".hndtk-panel").addClass("open");
     }
 } 
 $(".htcbtn").click(function() {
     $(this).htashow();
     console.log("htcbtn ln 433")
     $("#htalkbtn").removeClass("ssac");
     $("html").removeClass("hndcursor");
     $(".hovered-content .wr.dsl").removeClass("dsl");
 });
</script>

