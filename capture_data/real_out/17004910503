    if( fetchFile == stepsToGo)
    {

        clearInterval(myInterval);
        console.log(" stepsToGo CLeared  :: " + stepsToGo );    
    }//if( fetchFile == stepsToGo)

}//function goONLooping_AddHoveredContentClassOnEveryHTMLElement()

function AddHoveredContentClassOnEveryHTMLElement()
{ 
    $(".companyDetails h3").on("mouseover", function() { $(this).addClass("hovered-content");  });   
    //$(".companyDetails *")//$("html *").on("mouseover", function() { $(this).addClass("hovered-content");  });   
    //IMPORTANT TARIQ 11 jan 2023 //strtHandTalkActions() ;
}//function AddHoveredContentClassOnEveryHTMLElement()


