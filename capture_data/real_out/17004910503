<script>
//------------------------------- ProcessorFunction --- start --------------------------------------------------
showLC_Panel_DebugData = 0 ;

	

// handtalk processing start -------------------------------------------------------
	showhandTalk = 1 ;
if (showhandTalk == 1 )
{	
	handTalk_span_start = "<span class='hovered-content'>" ;
	handTalk_span_end   = "</span>" ;

}//if (showhandTalk == 1 )
// handtalk processing end -------------------------------------------------------



function getSingleJob(gsj_Company_jobLink, gsj_Company_job)
{
	jsDotDotStr = "..." ; 
	Company_Jobs_MaxLength = 28 ;
	gsj_Company_job_Shortened = gsj_Company_job ;

	gsj_Company_jobs_Length = gsj_Company_job.length ;
	if( gsj_Company_jobs_Length > Company_Jobs_MaxLength )
	{
		gsj_Company_job_Shortened = gsj_Company_job_Shortened.substring(0, Company_Jobs_MaxLength) + jsDotDotStr;

	}// if( gsj_Company_jobs_Length > Company_Jobs_MaxLength )

	if (showLC_Panel_DebugData == 1 )
	{
		console.log(" gsj_Company_job_Shortened " + gsj_Company_job_Shortened );
		
	}//if (showLC_Panel_DebugData == 1 )	

    getSingleJob_data = "                            <li>" ;
    getSingleJob_data += "                                <a rel='noopener' href='" + gsj_Company_jobLink + "' target='_blank' title='" + gsj_Company_job + "'  >" + handTalk_span_start + gsj_Company_job_Shortened + handTalk_span_end + "</a>" ;
    getSingleJob_data += "                            </li>" ;

    return getSingleJob_data ;
 
}//function getSingleJob()



//a = getOneOrTwoJob(gootj_howManyJob, gootj_CurrentCardsNumber )
//a = getOneOrTwoJob( 1, 5 ) ;// 1 job test '
///////a = getOneOrTwoJob( 1, 0 ) ;// 1 job test '
//a = getOneOrTwoJob( 2, 20 ) ;// 2 job test '
function getOneOrTwoJob(gootj_howManyJob, gootj_CurrentCardsNumber )
{
    
    Company_job_Array     = cardAll.Cards[ gootj_CurrentCardsNumber ].log_company.jobs  ;
    gootj_howManyJob     = cardAll.Cards[ gootj_CurrentCardsNumber ].log_company.jobCount  ;
	if (showLC_Panel_DebugData == 1 )
	{
	console.log( "LN-4223 gootj_howManyJob " + gootj_howManyJob  + " gootj_CurrentCardsNumber " + gootj_CurrentCardsNumber + "  " + "" ); 
		
	}//if (showLC_Panel_DebugData == 1 )	

    getOneOrTwoJob_data   = "                            <ul>" ;

    for( cjl = 0 ;  cjl < gootj_howManyJob  ;  cjl++ )
    {
		if (showLC_Panel_DebugData == 1 )
		{
			console.log( "LN-4231 gootj_howManyJob " + gootj_howManyJob  + " cjl " + cjl + "  " + "" ); 
			console.log( "LN-4232 gootj_CurrentCardsNumber " + gootj_CurrentCardsNumber  + " cjl " + cjl + "  " + "" ); 
			
		}//if (showLC_Panel_DebugData == 1 )	
        gootj_Company_job     = Company_job_Array[cjl].name ;
        gootj_Company_jobLink = Company_job_Array[cjl].link ; 
  		gootj_Company_jobLink = replaceSiteName( gootj_Company_jobLink );

 
		if (showLC_Panel_DebugData == 1 )
		{
			console.log( "LN4050 gootj_Company_job " + gootj_Company_job  + " gootj_Company_jobLink " + gootj_Company_jobLink + "  " + "" ); 
			console.log( "LN5245 gootj_CurrentCardsNumber " + gootj_CurrentCardsNumber  + " cjl " + cjl + "  " + "" ); 		
		}//if (showLC_Panel_DebugData == 1 )

        getOneOrTwoJob_data += getSingleJob(gootj_Company_jobLink, gootj_Company_job) ;

    }//for( cjl = 1 ;  cjl < gootj_howManyJob  ;  cjl++ )

    getOneOrTwoJob_data += "                            </ul>" ;

	if (showLC_Panel_DebugData == 1 )
	{
	console.log( "LN-4247 getOneOrTwoJob_data " + getOneOrTwoJob_data  + "  " + "" + "  " + "" ); 
		
	}//if (showLC_Panel_DebugData == 1 )	

    return getOneOrTwoJob_data ;
 
}//function getOneOrTwoJob()


//getMultipleJob(gootj_howManyJob, gootj_CurrentCardsNumber )
//MutipleJob = getMultipleJob( 3, 88 ) ;// 3 job test '
//MutipleJob = getMultipleJob( 3, 87 ) ;// 3 job test '

function getMultipleJob(gootj_howManyJob, gootj_CurrentCardsNumber )
{
    
    Company_job_Array     = cardAll.Cards[ gootj_CurrentCardsNumber ].log_company.jobs  ;
    gootj_howManyJob      = cardAll.Cards[ gootj_CurrentCardsNumber ].log_company.jobCount  ;
	
	// ---------------- First Two Jobs START ----------------------------------------

    getOneOrTwoJob_data   = "                            <ul>" ;

	//for( cjl = 0 ;  cjl < gootj_howManyJob  ;  cjl++ )
	for( cjl = 0 ;  cjl < 2  ;  cjl++ )
    {
        //gootj_Company_job     = Company_job_Array[cjl].name + " " + gootj_CurrentCardsNumber  + " " + cjl ;
        //gootj_Company_jobLink = Company_job_Array[cjl].link + " " + gootj_CurrentCardsNumber  + " " + cjl ;
        gootj_Company_job     = Company_job_Array[cjl].name ;
        gootj_Company_jobLink = Company_job_Array[cjl].link ;
		gootj_Company_jobLink = replaceSiteName( gootj_Company_jobLink );


		if (showLC_Panel_DebugData == 1 )
		{
			console.log( "LN4084 gootj_Company_job " + gootj_Company_job  + " gootj_Company_jobLink " + gootj_Company_jobLink + "  " + "" ); 
			console.log( "LN5245 gootj_CurrentCardsNumber " + gootj_CurrentCardsNumber  + " cjl " + cjl + "  " + "" ); 		
		}//if (showLC_Panel_DebugData == 1 )	


        getOneOrTwoJob_data += getSingleJob(gootj_Company_jobLink, gootj_Company_job) ;
    }//for( cjl = 1 ;  cjl < gootj_howManyJob  ;  cjl++ )

    getOneOrTwoJob_data += "                            </ul>" ;

	if (showLC_Panel_DebugData == 1 )
	{
		console.log( "LN5282 getOneOrTwoJob_data " + getOneOrTwoJob_data  + "  " + "" + "  " + "" ); 
		
	}//if (showLC_Panel_DebugData == 1 )	

	// ---------------- First Two Jobs END ----------------------------------------


	// ---------------- Remaining Jobs START ------------------------------------------------------------

	getOneOrTwoJob_data += "                        <div class='allJobs'> " ;
	getOneOrTwoJob_data += "                            <ul>" ;

	//for( cjl = 0 ;  cjl < gootj_howManyJob  ;  cjl++ )
	for( cjl = 2 ;  cjl < gootj_howManyJob  ;  cjl++ )
	{
		//gootj_Company_job     = Company_job_Array[cjl].name + " " + gootj_CurrentCardsNumber  + " " + cjl ;
		//gootj_Company_jobLink = Company_job_Array[cjl].link + " " + gootj_CurrentCardsNumber  + " " + cjl ;
		gootj_Company_job     = Company_job_Array[cjl].name  ;
		gootj_Company_jobLink = Company_job_Array[cjl].link  ;
  		gootj_Company_jobLink = replaceSiteName( gootj_Company_jobLink );
		
		if (showLC_Panel_DebugData == 1 )
		{
			console.log( "LN5245 gootj_CurrentCardsNumber " + gootj_CurrentCardsNumber  + " cjl " + cjl + "  " + "" ); 
			
		}//if (showLC_Panel_DebugData == 1 )	

		getOneOrTwoJob_data += getSingleJob(gootj_Company_jobLink, gootj_Company_job) ;
	}//for( cjl = 1 ;  cjl < gootj_howManyJob  ;  cjl++ )

	getOneOrTwoJob_data += "                            </ul>" ;
	getOneOrTwoJob_data += "                        </div> " ;

	if (showLC_Panel_DebugData == 1 )
	{
		console.log( "LN5308 getOneOrTwoJob_MultiplePart_data " + getOneOrTwoJob_data  + "  " + "" + "  " + "" ); 
		
	}//if (showLC_Panel_DebugData == 1 )	

// ---------------- Remaining Jobs END ----------------------------------------



    return getOneOrTwoJob_data ;
 
}//function getMultipleJob()

function getBlankCard(howManyBlankCards)
{	
	getBlankCard_data = "" ;
	for( var gbc = 0 ; gbc < howManyBlankCards ; gbc++)
	{

// for mobile single column desktop 4 column //		getBlankCard_data += "<div class='col-md-4 col-sm-4 col-xs-12 c-card br'>" ;

// for mobile 2 column desktop 4 column //
		getBlankCard_data += "<div class='col-md-3 col-xs-6 c-card br'>" ;
		getBlankCard_data += "<div class='row'>" ;
		getBlankCard_data += "" ;
		getBlankCard_data += "</div>" ;
		getBlankCard_data += "</div>" ;

	}//for( var gbc = 0 ; gbc < howManyBlankCards ; gbc++)

	
	return getBlankCard_data ;

}//function getMultipleJob()

function replaceSiteName(text_to_replace ) //replaceSiteName(Company_Logo_protocolLess )
{
	// replace GCP-companyLogo - start -----------------------------------------------------------------

	go_for_GCP_replacement = "0" ; //  "" ; //  "0" ; // "1" ; // 1 ;
	go_for_GCP_replacement = "0" ; // 1 ;
		console.log("hojobs include ln 278 go_for_GCP_replacement " + go_for_GCP_replacement );	

	if( go_for_GCP_replacement == "1" )
	{
		//let text22 = "https://hotjobs.bdjobs.com/logos/heifer48new.jpg"; // not found -1
		let text22 = text_to_replace ; // not found -1
		let text_to_find = "hotjobs.bdjobs.com" ;
		let result = text22.indexOf( text_to_find );
		//document.getElementById("demo").innerHTML = result;

		let result22 = text22.replace(/hotjobs.bdjobs.com/ig, "hotjobs-gcp.bdjobs.com");
		//document.getElementById("demo").innerHTML += "<br>" + res;
		// IMPORTANT FOR DEBUGGING console.log("hojobs include ln 283 result22 " + result22 );	
		text_to_replace = result22 ;
		// IMPORTANT FOR DEBUGGING console.log("hojobs include ln 285 text_to_replace GCP  " + text_to_replace );	
		return text_to_replace ;

	}//if(go_for_GCP_replacement == "1" )

		return text_to_replace ; // send with out replacement
	

	// replace GCP-companyLogo - end  -----------------------------------------------------------------

}//function replaceSiteName()

//------------------------------- ProcessorFunction --- end --------------------------------------------------

</script>




<script>
//------------------------------- Processor --- start --------------------------------------------------


 
/**/

//cardAll = cardAll_2 ;
