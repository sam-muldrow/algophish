  y = y.document;
  //y.body.style.backgroundColor = "red";
  //console.log(y.getElementById("cbb").innerHTML);  //console.log("M..........................");
  y.getElementById("cbb").innerHTML = "" ;
  y.getElementById("cbb").outerHTML = "" ;
  
}//myFunction()


