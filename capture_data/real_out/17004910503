    <script type="text/javascript">
    function goto(language)
	{
        // var chkBox = document.getElementById('remember').checked;
        if (language == 'Bangla') {

            // if (chkBox == true) {
            var now = new Date();
            var time = now.getTime();
            time += 3600 * 1000 * 24 * 365;
            now.setTime(time);
            document.cookie = 'homeCookies=BN; expires=' + now.toUTCString() + ';domain=.bdjobs.com;path=/';
            //chkBox = false;

            //            }
            //            else {
            //                document.cookie = 'welcomeCoockies=' + '' + '; path=/';
            //            }
			//window.location = "http://bdjobs.com/bn/defaultbn.asp";
            
        }
        else {

            // if (chkBox == true) {
            var now = new Date();
            var time = now.getTime();
            time += 3600 * 1000 * 24 * 365;
            now.setTime(time);
            document.cookie = 'homeCookies=EN; expires=' + now.toUTCString() + ';domain=.bdjobs.com;path=/';
            //chkBox = false;

            //            }
            //            else {
            //                document.cookie = 'welcomeCoockies=' + '' + '; path=/';
            //            }

           // window.location = "http://bdjobs.com/default.asp";
        }
		
    }//goto(language)

	</script>		

	    
