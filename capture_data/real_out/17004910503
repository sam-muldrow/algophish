function Generategglevent()
{
	if(document.getElementById("txtKeyword").value.trim()!="")
	{
		_gaq.push(['_trackEvent', 'SeachPanelKeyword' , document.getElementById("txtKeyword").value,'EN',1.00,true]);
	}
	
	var e = document.getElementById("qOT");
	var strorg = e.options[e.selectedIndex].value;
	var strorgText = e.options[e.selectedIndex].text.trim();
	if(strorg!="0")
	{
		_gaq.push(['_trackEvent', 'OrganizationDropdown' , strorgText,'EN',1.00,true]);
		
	}
	
	
	_gaq.push(['_trackEvent', 'SeachPanelButton' , 'Search','EN',1.00,true]);
}
</script>
<script src="js/odometer.js?ver=05042023"></script>   

<!-- Added for new 'LC panel stop Animation' START 11may2023-1151am -->
<script>
      odometerOptions = { auto: false };
      (function () {
          $('[data-theme]').each(function () {
              var v = 0;
              var o = new Odometer({
                  el: this,
                  value: 0,
                  theme: $(this).data('theme')
              });
              o.render();              
              var sip = true;
              setInterval(function() {
                  if (sip) {
                      o.update(v++);
                  }
              }, 3000);              
              $(document).on("click","#f-sanim",function() {
                sip = !sip;
                $(".at-button").toggleClass("stop");
                if ($(".rotation.pause").hasClass("pause")) {                  
                  $(".rotation.pause").removeClass("pause").addClass("play");
                  $(".rotation.play").trigger("click");
                }else{                  
                  $(".rotation.play").removeClass("play").addClass("pause");
                  $(".rotation.pause").trigger("click");
                }
              });
          });
      })();
</script>
<!-- Added for new 'LC panel stop Animation' END   11may2023-1151am -->

<script type="text/javascript" >
// Added On 21sep2020 1142 am
function bdCommaFormatedNumber(number)
{
    numberZZ = "" + number
    var res = numberZZ.split("");
    resLen = res.length
/*  for(i = 0; i < nLen; i++)
    {
        console.log(nLen)
    }//for
*/ 
    newstr = ""
    //for(i=0;i<resLen;i++)
    //for(i=resLen;i>=0;i--)
    zz = 0 ;
    for(i =  resLen-1;i>=0;i--)
    {
        zz++;
        //newstr += res[i]s
        commaStr = ""
        //commaStr = zz == 4 ||  zz == 6 ||  zz == 8 ||  zz == 10 ? "," : "" ;
        commaStr = ( zz != 2 &&  zz % 2 == 0  ) ? "," : "" ;
        newstr = res[i] + commaStr + newstr
        //console.log(resLen)
        //console.log(  " -zz- " + zz + " -i- " + i + " -- " + newstr)
    }//for  
     //IMPORTANT for debugging OFF   console.log(  " -zz- searchpanel job stats coma separating  " + zz + " -i- " + i + " -- " + newstr)

	jsAllowSignLanguage = "0" ;
	if(jsAllowSignLanguage == "1")  //'<span class="hovered-content">New Jobs</span>'
	{
		newstr = "<span class='hovered-content'>" + newstr + "</span>" ; 
	}//if(jsAllowSignLanguage == 1) 

    return newstr ;
}//bdCommaFormatedNumber(number)

</script>   


				</div>

				<div class="col-md-3 col-sm-12">
					<style type="text/css">
	 .quick-links ul li {
    width: 100%;
    float: none;
    font-size: 12px;
    font-weight: 400;
    line-height: 20px;
    padding: 5px 0px 5px 0px;
}
    .quick-links ul li a{
    font-size: 12px;
}
.ql-list{
    float: none;
}

