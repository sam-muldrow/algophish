if (showLC_Panel_DebugData == 1 )
{
console.log("161 JS_totalCards :: " + JS_totalCards );
	
}//if (showLC_Panel_DebugData == 1 )	

showGreyRow = 1 // 0 1 
row_color = "" ;

//Company_Name = "দি রেমন্ড শপ (রাজশাহী)" ;
//Companys_Logo = "https://TUVR2K05-a.akamaihd.net/logos/thussain48.png" ;

thisIsTheLastCard = 0 ;
Company_CARD_data = "" ; 

JS_NEWLINE = "\n"




//cardSingle[i].JobID
//for (i = 0; i < cardSingle.length; i++) {
//for (CardLoopI = 0; CardLoopI < 4; CardLoopI++) {
for (CardLoopI = 0; CardLoopI < JS_totalCards; CardLoopI++) 
{

	if (showLC_Panel_DebugData == 1 )
	{
		//console.log(JSON.parse(cardAll));
		console.log(  cardAll); //"294 cardAll " +
		console.log( "295 " + CardLoopI);
		
	}//if (showLC_Panel_DebugData == 1 )	
  Company_Name = cardAll.Cards[ CardLoopI ].log_company.name ;
  //Company_Name = cardAll.Cards[ CardLoopI ].log_company.name + " " + CardLoopI ;
  //Company_Name = cardAll.log_company.name + " " + CardLoopI ;
  //Company_Name = cardAll.log_company+[CardLoopI].name + " " + CardLoopI ;
  
  //IMPORTANT FOR DEBUG console.log("226 Company_Name :: " + Company_Name );

// --------------------------------- COMPANY LOGO __________________ Start ____________________________

  //Companys_Logo = "https://hotjobs.bdjobs.comjobs/raymondrajshahi/raymondrajshahi.htm/logos/thussain48.png" ;

  Company_Logo_protocolLess = cardAll.Cards[ CardLoopI ].log_company.logo ; //+ " " + CardLoopI ;

	console.log("hojobs include ln 337 Company_Logo_protocolLess " + Company_Logo_protocolLess );	

  
//-------------------- replace GCP-companyLogo - start ----------------------------------------------

  Company_Logo_protocolLess = replaceSiteName( Company_Logo_protocolLess );

//-------------------- replace GCP-companyLogo - end  -----------------------------------------------

//"showLogo": "True",
  Company_showLogo = cardAll.Cards[ CardLoopI ].log_company.showLogo ; //+ " " + CardLoopI ;

//  if_logo_jpg = Company_Logo_protocolLess.endsWith(".jpg"); //jpg png PNG jpeg gif
//toLowerCase text.lastIndexOf("locate") endsWith

handleLOGOWith_ASP_JSON = 1 ;
handleLOGOWith_JS = 0 ;//1 ;

if (handleLOGOWith_ASP_JSON == 1 )
{
	if (Company_showLogo == "True" )
	{
		//"showLogo": "True",
		if (showLC_Panel_DebugData == 1 )
		{
			console.log("hotjobs_JSON ln-387 if_showLogo :: " + Company_showLogo );
			
		}//if (showLC_Panel_DebugData == 1 )	
		showCompanyLogo = 1 ;

		if_logo = Company_Logo_protocolLess.startsWith("https:");
		if (showLC_Panel_DebugData == 1 )
		{
			console.log("hotjobs_JSON ln-393 if_logo :: " + if_logo );
			
		}//if (showLC_Panel_DebugData == 1 )	
		if ( if_logo == true )
		{
			// logo_link has https
			Company_Logo = "" + Company_Logo_protocolLess ; //+ " " + CardLoopI ;
		}
		else
		{  
			// logo_link doesnot have https
			Company_Logo = "https:" + Company_Logo_protocolLess ; //+ " " + CardLoopI ;

		}//if ( if_logo == true )


	}//if (Company_showLogo == "True" )
	else if (Company_showLogo == "False" )
	{
		//"showLogo": "True",
	if (showLC_Panel_DebugData == 1 )
	{
		console.log("hotjobs_JSON ln-387 else_if_showLogo :: " + Company_showLogo );
		
	}//if (showLC_Panel_DebugData == 1 )	
		showCompanyLogo = 0 ;
	}//if (Company_showLogo == "True" )


}//if (handleLOGOWith_ASP_JSON == 1 )

//===================================================================================================
if (handleLOGOWith_JS == 1 )
{
	showCompanyLogo = 1 ;
	if( Company_Logo_protocolLess == "//hotjobs.bdjobs.com/logos/" )
	{
		showCompanyLogo = 0 ;
	}//if( Company_Logo_protocolLess == "//hotjobs.bdjobs.com/logos/" )
	Company_Logo = "https:" + Company_Logo_protocolLess ; //+ " " + CardLoopI ;
	
	//if_logo = Company_Logo_protocolLess.firstIndexOf("https:");
	if_logo = Company_Logo_protocolLess.startsWith("https:");
	if (showLC_Panel_DebugData == 1 )
	{
		console.log("hotjobs_JSON ln-379 if_logo :: " + if_logo );
		
	}//if (showLC_Panel_DebugData == 1 )	
	if ( if_logo == true )
	{
		// logo_link has https
		Company_Logo = "" + Company_Logo_protocolLess ; //+ " " + CardLoopI ;
	}
	else
	{  
		// logo_link doesnot have https
		Company_Logo = "https:" + Company_Logo_protocolLess ; //+ " " + CardLoopI ;

	}//if ( if_logo == true )

	//text.lastIndexOf("locate"); //text.startsWith("Hello");

}//if (handleLOGOWith_JS == 1 )
// --------------------------------- COMPANY LOGO __________________ End ____________________________

	CurrentCardNumberForSingleCard = CardLoopI + 1 ;
	if (showLC_Panel_DebugData == 1 )
	{
		console.log(" LN 4197 CurrentCardNumberForSingleCard "+  CurrentCardNumberForSingleCard   );
		
	}//if (showLC_Panel_DebugData == 1 )	


 // if ( CurrentCardNumberForSingleCard == Cards_IN_A_ROW )
  //if ( CardLoopI == (Cards_IN_A_ROW - 1) )
  //if ( CardLoopI == (Cards_IN_A_ROW ) )
	if( (CurrentCardNumberForSingleCard % ( Cards_IN_A_ROW )) == 0 && CardLoopI >= Cards_IN_A_ROW - 1 )
{
    thisIsTheLastCard = 1 ;
    thisIsTheCardsLastClass = "" ;
	if (showLC_Panel_DebugData == 1 )
	{
	console.log("4201 thisIsTheLastCard :: " + thisIsTheLastCard + " CardLoopI " + CardLoopI );
		
