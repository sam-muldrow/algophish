     $(document).on("click", "#f-linkh", function() {
         $("a").toggleClass("lnk-highlited");
         $("button").toggleClass("lnk-highlited");
         $('input[type="submit"],input[type="button"]').toggleClass("lnk-highlited");
         $('div[role=button]').toggleClass("lnk-highlited");
     });
     var $affectedElementsmm = $("h1, h2, h3, h4, h5, h6, [role=heading]");
     $(document).on("click", "#f-sheadns", function() {
         $affectedElementsmm.each(function() {
             $(this).toggleClass("headings-highlited");
         });
     });
     $(document).on("click", "#f-readgde", function() {
         $(".rguide").toggleClass("shd");
         $(document).on('mousemove', function(e) {
             $('.rguide').css('top', e.pageY - 15);
         });
     });
     $(document).on("click", "#f-readmsk", function() {
         $(".acsb-reading-mask-element").toggleClass("hide");
     });
     $.fn.toggleAriaPressed = function() {
         if($(this).attr("aria-pressed") == "true") {
             $(this).attr("aria-pressed", "false");
         } else {
             $(this).attr("aria-pressed", "true");
         }
     }
 });
 $(window).on("keydown", function(e) {
     if(e.which == 9) {
         if(isOffLayer) {
             $(".skip-main").get(0).focus();
             isOffLayer = false;
             e.preventDefault();
         }
     }
 });
 $(document).mouseup(function(e) {
     var cusc = $(".control-panel");
     if(!cusc.is(e.target) && cusc.has(e.target).length === 0) {
         $(".control-panel").removeClass("open").removeAttr("tabindex");
         $(".control-panel").find(".btn").removeAttr("tabindex").removeAttr("role").removeAttr("aria-pressed");
         $(".control-panel").find(".acctoggle-btn").removeAttr("tabindex").removeAttr("role").removeAttr("aria-pressed");
         $(".acctoggle-btn").show();
     }
 });
 $(document).ready(function() {
     readingmask();
     $(window).on("resize", readingmask);
 
     function readingmask() {
         $(document).mousemove(function(e) {
             var pageHeight = document.documentElement.scrollHeight;
             $(".acsb-reading-mark-element-top").css({
                 "height": e.pageY - 55,
             });
             $(".acsb-reading-mark-element-bottom").css({
                 "height": pageHeight - e.pageY - 55,
             });
         });
     }
 });

 $("#htalkbtn").click(function() {
   if($(this).hasClass("ssac")) {       
     $(".hndtk-panel").removeClass("open");
     $("html").removeClass("hndcursor");
     $(".hovered-content .wr.dsl").removeClass("dsl");     
   } else {
     $(".hndtk-panel").addClass("open");
     $("html").addClass("hndcursor");
     $(".hovered-content .wr").addClass("dsl");
   }    
 });
</script>
