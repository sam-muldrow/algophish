	Company_CARD_data += JS_NEWLINE + "<div class='col-md-3 col-xs-6 c-card" + thisIsTheCardsLastClass + "'>" ;
	Company_CARD_data += JS_NEWLINE +  "    <div class='row'>" ;

	//jobCount  =  cardSingle.log_company.jobCount   ; //cardSingle[CardLoopI].jobCount 
	//jobCount  =  cardAll.log_company.jobCount   ;  
	jobCount  =  cardAll.Cards[ CardLoopI ].log_company.jobCount   ;  
