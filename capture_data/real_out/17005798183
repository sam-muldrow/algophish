        <script>
        window.addEventListener("DOMContentLoaded", function() {
            document.dispatchEvent(new Event("alpine:ready"));
            Alpine.start();
        });

        var loadDeferredStyles = function() {
            var addStylesNode = document.getElementById("deferred-styles");
            var replacement = document.createElement("div");
            replacement.innerHTML = addStylesNode.textContent;
            document.body.appendChild(replacement);
            addStylesNode.parentElement.removeChild(addStylesNode);
            // font
            if (!/Mac|iPhone|iPod|iPad/.test(navigator.platform)) {
                console.log('[DEBUG] 需要載入字型！');
                var addFontNode = document.getElementById("deferred-fonts");
                replacement = document.createElement("div");
                replacement.innerHTML = addFontNode.textContent;
                document.body.appendChild(replacement);
                addFontNode.parentElement.removeChild(addFontNode);
            }

        };
        var raf = requestAnimationFrame || mozRequestAnimationFrame ||
            webkitRequestAnimationFrame || msRequestAnimationFrame;
        if (raf) raf(function() {
            window.setTimeout(loadDeferredStyles, 0);
        });
        else {
            window.addEventListener('DOMContentLoaded', loadDeferredStyles);
        }
    </script>

    
