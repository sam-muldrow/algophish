  <script>
   function setCookiBrfreeHide(name) {
    var date = new Date();
    var totime = date.getTime() + 24 * 60 * 60 * 1000;
    MApp(2.2).cookie.set(name, totime);
  }
  function isBlank(data) {
    if (
      data == null ||
      data === 'null' ||
      data === '' ||
      data === undefined ||
      data === 'undefined' ||
      data === 'unknown'
    ) {
      return true;
    } else {
      return false;
    }
  }
 </script> 
 
  <div class="br-botpop-right" data-name="indxen">
  <svg class="br-close" onclick="">
    <use xlink:href="#pop_close"></use>
  </svg>
  <p>
    Be the first to know what's on sale this  <span>Black Friday</span>
  </p>
  <a onclick="gtag('event', 'en_bfsale_buyclick_index_rpopol');" target="_blank"  href="https://purchase.iobit.com/en/sale/blackfridaysale.php?ref=index_popol" class="blackfriday-btn middle-btn">
    Have A Look
  </a>
 </div>
 <script>
  $(function () {
    let multidate = new Date(),
      multibname = '',
      multibcookie = '',
      multibcookiestr = '';
    multibname = $(".br-botpop-right").attr("data-name");
    if (!isBlank(multibname)) {
      multibname = multibname.replace(/(^\s*)|(\s*$)/g, "") || multibname;
    }
    if (multibname != 'store') {
      console.log(multibname);
      multibcookie = multibname + 'botpop';
      multibcookiestr = MApp(2.2).cookie.get(multibcookie);
     
      if (!multibcookiestr || (multibcookiestr && (multibcookiestr < multidate.getTime()))) {
        gtag('event', 'en_bfsale_show_index_rpopol');
        $(window).on('scroll', function() {
        if ($(window).scrollTop() > 800) {
          $(".br-botpop-right").addClass(showclass);
          $(".backtop").addClass('on');
        } else {
          $(".br-botpop-right").removeClass(showclass);
          $(".backtop").removeClass('on');
        }
       
      });
      } else {
        $(".br-botpop-right").removeClass(showclass);
        setTimeout(function(e){
          $(".br-botpop-right").remove();
        },300);
        $(".backtop").removeClass('on');
      }
    
      $(".br-botpop-right .br-close").on('click', function (evt) {
        setCookiBrfreeHide(multibcookie);
        setTimeout(function(e){
          $(".br-botpop-right").remove();
        },300);
        $(".backtop").removeClass('on');
      });
      $(".br-botpop-right .blackfriday-btn").off('click').on('click', function (evt) {
        if (!multibcookiestr || (multibcookiestr && (multibcookiestr < multidate.getTime()))) {
          setCookiBrfreeHide(multibcookie);
        }
        $(".br-botpop-right").removeClass('on').removeClass("live0").removeClass("live");
        setTimeout(function (e) {
          $(".br-botpop-right").remove();
        }, 300);
      });

     

     
    }
  });
</script>
 
   
        
