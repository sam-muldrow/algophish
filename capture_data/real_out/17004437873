  var links = document.querySelectorAll('a[href*=".php"]');
	for (var i = 0; i < links.length; i++) {
	var link = links[i];
	var aff_href = link.getAttribute('href');
	if (aff_href) {
		var hasParams = aff_href.indexOf('?') !== -1;
		var newHref = hasParams ? aff_href + '&AFF='+url_aff : aff_href + '?AFF='+url_aff;
		link.setAttribute('href', newHref);
	}
	}
}
