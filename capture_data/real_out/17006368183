    <script>
        window.googletag.cmd.push(
            function(){
                // If GPT is not yet loaded, this code will be executed subsequently when
                // the command queue is processed. Every queued function will use the last value
                // assigned to x (most likely slotCount).
                // This is because the function closure captures the reference to x,
                // not the current value of x.
                googletag.pubads().addEventListener('slotRenderEnded', function(event) {
                    let slot = event.slot;
                    console.log("baner", event);

                    let bannerID = slot.getSlotElementId();
                    if(bannerID.includes("article")) {
                        console.log("slot: ", bannerID);
                        let articleContainerStyle = document.getElementById("google_ads_iframe_/23983747/kasvyksta.lt_560x300_"+bannerID+"_0");
                        if(articleContainerStyle.contentWindow) {
                            articleContainerStyle.contentWindow.document.getElementById("google_image_div").style.cssText += "text-align: center! important; position: relative !important;";
                        }
                    }
                    if(bannerID.includes("general")) {
                        //console.log("related slot: ", bannerID);
                        let relatedArticleContainerStyle = document.getElementById("google_ads_iframe_/23983747/kasvyksta.lt_860x200_"+bannerID);
                        if(relatedArticleContainerStyle.contentWindow) {
                            relatedArticleContainerStyle.contentWindow.document.getElementById("google_image_div").style.cssText += "text-align: center! important; position: relative !important;";
                        }
                    }

                });
            })

    </script>

