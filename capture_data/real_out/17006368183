	            googletag.display('kasvyksta_lt_300x600_sidebar_5');
						stpd.que.push(function(){ stpd.initializeAdUnit('kasvyksta_lt_300x600_sidebar_5'); })
	        });
    	} else if(window.innerWidth > 600 && window.innerWidth < 1279) {
    		googletag.cmd.push(function () {
				    googletag.defineSlot('/147246189,23983747/kasvyksta.lt_220x600_tablet_sidebar_5', [[220, 600], [160, 600] , 'fluid'],  'kasvyksta_lt_300x600_sidebar_5').addService(googletag.pubads());
	            googletag.display('kasvyksta_lt_300x600_sidebar_5');
				stpd.que.push(function(){ stpd.initializeAdUnit('kasvyksta_lt_300x600_sidebar_5'); })
	        });
    	}
    }));    
}
	
</script>
</div>
</div></div></li>
