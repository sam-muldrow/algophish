    <script type="text/javascript">
        getList();

        function getList(page) {
            http("get", "/homepage/widget/60/items").then(res => {
                var data = res.data.data;
                var href = ""
                if (data.length == 0) {
                    $(".toutiao").hide();
                    return
                }
                for (let i = 0; i < 7; i++) {
                    if (data[i].redirectable.redirect_type == "internal") {
                        href = data[i].redirectable.redirectable_type == "special" ? "specialDetail.html?id=" +
                            data[i].redirectable.redirectable_id : "articleDetail.html?id=" + data[i]
                            .redirectable.redirectable_id;
                    } else {
                        href = data[i].redirectable.redirect_url
                    }
                    if (i == 0) {
                        $(".toutiao .mainTitle a").text(data[0].title);
                        $(".toutiao .mainTitle a").attr("href", href);

                    } else {
                        var list = $(
                            `<li class="list"><a target="_blank" href="${href}" style="color: ${data[i].has_color==2?'#d9423b':"#000"}">${data[i].title}</a></li>`
                        )
                        $(".toutiao .lists").append(list)
                    }

                }

            }).catch(err => {

            })
        }
    </script>
