<script>
	$(function(){
		//ææºå·åç¨æ·åç»å½åè°å°å
		var backUrlNewLogin = window.location.href;
		backUrlNewLogin = encodeURIComponent(backUrlNewLogin);
		
		//å·æ°å¾å½¢éªè¯ç 
		function dochangePh(){
			$('#verifyImageRegPh').attr('src','/servlet/verifyCode?type=reg&t='+Math.random());
		}
		function dochangeZh(){
			$('#verifyImageRegZh').attr('src','/servlet/verifyCode?type=reg&t='+Math.random());
		}
		function checkPhone(phone){
		    if(!(/^1(3|4|5|6|7|8|9)\d{9}$/.test(phone))){
		        return false;
		    } else {
		        return true;
		    }
		}
		var canGetYzm = true;
		var newtimer = null;
		var conseconed = 60;
		
		function sendYzm() {
		    if(canGetYzm){
		        var userName = $("#userNamePh").val();
		        if(userName!==""){
		            var checkphone = checkPhone(userName);
		            if(checkphone){
		                $('#errorInfo').hide();
		                $('#errorInfo').html("");
		                newtimer = setInterval(function(){
		                    if(conseconed==0){
		                        $("#sendyzmbtn").html('è·åéªè¯ç ');
		                        clearInterval(newtimer);
		                        canGetYzm = true;
								conseconed = 60;
		                    }else{
		                        conseconed--;
		                        $("#sendyzmbtn").html(conseconed);
		                    }
		                },1000);
		                setTimeout(function(){
		                    $.ajax({
		                        type: 'POST',
		                        url: '/ssc/sendYzm.jsp',
		                        data: {"userName": userName},
		                        dataType: 'html',
		                        async: false,
		                        success: function (isExists) {
		                            if (isExists) {
		                                alert('éªè¯ç å·²ä¸å');
		                            } else {
		                                alert('éªè¯ç ä¸åå¤±è´¥');
		                            };
		                            $("#sendyzmbtn").html('è·åéªè¯ç ');
		                            clearInterval(newtimer);
		                            canGetYzm = true;
									conseconed = 60;
		                        }
		                    });
		                },3000)
		            }else{
		                $('#errorInfoPh').show();
		                $('#errorInfoPh').html("<em></em>æ¨çææºå·å¡«åæè¯¯");
		            }
		        }
		    }
		};
		
		//ç»å½æäº¤
		function onsub(type){
			var couldClick = $(".loginBtn").hasClass("canclick");
			if(!couldClick){
				return false;
			}
			if(type=="0"){
				var userName= $('#userNamePh').val();
				var userPwd= $('#userPwdPh').val();
				var verifyCode= $('#verifyCodePh').val();
				if(userName==''){
					$('#errorInfoPh').show();
					$('#errorInfoPh').html("<em></em>ææºå·ä¸è½ä¸ºç©º");
					return;
				}
				if(userPwd==''){
					$('#errorInfoPh').show();
					$('#errorInfoPh').html("<em></em>éªè¯ç ä¸è½ä¸ºç©º");
					return;
				}
				if(verifyCode==''){
					$('#errorInfoPh').show();
					$('#errorInfoPh').html("<em></em>å¾å½¢éªè¯ç ä¸è½ä¸ºç©º");
					$('#verfyPh').show();
					return;
				}
				$("#frmLoginPh").find("#retUrlPh").val(backUrlNewLogin);
				$('#frmLoginPh').submit();
			}else if(type=="1"){
				var userName= $('#userNameZh').val();
				var userPwd= $('#userPwdZh').val();
				var verifyCode= $('#verifyCodeZh').val();
				if(userName==''){
					$('#errorInfoZh').show();
					$('#errorInfoZh').html("<em></em>ç¨æ·åä¸è½ä¸ºç©º");
					return;
				}
				if(userPwd==''){
					$('#errorInfoZh').show();
					$('#errorInfoZh').html("<em></em>å¯ç ä¸è½ä¸ºç©º");
					return;
				}
				if(verifyCode==''){
					$('#errorInfoZh').show();
					$('#errorInfoZh').html("<em></em>å¾å½¢éªè¯ç ä¸è½ä¸ºç©º");
					$('#verfyZh').show();
					return;
				}
				if(userName==userPwd){
					alert("æ¨çç»å½å¯ç ä¸ä¼ååè¿äºç¸ä¼¼ï¼ä¸ºäºæ¨çå®å¨å»ºè®®æ¨æ´æ¢å¯ç ");
				}
				$("#frmLoginZh").find("#retUrlZh").val(backUrlNewLogin);
				$('#frmLoginZh').submit();
				
			}
		}
		
		$(".changetab .loginname").click(function(){
			var attrs = $(this).attr("attr")
			$(this).addClass("active").siblings().removeClass("active");
			if(attrs=="0"){
				$("#loginbutton").attr("attr","0")
				$("#iphonemodel").show()
				$("#zhpassmodel").hide()
			}else{
				$("#loginbutton").attr("attr","1")
				$("#iphonemodel").hide()
				$("#zhpassmodel").show()
			}
		})
		
		$(".logincloses").click(function(){
			$("#newloginmask").hide()
		});
		
		$("#userName,#userPwdPh,#userPwdZh").keypress(function(){
			var vals = $(this).val();
			if(vals!==""){
				$(".loginBtn").addClass("canclick");
			}else{
				$(".loginBtn").removeClass("canclick");
			}
		}).blur(function(){
			var vals = $(this).val();
			if(vals!==""){
				$(".loginBtn").addClass("canclick");
			}else{
				$(".loginBtn").removeClass("canclick");
			}
		});
		
		document.onkeydown = function(e) {
			e = e || event;
			if(e.keyCode == 13){
				onsub();
				return false;
			};
		}
		//Â éªè¯ç 
		$("#sendyzmbtn").click(function(){
			sendYzm();
		});
		
