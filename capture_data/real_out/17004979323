<script>
    var tourl = ''
    let baseUrl = 'http://cp.zgzcw.com/hongdan'
    let data = {
        type:6,
        sid:'26000000000',
        callback:'callback'
    }
    $.ajax({
        url:baseUrl+'/banner.action',
        method:'get',
        data:data,
        jsonp: 'callback',
        dataType: 'jsonp',
        success:function(res){
            if(res.code==='0000'){
                if(res.data.length>0){
                    let shouGg = localStorage.getItem('shouGg')
                    $('#footerBanner .cont_div img').attr('src',res.data[0].imgUrl)
                    if(res.data[0].toUrl){
                        tourl = res.data[0].toUrl
                    }
                    if(shouGg){
                        $('#footerBanner').hide()
                    }else{
                        $('#footerBanner').show()
                    }
                }else{
                    $('#footerBanner').hide()
                }
            }
        }
    })
    $('#footerBanner .cont_div .colse').on('click',function(e){
        localStorage.setItem('shouGg',true)
        $('#footerBanner').hide()
        // é»æ­¢åæ³¡
        e.stopPropagation()
    })
    $('#footerBanner .cont_div img').on('click',function(){
        if(tourl){
            window.open(tourl,'_blank')
        }else{
            return
        }
    })
</script>
