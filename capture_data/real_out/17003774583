				function showTips() {
					var tipsmodal = modal("tipsModal", "500px", "auto");

					if (document.querySelector("#tipsModal > .modal-content > .modal-body").innerHTML.length > 0) {
						tipsmodal.show();
					} else {
						var xmlHttp = new XMLHttpRequest();
						xmlHttp.onreadystatechange = function () {
							if (xmlHttp.readyState == 4 && xmlHttp.status == 200) {
								document.querySelector("#tipsModal > .modal-content > .modal-body").innerHTML = xmlHttp.responseText;
								tipsmodal.show();
							}
						}
						xmlHttp.open("GET", "/docs/autocomplete/tipscontent", true); // true for asynchronous
						xmlHttp.send(null);
					}
				}
			</script>
