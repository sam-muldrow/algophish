<script nonce="e_ooOIHqFsNQuTlE6KUa_pzdZV5ZsKnP8eIbpwqaTNw" type="text/javascript">
	$(document).ready(function() {
		var img = $('<a class="banner_configurar_cookies" href="javascript:Cookiebot.show();"><img src="/frontend/img/accesorio/video-bloqueado-cookies-es.jpg"  alt="video bloqueado cookies" class="img-responsive center-block"   /></a>');
		img.insertBefore('iframe[data-src*="youtube"]');
