function oeRqBvpwCt89c(object,isVisible) {
    if (object!=null) {
        if (object.style) {
            object.style.visibility = isVisible ? 'visible' : 'hidden';
        }
        else if (object.visibility) {
            object.visibility = isVisible ? 'SHOW' : 'HIDE';
        }
    }
}


