function contiene_caracteres_validos(cadena) {
    // &acute;`&iexcl;><*#?
    if(
        cadena.indexOf(" ") != -1 || // espacio en blanco
        cadena.indexOf("\264") != -1 || // &acute;
        cadena.indexOf("`") != -1 ||
        cadena.indexOf("\241") != -1 || // &iexcl;
        cadena.indexOf(">") != -1 ||
        cadena.indexOf("<") != -1 ||
        cadena.indexOf("*") != -1 ||
        cadena.indexOf("#") != -1 ||
        cadena.indexOf("?") != -1
    ) return false;
    else return true;
}

