	MMG.cmpCmd.push((success, tcString, tcData) => {
		const random = ((min, max) => {
			return Math.floor(Math.random() * (max - min + 1)) + min;
		})(0, 3);

		if (0 === random) {
			$('#vjs-rcp1').remove();

