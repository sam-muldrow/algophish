	MMG.abcCmd.push(() => {
		$('.abc-test-article-titles').abcTester({
			localStoragePrefix: 'article_titles',
			projectId: 4,
			mode: 'count',
			replaceCallback: function ($object, title) {
				if (title.length > 80 && $object.parents('.small-article-box').length) {
					$object.html(title.slice(0, 80) + '…');
				}
			}
		});

		$('.abc-test-article-thumbs').abcTester({
			localStoragePrefix: 'article_thumbs',
			projectId: 5,
			mode: 'count',
			subjectType: 'image',
			replaceCallback: function ($object) {
				$object.show();
			}
		});
	});
</script>

