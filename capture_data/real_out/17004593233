	<script>
		var MMG = MMG || {};
		MMG.abcCmd = MMG.abcCmd || [];

		MMG.abcCmd.push(() => {
			$('.abc-test-movie-titles').abcTester({
				localStoragePrefix: 'movie_titles',
				projectId: 10,
				mode: 'count'
			});

			$('.abc-read-movie-title').abcTester({
				localStoragePrefix: 'movie_titles',
				projectId: 10,
				mode: 'read',
				replaceCallback: function ($object, title) {
					const titleLength = parseInt($object.attr('data-title_length'));

					if (isNaN(titleLength) || title.length <= titleLength) {
						return;
					}

					$object.text(title.slice(0, titleLength) + '…');
				}
			});

			$('.abc-test-movie-thumbs').abcTester({
				localStoragePrefix: 'movie_thumbs',
				projectId: 9,
				mode: 'count',
				subjectType: 'image'
			});

			$('.abc-read-movie-thumb').abcTester({
				localStoragePrefix: 'movie_thumbs',
				projectId: 9,
				mode: 'read',
				subjectType: 'image'
			});
		});
	</script>



