			var wmd_join_multisite_signup_form_validate_account_blog = {
				fields: {},
				when: 'change',
				happy: function() {wmd_join_multisite_signup.form_status['account-blog'] = true;},
				unHappy: function() {wmd_join_multisite_signup.form_status['account-blog'] = false;}
			};

