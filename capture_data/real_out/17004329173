<!-- GA計測 -->

<script type="text/javascript">
  // タイムラインモーダル表示（page_view）
  $('[data-gtm-name="top_timeline_modal"]').on('click', function() {
    dataLayer.push({
      'page_name': 'トップ内タイムライン',
      'content_id': $(this).parent().data('gtm-value_content_id'),
      'content_title': $(this).parent().data('gtm-value_title'),
      'shop_id': $(this).parent().data('gtm-value_shop_id'),
      'shop_name': $(this).parent().data('gtm-value_shop_name')
    });
  });
  // クーポンモーダル表示（page_view）
  $('[data-gtm-name="top_coupon_modal"]').on('click', function() {
    dataLayer.push({
      'page_name': 'トップ内クーポン',
      'content_id': $(this).parent().data('gtm-value_content_id'),
      'content_title': $(this).parent().data('gtm-value_title'),
      'shop_id': $(this).parent().data('gtm-value_shop_id'),
      'shop_name': $(this).parent().data('gtm-value_shop_name')
    });
  });
  // ピックアップモーダル表示（page_view）
  $('[data-gtm-name="top_pickup_modal"]').on('click', function() {
    dataLayer.push({
      'page_name': 'トップ内ピックアップ',
      'content_id': $(this).parent().data('gtm-value_content_id'),
      'content_title': $(this).parent().data('gtm-value_title'),
      'shop_id': $(this).parent().data('gtm-value_shop_id'),
      'shop_name': $(this).parent().data('gtm-value_shop_name')
    });
  });

  // お気に入り登録
  $(document).on('click', '[data-gtm-name="top_fav"]', function() {
    if (!$(this).hasClass('btn_favorite_on')) {
      var contentType = $(this).parent().attr('class');
      var item_id = "";
      if(contentType.includes("minichirashi")) {
        // タイムライン
        item_id = "top_timeline_fav_on";
      } else if(contentType.includes("coupon")) {
        // クーポン
        item_id = "top_coupon_fav_on";
      } else if(contentType.includes("pickup")) {
        // ピックアップ
        item_id = "top_pickup_fav_on";
      } else if(contentType.includes("newshop")) {
        // 新規掲載店舗
        item_id = "top_newshop_fav_on";
      } else {
        // チラシ
        item_id = "top_chirashi_fav_on";
      }
      dataLayer.push({
        'event': 'top_fav_on',
        'shop_id': $(this).attr('item-id'),
        'shop_name': $(this).attr('item-name'),
        'item_id': item_id,
        'item_name': 'お気に入り登録',
        'value': 1
      });
    }
  });
  // お気に入り解除
  // 下記data属性は/misc/pmtshare/shufoo2/www/site/202207/js/common.jsに記載
  $(document).on('click', '[data-gtm-name="fav_off"]', function() {
    dataLayer.push({
      'event': 'top_fav_off',
      'shop_id': $(this).data('gtm-value_shop_id'),
      'shop_name': $(this).data('gtm-value_shop_name'),
      'item_id': $(this).data('gtm-value_item_id'),
      'item_name': 'お気に入り解除',
      'value': -1
    });
  });
</script>


