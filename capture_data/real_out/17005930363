<script>
    if (matchMedia) {
        const mq = window.matchMedia("(max-width: 993px)");
        mq.addListener(WidthChange);
        WidthChange(mq);
    }

    // media query change
    function WidthChange(mq) {
        if(mq.matches) {
            $('.new-landing #mobile-reports .featured-img').matchHeight({
                'byRow' : false,
                'target': $('#mobile-reports #reports .featured-img')
            });
        }
    }
    $(document).ready(function(){
        $('.featured-title').matchHeight({ 'byRow' : false});

        $('.featured-body').matchHeight({ 'byRow' : false});

        $('.reports').matchHeight({ 'byRow' : false});
    });
</script>

