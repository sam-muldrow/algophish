                }
              }
              if (isSubscriber == true) {
                localStorage.setItem("dL_tipoUsuario", "Suscriptor");
                location.reload();
              } else {
                localStorage.setItem("dL_tipoUsuario", "Registrado");
                location.reload();
              }
            });
          },
          loggedOut: function () {
            location.reload();
          },
        });
        e.preventDefault();
        e.stopPropagation();
      }
    if(document.getElementById("loginsso2")){
      document.getElementById("loginsso2").addEventListener("click", showLogin);
    }
    if(document.getElementById("loginsso2-mobile")){
      document.getElementById("loginsso2-mobile").addEventListener("click", showLogin);
    }
  
});
