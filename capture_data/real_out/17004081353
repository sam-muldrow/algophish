        var isSubscriber = false;
        if (dataZ.subscriptions.length > 0) {
          if (typeof dataZ.subscriptions != "undefined") {
            for (var i in dataZ.subscriptions) {
              if (dataZ.subscriptions[i].status ==="active") {
                tp.offer.close();
                isSubscriber = true;
                localStorage.setItem(
                  "dL_tipoSuscripcion",
                  dataZ.subscriptions[i].term.resource.name,
                );
              }
            }
          }
        }
        if (isSubscriber == true) {
          localStorage.setItem("dL_tipoUsuario", "Suscriptor");
          location.reload();
        } else {
          localStorage.setItem("dL_tipoUsuario", "Registrado");
