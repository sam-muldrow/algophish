        tp.pianoId.show({
          displayMode: "modal",
          screen: "login",
          containerSelector: "#my-account",
          loggedIn: function (data) {
            localStorage.setItem("dL_idPiano", data.user.uid);
            tp.api.callApi("/subscription/list", {}, function (dataZ) {
              var isSubscriber = false;      
              if (dataZ.subscriptions.length > 0) {
                if (typeof dataZ.subscriptions != "undefined") {
                  for (var i in dataZ.subscriptions) {
                    if (dataZ.subscriptions[i].status ==="active") {
                      tp.offer.close();
                      isSubscriber = true;
                      localStorage.setItem(
                        "dL_tipoSuscripcion",
                        dataZ.subscriptions[i].term.resource.name,
                      );
