                // SOURCE: https://github.com/tagticians/segment-onetrust-integration-cookie
                !function(){
                  function getCookie(name) {
                    var value = '; ' + document.cookie;
                    var parts = value.split('; ' + name + '=');
                    if (parts.length >= 2) return parts.pop().split(';').shift();
                  }
                  function getGaSessions() {
                    const cookie = {};
                    document.cookie.split(';').forEach((el) => {
                        const splitCookie = el.split('=');
                        const key = splitCookie[0].trim();
                        cookie[key] = splitCookie[1];
                    });
                    const sessions = Object.keys(cookie)
                          .filter(key => key.startsWith('_ga'));
                    const returnObj = {};
                    sessions.forEach((session) => {
                      let sessionSplit = session.split('_ga_');
                      if (sessionSplit.length > 1) {
                         let sessionString = cookie[session];
                         returnObj['G-' + session.split('_ga_')[1]] = sessionString.split('.')[2];
                      }
                    });
                    return returnObj;
                  }
                  function getGaClientId() {
                    return getCookie('_ga')?.substring(6);
                  }
                  
                  function checkIfCookieValueExists(cookiename) {
                    if (typeof(cookiename) == 'string' && cookiename != '') {
                      const COOKIES = document.cookie.split(';');
                      for (i = 0; i < COOKIES.length; i++) {
                        if (COOKIES[i].trim().startsWith(cookiename)) {
                          return COOKIES[i].split('=')[1];
                        }
                      }
                    }
                    return null;
                  }
                   
                    const isAgent = () => (getCookie('LOGGEDIN_AS_AGENT') !== undefined
                            ? getCookie('LOGGEDIN_AS_AGENT')
                            : false);
                    const isLoggedIn = () => (getCookie('IsGuestUser') !== undefined ? getCookie('IsGuestUser').toLowerCase() === 'false' : false);
        
                    const defaultProperties = {
                      is_agent: isAgent(),
                      is_logged_in: isLoggedIn(),
                      is_internal_user: false // what value is this?
                    }
        
                    function getUserContext(){
                      const getIntercomDeviceId = () =>
                              checkIfCookieValueExists(getCookie('intercom-device-id-cbw30hb8')) !== null ? getCookie('intercom-device-id-cbw30hb8')
                                      : getCookie('intercom-device-id-br2tge1x');
                      return {
                        google_click_id: getCookie('sem_gclid'),
                        bazaar_voice_brand_id: getCookie('BVBRANDID'),
                        quantum_metric_user_id: getCookie('QuantumMetricUserID'),
                        launch_darkly_id: '',
                        intercom_device_id: getIntercomDeviceId(),
                        user_pseudo_id: getGaClientId(),
                        optimizely_end_user_id: getCookie('optimizelyEndUserId'),
                        lz_cid: getCookie('lzCID'),
                        lz_uuid: getCookie('lz_uuid'),
                        ga_sessions: getGaSessions(),
                        fbp: getCookie('_fbp'),
                        fbc: getCookie('_fbc'),
                      }
                    }
                
                  function registerAndCall() {
                      window.analytics?.register({
                          name: 'OneTrust Integration Cookie',
                          version: '0.1.0',
                          type: 'enrichment',
                          isLoaded: () => true,
                          load: () => Promise.resolve(),
                          // context object and reference to the analytics.js instance
                          page: (ctx) => {
                              if(ctx.event.context.consent) {
                                ctx.event.context.consent.google = {ad_storage: ctx.event.context.consent.categoryPreferences["C0004"] ? "granted" : "denied", analytics_storage: ctx.event.context.consent.categoryPreferences["C0005"] ? "granted" : "denied"}
                              }
                              return ctx;
                          },
                          track: (ctx) => {
                              if(ctx.event.context.consent) {
                                ctx.event.context.consent.google = {ad_storage: ctx.event.context.consent.categoryPreferences["C0004"] ? "granted" : "denied", analytics_storage: ctx.event.context.consent.categoryPreferences["C0005"] ? "granted" : "denied"}
                              }
                              return ctx;
                          },
                          identify: (ctx) => {
                              if(ctx.event.context.consent) {
                                ctx.event.context.consent.google = {ad_storage: ctx.event.context.consent.categoryPreferences["C0004"] ? "granted" : "denied", analytics_storage: ctx.event.context.consent.categoryPreferences["C0005"] ? "granted" : "denied"}
                              }
                              return ctx;
                          },
                      });
                      
                      var excludedUserAgents = [
                        'Googlebot',
                        'Bingbot',
                        'Slurp',
                        'DuckDuckBot',
                        'Baiduspider',
                        'Datadog'                       
                      ];
                      
                      window.analytics?.register({
                          name: 'LegalZoom Bot Filter',
                          version: '1.0.0',
                          type: 'before',
                          isLoaded: () => true,
                          load: () => Promise.resolve(),
                          // context object and reference to the analytics.js instance
                          page: (ctx) => {
                              if(excludedUserAgents.some((ua) => navigator.userAgent.includes(ua))) {
                                return ctx.cancel();
                              } else {
                                return ctx;
                              }
                          },
                          track: (ctx) => {
                              if(excludedUserAgents.some((ua) => navigator.userAgent.includes(ua))) {
                                return ctx.cancel();
                              } else {
                                return ctx;
                              }
                          },
                          identify: (ctx) => {
                              if(excludedUserAgents.some((ua) => navigator.userAgent.includes(ua))) {
                                return ctx.cancel();
                              } else {
                                return ctx;
                              }
                          }
                      });
                      
                      // identifiers
                      window.analytics.register({
                        name: 'LegalZoom Cookie Additions',
                        version: '0.1.0',
                        type: 'enrichment',
                        isLoaded: () => true,
                        load: () => Promise.resolve(),
                        // context object and reference to the analytics.js instance
                        page: (ctx) => {
                          if(ctx.event.properties) {
                            ctx.event.properties = Object.assign({
                              ...ctx.event.properties,
                              ...defaultProperties,
                            });
                          }
                            if (ctx.event.context) {
                              ctx.event.context.identifiers = getUserContext()
                            }
                          return ctx;
                        },
                        track: (ctx) => {
                          if(ctx.event.properties) {
                            ctx.event.properties = Object.assign({
                              ...ctx.event.properties,
                              ...defaultProperties,
                            });
                          }
                          if (ctx.event.context) {
                              ctx.event.context.identifiers = getUserContext()
                              if (localStorage.getItem('ajs_user_traits')) {
                                  try {
                                    ctx.event.context.traits = JSON.parse(localStorage.getItem('ajs_user_traits'));
                                  } catch (e) {
                                  }
                              }
                          }
                          return ctx;
                        },
                        identify: (ctx) => {
                          if(ctx.event.properties) {
                            ctx.event.properties = Object.assign({
                              ...ctx.event.properties,
                              ...defaultProperties,
                            });
                          }
                          if (ctx.event.context) {
                              ctx.event.context.identifiers = getUserContext()
                          }
                          
                          return ctx;
                        },
                      });
                  
                      var keywords = document.head.querySelector("[name=keywords][content]")?.content || '';
                     
                      // Send Pageview
                      window.analytics.page("Page Viewed", {
                      keywords: [keywords],
                      is_tax: false, // TODO make it dynamic
                      topline_product_group: 'llc' // TODO make it dynamic
                      }); 
                  }
                  window.analytics.ready(() => {
                    registerAndCall();
                  });
                }();
