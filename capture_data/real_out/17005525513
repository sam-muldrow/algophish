<script type="text/javascript">
    function cmdGA() {
        var myPathName = location.pathname.trim();
    }

    function Set_divOut() {
        $("#UC035_divKeyWord").fadeOut();
    }

    function GA_Source() {
        var myPathName = location.pathname.trim();
        var mySource = "國內旅遊_大首頁搜尋";
        if (myPathName == "/C000_Portal/C000_HP06_Main.aspx")
        { mySource = "國內旅遊_國旅頁搜尋"; }
        return mySource;
    }

    // 20220519 新增全球自由行
    $(function () {
        var myJson = JSON.parse($("#C000_UC035_Package_hidDisplayMode").val());
        var DisplayMode = myJson.DisplayMode;
        var Source = myJson.Source

        if (DisplayMode == "Package") {
            $("#pkg_personal2").show();
            $("#pkg_tour").hide();
        }
        else {
            if (Source == "HP07" || Source == "HP12") {
                // 20230309 調整原因：行銷部提出 中部、南部出發館國內自由行搜尋面板調整為國外自由行面板
                $("#pkg_personal2").show();
                $("#pkg_tour").hide();
                $("#li_pkgtour").hide();

                var mySelect = $('#pkg_personal2').find('select[name="select"]');

                if (mySelect.length > 0) {
                    switch (Source) {
                        case "HP07":
                            mySelect.eq(0).val('KHH');
                            mySelect.eq(0).uniform();   // 各館首頁樣式使用uniform 所以修改值後要再執行一次讓畫面更新
                            break;
                        case "HP12":
                            mySelect.eq(0).val('TXG');
                            mySelect.eq(0).uniform();   // 各館首頁樣式使用uniform 所以修改值後要再執行一次讓畫面更新
                            break;
                    }
                }
            }
            else {
                // 預設使用此模式控制顯示
                // 因為使用的是pkgnew_search.css 會把pkg_tour隱藏，但是在UC035中需要變成pkg_tour預設顯示，所以直接改顯示
                $("#pkg_personal2").hide();
                $("#pkg_tour").show();
            }

            // 20220830 台灣民宿遊下線，只有一個搜尋面板時把頁籤文字隱藏
            if ($('div.pkg_search div.title ul.search-tab-link').find('li:visible').length < 2)
            { $('div.pkg_search div.title ul.search-tab-link').hide(); }
        }
    });
</script>
<!--自由行查詢 end-->


<script type="text/javascript">
    $(function () {
        $(document).bind("click", function (evt) {
            if ($(evt.target).parents('#UC035_txtKeyWord').length == 0 && $(evt.target).parents('#UC035_divKeyWord').length == 0) {
                $("#UC035_divKeyWord").fadeOut();
            }
        });
    });

    $(document).ready(function () {
        $('select.wide').niceSelect();
    });

    //Vue月曆function
    function addDays(date, days) {
        var myResult = date;
        myResult.setDate(myResult.getDate() + days);
        return myResult;
    }

    function SetWebtxtControlValue(ControlObj) {
        let myControlName = "";
        let myControValue = "";
        if (ControlObj != undefined) {
            if (ControlObj.txtControlName != undefined) {
                myControlName = ControlObj.txtControlName;
            }
            if (ControlObj.txtControlValue != undefined) {
                myControValue = ControlObj.txtControlValue;
            }
            if (document.getElementById(myControlName) != undefined) {
                document.getElementById(myControlName).value = myControValue
            }
        }
    }
</script>

<script type="module">
    import {C000_InitCityOption,C000_GetQueryType,C000_ListKeyWordForSearchBox} from "/COLA_AppFiles/A000_WebControls/js/Cola_Axios.js?WebAPI=true";

    var C000_UC035 = new Vue({
        el: '#C000_UC035',
        data: {
            myTravelZoneData:[],
            myDepartureCityData:[],
            myKeyWord:"",
            myTourDay:0,
            myDepartureDate:"",
            myDepartureCity:"不限",
            myKeyWordChangeMark:false,
            myKeyWordList:[],
            myShowKeyWordListMark:true
        },
        methods: {
            updatestartdate: function (obj) {
                this.myDepartureDate = obj.value;
            },
            cmdQuery: function(){
                if(this.myKeyWord!=""){
                    if(this.Verify_KeyWord()){return;}

                    C000_GetQueryType({Key_Word: this.myKeyWord})
                    .then((response) => {
                        var myRes = JSON.parse(response.data.d.c);

                        if(myRes.Query_Type=="KeyWord"){
                            location.href = "/C10G_DomTour/C10G01_TourQuery.aspx?webtype=keyword&keyword=" + C000_UC035.myKeyWord + "&departuredate=" + C000_UC035.myDepartureDate;
                        }
                        else{
                            var myUrl = "/C10G_DomTour/C10G01_TourQuery.aspx?webtype=tourquery&travelcity=" + ((myRes.City_Name=="")?C000_UC035.myKeyWord:myRes.City_Name);

                            if(C000_UC035.myDepartureCity!="不限"){
                                myUrl += "&departurecity=" + C000_UC035.myDepartureCity;
                            }
                            if(C000_UC035.myDepartureDate!=""){
                                myUrl += "&departuredate=" + C000_UC035.myDepartureDate;
                            }
                            if(C000_UC035.myTourDay!=0){
                                myUrl += "&tourday=" + C000_UC035.myTourDay;
                            }
                            
                            location.href = myUrl;
                            //window.open(myUrl);
                        }
                    })
                }
                else{
                    alert("請輸入旅遊城市 / 景點")
                }
            },
            Set_KeyWord: function(Type,KeyWord,Url){
                $("#UC035_divKeyWord").fadeOut();
                if(Type=="行程"){
                    window.open(Url);
                }
                else{
                    this.myShowKeyWordListMark = false;
                    this.myKeyWord             = KeyWord.replaceAll("<span>","").replaceAll("</span>","");
                }
            },
            Set_txtKeyWord: function(){
                if(this.Verify_KeyWord()){return;}

                $("#UC035_divKeyWord").fadeIn();


                if(this.myKeyWord!=""&&this.myKeyWordChangeMark){
                    this.Get_KeyWordList();
                }

                this.myKeyWordChangeMark = false;
            },
            Get_KeyWordList: function(){
                C000_ListKeyWordForSearchBox({Travel_Type:"全部",Key_Word: this.myKeyWord})
                .then((response) => {
                    C000_UC035.myKeyWordList  = JSON.parse(response.data.d.c);
                })
            },
            Set_divOut: function(Element_Id){
                $("#" + Element_Id).fadeOut();
            },
            Verify_KeyWord: function(){
                let reg = /[^\u4E00-\u9FA5\uF900-\uFA2Da-zA-Z\d ,/]/;
                if(reg.test(this.myKeyWord)){
                    alert("僅能輸入中文、英文或數字");
                    return true;
                }

                return false;
            }
        },
        components: {
            'cola-calendar': httpVueLoader('/COLA_AppFiles/A000_WebControls/vue/ColaCalendar.vue')
        },
        created: function () {
            var myDate               = new Date();
            myDate.setDate(myDate.getDate() + 1);

            this.myDepartureDate     = myDate.getFullYear() + "/" + ((myDate.getMonth() + 1) < 10 ? ("0" + (myDate.getMonth() + 1)) : (myDate.getMonth() + 1)) + "/" + (myDate.getDate() < 10 ? ("0" + myDate.getDate()) : myDate.getDate());

            C000_InitCityOption({Used_Type: "tourquery"})
            .then((response) => {
                C000_UC035.myTravelZoneData    = JSON.parse(response.data.d.c).Travel_Zone;
                var myDepartureList = JSON.parse(response.data.d.c).Departure_City;

                for(var i=0;i<myDepartureList.length;i++){
                    for(var j=0;j<myDepartureList[i].City_List.length;j++){
                        C000_UC035.myDepartureCityData.push(myDepartureList[i].City_List[j].Option_Text);
                    }
                }
                
                var myscript  = document.createElement("script");
                myscript.type = "text/javascript";
                myscript.src  = "/C000_Portal/JS/jquery.searchbox.js";
                document.body.appendChild(myscript);
            })
        },
        watch:{
            myKeyWord: function () {
                if(this.Verify_KeyWord()){return;}

                if(this.myShowKeyWordListMark){
                    this.Get_KeyWordList();
                }
                
                this.myKeyWordChangeMark   = true;
                this.myShowKeyWordListMark = true;
            }
        }
    })
</script>
                                </div>
                                <div id="tab3" class="tab_content">
                                    

