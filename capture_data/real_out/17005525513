<!-- HTML -->
<script type="text/javascript">
    var userAgent = navigator.userAgent;
    //console.log(userAgent);
    if ((/iPhone|ipad/i.test(userAgent))) {  //對ISO系統做判斷
        //MP 
        $("#txtOriginCityMP").bind('input propertychange', function (event) {
            MPAutoComplete(this.id, 'C000_UC026A_AirTicket_txtOriginCodeMP');
        });
        $("#txtDiffDestCityMP").bind('input propertychange', function (event) {
            MPAutoComplete(this.id, 'C000_UC026A_AirTicket_txtDiffDestinationMP');
        });
        $("#txtDestCityMP").bind('input propertychange', function (event) {
            MPAutoComplete(this.id, "C000_UC026A_AirTicket_txtDestinationMP", "C000_UC026A_AirTicket_txtRouteSummaryMP");
        });
        //TKT
        $("#txtDestCityTkt2").bind('input propertychange', function (event) {
            TKTAutoComplete(this.id, 'C000_UC026A_AirTicket_txtDestinationTkt2');
        });
        $("#txtDestCityTkt").bind('input propertychange', function (event) {
            TKTAutoComplete(this.id, 'C000_UC026A_AirTicket_txtDestinationTkt');
        });
        //LCC
        $("#txtOriginCityLCC").bind('input propertychange', function (event) {
            LCCAutoComplete(this.id, 'C000_UC026A_AirTicket_txtOriginCodeLCC');
        });
        $("#txtDestCityLCC").bind('input propertychange', function (event) {
            LCCAutoComplete(this.id, 'C000_UC026A_AirTicket_txtDestinationLCC');
        });
    };
    $(function () {
        $("#MP_HotCityHtml").load("/C000_Portal/HTML/HotCity_MPAirTicket.html");                   
        $("#MP_DiffHotCityDestHtml").load("/C000_Portal/HTML/HotCity_MPAirTicket_Dest.html");      
        $("#MP_HotCityDestHtml").load("/C000_Portal/HTML/HotCity_MPAirTicket_Dest.html");          
        $('#Tkt_DestHostCityHtml').load("/C000_Portal/HTML/HotCity_Ticket_Dest02.html");           
        $('#Tkt_DiffDestHostCityHtml').load("/C000_Portal/HTML/HotCity_Ticket_Dest02.html");       
        //$('#LCC_HotCityHtml').load("/C000_Portal/HTML/HotCity_LCC.html");                      
        //$('#LCC_HotCityDestHtml').load('/C000_Portal/HTML/HotCityDest_LCC.html');              
        $('#LCC_HotCityHtml').load("/EC_NoBackUp/A06C_LCC/HTML/RWD/HotCity_LCC.html");                         
        $('#LCC_HotCityDestHtml').load('/EC_NoBackUp/A06C_LCC/HTML/RWD/HotCityDest_LCC.html');     
    });

    //國內機票
    $(document).ready(function() {
        $('select.wide').niceSelect();
    })
</script>

