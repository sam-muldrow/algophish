<script type="text/javascript">
    $(function () {
        $('#cmdTourQuery2').click(function () {

            var myUrl = '/C10A_TourSell/C10A02_TourQuery.aspx?';
            var myDepartureCity, myRegionCode,
                myTourType, myTourBudget, myStartTourDate,
                myEndTourDate, myBookingTour

            myDepartureCity = $('#C000_UC24_ddlDepartureCity').val();
            myRegionCode = $('#C000_UC24_ddlRegionCode').val();
            myTourType = $('#C000_UC24_ddlTourType').val();
            myTourBudget = '預算不限';
            myStartTourDate = $('#C000_UC24_txtStartTourDate').val();

            if ($('#C000_UC24_chkBookingTour').prop('checked') == true) {
                myBookingTour = "";
            }
            else {
                myBookingTour = "全部";
            }

            QueryString = "DepartureCity=" + encodeURI(myDepartureCity);
            QueryString += "&RegionCode=" + encodeURI(myRegionCode);
            QueryString += "&TourType=" + encodeURI(myTourType);
            QueryString += "&TourBudget=" + encodeURI(myTourBudget);
            QueryString += "&StartTourDate=" + myStartTourDate;
            QueryString += "&BookingTour=" + encodeURI(myBookingTour);
            QueryString += "&bound=in";

            top.location.href = myUrl + QueryString;
            return false;
        });
    })
</script>
                                </div>
                                <div id="tab7" class="tab_content">
                                    

<style type="text/css">
    [v-cloak]{
        display:none;
    }
</style>

<div id="indexStyle" class="pkg_search dompkg_wrap">
    <div class="title">
        <p>高鐵旅遊搜尋</p>
    </div>
    <div id="C000_UC16" class="domtour-searchbox" v-cloak>
    <div class="tab-wrap">
    <div class="searchBox">
        <ul class="item-row">
            <li class="row01">
                <div class="leftBox">
                    <label class="information-title" for=""><i class="star"></i>旅遊地區</label>
                    <div id="UC16_txtKeyWord"><input id="" class="popup-open" type="text" v-model="myKeyWord" placeholder="輸入旅遊城市 / 景點" @click="Set_txtKeyWord()"></div>
                    <div id="UC16_divKeyWord" class="domtour-location-popup" style="display:none">
						<div class="region-wrap" v-show="myKeyWord==''">
							<div class="region-tabBox block-style">
                                <ul class="region-nav">
                                    <li v-for="(wkTitle,Index) in myTravelZoneData"><a :href="'#area2-'+Index">{{wkTitle.Zone_Name}}</a></li>
                                </ul>
								<div class="contentWrap">
                                    <div :id="'area2-'+Index" class="tabBox-content" v-for="(wkTravel,Index) in myTravelZoneData">
                                        <ul class="area-row">
                                            <li v-for="wkZoneList in wkTravel.Zone_List">
                                                <p class="subtitle" v-if="wkZoneList.Option_Type!=''">{{wkZoneList.Option_Type}}</p>
