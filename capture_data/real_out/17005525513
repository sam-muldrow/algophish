<script type="text/javascript">
    $(function () {
        $('#cmdTourQuery').click(function () {

            var myUrl = '/C10A_TourSell/C10A02_TourQuery.aspx?';
            var myDepartureCity, myRegionCode,
                myTourType, myTourBudget, myStartTourDate,
                myEndTourDate, myBookingTour

            myDepartureCity = $('#C000_UC23_ddlDepartureCity').val();
            myRegionCode = $('#C000_UC23_ddlRegionCode').val();
            myTourType = $('#C000_UC23_ddlTourType').val();
            myTourBudget = '預算不限';
            myStartTourDate = $('#C000_UC23_txtStartTourDate').val();

            if ($('#C000_UC23_chkBookingTour').prop('checked') == true) {
                myBookingTour = "";
            }
            else {
                myBookingTour = "全部";
            }

            QueryString = "DepartureCity=" + encodeURI(myDepartureCity);
            QueryString += "&RegionCode=" + encodeURI(myRegionCode);
            QueryString += "&TourType=" + encodeURI(myTourType);
            QueryString += "&TourBudget=" + encodeURI(myTourBudget);
            QueryString += "&StartTourDate=" + myStartTourDate;
            QueryString += "&BookingTour=" + encodeURI(myBookingTour);
            QueryString += "&bound=out";

            top.location.href = myUrl + QueryString;
            return false;
        });
        // 解決回上一頁 span內容錯誤
        if (document.getElementById('C000_UC23_ddlRegionCode')) { document.getElementById('C000_UC23_ddlRegionCode').click(); }
        if (document.getElementById('C000_UC23_ddlDepartureCity')) { document.getElementById('C000_UC23_ddlDepartureCity').click(); }
        if (document.getElementById('C000_UC23_ddlTourType')) { document.getElementById('C000_UC23_ddlTourType').click(); }
    })
</script>
                                </div>
                                <div id="tab2" class="tab_content">
                                    

