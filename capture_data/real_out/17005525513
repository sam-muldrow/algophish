        $(function () {
            function mobilecss() { /*手機專用CSS*/
                $("link[href*='desktop_index.min.css?v=1013']").remove();
                $("link[href*='/C000_Portal/Css/Search/Coupon/coupon_search_uc.css']").remove();
                $('head').append('<link href="/C000_Portal/Css/Search/Coupon/coupon_phone_search_uc.css" rel="stylesheet" type="text/css">	');

            }

            $('#slider-nav-bigbn').removeClass('stab');

            //判斷裝置_執行上述動作
            var userAgent = navigator.userAgent;
            if (/Android/i.test(userAgent)) {
                mobilecss();
            } else if (/iPhone/i.test(userAgent)) {
                mobilecss();
            } else if (/webOS|iPod|BlackBerry|IEMobile/i.test(userAgent)) {
                mobilecss();
            }
        });
    </script>
