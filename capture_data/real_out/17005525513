<script type="text/javascript">
    var myChangeTabCode;  //存取 html id 
    var TripType;         //行程類別 
    var myKeyCode;        //紀錄鍵盤點擊Code

    // 紀錄使用者鍵盤點擊  
    $(document).keydown(function (event) {
        myKeyCode = event.which;
        //console.log(event.which);
    });

    $(document).ready(function () {
        //Timeout 觸發 C10C_10_ChooseFareType 查詢    
        setTimeout(function () {
            if (typeof angular != 'undefined') {
                console.log(angular.element('#cmdRefresh'));
                angular.element('#btnInit').trigger('click');
            }
        }, 200);

        // 點擊非選擇區域  hide
        $('body').click('click', function (e) {
            /* MP */
            $('#txtOriginCityMP').each(function () {
                if (!$(this).is(e.target) && $(this).has(e.target).length === 0 && $('#txtOriginCityMP').has(e.target).length === 0) {
                    $('#MP_HotCityHtml.location-popup').hide();
                }
            });
            $('#txtDestCityMP').each(function () {
                if (!$(this).is(e.target) && $(this).has(e.target).length === 0 && $('#txtDestCityMP').has(e.target).length === 0) {
                    $('#MP_HotCityDestHtml.location-popup').hide();
                }
            });
            $('#txtDiffDestCityMP').each(function () {
                if (!$(this).is(e.target) && $(this).has(e.target).length === 0 && $('#txtDiffDestCityMP').has(e.target).length === 0) {
                    $('#MP_DiffHotCityDestHtml.location-popup.styleChange').hide();  //A進B出 修正className
                }
            });
            /* Ticket */
            $('#txtDestCityTkt').each(function () {
                if (!$(this).is(e.target) && $(this).has(e.target).length === 0 && $('#txtDestCityTkt').has(e.target).length === 0) {
                    $('#Tkt_DestHostCityHtml.location-popup').hide();
                }
            });
            $('#txtDestCityTkt2').each(function () {
                if (!$(this).is(e.target) && $(this).has(e.target).length === 0 && $('#txtDestCityTkt2').has(e.target).length === 0) {
                    $('#Tkt_DiffDestHostCityHtml.location-popup.styleChange').hide();  //A進B出 修正className
                }
            });
            /* LCC */
            $('#txtOriginCityLCC').each(function () {
                if (!$(this).is(e.target) && $(this).has(e.target).length === 0 && $('#txtOriginCityLCC').has(e.target).length === 0) {
                    $('#LCC_HotCityHtml.location-popup').hide();
                }
            });
            $('#txtDestCityLCC').each(function () {
                if (!$(this).is(e.target) && $(this).has(e.target).length === 0 && $('#txtDestCityLCC').has(e.target).length === 0) {
                    $('#LCC_HotCityDestHtml.location-popup').hide();
                }
            });
            $('.ui-autocomplete.ui-front.ui-menu.ui-widget.ui-widget-content.ui-corner-all').hide();
        });
    });
    /*** 搜尋tab切換 ***/
    function Tab(ChangeTab_Id) {
        myChangeTabCode = ChangeTab_Id;  //各 HTML id   

        $('' + ChangeTab_Id + ' .destination-tabBox').each(function () {  // tab 切換功能 由原本searchbox_animation.js 複製過來 
            var $tab = $(this);
            var _showTab = 0;
            $('ul.destination-nav li').removeClass('active');  //移除所有
            $('#AirTicket_Area-01').show();      //MP 出發地
            $('#AirTicket_DestArea-01').show();  //MP 目的地
            $('#TicketDest_home').show();        //Ticket 目的地(回程起點)
            $('#TicketDiffDest_home').show();    //Ticket 雙程(目的地)
            $('#LCCArea_home').show();           //LCC 出發地
            $('#LCCAreaDest_home').show();       //LCC目的地

            var $defaultLi = $('ul.destination-nav li', $tab).eq(_showTab).addClass('active');
            $($defaultLi.find('a').attr('href')).siblings().hide();
            $('ul.destination-nav li', $tab).click(function () {
                var $this = $(this),
                    _clickTab = $this.find('a').attr('href');
                //console.log('Tab');
                $this.addClass('active').siblings('.active').removeClass('active');
                $(_clickTab).stop(false, true).fadeIn().siblings().hide();

                return false;
            }).find('a').focus(function () {
                this.blur();
            });
        });
    };
    //判斷使用者是否使用鍵盤"Enter"鍵  造成cmdQuery按鈕submit 而無驗證動作 故在 OnClientClick 阻擋錯誤
    function CheckKeyCode() {
        //console.log("test" + myKeyCode);
        if (myKeyCode == 13) {
            return false;
        }
    };
</script>
