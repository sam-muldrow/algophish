      } else {
        t.async = false;
        t.defer = true;
        t.crossOrigin = 'anonymous';
        if (c.props){
          for (var key in c.props){
            t[key] = c.props[key];
          }
        }
        if (c.id) t.id = c.id;

        if (c.s) t.src = c.s;
        var x = t.src;
        cbStack[x] = [];
