<script type="text/javascript">
    $(document).ready(function () {
        if ($().iCheck)
            $("input").iCheck({
                checkboxClass: "icheckbox_flat-blue",
                radioClass: "iradio_flat-blue"
            });
    });

    $('input').on('ifChecked', function (event) {
        $("#divChartEuro").hide();
        $("#divChartDollar").hide();
        $("#divChartAed").hide();
        $("#divChart" + event.target.value).show();
    });

    $("#divChartDollar").hide();
    $("#divChartAed").hide();
