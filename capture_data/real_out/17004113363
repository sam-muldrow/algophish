    function DoSearch(e) {
        if (!e)
            e = window.event;

        if (e.type === "keypress" && e.keyCode === 13 || e.type === "click") {
            window.location.assign("https://bmi.ir/fa/search/news/page-1/" + document.getElementById('txtSearch').value);
            return false;
        }

        return true;
    }
