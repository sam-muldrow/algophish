<script>
(function (boxId) {
    jQuery('[data-box-id=' + boxId + '] [data-is-tab]').on('click', function(event) {

        if (jQuery(this).data('is-selected') == true) {
            return true;
        }

        var country = jQuery(this).data('country');
        var container = jQuery('[data-box-id=' + boxId + ']');

        container.find('[data-is-tab]').removeClass('selected').data('is-selected', false);
        container.find('[data-is-boxoffice-date]').hide();
        container.find('[data-is-tab-content]').hide();

        container.find('[data-is-tab][data-country="' + country + '"]').addClass('selected').data('is-selected', true);
        container.find('[data-is-boxoffice-date][data-country="' + country + '"]').show();
        container.find('[data-is-tab-content][data-country="' + country + '"]').show();

        return false;
    });
})("1331");
</script>        </div>
                
