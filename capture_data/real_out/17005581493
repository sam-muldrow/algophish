<link rel="preload" as="script" href="https://live.demand.supply/up.js"><script async data-cfasync="false" type="text/javascript" src="https://live.demand.supply/up.js"></script><script>

if (typeof window.__tcfapi === 'function') {
    window.__tcfapi('addEventListener', 2, function(tcData, success) {
        var persAds = 0;
        if (success) {
            console.log('CMP: ' + tcData.eventStatus);
            //console.log(tcData);
            if (tcData.gdprApplies) {
                if (tcData.eventStatus == 'useractioncomplete' || tcData.eventStatus == 'tcloaded') {
                    if (Object.keys(tcData.purpose.consents).length == 0) {
                        console.log('CMP: No consents');
                    } else {
                        persAds = 1;
                    }
                }
            } else if (tcData.gdprApplies === false) {
                console.log('CMP: GDPR does not apply');
                persAds = 1;
            }
        }
        console.log(! persAds ?
            "Loading non-personalized ads" :
            "Loading personalized ads"
        );
        googletag.cmd.push(function () {
            googletag.pubads().setRequestNonPersonalizedAds(! persAds);
            googletag.pubads().refresh();
        });
    });
} else {
    googletag.cmd.push(function () {
        console.log('No CMP: Loading non-personalized ads');
        googletag.pubads().setRequestNonPersonalizedAds(1);
        googletag.pubads().refresh();
    });
}

</script>

