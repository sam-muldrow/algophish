<script>
// Trigger an event on window.almComplete to allow extensibility
// Assuming the event is var e, access alm object with e.detail.alm
// window.addEventListener('almComplete', (e) => {
//     console.log(`ALM COMPLETE: ${e.detail.alm}`);
// });
(function(){
    var old_cb = window.almComplete;
    window.almComplete = function(alm) {
        try{ old_cb && old_cb(alm); }catch(e){ console.error('almComplete', e); }
        const almCallbackEvent = new CustomEvent('almComplete', {
            detail: {
                alm: alm
            }
        });
        window.dispatchEvent(almCallbackEvent);
    }
})();
</script>
