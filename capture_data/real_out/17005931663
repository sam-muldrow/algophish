<script type="text/javascript">
$(function() {
    $(".acme_outer").hide();
    $(".acme_fader").hide();

    function acme_x() {
        $(".acme_outer").fadeOut(300);
        $(".acme_fader").fadeOut(300);
    }

    function acme_on() {
        $(".acme_outer").fadeIn(200);
        $(".acme_fader").fadeIn(400);
    }
    $(".acme_regform, .open_regform").click(function(e) {
        e.preventDefault();
        $("#acme_regform").show();
        $("#acme_login").hide();
        acme_on();
        return false;
    });
    $(".acme_loginform, .open_loginform").click(function(e) {
        e.preventDefault();
        $("#acme_login").show();
        $("#acme_regform").hide();
        acme_on();
        return false;
    });
    $(".acme_open").click(function(e) {
        e.preventDefault();
        $(".acme_outer").fadeIn(300);
        $(".acme_fader").fadeIn(300);
    });
    $('.acme_close').click(function(e) {
        acme_x()
    });
    $(document).keyup(function(e) {
        if (e.keyCode == 27) {
            acme_x()
        }
    });

    function stopPropagation(e) {
        e.stopPropagation();
    }

    document.getElementById("acme_outer").addEventListener("click", acme_x, false);
    document.getElementById("acme_fader").addEventListener("click", acme_x, false);
    document.getElementById("acme_popup").addEventListener("click", stopPropagation, false);
});
</script>


