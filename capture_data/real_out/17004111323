                            <script type="text/javascript">
	function has_cookie(name){
  		return document.cookie.split(';').some( c => {
    		return c.trim().lastIndexOf(name, 0) === 0;
  		})
  	;}
	if (has_cookie("fontsize")) {
  		document.cookie="fontsize=; path=/; expires=Thu, 01 Jan 1970 00:00:01 GMT;";
	}
