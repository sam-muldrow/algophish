<script type="text/javascript">
    (function () { 
        
        var _getCookieValue = function (key) {
            var cookie = document.cookie;

            if (cookie && cookie !== '') {
                var items = cookie.split(";");
                for (var i = 0; i < items.length; i++) {
                    var value = items[i].trim();
                    if (value.substring(0, key.length + 1) === (key + '=')) {
                        return decodeURIComponent(value.substring(key.length + 1));
                    }
                }
            }
        };

        
        var _b64DecodeUtf8 = function (str) {
            return decodeURIComponent(atob(str).split('').map(function(c) {
                return '%' + ('00' + c.charCodeAt(0).toString(16)).slice(-2);
            }).join(''));
        }

        
        var _getAccessInfo = function () {
            var fptCookieValue = _getCookieValue("fpt");

            if (!fptCookieValue) {
                return;
            }

            var fptString = fptCookieValue ? _b64DecodeUtf8(fptCookieValue) : undefined;

          if (fptString && fptString.length) {
                          var accessInfo = JSON.parse(fptString)

                if (accessInfo && accessInfo.type) {
                    return accessInfo;
                }
            }
        }

        window.dataLayer = window.dataLayer || [];

                var layer = {"breadcrumb":{"path":[]},"page":{"contentParent":"FAZ.NET","contentType":"Homepage","contentId":"2.1648","contentName":"Aktuell","contentTitle":"Aktuell","contentSEOName":"Aktuelle Nachrichten online - FAZ.NET"},"event":"dataLayerReady"};

        accessInfo = _getAccessInfo();
        if (accessInfo) {
                        layer.accessInfo = accessInfo;
            layer.customerInfo = {
                    usedProduct: {
                    sku: accessInfo.sku,
                    name: accessInfo.message
                }
            };
        }

        dataLayer.push(layer);
    })();
