<script>
$(document).ready(function()
      {
        $('.accordion-toggle').click(function(){
          var dataId = jQuery(this).attr('href');
          if(jQuery(dataId).hasClass('in'))
          {
            setTimeout(function(){$(dataId).removeClass('in')},1);
            setTimeout(function(){$(dataId).addClass('collapse')},1);
          }
          if($(dataId).hasClass('collapse'))
          {
            
            setTimeout(function(){$(dataId).removeClass('collapse')},1);
            setTimeout(function(){$(dataId).addClass('in')},1);
          }
        }) 
      })
</script>
