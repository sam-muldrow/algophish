  function redirectToOrderOnline () {
    event.preventDefault();
    if(window.navigator.userAgent.indexOf('Mobile') !== -1) {
      window.location.href = "https://m.dominos.co.in/changeAddress?src=brand";
    } else {
      window.location.href = "https://pizzaonline.dominos.co.in/?src=brand";
    }
  }

  function gotoHygienePage() {
    event.preventDefault();
    event.stopPropagation();
    window.dataLayer.push({
      screen:'Brand_HP',
      event: "Bannerclick",
      eventCategory: 'Banner click',
      eventAction: 'Vaccination BP',
      eventLabel: 'Vaccination banner click'
    });
    if(window.navigator.userAgent.indexOf('Mobile') !== -1) {
      window.location.href = "https://m.dominos.co.in/changeAddress?src=brand";
    } else {
      window.location.href = "https://pizzaonline.dominos.co.in/?src=brand";
    }
  }

  var container = document.getElementById('container');
  var landscapeContainer = document.getElementById('landscape-mode');
  var landscapeMessage = document.getElementById('landscape-mess');
  
  if (window.matchMedia("(orientation: landscape)").matches && window.matchMedia("(max-width: 1024px)").matches) {
    container.classList.add('hide');
    landscapeContainer.classList.add('landscape-container');
    landscapeMessage.classList.remove('hide');
  }
  window.addEventListener("orientationchange", function() {
    if(window.orientation !== 0) { 
      container.classList.add('hide');
      landscapeContainer.classList.add('landscape-container');
      landscapeMessage.classList.remove('hide');
    } else {
      container.classList.remove('hide');
      landscapeContainer.classList.remove('landscape-container');
      landscapeMessage.classList.add('hide');
    }
  }, false);

  function openDrawer () {
    document.getElementById("sideNavBar").style.width = "75vw";
    document.getElementById("sideNavBar").style.visibility = "visible";
    document.getElementById("sideNavOverlay").classList.add('side-nav-overlay');
    document.getElementById('landscape-mode').classList.add('no-scroll');
  }

  function closeDrawer () {
    event.stopPropagation();
    document.getElementById("sideNavBar").style.width = "0";
    document.getElementById("sideNavBar").style.visibility = "hidden";
    document.getElementById("sideNavOverlay").classList.remove('side-nav-overlay');
    document.getElementById('landscape-mode').classList.remove('no-scroll');
  }

  var header = document.getElementById("domino-header");
  var banner = document.getElementById('online-banner');
  var bannerImage = document.getElementById('bannerImage');
  var higeneImage = document.getElementById('higeneImage');
  var bannerImageSticky = document.getElementById('bannerImageSticky');
  var delText = document.getElementById('delivery-text');
  var bodyContainer = document.getElementById('bodyClass');
  var topHeader = document.getElementById('top-header');
  
  window.onscroll = function() {stickyHeader()};

  function stickyHeader() {
    if (window.pageYOffset > 150) {
      bannerImageSticky.classList.remove('hide');

      bannerImage.classList.add('hide');
      higeneImage.classList.add('hide');
      bannerImageSticky.classList.add('banner-sticky');
      topHeader.classList.add('hide');
      delText.classList.add('del-text');
      bodyContainer.classList.add('body');

      header.classList.add("sticky");
      banner.classList.add('sticky-header');

    } else {
      header.classList.remove("sticky");
      banner.classList.remove('sticky-header');

      bannerImage.classList.remove('hide');
      higeneImage.classList.remove('hide');
      bannerImageSticky.classList.add('hide');
      bannerImageSticky.classList.remove('banner-sticky');

      delText.classList.remove('del-text');
      bodyContainer.classList.remove('body');
      topHeader.classList.remove('hide');
    }
  }

