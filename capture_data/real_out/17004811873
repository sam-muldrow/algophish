var html =  '';
for(var i = 0; i < 10; i++) {
   html +='<span class="link-item shuffle label label-default"><a href="'+shuffled[i][1]+'" target="_blank">'+shuffled[i][0]+'</a></span>';
}
$('#container-store').append(html);
    })
    
    function fisherYates(myArray) {
var i = myArray.length, j, tempi, tempj;
if (i === 0) return false;
while (--i) {
    j = Math.floor(Math.random() * (i + 1));
    tempi = myArray[i];
    tempj = myArray[j];
    myArray[i] = tempj;
    myArray[j] = tempi;
}
return myArray;
}
  </script>
