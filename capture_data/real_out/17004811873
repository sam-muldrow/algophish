  closeButton.addEventListener('click', toggleModal);
  
  var ua = window.navigator.userAgent;
  if(ua.indexOf('Trident/') > 0) {
    var xhttp = new XMLHttpRequest();
    xhttp.onreadystatechange = function() {
      if (this.readyState == 4 && this.status == 200) {
        createOffersSlider(JSON.parse(xhttp.response));
      }
    };
    xhttp.open("GET", "https://api.dominos.co.in/prod-olo-api/v1/widgetcontent/brandOffers", true);
    xhttp.send();
  } else {
    fetch('https://api.dominos.co.in/prod-olo-api/v1/widgetcontent/brandOffers')
    .then(function(response) {
      return response.json();
    })
    .then(function(resJson) {
        createOffersSlider(resJson);
    });
  }
  
  function createOffersSlider(resJson) {
      var offersContainer = document.getElementById('offers');
      for(let idx=0;idx<resJson.data.length;idx++) {
        let slide = document.createElement('img');
        slide.setAttribute('class', "offers-slide");
        slide.setAttribute('src', resJson.data[idx].imageUrl);
        slide.setAttribute('alt', resJson.data[idx].alt);
        offersContainer.appendChild(slide);
      }
      
      /* insert dummy slider since visible slider is 1.2 */
      let slide = document.createElement('img');
      slide.setAttribute('class', "offers-slide extra-slider");
      slide.setAttribute('src', resJson.data[resJson.data.length -1].imageUrl);
      offersContainer.appendChild(slide);

      var slider = tns({
        container: '.offers-slider',
        loop: false,
        touch: true,
        mouseDrag: true,
        nav: false,
        controls: false,
        responsive: {
          320: {
            mouseDrag: true,
            items: 1.2,
            gutter: 10,
          },
          640: {
            mouseDrag: true,
            items: 2.2,
            gutter: 10,
          },
          900: {
            mouseDrag: false,
            items: 2.2,
            gutter: 20,
            container: '.offers-slider',
            controlsContainer: '#customize-controls',
          }
        }
        });

      document.querySelector('.prev').onclick = function () {
        event.stopPropagation();
        document.querySelector('.next').style.visibility = "visible";
        var isFirstItemVisible = slider.getInfo().slideItems[1].classList.contains('tns-slide-active');
        if(isFirstItemVisible) {
          document.querySelector('.prev').style.visibility = "hidden";
        }
        slider.goTo('prev');
      };

      document.querySelector('.next').onclick = function () {
        event.stopPropagation();
        var isLastItemVisible =  slider.getInfo().slideItems[slider.getInfo().slideItems.length - 2].classList.contains('tns-slide-active');
        if(isLastItemVisible) {
          document.querySelector('.next').style.visibility = "hidden";
        } 
        document.querySelector('.prev').style.visibility = "visible";
        slider.goTo('next');
      };
      }

      function corpMenuTopClicked(){
        window.dataLayer.push({
          event: "corporateEnquiry",
          eventCategory: 'Corporate Enquiry',
          eventAction: 'Top Available Options'
        });
        window.dataLayer.push({
          event: "corporateEnquiry",
          eventCategory: 'Corporate Enquiry',
          eventAction: 'Virtual Pizza Party'
