$(function () {
  var ksaddr = document.getElementById( 'ksaddr' );
  ksaddr.onfocus = function(){
    if( this.value == this.defaultValue ){
      this.value = '';
      this.style.color = '';
    }
  }

  ksaddr.onblur = function(){
    var defaultStr = "å¸åºçºæã»çºåãå¥å";
    if( this.value == '' ){
      this.value = this.defaultValue;
      if(this.defaultValue == defaultStr){
        this.style.color = 'gray';
      } else {
        this.defaultValue = "å¸åºçºæã»çºåãå¥å";
        this.style.color = '';
      }
    } else if((this.value != '') && (this.value != this.defaultValue) ) {
      this.style.color = 'black';
    }
  }

  ksaddr.defaultValue = ksaddr.value;
  ksaddr.value = '';
  ksaddr.onblur();
	
	
