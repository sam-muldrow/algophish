$(function () { 
                 $('#sampleCarouselCycle').click(function () { 
                                  $('#sampleCarousel').carousel('cycle'); 
                 }); 
                 $('#sampleCarouselPause').click(function () { 
                                  $('#sampleCarousel').carousel('pause'); 
