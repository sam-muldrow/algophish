    var reqUrl = '';
    
    if (location.host.indexOf('test') > -1) {
        reqUrl = '//buyer.lazada.test/user/api/getContextInfo';
    } else {
        reqUrl = "//member." + domainMap[g_config.regionID].domain + '/user/api/getContextInfo';
