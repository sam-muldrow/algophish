<script>
        let vSlideIndex = 1;
        vShowSlides(vSlideIndex);
        function vNextSlide() {
           vShowSlides(vSlideIndex += 1);
        }
        function vPreviousSlide() {
           vShowSlides(vSlideIndex -= 1);  
        }
        function vCurrentSlide(n) {
           vShowSlides(vSlideIndex = n);
        }
        function vShowSlides(n) {
           let slides = document.getElementsByClassName("v-slider-inner");
            if (n > slides.length) {
               vSlideIndex = 1
            }
            if (n < 1) {
               vSlideIndex = slides.length
            }
            for (let slide of slides) {
               slide.style.display = "none";
            }
            slides[vSlideIndex - 1].style.display = "block";    
        }
    </script>
			 
			
