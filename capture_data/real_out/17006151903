<script>
const vmbInput = document.querySelector('.v-header-burger-input')

vmbInput.addEventListener('change', function (event) {
  const vmbId = 'v-mob-menu';
  const vmbDisplay = document.getElementById(vmbId).style.display;
        if (vmbDisplay=='block') {
            document.getElementById(vmbId).style.display='none';
            document.getElementsByTagName('html')[0].style.overflow = 'auto';
            document.getElementsByTagName('body')[0].style.overflow = 'auto';
        } else {
            document.getElementById(vmbId).style.display='block';
            document.getElementsByTagName('html')[0].style.overflow = 'hidden';
            document.getElementsByTagName('body')[0].style.overflow = 'hidden';
    }
})

function vCloseDiv(vId) {
    document.getElementById(vId).style.display='none';
    setTimeout(function(){document.getElementById(vId).style.display=''},50);
    return true;
}

function vCloseMobileMenu() {
    document.getElementById('v-header-burger-check').checked = false;
    document.getElementById('v-mob-menu').style.display='none';
    document.getElementsByTagName('html')[0].style.overflow = 'auto';
    document.getElementsByTagName('body')[0].style.overflow = 'auto';
    return true;
}
</script>
			 
			
