<script>
    function skipToLogin() {
        let loginFocus =  $('.right .login a');

        if (loginFocus.length > 1) {
            loginFocus = $('.right .login button');

            if (loginFocus.length === 0) {
                loginFocus = $('.right .login');
            }
        }

        $(loginFocus).first().focus();
    }
</script>
