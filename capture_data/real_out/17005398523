<script>
	$(".techbeat-nav").click(function () {
			var objThis = $(this);
			var nav_tab = encodeURIComponent($(this).data('nav_tab_val'));

			if (typeof nav_tab == 'undefined') {
				nav_tab = '';
			}

			var url = base_url + '/ajax_files/landingpage_techbeat_block.php';
			$.post(url, {
				'nav_tab': nav_tab
			}, function (data) {
				data = $.trim(data);
				if (data) {

					// make Tab Active
					$('.techbeat-nav').removeClass("active");

					$('#'+nav_tab).addClass("active");

					$('#tab-inner-block').html(data);
					if(typeof (lazyLoadInstance) == "object") {
						lazyLoadInstance.update();
					}
				}
			});

		});

		$(document).ready(function () {
			// Default click on page load
			$(".techbeat-nav").eq(0).trigger("click");
		});
</script><!-- Developer Zone -->
