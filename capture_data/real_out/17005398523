<script>
	$(document).on('click', '#newsletter_subscrption', function() {
		var newsletter_chked_response = $('#newsletter_chkbox_agree').is(':checked');
		if (!newsletter_chked_response) {
			$("#newsletter-consent").modal('show');
		} else {
			$("#Subscribe-Email").trigger('click');
		}
	});
</script>

