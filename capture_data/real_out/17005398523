<script>
$(function(){
	function googleSignon(){
		(function(d, s, id){
			var js, gjs = d.getElementsByTagName(s)[0];
			if (d.getElementById(id)) {return;}
			js = d.createElement(s); js.id = id;
			js.defer = true;
			js.src = "https://accounts.google.com/gsi/client";
			gjs.parentNode.insertBefore(js, gjs);
		}(document, 'script', 'google-loginClient'));
	}

	$(window).on("load",function(){
		setTimeout(function(){
			googleSignon();
		},1000);
	})
})
</script>

  
<script>
  var ajax_list = 0;


  // Here we run a very simple test of the Graph API after login is
  // successful.  See statusChangeCallback() for when this call is made.
 function fb_login(return_url){
	FB.login(function(response) {
		 if (response.status === 'connected') {
			// Logged into your app and Facebook.
			var accessToken = response.authResponse.accessToken;
				if (accessToken) {
					window.location.href = base_url+'/user_login_validate.php?accessToken='+accessToken+'&return_url='+return_url;
				} else {
					window.location.href = base_url+'?msg_id=2600003&msg_type=3';
				}
		}
	}, {scope: 'public_profile,email'}); 
  
  }
  
 //loading captcha login page 
 var showRecaptchaFlag='';
 if(showRecaptchaFlag=='Y')
		   {
				var recaptchaDivID='login_recaptcha';
				var container = document.getElementById(recaptchaDivID);
				if(container!=null){
					container.innerHTML = '';
					var login_recaptcha= grecaptcha.render(container, {
						'sitekey': reCaptchaSiteKey
					});
				}
			}
			
			
		$(document).ready(function() {
		Tg_WebinarPage.init();
	});
	$(function(){
