<script>
var  reCaptchaSiteKey="6LcUITgUAAAAAOw4MPXzeOCCqvLsBzT7RsA-a6OA";

//Calling Captcha on dynamic loaded forms
function recaptchaCallback (selector_id) {
	
  	if(Array.isArray(selector_id))
	{
		$.each(selector_id,function(k,v){
			recaptchaCallbackInner(v);
	});

	}
	else{
		recaptchaCallbackInner(selector_id);
	}
}
function recaptchaCallbackInner(selector){
			var container = document.getElementById(selector);
			if(container!=null){
				container.innerHTML = '';
				grecaptcha.render(container, {
				'sitekey': reCaptchaSiteKey
				});
			}
}
</script>

