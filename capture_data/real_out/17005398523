	$(function(){
		$(".tgpodcast").click(function(){
			var _src = $(this).attr("data-vnode") + "&autoplay=true";
			$("#iframepodvideo").html("");
			$("#iframepodvideo").append('<iframe width="100%" height="315" src="'+ _src +'" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>');
		})
		$(".tgvideos").click(function(){
			var _src = $(this).attr("data-vnode") + "&autoplay=true";
