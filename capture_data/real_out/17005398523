<script language="javascript">
$(document).ready(function() {
    function hostReachable() {
       
        try {
            if (navigator.onLine) {
                $('.nointernetbar').hide();
            } else {
                $('.nointernetbar').show();
            }
        } catch (error) {
            $('.nointernetbar').show();
        }
        
    }
    setInterval(hostReachable, 5 * 1000);


    function setLoggedinTTL() {
	  	$.ajax({
		    type: "POST",
		    url: base_url + '/ajax_files/saas_candidate_function.php?action=setLoggedinTTL',
		    success: function(response) {
		       //
		    }
		});
	}

    
});
</script>

