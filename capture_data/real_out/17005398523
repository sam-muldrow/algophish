	$(function(){
		$(".polloptions .poll_act_block").click(function () {
			var poll_id_option = encodeURIComponent($(this).data('poll_id_option'));
			var poid = $("#poid").val();
			var url = base_url + '/general_ajax_task.php?action=submit_poll_response';
			$.post(url, {poll_id_option: poll_id_option,poll_id:poid}, function (data) {
				//console.log("result")
				$(".uloption").addClass("completed");
				data = $.trim(data);
				if (data == 'Y') {
					//
				}
			});		
		});
	})	
