    <script>
        window._sp_queue = [];
        window._sp_ = {
            config: {
                accountId: 1425,
                authCookie: "_breg-uid",
                baseEndpoint: "https://sourcepointcmp.bloomberg.co.jp",
                propertyId: 31489,
                propertyHref: 'https://mc.bloomberg.com',
