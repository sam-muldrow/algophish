<script>
function show(type){
	document.getElementById(type+"S").style.display='none';
	document.getElementById(type+"H").style.display='block';
}
function hidd(type){
	document.getElementById(type+"S").style.display='block';
	document.getElementById(type+"H").style.display='none';
}
function Ds(wx){
document.getElementById(wx).style.display='block';
}
function Dh(wx){
document.getElementById(wx).style.display='none';
}
function xiugai(){
document.getElementById("light").className = "white_content2";
}
function huifu(){
document.getElementById("light").className = "white_content";
}
function fks(f){
document.getElementById(f).style.display='block';
}
function fkh(f){
document.getElementById(f).style.display='none';
}
</script>



