 <script type="text/javascript">
     function updateImage(gupiao_code,area) {
        //alert(gupiao_code);
       var Image_url = "http://image.sinajs.cn/newchart/min/n/"+ area + gupiao_code + ".gif";
       var gupiao_link = "http://finance.sina.com.cn/realstock/company/" + area + gupiao_code +"/nc.shtml";
       $("#HQZS img").attr("src",Image_url);
       $("#HQZS a").attr("href",gupiao_link);
       //console.log(Image_url);
     }
     //console.log($("#HQZS img"));

     updateImage('300484','sz');
 function updateImages(gupiao_data) {
    var gupiao_datas = gupiao_data.split("|");
    var gupiao_code = gupiao_datas[0];
    var area = gupiao_datas[1];
    var Image_url = "http://image.sinajs.cn/newchart/min/n/"+ area + gupiao_code + ".gif";
    var gupiao_link = "http://finance.sina.com.cn/realstock/company/" + area + gupiao_code +"/nc.shtml";
       $("#HQZS img").attr("src",Image_url);
       $("#HQZS a").attr("href",gupiao_link);
 }
     </script>
