inoise","url":"/jeux-olympiques/nouvelle/1863606/carnet-pekin-reportage-chronique-jean-francois-poirier","signature2":{"signedBy":[{"biography":"","description":null,"id":21856,"fullName":"Jean-François Poirier","firstName":"Jean-François","handWrittenSignature":null,"lastName":"Poirier","picture":{"alt":"Jean-François Poirier","credit":"Radio-Canada","legend":"Jean-François Poirier","pattern":"https://images.radio-canada.ca/q_auto,w_{width}/v1/audio/animateur/{ratio}/poirier-jean-francois.png","url":"https://images.radio-canada.ca/q_auto,w_{0}/v1/audio/animateur/{1}/poirier-jean-francois.png"},"publicFigurePageIsActive":false,"publicFigurePageUrl":null,"role":"Chroniqueur","socialMedias":null,"socialMediaLinks":[{"id":"email","url":"jean-francois.poirier@radio-canada.ca","title":"Courriel"},{"id":"twitter","url":"https://twitter.com/jfpoirier","title":"Twitter"}],"title":null,"wording":null}],"type":"chronique"},"titlePrefix":"Chronique","relatedContent":[],"closedCaption":false,"videoDescription":false,"containsMedia":true,"backgroundColor":null,"startsAt":null,"endsAt":null}],"lineupEditor":"Scoop","lineupTypeId":1,"name":null,"pagedConfiguration":{"pageMaxLength":0,"nextPageUrl":null,"pageNumber":1,"pageSize":3,"previousPageUrl":null,"totalNumberOfItems":3},"partner":null,"partnership":null,"picture":null,"subtitle":null,"template":{"attribute":null,"description":"Template pour des contenus standards sports","group":"Olympics","id":"Sports-103"},"templateId":300,"title":"Vidéos à la Une","url":null},"graceNoteMedals":{"id":"1393-wintergames-medaltable-fr_ca","attributes":{"data-default_sort":"total","data-enable_medaltable_expansion":"no","data-enable_show_more":"no","data-season_id":"20212022","data-show_seasonfilter":"no","data-top_x":"4","data-widget_id":"1393-wintergames-medaltable-fr_ca","data-custom_css":"https://ici.radio-canada.ca/web-adhoc/content/gracenote/medaltable-top-four.css","data-custom-dotca_css":"https://ici.radio-canada.ca/web-adhoc/content/g