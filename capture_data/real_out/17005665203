[t?e.getDay():e.getUTCDay()]}function getFormatted(e){return e<10?"0".concat(e):e}function getMonthFr(e){var t=!(arguments.length>1)||void 0===arguments[1]||arguments[1];return["janvier","f\xe9vrier","mars","avril","mai","juin","juillet","ao\xfbt","septembre","octobre","novembre","d\xe9cembre"][t?e.getMonth():e.getUTCMonth()]}function getSmallDayFr(e){var t=!(arguments.length>1)||void 0===arguments[1]||arguments[1];return["dim","lun","mar","mer","jeu","ven","sam"][t?e.getDay():e.getUTCDay()]}function getSmallMonthFr(e){var t=!(arguments.length>1)||void 0===arguments[1]||arguments[1];return["janv","f\xe9vr","mars","avr","mai","juin","juil","ao\xfbt","sept","oct","nov","d\xe9c"][t?e.getMonth():e.getUTCMonth()]}function getTimeFromNewDate(){return new Date().getTime()}function getTimeZoneLongFr(e){var t=getTimeZone(e);return null!=t?t.long:""}function getTimeZoneShortFr(e){var t=getTimeZone(e);return null!=t?t.short:""}function getTimeZone(e){var t=e.toString().match(/(GMT-\d{4})/g),r=null!=t&&t.length>0?t[0]:"",n=isDST(e);return[{gmt:"GMT-0230",isDST:!0,long:"heure avanc\xe9e de Terre-Neuve",short:"hatn"},{gmt:"GMT-0330",isDST:!1,long:"heure normale de Terre-Neuve",short:"hntn"},{gmt:"GMT-0300",isDST:!0,long:"heure avanc\xe9e de l'Atlantique",short:"haa"},{gmt:"GMT-0400",isDST:!1,long:"heure normale de l'Atlantique",short:"hna"},{gmt:"GMT-0400",isDST:!0,long:"heure avanc\xe9e de l'Est",short:"hae"},{gmt:"GMT-0500",isDST:!1,long:"heure normale de L'Est",short:"hne"},{gmt:"GMT-0500",isDST:!0,long:"heure avanc\xe9e du Centre",short:"hac"},{gmt:"GMT-0600",isDST:!1,long:"heure normale du Centre",short:"hnc"},{gmt:"GMT-0600",isDST:!0,long:"heure avanc\xe9e des Rocheuses",short:"har"},{gmt:"GMT-0700",isDST:!1,long:"heure normale des Rocheuses",short:"hnr"},{gmt:"GMT-0700",isDST:!0,long:"heure avanc\xe9e du Pacifique",short:"hap"},{gmt:"GMT-0800",isDST:!1,long:"heure normale du Pacifique",short:"hnp"}].find(function(e){return e.isDST===n&&e.gmt===r})}function isDST(e){return Math.max(new Date(e.getFullYear(),0,1).getTimezoneOffset(),new Date(e.getFullYear(),6,1).getTimezoneOffset())!==e.getTimezoneOffset()}function isToday(e){var t=new Date,r=new Date(e);return r.getDate()===t.getDate()&&r.getMonth()===t.getMonth()&&r.getFullYear()===t.getFullYear()}function getSimpleHour(e,t,r,n){return"".concat(e).concat(t).concat(r).concat(n)}function formatTime(){if("undefined"!=typeof document){for(var e=[],t=document.getElementsByTagName("time"),r=0;r<t.length;r+=1)t[r].getAttribute("data-timeutc")&&e.push(t[r]);for(var t1=0;t1<e.length;t1+=1){var r1=e[t1].getAttribute("data-prefix"),n=new Date(e[t1].getAttribute("data-timeutc")),a=e[t1].getAttribute("data-format");e[t1].innerHTML=formatDate({timeUtc:n,format:a,prefix:r1})}}}function transformDateSlugToDateOffset(e){var t=new Date().getTime();return Math.round((new Date(e).getTime()-t)/864e5)+1}document.getElementsByTagName("esi:include").length>0&&console.error("WARNING <ESI> markup should not be there"),formatTime();</script>
