  </script><script>function formatDate(e){var t=e.timeUtc,r=e.format,tmp=e.hasTimeZone,n=void 0===tmp||tmp,a=e.isA11y,o=e.prefix,i=Math.floor((new Date-t)/1e3/60),l=Math.floor(i/60),g=Math.floor(l/24),m=Math.floor(g/31),$=Math.floor(m/12),h=a?"":"\xa0",f=t.getHours()>1?"s":"",d=a?" heure".concat(f):"\xa0h",c=a?" heure".concat(f," "):"\xa0h\xa0",T=t.getMinutes()>1?"s":"",D=a?" minute".concat(T):"",p=n?t.getDate():t.getUTCDate(),M=getDayFr(t,n),y=M.replace(/^\w/,function(e){return e.toUpperCase()}),S=n?t.getFullYear():t.getUTCFullYear(),F=n?t.getHours():t.getUTCHours(),x=n?t.getMinutes():t.getUTCMinutes(),w=n?t.getMonth():t.getUTCMonth(),v=getMonthFr(t,n),C=n?t.getSeconds():t.getUTCSeconds(),b=getSmallDayFr(t,n),G=b.replace(/^\w/,function(e){return e.toUpperCase()}),U=getSmallMonthFr(t,n),j=getFormatted(p),H=getFormatted(F),Z=getFormatted(x),z=getFormatted(w),Y=getFormatted(C);switch(r){case"date":case"simple-date-full-month-year":return"".concat(p," ").concat(v," ").concat(S);case"date-time":return"".concat(S,"-").concat(z,"-").concat(j," ").concat(H,":").concat(Z,":").concat(Y);case"date-with-hours":return"".concat(p).concat(h).concat(v).concat(h).concat(S," \xe0 ").concat(F).concat(c).concat(Z).concat(D);case"date-with-weekday":case"full-date-and-year":return"".concat(M," ").concat(p," ").concat(v," ").concat(S);case"diffusion-date-time":return"".concat(y," ").concat(p," ").concat(v,", ").concat(F).concat(c).concat(Z).concat(D);case"full-date":return"".concat(M," ").concat(p," ").concat(v);case"full-date-hours":return"".concat(M).concat(h).concat(p).concat(h).concat(v).concat(h).concat(S," \xe0 ").concat(F).concat(c).concat(Z).concat(D);case"full-day":return"".concat(M);case"hour":return"".concat(F).concat(d);case"hours":return l<=1?"".concat("string"==typeof o?o:"","1 heure"):"".concat("string"==typeof o?o:"").concat(l," heures");case"minutes":return i<=1?"".concat("string"==typeof o?o:"","1 minute"):"".concat("string"==typeof o?o:"").concat(i," minutes");case"day