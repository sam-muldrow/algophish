<script>
    function removeTopbanner() {
        const el = document.getElementById("removeTopbanner1");
        const el2 = document.getElementById("removeTopbanner2");
        const el3 = document.getElementById("topbanner");
        const el4 = document.getElementById("skyscrapperHome");
        el.classList.remove("has_topbanner");
        el2.classList.remove("has_topbanner");
        el3.remove();
        el4.classList.remove("hasTopBanner");
    }
</script>

