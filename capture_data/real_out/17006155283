<!-- コース番号検索 -->
<section id="SearchCourse">
    <div class="SearchCourse__inner">
        <h2 class="SearchCourse__ttl">コース番号検索</h2>
        <form action="/course/index.php" method="post" id="search_no">
            <div class="SearchCourse__form">
                <input type="radio" name="p_naigai" id="kokunai" checked value="J">
                <label for="kokunai" class="SearchCourse__radio-label"><span class="SearchCourse__radio-btn"></span>国内旅行</label>
                <input type="radio" name="p_naigai" id="kaigai" value="W">
                <label for="kaigai" class="SearchCourse__radio-label"><span class="SearchCourse__radio-btn"></span>海外旅行</label>
                <span class="SearchCourse__input-text-wrap">
                    <input type="text" class="SearchCourse__input-text" placeholder="例）12192" name="p_course_id" value="" id="p_course_id">
                    <span class="SearchCourse__alert" style="display:none;"><i class="SearchCourse__alert-icn">!</i><span class="SearchCourse__alert-txt">コース番号を2桁以上で入力ください</span></span>
                </span>
                <button class="SearchCourse__submit">検索する</button>
            </div>
        </form>
    </div>
</section>

<script>
    document.addEventListener('DOMContentLoaded', function () {
        document.getElementById('kaigai').addEventListener('change', function (e) {
            if (!e.target.checked) return;
            document.getElementById('p_course_id').placeholder = '例）E509';
        }, true);
        document.getElementById('kokunai').addEventListener('change', function (e) {
            if (!e.target.checked) return;
            document.getElementById('p_course_id').placeholder = '例）12192';
        }, true);

        function validateCourseId() {
            var course_id = document.getElementById('p_course_id').value;

            if (!course_id) {
                $('.SearchCourse__alert-txt').html('コース番号を2桁以上で入力してください');
                $('.SearchCourse__alert').css('display', 'flex');
                return false;
            }

            //全角英数字だったら半角に
            course_id = course_id.replace(/[Ａ-Ｚａ-ｚ０-９]/g, function (s) {
                return String.fromCharCode(s.charCodeAt(0) - 0xFEE0)
            });

            if (course_id.startsWith('#') || course_id.startsWith('＃')) {
                $('.SearchCourse__alert-txt').html('コース番号の一文字目は英数字で入力ください');
                $('.SearchCourse__alert').css('display', 'flex');
                return false;
            }

            var course_id_len = course_id.length;
            var array = ["の"];
            var index = -1;
            for (var i = 0; i < array.length; i++) {
                if (course_id.indexOf(array[i]) != -1) {
                    index = course_id.indexOf(array[i]);
                    break;
                }
            }

            var array2 = ["-", "−", "ー", "―", "–", "‐", "－"];
            var index2 = -1;
            for (var i = 0; i < array2.length; i++) {
                if (course_id.indexOf(array2[i]) != -1) {
                    index2 = course_id.indexOf(array2[i]);
                    break;
                }
            }

            var indexPosition;
            if (index != -1) {
                //「の」あり
                indexPosition = course_id_len - index;

                if (indexPosition) {
                    course_id = course_id.slice(0, -indexPosition);
                }
                course_id_len = course_id.length;
                if (course_id_len < 2) {
                    $('.SearchCourse__alert-txt').html('ひらがなの『の』より前のコース番号は\n2桁以上で入力してください');
                    $('.SearchCourse__alert').css('display', 'flex');
                    return false;
                }
            /*} else if (index2 != -1) {
                //ハイフンあり
                indexPosition = course_id_len - index2;
                var edaban = course_id.slice(-indexPosition + 1);
                var edaLen = edaban.length;
                if (edaLen != 3) {
                    $('.SearchCourse__alert-txt').html('ハイフン付きはハイフン以降を3桁で入力してください');
                    $('.SearchCourse__alert').css('display', 'flex');
                    return false;
                }*/
            } else {
                //ハイフンなし
                if (course_id_len < 2) {
                    $('.SearchCourse__alert-txt').html('コース番号を2桁以上で入力ください');
                    $('.SearchCourse__alert').css('display', 'flex');
                    return false;
                }
            }

            $('.SearchCourse__alert').css('display', 'none');

            return course_id;
        }

        document.getElementById('p_course_id').addEventListener('keyup', function(e) {
            if ($('.SearchCourse__alert').css('display') != 'none') {
                validateCourseId();
            }
        }, true);

        document.getElementById('search_no').addEventListener('submit', function (e) {
            var naigai = document.querySelector('input[name=p_naigai]:checked');
            if (!naigai) {
                e.preventDefault();
                alert('海外旅行または国内旅行を選択してください');
                return false;
            }

            var course_id = validateCourseId();
            console.log(course_id);
            if (!course_id) {
                e.preventDefault();
                return false;
            }

            document.getElementById('p_course_id').value = course_id;
        }, true);
    });
</script>

