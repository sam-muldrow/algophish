    // あなたへのおすすめツアー
    var ppz_recommend_myHatsu = "";
    var ppz_recommend_myHatsuSub = "";
    var ppz_pctop01 = new _PPZ();
    ppz_pctop01.cid = 21017;
    ppz_pctop01.rid = 13;
    ppz_pctop01.customer_id = ''; //顧客IDを代入
    ppz_pctop01.rows = 20;//表示したいMAX件数を設定(最大20件)
    ppz_pctop01.cb = 'ppz_pctop_personal';//ここの関数でレコメンド表示を作成してください。(ppz_draw.js)
    ppz_pctop01.div_id = 'ppz_recommend_pctop';//レコメンド表示を行うタグのIDを指定(divのIDと一致させます)
    ppz_pctop01.alt_html = '';
    ppz_pctop01.request();
</script>

