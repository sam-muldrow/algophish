    window.hit_num['#d-search' + data] = 143367;

    $('#d-search-form').on('submit', function(e) {
        if (!should_submit_d) {
            e.preventDefault();
            return false;
        }
    });

    $('.Search__submit', '#d-search').on('click', function(e) {
        should_submit_d = true;
        $('#d-search-form').submit();
    });

    $('input', '#d-search').on('change', function() {
        if (!$(this).hasClass('departure-selector') && !$(this).hasClass('destination-selector') && !$(this).hasClass('brand-selector')) {
            d_updateHitNum();
        }
    });

    $('select', '#d-search').on('change', function() {
        if (this.id == 'p_transport') return;
        d_updateHitNum();
    });

    $('#p_transport', '#d-search').on('change', function() {
        if ($(this).val() == '3') { // 飛行機
            $('#departure-box', '#d-search').hide();
            $('#airport-box', '#d-search').show();
            prev_p_hatsu_sub = $('#p_hatsu_sub', '#d-search').val();
            $('#p_hatsu_sub', '#d-search').val('');
            if(prev_p_dep_airport_code){
                $('#p_dep_airport_code', '#d-search').val(prev_p_dep_airport_code);
            }
            depAirportOpen('#d-search', kyotens, d_updateHitNum);
            depAirportConfirm('#d-search', d_updateHitNum);
        } else {
            $('#departure-box', '#d-search').show();
            $('#airport-box', '#d-search').hide();
            $('#p_hatsu_sub', '#d-search').val(prev_p_hatsu_sub);
            prev_p_dep_airport_code = $('#p_dep_airport_code', '#d-search').val();
            $('#p_dep_airport_code', '#d-search').val('');
            departureOpen('#d-search', kyotens, d_updateHitNum);
            departureConfirm('#d-search', d_updateHitNum);
        }
        d_updateHitNum();
    });

    departureOpen('#d-search', kyotens, d_updateHitNum);
    departureConfirm('#d-search', d_updateHitNum);
    brandOpen('#d-search', d_updateHitNum);
    brandConfirm('#d-search');
    depAirportOpen('#d-search', kyotens, d_updateHitNum);
    depAirportConfirm('#d-search', d_updateHitNum);
    init_d = false;
});

function d_updateHitNum(override, cb) {
    var data;
    if (override) {
        var before = $('#d-search-form').values();
        Object.keys(override).forEach(function(k) {
            $('input[name=' + k + ']', '#d-search').val(override[k]);
        });
        data = $('#d-search-form').serialize();
        $('#d-search').values(before);
    } else {
        data = $('#d-search-form').serialize();
        if ('pushState' in window.history && !init_d) {
            var state = window.history.state ? (JSON.parse(window.history.state) || {}) : {};
            if (!state['#d-search']) state['#d-search'] = {};
            data.split('&').forEach(function(val) {
                var parts = val.split('=');
                state['#d-search'][parts[0]] = decodeURIComponent(parts[1]);
            });
            window.history.pushState(JSON.stringify(state), '', window.location.href);
        }
    }
    if (window.p_region_cn_facet['#d-search' + data] && window.p_hatsu_name_facet['#d-search' + data] && window.p_mainbrand_facet['#d-search' + data] && window.p_dep_airport_name_facet['#d-search' + data]) {
        if (!override) {
            $('#dp_hit_num').text(window.hit_num['#d-search' + data]);
        }
        return cb && cb(data);
    }
    $.ajax({
        type: "POST",
        data: data,
        url: '/sharing/__br/phpsc/ajax_searchBox21.php',
        dataType: 'json',
        success: function(result) {
            if (result.ok) {
                if (!override) {
                    $('#dp_hit_num').text(result.p_hit_num);
                }
                window.hit_num['#d-search' + data] = result.p_hit_num;
                window.p_region_cn_facet['#d-search' + data] = result.p_region_cn;
                window.p_hatsu_name_facet['#d-search' + data] = result.p_hatsu_name;
                window.p_dep_airport_name_facet['#d-search' + data] = result.p_dep_airport_name;
                window.p_mainbrand_facet['#d-search' + data] = result.p_mainbrand;
                window.p_dep_date_facet['#d-search' + data] = result.p_dep_date;
                cb && cb(data);
            }
        },
    });
}
</script>

