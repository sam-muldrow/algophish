    window.hit_num['#i-search' + data] = 74045;

    function decideGetValues() {
        var names = [];
        var decide = [];
        $('.decide-select:checked', '#i-search').each(function() {
            decide.push($(this).val());
            names.push($(this).data('name'));
        });
        return {
            names: names,
            decide: decide,
        };
    }

    function decideConfirm() {
        var result = decideGetValues();
        var names = result.names;
        var decide = result.decide;
        $('input[name=p_decide]', '#i-search').val(decide.join(','));
        if (decide.length) {
            $('.decide-open', '#i-search').val(names.join('、'));
        } else {
            $('.decide-open', '#i-search').val('指定しない');
        }
    }

    function decideNoResults(data) {
        $('input.decide-select:disabled', '#i-search').prop('disabled', false);
        if (window.p_decide_facet['#i-search' + data]) {
            $('input.decide-select', '#i-search').each(function() {
                if (!window.p_decide_facet['#i-search' + data][$(this).val()]) {
                    $(this).prop('disabled', true);
                }
            });
        }
    }

    function decideOpen() {
        var data = $('form', '#i-search').serialize();
        decideNoResults(data);
        var decide = $('input[name=p_decide]', '#i-search').val();
        $('.decide-select', '#i-search').prop('checked', false);
        if (decide) {
            decide.split(',').forEach(function(val) {
                $('.decide-' + val, '#i-search').prop('checked', true);
            });
        }
    }

    $('.decide-confirm', '#i-search').on('click', function(e) {
        decideConfirm();
        i_updateHitNum();
    });

    $('.decide-open', '#i-search').on('click', function(e) {
        decideOpen();
    });

    $('#i-search-form').on('submit', function(e) {
        if (!should_submit_i) {
            e.preventDefault();
            return false;
        }
    });

    $('.Search__submit', '#i-search').on('click', function(e) {
        should_submit_i = true;
        $('#i-search-form').submit();
    });

    $('input', '#i-search').on('change', function() {
        if (!$(this).hasClass('departure-selector') && !$(this).hasClass('destination-selector') && !$(this).hasClass('brand-selector')) {
            i_updateHitNum();
        }
    });

    $('select', '#i-search').on('change', function() {
        i_updateHitNum();
    });

    departureOpen('#i-search', kyotens, i_updateHitNum);
    departureConfirm('#i-search', i_updateHitNum);
    brandOpen('#i-search', i_updateHitNum);
    brandConfirm('#i-search');
    decideOpen();
    decideConfirm();
    init_i = false;
});

function i_updateHitNum(override, cb) {
    var data;
    if (override) {
        var before = $('#i-search-form').values();
        Object.keys(override).forEach(function(k) {
            $('input[name=' + k + ']', '#i-search').val(override[k]);
        });
        data = $('#i-search-form').serialize();
        $('#i-search').values(before);
    } else {
        data = $('#i-search-form').serialize();
        if ('pushState' in window.history && !init_i) {
            var state = window.history.state ? (JSON.parse(window.history.state) || {}) : {};
            if (!state['#i-search']) state['#i-search'] = {};
            data.split('&').forEach(function(val) {
                var parts = val.split('=');
                state['#i-search'][parts[0]] = decodeURIComponent(parts[1]);
            });
            window.history.pushState(JSON.stringify(state), '', window.location.href);
        }
    }
    if (window.p_region_cn_facet['#i-search' + data] && window.p_hatsu_name_facet['#i-search' + data] && window.p_mainbrand_facet['#i-search' + data]) {
        if (!override) {
            $('#ip_hit_num').text(window.hit_num['#i-search' + data]);
        }
        return cb && cb(data);
    }
    $.ajax({
        type: "POST",
        data: data,
        url: '/sharing/__br/phpsc/ajax_searchBox21.php',
        dataType: 'json',
        success: function(result) {
            if (result.ok) {
                if (!override) {
                    $('#ip_hit_num').text(result.p_hit_num);
                }
                window.hit_num['#i-search' + data] = result.p_hit_num;
                window.p_region_cn_facet['#i-search' + data] = result.p_region_cn;
                window.p_hatsu_name_facet['#i-search' + data] = result.p_hatsu_name;
                window.p_mainbrand_facet['#i-search' + data] = result.p_mainbrand;
                window.p_decide_facet['#i-search' + data] = result.p_decide;
                window.p_dep_date_facet['#i-search' + data] = result.p_dep_date;
                cb && cb(data);
            }
        },
    });
}
</script>

