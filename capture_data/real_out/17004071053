<script>

fetch("https://www.gazetaexpress.com/data/countries.json")
  .then(response => response.json())
  .then(data => {
	  let countries = data.countries;
	  let countryName = [];
	  countries.forEach(function (arrayItem) {
		  	
			countryName.push(arrayItem.name);
		  
	});
	countryName.forEach(function(country){
		let	CountryTitle = country
		if(CountryTitle == 'Kosovo'){
			CountryTitle = 'Kosova';
		}
		jQuery('#country-list')
		   .append(jQuery('<a href="javascript:void(0)" onclick="countryLink(\''+ country +'\')" class="country-link"' + country + '">' + CountryTitle + '</a>)'))
	})
  });
fetch("https://www.gazetaexpress.com/data/Total.json")
.then(responseTotal => responseTotal.json())
	.then(dataTotal => { 
		let confirmedTotal = dataTotal[0].confirmed;
		let recoveredTotal = dataTotal[0].recovered;
		let deathsTotal = dataTotal[0].deaths;
		jQuery("#infected-number" ).html( confirmedTotal );
		jQuery("#recovered-number" ).html( recoveredTotal );
		jQuery("#deaths-number" ).html( deathsTotal);
});
jQuery("#location-country").click(function(){
  jQuery(".dropdown-content").removeClass("d-none-content");
});
function countryLink(countryName){
	fetch("https://www.gazetaexpress.com/data/" +countryName+".json")
	.then(responseSingle => responseSingle.json())
	.then(dataSingle => { 
			let confirmed = dataSingle[0].confirmed;
			let recovered = dataSingle[0].recovered;
			let deaths = dataSingle[0].deaths;
			jQuery("#infected-number" ).html( confirmed );
			jQuery("#recovered-number" ).html( recovered );
			jQuery("#deaths-number" ).html( deaths);
			if(countryName == 'Kosovo'){
				countryName = 'Kosova';
			}
			jQuery("#location-country" ).html( countryName);
			jQuery(".dropdown-content").addClass("d-none-content");
	});
}

</script>

