<script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script>
    var scrollEventName = (!!navigator.userAgent.match(/(iPad|iPhone|iPod)/g)) ? 'touchend' : 'scroll';
    window.dataLayer = window.dataLayer || [];

    function sendPostMessageToPiano(iframeId, success, message, object) {
        var iframe = document.getElementById(iframeId);

        if (iframe) {
            iframe.contentWindow.postMessage({
                piano: {
                    success: success,
                    message: message,
                    object: object
                }
            }, '*');
        }
    }

    function pianoSetCustomVariableCookie(name, value) {
        // Get all existing values from the cookie
        var cookieValue = pianoReadCustomVariableCookie();
        // Set the value
        cookieValue[name] = value;

        // Get cookie expiration date in 3 years
        var d = new Date();
        d.setTime(d.getTime() + (94608000000));
        var expires = "expires=" + d.toUTCString();

        // Write the cookie value
        document.cookie = "__pcvc=" + JSON.stringify(cookieValue) + ";" + expires + ";path=/";
    }

    function pianoReadCustomVariableCookie(specificName) {
        var cookieValue;
        try {
            // Try to get the cookie value with regexp
            var match = document.cookie.match(new RegExp('(^| )__pcvc=([^;]+)'));
            if (match) {
                // If cookie with this name was found - try to parse the JSON value
                cookieValue = JSON.parse(match[2]);
            }
        } catch (e) {
            // By default - set value as empty object
            cookieValue = {};
        }
        if (!cookieValue) {
            // By default - set value as empty object
            cookieValue = {};
        }
        if (typeof specificName != "undefined") {
            // Get the specific value from the set
            if (typeof cookieValue[specificName] != "undefined") {
                return cookieValue[specificName];
            }
            return null;
        }
        return cookieValue;
    }

    function inScreenOfferTracking(event) {
        var el = document.querySelector(event.data.container);

        if (el && utils.elementInViewport(el)) {
            $(window).off(scrollEventName + '.' + event.data.offerId, inScreenOfferTracking);
            gtmEvent('piano_showOfferInView', {'piano_offerId': event.data.offerId});

            let offer = $.getJSON('/ajax/piano/get/offer', {
                offerId: event.data.offerId,
                cachable: window.__DEV ? 0 : 1
            });

            offer.done(function(data) {
                if (data.success) {
                    gtmEvent('view_item_list', data.rs, true);
                }
            });
        }
    }

    (function(src) {
        var a = document.createElement('script');
        a.type = 'text/javascript';
        a.async = true;
        a.src = src;
        var b = document.getElementsByTagName('script')[0];
        b.parentNode.insertBefore(a, b);
    })('//experience-eu.piano.io/xbuilder/experience/load?aid=prevcRcKpe');
    (function() {
        tp = window['tp'] || [];

        var cookieValues = pianoReadCustomVariableCookie(),
            termGtmObject;

        for (var i in cookieValues) {
            // Set custom variables from cookie
            tp.push(['setCustomVariable', i, cookieValues[i]]);
        }

                tp.push(['setTags', []]);
                        tp.push(['setZone', 'FreeContent']);
                                tp.push(['setContentAuthor', '15min']);
                                
        
        tp.push(["setLocale", "lt_LT"]);

        //for login
        tp.push(['setUseTinypassAccounts', false]);
        tp.push(['setUsePianoIdUserProvider', false]);
        tp.push(['setUsePianoIdLiteUserProvider', true]);
        tp.push(['setPianoIdUrl', 'https://id-eu.piano.io/']);
        tp.push(['setApplePayMerchantId', 'merchant.lt.15min']);
        tp.push(['addHandler', 'loginRequired', function(params) {
            //REDIRECT LOGIN
            localStorage.setItem('checkoutParams', JSON.stringify(params));

            // Redirect user to login page
            location.href = "/profilis/registruotis?u=" + encodeURIComponent(window.location.href);

            return false;
        }]);
                
        // Newsletters
        tp.push(["addHandler", "customEvent", function(event, b, c, d) {
            switch (event.eventName) {
                case 'email-signup':
                    let email = '';
                    let params;
                    let iframeId;

                    params = JSON.parse(event.params.params);
                    iframeId = params.iframeId;

                    if ((typeof event.params.email != 'undefined') && (event.params.email.length > 0)) {
                        email = event.params.email;
                    }

                    let response = $.getJSON('/ajax/news-letter/subscribe', {
                        email: email,
                        user: '',
                        return_type: 'json'
                    });

                    response.done(function(data) {
                        if (data.success) {
                            // If the email was added successfully set a cookie for Composer tracking.
                            pianoSetCustomVariableCookie('newsletter', 'true');
                            sendPostMessageToPiano(iframeId, true, '', '');
                        } else {
                            sendPostMessageToPiano(iframeId, false, data.errors[0], 'email');
                        }
                    });
                    break;
            }
        }]);

        tp.push(["addHandler", "showOffer", function(data) {
            if (typeof data.offerId != 'undefined') {
                gtmEvent('piano_showOffer', {'piano_offerId': data.offerId});

                if (data.displayMode === 'modal') {
                    gtmEvent('piano_showOfferInView', {'piano_offerId': data.offerId});
                } else {
                    var selector = data.containerSelector;

                    if (data.containerSelector.indexOf('piano-article-inline') > -1) {
                        var container = document.querySelector(data.containerSelector);

                        if (!container) {
                            return;
                        }

                        var article = container.closest('.article');
                        var id = article.dataset.id;

                        container.classList.remove('piano-article-inline');
                        container.classList.add('piano-article-inline-loaded');

                        selector = '.article-' + id + ' .piano-article-inline-loaded';
                    }

                    $(window).on(
                        scrollEventName + '.' + data.offerId,
                        {container: selector, offerId: data.offerId},
                        inScreenOfferTracking
                    );

                    inScreenOfferTracking({data: {container: selector, offerId: data.offerId}});
                }
            }
        }]);

        tp.push(["addHandler", "startCheckout", function(data) {
            if (typeof data.offerId != 'undefined') {
                gtmEvent('piano_startCheckout', {'piano_offerId': data.offerId});

                let term = $.getJSON('/ajax/piano/get/term', {
                    termId: data.termId,
                    offerId: data.offerId,
                    cachable: window.__DEV ? 0 : 1
                });

                term.done(function(data) {
                    if (data.success) {
                        gtmEvent('select_item', {'ecommerce': {'items': data.rs.ecommerce.items}}, true);
                        gtmEvent('add_to_cart', data.rs, true);
                        gtmEvent('begin_checkout', data.rs, true);
                        termGtmObject = data.rs;
                    }
                });
            }
        }]);

        tp.push(["addHandler", "submitPayment", function(data) {
            if (typeof data.offerId != 'undefined') {
                gtmEvent('piano_submitPayment', {'piano_offerId': data.offerId});
                gtmEvent('add_payment_info', termGtmObject, true);
            }
        }]);

        tp.push(["addHandler", "checkoutError", function(errorData) {
            if (typeof errorData.message != 'undefined') {
                gtmEvent('piano_checkoutError', {'piano_errorMessage': errorData.message});
            }
        }]);

        tp.push(["addHandler", "checkoutComplete", function(conversion) {
            if (typeof conversion.termConversionId != 'undefined') {
                termGtmObject['ecommerce']['transaction_id'] = conversion.termConversionId;
                tp.push(["setCustomVariable", "lastConversionId", conversion.termConversionId]);
                gtmEvent('piano_checkoutComplete', {'piano_termConversionId': conversion.termConversionId});
                gtmEvent('purchase', termGtmObject, true);
            }
        }]);

        tp.push(["addHandler", "checkoutClose", function(event) {
            // The event object contains information about the state of closed modal
            switch (event.state) {
                case 'checkoutCompleted':
                    // Checkout complete close
                    if (typeof tp.customVariables.lastConversionId != 'undefined'
                        && tp.customVariables.lastConversionId) {
                        const check = $.getJSON('/ajax/piano/check/conversion', {
                            conversionId: tp.customVariables.lastConversionId
                        });

                        check.done(function(data) {
                            if (data.success) {
                                window.location.reload();
                            }
                        });

                        tp.push(["setCustomVariable", "lastConversionId", false]);
                    }
                    // User completed the purchase and now has access
                    // Usually it's a good practice to reload the page
                    break;
                case 'alreadyHasAccess':
                    // User already has access
                    // This state could be a result of user logging in during checkout process
                    // Usually it's a good practice to reload the page here as well
                    break;
                case 'voucherRedemptionCompleted':
                    // User redeemed a gift voucher
                    // Normally gift redemption happens on a landing page,
                    // so logically it makes sense to redirect user to a home page after this
                    break;
                case 'close':
                    // User did not complete the purchase and simply closed the modal
                    gtmEvent('remove_from_cart', termGtmObject, true);
            }
        }]);

        tp.push(['init', function() {
            tp.experience.init();

            // If user is logged in
            if (tp.user.isUserValid()) {
                const checkoutParams = localStorage.getItem('checkoutParams');
                let params;

                try {
                    // Try to parse stored JSON data
                    params = JSON.parse(checkoutParams);
                } catch (e) {
                    params = false;
                }
                // If params object is valid - start checkout
                if (params) {
                    tp.offer.startCheckout(params);
                }

                localStorage.removeItem('checkoutParams');
            }
        }]);

        window.googletag && googletag.cmd.push(function() {
            if ($('.piano-front-bottom-strip').length) {
                $('.piano-front-bottom-strip').hide();
            }
            if ($('.piano-product-offer-bottom-strip').length) {
                $('.piano-product-offer-bottom-strip').hide();
            }

            googletag.pubads().addEventListener('slotRenderEnded', function(event) {
                if (event.slot.getAdUnitPath().match(/1x1|1x1_2$/)) {
                    if (event.isEmpty) {
                        if ($('.piano-front-bottom-strip').length) {
                            $('.piano-front-bottom-strip').show();
                        }
                        if ($('.piano-product-offer-bottom-strip').length) {
                            $('.piano-product-offer-bottom-strip').show();
                        }

                    }
                }
            });
        });
    })();

    function __pageBreakCallback() {
      if (window.tp) {
        tp.push(['setCustomVariable', 'page-break', 1]);
      }
    }
</script>


