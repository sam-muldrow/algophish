      (function() {
        googletag.cmd.push(function() {
          googletag.pubads().addEventListener('slotRenderEnded', function (event) {
            if (event.slot.getAdUnitPath().match(/15min_front_300x600_3$/)) {
              var el = document.getElementById(event.slot.getSlotElementId());
              if (el) {
                var parent = el.closest('.widget.ads');
                parent && parent.classList.add('black-friday-deal');

                var lineAfterElement = document.querySelector('.widget-group-3043 .widget.front');

                if (lineAfterElement) {
                  var line = document.createElement('div'),
                      text1 = document.createElement('div'),
                      text2 = document.createElement('div'),
                      text3 = document.createElement('div'),
                      arrow = document.createElement('div');

                  line.classList.add('black-friday-deal-line');
                  text1.textContent = 'Black Friday';
                  text2.textContent = 'Black Friday';
                  text3.textContent = 'Black Friday';

                  line.appendChild(text1);
                  line.appendChild(text2);
                  line.appendChild(text3);
                  line.appendChild(arrow);

                  lineAfterElement.parentNode.insertBefore(line, lineAfterElement);
                }
              }
            }

            if (event.slot.getAdUnitPath().match(/m\.15min_front_300x250_5$/)) {
              var lineAfterElement = document.querySelector('.widget-4384');

              if (lineAfterElement) {
                var line = document.createElement('div'),
                    text1 = document.createElement('div'),
                    text2 = document.createElement('div'),
                    arrow1 = document.createElement('div'),
                    arrow2 = document.createElement('div'),
                    arrow3 = document.createElement('div');

                line.classList.add('black-friday-deal-line-mobile');
                text1.textContent = 'Black Friday';
                text2.textContent = 'Black Friday';

                  line.appendChild(arrow1);
                  line.appendChild(text1);
                  line.appendChild(arrow2);
                  line.appendChild(text2);
                  line.appendChild(arrow3);

                lineAfterElement.parentNode.insertBefore(line, lineAfterElement);
              }
            }
          });
        });
      })();
    </script>

