<script>
   (function() {
      /* Burger Menu */
      nav_pub = document.querySelector("#nav_publications");
      burger_menu = document.getElementById("header_burger");
      top_navig = document.getElementById("top-navig");

      // thisUrl = window.location.pathname;
      // console.log("url_last segment" + thisUrl.substring(thisUrl.lastIndexOf('/') + 1));

      burger_menu.addEventListener("click", function(event) {
         this.classList.toggle("burger_active");
         nav_pub.style.top = document.getElementById("nav_container").offsetHeight + "px";
         nav_pub.classList.add("animate__animated");
         nav_pub.classList.add("animate__slideInLeft");
         if (!this.classList.contains("burger_active"))
            nav_pub.classList.add("animate__slideOutLeft");
         else
            nav_pub.classList.remove("animate__slideOutLeft");
      });

      function detectwinWidth() {

         if (window.innerWidth >= 768)
            nav_pub.classList.add("desktop-nav");
         else {
            nav_pub.classList.remove("desktop-nav");
            nav_pub.classList.add("expand-scroll");
         }
      }

      detectwinWidth();
      $(window).resize(function() {
         detectwinWidth();
      });

      var subSecs = document.querySelectorAll(".sub-secs");
      var mobnav = document.querySelectorAll(".mobile-navig");

      subSecs.forEach((el, index) => el.addEventListener("click", event => {
         pmn = el.parentNode.querySelector(".mobile-navig");
         pnav = document.getElementById("nav_publications");
         console.log(index);
         if (!pnav.classList.contains("desktop-nav")) {
            pmn.classList.toggle("expand");
            el.querySelector(".arrow-right").classList.toggle('arrow-down');
            pnav.classList.add("expand-scroll");

            // detect expand scroll
            r = true;
            for (i = 0; i < mobnav.length; i++) {
               if (mobnav[i].classList.contains("expand")) {
                  r = false;
                  break;
               }
            }
            if (r == true)
               pnav.classList.remove("expand-scroll");

         } else {

            for (i = 0; i < mobnav.length; i++) {
               if (i == index) {
                  pmn.classList.toggle("expand");
                  subSecs[index].classList.toggle("active");
               } else {
                  mobnav[i].classList.remove("expand");
                  subSecs[i].classList.remove("active");
               }
            }

         }
         return false;
      }));

      var arrowLb = document.querySelectorAll(".arrow-lb"),
         navig_subnav = document.querySelector(".mobile-navig_subnav");
      subnav_close = document.querySelector(".subnav_close");

      console.log(arrowLb);
      arrowLb.forEach((el, index) => el.addEventListener("click", event => {
         top_navig.style.display = "none";
         myID = el.getAttribute('id');
         navig_subnav.classList.add("open");
         // console.log(myID);
         return false;
      }));

      subnav_close.addEventListener("click", function(event) {
         top_navig.style.display = "block";
         navig_subnav.classList.remove("open");
      });

      /* contrib button in subnav */
      // let liContrib = document.createElement("li");
      html = '<a class="contrib-button__nav" href="/support-our-work">SUPPORT PHILSTAR</a>';
      // liContrib.innerHTML = html;
      let liContrib2 = document.createElement("li");
      liContrib2.innerHTML = html;
      // document.getElementById("top-navig").appendChild(liContrib);
      document.getElementById("main-navig").appendChild(liContrib2);

      //adjust leaderboardtop margin
      document.getElementById("skin_holder").style.marginTop = document.getElementById("header").offsetHeight + "px";

   })();
</script>
