<script type="text/javascript">

var jscrolls = 0;

function endless(slotName) {

      //googletag.pubads().disableInitialLoad();
   googletag.cmd.push(function() {
	   googletag.defineSlot('/1015401/Philstar_all_MRec_Left1_300x250', [[1, 1], [300, 250]], slotName).addService(googletag.pubads());
	   googletag.pubads().enableSingleRequest();
	   googletag.pubads().collapseEmptyDivs();
	   googletag.enableServices();
      // Display has to be called before
      googletag.display(slotName);
   });
}

(function($){

   $.getScript( "https://www.philstar.com/js/jquery.jscroll.min.js", function( data, textStatus, jqxhr ) {
      $('#news_main').jscroll({
         loadingHtml: '<div class="loading_gif"><img style="width: 20px; height: 20px" src="https://www.philstar.com/images/Home/loader.gif"></div>',
         callback: function() {
            call_google("Homepage article bottom lazy load " + (jscrolls + 1));
			console.log('jscrolls'+jscrolls);
            ready_sharing();

            $(".jscroll-added").each(function(index) {
               if (jscrolls % 2 == 0 && index == jscrolls) {
                  $(this).find(".news_column").each(function(index) {
                     if (index == 0) {
                        console.log("left: #left" + Math.floor(jscrolls / 2));
                        $(this).prepend('<div class="tiles late trend alwaystable"><div id="left' + Math.floor(jscrolls / 2) + '" class="TilesText TilesAd"></div></div>');
                        endless("left" + Math.floor(jscrolls / 2));
                     }
                  });

                  jscrolls += 1;
               }
               else if (jscrolls % 2 == 1 && index == jscrolls - 1) {
                  $(this).find(".news_column").each(function(index) {
                     if (index == 1) {
                        console.log("right: #right" + Math.floor(jscrolls / 2));
                        $(this).append('<div class="tiles late trend alwaystable"><div id="right' + Math.floor(jscrolls / 2) + '" class="TilesText TilesAd"></div></div>');
                        endless("right" + Math.floor(jscrolls / 2));
                     }
                  });

                  jscrolls += 1;
               }
            });

            if ($("#section_checker").css('display') != "none") {
               $(".section_switcher").each(function() {
                  if ($(this).hasClass('active')) {
                     var section = $(this).attr('targ');

                     $(".tiles, .typeNews").css({display: "none"});

                     if (section == "articles") {
                        $(".tiles").css({display: "inline-block"});
                        $("#sidebar").css({display: "none"});
                     }
                     else if (section == "columnist") {
                        $(".tiles").css({display: "none"});
                        $("#sidebar").css({display: "inline-block"});
                     }
                     else {
                        $("." + section).css({display: "inline-block"});
                        $(".typeNews." + section).css({display: "none"});
                     }
                  }
               });
            }
         }
      });
   });

})(jQuery);


   // owl_carousel

   var checkWidth = jQuery(window).width();
   
   function carousel_actual_config(){
      return {
            items: 1,
            loop: true,
            autoplay: true,
            autoplayTimeout: 4000,
            autoplayHoverPause: true,
            responsiveClass: true,
            responsive: {
               0: {
                  nav: false
               },
               768: {
                  nav: true
               },
            }
      };
   }
   function owl_carousel_bites_config() {
      return {
            items: 1,
            nav: true,
            responsiveClass: true,
            responsive: {
               0: {
                  autoHeight: true,
               },
               600: {
                  slideBy: 2,
                  items: 2,
               },
            }
      };
   }
   function owl_home_columnists_config(){
      return {
            items: 2,
            nav: true,
            responsiveClass: true,
            responsive: {
               0: {
                  items: 1,
               },
               600: {
                  slideBy: 2,
                  items: 2,
                  dots: true
               },
            }
      };
   }


   var carousel_actual = $('#carousel_actual');
   carousel_actual.owlCarousel(carousel_actual_config());
   carousel_actual.on('click','.owl-dot', function(){
      carousel_actual.trigger('play.owl.autoplay', [4000]);
   });
   
   if (checkWidth <= 1023) {
      owl_carousel_bites = $('#bite_entries');
      owl_carousel_bites.addClass('owl-carousel').owlCarousel(owl_carousel_bites_config());
   }

   window.onresize = function(){

      owl_carousel_bites = $('#bite_entries');
      checkWidth = jQuery(window).width();
      if (checkWidth > 1023)
            owl_carousel_bites.trigger('destroy.owl.carousel').removeClass('owl-carousel');
      else
            owl_carousel_bites.addClass('owl-carousel').owlCarousel(owl_carousel_bites_config());

   };

   owl_home_columnists = $('#home_columnists_actual');
   owl_home_columnists.owlCarousel(owl_home_columnists_config());

</script>
</body>
</html>
