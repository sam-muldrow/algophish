<script>
(function ($) {
        var publicationScreentip;
	var getPublication = $("#sports_article_credits").text();
	var publication = ["The Philippine Star", "Pilipino Star Ngayon", "The Freeman", "Pang-masa", "Banat"];
		for (var i = 0; i < publication.length; i++) {
			if(getPublication.indexOf(publication[i]) != -1) {
				publicationScreentip = publication[i];
			}
	}	
    function replaceDisclaimer(){
        return ''+publicationScreentip+' <span id="pub-dis" ><span class="dis-btn"><img src="https://media.philstar.com/assets/icons/info-icon.webp"></span><div class="dis-tooltip">This content was originally published by <a href="https://www.philstar.com/'+publicationScreentip.replace(/\s+/g, '-').toLowerCase()+'/about-us">'+publicationScreentip+'</a> following its editorial guidelines. <b>Philstar.com</b> hosts its content but has no editorial control over it.</div></span>';
    }	
    if ($('#sports_article_credits').length  && publicationScreentip){
        var sports_article_credits = $('#sports_article_credits');		
        sports_article_credits.html(sports_article_credits.html().replace(publicationScreentip, replaceDisclaimer()));
        if ($(window).width() <= 1024) {
            sports_article_credits.on('click','.dis-btn',function(){
                dis_tooltip = sports_article_credits.find('.dis-tooltip');
                if (!dis_tooltip.hasClass('m-show')){
                    tl = $(window).width() / 2 >= $(this).offset().left ?  "tl-left" : "";
                    dis_tooltip.addClass('m-show '+ tl);
                }else{
                    dis_tooltip.removeClass('m-show');
                }
            }).on('click', '.dis-tooltip',function(){
                $(this).removeClass('m-show');
            });

        }
    }
	
})(jQuery);
</script>
<style>#pub-dis {position: relative;}.dis-btn {display: inline-block;vertical-align: top;height: 17px;padding: 0 2px;}.dis-tooltip {visibility: hidden;position: absolute;background-color: lightyellow;border: 1px solid gray;padding: 3px;font-size: 12px;width: 200px;z-index: 10;right: 50%;top: 100%;}.dis-tooltip.m-show.tl-left {right: auto;}#pub-dis .dis-tooltip.m-show {visibility: visible;}.dis-btn img {height: 17px;cursor: pointer;opacity: .7;}@media only screen and (min-width:600px) {#pub-dis:hover .dis-tooltip {visibility: visible;top: 50%;left: 50%;right: initial;}}</style>

<script type="text/javascript" id="script__newsletter-signup">
    // shows in homepage, article and pages if allowed_urls is empty
    var allowed_urls = [
        // "homepage", "headlines","sports",
    ];
    var excluded_urls = [
        "events",
        "newsletter",
        "elections-2022"
    ];

    var pathArray = window.location.pathname.split('/');
    var inPublication = pathArray[1];

    // sign up functions
    if (getCookie('newsletter_hide') == null || getCookie('newsletter_hide') == 0) {
        // newsletter_show();
    }

    function newsletter_create(elem, tag) {
        // create link tag		
        scriptTag = document.getElementById("script__newsletter-signup");
        var linkTag = document.createElement("link");
        linkTag.setAttribute("rel", "stylesheet");
        linkTag.setAttribute("href", "https://media.philstar.com/assets/newsletter/signup/newsletter_signup_v2.css");
        // linkTag.setAttribute("href", "https://dev.philstar.com/css/newsletter_signup_v2.css");
        scriptTag.parentNode.insertBefore(linkTag, scriptTag);

        var newsletterSignUpWrap = document.createElement("div");
        newsletterSignUpWrap.setAttribute("class", "newsletter-sign-up-wrap " + tag);

        var newsletterSignupForm = document.createElement("div");
        newsletterSignupForm.setAttribute("id", "newsletter-signup-form");
        newsletterSignupForm.innerHTML = '<div class="newsletter-signup-form_container">\
            <h2>Relevant. <span class="br"></span>Witty. Daily</h2>\
            <p class="newsletter-signup-form_txt">Morning news briefings <span class="br"></span>from Philstar.com</p>\
            <input type="text" id="newsletter-signup-form_email" name="email" placeholder="Your Email Address"></input>\
            <button type="submit" id="newsletter-signup-form_signup" name="signup">SIGN-UP</button>\
        </div>';

        newsletterSignUpWrap.appendChild(newsletterSignupForm);
        if (elem.length > 0)
            $(newsletterSignUpWrap).insertAfter(elem);

        document.getElementById("newsletter-signup-form_signup").addEventListener('click', function (e) {
            sign_up_now();
        });
        document.getElementById("newsletter-signup-form_email").addEventListener('click', function (e) {
            this.value = "";
        });

    }

    function sign_up_now() {
        email = document.getElementById("newsletter-signup-form_email").value;
        if (email && validateEmail(email)) {

            var providers = ['list', 'sendy'];
            var xmlhttp = new XMLHttpRequest();

            (function loop(i, length) {

                if (i >= length) return;

                var url = 'https://www.philstar.com/newsletter_signup_widget.php';
                xmlhttp.open("POST", url, true);
                xmlhttp.onreadystatechange = function () {
                    if (xmlhttp.readyState === XMLHttpRequest.DONE && xmlhttp.status === 200) {
                        var data = xmlhttp.responseText;
                        console.log(data);
                        msg_newsletter("Thank you for signing up!", "success");
                        loop(i + 1, length);
                    }
                };
                xmlhttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
                xmlhttp.send('email=' + email + '&provider=' + providers[i]);

            })(0, providers.length);

        } else {
            msg_newsletter("Please enter valid email address.", "error");
        }
    }

    function msg_newsletter(msg, cla = "") {
        signupMsg = $(".signup-msg");
        if (signupMsg.length)
            signupMsg.text(msg).attr("class", "signup-msg " + cla);
        else
            $('<div class="signup-msg ' + cla + '">' + msg + '</div>').insertAfter($("input#newsletter-signup-form_email"));

    }

    function close_newsletter() {
        var nsf = document.getElementById("newsletter-signup-form");
        nsf.classList.add("slideOutDown");
        nsf.classList.remove("slideInUp");
        console.log("no thanks click");
        return false;
    }
    function validateEmail(email) {
        var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
        return re.test(String(email).toLowerCase());
    }

    function setCookie(name, value, days) {
        var expires = "";
        if (days) {
            var date = new Date();
            date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
            expires = "; expires=" + date.toUTCString();
        }
        document.cookie = name + "=" + (value || "") + expires + ";path=/";
    }
    function getCookie(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') c = c.substring(1, c.length);
            if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
        }
        return null;
    }
    function eraseCookie(name) {
        document.cookie = name + '=; Max-Age=-99999999;';
    }
    function filter_url() {
        p = false;
        cur_url = window.location.pathname;
        "/" == cur_url && (cur_url += "homepage");

        if (allowed_urls.length)
            $.each(allowed_urls, function (key, allowed) {
                if (cur_url.search("/" + allowed) >= 0) {
                    p = true;
                    return false;
                }

            });
        else
            p = true;

        if (excluded_urls.length)
            $.each(excluded_urls, function (key, excluded) {
                if (cur_url.search("/" + excluded) >= 0) {
                    p = false;
                    return false;
                }

            });

        return p;
    }

    // create newsletter
    classNewsleter = "";
    //regular page
    if ($("#sidebar .recommended_cell").length > 0)
        insertNewsleter = $("#sidebar .recommended_cell").eq(0), classNewsleter = "homepage";
	//lifestyle homepage
    if ($("#section__newsletter .square-ad").length > 0)
        insertNewsleter = $("#section__newsletter .square-ad"), classNewsleter = "lifestyle";
    //regular article
    if ($("#article_tags").length > 0)
        insertNewsleter = $("#article_tags"), classNewsleter = "article";
    //as it happen article
    else if ($("#sports_article_writeup").length > 0)
        insertNewsleter = $("#sports_article_writeup"), classNewsleter = "article";
    //microsite page
    else if ($("#right-sidebar-mrec-ad").length > 0)
        insertNewsleter = $("#right-sidebar-mrec-ad");
    // create newsletter
    if (filter_url()  && classNewsleter)
        newsletter_create(insertNewsleter, classNewsleter);

</script>
