<div id="lotto_title">Lotto <span class="lotto_date">Nov 20, 2023</span></div>
<div id="lotto_results_holder">
<p>EZ2/LVM - 28 4</p>
<p>SUERTRES - 1 1 2</p>
<p>4D LOTTO - 8 5 9 5</p>
<p>6/45 Mega Lotto - 6 31 32 27 44 8</p>
<p>P26,404,316.00</p>
<p>Grand Lotto - 38 17 27 33 42 48</p>
<p>P29,700,000.00</p>
</div>
<div id="lotto_past">
<a href="/lotto-results">More Lotto Results</a>
</div>
<link rel="stylesheet" type="text/css" href="https://www.philstar.com/css/slick.min-12222020.css" />
<script type="text/javascript" src="https://www.philstar.com/js/slick.min.js"></script>
<script type="text/javascript">
(function($){
	fmt = {
		sep : ' - ', 
		del : ['(In exact order)'],
		cur : [', ', ' ', ','],
	}
	function split_cur(str){
		spl = "";
		$.each(fmt.cur, function(key, val){
			if (str.search(val) > 0 ){
				spl = val;
				return false;
			}
		});
		return str.split(spl);
	}
	function sanitize_name(str){
		$.each(fmt.del, function(key, val){
			str = str.replace(val, "");
		});
		return str;
	}
	function sanitize_number(str){
		return split_cur(str);
	}
	function sanitize_price(str){
		return str.replace("P", "").trim();		
	}
	function isStr(str){
		r = "int";
		arStr = split_cur(str);
		$.each(arStr, function(key, val){
			if ( isNaN( parseInt(val) ) ){
				r = "str";
				return false;
			}
		});
		if (str.search(".00") > 0){
			r = "amt";
		}
		return r;
	}
	function display_lotto(lotDat){
		str = '<div class="widget_lotto_wrap">';
		lotDat.reverse();
		console.log(lotDat);
		$.each(lotDat, function(key, attr){
			lotNum = '';
			$.each(attr.number, function(k, v){
				lotNum += '<span class="num">'+v+'</span>';
			});
			if (attr.price == "NaN"){
				lotPri = '';
			}else{
				lotPri = '<div class="lotto-price"><span class="jp">JACKPOT PRIZE: </span>'+attr.price+' PHP</div>';
			}
			str += '<div class="item item-'+key+'">\
						<div class="lotto-name">'+attr.name+'</div>\
						<div class="lotto-number">'+lotNum+'</div>\
						'+lotPri+'\
				   </div>';
		});
		str += '</div>';
		return str;
	}
	function init_adjust_format(lrh){		
		b = "";
		lrh.find("p").each(function(key){
			a = $(this).html().replace(/<br>\n\t<br>/gi,'</p><p>');
			b += "<p>"+a+"</p>";
		});
		lrh.html(b);
	}
	function lotto_detect_format(lrh){
		lotNam = "";
		lotDat = [];
		lrh.find("p").each(function(key){
			thiTex = $(this).html();
			arrLot = thiTex.split( fmt.sep );
			lnam = "";
			lnum = "";
			lpri = "NaN";
			if (arrLot.length == 1){
				if (isStr(arrLot[0]) == "amt"){
					l = lotDat.length - 1;
					lotDat[l].price = sanitize_price(arrLot[0]);
				}else{
					lotNam = sanitize_name(arrLot[0]);
					lotNam = lotNam.trim() + " ";
				}
			}else{
				if ( isStr(arrLot[0]) == "str" ){
					lotNam = "";
				}
				$.each(arrLot, function(key, val){
					arrLot[key] = val.trim();
					if ( isStr(arrLot[key]) == "str" ){
						lnam = arrLot[key];
					}else{
						lnum = arrLot[key];
						if (arrLot[key].search("<br>") > 0){
							spli_lnum = arrLot[key].split("<br>");
							if (spli_lnum.length > 1){
								lnum = spli_lnum[0];
								if (isStr(spli_lnum[1]) == "amt"){
									lpri = sanitize_price(spli_lnum[1]);
								}
							}
						}
					}
				});				
				lotDat.push({
					name : lotNam + lnam,
					number : sanitize_number(lnum),
					price : lpri,
				});
			}
		});
		lrh.show().empty().html(display_lotto(lotDat));
		$(".widget_lotto_wrap").slick({
		  dots: false,
		  infinite: false,
		  speed: 200,
		  fade : true,
		  slidesToShow: 1,
		  adaptiveHeight: false,
		})
	}
	lrh = $("div#lotto_results_holder");
	if (lrh.length > 0 ){
		console.log("found");
		init_adjust_format(lrh);
		lotto_detect_format(lrh);
	}
})(jQuery);
</script>
</div>
