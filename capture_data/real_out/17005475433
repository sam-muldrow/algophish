    var cl = document.getElementById("carousel_light");
    if (cl && window.innerWidth >= 768){
        cih = cl.querySelector(".carousel__items").firstElementChild;
        cih_titleH = cih.querySelector(".carousel__item__title").offsetHeight;
        cih_author = cih.querySelector(".carousel__item__author");
        cih_summary = cih.querySelector(".carousel__item__summary");
        if (cih_titleH == 40 && cih_author){
            cih_summary.style['-webkit-line-clamp'] = 2;
        }else if (cih_titleH == 40 && !cih_author){
            cih_summary.style['-webkit-line-clamp'] = 3;
        }else if (cih_titleH == 80 ){
            cih_summary.style['-webkit-line-clamp'] = 1;
        }else if (cih_titleH == 120){
            cih_summary.style.display = 'none';
            cih_author.style.display = 'none';
        }
    }  
</script>
