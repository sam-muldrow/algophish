    <!--Scripts: PagePing-->
<script>
  window.addEventListener('load', (event) => {
      console.log(event);
    const isoStr = new Date().toISOString();
    var appname =
      document.querySelector('meta[name="application-name"]')?.content || '';

    var evt = new URL('https://evt.24.com/v1/collect');
    if (appname.toLowerCase().endsWith('dev'))
    {
        evt = new URL('https://dev-evt.24.com/v1/collect');
    }

    evt.searchParams.append('eventType', 'events');
    evt.searchParams.append('eventName', 'page_ping');
    evt.searchParams.append('eventDate', isoStr);
    evt.searchParams.append('pagetitle', document.title);
    var isbrave =
      (navigator.brave && navigator.brave.isBrave() != undefined) || false;
    var tfuid = ('; ' + document.cookie).split(`; 24uid=`).pop().split(';')[0];
    var platform = document.querySelector('meta[name="platform"]')?.content || '';
    var breadcrumb = document.querySelector('meta[name="breadcrumb"]')?.content || '';
    var usertype = document.querySelector('meta[name="usertype"]')?.content || '';
    var permatitle = document.querySelector('meta[name="permatitle"]')?.content || '';
    var access = document.querySelector('meta[name="access"]')?.content || '';
    var articleState = document.querySelector('meta[name="locked"]')?.content || '';
    var articleid = document.querySelector('meta[name="articleid"]')?.content || '';
    var pageviewtype = document.querySelector('meta[name="pageviewtype"]')?.content || '';
    var isbreakingnews = document.querySelector('meta[name="isbreakingnews"]')?.content || '';
    var isexclusive = document.querySelector('meta[name="isexclusive"]')?.content || '';
    var url = window.location.href;

    var userAgent = navigator.userAgent;
    articleState = articleState == 'true' ? 'locked' : 'unlocked';
    articleid =
      articleid == '00000000-0000-0000-0000-000000000000' ? '' : articleid;
    articleState = articleid === '' ? '' : articleState;
    var tfuat = ('; ' + document.cookie).split(`; 24uat=`).pop().split(';')[0];
    if (tfuat !== '') {
      var base64Url = tfuat.split('.')[1];
      var base64 = base64Url.replace(/-/g, '+').replace(/_/g, '/');
      var jsonPayload = decodeURIComponent(
        window
          .atob(base64)
          .split('')
          .map(function (c) {
            return '%' + ('00' + c.charCodeAt(0).toString(16)).slice(-2);
          })
          .join('')
      );
      var model = JSON.parse(jsonPayload);
      tfuat = model.user_id;
    }
    if (isbrave !== '') {
      evt.searchParams.append('isBrave', isbrave);
    }
    if (platform !== '') {
      evt.searchParams.append('platform', platform);
    }
    if (tfuid !== '') {
      evt.searchParams.append('24uid', tfuid);
    }
    if (tfuat !== '') {
      evt.searchParams.append('firebaseId', tfuat);
    }
    if (appname !== '') {
      evt.searchParams.append('appName', appname);
    }
    if (breadcrumb !== '') {
      evt.searchParams.append('breadcrumb', breadcrumb);
    }
    if (usertype !== '') {
      evt.searchParams.append('userState', usertype.toLowerCase());
    }
    if (articleState !== '') {
      evt.searchParams.append('articleState', articleState);
    }
    if (permatitle !== '') {
      evt.searchParams.append('permatitle', permatitle);
    }
    if (access !== '') {
      evt.searchParams.append('access', access);
    }
    if (articleid !== '') {
      evt.searchParams.append('articleid', articleid);
    }
    if (url !== '') {
      evt.searchParams.append('url', url);
    }
    if (pageviewtype !== '') {
      evt.searchParams.append('source', pageviewtype);
    }
    if (userAgent !== '') {
      evt.searchParams.append('userAgent', userAgent);
    }
    if (isbreakingnews !== '') {
        evt.searchParams.append('IsBreakingNews', isbreakingnews);
    }
    if (isexclusive !== '') {
        evt.searchParams.append('IsExclusive', isexclusive);
    }

    setTimeout(function () {
      fetch('https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js', {
        method: 'HEAD',
        mode: 'no-cors',
        cache: 'no-store',
      })
        .then(() => {
          evt.searchParams.append('adblock', false);
          var googleId = typeof ga != "undefined" ? ga.getAll()[0].get('clientId') : '';
          var googleId = ('; ' + document.cookie).split(`; _ga=`).pop().split(';')[0];
            if (googleId !== '') {
                googleId=googleId.substring(6)
            }
          if (googleId !== '') {
            evt.searchParams.append('googleId', googleId);
          }
          var downloadingImage = new Image();
          downloadingImage.src = evt.href;
        })
        .catch(() => {
          evt.searchParams.append('adblock', true);
          var downloadingImage = new Image();
          downloadingImage.src = evt.href;
        });
    }, 2000);
  });
</script>
    

