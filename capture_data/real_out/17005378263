<div class="new-gallery-slider-holder">
<script>
        function debounce(func, wait, immediate) {
            var timeout;
            return function() {
                var context = this, args = arguments;
                var later = function() {
                    timeout = null;
                    if (!immediate) func.apply(context, args);
                };
                var callNow = immediate && !timeout;
                clearTimeout(timeout);
                timeout = setTimeout(later, wait);
                if (callNow) func.apply(context, args);
            };
        };


        $(document).ready(function() {
            window.gallery_slider = {
                totalItemsCount: 36,
                filteredFor: null
            };
            $('.new-gallery-slider').slick({
                infinite: false,
                slidesPerRow: 4,
                rows: 3,
                responsive: [
                    {
                        breakpoint: 1340,
                        settings: {
                            infinite: false,
                            slidesPerRow: 3,
                            rows: 3
                        }
                    },
                    {
                        breakpoint: 766,
                        settings: {
                            infinite: false,
                            slidesPerRow: 1,
                            rows: 2
                        }
                    }
                ]
            });


            var filterSlides = debounce(function () {
                var pagesToShow = parseInt(window.gallery_slider.totalItemsCount / 12);
                if (window.viewport().width <= 1340) {
                    pagesToShow = parseInt(window.gallery_slider.totalItemsCount / 9);
                }
                if (window.viewport().width <= 766) {
                    pagesToShow = parseInt(window.gallery_slider.totalItemsCount / 2);
                }
                if (window.gallery_slider.filteredFor != pagesToShow) {
                    console.log("Refilter gallery slider");
                    $('.new-gallery-slider').slick('slickUnfilter');
                    $('.new-gallery-slider').slick('slickFilter', function(pageIndex){ return (pageIndex < pagesToShow); });
                    window.gallery_slider.filteredFor = pagesToShow;
                }
            }, 200);

            $('.new-gallery-slider').on('beforeChange', function(slick) { filterSlides(); });
        });
    </script>
