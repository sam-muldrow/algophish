    (function($, undefined){
        var poll_id = $('#answer_poll').data('poll-id');
        var poll_text = $('#answer_poll').data('poll-text');
        if (isVotedByText(poll_text) === true){
            $.ajax({
                url: '/get-poll/',
                method: 'post',
                dataType: 'html'
            }).done(function (data, textStatus, jqXHR) {
                $('#poll-index-chart').html(data);
            });
            $('#poll-index-chart').removeClass('hidden');
        } else {
            $('#poll-index-chart').addClass('hidden');
            $('#poll-index-form').removeClass('hidden');
        }
    })(jQuery);
