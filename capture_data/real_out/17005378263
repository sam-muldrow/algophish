<script>
    
    $(document).ready(function(){
        $('.main-article-slider').slick({
            slide: '.main-article-slider-item',
            waitForAnimate: true,
            autoplay: true,
            autoplaySpeed: 5000,
            dots: true
        });

        $('.main-article-slider').on('beforeChange', function(event, slick, currentSlide, nextSlide){
            window.main_article_slider_is_sliding = true;
            $('.video.index-video').css('pointer-events', 'none');
        });

        $('.main-article-slider').on('afterChange', function(event, slick, currentSlide){
            window.main_article_slider_is_sliding = false;
            $('.video.index-video').css('pointer-events', '')
        });
        $('.main-article-slider').on('swipe', function(event, slick) {
            $('.video.index-video video').each(function () {
                $(this).get(0).pause();
            });
        });

    });
</script>
