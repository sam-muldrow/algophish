(function() {
var containers = $all(".widget--tournament-stats-condensed");
function init() {
containers.forEach(function(container, i) {
trackImpressionGAEventWhenInViewport({
element: container,
identifier: "widget_tournament_stats_condensed__" + i,
eventName: "WIDGET_STATS_CONDENSED_IMPRESSION"
});
});
}
