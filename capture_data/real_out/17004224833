<script>
(function() {
var containers = $all(".widget--taxonomy-social");
function init() {
containers.forEach(function(container, i) {
trackImpressionGAEventWhenInViewport({
element: container,
identifier: "widget_taxonomy_social__" + i,
eventName: "WIDGET_TAX_SOCIAL_IMPRESSION"
});
});
}
window.addEventListener("DOMContentLoaded", function() {
init();
});
})();
