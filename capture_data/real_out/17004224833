}
.keeda-data-table-container table.keeda-data-table th:last-child,
.keeda-data-table-container table.keeda-data-table td:last-child {
padding-right: var(--containerXSpacing);
}
.keeda-data-table-container table.keeda-data-table tbody tr:hover td {
background-color: #ECECFE;
}
</style>
<script>
(function() {
function onColumnCTAClicked(e) {
var target = e.target.closest("a.column-cta");
if (!target) return;
var destinationUrl = target.getAttribute("href");
if (!destinationUrl) return;
var label = target.innerText;
trackGAEvent("COLUMN_CTA_CLICKED", {
section: "stats_table",
destination_url: destinationUrl,
label: label
});
}
function initTable(tableContainer) {
var shouldAutoScroll = tableContainer.dataset["autoScroll"] == "true";
var shouldSmoothAutoScroll = tableContainer.dataset["smoothAutoScroll"] == "true";
if (!shouldAutoScroll && !shouldSmoothAutoScroll) return;
if (!tableContainer.scrollTo);
var tableContainerBox = tableContainer.getBoundingClientRect();
if (!tableContainerBox) return;
var selectedTH = tableContainer.querySelector("th.selected");
if (!selectedTH) return;
if (!selectedTH.offsetLeft) return;
var selectedTHBox = selectedTH.getBoundingClientRect();
if (!selectedTHBox) return;
var middlePoint = ((tableContainerBox.width - selectedTHBox.width) / 2);
var targetX = selectedTH.offsetLeft - middlePoint;
var params = {
top: 0,
left: targetX,
}
if (shouldSmoothAutoScroll) {
params["behavior"] = "smooth";
}
tableContainer.scrollBy(params);
}
function initAnalytics(tableContainer) {
var tableHeader = tableContainer.querySelector("thead");
if (tableHeader) {
tableHeader.querySelectorAll("a.column-cta").forEach(function(ctaAnchor) {
ctaAnchor.addEventListener("click", onColumnCTAClicked);
});
}
}
function init() {
var tableContainers = $all(".keeda-data-table-container");
tableContainers.forEach(function(tableContainer) {
initTable(tableContainer);
initAnalytics(tableContainer);
});
}
window.addEventListener("DOMContentLoaded", init);
})();
</script>
