<script>
(function() {
function onSmartMenuItemClicked(e) {
var target = e.target.closest(".smart-menu-cta");
var menuName = target.innerText;
var destinationUrl = target.getAttribute("href");
var sequence = target.dataset["sequence"];
var section = target.dataset["section"];
trackGAEvent("SMART_MENU_ITEM_CLICK", {
destination_url: destinationUrl,
menu_name: menuName,
sequence: sequence,
section: section
});
}
function init() {
$all(".smart-menu-container").forEach(function(container, i) {
trackImpressionGAEventWhenInViewport({
element: container,
identifier: "smart_menu_container__" + i,
eventName: "SMART_MENU_IMPRESSION"
});
container.querySelectorAll(".smart-menu-cta").forEach(function(ctaItem) {
ctaItem.addEventListener("click", onSmartMenuItemClicked);
})
})
}
window.addEventListener("DOMContentLoaded", function() {
init();
})
})();
</script>
</section>
