<img width="35" height="35" alt="Akshar Patel" src="data:image/svg+xml;charset=utf-8,<svg width='100' height='100' xmlns='http://www.w3.org/2000/svg'><rect width='100' height='100' style='fill:rgb(128, 128, 128);'/></svg>" data-lazy="https://staticg.sportskeeda.com/editor/2023/02/29c8f-16760099800693-1920.jpg?h=70" onerror="this.src='//staticg.sportskeeda.com/football_images/shield.png'" />
</div>
<div class>
<div class="player-team-name">
<a class="player-team-name-primary" href="/player/axar-patel">Akshar Patel</a>
<div class="player-team-name-secondary">IND</div>
</div>
</div>
</div>
<div class="player-team-rating">298</div>
</div>
</div>
</div>
</div>
</div>
<script>
(function() {
var container = $(".icc-ranking-container");
var contentWrapper = container && container.querySelector(".ranking-content-wrapper");
var selectedFormat = "t20";
var selectedKind = "team";
function onTabChange() {
var selectedFormatRadio = container.querySelector('input[type="radio"][name="icc_ranking_format"]:checked');
var selectedKindRadio = container.querySelector('input[type="radio"][name="icc_ranking_kind"]:checked');
var currentSelectedFormat = selectedFormatRadio.value;
var currentSelectedKind = selectedKindRadio.value;
if (currentSelectedFormat && currentSelectedKind) {
contentWrapper.querySelectorAll(".ranking-content").forEach(function(contentContainer) {
var format = contentContainer.dataset["contentFormat"];
var kind = contentContainer.dataset["contentKind"];
if (format == currentSelectedFormat && kind == currentSelectedKind) {
removeClass(contentContainer, "hidden");
} else {
addClass(contentContainer, "hidden");
}
});
}
if (selectedFormat != currentSelectedFormat || selectedKind != currentSelectedKind) {
trackGAEvent("ICC_RANKING_TAB_CHANGE", {
menu_name: String(currentSelectedFormat).toLowerCase(),
sub_menu_name: String(currentSelectedKind).toLowerCase(),
});
if (typeof lazyLoadAll == "function") {
lazyLoadAll();
}
}
selectedFormat = currentSelectedFormat;
selectedKind = currentSelectedKind;
}
function onItemClick(e) {
var target = e.target.closest("a.player-team-name-primary");
var itemName = target.innerText;
var destinationUrl = target.getAttribute("href");
trackGAEvent("ICC_RANKING_ITEM_CLICK", {
destination_url: destinationUrl,
item_name: itemName,
item_type: selectedKind == "team" ? "team" : "player"
});
}
window.addEventListener("DOMContentLoaded", function() {
if (!container) return;
if (!contentWrapper) return;
trackImpressionGAEventWhenInViewport({
element: container,
identifier: "widget_icc_rankings",
eventName: "WIDGET_ICC_RANKINGS_IMPRESSION"
});
var radios = container.querySelectorAll('input[type="radio"]');
radios.forEach(function(radio) {
radio.addEventListener("change", onTabChange);
});
var anchors = contentWrapper.querySelectorAll("a.player-team-name-primary");
anchors.forEach(function(anchor) {
anchor.addEventListener("click", onItemClick);
});
});
})();
</script>
</div>
</div>
</section>
<div class="section-separator "></div>
