<script>
(function() {
var userCountry = getCookie("country_code");
var container = $(".featured-tools-holder");
function onToolClick(e) {
var cta = e.target.closest(".tool-cta");
var toolName = cta.dataset["toolName"];
var destinationURL = cta.getAttribute("href");
trackGAEvent("Tools_Widget_Clicks", {
tool: toolName,
destinationURL: destinationURL
});
}
window.addEventListener("DOMContentLoaded", function() {
if (!container) return;
container.querySelectorAll("a.tool-cta").forEach(function(featuredTool) {
featuredTool.addEventListener("click", onToolClick);
});
});
})();
</script>
