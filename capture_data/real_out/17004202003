        promosSliderSettings = {
            indexSlide:1,
            infinite: false,
            arrows: true,
            dots: false,
            speed: 300,
            slidesToShow: 3,
            slidesToScroll: 3,
            prevArrow: $(".promos-container .arr-btn-prev"),
            nextArrow: $(".promos-container .arr-btn-next"),
            adaptiveHeight: false,
            responsive: [
                {
                    breakpoint: 1500,
                    settings: {
                        slidesToShow: 3,
                        slidesToScroll: 1,
                        infinite: false,
                        dots: false,
                        rows:1,
                        slidesPerRow:1,
