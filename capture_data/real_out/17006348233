     <script>

         $(function () {
             $(window).scroll(function () {

                 if ($(window).scrollTop() > 300) {  //距顶部多少像素时，出现返回顶部按钮
                     $("#side-bar .text").fadeIn();
                 }
                 else {
                     $("#side-bar .text").hide();
                 }
             });
             $("#side-bar .gotop").click(function () {
                 $('html,body').animate({ 'scrollTop': 0 }, 500); //返回顶部动画 数值越小时间越短
             });
         });
         $(".side-bar li div .form-group .zhandian").hover(function () {
             $(".zhandianshili").css("display", "block");
         }, function () {
             $(".zhandianshili").css("display", "none");
         });

         $(".side-bar li div .question").click(function () {
             $(".xufei").css("display", "block");
         });
         $(".xufei .close").click(function () {
             $(".xufei").css("display", "none");
         });
         $(".xufei .btn").click(function () {
             $(".xufei").css("display", "none");
         });

         window.requestAnimFrame = (function () {
             return window.requestAnimationFrame ||
                 window.webkitRequestAnimationFrame ||
                 window.mozRequestAnimationFrame ||
                 function (callback) {
                     window.setTimeout(callback, 1000 / 60);
                 };
         })();

         //showRightMenu();



         function showRightMenu() {
             if ($("#LRdiv2").css("display") == "block" && $("#LRMINIBar").css("display") == "none") {
                 $("#side-bar").fadeOut();
             }
             else {
                 $("#side-bar").fadeIn();
             }

             window.requestAnimFrame(showRightMenu);
         }
         function _OpenLRChat(msg) {
             if (typeof (LR_explain) != undefined) {
                 LR_explain = "访客点击官网浮动图标：" + msg;
             };
             if (typeof (LR_msg) != undefined) {
                 LR_msg = "";
             }
             openZoosUrl('chat', '');
         };
         function openPage(page) {

         }

         $(document).ready(function () {
             $(".showmodel").show();
             var nT = 10;
             var time = setInterval(function () {
                 nT--;
                 if (nT == 0) {
                     $(".showmodel").hide();
                     clearInterval(time);
                 }
                 $("#txtModel").html("跳过" + nT + "s");
             }, 1000);

             $(document).on("click", "#btnModel", function () {
                 $(".showmodel").hide();
                 if (time != null) {
                     clearInterval(time);
                 }
             });
  InitTanChuangClose();
         });

     var InitTanChuangClose = function () {
             $(document).on("click", ".tangchuangconclose", function () {
                 $(".tangchuangcon").hide();
                 $(".tangchuangconclose").hide();
             });
         };

    </script>
