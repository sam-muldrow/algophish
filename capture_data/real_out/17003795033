        $(function () {
            function show_event() {
                $('.activity-app-layer__close').parent().removeClass('fn-hide');
                trackCustomEvent('auto_common_event', { 'biz': 'auto', 'type': 'show', 'action': 'pc_first_page_bottom_banner_mini_show', 'ctime': new Date().getTime(), 'area': 'bottom', 'element': 'details', 'pmark': '0' });
            }
            $('.activity-app-layer__close').on('click', function () {
                if (window.localStorage) {
                    window.localStorage.setItem('index_app_acti', new Date().getDate());
                }
                $(this).parent().addClass('fn-hide');
                trackCustomEvent('auto_common_event', { 'biz': 'auto', 'type': 'click', 'action': 'pc_first_page_bottom_banner_mini_click', 'ctime': new Date().getTime(), 'area': 'bottom ', 'element': 'details', 'pmark': 0 });
            });
            if (window.localStorage) {
                var __actiValue = window.localStorage.getItem('index_app_acti');
                if (__actiValue == null || __actiValue != new Date().getDate()) {
                    show_event();
                }
            } else {
                show_event();
            }
        });</script>
