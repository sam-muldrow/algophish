        Auto.getCookie = function (name, defaultValue) { var re = new RegExp(name + "=([^;]*);?", "gi"); var v = typeof defaultValue == "undefined" ? null : defaultValue; var r = re.exec(document.cookie) || []; return r.length > 1 ? unescape(r[1]) : v }
        Auto.setCookie = function (name, value, option) { var str = name + '=' + encodeURIComponent(value); if (option) { if (option.expireHours) { var d = new Date(); d.setTime(d.getTime() + option.expireHours * 3600 * 1000); str += '; expires=' + d.toGMTString() }; if (option.path) str += '; path=' + option.path; else str += '; path=/'; if (option.domain) str += '; domain=' + option.domain; if (option.secure) str += '; true' }; document.cookie = str }

        Auto.getCityIdByCookie = function () {
            var unExpInfoArr = { "1102": "1101", "1202": "1201", "3102": "3101", "5002": "5001" };
            var id = Auto.getCookie("cookieCityId", "0");
            id = id == "0" ? Auto.getCookie("area", "0") : id;

            id = id.toString();
            if (id == undefined || id == "undefined" || id.length != 6) {
                return 0;
            }

            var pId = id.substr(0, 2);
            var cId = id.substr(2, 2);
            if (pId == "99" || cId == "99" || cId == "00") {
                return 0;
            };
            if (cId == "90") {
                return parseInt(id, 10);
            };
            var _tmpId = id.substr(0, 3);
            if (_tmpId == '659') {
                return parseInt(_tmpId + '000', 10);
            }
            _tmpId = id.substr(0, 4);
            if (unExpInfoArr[_tmpId]) {
                _tmpId = unExpInfoArr[_tmpId];
            };

            return parseInt(_tmpId + "00", 10);
        }
        Auto.findIdByName = function (name, json) {
            try {
                for (var i = 0; i < json.length; i++) {
                    for (var j = 0; j < json[i].City.length; j++) {
                        if (json[i].City[j].Pinyin == name) {
                            return json[i].City[j].Id;
                        }
                    }
                }
            } catch (err) {
                return 0;
            }
            return 0;
        }
        Auto.findNameById = function (id, json, defaultValue) {
            if (!id) return defaultValue;
            try {
                for (var i = 0; i < json.length; i++) {
                    for (var j = 0; j < json[i].City.length; j++) {
                        if (json[i].City[j].Id == id) {
                            return json[i].City[j].Pinyin;
                        }
                    }
                }
            } catch (err1) {
                return defaultValue;
            }
            return defaultValue;
        }
