        var isGuideShow = !autoHeaderObj.user.isLogin;
        $(window).scroll(function () {
            var guide = $(window).scrollTop();
            if (guide > 300) {
                if (isGuideShow) {
                    $('#loginGuide').removeClass('athm-hide');
                }
            } else {
                $('#loginGuide').addClass('athm-hide');
            }
        })
