    <script type="text/javascript">
        if (typeof window._aad === 'undefined') {
            _aad = new Object();
            _aad.setData = function () { };
            _aad.Loader = function () { };
        }
        _aad.setData({ filter: 1 });
        var provinceid = Math.floor(Auto.getCookie("cookieCityId", Auto.getCookie("area", 0)) / 10000) * 10000;
        if (adCityId) {
            _aad.setData({ city: adCityId, province: provinceid });
        }
        else {
            _aad.setData({ city: indexCityId, province: provinceid });
        }

        //天幕、主打车
        _aad.Loader('4667,4615,4612,1075,1076,1077,1078,1079,1080,1081,1361,1362,1363,1364,5470,5471,5473,5475,5476,5477,5478,5479,5481,6682', '142');

