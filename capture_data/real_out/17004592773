<input type="hidden" name="pl_latitude" id='pl_latitude'>
<input type="hidden" name="pl_longitude" id='pl_longitude'>
<script>
    $(document).ready(function (){
                    initMap1();

        $(window).scroll(function() {
            if ($(this).scrollTop() > 10) {
              $(".showloccity").text(getCookie('plotch_city'));
            } else {
              $(".showloccity").text(getCookie('plotch_streetcity'));
            }
          });
            });
    
    var map, infoWindow;
    function initMap1() {
        if(getCookie('plotch_latitude') !='' && getCookie('plotch_longitude')!=''){
            $('#pl_latitude').val(getCookie('plotch_latitude'));
            $('#pl_longitude').val(getCookie('plotch_longitude'));
            var latitude = parseFloat($('#pl_latitude').val()); // YOUR LATITUDE VALUE
            var longitude = parseFloat($('#pl_longitude').val()); // YOUR LONGITUDE VALUE
        }else{
            var latitude = 21.1458; // YOUR LATITUDE VALUE
            var longitude = 79.0882; // YOUR LONGITUDE VALUE 
        }
        
        var myLatLng = {lat: latitude, lng: longitude};
    
        map = new google.maps.Map(document.getElementById("map"), {
            center: myLatLng,
            zoom: 15,
            mapTypeControl: false,
            disableDefaultUI: true,
        });
        infoWindow = new google.maps.InfoWindow();
        
        geocoder = new google.maps.Geocoder();
        showMapAddress(myLatLng);
        marker = new google.maps.Marker({
            position: myLatLng,
            map: map,
            draggable: true,
            title: latitude + ', ' + longitude
        });

        $('.locationDrawerMob').fadeOut();
        marker.setVisible(true);
        marker.setPosition(myLatLng);
        showMapAddress(myLatLng);
        document.getElementById('showMapAddress').setAttribute('latlong',myLatLng);

        google.maps.event.addListener(marker,'dragend',function(event) {
            infoWindow.close();
            map.setZoom(15);
            showInfowindowText(event.latLng);
            showMapAddress(event.latLng);
            document.getElementById('showMapAddress').setAttribute('latlong',event.latLng);
        });

         google.maps.event.addListener(map, 'click', function(event) {
            showMapAddress(event.latLng);
            marker.setPosition(event.latLng);
            document.getElementById('showMapAddress').setAttribute('latlong',event.latLng);
        });

        const locationButton = document.createElement("button");
        locationButton.innerHTML = "<i class='fas fa-crosshairs' aria-hidden='true'></i> &nbsp; Use Current Location";
        locationButton.classList.add("custom-map-control-button");
          map.controls[google.maps.ControlPosition.BOTTOM_CENTER].push(locationButton);
          locationButton.addEventListener("click", () => {
            // // Try HTML5 geolocation.
            if (navigator.geolocation) {
              navigator.geolocation.getCurrentPosition(
                (position) => {
                  const pos = {
                    lat: position.coords.latitude,
                    lng: position.coords.longitude,
                  };
                console.log(pos);
                showInfowindowText(pos);
                showMapAddress(pos);
                document.getElementById('showMapAddress').setAttribute('latlong',pos);
                },
                () => {
                  handleLocationError(true, infoWindow, map.getCenter());
                },
              );
            } else {
              // Browser doesn't support Geolocation
              handleLocationError(false, infoWindow, map.getCenter());
            }
          }); 
    }
    
    function handleLocationError(browserHasGeolocation, infoWindow, pos) {
        infoWindow.setPosition(pos);
        infoWindow.setContent(
        browserHasGeolocation
          ? "Error: The Geolocation service failed."
          : "Error: Your browser doesn't support geolocation.",
        );
        infoWindow.open(map);
    }
    window.initMap1 = initMap1;

    $('#savedAddress').on('input', function() {
        var searchValue = $(this).val().toLowerCase();
        
        $('.shippingAddrList').each(function() {
            var addressDiv = $(this);
            var addressText = addressDiv.text().toLowerCase();
            
            if (addressText.includes(searchValue)) {
                addressDiv.show();
            } else {
                addressDiv.hide();
            }
        });
    });


$('#mapsearch').on('click',function () {
    $('.locationDrawerMob').fadeIn();
});
function showMapAddress(latLng){
    var geocoderOptions = {
        location: latLng,
        language: 'en'
      };
    geocoder.geocode(geocoderOptions,function(results, status){
        if (status == google.maps.GeocoderStatus.OK && results.length) {
            result=results[0].address_components;
            var info=[];
            var address = results[0].formatted_address;
            var parts = address.split(',');
            var modifiedAddress = parts.slice(0, 3).join(', ').trim();
            document.getElementById('address').value = modifiedAddress;
            var map_add = (results[0].formatted_address.length > 70)? results[0].formatted_address.substring(0, 70) + '...' : address;
            document.getElementById('descMap').innerHTML=map_add;
            var full_add = (results[0].formatted_address.length > 30)? results[0].formatted_address.substring(0, 30) + '...' : address;
            document.getElementById('descAddMap').innerHTML = full_add;

            document.getElementById('firstName').value = decryptWithAES(getCookie('plotch_first'));
            document.getElementById('lastName').value =  decryptWithAES(getCookie('plotch_last'));
            document.getElementById('email').value =  decryptWithAES(getCookie('plotch_email'));
            document.getElementById('telephone').value =  X_MOBILENO;
            for(var i=0;i<result.length;++i){
                if(result[i].types[0]=="administrative_area_level_3"){
                    document.getElementById('city').value = result[i].long_name;
                    document.getElementById('titleMap').innerHTML = result[i].long_name;
                    document.getElementById('titleAddMap').innerHTML = result[i].long_name;
                }
                if(result[i].types[0]=="administrative_area_level_1"){
                    document.getElementById('state').value = result[i].long_name;
                }
                if(result[i].types[0]=="country"){
                    document.getElementById('country').value = result[i].short_name;
                }
                if(result[i].types[0]=="postal_code"){
                    document.getElementById('pincode').value = result[i].long_name;
                    document.getElementById('pincodeMap').innerHTML = result[i].long_name;
                    document.getElementById('pincodeAddMap').innerHTML = result[i].long_name;
                }                                       
            }
        }
    });
}
$(document).on('click', '#showMapAddress',function(){
    $('.edit_add_text').html('Add New Address');
    $('.addaddr-div-mob').fadeIn();
    $('#addAddressForm').html($("#address_form"));
    $('#mapAddshow').show();
    var latlng = $(this).attr('latlong');
    showMapAddress(latlng);
});
$('#mapchangebtn').click(function () {
    $('.locationDrawerMob').fadeIn();
});

function showInfowindowText(pos){
    geocoder.geocode({'latLng': pos},function(results, status){
        if (status == google.maps.GeocoderStatus.OK && results.length) {
        result=results[0].address_components;
        var address = results[0].formatted_address;
        marker.setPosition(pos);
        map.setCenter(pos);
        infoWindow.setPosition(pos);
        infoWindow.setContent(address);
        infoWindow.open(map,marker);

        }
    });
}


function showLocationPopup(){
    $('.remove-btn').removeClass("removeClose");
    var x_token = X_TOKEN;
    if(x_token == '' || x_token == undefined){
        openLocationPopup();
       $('#withLoginLocationDiv').hide();
    }else{
        openselectAddressPopup();
        $('#withLoginLocationDiv').show();
        locationLoginAddressList(false);
    }
}

function openLocationPopup() {
    if(window.matchMedia("(max-width: 767px)").matches){
        $('.locationDrawerMob').fadeIn();
    }else{
       document.getElementById("locationPopup").classList.toggle("active");
    }
}
function openselectAddressPopup() {
    if(window.matchMedia("(max-width: 767px)").matches){
        getAddressList();
        // document.cookie ="location="+1+";path=/";
        var cSecure =";";
        if (location.protocol === 'https:') {
            cSecure = "; secure";
          }
        document.cookie ="location="+1+";path=/;SameSite=Lax"+ cSecure;
        $('.selectaddr-div-mob').fadeIn();
    }else{
       document.getElementById("locationPopup").classList.toggle("active");
    }
}
function closeLocationPopup(){
    if(window.matchMedia("(max-width: 767px)").matches){
        $('.locationDrawerMob').fadeOut();
    }else{
       document.getElementById("locationPopup").classList.toggle("active");
    }
}

function btnEnable(){
  if($('#citysearch').val() == '' && $('#latitude').val() == ''){
    $('#btnSaveLocation').attr('disabled',true)
  }else{
    $('#btnSaveLocation').removeAttr('disabled');
  }
}

function saveLocation(){
    deleteCookie('plotch_addr');
    deleteCookie('plotch_latitude');
    deleteCookie('plotch_longitude');
    deleteCookie('plotch_city');
    deleteCookie('plotch_streetcity');

    var today = new Date();
    var expire = new Date();
    expire.setTime(today.getTime() + 3600000*24*7);
    var extime = expire.toGMTString();

    var cSecure =";";
    if (location.protocol === 'https:') {
        cSecure = "; secure";
    }
    document.cookie ="plotch_addr="+$('#citysearch').val()+";expires="+extime+";path=/;SameSite=Lax"+ cSecure;
    document.cookie ="plotch_latitude="+$('#latitude').val()+";expires="+extime+";path=/;SameSite=Lax"+ cSecure;
    document.cookie ="plotch_longitude="+$('#longitude').val()+";expires="+extime+";path=/;SameSite=Lax"+ cSecure;
    document.cookie ="plotch_city="+$('#city_name').val()+";expires="+extime+";path=/;SameSite=Lax"+ cSecure;
    document.cookie ="plotch_streetcity="+$('#street_city').val()+";expires="+extime+";path=/;SameSite=Lax"+ cSecure;

    // document.cookie="plotch_addr="+$('#citysearch').val()+";expires="+extime+";path=/";
    // document.cookie ="plotch_latitude="+$('#latitude').val()+";expires="+extime+";path=/";
    // document.cookie ="plotch_longitude="+$('#longitude').val()+";expires="+extime+";path=/";
    // document.cookie ="plotch_city="+$('#city_name').val()+";expires="+extime+";path=/";
    // document.cookie ="plotch_streetcity="+$('#street_city').val()+";expires="+extime+";path=/";

    closeLocationPopup();
    if(window.matchMedia("(min-width: 768px)").matches){
        location.reload();
    }else{
        initMap1();
    }
    setTimeout(function() {
    $('#location_btn a').attr('data-content', getCookie('plotch_addr'));
    // var popover = $('#location_btn a').data('popover');
    // popover.setContent();
    // popover.$tip.addClass(popover.options.placement);
    },500);
    $('.showloccity').html(getCookie('plotch_streetcity'));
    var city = getCookie('plotch_city');
    if(city !== ''){
        $('.search-text').attr("placeholder" , "Search here for products in "+city);
    }else{
        $('.search-text').attr("placeholder" , 'Search');
    } 
    // location.reload();
    if('' == 'pdpPage'){
       // initMap1();
    }

    var feed_url = $("#feed_url").val();
    var page_type = $("#page_type").val();
    if($('.errordiv').css('display') == 'block' || feed_url!=undefined && page_type!=undefined){
        location.reload();
    }
    if($('#addressFlag').val()==1){
        getPickupAddress();
    }
    return false;
}

function locationLoginAddressList(showAll){
    var url = WEB_PATH+"/myAccountAjax.php";
    var postdata = {"action":"locationLoginAddressList","showAll":showAll};
    $("#spinner_loader_div").show();
    jQuery.ajax({
        type:'POST',
        url: url,
        data:postdata,
        dataType: "json",
        async: true,
        success: function (res) {
            $("#spinner_loader_div").hide();
            if(res['m']==""){
                $('#withLoginLocationDiv').hide();
            }else{
                $('#withLoginLocationDiv').show();
                $("#addressList").html(res['m']);
            }
        }
    });
}

function addLocationOnAutomplete(address,city,region,country,longitude,latitude,addressId) {
    if((longitude == '' || longitude === null) && (latitude == '' || latitude === null))
    {
        NotifyToast('danger',"Couldn't get your location information, please try another address/modify address.");
        return false;
    }else{
        var addr = address+','+city+','+region+','+country;
        var faddr = address.substring(0,10);
        var final_addr = address.length > 10 ? faddr+'... , '+city : faddr+' , '+city;
        $('#city_name').val(city);
        $('#street_city').val(final_addr);
        $('#citysearch').val(addr);
        $('#latitude').val(latitude);
        $('#longitude').val(longitude);
        setCookie('plotch_address_id', addressId);
        if(window.matchMedia("(max-width: 767px)").matches){
            $('.addressListGap').removeClass('selectedShipPickupAddre');
            $('input[name=locationAddressList][value='+addressId+']').attr('checked', true);
            $('#addressloc_'+addressId).addClass('selectedShipPickupAddre');
        }
        saveLocation();
        location.reload();
    }
}

(function () {
    var locatorSection = document.getElementById("locator-input-section")
    function init() {
        var locatorMobButton = document.getElementById("locatorMob-button");
        locatorMobButton.addEventListener("click", locatorButtonPressed);
    }
    function initDesk(){    
        var locatorButton = document.getElementById("locator-button");
        locatorButton.addEventListener("click", locatorButtonPressed);
    }

    function locatorButtonPressed() {
        $("#spinner_loader_div").show();

        navigator.geolocation.getCurrentPosition(function (position) {
                getUserAddressBy(position.coords.latitude, position.coords.longitude)
            },
            function (error) {
                $("#spinner_loader_div").hide();
                alert("The Locator was denied :( Please add your address manually")
            })
    }

    function getUserAddressBy(lat, long) {
        var xhttp = new XMLHttpRequest();
        xhttp.onreadystatechange = function () {
            if (this.readyState == 4 && this.status == 200) {
                var address = JSON.parse(this.responseText)
                var address1 = [];
                var address2 = [];
                address.results[0].address_components.forEach( function(el) {
                    address1[el.types[0]] = el.short_name;
                    address2[el.types[0]] = el.long_name;
                });
                var city = (address1.administrative_area_level_2 && address1.administrative_area_level_2 != '' ? address1.administrative_area_level_2 : address2.administrative_area_level_1);
                var street = address1.political;

                setAddressToInputField(address.results[0].formatted_address,city,street,lat,long)
            }
        };
        xhttp.open("GET", "https://maps.googleapis.com/maps/api/geocode/json?latlng=" + lat + "," + long + "&key=AIzaSyCNEfAnTeqL6wUprs_krip1ymL1EL4f87w", true);
        xhttp.send();
    }

    function setAddressToInputField(address,city,street,latitude,longitude) {
        document.getElementById("citysearch").value = address;
        document.getElementById('latitude').value = latitude;
        document.getElementById('longitude').value = longitude;
        document.getElementById("city_name").value = city;
        if(street != undefined){
            var faddr = street.substring(0,10);
            var final_addr = street.length > 10 ? faddr+'... , '+city : faddr+' , '+city;
            document.getElementById("street_city").value = final_addr; 
        }else{
            document.getElementById("street_city").value = city;
        }
        $("#spinner_loader_div").hide();
        saveLocation();
    }
            if(window.matchMedia("(max-width: 767px)").matches){
            init()
        }else{
            initDesk()
        }
    })();
</script>    <script type="text/javascript">

    function closeAlertMsgPopup() {
        document.getElementById("alertMsgPopup").classList.toggle("active");
    }

    function showAlertMsg(msg, status = 1) {
        if (status == 1) {
            $('#alertMsgPopupTxt').html(msg).css('color', 'green');
        } else {
            $('#alertMsgPopupTxt').html(msg).css('color', 'red');
        }
        document.getElementById("alertMsgPopup").classList.toggle("active");
    }

    $(document).on('click', '#registerNew', function() {
        $("#login-signup-heading").hide();
        $('#register-headding').show();
        $('#loginAlready , #termsDiv').show();
        $('#registerNew').hide();
        $('#login-signup-msg').hide()
        $('#register-msg').show()
    })
    $(document).on('click', '#loginAlready', function() {
        $("#loginAlready , #termsDiv").hide();
        $('#registerNew').show();
        $("#login-signup-heading").show();
        $('#register-headding').hide();
        $('#login-signup-msg').show()
        $('#register-msg').hide()
    })

    $(window).ready(function(){
          var city = getCookie('plotch_addr');
      if(city !== ''){
        $('#citysearch').val(city);
        $('#longitude').val(getCookie('plotch_longitude'))
        $('#latitude').val(getCookie('plotch_latitude'))
      }
        });
    </script>
    <script>
    $('#citysearch').keyup(function () {
      var len = $(this).val().length;

      if (len >= 1) {
        $("#search").prop("disabled", false);
        $("#btn").prop("disabled", false);
      }
      else{
        $("#search").prop("disabled", true);
        $("#btn").prop("disabled", true);
      }
    });
    </script>
