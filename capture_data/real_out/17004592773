    <script type="text/javascript">
    $(document).on("keyup", ".otp-input", function(e) {
        e.preventDefault(), (pressedKeyCode = e.keyCode), (pressedKey = e.key), (otpInput = $(this).val()), 8 ==
            pressedKeyCode && ($(this).prev().focus(), $(this).prev().val("")), 69 == pressedKeyCode && $(this)
            .val("");
        if (!/^[0-9]{1,1}$/.test(otpInput)) return $(this).val(""), e.stopImmediatePropagation(), !1;
        if (1 == otpInput.length) {
            if (!$(this).next()) return !1;
            $(this).next().focus();
        }
    });

    $(".otp-input").on("keypress", function(e) {
        if (($(".shell").next(".js-error-msg").html("").hide(), $(this).val())) return !1;
    });


    function top_link(obj){
        if (window.matchMedia("(max-width: 768px)").matches){
            var innerUl = $(obj).closest('li').find('ul').length;
            if(innerUl == 0){
                window.location.href = $(obj).attr('href');
            }
        }
    } 

    function subMenuAction(obj, link){
        if (window.matchMedia("(max-width: 767px)").matches){
            var innerUl = $(obj).closest('li').find('ul').length;
            if(innerUl == 0 || innerUl == 1){
                window.location.href = link;
            }
        } else {
            window.location.href = link;
        }
    }

