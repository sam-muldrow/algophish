<script>
$(document).ready(function(){
            var submitIcon = $('.searchbox-icon');
            var inputBox = $('.searchbox-input');
            var searchBox = $('.searchbox2');
            var isOpen = false;
            submitIcon.click(function(){
                if(isOpen == false){
                    searchBox.addClass('searchbox-open');
                    inputBox.focus();
                    isOpen = true;
                    
                } else {
                    searchBox.removeClass('searchbox-open');
                    inputBox.focusout();
                    isOpen = false;
                }
            });  
             submitIcon.mouseup(function(){
                    return false;
                });
            searchBox.mouseup(function(){
                    return false;
                });
            $(document).mouseup(function(){
                    if(isOpen == true){
                        $('.searchbox-icon').css('display','block');
                        submitIcon.click();
                    }
                });
            $(function () {
              $('[rel=popover]').popover({ html : true });
            });
        
        // Google Login JS
        
                var city = getCookie('plotch_addr');
        if(city == ''){
           $('.remove-btn').addClass("removeClose");
           $('#sortbyClose2').addClass("removeClose");
            var x_token = X_TOKEN;
            if(x_token == '' || x_token == undefined){
                openLocationPopup();
               $('#withLoginLocationDiv').hide();
            }else{
                openselectAddressPopup();
                $('#withLoginLocationDiv').show();
                locationLoginAddressList(false);
            }
        }
            });

    function decodeJwtResponse(token) {
        var base64Url = token.split('.')[1];
        var base64 = base64Url.replace(/-/g, '+').replace(/_/g, '/');
        var jsonPayload = decodeURIComponent(window.atob(base64).split('').map(function(c) {
            return '%' + ('00' + c.charCodeAt(0).toString(16)).slice(-2);
        }).join(''));

        return JSON.parse(jsonPayload);
    };

    function handleCredentialResponse(response) {
        const responsePayload = decodeJwtResponse(response.credential);
        social_media_login(responsePayload.email)
        console.log(responsePayload.email)
        // console.log("ID: " + responsePayload.sub);
        // console.log('Full Name: ' + responsePayload.name);
        // console.log('Given Name: ' + responsePayload.given_name);
        // console.log('Family Name: ' + responsePayload.family_name);
        // console.log("Image URL: " + responsePayload.picture);
        // console.log("Email: " + responsePayload.email);
    }

    // Google Login JS Ended

    // FB Login JS
    
    (function(d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) return;
        js = d.createElement(s); js.id = id;
        js.src = "//connect.facebook.net/en_US/sdk.js";
        fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));

    function fbLogin() {
        FB.login(function (response) {
            if (response.authResponse) {
                getFbUserData();
            }
        }, {scope: 'email'});
    }

    function getFbUserData(){
        FB.api('/me', {locale: 'en_US', fields: 'email'},
        function (response) {
            console.log(response.email);
            social_media_login(response.email)
        });
    }
    // FB Login JS ended

    function social_media_login(email){
        var url = "ajax.php";
        var postdata = {"action": "socialMediaLogin", "email": email};
        $("#spinner_loader_div").show();
        jQuery.ajax({
            type:'POST',
            url: url,
            async: false,
            data:postdata,
            dataType: "json",
            success: function (res) {
                $("#spinner_loader_div").hide();
                if(res['s'] == '1'){
                    console.log('Logged in Successfully!');
                    // deleteCookie('x-plotch-session');

                    // var session_expiry = res['d']['sessionExpiry'] != '' && res['d']['sessionExpiry'] != undefined ? res['d']['sessionExpiry'] : "604800" ;
                    // document.cookie="x-plotch-token="+res['d']['Authorization']+";max-age="+session_expiry+ ";path=/";

                    // setCookie('x-plotch-customer-id', res['d']['customerId'], "7");
                    setCookie('x-plotch-customer-email', email, "7");
                    location.reload();
                }else{
                    alert('Something went wrong!');
                }
            }
        });
    }
        
            function buttonUp(){
                var inputVal = $('.searchbox-input').val();
                inputVal = $.trim(inputVal).length;
                if( inputVal !== 0){
                    $('.searchbox-icon').css('display','none');
                } else {
                    $('.searchbox-input').val('');
                    $('.searchbox-icon').css('display','block');
                }
            }
    
    
    
    // searchbox3       
    $('.search-toggle').addClass('closed');
    
    
$('.search-toggle .search-icon').click(function(e) {
  if ($('.search-toggle').hasClass('closed')) {
    $('.search-toggle').removeClass('closed').addClass('opened');
    $('.search-toggle, .search-container').addClass('opened');
    $('#search-text').focus();
    
  } else {
    $('.search-toggle').removeClass('opened').addClass('closed');
    $('.search-toggle, .search-container').removeClass('opened');
    
  }
}); 

jQuery(document.body).on('click', function(ev){
    if(jQuery(ev.target).closest('.searchbox3').length) return; // Not return false

    $('.search-toggle').removeClass('opened').addClass('closed');
    $('.search-toggle, .search-container').removeClass('opened');
});

// mobile search
$('.search-toggle1').addClass('closed');  
$('.search-container1').slideUp(200);
$('.search-toggle1 .search-icon1').click(function(e) {
  if ($('.search-toggle1').hasClass('closed')) {
    $('.search-toggle1').removeClass('closed').addClass('opened');
    $('.search-toggle1, .search-container1').addClass('opened');
    $('.search-container1').slideDown(200);
    $('#search-text').focus();
} else {
    $('.search-toggle1').removeClass('opened').addClass('closed');
    $('.search-toggle1, .search-container1').removeClass('opened');
    $('.search-container1').slideUp(200);
  }
}); 

jQuery(document.body).on('click', function(ev){
    if(jQuery(ev.target).closest('.searchbox4').length) return; // Not return false
    $('.search-toggle1').removeClass('opened').addClass('closed');
    $('.search-toggle1, .search-container1').removeClass('opened');
});

$('.search-text').keyup(function(){
    var searchTxt = $(this).val();

    if(searchTxt.length>2){
    var postdata = ({'action':'searchProduct','searchTxt':searchTxt});
    
        jQuery.ajax({
        type: 'POST',
        url: 'ajax.php',
        data:postdata,
        dataType: "json",
        success: function(res, response) {
            if (res['s'] == 1) {
                console.log("=>",res['n']);
                 $( ".search-text" ).autocomplete({
                      source: res['n'],
                      select: function( event, ui ) {
                         $(".search-text").val(ui.item.value);
                         $(".search-form").submit();
                          }                 
                    });
                }      
            }
        },'json');
    }
});

$(document).on("click", ".inputBoxs", function() {
    var searchTxt = $(this).parent('div').find('.search-text').val();
    if(searchTxt.length>2){
        $(".search-form").submit();
    }
});
</script>
