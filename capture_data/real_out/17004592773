<script>
        (function($){
            $(window).on("load",function(){
                
                $("a[rel='load-content']").click(function(e){
                    e.preventDefault();
                    var url=$(this).attr("href");
                    $.get(url,function(data){
                        $(".filter-values .mCSB_container").append(data); //load new content inside .mCSB_container
                        //scroll-to appended content 
                        $(".filter-values").mCustomScrollbar("scrollTo","h2:last");
                    });
                });
                
                $(".filter-values").delegate("a[href='top']","click",function(e){
                    e.preventDefault();
                    $(".filter-values").mCustomScrollbar("scrollTo",$(this).attr("href"));
                });
                
            });
        })(jQuery);
    </script>

<script type="text/javascript">
$(document).ready(function(){
    $('#navbar-menu-menu').click(function(){
        $('#sort-by-overlay').show();
        $('#sort-by-overlay').click(function(){
            if($('#navbar-menu').hasClass('in')){
                $('#navbar-menu-menu').trigger('click');
                $('#sort-by-overlay').hide();
            }
        });
    });
});

    $(window).load(function(){
        if($('.announceMent_popup').length > 0){
            var plotch_announcement_popup = getCookie('plotch_announcement_popup');
            if(plotch_announcement_popup == "" && $('#viewType').val() == 'entry'){
                $(".announceMent_popup").addClass("activeAnnouncement");
                if($("#newsLetterPopup").val()==1){
                    var id = $('#formData').attr('formTemplatedId');
                    var url= 'https://plotchappsapi.plotch.ai/getFormHtml?id='+id
                    jQuery.ajax({  
                            type:"get",
                            async:false, 
                            dataType:"json", 
                            url: url,
                            success: function (res) {
                                $('#formData').html(res['d']['formHtml']);
                                $('#formData').append('<input type="hidden" class="form_token" id="formToken" value="'+res['d']['formToken']+'">');
                                $('#formData').append('<input type="hidden" class="sendFormFlag" id="sendFormFlag" value="1">');
                                $('#formData').addClass('form_id');
                                saveResponse();
                            },
                        });
                }
                setCookie('plotch_announcement_popup', 1, 1);
            }
        }
    });

    $(document).on("click", "#announcementClose",  function(){
        document.getElementById("announceMent_popup").classList.toggle("activeAnnouncement");
    });
       $(document).on("click", "#enquiryClose",  function(){
        document.getElementById("product_popup_enquiry").classList.toggle("activeProduct_enquiry");
    });

    function redirectLink(link){
        window.location.href = link;
    }

$(document).scroll(function() {
  var y = $(this).scrollTop();
  if (y > 400) {
    $('#scroll_to_top').show();
  } else {
    $('#scroll_to_top').hide();
  }
});
    
    $("#scroll_to_top").click(function() {
  $("html, body").animate({ scrollTop: 0 }, "slow");
  return false;
});
</script>
