<script>
$(function () {
    $('[rel=popover]').popover({ html : true });
});
function addFontLink(font, font_link){
	if(font != ''){
		var css = '<link href="https://fonts.googleapis.com/css2?family='+font+'&amp;display=swap" rel="stylesheet">';
	 	$(document.head).append(css);
	}
}

window.onload = (event) => {
  $('.video-container').each(function() {
  	var data_col_md = $(this).attr('data-col-md');
  	if(data_col_md == '2') {
  		$(this).find('.youtube-img').css({'height':'145px'});
  	} else if(data_col_md == '12') {
  		$(this).find('.youtube-img').css({'height':'790px'});
  	} else if(data_col_md == '3') {
  		$(this).find('.youtube-img').css({'height':'185px'});
  	} else if(data_col_md == '4') {
  		$(this).find('.youtube-img').css({'height':'250px'});
  	} else if(data_col_md == '6') {
  		$(this).find('.youtube-img').css({'height':'385px'});
  	}
  });
};

$(document).on('click', '#signin_btn2', function(){

	var user = $('#user').val();
    var pass = $('#pass').val();
    var ref_url = $('#ref_url').val();

    if (user == "") {
        NotifyToast("danger", 'Please provide a user');
        return false;
    }
    if (pass == "") {
        NotifyToast("danger", 'Please provide a password');
        return false;
    }

    $("#spinner_loader_div").show();
	var postdata = {'action':'checkLogin','user': user, 'pass': pass,'ref_url':ref_url};
	jQuery.ajax({
        type: 'POST',
        url: 'ajax.php',
        data: postdata,
        dataType: "json",
        success: function(res) {
        	$("#spinner_loader_div").hide();
            if (res.s == 1) {
            	$('#signInForm').submit();
            } else {
                $('#reg_msg').html('');
                NotifyToast("danger", res['m']);
            }
        }
    });
});

$('#registration_btn').click(function() {
    var email = $("#email").val();
    if (email == "") {
        NotifyToast("danger", 'Please provide a valid email');
        return false;
    }

    var mobile = $("#mobile").val();
    if(mobile == "" || mobile.length!=10 || isNaN(mobile)){
    	NotifyToast("danger", 'Please provide a valid mobile.');
        return false;
    }


    if ($('#is_public_mark').is(':checked')) {
        var action = "registerRootAccount";
        var country = $('#country_code').val().split('_');
        var country_mobile_code = country[1];
        var countryCode = country[0];
        var flag = 0 ;
        if( country_mobile_code == '+91'){
          flag = 1
          var mob_regex = /^(?:(?:\+|0{0,2})91(\s*[\-]\s*)?|[0]?)?[6789]\d{9}$/;
          if(mob_regex.test(mobile)){
         } else {
           NotifyToast("danger", 'Please provide a valid mobile.');
           return false;
         }
       }
       $('#flag').val(flag);
        var postdata = {
            "action": action,
            "email": email,
            "mobile": mobile,
            "country_code":countryCode,
            "country_mobile_code":country_mobile_code
        };
        $('#reg_msg').html('Processing...').css('color', '#29ad29');
        $("#spinner_loader_div").show();
        jQuery.ajax({
            type: 'POST',
            url: 'ajax.php',
            data: postdata,
            dataType: "json",
            success: function(res) {
            	$("#spinner_loader_div").hide();
                if (res['s'] == 1) {
                	$('#register_form').hide();
                	$('#otp_form').show();
                    $("#otp_email_span").html(res['d']['email']);
                    $("#otp_email").val(res['d']['email']);
                    $('#reg_msg').html(res['m']).css('color', 'green');
                } else {
                    $('#reg_msg').html('');
                    NotifyToast("danger", res['m']);
                }
            }
        });
    } else {
        NotifyToast('danger', 'Please check this box if you want to proceed.');
    }
});

$('#otp_btn , #otp_btn2').click(function() {
	var otp = $("#otp").val();

	if(otp == ''){
		NotifyToast("danger", 'Please provide otp');
        return false;
	} else {
        var action = "verifiedOtp";
        var email = $("#otp_email").val();
        var flag = $('#flag').val();
        var postdata = {
            "action": action,
            "otp": otp,
            "email": email,
            "flag" :flag
        };
        $('#otp_msg').html('Processing...').css('color', '#29ad29');
        $("#spinner_loader_div").show();
        jQuery.ajax({
            type: 'POST',
            url: 'ajax.php',
            data: postdata,
            dataType: "json",
            success: function(res) {
                if (res['s'] == 1) {
                    var user = res['d']['email'];
                    var pass = res['d']['random_code'];
                    var ref_url = $('#ref_url').val();
                    var postdata = {
                        'user': user,
                        'pass': pass,
                        'ref_url': ref_url,
                        'action':'checkLogin'
                    };
                    $('#user_reg').val(user);
                    $('#pass_reg').val(pass);

					jQuery.ajax({
				        type: 'POST',
				        url: 'ajax.php',
				        data: postdata,
				        dataType: "json",
				        success: function(res) {
				        	$("#spinner_loader_div").hide();
				            if (res.s == 1) {
				            	$('#otp_form').submit();
				            } else {
				                $('#reg_msg').html('');
				                NotifyToast("danger", res['m']);
				            }
				        }
				    });
                } else {
                    $('#otp_msg').html(res['m']).css('color', 'red');
                }
            }
        });
    }
});

$('#resend_otp').click(function() {
    var action = "resendOtp";
    var email = $("#otp_email").val();

    var postdata = {
        "action": action,
        "email": email
    };
    jQuery.ajax({
        type: 'POST',
        url: 'ajax.php',
        data: postdata,
        dataType: "json",
        success: function(res) {
            if (res['s'] == 1) {
                $('#resend_div').html('OTP send successfully,please check your mail');
            } else {
                $('#resend_div').html(res['m']).css('color', 'red');
            }
        }
    });
});

function loadscript(ev) {
	var youtube_id = $(ev).parent('.video-container .div-video-container').find('.youtube-url').html();

	if (typeof(YT) == 'undefined' || typeof(YT.Player) == 'undefined') {
		var tag = document.createElement('script');
		tag.src = "https://www.youtube.com/iframe_api";
		var firstScriptTag = document.getElementsByTagName('script')[0];
		firstScriptTag.parentNode.after(tag, firstScriptTag);
		window.onYouTubePlayerAPIReady = function() {
			onYouTubePlayer(youtube_id,ev);
		};
	} else {
		onYouTubePlayer(youtube_id,ev);
	}
}

var player;
function onYouTubePlayer(youtube_id,ev) {
	var this_span = ev;
	var this_player = this_span.parentNode;
	player = new YT.Player(this_player, {
		width: '100%',
		heigh: 'auto',
		videoId: youtube_id,
		events: {
			'onReady': onPlayerReady,
			'onStateChange': function(event){
				switch(event.data){
					case 0:     // video ended
						player.stopVideo();
						break;
					case 2:     // video paused
						// document.getElementsByClassName("ytp-pause-overlay ytp-scroll-min").style.display='none';
						player.pauseVideo();
						break;
					default:
						break;
				}
			}
		}
	});
}

function onPlayerReady(event) {
	event.target.playVideo();
}

 $('.panel-collapse').on('show.bs.collapse', function () {
    $(this).siblings('.panel-heading').addClass('active');
    $(this).parent().find('.panel-title > a').attr('style', 'color: #B63017 !important');
  });

  $('.panel-collapse').on('hide.bs.collapse', function () {
    $(this).siblings('.panel-heading').removeClass('active');
    $(this).parent().find('.panel-title > a').attr('style', 'color: #000 !important');
  });
   function store(url){
       window.location.href = url;
    }
    function sideScroll(element,direction,speed,distance,step){
										
										var container = document.getElementById(element);
										console.log(element)
										console.log(direction)
										scrollAmount = 0;
										var slideTimer = setInterval(function(){
											if(direction == 'left'){
												container.scrollLeft -= step;
												
											} else {
												container.scrollLeft += step;
											}
											scrollAmount += step;
											if(scrollAmount >= distance){
												window.clearInterval(slideTimer);
											}
										}, speed);
									}
</script>
