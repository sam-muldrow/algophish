<script type="text/javascript">
    setTimeout(function(){
        // $('.owl-carousel').owlCarousel({
        //     loop:true,
        //     margin:10,
        //     responsive:{
        //         0:{
        //             items:2
        //         }
        //     }
        // });
    }, 3000);
    $(window).load(function(){
var code=$('#countryPhoneCode').val();
if (code=='+91') {
    $("#telephone").attr('minlength','10');
    $("#telephone").attr('maxlength','10');
}else{
    $("#telephone").attr('maxlength','12');
}

});
</script>
