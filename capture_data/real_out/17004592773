$(window).scroll(function() {    
    $("#navbar").addClass("navbar-fixed");
    var scroll = $(window).scrollTop();
    if (scroll >= 10) {
        $("#navbar").addClass("navbar-fixed");
        $(".headerDesktop7").addClass("navbar-fixed");
        $(".headerDesktop").addClass("navbar-fixed");
        //$(".header-marquee").not($('.plotch_footer').find('.header-marquee')).hide();
        if($(".header-marquee")[0]){
        $(".plotch-hamburger-bg-color").addClass("margin-top-toggle");   
         $(".plotch-hamburger-bg-color1").addClass("margin-top-toggle1");}
    }
    else{
        $(".plotch-hamburger-bg-color").removeClass("margin-top-toggle");
         $(".plotch-hamburger-bg-color1").removeClass("margin-top-toggle1");
        //$(".header-marquee").show();
        $("#navbar").removeClass("navbar-fixed");
        $(".headerDesktop7").removeClass("navbar-fixed");
        $(".headerDesktop").removeClass("navbar-fixed");
    }
});


$(document).on('click','#closeNav',function(){
    $(".menuBtn").toggleClass('open');
    $("#sort-by-overlay").toggleClass('sortShow');
    $(".collapse").fadeToggle(100);
});

$(document).on('click','#sort-by-overlay',function(){
    $('.menuBtn').addClass('collapsed');
    $('#navbar-menu').removeClass('in').css('height', '0');
    $("#sort-by-overlay").removeClass('sortShow');
    $(".menuBtn").removeClass('open');
});

