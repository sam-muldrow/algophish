        firebase.initializeApp(config);

        const messaging = firebase.messaging();
        messaging.usePublicVapidKey('BPqXa89ufCBazg3V6r_lFhS5JnLt936fRVZIZ-fuLQeXN_Nugc97A0obpwXM5mhK883IRyYjGVv8xdjKHJuy4Zk');
