	<script type="text/javascript">
	window.addEvent('domready', function() {

		var vw_break_point = 1060;
		var width = window.getSize().x;
		if(width < vw_break_point){
			$('anchor-blocks-section').destroy(); //remove the element
		}

		if (!$('anchor-blocks-section')) return;

		//init values
		var slideIndex = 0;

		function moveSlide(n) {
			showSlides(slideIndex + n);
		}

		function currentSlide(n) {
			showSlides(n);
		}

		function showSlides(n) {
			var nTop = $('hw-slideshow-container').getPosition().y;
			var nBottom = nTop + $('hw-slideshow-container').getSize().y;
			var nWinTop = $(window).getScroll().y;
			var nWinBottom = nWinTop + $(window).getSize().y;
			if (nBottom < nWinTop || nTop > nWinBottom) return;

			var dots = $('anchor-blocks-section').getElements('.ancher-dots .dot');
			var slides = $('hw-slideshow-container').getElements('.ancher-block-slide');

			if( n > slides.length){
				n = 1;
			}

			if(n < 1 ){
				n = slides.length;
			}

			if (slideIndex == n) return;
			slideIndex = n;

			dots.each(function(item, index) {
				item.removeClass("active");
			});

			dots[slideIndex-1].addClass('active');

			if(dots.length == 0){
				$('div.ancher-dots').setStyle('display', 'none' );
			}

			//$('hw-slideshow-container').setStyle('transform', 'translate(' + (692 * (slideIndex -1) * (-1) ) + 'px, 0)' );

			$('hw-slideshow-container').set('styles', {
				'-webkit-transform': 'translate(' + (692 * (slideIndex -1) * (-1) ) + 'px, 0)',
				'-moz-transform': 'translate(' + (692 * (slideIndex -1) * (-1) ) + 'px, 0)',
				'-o-transform': 'translate(' + (692 * (slideIndex -1) * (-1) ) + 'px, 0)',
				'-ms-transform': 'translate(' + (692 * (slideIndex -1) * (-1) ) + 'px, 0)',
				'transform': 'translate(' + (692 * (slideIndex -1) * (-1) ) + 'px, 0)'
			});

			//send GA event
			if (typeof(hwz_ga) != 'undefined') {
				var aTitle = $('hw-slideshow-container').getElements('.ancher-blk-big-title');
				if (slideIndex > aTitle.length) return;
				var sTitle = $(aTitle[slideIndex - 1]).get('text').trim();
				var aLabel = {
					'SectionName': 'Homepage',
					'SectionTitle': 'Anchor Blocks',
					'ArticleName': sTitle,
					'Position': slideIndex + 2
				};
				hwz_ga('send', 'event', 'Content-Engagement', 'Impression', JSON.stringify(aLabel), { data:{ ux_element_type:'Anchor Block', ux_element_name:'Carousel Anchor Block' } });
			}
		}


		var ancherTimer = new Class({
			Implements: [Options, Events],

			options: {
				interval: 5000,
				fn: function () {
				}
			},

			initialize: function (options) {
				this.setOptions(options);
			},

			start: function () {
				// start the timer
				if (this.id) return;
				this.id = setInterval(this.options.fn, this.options.interval);
			},

			pause: function () {
				// pause the timer
				if (!this.id) return;
				clearInterval(this.id);
				this.id = null;
			},

			stop: function () {
				// stop the timer
				clearInterval(this.id);
				this.id = null;
			}
		});

		var actimer = new ancherTimer({
			fn: function () {
				moveSlide(1);
			}
		});

		$('hw-slideshow-container').addEvents({

			mouseover: function(){
				//actimer.pause();
			},
			mouseout: function(e){
				//actimer.start();
				var o = $('anchor-block-slider');
				if (!o) return;
				var oPos = o.getPosition();
				var oSize = o.getSize();
				var nAllow = 10;
				var bIn = (e.page.x > oPos.x + nAllow && e.page.x < oPos.x + oSize.x - nAllow && e.page.y > oPos.y + nAllow && e.page.y < oPos.y + oSize.y - nAllow);
				if (!bIn) actimer.start();
			},
			mousemove: function(e) {
				var o = $('anchor-block-slider');
				if (!o) return;
				var oPos = o.getPosition();
				var oSize = o.getSize();
				var nAllow = 10;
				var bIn = (e.page.x > oPos.x + nAllow && e.page.x < oPos.x + oSize.x - nAllow && e.page.y > oPos.y + nAllow && e.page.y < oPos.y + oSize.y - nAllow);
				if (bIn) actimer.pause();
				else actimer.start();
			}

		});
		$('anchor-blocks-section').getElements('.prev,.next').addEvent('click', function() {
			if ($(this).hasClass('next')) moveSlide(1);
			else if ($(this).hasClass('prev')) moveSlide(-1);
		});
		$('anchor-blocks-section').getElements('.ancher-dots .dot').addEvent('click', function() {
			var sId = $(this).get('id');
			if (!/^anchor-dot-[1-9]$/.test(sId)) return;
			var n = ~~sId.replace('anchor-dot-', '');
			currentSlide(n);
		});

		if (typeof(hwz_ga) != 'undefined') {
			$('anchor-blocks-section').getElements('.ancher-block-static, .ancher-block-slide').addEvent('click', function(e) {
				var oTitle = $(this).getElement('.ancher-blk-big-title, .ancher-blk-small-title');
				if (!oTitle) return;
				var sTitle = oTitle.get('text').trim();
				var sPos = $(this).get('id');
				var nPos = (typeof(sPos) == 'string' ? ~~sPos.replace(/[^0-9]/g,'') : 0);
				var aLabel = {
					'SectionName': 'Homepage',
					'SectionTitle': 'Anchor Blocks',
					'ArticleName': sTitle,
					'Position': nPos
				};
				if ($(this).hasClass('ancher-block-static')) {
					hwz_ga('send', 'event', 'Content-Engagement', 'Click', JSON.stringify(aLabel), { data:{ ux_element_type:'Anchor Block', ux_element_name:'Sticky Anchor Block' } });
				} else {
					hwz_ga('send', 'event', 'Content-Engagement', 'Click', JSON.stringify(aLabel), { data:{ ux_element_type:'Anchor Block', ux_element_name:'Carousel Anchor Block' } });
				}
			});
			$('anchor-blocks-section').getElements('.ancher-block-static').each(function(o, i) {
				var oTitle = $(o).getElement('.ancher-blk-big-title, .ancher-blk-small-title');
				if (!oTitle) return;
				var sTitle = oTitle.get('text').trim();
				var sPos = $(o).get('id');
				var nPos = (typeof(sPos) == 'string' ? ~~sPos.replace(/[^0-9]/g,'') : 0);
				var aLabel = {
					'SectionName': 'Homepage',
					'SectionTitle': 'Anchor Blocks',
					'ArticleName': sTitle,
					'Position': nPos
				};
				hwz_ga('send', 'event', 'Content-Engagement', 'Impression', JSON.stringify(aLabel), { data:{ ux_element_type:'Anchor Block', ux_element_name:'Sticky Anchor Block' } });
			});
		}


		showSlides(1);

		actimer.start();
	});

	</script>


