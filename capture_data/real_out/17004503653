			<a class="dk-home-gallery dk-gameaxis-next home-gameaxis-slider-next"><i class="sprite-gallery-arrrows sprite-gallery-arrow-right"></i></a>

		</section>

		<script type="text/javascript">

			var gameaxisSlideIndex = 1;
			var gameaxisslides = null;
			var __dk_gameaxis = false;
			var __mb_gameaxis = false;
			var timer;

			$('home-gameaxis-slider-section').getElements('a').addEvent('click', function(e){
				if ($(this).hasClass('home-gameaxis-slider-next')) { moveGameAxisSlides(1); return; }
				if ($(this).hasClass('home-gameaxis-slider-prev')) { moveGameAxisSlides(-1); return; }
				var oContainer = $(this).getParent('.gallery-container');
				if (!!oContainer.get('data-dragend')) {
					e.preventDefault();
					e.stopPropagation();
					return;
				}
			});

			window.addEvent('domready', function(){

				__dk_gameaxis = $('gameaxis-slideshow-container').isDisplayed();
				__mb_gameaxis = $('mb-gameaxis-slideshow-container').isDisplayed();

				if(__dk_gameaxis == true){
					gameaxisslides = $$('div.gameaxis_wrapper');
				}
				else if(__mb_gameaxis == true){
					gameaxisslides = $$('div.mb_home-gameaxis-slide');
				}

				gameaxisSlideIndex = 1;

				showGameaxisSlides(gameaxisSlideIndex);


				$$('a.home-gameaxis-slider-next').addEvents({

					mouseenter: function(){
						this.getElement('i').removeClass('sprite-gallery-arrow-right');
						this.getElement('i').addClass('sprite-gallery-arrow-right_inactive');
					},
					mouseleave: function(){
						this.getElement('i').removeClass('sprite-gallery-arrow-right_inactive');
						this.getElement('i').addClass('sprite-gallery-arrow-right');
					}

				});

				$$('a.home-gameaxis-slider-prev').addEvents({

					mouseenter: function(){
						this.getElement('i').removeClass('sprite-gallery-arrow-left');
						this.getElement('i').addClass('sprite-gallery-arrow-left_inactive');
					},
					mouseleave: function(){
						this.getElement('i').removeClass('sprite-gallery-arrow-left_inactive');
						this.getElement('i').addClass('sprite-gallery-arrow-left');
					}

				});


				// just for safty when user tries to resize the window
				window.addEvent('resize', function() {
					return;
					 __dk_gameaxis = $('gameaxis-slideshow-container').isDisplayed();
					__mb_gameaxis = $('mb-gameaxis-slideshow-container').isDisplayed();

					if(__dk_gameaxis == true){
						gameaxisslides = $$('div.gameaxis_wrapper');
					}
					else if(__mb_gameaxis == true){
						gameaxisslides = $$('div.mb_home-gameaxis-slide');
					}

					gameaxisSlideIndex = 1;
					showGameaxisSlides(gameaxisSlideIndex);
				});

			});


			function moveGameAxisSlides(n) {
				showGameaxisSlides(gameaxisSlideIndex += n);
			}

			function showGameaxisSlides(n) {

				if( n > gameaxisslides.length ){
					gameaxisSlideIndex = gameaxisslides.length;
				}

				if(n < 1 ){
					gameaxisSlideIndex = 1;
				}

				if(gameaxisSlideIndex == 1){

					$$('a.dk-gameaxis-prev').addClass('blk-hide');
					$$('a.dk-gameaxis-prev').removeClass('blk-show');

					$$('a.dk-gameaxis-next').addClass('blk-show');
					$$('a.dk-gameaxis-next').removeClass('blk-hide');
				}
				else if( gameaxisSlideIndex == gameaxisslides.length){

					$$('a.dk-gameaxis-next').addClass('blk-hide');
					$$('a.dk-gameaxis-next').removeClass('blk-show');

					$$('a.dk-gameaxis-prev').addClass('blk-show');
					$$('a.dk-gameaxis-prev').removeClass('blk-hide');

				}
				else{

					$$('a.dk-gameaxis-prev').removeClass('blk-hide');
					$$('a.dk-gameaxis-next').removeClass('blk-hide');

					$$('a.dk-gameaxis-prev').addClass('blk-show');
					$$('a.dk-gameaxis-next').addClass('blk-show');
				}

				if(gameaxisslides.length == 1){
					$$('a.dk-gameaxis-next').addClass('blk-hide');
					$$('a.dk-gameaxis-next').removeClass('blk-show');

					$$('a.dk-gameaxis-prev').removeClass('blk-show');
					$$('a.dk-gameaxis-prev').addClass('blk-hide');
				}

				if(__dk_gameaxis == true){
					$('gameaxis-slideshow-container').set('styles', {
						'left': ( (gameaxisSlideIndex - 1) * -100 ) + '%'
					});
				}
				else if(__mb_gameaxis == true){
					$('mb-gameaxis-slideshow-container').set('styles', {
						'left': ( (gameaxisSlideIndex - 1) * -100 ) + '%'
					});
				}

			}

			(function(){

				$$('#gameaxis-slideshow-container,#mb-gameaxis-slideshow-container').addClass('draggable');

				interact('#gameaxis-slideshow-container,#mb-gameaxis-slideshow-container').draggable({
					axis: 'x',
					startAxis: 'x',
					lockAxis: 'x',
					// enable inertial throwing
					inertia: false,
					// keep the element within the area of it's parent
					/*restrict: {
						restriction: "self",
						endOnly: true,
						elementRect: { top: 0, left: 0, bottom: 1, right: 1 }
					},*/
					// enable autoScroll
					autoScroll: true,

					// call this function on every dragmove event
					onmove: function (event) {
						var oTarget = $(event.target);
						// keep the dragged position in the data-x/data-y attributes
						var x = (parseFloat(oTarget.get('data-x')) || 0) + event.dx;
						if (x == 0 || (x < 0 && gameaxisSlideIndex >= gameaxisslides.length) || (x > 0 && gameaxisSlideIndex <= 1)) return;
						// translate the element
						oTarget.style.webkitTransform = oTarget.style.transform = 'translate(' + x + 'px, 0px)';
						// update the posiion attributes
						oTarget.setAttribute('data-x', x);
					},
					// call this function on every dragend event
					onend: function (event) {
						var oTarget = $(event.target);
						// keep the dragged position in the data-x/data-y attributes
						var x = (parseFloat(oTarget.get('data-x')) || 0);
						oTarget.set('data-dragend', 1);

						//x is in px
						oTarget.addClass('notransition');
						oTarget.setStyle('left', 'calc(' + x + 'px + ' + ( (gameaxisSlideIndex - 1) * -100 ) + '%)');
						oTarget.style.webkitTransform = oTarget.style.transform = 'translate(0px, 0px)';
						(function(x) {

							var oParent = $(this).getParent();
							var nThreshold = Math.floor(oParent.getSize().x / 2);
							var isNext = (x < -1 * nThreshold && gameaxisSlideIndex < gameaxisslides.length);
							var isPrev = (x > nThreshold && gameaxisSlideIndex > 1);

							$(this).removeClass('notransition');
							$(this).setAttribute('data-x', 0);
							if (isNext) {
								moveGameAxisSlides(1);
							} else if (isPrev) {
								moveGameAxisSlides(-1);
							} else {
								$(this).setStyle('left', ( (gameaxisSlideIndex - 1) * -100 ) + '%');
							}
							//end the whole drag
							$(this).set('data-dragend');
						}).delay(0, oTarget, x);
					}
				});
			})();

		</script>
 <!-- home page gallery slider -->

	
