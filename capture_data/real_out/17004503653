		</section>

		<script type="text/javascript">

			var glyslideIndex = 1;
			var glyslides = null;
			var __dk = false;
			var __mb = false;
			var timer;

			$('home-gallery-slider-section').getElements('a').addEvent('click', function(e){
				if ($(this).hasClass('home-gly-slider-next')) { moveGlySlides(1); return; }
				if ($(this).hasClass('home-gly-slider-prev')) { moveGlySlides(-1); return; }
				var oContainer = $(this).getParent('.gallery-container');
				if (!!oContainer.get('data-dragend')) {
					e.preventDefault();
					e.stopPropagation();
					return;
				}
			});

			window.addEvent('domready', function(){

				__dk = $('gl-slideshow-container').isDisplayed();
				__mb = $('mb-gl-slideshow-container').isDisplayed();

				if(__dk == true){
					glyslides = $$('div.gallery_wrapper');
				}
				else if(__mb == true){
					glyslides = $$('div.mb_home-gallery-slide');
				}

				glyslideIndex = 1;

				showGlySlides(glyslideIndex);


				$$('a.home-gly-slider-next').addEvents({

					mouseenter: function(){
						this.getElement('i').removeClass('sprite-gallery-arrow-right');
						this.getElement('i').addClass('sprite-gallery-arrow-right_inactive');
					},
					mouseleave: function(){
						this.getElement('i').removeClass('sprite-gallery-arrow-right_inactive');
						this.getElement('i').addClass('sprite-gallery-arrow-right');
					}

				});

				$$('a.home-gly-slider-prev').addEvents({

					mouseenter: function(){
						this.getElement('i').removeClass('sprite-gallery-arrow-left');
						this.getElement('i').addClass('sprite-gallery-arrow-left_inactive');
					},
					mouseleave: function(){
						this.getElement('i').removeClass('sprite-gallery-arrow-left_inactive');
						this.getElement('i').addClass('sprite-gallery-arrow-left');
					}

				});


				// just for safty when user tries to resize the window
				window.addEvent('resize', function() {
					return;
					__dk = $('gl-slideshow-container').isDisplayed();
					__mb = $('mb-gl-slideshow-container').isDisplayed();

					if(__dk == true){
						glyslides = $$('div.gallery_wrapper');
					}
					else if(__mb == true){
						glyslides = $$('div.mb_home-gallery-slide');
					}

					glyslideIndex = 1;
					showGlySlides(glyslideIndex);
				});

			});


			function moveGlySlides(n) {
				showGlySlides(glyslideIndex += n);
			}

			function showGlySlides(n) {

				if( n > glyslides.length ){
					glyslideIndex = glyslides.length;
				}

				if(n < 1 ){
					glyslideIndex = 1;
				}

				if(glyslideIndex == 1){

					$$('a.dk-prev').addClass('blk-hide');
					$$('a.dk-prev').removeClass('blk-show');

					$$('a.dk-next').addClass('blk-show');
					$$('a.dk-next').removeClass('blk-hide');
				}
				else if( glyslideIndex == glyslides.length){

					$$('a.dk-next').addClass('blk-hide');
					$$('a.dk-next').removeClass('blk-show');

					$$('a.dk-prev').addClass('blk-show');
					$$('a.dk-prev').removeClass('blk-hide');

				}
				else{

					$$('a.dk-prev').removeClass('blk-hide');
					$$('a.dk-next').removeClass('blk-hide');

					$$('a.dk-prev').addClass('blk-show');
					$$('a.dk-next').addClass('blk-show');
				}

				if(glyslides.length == 1){
					$$('a.dk-next').addClass('blk-hide');
					$$('a.dk-next').removeClass('blk-show');

					$$('a.dk-prev').removeClass('blk-show');
					$$('a.dk-prev').addClass('blk-hide');
				}

				if(__dk == true){
					$('gl-slideshow-container').set('styles', {
						'left': ( (glyslideIndex - 1) * -100 ) + '%'
					});
				}
				else if(__mb == true){
					$('mb-gl-slideshow-container').set('styles', {
						'left': ( (glyslideIndex - 1) * -100 ) + '%'
					});
				}

			}

			(function(){

				$$('#gl-slideshow-container,#mb-gl-slideshow-container').addClass('draggable');

				interact('#gl-slideshow-container,#mb-gl-slideshow-container').draggable({
					axis: 'x',
					startAxis: 'x',
					lockAxis: 'x',
					// enable inertial throwing
					inertia: false,
					// keep the element within the area of it's parent
					/*restrict: {
						restriction: "self",
						endOnly: true,
						elementRect: { top: 0, left: 0, bottom: 1, right: 1 }
					},*/
					// enable autoScroll
					autoScroll: true,

					// call this function on every dragmove event
					onmove: function (event) {
						var oTarget = $(event.target);
						// keep the dragged position in the data-x/data-y attributes
						var x = (parseFloat(oTarget.get('data-x')) || 0) + event.dx;
						if (x == 0 || (x < 0 && glyslideIndex >= glyslides.length) || (x > 0 && glyslideIndex <= 1)) return;
						// translate the element
						oTarget.style.webkitTransform = oTarget.style.transform = 'translate(' + x + 'px, 0px)';
						// update the posiion attributes
						oTarget.setAttribute('data-x', x);
					},
					// call this function on every dragend event
					onend: function (event) {
						var oTarget = $(event.target);
						// keep the dragged position in the data-x/data-y attributes
						var x = (parseFloat(oTarget.get('data-x')) || 0);
						oTarget.set('data-dragend', 1);

						//x is in px
						oTarget.addClass('notransition');
						oTarget.setStyle('left', 'calc(' + x + 'px + ' + ( (glyslideIndex - 1) * -100 ) + '%)');
						oTarget.style.webkitTransform = oTarget.style.transform = 'translate(0px, 0px)';
						(function(x) {

							var oParent = $(this).getParent();
							var nThreshold = Math.floor(oParent.getSize().x / 2);
							var isNext = (x < -1 * nThreshold && glyslideIndex < glyslides.length);
							var isPrev = (x > nThreshold && glyslideIndex > 1);

							$(this).removeClass('notransition');
							$(this).setAttribute('data-x', 0);
							if (isNext) {
								moveGlySlides(1);
							} else if (isPrev) {
								moveGlySlides(-1);
							} else {
								$(this).setStyle('left', ( (glyslideIndex - 1) * -100 ) + '%');
							}
							//end the whole drag
							$(this).set('data-dragend');
						}).delay(0, oTarget, x);
					}
				});
			})();

		</script>
