		<script>
		window.addEvent('domready', function() {
			let oObserver = new IntersectionObserver(function(aElem, o){
				aElem.forEach((e) => {
					if (!e.isIntersecting) return;
					var oImg = $(e.target);
					var sUrl = oImg.get('data-thumbsrc');
					if (!!sUrl && sUrl != oImg.get('src')) oImg.set('src', sUrl);
					oObserver.unobserve(e.target);
				});
			}, {root:null,rootMargin:'0px',threshold:0});
			let a = document.getElementById('home-feature-wrapper').querySelectorAll('img[src^="/thumbs/placeholder/"]');
			a.forEach((e) => {
				oObserver.observe(e);
			});
		});
		</script>

