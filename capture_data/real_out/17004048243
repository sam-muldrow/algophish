          function publishNRError(eventName) {
            if (window.newrelic && newrelic.noticeError) {
              newrelic.noticeError('ensighten event called with ' + eventName);
            }
          }
          if (window.publisherFW && window.publisherFW.publishEvent) {
            var publisherFW = (function (oldPublisherFW) {
              var publisherFWCpy = Object.assign({}, oldPublisherFW);
              publisherFWCpy.publishEvent = function (eventName, eventData) {
