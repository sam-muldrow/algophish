            <script>
            function activeUnion(){
                $(".unionsc").mouseover(function(){
            		$("#izqAb").animate({"margin-left": "426"}, "slow").fadeOut();
            		$("#derAb").animate({"margin-left": "426"}, "slow").fadeOut();
            		$( "#union" ).fadeIn("slow");
            	});
                $("#remLayer").click(function(event){
            		//$(".unionsc").animate({"margin-left": "0"}, "fast").show("fast");
                    $("#izqAb").animate({"margin-left": "-155px"}, "fast");
                    $("#derAb").animate({"margin-left": "1005px"}, "fast");
            		$( "#derAb" ).fadeIn("slow");
            		$( "#izqAb" ).fadeIn("slow");
                    $( "#union" ).fadeOut();
              });
            }
            //activeUnion();
            </script>

