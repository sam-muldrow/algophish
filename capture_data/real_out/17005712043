    <script type="text/javascript">
        window.onload=function(){
            var nav_height=$(".item_nav").height()
            $(".navbar").click(function () {

                var num_son=$(this).next().children().length
                if($(this).hasClass("active")){
                    $(this).next().css("height","0px")
                    $(this).removeClass("active")
                    //ç®­å¤´
                    $(this).parent().find(".arrowhead").css("transform","rotate(45deg)")



                }else{
                    $(this).parent().find(".arrowhead").css("transform","rotate(-135deg)")


                    $(this).next().css("height",nav_height*num_son)
                    //å»é¤åå¼èç¹çactiveç¶æåè®¾ç½®é«åº¦ä¸º0
                    $(this).parent().siblings().find(".son_total").css("height","0px")
                    $(this).parent().siblings().find(".navbar").removeClass("active")
                    //ç®­å¤´
                    $(this).parent().siblings().find(".arrowhead").css("transform","rotate(45deg)")
                    $(this).addClass("active")
                }

            })
            $(".postion-li").mouseover(function () {
                $("#wechat").css("display","block")
            })
            $(".postion-li").mouseout(function () {
                $("#wechat").css("display","none")
            })
            $("#wechat_word").click(function () {
                $("#phone_wechat").css({"display":"block"})
            })
            $(document).click(function (e) {
                var $target = $(e.target);
                if (!$target.is('#phone_wechat')&&!$target.is('#wechat_word')){
                    $('.phone_wechat').css({"display":"none"});
                }
            });

        }


    </script>
