<script>
// Verifica si es un dispositivo de escritorio
function isDesktop() {
    return window.matchMedia('(min-width: 1024px)').matches;
}

// Función para mostrar u ocultar el div según el tipo de dispositivo
function toggleDivVisibility() {
    var div = document.getElementById('adsDesktop'); // Reemplaza 'miDiv' con el ID de tu div
    
    if (isDesktop()) {
        div.style.display = 'block'; // Muestra el div en dispositivos de escritorio
    } else {
        div.style.display = 'none'; // Oculta el div en dispositivos móviles
    }
}

// Llama a la función al cargar la página y cuando se redimensiona la ventana
toggleDivVisibility();
window.addEventListener('resize', toggleDivVisibility);

</script>
