  <div class="content">
    
    <article id="node-2873" class="node node-panel clearfix" about="/node/2873" typeof="sioc:Item foaf:Document">
      <div class="panel-flexible panels-flexible-515 clearfix" id="full-width">
<div class="panel-flexible-inside panels-flexible-515-inside">
<div class="panels-flexible-region panels-flexible-region-515-center panels-flexible-region-first panels-flexible-region-last">
  <div class="inside panels-flexible-region-inside panels-flexible-region-515-center-inside panels-flexible-region-inside-first panels-flexible-region-inside-last">
<div class="content full-view panel-pane pane-views pane-home-slider"  >
  
      
  
  <div class="pane-content">
    <div class="view view-home-slider view-id-home_slider view-display-id-block home-slider view-dom-id-6c06027a5ef38b940f2709be550c4e1e">
        
  
  
      <div class="view-content">
      
  <div class="skin-default">
    
    <div id="flexslider_views_slideshow_main_home_slider-block_1" class="flexslider_views_slideshow_main views_slideshow_main"><div class="flex-nav-container">
  <div class="flexslider">
    <ul id="flexslider_views_slideshow_home_slider-block_1" class="flexslider-views-slideshow-main-frame slides">
        <li class="flexslider-views-slideshow-main-frame-row flexslider_views_slideshow_slide views-row-1 views-row-odd">
      
  <div class="views-field views-field-field-gallery-image">        <div class="field-content"><div class="slide-image" style="background-image: url('/sites/default/files/black-friday-lp_1.png')"></div></div>  </div>  
