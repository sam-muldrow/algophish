<script>
    $(function () {
        //setCookie("lang","",1);
        var localLang = getCookie("lang");
        if (localLang != null && localLang != "") {
            $('input[name="optLang"][value="' + localLang + '"]').prop('checked', true);
        }
    });
    function pageLoadonLangSelect() {

        var event = document.getElementById("LangSel");
        var language = event.options[event.selectedIndex].value;
        var url = window.location.href;
        // url = url.split('?')[0];
        var stringCutted = url.split('/').pop();

        if (language == "") { url = "https://www.easemytrip.com/"; }
        else {
            //url = url.replace(stringCutted, language)
            url = "https://www.easemytrip.com/lang/" + language;
        }
        window.location.href = url;
    }
    function pageLoadonLangSelectNew(lang) {
        var language = document.getElementById("rb" + lang).value;
        //var language = event.options[event.selectedIndex].value;
        var url = window.location.href;
        // url = url.split('?')[0];
        var stringCutted = url.split('/').pop();
        setCookie("lang", language, 360);
        if (language == "") {
            //url = "https://www.easemytrip.com/";
            url = window.location.href.split('?')[0];
        }
        else {
            //url = url.replace(stringCutted, language)
            //url = "https://www.easemytrip.com?lang="+language;

            url = window.location.href.split('?')[0] + "?lang=" + language;

        }
        window.location.href = url;
    }
    function SetLangSelect() {
        var url = window.location.href;
        var lang = url.split('/').pop();
        document.getElementById("LangSel").value = lang;
    }
</script>
