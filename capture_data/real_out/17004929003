	<script>
        function loadExOffrs(tabType) {
            $("._offmenuD").removeClass('activetb');
            $("#" + tabType).addClass('activetb');
            $("#divExOff").find('._OffrScrll').hide();
            if ($("#divExOff").find('.' + tabType).length > 0) {
                $("#divExOff").find('.' + tabType).show();
            } else {
                $.ajax({
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    dataType: "text",
                    url: "/Desktop/bindDeskExcluciveOffers",
                    aysnc: false,
                    data: "{tab:'" + tabType + "'}",
                    success: function (response) {
                        if (response != null && response != '') {
                            $("#divExOff").append(response);
                        }
                    },
                    beforeSend: function (XMLHttpRequest) {

                    },
                    error: function (xmlHttpRequest, status, err) {

                    }
                });
            }
        }

