			<div rel="business" class="load-more load-more-business">
	<p><i class="fas fa-chevron-circle-down"></i> LOAD MORE</p>
</div>
<script type="module">
	import { loadMoreNg, setAdsVisibility } from "/wp-content/client-mu-plugins/frontend/dom.js";
	import { sendPageView } from "/wp-content/client-mu-plugins/frontend/event.js";

	jQuery(window).ready(function() {
		jQuery(".load-more-business").on("click", function() {
			const newPage = 1;
			loadMoreNg(jQuery(this), 'business', newPage, true)
			.then(() => {
				try {
					sendPageView(newPage);
				} catch (err) {
					console.log("Failed to send load more event");
					console.warn(err);
				}
			})
			.then(async () => {
				try {
					if (account.is_logged_in == true && account.adfree_option == true) {
						setAdsVisibility(`.article-ad, .ad`);
					} else {
						const ads = await import("/wp-content/client-mu-plugins/frontend/ads.js");
						await ads.setupContentAds();
					}
				} catch (err) {
					console.log("Failed to setup ads");
					console.warn(err);
				}
			})
			.catch(err => console.warn(err));
		});
	});
</script>		
</div>					<div rel="pphh" class="tile-container section-container-pphh compact-view">
		
		
	
	
		
			<h3>More News</h3>
		
			<div rel="pphh" class="load-more load-more-pphh">
	<p><i class="fas fa-chevron-circle-down"></i> LOAD MORE</p>
</div>
