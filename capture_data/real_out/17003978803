<script>
    jQuery(".slideBox").slide({
        mainCell: ".bd ul",
        autoPage: "<li><a></a></li>",
        titCell: ".hd ul",
        effect: "left",
        autoPlay: true
    });

    jQuery(".doctors").slide({
        mainCell: ".doctors-list",
        effect: "left",
        pnLoop: false,
        prevCell: '.pre',
        nextCell: '.next',
        autoPlay: true
    });
    // tab切换
    $('.Js_Tab').on('click', function(){
        $(this).parents('.Js_TabBox').find('.Js_Tab').removeClass('on');
        $(this).addClass('on')
        $('.' + $(this).attr('data-all')).hide();
        $('.' + $(this).attr('data-content')).show();

        var fn = $(this).attr('data-fn');
        if(fn){
            $(this).removeAttr('data-fn');
            getLists({
                fn: fn,
                contentClass: $(this).attr('data-content'),
                cid1: $(this).attr('data-cid1'),
                cid2: $(this).attr('data-cid2')
            });
        }
    });

    function getLists(opt){
        if(opt.fn == 'ask'){
            var $content = $('.' + opt.contentClass);
            if($content.hasClass('loading')) return false;
            $content.addClass('loading');
            $.get('/fhwwwpcindex/ajax/asklist/'+ opt.cid1 +'_'+ opt.cid2 +'_1_5', function(res){
                console.log(res)
                if(res.errno == 0){
                    var str = '';
                    $content.removeClass('loading');
                    if(res.data && res.data.questions && res.data.questions.length > 0){
                        for(var i=0; i<res.data.questions.length; i++){
                            str += askHtml(res.data.questions[i], i, res.data.questions.length);
                        }
                    }else{
                        str = '<p class="tac f16 fc9 pt50">暂无数据</p>';
                    }
                    
                    $content.html(str)
                }
            })
        }else{
            var $content = $('.' + opt.contentClass);
            if($content.hasClass('loading')) return false;
            $content.addClass('loading');
            $.get('/fhwwwpcindex/ajax/diselist/'+ opt.cid1 +'_'+ opt.cid2 +'_1_4', function(res){
                console.log(res)
                if(res.errno == 0){
                    var str = '';
                    $content.removeClass('loading');
                    if(res.data && res.data.articles && res.data.articles.length > 0){
                        for(var i=0; i<res.data.articles.length; i++){
                            str += diseHtml(res.data.articles[i], i);
                        }
                    }else{
                        str = '<p class="tac f16 fc9 pt50">暂无数据</p>';
                    }
                    
                    $content.html(str)
                }
            })
        }
    }

    function diseHtml(list, i){
        var str = '';
        if(list){
            str = '<a href="' +list.url+ '" target="_blank" class="fll img-hover card">'
                    +'<div class="img"><img class="w100" src="' + list.litpic + '"></div>'
                    +'<div class="des posr">'
                        +'<h5 class="f16 h5 fn wbwr hidden2 mb10">' + list.title + '</h5>'
                        +'<p class="f14 fc9">' +dateFtt('yyyy-MM-dd', list.ctime) + '</p>'
                        //+'<span class="flag f14 fcf tac">脑梗塞</span>'
                    +'</div>'
                +'</a>'
        }
        return str;
    }

    function askHtml(list, i, ln){
        var str = '';
        if(list){
            str = '<div class="doctor-ask-lists clr f14 '+ (i == ln-1 ? "" : "bb1-dashed") +'">'
                    +'<a href="' +list.user_url+ '" target="_blank" class="fll doctor-side posr">'
                        +'<p class="img db img-hover"><img class="w100 h100" src="' +list.avatar+ '"></p>'
                        +'<p class="fn ellipsis db"><span class="f18 mr5">' +list.user_nickname+ '</span><span class="fc9">' +list.holderofanoffice+ '</span></p>'
                        +'<p class="ellipsis mb5 fc9">' +list.hospital+ '</p>'
                        +'<p class="bottom-flag"><span>立即挂号</span></p>'
                    +'</a>'
                    +'<div class="flr ask-side">'
                        +'<a href="' +list.pc_url+ '" target="_blank" class="f16 fn db fc3 ellipsis mb5 h4"><span class="fcb">Q</span>：' +list.title+ '</a>'
                        +'<a href="' +list.pc_url+ '" target="_blank" class="des db hidden2 mb5 fc9">A：' +list.answer_content+ '</a>'
                        +'<p class="f12 fc9"><time>' + dateFtt('yyyy-MM-dd hh:mm:ss', list.question_time) + '</time></p>'
                    +'</div>'
                +'</div>'
        }
        return str;
    }





// dateFtt("yyyy-MM-dd hh:mm", list.ctime)
function dateFtt(fmt,date)   { 
  date = new Date(date*1000); 
  var o = {   
    "M+" : date.getMonth()+1,                 //月份
    "d+" : date.getDate(),                    //日
    "h+" : date.getHours(),                   //小时
    "m+" : date.getMinutes(),                 //分
    "s+" : date.getSeconds(),                 //秒
    "q+" : Math.floor((date.getMonth()+3)/3), //季度
    "S"  : date.getMilliseconds()             //毫秒
  };   
  if(/(y+)/.test(fmt)){
      fmt=fmt.replace(RegExp.$1, (date.getFullYear()+"").substr(4 - RegExp.$1.length));
      for(var k in o){
        if(new RegExp("("+ k +")").test(fmt)){
            fmt = fmt.replace(RegExp.$1, (RegExp.$1.length==1) ? (o[k]) : (("00"+ o[k]).substr((""+ o[k]).length))); 
        }
      }
  }
  return fmt;
}
