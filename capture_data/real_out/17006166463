<script>
    function getCookie(cname) {
      var name = cname + "=";
      var ca   = document.cookie.split(';');
      for (var i = 0; i < ca.length; i++) {
        var c = ca[i].trim();
        if (c.indexOf(name) === 0) return c.substring(name.length, c.length);
      }
      return "";
    }
    function disqus_config() {
      this.callbacks.onNewComment = [function (comment) {
        dataLayer.push({ 'event': 'commentsTrackingEvent', 'commentText': comment.text, 'commentID': comment.id });
      }];
      this.callbacks.onIdentify   = [function (ident) {
        var GTMCookie = function checkCookie(cname) {
          var cookie = getCookie(cname);
          return (cookie === "") ? false : true;
        }('useridp');
        if (!GTMCookie) {
          document.cookie = "useridp=" + ident + "; expires=Fri, 11 Jul 2031 15:35:13 GMT;path=/";
        }
