    $('.ui-controller').on('click', function(e){
        controller($(this));
        return false;
    });

    $('body').on('click', function(e) {
        $('.ui-controller, .projects__items, .hamburger_projects-switcher-js, .userbar-user, .action-bars__user-tools, .header__user-nav, .userbar-user__bars, .hamburger_userbar-user-switcher-js, .change-lang__lang-items').removeClass('active');
    });

    function controller(dataNav){
        var domElements = dataNav.data();
        $(domElements.remove).removeClass('active');
        dataNav.add($(domElements.target)).stop().toggleClass('active');
    }

    var userbar = $('.userbar-switcher'),
        actionbar = $('.header__controls-dropdown'),
        hamburgerTrigger = $('.header-nav__burger'),
        hamburger = $('.hamburger'),
        navbar = $('.header-nav');


    userbar.click(function(){
        userbar.add(actionbar).toggleClass('active');
        hamburger.add(navbar).removeClass('active');
    });
    hamburgerTrigger.click(function(){
        hamburger.add(navbar).toggleClass('active');
        userbar.add(actionbar).removeClass('active');
    });

    jQuery(function() {
        wsSubscribe("");

        $('<audio id="chatAudio"><source src="/images/common/notify.ogg" type="audio/ogg"><source src="/images/common/notify.mp3" type="audio/mpeg"><source src="/images/common/notify.wav" type="audio/wav"></audio>').appendTo('body');

        wsRegisterHandler("webnotify", function(data) {
            webnotify(data);
        });

        wsRegisterHandler("moneybets", function(data) {
            $(".sys-userwallet").html(data);
        });
    });

    function webnotify(data){
        data = JSON.parse(data);

        data.click = false;
        data.iconUrl = '/images/logo_cs.png';
        if(data.url){
            data.click = function(){
                var win = window.open(data.url, '_blank');
                win.focus();
            };
        }

        data.open = function() {
            //console.log('Уведомление показано :-)');
        };

        jQuery.jGrowl(data.text, {
            sticky: true,
            position: "bottom-right",
            themeState: "highlight",
            click: data.click,
            open: data.open,
        });
        $('#chatAudio')[0].play();
    }
</script>        </div>

        
