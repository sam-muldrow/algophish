            this.renderBets();
        }
    };

    var matchWrappers = {
        live: $(".sys-games-live"),
        next: $(".sys-games-next"),
        past: $(".sys-games-past")
    };


    $(function () {
        _bets.init();

        if (typeof makeAgreementDialogAlive === 'function') {
            makeAgreementDialogAlive();
        }

        wsSubscribe("bets");

        WsEvents.cancelBet();
        WsEvents.placeBet();
        WsEvents.matchStatus();
        WsEvents.updateBets();

        wsRegisterHandler("newmatch_cs", function(data) {
            if (data.m_parent || data.m_mapof) return;

            var bet = normalizeBet(data);
            bet.sumbets = {};
            bet.user_currency = USER_CURRENCY;

            Object.observe(bet, betLoungeIndexObserver);

            _bets.bets.push(bet);
            _bets.createNewBlock(bet);
            _bets.handleNoActiveMessage();
        });

        WsEvents.updateMatch("cs", []);
        WsEvents.currencyUpdate(_bets.bets);


        $('.sys-tab').on('click', function (event) {
            event.preventDefault();

            var e = $(this);

            if (e.hasClass('active')) {
                return;
            }

            $('.sys-tab').removeClass('active');
            e.addClass('active');

            var action = e.hasClass('sys-total-tab') ? 'future' : 'past';

            $(".sys-games").html('');

            if (action === 'future') {
                matchWrappers.next.addClass('active');
                matchWrappers.live.addClass('active');
                matchWrappers.past.removeClass('active');

                $(".sys-bets-block").removeClass("history").addClass("total");
                setCookie("page-tab", "1", 7);

            } else if (action === 'past') {
                matchWrappers.next.removeClass('active');
                matchWrappers.live.removeClass('active');
                matchWrappers.past.addClass('active');

                $(".sys-bets-block").removeClass("total").addClass("history");
                setCookie("page-tab", "2", 7);

            }

            _bets.renderBets();
        });

        (function datetime_update() {
            for (key in _bets.bets) {

                var bet = _bets.bets[key];

                var block = $(".sys-betting[data-id="+bet['m_id']+"]");
                var timestamp = block.find(".sys-time-left").data('timestamp');

                if (bet.m_status == 0) {
                    block.find(".sys-time-left").html($.trim(_dt.timestampToLeftString(timestamp)));
                } else {
                    block.find(".sys-time-left").hide();
                }
            }

            setTimeout(datetime_update, 1000);
        })();
    });
</script>


