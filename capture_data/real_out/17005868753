<script src="/css/js/slick.min.js"></script>
<script src="/css/js/jquery.cookie.js"></script>

<script>

	//マウスでの操作
	jQuery(window).on('mouseover', function() {
		//aタグ以外でもフォーカスが当たるように設定してたのを外す
		jQuery('.predict .list-wrap ul li').removeAttr('tabIndex','0');

	});

	//キーボードでの操作
	jQuery(window).on('keydown', function(e) {

		// 現在のフォーカスを取得
		var seach_box = jQuery('#IS_SASP_TARGET.header-container .search-container input[type=text]');
		var seach_box_fc = seach_box.is(':focus');
		var sug = jQuery('.predict .list-wrap ul li');
		var sug_i = sug.index(jQuery(':focus'));

		var word;
		var track_id;
		var cid;

		//aタグ以外でもフォーカスがあたるようにする
		sug.attr('tabIndex','0');

		//サジェストリストが表示されている場合
		if(jQuery('.predict').css('display') == 'block'){

			//矢印キー（↓）
			if(e.keyCode === 40) {

				//サジェストリストを表示させておく
				jQuery('.predict').addClass('show');

				//サジェストリストにフォーカスしている場合
				if(sug_i >= 0) {

					for (var i = 0; i < sug.length; i++) {

						//サジェストリストの最後の要素にフォーカスしている場合
						if(sug_i === sug.length - 1){

							//検索ボックスに移動
							seach_box.focus();

							//クッキーに保持しているキーワードを検索ボックスに表示
							var seach_txt = jQuery.cookie("seach_txt");
							seach_box.val(seach_txt);

						//サジェストリストにフォーカスしているかつ、サジェストリストの最後の要素ではない場合
						} else if(i === sug_i && i < sug.length - 1) {

							//次のサジェストに移動
							sug.eq(i + 1).focus();

							//サジェストリストのキーワードを検索ボックスに表示
							word = jQuery('li:focus p span.suggest').text();
							seach_box.val( word );
						}
					}

				} else {

					//キーワードをクッキーに保持
					searchTxtToCookie();

					//サジェストリストの一番最初にフォーカスする
					sug.eq(0).focus();

					//サジェストリストのキーワードを検索ボックスに表示
					word = jQuery('li:focus p span.suggest').text();
					seach_box.val( word );

				}

				return false;

			//矢印キー（↑）
			}else if(e.keyCode === 38) {

				//サジェストリストを表示させておく
				jQuery('.predict').addClass('show');

				//サジェストリストにフォーカスしている場合
				if(sug_i >= 0) {

					for (var i = 0; i < sug.length; i++) {

						//サジェストリストの最初の要素にフォーカスしている場合
						if(sug_i === 0){

							//検索ボックスに移動
							seach_box.focus();

							//クッキーに保持しているキーワードを検索ボックスに表示
							var seach_txt = jQuery.cookie("seach_txt");
							seach_box.val(seach_txt);

						//サジェストリストにフォーカスしているかつ、サジェストリストの最初の要素ではない場合
						} else if(i === sug_i && i > 0) {

							//サジェストリストの一番最後にフォーカスする
							sug.eq(i - 1).focus();

							//サジェストリストのキーワードを検索ボックスに表示
							word = jQuery('li:focus p span.suggest').text();
							seach_box.val( word );

						}
					}

				} else {

					//キーワードをクッキーに保持
					searchTxtToCookie();

					//サジェストリストの一番最後にフォーカスする
					sug.eq(-1).focus();

					//サジェストリストのキーワードを検索ボックスに表示
					word = jQuery('li:focus p span.suggest').text();
					seach_box.val( word );

				}

				return false;

			//Enterキー
			}else if( e.keyCode === 13 ){

				if(!seach_box_fc){

					word = jQuery('li:focus p span.suggest').text();
					track_id = jQuery('li:focus p span.suggest').data('tid');
					cid = jQuery('li:focus p span.s_cid').data('cid');

					jQuery('#IS_SASP_TARGET.header-container .search-container input[type=text]').val( word );
					jQuery('.header-container form').submit();

					//検索結果遷移
					// カテゴリサジェスト　ただし、ブランドカテはブランドインデックスのため除外とする
					if ( cid && cid != "100000001" ) {
						location.href = "/_l?_CODE=%82%A0&_SRC=topsp1_sr&r=fc&gq=1&mem=1&q=" + word + "&c=" + cid + "&track_id=" + track_id ;
					} else {
						location.href = "/_l?_CODE=%82%A0&_SRC=topsp1_sr&r=fc&gq=1&mem=1&q=" + word + "&track_id=" + track_id ;
					}

				}

			//上下キー・Enterキー以外
			} else {

				//サジェストリストを非表示にする
				jQuery('.predict').removeClass('show');

				//検索ボックスにフォーカスする
				seach_box.focus();
			}
		}
	});

	//検索ボックスに入力したキーワードをクッキーに保持する関数
	function searchTxtToCookie(){

		jQuery('#IS_SASP_TARGET.header-container .search-container input[type=text]').change(function(){

			var seach_txt = jQuery(this).val();

			jQuery.cookie("seach_txt", seach_txt);

		});
	}

	jQuery( document ).ready(function(){

		// #13967【編集対応依頼】CAMPワンタグ設置依頼
		jQuery(".eraser iframe").attr("height","0");

		var slick_elm = jQuery(".slick-content-wrap");

		// slickさせる中身をランダムにする
		random( slick_elm );

		initSlick( slick_elm );

		// slickさせる中身をランダムにする
		function random( obj ){
			obj.children().sort(function(){
				return Math.round(Math.random()) - 0.5;
			}).each(function(){
				jQuery(this).appendTo( obj );
			});
		}

		function initSlick( tags ){
			// ランダムにしたものをslick init
			tags.slick({
				slidesToShow: 4, //スライドが見える数
				slidesToScroll: 1, //スライドがスクロールする数
				infinite: true, //無限スクロール
				dots: true,
				autoplay: true
			});
		}

	});

	jQuery( window ).load( function(){
		jQuery(".slider-container > .inner").removeClass("loading");
		jQuery(".slick-content-wrap").animate({ opacity: 1 },1000 );
	});
</script>
</body>
</html>
