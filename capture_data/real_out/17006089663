    const tags = [...document.querySelectorAll('.entry-tags a')]?.map(({ innerText }) => innerText).toString();
    const wordCount = [...document.querySelectorAll('.entry-content p:not(.pmpro_content_message p)')]?.map(({ innerText }) => innerText).join(' ').split(' ').length;
  
	const arguments = {
		'event': 'mg_article_view',
    	'author': author,
        'articletitle': articleTitle,
        'category': category,
        'content':'registered',
        'access': access,
        'subslevel': '15',
        'subsonly': subsOnly,
        'memberid': 'memberId',
        'memberName': memberName,
        'date': date,
        'tags': tags,
        'wordcount': wordCount
	};
	
	dataLayer.push(arguments);
