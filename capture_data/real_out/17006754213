	jQuery(document).ready(function(){
		jQuery(".read_more_bio").click(function(e){
			e.preventDefault();
			jQuery(this).css("display","none");
			jQuery(this).closest(".grp_bios").find(".caz-bio").css("display","block");
		});
	});
