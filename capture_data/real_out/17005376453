<script>
        window.renderLoadMoreBillboard = function (divId) {
            gas.log('Called window.renderLoadMoreBillboard', divId);

            if (typeof googletag.display !== "function") {
                return false;
            }

            var ad = document.getElementById(divId);
            var format;
            try {
                var formatJson = window.isMobileBreakpoint() ? ad.dataset.mobileFormat : ad.dataset.desktopFormat;
                format = JSON.parse(formatJson);
            } catch (e) {
                format = null;
            }
            gas.log('renderLoadMoreBillboard Format', ad, formatJson, format);

            if (format && ad) {
                ad.dataset.name = format.name;
                googletag.cmd.push(function () {
                    var slot = googletag.defineSlot(format.slot, format.sizes, divId).addService(googletag.pubads());

                    format.targets.forEach(function (target) {
                        var key = target.key || null;
                        var value = target.value || null;
                        if (key && value) {
                            if (value.indexOf(';') !== -1) {
                                value = value.split(';');
                            }
                            slot.setTargeting(key, value);
                        }
                    });

                    var mapping = gas.getSizeMapping(format);
                    if (mapping) {
                        gas.log('SizeMapping', mapping);
                        slot.defineSizeMapping(mapping);
                    }
                    googletag.display(divId);
                    googletag.cmd.push(function () {
                        googletag.pubads().refresh([slot]);
                    });
                });
                return true;
            } else {
                gas.error('Not found format in window.renderLoadMoreBillboard', formatName);
            }
            return false;
        }
        window.renderLoadMoreRectangle = function (divId) {
            gas.log('Called window.renderLoadMoreRectangle', divId);

            if (typeof googletag.display !== "function") {
                return false;
            }

            if(window.isMobileBreakpoint()) {
                return false;
            }

            var ad = document.getElementById(divId);
            var format;
            try {
                format = JSON.parse(ad.dataset.format);
            } catch (e) {
                format = null;
            }
            gas.log('renderLoadMoreRectangle Format', format);

            if (format && ad) {
                ad.dataset.name = format.name;
                googletag.cmd.push(function () {
                    var slot = googletag.defineSlot(format.slot, format.sizes, divId).addService(googletag.pubads());

                    format.targets.forEach(function (target) {
                        var key = target.key || null;
                        var value = target.value || null;
                        if (key && value) {
                            if (value.indexOf(';') !== -1) {
                                value = value.split(';');
                            }
                            slot.setTargeting(key, value);
                        }
                    });

                    var mapping = gas.getSizeMapping(format);
                    if (mapping) {
                        gas.log('SizeMapping', mapping);
                        slot.defineSizeMapping(mapping);
                    }
                    googletag.display(divId);
                    googletag.cmd.push(function () {
                        googletag.pubads().refresh([slot]);
                    });
                });
                return true;
            } else {
                gas.error('Not found format in window.renderLoadMoreBillboard', name);
            }
            return false;
        }
        window.renderLoadMoreScript = function (divId) {
            gas.log('Called window.renderLoadMoreScript', divId);

            if (typeof googletag.display !== "function") {
                return false;
            }

            var ad = document.getElementById(divId);
            var format;
            try {
                format = window.isMobileBreakpoint() ? JSON.parse(ad.dataset.mobileFormat) :  JSON.parse(ad.dataset.format);
            } catch (e) {
                format = null;
            }
            gas.log('renderLoadMoreScript Format', format);

            if (format && ad) {
                ad.dataset.name = format.name;
                googletag.cmd.push(function () {
                    var slot = googletag.defineSlot(format.slot, format.sizes, divId).addService(googletag.pubads());

                    format.targets.forEach(function (target) {
                        var key = target.key || null;
                        var value = target.value || null;
                        if (key && value) {
                            if (value.indexOf(';') !== -1) {
                                value = value.split(';');
                            }
                            slot.setTargeting(key, value);
                        }
                    });

                    var mapping = gas.getSizeMapping(format);
                    if (mapping) {
                        gas.log('SizeMapping', mapping);
                        slot.defineSizeMapping(mapping);
                    }
                    googletag.display(divId);
                    googletag.cmd.push(function () {
                        googletag.pubads().refresh([slot]);
                    });
                });
                return true;
            } else {
                gas.error('Not found format in window.renderLoadMoreScript', name);
            }
            return false;
        }
    </script>

