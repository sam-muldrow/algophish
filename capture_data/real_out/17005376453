<script type="application/javascript">
    window.adDebug = false;

    window.commercialBreakConfig = {"renderDaily":3,"interval":5400,"renderDailyForLogged":0,"intervalForLogged":86400,"cookieNameDailyCounter":"commercialBreakDailyCounter","cookieNameBlockade":"commercialBreakBlockade","cookieNameBlockadeExpired":"commercialBreakBlockadeExpired"};

    window.showAdditionalMarketingBlock = function (formatName, height) {
        height = parseInt(height);
        if (formatName === 'slot_right_1' && height <= 350) {
            console.log('show additional marketing block', formatName, height);
            var mainTopicAdditionalMarketingBlock = document.getElementById('mainTopicAdditionalMarketingBlock');
            if (mainTopicAdditionalMarketingBlock) {
                mainTopicAdditionalMarketingBlock.style.display = "block";
            }
        }
    };

    window.contentBodySectionAdvertFallback =
        '<div class="fallbackContainer">'
        + '<div class="row">'
        + '<div class="w-81 lg:block hidden relative">'
        + '</div>'
        + '<div class="col--fill order-first lg:border--right">'
        + '<div style="height:24px;"></div>'
        + '</div>'
        + '</div>'
        + '</div>';

    window.hideAdvertLabel = function (formatName, divId) {
        window.showAdditionalMarketingBlock(formatName, 0);
        var ad = document.getElementById(divId);
        var label = ad.closest('.advertOuter').querySelector('.advertTitle');
        var contentBodyInnerAdvert = ad.closest('.contentBodyInnerAdvert');
        var contentBodySectionAdvert = ad.closest('.contentBodySectionAdvert');
        var categorySectionAdvert = ad.closest('.categorySectionAdvert');

        if (ad) {
            ad.style.display = 'none';
        }
        if (label) {
            label.style.display = 'none';
        }

        if (contentBodyInnerAdvert) {
            contentBodyInnerAdvert.innerHTML = "";
            contentBodyInnerAdvert.classList.remove("inner--ad--breaker");
        }

        if (contentBodySectionAdvert) {
            contentBodySectionAdvert.innerHTML = window.contentBodySectionAdvertFallback;
            contentBodySectionAdvert.classList.remove("section--ad--breaker");
        }

        if(categorySectionAdvert)  {
            categorySectionAdvert.innerHTML = "";
            categorySectionAdvert.classList.add("--category--no-advert");
        }
    }

    window.checkAdBlock = function () {
        var ads = document.querySelectorAll(".advertElement");
        if (!ads.length) {
            return;
        }

        var isAdblock = true;
        Array.prototype.slice.call(ads).map((element) => {
            if (element.offsetHeight) {
                isAdblock = false;
            }
        });

        return isAdblock;
    };

    window.getBreakpointName = function () {
        var breakpoints = [{"name":"BP-XS","width":0},{"name":"BP-SM","width":489},{"name":"BP-MD","width":705},{"name":"BP-LG","width":1029},{"name":"BP-XL","width":1353}];
        var width = window.innerWidth;
        var breakpointName = '';
        for (var breakpoint of breakpoints) {
            if (breakpoint.hasOwnProperty('width') && width >= breakpoint.width) {
                breakpointName = breakpoint.name;
            }
        }
        return breakpointName;
    }

    window.isMobileBreakpoint = function () {
        var mobileBreakpoints =["BP-XS","BP-SM","BP-MD"];
        var breakpoint = window.getBreakpointName();
        return mobileBreakpoints.indexOf(breakpoint) !== -1;
    }

    window.checkCommercialBreak = function () {
        var commercialBrake = false;
        if (window.getCookie(window.commercialBreakConfig.cookieNameBlockade) !== null) {
            var timestamp = window.getCookie(window.commercialBreakConfig.cookieNameBlockade);
            var difference = new Date().getTime() - timestamp;
            if (difference <= 60000) {
                var expiredTimestamp = window.getCookie(window.commercialBreakConfig.cookieNameBlockadeExpired);
                if (expiredTimestamp) {
                    var expiredAt = new Date();
                    expiredAt.setTime(expiredTimestamp);
                } else {
                    var expiredAt = new Date();
                    expiredAt.setHours(24, 59, 59, 1);
                }
                window.setCookie(
                    window.commercialBreakConfig.cookieNameBlockade,
                    (timestamp - 60000),
                    expiredAt
                )
                commercialBrake = true;
            }
        }
        return commercialBrake;
    }

</script>
<script async src="https://cdn.rp.pl/brio-cdn/fonts/prebid8.19.0_SG.js"></script>
<script async src="//securepubads.g.doubleclick.net/tag/js/gpt.js"></script>
<script>
        window.renderAdvert = function (name, divId) {
            gas.log('Called renderAdvert', name);

            var ad = document.getElementById(divId);
            if (!ad) {
                gas.error('Not found divId in window.renderAdvert', name);
                return;
            }

            var container = ad.closest('.advertOuter');

            var format = gas.getFormatByName(name);
            if (!format) {
                if (container) {
                    var contentBodyInnerAdvert = container.closest('.contentBodyInnerAdvert');
                    if (contentBodyInnerAdvert) {
                        contentBodyInnerAdvert.classList.remove('inner--ad--breaker')
                    }

                    var contentBodySectionAdvert = container.closest('.contentBodySectionAdvert');
                    if (contentBodySectionAdvert) {
                        contentBodySectionAdvert.innerHTML = window.contentBodySectionAdvertFallback;
                        contentBodySectionAdvert.classList.remove("section--ad--breaker");
                    }

                    var categorySectionAdvert = ad.closest('.categorySectionAdvert');
                    if(categorySectionAdvert)  {
                        categorySectionAdvert.innerHTML = "";
                        categorySectionAdvert.classList.add("--category--no-advert");
                    }

                    container.remove();
                }
                gas.error('Not found format in window.renderAdvert', name);
                return;
            }

            var desktopAdvert = container.querySelector('.desktopAdvert');
            var mobileAdvert = container.querySelector('.mobileAdvert');
            if (window.isMobileBreakpoint() && mobileAdvert) {
                gas.log('Render mobile format', name)
                if (desktopAdvert) {
                    desktopAdvert.remove();
                }
                window.addEventListener("advertCanBeRendered", function(){
                    gas.log('advertCanBeRendered', mobileAdvert.id);
                    googletag.cmd.push(function () {
                        googletag.display(mobileAdvert.id);
                    });
                });
            } else {
                gas.log('Render desktop format', name)
                if (mobileAdvert) {
                    mobileAdvert.remove();
                }
                window.addEventListener("advertCanBeRendered", function(){
                    gas.log('advertCanBeRendered', ad.id);
                    googletag.cmd.push(function () {
                        googletag.display(ad.id);
                    });
                });
            }
        };
    </script>
<script>

        var gas = {
            formats: [],
            lazyLoadOptions: {},
            prebidConfig: {},
            bidderSettings: {},
            isContentPage: false,
            contentConnectedFormats: {},
            definedSlots: [],
            amazonPubID: null,
            amazonTimeout: 2000,

            init: function () {
                window.googletag = window.googletag || {cmd: []};

                function doTCFStuff() {
                    if (typeof __tcfapi !== 'function') {
                        console.log('tcfapi', false);
                        setTimeout(doTCFStuff, 50);
                    } else {
                        __tcfapi('addEventListener', 2, function (tcData, success) {
                            if (success) {
                                var cmpStatus = tcData.eventStatus;
                                gas.log('tcData', tcData)
                                if ((cmpStatus === 'tcloaded' && tcData.tcString) || cmpStatus === 'useractioncomplete') {
                                    gas.log('tcDataCanRun');
                                    __tcfapi('removeEventListener', 2, function (success) {
                                        if (success) {
                                            gas.log('event listener removed');
                                        }
                                    }, tcData.listenerId);
                                    gas.start();
                                }
                            }
                        });
                    }
                }
                doTCFStuff();
            },

            start: function () {

                gas.load();

                setTimeout(function () {
                    var isAdblock = window.checkAdBlock();
                    console.log('check if is adblock ' + isAdblock);
                    if (isAdblock) {
                        window.showAdditionalMarketingBlock('slot_right_1', 0);
                    }
                }, 1000);
            },

            load: function () {
                googletag.cmd.push(function () {

                    Array.prototype.slice.call(gas.formats).map(function (format) {
                        var definedSlot = googletag.defineSlot(format.slot, format.sizes, format.divId).addService(googletag.pubads());
                        var mapping = gas.getSizeMapping(format);
                        if (mapping) {
                            gas.log('SizeMapping', mapping);
                            definedSlot.defineSizeMapping(mapping);
                        }
                        gas.definedSlots[format.name] = {
                            id: format.divId,
                            slot: definedSlot,
                            place: format.slot
                        };
                    })

                    gas.log('Init DFP', gas.definedSlots);

                    googletag.pubads().disableInitialLoad();

                    googletag.pubads().enableSingleRequest();
                    googletag.pubads().collapseEmptyDivs();

                    const event = new CustomEvent("advertCanBeRendered");
                    window.dispatchEvent(event);

                    window.adTargets.forEach(function (target) {
                        var key = target.key || null;
                        var value = target.value || null;
                        if (key && value) {
                            if (value.indexOf(';') !== -1) {
                                value = value.split(';');
                            }
                            gas.log('Set target', key, value);
                            googletag.pubads().setTargeting(key, value);
                        }
                    });

                    googletag.pubads().addEventListener('slotRenderEnded', function (event) {
                        var divId = event.slot.getSlotElementId();
                        var element = document.getElementById(divId);

                        if (!element) {
                            gas.log('slotRenderEnded No found dom element', divId);
                            return;
                        }

                        var formatName = element.dataset.name

                        if (event.isEmpty) {
                            console.log('slotRenderEnded Event- No Advert', formatName, divId);
                            gas.error('slotRenderEnded No Advert', formatName, divId);
                            window.hideAdvertLabel(formatName, divId);
                        } else {
                            var container = element.closest('.advertOuter');
                            var label = container.querySelector('.advertTitle');
                            label.style.display = 'block';
                            label.style.opacity = 1;
                        }

                        if (formatName === 'slot_right_1') {
                            if (element) {
                                console.log('GAM - Reklama slot_right_1 zwrocila wysokość', element.offsetHeight);
                                var height = element.offsetHeight;
                                if (height > 1) {
                                    window.showAdditionalMarketingBlock(formatName, height);
                                }
                            }
                        }
                    });

                    googletag.enableServices();

                    var disabledLazyLoadDFPs = [];
                    Array.prototype.slice.call(gas.formats).map(function (format) {
                        if (!format.lazyLoad) {
                            if (!format.hb) {
                                disabledLazyLoadDFPs.push(format.name)
                            }
                        }
                    })
                    gas.log('Load disabledLazyLoadDFPs', disabledLazyLoadDFPs)
                    if (disabledLazyLoadDFPs.length) {
                        gas.loadDFP(disabledLazyLoadDFPs);
                    }

                    var disabledLazyLoadHBs = [];
                    Array.prototype.slice.call(gas.formats).map(function (format) {
                        if (!format.lazyLoad) {
                            if (format.hb) {
                                disabledLazyLoadHBs.push(format.name)
                            }
                        }
                    })
                    gas.log('Load disabledLazyLoadHBs', disabledLazyLoadHBs)
                    if (disabledLazyLoadHBs.length) {
                        gas.loadHB(disabledLazyLoadHBs);
                    }

                    gas.startInitLazyLoadAds();
                });
            },

            startInitLazyLoadAds: function() {

                gas.log('START INIT LAZY LOAD EXECUTED')

                if (document.readyState !== 'loading') {
                    startLogic();
                } else {
                    document.addEventListener('DOMContentLoaded', function () {
                        startLogic();
                    });
                }

                function startLogic() {
                    gas.log('START INIT LAZY LOAD RUN')
                    var timestamp = sessionStorage.getItem(
                        "auto_refresh_homepage"
                    );
                    if (!timestamp) {
                        timestamp = Date.now() - 60 * 1000;
                    }

                    const seconds = (Date.now() -timestamp) / 1000;
                    gas.log('SECONDS', seconds);
                    if (seconds < 10) {
                        gas.initLazyLoadInViewPortOnAutoRefreshHP();
                        const activityEvents = [
                            "mousedown",
                            "mousemove",
                            "mouseenter",
                            "mousewheel",
                            "keydown",
                            "scroll",
                            "touchstart",
                            "touchmove",
                            "click",
                        ];

                        var startLazyLoad = function() {
                            activityEvents.forEach(function(eventName) {
                                window.removeEventListener(eventName, startLazyLoad, true);
                            });
                            gas.initLazyLoadAds();
                        }

                        activityEvents.forEach(function (eventName) {
                            window.addEventListener(eventName, startLazyLoad, true);
                        });
                    } else {
                        gas.initLazyLoadAds();
                    }
                    sessionStorage.removeItem("auto_refresh_homepage")
                }
            },

            initLazyLoadInViewPortOnAutoRefreshHP: function () {
                gas.log('INIT LAZY LOAD IN VIEW PORT ON AUTO REFRESH HP')

                var lazyAds = document.querySelectorAll('.lazy-ad');
                gas.log('LazyLoad on auto refresh found elements', lazyAds);

                const elementIsVisibleInViewport = (elem) => {
                    const bounding = elem.getBoundingClientRect();
                    return (
                        bounding.top >= 0 &&
                        bounding.left >= 0 &&
                        bounding.bottom <=
                        (window.innerHeight || document.documentElement.clientHeight) &&
                        bounding.right <=
                        (window.innerWidth || document.documentElement.clientWidth)
                    );
                };

                lazyAds.forEach(ad => {
                    if(elementIsVisibleInViewport(ad)) {
                        if (gas.getDisabledHBFormatNames().includes(ad.dataset.name)) {
                            gas.loadDFP(ad.dataset.name);
                        } else {
                            gas.loadHB(ad.dataset.name);
                        }
                        ad.classList.remove('lazy-ad');
                    }
                });
            },

            initLazyLoadAds: function () {
                gas.log('INIT LAZY LOAD EXECUTED')

                if (gas.isContentPage) {
                    for (var property in gas.contentConnectedFormats) {
                        var baseFormat = gas.getFormatByName(property);
                        if (baseFormat) {
                            var formatName = gas.contentConnectedFormats[property];
                            var format = gas.getFormatByName(formatName);
                            if (format) {
                                var ad = document.getElementById(format.divId);
                                gas.log('LazyLoad connected format', ad);
                                if (ad) {
                                    ad.classList.remove('lazy-ad');
                                }
                            }
                        }
                    }
                }

                var lazyAds = document.querySelectorAll('.lazy-ad');
                gas.log('LazyLoad found elements', lazyAds);
                if ('IntersectionObserver' in window) {
                    var adObserver = new IntersectionObserver(function (entries) {
                        entries.forEach(function (entry) {
                            if (entry && entry.isIntersecting) {
                                var ad = entry.target;
                                gas.log('LazyLoad ad isIntersecting', ad.dataset.name);
                                if (gas.getDisabledHBFormatNames().includes(ad.dataset.name)) {
                                    gas.loadDFP(ad.dataset.name);
                                } else {
                                    gas.loadHB(ad.dataset.name);
                                }
                                ad.classList.remove('lazy-ad');
                                adObserver.unobserve(ad);
                            }
                        })
                    }, gas.lazyLoadOptions);
                    lazyAds.forEach(ad => {
                        adObserver.observe(ad);
                    });
                } else {
                    lazyAds.forEach(function (ad) {
                        if (gas.getDisabledHBFormatNames().includes(ad.dataset.name)) {
                            gas.loadDFP(ad.dataset.name);
                        } else {
                            gas.loadHB(ad.dataset.name);
                        }
                    })
                }
            },

            loadHB: function (adIds) {
                if (!Array.isArray(adIds)) {
                    adIds = [adIds]
                }
                var calledNames = adIds;
                var slots = [];
                var places = [];
                var apsSlots = [];
                adIds.forEach(function (adId) {
                    var adSlot = gas.definedSlots[adId] || null;
                    var format = gas.getFormatByName(adId);
                    if (adSlot && format) {
                        slots.push(adSlot.slot);
                        places.push(adSlot.place);
                        apsSlots.push({
                            slotID: adSlot.id,
                            slotName: adSlot.id,
                            sizes: format.bidderSizes || []
                        });
                    }
                    if (gas.isContentPage && !window.isMobileBreakpoint() && gas.contentConnectedFormats[adId] !== undefined) {
                        adSlot = gas.definedSlots[gas.contentConnectedFormats[adId]] || null;
                        if (adSlot) {
                            calledNames.push(gas.contentConnectedFormats[adId])
                            slots.push(adSlot.slot);
                            places.push(adSlot.place);
                            var connectedFormat = gas.getFormatByName(gas.contentConnectedFormats[adId]);
                            apsSlots.push({
                                slotID: adSlot.id,
                                slotName: adSlot.id,
                                sizes: connectedFormat.bidderSizes || []
                            });
                        }
                    }
                });

                gas.log('Called HB Request', calledNames);
                if(gas.amazonPubID) {
                    gas.processLoadHBWithAPS(slots,places, apsSlots);
                } else {
                    gas.processLoadHBOnlyPrebid(slots,places);
                }
            },

            processLoadHBOnlyPrebid: function(slots, places) {
                if (slots.length && places.length) {
                    googletag.cmd.push(function () {
                        pbjs.que.push(function () {
                            pbjs.requestBids({
                                adUnitCodes: places,
                                bidsBackHandler: function () {
                                    pbjs.setTargetingForGPTAsync(places);
                                    googletag.pubads().refresh(slots);
                                }
                            });
                        });
                    });
                }
            },

            processLoadHBWithAPS: function(slots, places, apsSlots) {
                if (slots.length && places.length && apsSlots.length) {

                    var requestManager = {
                        adserverRequestSent: false,
                        aps: false,
                        prebid: false
                    };

                    // when both APS and Prebid have returned, initiate ad request
                    function biddersBack() {
                        gas.log('APS biddersBack', requestManager);
                        if (requestManager.aps && requestManager.prebid) {
                            sendAdserverRequest();
                        }
                        return;
                    }

                    // sends adserver request
                    function sendAdserverRequest(timeout = false) {
                        if (requestManager.adserverRequestSent === true) {
                            return;
                        }
                        gas.log('APS refresh')
                        if(timeout) {
                            console.log('APS timeout');
                        }
                        requestManager.adserverRequestSent = true;
                        googletag.cmd.push(function () {
                            googletag.pubads().refresh(slots);
                        });
                    }

                    // sends bid request to APS and Prebid
                    function requestHeaderBids() {

                        gas.log('APS slots', apsSlots)

                        // APS request
                        apstag.fetchBids({
                                slots: apsSlots
                            }, function (bids) {
                                gas.log('APS fetch bids', bids);
                                googletag.cmd.push(function () {
                                    apstag.setDisplayBids();
                                    requestManager.aps = true; // signals that APS request has completed
                                    biddersBack(); // checks whether both APS and Prebid have returned
                                });
                            }
                        );

                        // put prebid request here
                        pbjs.que.push(function () {
                            pbjs.requestBids({
                                adUnitCodes: places,
                                bidsBackHandler: function () {
                                    googletag.cmd.push(function () {
                                        pbjs.setTargetingForGPTAsync();
                                        requestManager.prebid = true; // signals that Prebid request has completed
                                        gas.log('APS prebid bids');
                                        biddersBack(); // checks whether both APS and Prebid have returned
                                    })
                                }
                            });
                        });
                    }

                    // initiate bid request
                    requestHeaderBids();

                    // set failsafe timeout
                    window.setTimeout(function () {
                        sendAdserverRequest();
                    }, gas.amazonTimeout);
                }
            },

            loadDFP: function (adIds) {
                if (!Array.isArray(adIds)) {
                    adIds = [adIds]
                }

                var calledNames = adIds;
                var slots = [];
                adIds.forEach(function (adId) {
                    var adSlot = gas.definedSlots[adId] || null;
                    if (adSlot) {
                        slots.push(adSlot.slot);
                    }
                    if (gas.isContentPage && !window.isMobileBreakpoint() && gas.contentConnectedFormats[adId] !== undefined) {
                        adSlot = gas.definedSlots[gas.contentConnectedFormats[adId]] || null;
                        if (adSlot) {
                            calledNames.push(gas.contentConnectedFormats[adId])
                            slots.push(adSlot.slot);
                        }
                    }
                })
                gas.log('Called DFP Request', calledNames);
                if (slots.length) {
                    googletag.cmd.push(function () {
                        googletag.pubads().refresh(slots);
                    });
                }
            },

            getSizeMapping: function (format) {
                var mapping = googletag.sizeMapping();
                var count = 0;
                if (Array.isArray(format.sizeMappings)) {
                    format.sizeMappings.forEach(function (map) {
                        if (Array.isArray(map) && map.length === 2) {
                            console.log()
                            mapping.addSize(map[0], map[1]);
                            count++;
                        }
                    });
                }
                if (count) {
                    return mapping.build();
                }
                return null;
            },

            getFormatByDivId: function (divId) {
                for (var i = 0; i < gas.formats.length; i++) {
                    var format = gas.formats[i];
                    if (format.divId === divId) {
                        return format;
                    }
                }
                return null;
            },

            getFormatByName: function (name) {
                for (var i = 0; i < gas.formats.length; i++) {
                    var format = gas.formats[i];
                    if (format.name === name) {
                        return format;
                    }
                }
                return null;
            },

            getHBUnits: function () {
                return gas.formats.reduce(function (filtered, format) {
                    if (format.hb) {
                        filtered.push({
                            code: format.slot,
                            mediaTypes: format.mediaTypes,
                            bids: format.bids,
                        });
                    }
                    return filtered;
                }, [])
            },

            getDisabledHBFormatNames: function () {
                return gas.formats.reduce(function (filtered, format) {
                    if (!format.hb) {
                        filtered.push(format.name);
                    }
                    return filtered;
                }, [])
            },

            log: function (...data) {
                if (window.adDebug) {
                    console.log('GAS' + ' - ' + data.shift(), data)
                }
            },

            error: function (...data) {
                if (window.adDebug) {
                    console.error('GAS' + ' - ' + data.shift(), data)
                }
            },

            setLazyLoadOptions: function (mobile, desktop) {
                gas.lazyLoadOptions = window.isMobileBreakpoint() ? mobile : desktop
            },

            setPrebidConfig: function (mobile, desktop) {
                gas.prebidConfig = window.isMobileBreakpoint() ? mobile : desktop
            },

            setBidderSettings: function (mobile, desktop) {
                gas.bidderSettings = window.isMobileBreakpoint() ? mobile : desktop
            },

            setAmazonTimeout: function(mobile, desktop) {
                gas.amazonTimeout = window.isMobileBreakpoint() ? mobile : desktop
            },

            setFormats: function (desktopFormats, mobileFormats) {
                gas.formats = window.isMobileBreakpoint() ? mobileFormats : desktopFormats
            }
        };

