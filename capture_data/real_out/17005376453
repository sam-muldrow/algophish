content-name",p.name);if(e.querySelector("img")){e.setAttribute("data-content-piece","image")}else{e.setAttribute("data-content-piece","link")}if(localStorage.getItem("__gm_debug")){e.style.borderCollapse="collapse";e.style.borderColor="red";e.style.borderWidth="1px";e.style.borderStyle="solid";e.style.backgroundColor="rgba(255, 0, 0, 0.10)";e.style.display="block"}})})});_paq.push(["trackVisibleContentImpressions",true,750]);if(localStorage.getItem("__gm_debug")){_paq.push(["logAllContentBlocksOnPage"])}const onIntersection=entries=>{for(const entry of entries){if(entry.isIntersecting){const target=entry.target;const contentName=target.getAttribute("data-content-name");const contentPiece=target.getAttribute("data-content-piece");const contentTarget=target.getAttribute("data-content-target")||target.href;if(localStorage.getItem("__gm_debug")){console.log("GM::CONTENT::TRACKING visible",contentName,contentPiece,contentTarget,entry.target,entry)}dataLayer.push({event:"content_tracking",content_name:contentName,content_piece:contentPiece,content_target:contentTarget,type:"view_element"});gtag("event","Content tracking - view",{event_category:`CT:${contentName}__${contentPiece}`,event_label:contentTarget})}}};const observer=new IntersectionObserver(onIntersection,{rootMargin:"0px 0px 0px 0px",threshold:.95});let elems=document.querySelectorAll("[data-track-content]");elems.forEach(i=>observer.observe(i));elems.forEach(i=>{i.addEventListener("click",event=>{const target=event.currentTarget;if(target.getAttribute("data-track-content")){const contentName=target.getAttribute("data-content-name");const contentPiece=target.getAttribute("data-content-piece");const contentTarget=target.getAttribute("data-content-target")||target.href;if(localStorage.getItem("__gm_debug")){console.log("GM::CONTENT::TRACKING clicked",contentName,contentPiece,contentTarget,target)}dataLayer.push({event:"content_tracking",content_name:contentName,content_piece:contentPiece,content_target:contentTarget,type:"click_element"});gtag("event","Content tracking - click",{event_category:`CT:${contentName}__${contentPiece}`,event_label:contentTarget})}},false)});console.log("GM::CONTENT::TRACKING end",new Date)})();
</script>
<script type="text/javascript" id="marfeel-additions">

window.marfeel = window.marfeel || { cmd: [] };
(function() {
    if (document.querySelector('[data-paywall-type="deep_dynamic"]')) {
        window.marfeel.cmd.push(["compass", function(compass) {
            compass.setPageVar("closed", "dynamic-paywall")
        }])
    }
    if (document.querySelector('[data-paywall-type="limit_reached_need_normal"]')) {
        window.marfeel.cmd.push(["compass", function(compass) {
            compass.setPageVar("closed", "dynamic-metered-limit-reached-standard")
        }])
    }
    if (document.querySelector('[data-paywall-type="limit_reached_need_premium"]')) {
        window.marfeel.cmd.push(["compass", function(compass) {
            compass.setPageVar("closed", "dynamic-metered-limit-reached-premium")
        }])
    }
    if (document.querySelector('[data-paywall-type="preview"]')) {
        window.marfeel.cmd.push(["compass", function(compass) {
            compass.setPageVar("closed", "dynamic-metered-preview")
        }])
    }
    if (document.querySelector('[data-paywall-type="need_premium_access_has_normal"]')) {
        window.marfeel.cmd.push(["compass", function(compass) {
            compass.setPageVar("closed", "hard-need-upgrade-to-premium")
        }])
    }
    if (document.querySelector('[data-paywall-type="agents_limit_exceeded"]')) {
        window.marfeel.cmd.push(["compass", function(compass) {
            compass.setPageVar("closed", "hard-browers-limit-exceeded")
        }])
    }
    if (document.querySelector('[data-paywall-type="need_premium_access"]')) {
        window.marfeel.cmd.push(["compass", function(compass) {
            compass.setPageVar("closed", "hard-need-premium-access")
        }])
    }
    if (document.querySelector('[data-paywall-type="need_normal_access"]')) {
        window.marfeel.cmd.push(["compass", function(compass) {
            compass.setPageVar("closed", "hard-need-normal-access")
        }])
    }
})();

</script>
