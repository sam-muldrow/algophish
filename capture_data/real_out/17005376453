<script>
function setRPPLUuid() {
	const rpplUuid = getCookie('__rppl_uid');

	if (!rpplUuid) {
		const expireAt = new Date();
		expireAt.setFullYear(expireAt.getFullYear()+1);
		setCookie('__rppl_uid', self.crypto.randomUUID(), expireAt);
	}
}
setRPPLUuid();
</script>
