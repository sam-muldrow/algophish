<script>!function(){"use strict";var e=function(){try{return document.createElement("link").relList.supports("preload")}catch(e){return!1}}();if(window.loadCSS=function(e,t,n){var r=window.document.createElement("link"),l=t||window.document.getElementsByTagName("script")[0];r.rel="stylesheet",r.href=e,r.media="only x",l.parentNode.insertBefore(r,l),setTimeout(function(){r.media=n||"all"})},!1===e)for(var t,n=document.getElementsByTagName("link"),r=0;r<n.length;r++)t=n[r],"preload"===t.rel&&"style"===t.getAttribute("as")&&(loadCSS(t.href,t),t.rel=null)}();var smphr_track_url = 'https://service.sme.sk/corporate-stat/collect/smphr_obj_log';
                var smphr_fetch_url = '/ajax/sme/artemis/web/smphrajaxfetch/0';(function() {
    function hasClass(element, cls) {
        return (' ' + element.className + ' ').indexOf(' ' + cls + ' ') > -1;
    }
    function sleepComp(milliSeconds)
    {
        var startTime = new Date().getTime();
        while (new Date().getTime() < startTime + milliSeconds);
    }
    function hasAttribute(element, name) {
        if (typeof element.getAttribute(name) == 'undefined'
            || element.getAttribute(name) == null
            || element.getAttribute(name) == '') {

            return false;
        }

        return true;
    }

    var maxVzlzWait = 9;

    // scan and append events
    function smphrScanElements() {

        var smphrIdList = '',
            smphrActive = false,
            scanParentElems = document.getElementsByClassName('js-smphr');
        if (scanParentElems.length == 0) {
            return false;
        }

        for (s = 0; s < scanParentElems.length; s++) {
            var scanedParentElem = scanParentElems[s];

            // check if we got all data
            if (!hasAttribute(scanedParentElem, 'data-smphr-id')) {
                continue;
            }
            // parent id
            var smphrId = scanedParentElem.getAttribute("data-smphr-id");
            smphrActive = false;

            // items
            var scanElems = scanedParentElem.getElementsByClassName('js-smphr-item');
            if (scanElems.length == 0) {

                // check if the wrapper isn't the item itself
                if (hasAttribute(scanedParentElem, 'data-smphr-object')
                    && hasAttribute(scanedParentElem, 'data-smphr-pos')) {
                    // construct child from myself
                    scanElems = new Array(scanedParentElem);
                } else {
                   continue;
                }
            }

            // item children
            for (i = 0; i < scanElems.length; i++) {

                var scanedElem = scanElems[i];

                // check if we got all data
                if (!hasAttribute(scanedElem, 'data-smphr-object')
                    || !hasAttribute(scanedElem, 'data-smphr-pos')) {
                    continue;
                }

                // append event on the parent itself
                if (scanedElem.tagName.toLowerCase() == 'a' || scanedElem.classList.contains('js-article-short')) {
                    smphrAppendEvent(scanedElem, 'itself', smphrId);
                    smphrActive = true;
                }

                // append events on children
                if (!scanedElem.classList.contains('js-article-short')) {
                    var elemChildren = scanedElem.getElementsByTagName('a');
                    if (elemChildren.length > 0){
                        for (y = 0; y < elemChildren.length; y++) {
                            if (!hasClass(elemChildren[y], 'js-smphr-off')) {
                                smphrAppendEvent(elemChildren[y], 'parent', smphrId);
                                smphrActive = true;
                            }
                        }
                    }
                }

            }

            if (smphrActive) {
                if (smphrIdList.indexOf(',' + smphrId + ',') < 0) {
                    smphrIdList = smphrIdList + ( smphrIdList == '' ? ',' : '' ) + smphrId + ',';
                }
            }
        }

        if (smphrIdList != '') {
            smphrIdList = smphrIdList.substring(1, smphrIdList.length -1);

            // send for further inspect
            smphrVzlzInit(smphrIdList);
            smphrIdList = null;
        }
    }

    // append event function
    function smphrAppendEvent(element, type, parent) {

        element.onclick = (function (onclick) {
            return function(oEvent) {
                // reference to event to pass argument properly
                oEvent  = oEvent || event;
                if (onclick)
                    onclick(oEvent);
                // new code "injection"
                return smphrHit(this, type, parent);
            }
        })(element.onclick);
    }

    window.addEventListener('SmeSemaphore.addEventListener', (e) => {
        smphrAppendEvent(document.querySelector('#' + e.detail.elementId), 'itself',  e.detail.semaphoreId);
    });

    window.addEventListener('SmeSemaphore.addEventListenerAll', (e) => {
        document.querySelectorAll('.js-article-short').forEach((element) => {
            smphrAppendEvent(element, 'itself',  e.detail.semaphoreId);
        })
    });

    // trigger scan for smphr
    smphrScanElements();

    // hit (we are sure, we have all data)
    function smphrHit(element, type, parent)
    {

        var objId, pos;
        if (type == 'itself') {
            objId = element.getAttribute("data-smphr-object");
            pos = element.getAttribute("data-smphr-pos");
        }
        if (type == 'parent') {

            var give_a_try = 0;
            while (give_a_try < 5
                   && !hasAttribute(element.parentNode, 'data-smphr-object')) {
                element = element.parentNode;
                give_a_try++;
            }
            if (give_a_try == 5
                && !hasAttribute(element.parentNode, 'data-smphr-object')) {
                return false;
            }
            objId = element.parentNode.getAttribute("data-smphr-object");
            pos = element.parentNode.getAttribute("data-smphr-pos");
        }

        smphrHitCall(parent, objId, pos);

        sleepComp(400);
        return true;
    }

    // track
    function smphrHitCall(parentId, objectId, position) {

        if (typeof smphr_track_url === 'undefined'
            || (typeof smphr_track_url !== 'undefined'
                && smphr_track_url == '')
            ) {
            return false;
        }

        var l = new String('');
        var d = document;
        var href = new String(d.location.href);
        var ref = '';

        l = smphr_track_url + '/' + objectId + '?img=1';
        l = l + '&id=' + parentId;
        l = l + '&position=' + position;
        l = l + '&href='+ encodeURIComponent(href.substring(0,200));
        l = l + '&t='+ (new Date()).getTime() +'&t2='+ Math.floor(100000*Math.random());

        if (d.referrer) {
            ref = new String(d.referrer);
            l = l + '&ref='+ encodeURIComponent(ref.substring(0,200));
        }

        var smphr_track_img = new Image();
        smphr_track_img.src = l;
    }

    function smphrAllowRun() {
        var cookie = stormCookie.getCookie('artemisAdmin');

        if (cookie !== null) {
            // if user is not logged in, clear easyAdmin cookie
            if (!stormCentralAccessUser.user.isAuthenticated() ) {
                var hostname = window.location.hostname;
                hostname_split = hostname.split('.');
                if (hostname_split.length > 2) {
                    hostname = '';
                    for (var x = 1; x < hostname_split.length; x++) {
                        hostname += '.'+hostname_split[x]
                    }
                }
                stormCookie.removeCookie(ea_g.caseAllow[i], hostname);
            }

            var cookiePart = cookie.split("_");
            if ((cookiePart[0] == 'admin')
                || (cookiePart[0] == 'user')) {
                return true;
            }
        }

        return false;
    }

    // vizaluzation init
    // - we wait here for the ea dependency!
    function smphrVzlzInit(idList) {

        if (typeof ea_smphr === 'undefined'
            && maxVzlzWait > 0) {
            maxVzlzWait--;
            setTimeout(function() {smphrVzlzInit(idList);}, 400);
            return false;
        }

        allowRun = smphrAllowRun();

        if (allowRun) {
            ea_smphr.fetch(idList);
        }
    }
