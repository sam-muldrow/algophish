                             <script>
                    function collectUserAnalyticData() {
                        smeClWhenUserLoaded
                            .then(function (user) {
                                StormCollectorDataLayer.push({scope: 'user', data: user});
                                if ('undefined' !== typeof window.smeCkMetric) {
                                    StormCollectorDataLayer.push({scope: 'user', data: {'ck-metric': window.smeCkMetric} });
                                }if ('undefined' !== typeof StormExternalData) {
                                    StormExternalData.collector.sendData('smeClUserLoaded');
                                }
                            })
                            .catch(function (error) {
                                StormCollectorDataLayer.push({scope: 'user', data: error.user});
                                if ('undefined' !== typeof window.smeCkMetric) {
                                    StormCollectorDataLayer.push({scope: 'user', data: {'ck-metric': window.smeCkMetric} });
                                }if ('undefined' !== typeof StormExternalData) {
                                    StormExternalData.collector.sendData('smeClUserLoaded');
                                }
                            });
                    }
                    function resolveCkSizeMetric() {
                        const ckSize = Math.round(document.cookie.length/1000);
                        window.smeCkMetric = {'segment': ckSize};
                        if (4 <= ckSize) {
                            const ca = document.cookie.split(';'),
                                ckBig = {};
                            for (let i = 0; i < ca.length; i++) {
                                let c = ca[i];
                                while (c.charAt(0) === ' ') {c = c.substring(1, c.length);}
                                let csize = c.length;
                                if (csize >= 500) {
                                    ckBig[c.substring(0,c.indexOf('=')).toLowerCase()] = Math.round(csize/1000)
                                }
                            }
                            if (Object.keys(ckBig).length) {
                                window.smeCkMetric['large'] = ckBig
                            }
                        }
                    }
                    document.addEventListener("DOMContentLoaded", function() {
                        if ('undefined' === typeof window.smeClWhenUserLoaded) {
                            document.body.addEventListener('Sme.UserLib.User.load.initialized', () => {
                                resolveCkSizeMetric();
                                collectUserAnalyticData();
                            });
                        } else {
                            resolveCkSizeMetric();
                            collectUserAnalyticData();
                        }
                    });
                </script>
