						mediaTypes: {
							banner: {
								sizes: pb_sizes_copy
							}
						},
						bids: bids,
						code: ad_unit
					} );
				}

				FFADS.units[ad_unit].instances = {};
				if( FFADS.units[ad_unit].repeating ) for( let i = 0; i < FF_REP_COUNT; i++ ) FFADS.units[ad_unit].instances['REP_' + i] = {};
