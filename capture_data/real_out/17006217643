<script>
    var getSec = function (str) {
        var str1 = str.substr(0, str.length - 1);  //时间数值
        var str2 = str.substr(str.length - 1, 1);    //时间单位
        if (str2 == "s") {
            return str1 * 1000;
        }
        else if (str2 == "m") {
            return str1 * 60 * 1000;
        }
        else if (str2 == "h") {
            return str1 * 60 * 60 * 1000;
        }
        else if (str2 == "d") {
            return str1 * 24 * 60 * 60 * 1000;
        }
    }
    //添加cookie
    var addCookie = function (name, value, time, path = '/') {
        var strSec = getSec(time);
        var exp = new Date();
        exp.setTime(exp.getTime() + strSec * 1);
        console.log(exp)
        //path=/，表示cookie能在整个网站下使用，path=/temp，表示cookie只能在temp目录下使用
        path = path == "" ? "" : ";path=" + path;
        //设置cookie的名称、值、失效时间
        document.cookie = name + "=" + value + ";expires=" + exp.toUTCString() + path;

    }
    //删除cookie
    var delCookie = function (name, path = '/') {
        var exp = new Date();
        exp.setTime(exp.getTime() - 1);
        //获取cookie是否存在
        var value = getCookie(name);
        path = path == "" ? "" : ";path=" + path;
        if (value != null) {
            document.cookie = name + "=" + value + ";expires=" + exp.toUTCString() + path+";domain=.haitao.com";
        }
    }
    //获取cookie
    var getCookie = function (name) {
        //获取当前所有cookie
        var strCookies = document.cookie;
        //截取变成cookie数组
        var array = strCookies.split('; ');
        //循环每个cookie
        for (var i = 0; i < array.length; i++) {
            //将cookie截取成两部分
            var item = array[i].split("=");
            console.log("item", item)
            //判断cookie的name 是否相等
            if (item[0] == name) {
                return item[1];
            }
        }
        return null;
    }
    //获取时间的秒数（参数：d，h,m,s） 12m


    if (getCookie('close') == null) {
        console.log(getCookie('close'))
        $('.tanchuang_box').show();
        // $('.tanchuang_box').hide();
    }
    $('svg').on('click', function () {
        addCookie('close', 'close', '1d')
        $('.tanchuang_box').hide();
    });
    $('.radio .block').click(function () {
        if ($(this).hasClass('option')) {
            $(this).removeClass('option')
            delCookie('close')
        } else {
            $(this).addClass('option')
            addCookie('close', 'close', '7d')
            $('.tanchuang_box').hide();
        }

    })

</script>
