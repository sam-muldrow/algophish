    <script>
        window.delayJs = window.delayJs || [];
        window.dataLayer = window.dataLayer || [];
        var isLighthouse = "0";
        (function(){
            var oldOnError = window.onerror;
            window.onerror = function(){
                if (window.console) {
                    console.log(arguments);
                }

                var data = JSON.stringify({
                    "type": "error",
                    "msg": arguments[0],
                    "url": arguments[1],
                    "lineno": arguments[2],
                    "colno": arguments[3],
                    "error": arguments[4]
                });

                (new Image()).src = "/webservices/logJs.php?type=JSError&msg=" + encodeURIComponent(data);

                if (oldOnError) {
                    oldOnError.apply(this, arguments);
                }

                return false;
            };

        })();

    </script>

