<script>
    // Check if avqUiDialog is defined
    if (typeof avqUiDialog === 'undefined') {
        var avqUiDialog = {
            show: function(options) {
                var id = options.id || 'dialog';
                var title = options.title || 'Confirm';
                var content = options.content || '';
                var buttons = options.buttons || {
                    'Yes': () => {
                        this.hide(id);
                    },
                    'No': () => {
                        this.hide(id);
                    }
                };
                var dialogEleObj = $('[e-component-' + id + ']');
                // Update title
                dialogEleObj.find('.title h4').html(title);
                // Update Content
                dialogEleObj.find('.content').html(content);
                // Update buttons
                var buttonsHtml = '';
                for (var button in buttons) {
                    buttonsHtml += '<button class="g-btn reverse">' + button + '</button>';
                }
                dialogEleObj.find('.buttons').html(buttonsHtml);
                // Bind events on buttons
                var buttonsEleObj = dialogEleObj.find('.buttons button');
                var i = 0;
                for (var button in buttons) {
                    var callback = buttons[button];
                    if (typeof callback === 'function') {
                        buttonsEleObj.eq(i).on('click', callback);
                    }
                    i++;
                }
                // Bind event on close button
                dialogEleObj.find('.g-btn-close').on('click', function() {
                    avqUiDialog.hide(id);
                });
                // Show dialog
                dialogEleObj.removeClass('hide');
                // Show dialog overlay
                $('.component-dialog-overlay').removeClass('hide');

            },
            hide: function(id) {
                // Hide dialog overlay
                $('.component-dialog-overlay').addClass('hide');
                // Hide dialog
                $('[e-component-' + id + ']').addClass('hide');
                // Unbind events on close button
                $('[e-component-' + id + '] .g-btn-close').off('click');
            }
        };
    }
</script>
<script>
    function adjustContentHeight() {
        var page_content = $('.page-content'),
            page_footer = $('.container-footer');
        page_content.css(
            'min-height',
            $(window).height() - page_content.offset().top - page_footer.outerHeight() - parseInt(page_footer.css('marginTop'))
        );
    }
    $(function() {
        adjustContentHeight();
        $(window).on('resize', function() {adjustContentHeight();});
    })
</script>
