<script>
    window.localStorage = window.localStorage || {};
    function updateLocalUtmInfo (new_utm_obj) {
        var old_utm_obj = JSON.parse(localStorage['utm_info'] || '{}');
        var utm_sub;
        var utm_item_change = false;
        for (utm_sub in new_utm_obj) {
            if ((new_utm_obj[utm_sub] && !old_utm_obj[utm_sub]) || (new_utm_obj[utm_sub] && old_utm_obj[utm_sub] && new_utm_obj[utm_sub] != old_utm_obj[utm_sub])) {
                old_utm_obj[utm_sub] = new_utm_obj[utm_sub];
                utm_item_change = true;
            }
        }

        if (utm_item_change) {
            localStorage['utm_info'] = JSON.stringify(old_utm_obj);
        }
    }

    $(document).ready(function() {
        updateLocalUtmInfo(utm_info);
    });
</script>

