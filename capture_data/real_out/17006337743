        function trackEvent(category, action, label, value) {
            dataLayer.push({
                'event': 'google_analytics_measurement',
                'gtm.pa_event_catgory': category,
                'gtm.pa_event_action': action,
                'gtm.pa_event_label': label,
                'gtm.pa_event_value': value
            });
