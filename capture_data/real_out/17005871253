/********** Breaking News & Now Showing Ticker **********/
$(document).ready(function($) {
    $.fn.Ticker = function(options) {
        var defaults = {
            pause: 2000,
            fadeIn: 300,
            fadeOut: 300,
            delay: 500,
            typewriter: true,
            speed: 35,
            cursor: '_'
        };

        var opts = $.extend({}, defaults, options);

        return $(this).each(function() {
            var list = $(this),
                typewriter = {},
                interval;

            list
                .addClass('ticker-active')
                .children(':first')
                .css('display', 'block');

            function changeItem() {
                var item = list.children(':first'),
                    next = item.next(),
                    copy = item.clone();

                clearTimeout(interval);

                $(copy)
                    .css('display', 'none')
                    .appendTo(list);

                item.fadeOut(opts.fadeOut, function() {
                    $(this).remove();

                    if (opts.typewriter) {
                        typewriter.string = next.text();

                        next
                            .text('')
                            .addClass('item')
                            .css('display', 'block');

                        typewriter.count = 0;
                        typewriter.timeout = setInterval(type, opts.speed);
                    } else {
                        next
                            .delay(opts.delay)
                            .fadeIn(opts.fadeIn, function() {
                                setTimeout(changeItem, opts.pause);
                            });
                    }
                });
            }

            function type() {
                typewriter.count++;

                var text = typewriter.string.substring(0, typewriter.count);

                if (typewriter.count >= typewriter.string.length) {
                    clearInterval(typewriter.timeout);
                    setTimeout(changeItem, opts.pause);
                } else if (opts.cursor) {
                    text += ' ' + opts.cursor;
                }

                list
                    .children(':first')
                    .text(text);
            }

            if (list.find('li').length > 1) {
                interval = setTimeout(changeItem, opts.pause);
            }
        });
    };

    $('.ticker').Ticker();

    var mobile_browser = (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator
        .userAgent));

    /** Closing the Breaking News **/
    $('.close_breaking_icon').click(function() {
        if (mobile_browser) {
            $('.breaking_news_ticker').parent().fadeOut();
        } else
            $('.breaking_news_ticker').fadeOut();
    });

    /** Closing the Now Showing **/
    $('.close_showing_icon').click(function() {
        if (mobile_browser) {
            $('.now_showing_ticker').parent().fadeOut();
        } else
            $('.now_showing_ticker').fadeOut();
    });

    /** Closing the Survey **/
    $('.close_survey_icon').click(function() {
        if (mobile_browser) {
            $('.survey_ticker').parent().fadeOut();
        } else
            $('.survey_ticker').fadeOut();
    });

    $('.close_event_icon').click(function() {
        if (mobile_browser) {
            $('.event_ticker').parent().fadeOut();
        } else
            $('.event_ticker').fadeOut();
    });

    /** Closing the Live Stream **/
    $('.close_live_icon').click(function() {
        $('.live_ticker').fadeOut();
    });
});
</script>
<script>
let emailPattern =
    /^(([^<>()[\]\.,;:\s@\"]+(\.[^<>()[\]\.,;:\s@\"]+)*)|(\".+\"))@(([^<>()[\]\.,;:\s@\"]+\.)+[^<>()[\]\.,;:\s@\"]{2,})$/i;
let emailValidate = false;
$(document).ready(function() {
    var bLazy = new Blazy().setTimeout(function() {
        bLazy.revalidate();
    }, 1000);

});

fixedLeftSide = 'false';
readmore = 'false';

/** Search AutoComplete **/
$(function() {
    $(document).on("keyup", '#keyword', function() {
        if ($(this).val().length > 1) {
            var searchFor = $(this).val();
            if (searchFor != '') {
                $.ajax({
                    url: "https://royanews.tv/search/autocomplete",
                    method: "get",
                    data: {
                        term: searchFor
                    },

                    success: function(data) {
                        $('#searchForList').fadeIn();
                        $('#searchForList').html(data);
                    }
                });
            }
        } else {
            $('#searchForList').fadeOut();
        }
    });

    $(document).on('click', 'li', function() {
        $('#keyword').val($(this).text());
        $('#searchForList').fadeOut();
    });
});

$(document).ready(function() {
    const isRegistered = emailNewsLetterChecker();
    const showNewsletterExp = getShowNewsletter();
    if (!isRegistered) {
        if (!showNewsletterExp) {
            $('#newsletter').delay(3000).slideToggle(800);
        } else {
            $(".envelopeNewsletter").fadeIn();

        }
    }
    /** NewsLetter Visibility **/
    $("#newsletter-closebtn").click(function() {
        $('#newsletter').slideToggle(800);
        $(".envelopeNewsletter").fadeIn();
        setShowNewsletter();

    });
    $(".envelopeNewsletter").click(function() {
        $(this).fadeOut();
        $('#newsletter').slideToggle(800);
    });

    $("#newsletter-email").blur(function() {
        const value = $(this).val();
        if (!emailPattern.test(value)) {
            $(".newsletter-validation").fadeIn();
            emailValidate = true;
        } else {
            $(".newsletter-validation").fadeOut();
            emailValidate = false;
        }
    });

    $("#newsletter-submit").click(function(e) {
        const email = $("#newsletter-email").val();

        if (email && emailValidate) {
            e.preventDefault();

        } else if (email && !emailValidate) {
            localStorage.setItem("reg_email", email);
            $('#newsletter').slideToggle(800);
        }


    })


    $(".close-btn").click(function() {
        $('#snackbar').fadeOut();
        $('.envelope_news').fadeIn();
        $('#item').fadeIn();
    });

    $(".envelope_news2").click(function() {
        $('#snackbar').fadeIn();
        $('#item').fadeOut();
    });

    $(".close-btn-news").click(function() {
        $('.alert-news_area').fadeOut();
    });
});

$(document).ready(function() {
    if ($("#item").css("transform") == "translate3d(0px, 0px, 0px)") {
        $('body').fadeOut();
    }


});



function emailNewsLetterChecker() {
    const email = localStorage.getItem('reg_email');
    return email ? true : false;
}


function setShowNewsletter() {
    const now = new Date()
    const expiry = now.getTime() + 86400000;

    localStorage.setItem('newsletterExp', expiry)
}

function getShowNewsletter() {
    const newsletterExpStr = localStorage.getItem('newsletterExp')

    if (!newsletterExpStr) {
        return null;
    }
    const item = JSON.parse(newsletterExpStr)
    const now = new Date();
    if (now.getTime() > newsletterExpStr) {
        localStorage.removeItem('newsletterExp')
        return null;
    }
    return newsletterExpStr
}
</script>
<script>
$(document).ready(function() {
    /** Application Download Screen **/
    $(window).scroll(function() {
        if ($(this).scrollTop() > 200) {
            $('.popUp_block').fadeOut();
        }
    });
    $('.popUp_block .close_popUp').click(function() {
        $('.popUp_block').fadeOut();
    });


});
</script>
<script>
/********** Live Stream Ticker **********/
jQuery(function() {
    jQuery("ul#news_ticker").liScroll();
    jQuery("ul#live_ticker").liScroll();
});

jQuery.fn.liScroll = function(settings) {
    settings = jQuery.extend({
        travelocity: 0.07
    }, settings);
    return this.each(function() {
        var $strip = jQuery(this);
        $strip.addClass("newsticker")
        var stripWidth = 1;
        $strip.find("li").each(function(i) {
            stripWidth += jQuery(this, i).outerWidth(true);
        });
        var $mask = $strip.wrap("<div class='mask'></div>");
        var $tickercontainer = $strip.parent().wrap("<div class='tickercontainer'></div>");
        var containerWidth = $strip.parent().parent().width();
        $strip.width(stripWidth);
        var totalTravel = stripWidth + containerWidth;
        var defTiming = totalTravel / settings.travelocity;

        function scrollnews(spazio, tempo) {
            $strip.animate({
                right: '-=' + spazio
            }, tempo, "linear", function() {
                $strip.css("right", containerWidth);
                scrollnews(totalTravel, defTiming);
            });
        }
        scrollnews(totalTravel, defTiming);
        $strip.hover(function() {
                jQuery(this).stop();
            },
            function() {
                var offset = jQuery(this).offset();
                var residualSpace = offset.right + stripWidth;
                var residualTime = residualSpace / settings.travelocity;
                scrollnews(residualSpace, residualTime);
            });
    });
};
</script>
<script>
/********** Most Watched News Background Percentage **********/
(function(document, $) {

    $(document).ready(function() {
        $('.rating-bar').each(function() {
            var _this = $(this),
                size = _this.data('size');

            _this.animate({
                width: size + '%'
            }, {
                duration: 2500,
                step: function(progress) {
                    _this.css({
                        backgroundColor: progressColor(progress)
                    });
                }
            });
        });
    });

    function progressColor(progress) {
        return '/images/site/opacity.png';
    }
})(document, jQuery);
</script>
<script>
/** Fixed Main Menu on Scroll **/
function stickIt() {
    var e = $(".original").offset();
    orgElementTop = e.top,
        $(window).scrollTop() >= orgElementTop ? (orgElement = $(".original"),
            coordsOrgElement = orgElement.offset(),
            leftOrgElement = coordsOrgElement.left,
            topOrgElement = coordsOrgElement.top,
            widthOrgElement = orgElement.css("width"),
            $(".cloned").css("left", leftOrgElement + "px").css("top", '48px').css("margin", 0).css("width",
                widthOrgElement).show(),
            $(".menu-Logo").show().css("display", "inline").css("float", "right"),
            // $(".priority_ticker").addClass("fix_ticker"),
            $('.col-xs-5.menu_icons').css('display', 'block'),
            $(".navbar-default .navbar-nav>li>a")) : ($(".cloned").hide(),
            $(".menu-Logo").hide())
    // $(".priority_ticker").removeClass("fix_ticker"))
}

$(".navbar").addClass("original").clone().insertAfter(".navbar").addClass("cloned").css("position", "fixed").css("top",
        "48px").css("margin-top", "0").css("z-index", "499").removeClass("original").hide(),
    scrollIntervalID = setInterval(stickIt, 10);
</script>
<script>
/** Mobile Menu Items Activation **/
$(function() {
    var path = window.location.pathname;
    var home = window.location.href;
    var domain = window.location.origin + '/';

    if (home === domain) {
        $(".home a").css("color", "#000757");
        $("#homeIcon").addClass("active");
    } else if (path.indexOf("breaking-list") > -1) {
        $(".notification a").css("color", "#000757");
        $("#notificationIcon").addClass("active");
    } else if (path === "/watch-live") {
        $(".live a").css("color", "#000757");
        $("#liveIcon").addClass("active");
    } else if (path.indexOf("latest-news") > -1) {
        $(".latest a").css("color", "#000757");
        $("#latestIcon").addClass("active");
    } else if (path.indexOf("common-news") > -1) {
        $(".common a").css("color", "#000757");
        $("#commonIcon").addClass("active");
    }
});


/** Weather API **/
$(document).ready(function() {
    $.ajax({
        url: "https://widgets.media.devops.arabiaweather.com/widget/royaa-web-API?location_name=West%20Amman",
        success: function(data) {
            if (data) {
                var load_section = [
                    '<span>' + data.locations["2680"].daily[0].day_time.temperature + '</span>',
                    '<img src="' + data.locations["2680"].daily[0].day_time.status_img +
                    '" alt="الطقس" />'
                ];
                $('.weather_icon').append(load_section.join(''));
            } else {
                $('.weather_icon').append('<i class="fa fa-thermometer-half"></i>')
            }
        }
    });

});
</script>
