<script>
/** Desktop Sliders **/
$('.owl-carousel.eventSlider').owlCarousel({
    margin: 20,
    responsiveClass: true,
    loop: false,
    responsive: {
        0: {
            items: 1
        },
        568: {
            items: 2
        },
        600: {
            items: 3
        },
        1000: {
            items: 3
        }
    }
});

/** Desktop Event Image Gallery **/
$(document).ready(function() {
    $('#event_images, .main_image').magnificPopup({
        delegate: 'a',
        type: 'image',
        preloader: false,
        image: {
            cursor: null,
            titleSrc: 'title'
        },
        gallery: {
            enabled: true,
            preload: [0, 1],
            navigateByImgClick: true
        }
    });
});

/** Mobile Event Images Gallery **/
$('.mobile_event_images').magnificPopup({
    delegate: 'a',
    type: 'image',
    image: {
        cursor: null,
        titleSrc: 'title'
    },
    gallery: {
        enabled: true,
        preload: [0, 1],
        navigateByImgClick: true
    }
});

/** Slider Elements Display on Desktop **/
if ($('#event_news .owl-stage').children().length <= 4) {
    $('#event_news .owl-nav').hide();
}

if ($('#event_videos .owl-stage').children().length <= 4) {
    $('#event_videos .owl-nav').hide();
}

if ($('#event_images .owl-stage').children().length <= 4) {
    $('#event_images .owl-nav').hide();
}

/** Content Images Zoom **/
function zoomin() {
    $('.zoomBtn.zoomOut').css('display', 'block');
    var myImgWidth = $(".mfp-img").width();
    if (myImgWidth >= 1000) return false;
    else {
        var new_myImgWidth = myImgWidth + 200 + "px";
        $(".mfp-img").css({
            width: new_myImgWidth
        });
        $(".mfp-img").css('max-height', '100%', '!important');
        $('.mfp-container').css('position', 'relative', '!important')
    }
}

function zoomout() {
    var myImgWidth = $(".mfp-img").width();
    if (myImgWidth <= 350) return false;
    else {
        var new_myImgWidth = myImgWidth - 200 + "px";
        $(".mfp-img").css({
            width: new_myImgWidth
        });
    }
}
</script>
<style>
.hide_section {
    display: none;
}
</style>
<script>
