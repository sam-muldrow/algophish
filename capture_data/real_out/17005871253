                                    var secondAdform = document.createElement('script');
                                    firstAdform.setAttribute('data-adfscript',
                                        'adx.adform.net/adx/?mid=1153386&rnd=%%CACHEBUSTER%%');
                                    secondAdform.src = "//s1.adform.net/banners/scripts/adx.js";
                                    secondAdform.async = true;
                                    secondAdform.defer = true;
                                    $("#royanews_homepage_latestMostRead").append(firstAdform);
                                    $("#royanews_homepage_latestMostRead").append(secondAdform);
                                }
                            }
                        });
                    });
                    </script>
</div>
<div id="royanews_homepage_latestMostRead">


</div>
</section>

