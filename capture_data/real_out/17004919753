		$("#search_input").bind("focus click", function(){
		 	if($(this).val()=="Search")
		 		{
		 			$(this).val('');
		 		}
		 });
		 
		 $("#search_input").bind("blur", function(){
		 	if(($(this).val()=="Search") || ($(this).val()==''))
		 		{
		 			$(this).val('Search');
