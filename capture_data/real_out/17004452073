<script type="text/javascript" src="/~/media/CORP2020/Assets/corp/2022/common.js?ver=111423"></script>

<script>
    $(function () {
        var lang = $("#hidLanguage").val();
        if (lang === "en" || lang === "zh") {
            var target = $("a[data-id='{BDDE876F-D975-4EDE-B534-33EDF05110B0}']");
            var path = target.attr("href");
            var url = "";
            if (/\?/.test(path)) {
                var pattern = 'lang=([^&]*)';
                var replaceText = 'lang=' + lang;
                url = path.match(pattern) ? path.replace(eval('/(lang=)([^&]*)/gi'), replaceText) : (path.match('[\?]') ? path + '&' + replaceText : path + '?' + replaceText);
            } else {
                url = path + "?lang=" + lang;
            }
            target.attr("href", url);
        }
    });
window.page_init=(function(){
jQuery.fn.hoverIntent = function (hoverIn, hoverOut, selector) {
    var timer1 = 0;
    var timer2 = 0;
    var delay = 250;
    this.on("mouseenter", selector, function (e) {
      clearTimeout(timer1);
      timer1 = 0;
      var that = this;
      (function (th) {
        timer1 = setTimeout(function () {
          hoverIn.apply(th, [e]);
        }, delay);
      }(that));
    }).on("mouseleave", selector, function (e) {
      clearTimeout(timer1);
      timer1 = 0;
      // clearTimeout(timer2);
      // timer2 = 0;
      var that = this;
      (function (th) {
        setTimeout(function () {
          hoverOut.apply(th, [e]);
        }, delay);
      }(that));
    });
    return this;
  };

})
 
</script>




