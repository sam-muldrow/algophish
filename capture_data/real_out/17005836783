  <script>
    var sessionFixedBanner = sessionStorage.getItem('closefixedbanner');
    function closeFixedBanner(event){
      event.preventDefault();
      event.stopPropagation();
      if(!sessionFixedBanner){
        sessionStorage.setItem('closefixedbanner', 'close');
      }
      document.querySelector('.specialProductView').style = 'display: none;';
    }
    if(sessionFixedBanner){
      document.querySelector('.specialProductView').style = 'display: none;';
    }
  
  </script>
  
  
  
  
  
    
    
      
    
