        <script>
          function createCookie(cname, cvalue, ctime){
            var dt = new Date()
            var expires = dt.setHours(dt.getHours() + ctime);
            var expiresString = new Date(expires)

            document.cookie = cname+"="+cvalue+"; expires="+expiresString+";path=/";
          }
          function checkCookie(cname){
            if(document.cookie.indexOf(cname) >= 0) {
              return true;
            } else {
              return false;
            }
          }
          $(function(){
            //var ratingBadgeClosed = sessionStorage.getItem('ws_rating_badge_closed');
            var ratingBadgeClosed = checkCookie('ws_rating_badge_closed');
            if(!ratingBadgeClosed) {
              $('.shop-rating-badge').removeClass('d-none');
            }
            $('.rating-close').click(function(e){
              e.preventDefault();
              //sessionStorage.setItem('ws_rating_badge_closed', true);
              createCookie('ws_rating_badge_closed', true, 2);
              $('.shop-rating-badge').addClass('d-none');
            });
          });
        </script>
        
        

        
        
        
        
        
        
        
        
        
        
         
        
        
        
        
        
        
        

        
        
        
