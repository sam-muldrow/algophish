<script>
document.addEventListener("ffReady", function () {
      const key = factfinder.communication.ResultDispatcher.addCallback("suggest", function (suggestData) {
        if(suggestData){
          for(i = 0; i < suggestData.length; i++){
            if (suggestData[i].type === "FEEDBACK") {
              const productCount = Number(suggestData[i].attributes.productsCount);
              var productItem = [];
              for(x = 0; x < productCount; x++){
                const productItemNumber = suggestData[i].attributes["Number:"+x];
                const productItemID = suggestData[i].attributes["Number:"+x];
                const productItemDeeplink = suggestData[i].attributes["ProductLink:"+x];
                const productItemName = suggestData[i].attributes["Name:"+x];
                const productItemImage = suggestData[i].attributes["Image:"+x];

                productItem.push({attributes: {Number: productItemNumber, articleNr: productItemNumber, id: productItemID, deeplink: productItemDeeplink,}, name: productItemName, image: productItemImage, type: "productName"});
              }
            }
          }
          if(productItem){
            for(y = 0; y < productItem.length; y++){
              suggestData.unshift(productItem[y]);
            }
          }
        }
      });
  });
</script>

