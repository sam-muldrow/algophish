<script>
  document.querySelector("ff-suggest").addEventListener("suggest-item-clicked", function (e) {
    var ffSuggestItem = e.detail.element;
    var suggestionData = e.detail.suggestion;
    var mainLink = "https://www.kopp-verlag.de/?websale8=kopp-verlag&amp;act=search".replace("&amp;", "&")+'&';
    if(suggestionData.type === "searchTerm"){
      var searchName = suggestionData.name;
      searchName = searchName.replace(/<\/?[^>]+(>|$)/g, "");
      var query = "query="+searchName;
      mainLink += query;
      location.href = mainLink;
    }
    else if (suggestionData.type === "productName") {
      var productLink = suggestionData.attributes["deeplink"];
      var homePageLink = "https://www.kopp-verlag.de";
      //location.href = homePageLink+"/?pn="+suggestionData.attributes["Number"];
      location.href = productLink.replace('https://www.kopp-verlag.de','https://www.kopp-verlag.de').replace('websale8=kopp-verlag','websale8=kopp-verlag.01-aa');
      ffSuggestItem.ffPreventDefault = true;
    }
    else if (suggestionData.type === "category") {
      var catData = suggestionData.searchParams;
      var index = catData.indexOf('?');
      catData = catData.slice(index+1);
      console.log(catData);
      location.href = "https://www.kopp-verlag.de/?websale8=kopp-verlag&amp;act=search".replace("&amp;", "&")+'&'+catData;
      ffSuggestItem.ffPreventDefault = true;
    }
    else if (suggestionData.type === "FEEDBACK") {
      console.log(this);
    }
  });
</script>


