<script>
var didScroll;
var lastScrollTop = 0;
var delta = 5;
var startheight = 0;

$(window).scroll(function(event){
    didScroll = true;
});

setInterval(function() {
    if (didScroll) {
        hasScrolled();
        didScroll = false;
    }
}, 250);

function hasScrolled() {
    var st = $(this).scrollTop();
    var $classes = $('.shop-rating-badge, .deutschlandtest--logo, .computerbild--logo');

    if(Math.abs(lastScrollTop - st) <= delta)
        return;

    if (st > lastScrollTop && st > startheight){
        // Scroll Down
        $classes.removeClass('up').addClass('down');
    } else {
        if(st + $(window).height() < $(document).height()) {
            $classes.removeClass('down').addClass('up');
        }
    }

    lastScrollTop = st;
}
</script>


<script>
  var hidetimeout;

  $(document).on('click','.quantity-down',function(){
      var inputValue = parseInt($(this).siblings('.quantity-input').val());
      if(inputValue > "1"){
          $(this).siblings('.quantity-input').val(inputValue-1).trigger('keyup');
          clearTimeout(hidetimeout);
          basketspinntertooltip.call(this);
      }
  });
  $(document).on('click','.quantity-up',function(){
      var inputValue = parseInt($(this).siblings('.quantity-input').val());
      $(this).siblings('.quantity-input').val(inputValue+1).trigger('keyup');
      clearTimeout(hidetimeout);
      basketspinntertooltip.call(this);
  });

  function basketspinntertooltip(){
    var $price = parseFloat($(this).siblings('.quantity-input').data('pr-price'));
    var $quantity = parseFloat($(this).siblings('.quantity-input').val());
    var $input = $(this).siblings('.quantity-input');
    var $endprice = $price*$quantity;
    $endprice = new Intl.NumberFormat('de-DE', { style: 'currency', currency: 'EUR' }).format($endprice)

  
    $input.attr('data-original-title', 'Gesamtpreis '+$endprice).tooltip('show');

    hidetimeout = setTimeout(function(){
      $('.quantity-input').tooltip('hide');
    }, 3000);
  }
</script>


<script>
  $('.multiimagesproductbox img').on('mouseenter',function(){
    var $bigimg = $(this).data('hover-image');
    var $currentimg = $(this).parents('form').find('.productbox--img')
    $(this).parents('form').find('.multiimagesproductbox .active').removeClass('active');
    $(this).parent().addClass('active');
    $currentimg.attr('src',$bigimg);
  })
</script>
<script>
  $('.productformbasketspinner').on('mouseleave',function(){
    var $bigimg = $(this).find('.multiimagesproductbox img').first().data('hover-image');
    var $currentimg = $(this).find('.productbox--img')
    $currentimg.attr('src',$bigimg);
    $(this).find('.multiimagesproductbox .active').removeClass('active');
    $(this).find('.multiimagesproductbox img').first().parent().addClass('active');
  })
</script>



	

	
	
	
	


<script>
    function econda_callback(targetElement, data, templateName) {
        if (data.products.length == 0)
            return;
        var payload = ws_getEcondaPayload(data.products);
        var title = data.widgetDetails.getTitle();

        ws_AJAXloadTemplatePOST('/incl_econda_cross_sell%2ehtm?websale8=kopp-verlag&tpl=incl_econda_cross_sell.htm&otp2='+encodeURIComponent(title), 'ISO-8859-1', ws_AJAXloadEcondaRecommendationsStart, ws_AJAXloadEcondaRecommendationsError, 'ws_AJAXloadEcondaRecommendationsResponseSuccess()', 'ws_AJAXloadEcondaRecommendationsResponseError()', payload);

    }

		
    function econda_callback2(targetElement, data, templateName) {
        if (data.products.length == 0)
            return;
        var payload = ws_getEcondaPayload(data.products);
        var title = data.widgetDetails.getTitle();

        ws_AJAXloadTemplatePOST('/incl_econda_cross_sell%2ehtm?websale8=kopp-verlag&tpl=incl_econda_cross_sell.htm&otp1=bestseller&otp2='+encodeURIComponent(title), 'ISO-8859-1', ws_AJAXloadEcondaRecommendationsStart, ws_AJAXloadEcondaRecommendationsError, 'ws_AJAXloadEcondaRecommendationsResponseSuccess()', 'ws_AJAXloadEcondaRecommendationsResponseError()', payload);

    }

		
    function econda_callback3(targetElement, data, templateName) {
      
        if (data.products.length == 0) {
          return;
        }
      
      
        var payload = ws_getEcondaPayload(data.products);
        var title = data.widgetDetails.getTitle();

        ws_AJAXloadTemplatePOST('/incl_econda_cross_sell%2ehtm?websale8=kopp-verlag&tpl=incl_econda_cross_sell.htm&otp1=topseller&otp2='+encodeURIComponent(title), 'ISO-8859-1', ws_AJAXloadEcondaRecommendationsStart, ws_AJAXloadEcondaRecommendationsError, 'ws_AJAXloadEcondaRecommendationsResponseSuccess()', 'ws_AJAXloadEcondaRecommendationsResponseError()', payload);

    }

    
    function econda_callback_equipment(targetElement, data, templateName) {
        if (data.products.length == 0)
            return;
        var payload = ws_getEcondaPayload(data.products);
        var title = data.widgetDetails.getTitle();

        ws_AJAXloadTemplatePOST('/incl_econda_cross_sell%2ehtm?websale8=kopp-verlag&tpl=incl_econda_cross_sell.htm&otp3=equipmentproducts&otpproductnumber=&otp2='+encodeURIComponent(title), 'ISO-8859-1', ws_AJAXloadEcondaRecommendationsStart, ws_AJAXloadEcondaRecommendationsError, 'ws_AJAXloadEcondaRecommendationsEquipmentResponseSuccess()', 'ws_AJAXloadEcondaRecommendationsResponseError()', payload);

    }

    var widget = new econda.recengine.Widget({
        renderer: {
           type: 'websale',
           callback: econda_callback
        },
        accountId: '000006a3-7cd69244-222c-4ce5-870e-fdcc7fad7430-1',
        
        
        
        
        
        
        id: 8,
        context: {
					
					
          
          }
    });

		
        function renderTopsellerWidget(resultOfAlsoInterestedWidget) {

            var productsRecommendedInAlsoInterestedWidget = resultOfAlsoInterestedWidget.getProducts();
             var widget2 = new econda.recengine.Widget({
            renderer: {
               type: 'websale',
               callback: econda_callback2
            },
            accountId: '000006a3-7cd69244-222c-4ce5-870e-fdcc7fad7430-1',
            
            
            
            
            id: 7,
            context: {productsExcluded: productsRecommendedInAlsoInterestedWidget}
            
        });
        widget2.render();
    }

    var widget3 = new econda.recengine.Widget({
        renderer: {
           type: 'websale',
           callback: econda_callback3
        },
        accountId: '000006a3-7cd69244-222c-4ce5-870e-fdcc7fad7430-1',
        
        
        
        
        id: 7,
        context: {
					
					
          }
    });

    

    
		$(document).ready(function(){

    widget.setCallback(renderTopsellerWidget);

    
    
    
		});
    
</script>

	
<script>
  $(document).ready(function(){
    initProductSlider();
   });

   function initProductSlider(){
    $('.product_slider').not('.slick-initialized').slick({
      dots: false,
      arrows: true,
      infinite: false,
      speed: 300,
      slidesToShow: 6,
      slidesToScroll: 6,
      responsive: [
        {
          breakpoint: 1200,
          settings: {
          slidesToShow: 5,
          slidesToScroll: 5,
          }
        },
          {
          breakpoint: 992,
          settings: {
          slidesToShow: 4,
          slidesToScroll: 4,
          }
        },
        {
          breakpoint: 768,
          settings: {
          slidesToShow: 3,
          slidesToScroll: 3
          }
        },
        {
          breakpoint: 576,
          settings: {
          slidesToShow: 2,
          slidesToScroll: 2
          }
        }
      ]
    });
   }
</script>
	

	

	
  
