<script>
  $(document).on('click','.switch--shop', function(){
    var $currenturl = $(this).data('currenturl');
    var $nexturl = $(this).data('nexturl');
    var link;
    if(window.location.href.includes('/websale8/')){
      link = $nexturl ;
    }
    else {

      if(window.location.href.indexOf("?") < 0){
        link = window.location.href.replace($currenturl,$nexturl).replace('websale8=kopp-verlag','websale8=kopp-verlag.02-06') ;
      }
      else {
        link = window.location.href.replace($currenturl,$nexturl).replace('websale8=kopp-verlag','websale8=kopp-verlag.02-06') ;
      }

    }

    window.location.href = link;
  })
</script>




<script>
function viewPasswort(inputeye) {
  var eye = inputeye;
  var input = inputeye.previousElementSibling;
  if (input.type === "password") {
    input.type = "text";
    eye.classList.add('text-primary');
  } else {
    input.type = "password";
    eye.classList.remove('text-primary');
  }
}
</script>





<script>
  $(document).ready(function(){
    if ($(window).width() > 991) {
      window.onscroll = function() {myFunction()};
      
      var navigation = document.querySelector(".navigation--container");
      if(navigation !== null){
        var sticky = navigation.offsetTop -30;
        var body = document.querySelector("body");
      }
      
      function myFunction() {
        if(navigation !== null){
          if (window.pageYOffset > sticky) {
            navigation.classList.add("wsNavigationFixed");
            body.classList.add("wsBodyNavigationFixed");
          } else {
            navigation.classList.remove("wsNavigationFixed");
            body.classList.remove("wsBodyNavigationFixed");
          }
        }
      }
    }
  });
</script>


<style>
