<script>
    document.getElementById('ffsearchform').addEventListener('submit',function(e){
      var inputElement = document.getElementById('quicksearch_inputfield');
      var inputElementValue = document.getElementById('quicksearch_inputfield').value;
      if(inputElementValue === "") {
        e.preventDefault();
        inputElement.placeholder = 'Bitte Artikelname eingeben';
        inputElement.classList.add('red-placeholder');
        setTimeout(function(){
          inputElement.placeholder = '';
          inputElement.classList.remove('red-placeholder');
        }, 3000);
        console.log("The input field is empty");
      }
    })
</script>

					

					
          
