            </script><script id="sna-iframe-storage" type="text/javascript">
            let storageSupported = false;
            try {
            storageSupported = window.localStorage && true;
            } catch (e) {}

            if (!storageSupported) {
            Object.defineProperty(window, 'localStorage', {
                value: {
                _data: {},
                setItem: function (id, val) {
                    return (this._data[id] = String(val));
                },
                getItem: function (id) {
                    return this._data.hasOwnProperty(id) ? this._data[id] : undefined;
                },
                removeItem: function (id) {
                    return delete this._data[id];
                },
                clear: function () {
                    return (this._data = {});
                }
                }
            });

            Object.defineProperty(window, 'sessionStorage', {
                value: {
                _data: {},
                setItem: function (id, val) {
                    return (this._data[id] = String(val));
                },
                getItem: function (id) {
                    return this._data.hasOwnProperty(id) ? this._data[id] : undefined;
                },
                removeItem: function (id) {
                    return delete this._data[id];
                },
                clear: function () {
                    return (this._data = {});
                }
                }
            });
            }</script><script id="sna-consent" type="text/javascript">
(() => {
  const callbacks = [];
  let consentRef = null;
  window.getSnaConsent = (callback) => {
      if(consentRef){ callback(consentRef) };
      callbacks.push(callback);
  };
  window.updateSnaConsent = (consent) => {
      consentRef = consent;
      callbacks.forEach((callback) => callback(consent));
  }
  getSnaConsent(() => { dataLayer.push({event: 'SNAConsentUpdated'}) });
})()</script><script id="one-trust2" async="" defer="" charset="UTF-8" data-domain-script="bf8c3351-46a8-4e25-8ff8-af1fb9b65ad9" type="text/javascript" src="https://cdn.cookielaw.org/scripttemplates/otSDKStub.js"></script><script src="https://flex.cybersource.com/cybersource/assets/microform/0.11/flex-microform.min.js" async=""></script>
