	<script type="text/javascript">
		( function() {
			var LOCALES = {"en_US":{"name":"English (United States)","slug":"en-us"},"es_ES":{"name":"Espa\u00f1ol","slug":"es-es"},"fr_FR":{"name":"Fran\u00e7ais","slug":"fr-fr"},"pt_BR":{"name":"Portugu\u00eas (Brasil)","slug":"pt-br"},"it_IT":{"name":"Italiano","slug":"it-it"},"de_DE":{"name":"Deutsch","slug":"de-de"},"de_AT":{"name":"Deutsch (\u00d6sterreich)","slug":"de-at"},"nl_BE":{"name":"Nederlands (Belgi\u00eb)","slug":"nl-be"},"fr_BE":{"name":"Fran\u00e7ais (Belgique)","slug":"fr-be"},"nl_NL":{"name":"Nederlands","slug":"nl-nl"},"pl_PL":{"name":"Polski","slug":"pl-pl"},"pt_PT":{"name":"Portugu\u00eas","slug":"pt-pt"},"de_CH":{"name":"Deutsch (Schweiz)","slug":"de-ch"},"fr_CH":{"name":"Fran\u00e7ais (Suisse)","slug":"fr-ch"},"it_CH":{"name":"Italiano (Svizzera)","slug":"it-ch"},"zh_SG":{"name":"\u4e2d\u6587 (\u65b0\u52a0\u5761)","slug":"zh-sg"},"zh_HK":{"name":"\u4e2d\u6587 (\u9999\u6e2f)","slug":"zh-hk"}};
			var cookieKey ='wccom-locale';
			var supportedLocales = ["en_US","es_ES","de_DE","fr_FR","it_IT","pt_BR"];
			var defaultLocale = 'en_US';
			// Locale from either URL path or a GET parameter
			var urlBasedLocale = 'en_US';
			var noticeText = 'This page is also <a href="%1$s" class="language-link" data-locale="%2$s">available in %3$s</a>.';

			/**
			 * Show the change locale notice notice
			 */
			function showChangeLocaleNotice( locale ) {
				var target = document.querySelector('.woocommerce-notices-wrapper');
				if ( target ) {
					target.classList.add( 'woocommerce-notices-wrapper-i18n' );
					var notice = document.createElement( 'div' );
					notice.classList.add( 'woocommerce-message' );
					notice.attributes['role'] = 'alert';

					notice.onclick = function() {
						setCookie( cookieKey, locale, 365 );
					};

					notice.innerHTML = noticeText
						.replace( '%1$s', localeUrl( locale ) )
						.replace( '%2$s', locale )
						.replace( '%3$s', localeName( locale ) );

					target.appendChild( notice );

				}
			}

			/**
			 * Get cookie from document cookie.
			 */
			function getCookie( cname ) {
				var name = cname + '=';
				var decodedCookie = decodeURIComponent( document.cookie );
				var ca = decodedCookie.split( ';' );
				for ( var i = 0; i < ca.length; i++ ) {
					var c = ca[ i ];
					while ( c.charAt(0) == ' ' ) {
						c = c.substring( 1 );
					}
					if ( c.indexOf( name ) == 0 ) {
						return c.substring( name.length, c.length );
					}
				}
				return '';
			}

			/**
			 * Set cookie
			 */
			function setCookie( name, value, days ) {
				var expires = '';
				if (days) {
					var date = new Date();
					date.setTime( date.getTime() + ( days*24*60*60*1000 ) );
					expires = '; expires=' + date.toUTCString();
				}
				document.cookie = name + '=' + (value || '')  + expires + '; sameSite=Strict; path=/';
			}

			/**
			 * Get locale slug from a given locale code.
			 */
			function localeSlug( locale ) {
				return LOCALES[ locale ]
					? LOCALES[ locale ].slug
					: '';
			}

			/**
			 * Get locale name from a given locale code.
			 */
			function localeName( locale ) {
				return LOCALES[ locale ]
					? LOCALES[ locale ].name
					: locale;
			}

			/**
			 * Get locale code from a given locale slug.
			 */
			function slugToLocale( slug ) {
				if ( '' === slug ) {
					return defaultLocale;
				}

				var locale = slug.substr( 0, 2 ) + '_' + slug.substr( -2 );
				return LOCALES[ locale ]
					? locale
					: '';
			}

			function generalizeLocale( locale ) {
				var localeMap = {
					es: 'es_ES',
					fr: 'fr_FR',
					pt: 'pt_BR',
					it: 'it_IT',
					de: 'de_DE',
				};

				var shortLocale = locale.substring( 0, 2 );

				return localeMap.hasOwnProperty( shortLocale )
					? localeMap[ shortLocale ]
					: locale;
			}

			/**
			 * Get browser locale
			 */
			function getBrowserLocale() {
				// browser locale uses - instead of _ e.g. en-US. We need to convert it to _ before using e.g. en_US
				var locale = window.navigator.language.replace( '-', '_' );
				return generalizeLocale( locale );
			}

			/**
			 * Get locale url
			 */
			function localeUrl( locale ) {
				var path = window.location.pathname;

				// Check if path has locale
				var urlLocale = defaultLocale;
				for ( const locale in LOCALES ) {
					if ( path.startsWith( '/' + localeSlug( locale ) ) ) {
						urlLocale = locale;
						break;
					}
				}

				// If path has locale remove it
				if ( urlLocale ) {
					path = path.replace( '/' + localeSlug( urlLocale ), '' );
				}

				var newPath = path;
				const slug = localeSlug( locale );
				if ( defaultLocale !== locale && slug ) {
					newPath = newPath.replace( /^\//, '' ); // Remove a prepending slash.
					newPath = '/' + slug + '/' + newPath;
				}

				const [_, pathnameWithQuery] = window.location.href.split( window.location.origin );
				const newPathNameWithQuery = pathnameWithQuery.replace(
					window.location.pathname,
					newPath
				);

				return `${ window.location.origin }${ newPathNameWithQuery }`;
			}

			function processLocales( localeCookie, browserLocale, urlBasedLocale ) {
				let isCookieChangeNeeded = false,
					newCookieValue = null,
					isRedirectNeeded = false,
					redirectURL = null,
					refererLocale = null;

				if ( ! localeCookie ) { // Locale cookie was not set
					isCookieChangeNeeded = true;
					newCookieValue = urlBasedLocale;
					if ( urlBasedLocale !== browserLocale ) {
						refererLocale = browserLocale;
					}

					if ( urlBasedLocale === defaultLocale
						&& browserLocale !== defaultLocale
						&& supportedLocales.includes( browserLocale )
					) {
						isRedirectNeeded = true; // User's preferences should be satisfied
						redirectURL = localeUrl( browserLocale );
						refererLocale = urlBasedLocale;
					}
				} else if ( urlBasedLocale !== defaultLocale
					&& ( browserLocale === defaultLocale || browserLocale !== urlBasedLocale )
				) {
					isCookieChangeNeeded = true; // Assume that user intentionally changed a URL
					newCookieValue = urlBasedLocale;
					refererLocale = browserLocale;
				} else if ( urlBasedLocale === defaultLocale
					&& browserLocale !== defaultLocale // A user set non-default locale earlier
					&& supportedLocales.includes( browserLocale )
				) {
					isRedirectNeeded = true; // Redirect to locale of their previous choice
					redirectURL = localeUrl( browserLocale );
					refererLocale = browserLocale;
				}

				return {
					isCookieChangeNeeded,
					newCookieValue,
					isRedirectNeeded,
					redirectURL,
					refererLocale,
				};
			}

			// Using this function because the object Cookie isn't available
			var localeCookie = getCookie( cookieKey );

			// Cookie locale has priority over browser locale
			var browserLocale = localeCookie ? localeCookie : getBrowserLocale();

			const {
				isCookieChangeNeeded,
				newCookieValue,
				isRedirectNeeded,
				redirectURL,
				refererLocale,
			} = processLocales( localeCookie, browserLocale, urlBasedLocale );

			if ( isCookieChangeNeeded && newCookieValue ) {
				setCookie( cookieKey, newCookieValue, 365 );
			}

			if ( isRedirectNeeded && redirectURL ) {
				window.location.href = redirectURL;
			}

			// Show locale changed notice if referer is the same page with a different language.
			if ( refererLocale && urlBasedLocale !== refererLocale ) {
				window.addEventListener(
					'DOMContentLoaded',
					function() {
						showChangeLocaleNotice( refererLocale );
					}
				);
			}
		}() );
	</script>
	
