signedIn: Boolean(window.FUTR && window.FUTR.Connect && window.FUTR.Connect.Auth && window.FUTR.Connect.Auth.UserId),
paywall: Boolean(window.FUTR && window.FUTR.Kiosq && window.FUTR.Kiosq.hasBarrier),
