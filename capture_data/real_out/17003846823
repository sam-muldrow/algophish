        (function() {

            
            var currentHost = window.location.hostname.toLowerCase();

            if(!(window.location.href.toLowerCase().includes('gtmdisabled=true'))) {
            if ((/qa1.|localhost/.test(currentHost)) )   {               
                    document.write('<script src="assets/js/gtm_qa1.js"><\/script>');

            }else if ((/qa2./.test(currentHost)) ) {  

                    document.write('<script async src="assets/js/gtm_qa2.js"><\/script>');

            }else if (/qa3./.test(currentHost)) {                

                    document.write('<script src="assets/js/gtm_qa3.js"><\/script>');

            }else if (/redemption./.test(currentHost)) {                

                document.write('<script src="assets/js/gtm_redemption.js"><\/script>');

            } else if ( (/beta-rd./.test(currentHost)) ){               
  
                    document.write('<script src="assets/js/gtm_pilot.js"><\/script>');

            }else {
   
                document.write('<script src="assets/js/gtm_prod.js"><\/script>');
            }
            
        }
        })();
