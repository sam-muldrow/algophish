<script>
var url = document.location.toString();
  if ( url.match('#') ) {
    $('#'+url.split('#')[1]).addClass('in');
}    
</script>



            <script>
              if(imageData!=""){
                var objData = eval( '( [' + imageData + '] )');
                for(var i=0; i< objData.length; i++){
                  loadImage(objData[i].guid, (i==0));
                  }
               }
              
              function loadImage(img_guid, flag){
                var url = window.location.protocol + "//" + window.location.host + "/CMSPages/GetFile.aspx?guid=" + img_guid + "&id=23112023012652";
                
                setTimeout(function(){
jQuery.ajax({ 
    url : url, 
    cache: false,
    processData : false,
    async : true
}).always(function(){
    jQuery("#IMAGE_ID" + img_guid).attr("src", url);
    //jQuery("#IMAGE_ID" + img_guid).attr("data-src=", url);

});                   
                }, (flag ? 0 : 1000));

              }
</script>


    
