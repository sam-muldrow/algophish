<script type="text/javascript">
     $(document).ready(function() {
  if(window.location.href.indexOf("PB-RCB-Mastercard-Premier-Debit-Cards") > -1 || window.location.href.indexOf("PB-Corporate-MyDebit-Card") > -1 || window.location.href.indexOf("PB-UnionPay-Lifestyle-Debit-Card") > -1 || window.location.href.indexOf("PB-Visa-Business-Card") > -1 || window.location.href.indexOf("PB-MasterCard-Lifestyle-Debit-Card") > -1 || window.location.href.indexOf("Mutual-Gold-PB-Visa-Platinum-Credit-Card") > -1 || window.location.href.indexOf("PB-Visa-Commercial-Card") > -1 || window.location.href.indexOf("PB-Visa-Classic") > -1 || window.location.href.indexOf("PB-MasterCard-Standard") > -1 || window.location.href.indexOf("PB-Day2Day-Card") > -1) {
    $('a').filter(function(index){return $(this).text()=== "Apply Now"; }).parent("li").hide();
    $('a').filter(function(index){return $(this).text()=== "Resume Application"; }).parent("li").hide();
         }
     });
