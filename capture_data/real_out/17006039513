				// FunciÃÂ³ per a definir que l'ultim marker pulsat ocupe el focus principal

				if(selectedValue!==0)
				{

					var markerAux = "marker"+selectedValue

					window[markerAux].setZIndexOffset(prioritatZ);  // Augmentem la prioritat de l'ultim marker polsat

					//console.log(markerAux);

					prioritatZ ++;  // El nivel de prioritat s'incrementarar en cada seleccio per a aconseguir que l'ultim marker polsat aparegue per damunt dels altres
				}

				Tancar();  // Amaguem el sidebar

			}


			// Funcio que serveix per a mostrar / ocultar el Google Maps


			function ActivarGoogleViewd5(ipunto)   // Recordem que en "ipunto" tenim la referÃÂ¨ncia de l'index de l'opciÃÂ³ seleccionada desde el selector
			{
				if (GoogleViewActiu==false)  // Si el pano estÃÂ  inactiu
				{
					var elemento=document.getElementById("pano");  // Recollim la referÃÂ¨ncia del div "pano" (on es dibuixa l'Street View)
					elemento.style.display = "block";   //  Mostrem el div "pano" amb Street View
					GoogleViewActiu=true;
					//console.log("Dins del false -> true");
				}
				else    // Si el pano ja estÃÂ  actiu
				{
					var elemento=document.getElementById("pano");   // Recollim la referÃÂ¨ncia del div "pano" (on es dibuixa l'Street View)
					elemento.style.display = "none";     //  Mostrem el div "pano" amb Street View
					GoogleViewActiu=false;
					//console.log("Dins del true -> false");
				}

				if (ipunto==537){var fenway = new google.maps.LatLng(39.4753788,-0.386349); var panoramaOptions ={position: fenway,pov: {heading: 45, pitch: 0}};var panorama = new  google.maps.StreetViewPanorama(document.getElementById('pano'),panoramaOptions);}if (ipunto==539){var fenway = new google.maps.LatLng(39.945696,-1.1019614); var panoramaOptions ={position: fenway,pov: {heading: 45, pitch: 0}};var panorama = new  google.maps.StreetViewPanorama(document.getElementById('pano'),panoramaOptions);}if (ipunto==540){var fenway = new google.maps.LatLng(39.514739, -0.424333); var panoramaOptions ={position: fenway,pov: {heading: 45, pitch: 0}};var panorama = new  google.maps.StreetViewPanorama(document.getElementById('pano'),panoramaOptions);}if (ipunto==541){var fenway = new google.maps.LatLng(39.479415, -0.364685); var panoramaOptions ={position: fenway,pov: {heading: 45, pitch: 0}};var panorama = new  google.maps.StreetViewPanorama(document.getElementById('pano'),panoramaOptions);}    <!-- INYECTEM LA INFORMACIO DE L'STREET VIEW -->   <!--  infoGoogleView2 = infoGoogleView2 + "if (ipunto=="+contador+"){var fenway = new google.maps.LatLng("+latlng+"); var panoramaOptions ={position: fenway,pov: {heading: 45, pitch: 0}};var panorama = new  google.maps.StreetViewPanorama(document.getElementById('pano'),panoramaOptions);}";  -->
			}


