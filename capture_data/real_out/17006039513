					var markerAux = "marker"+selectedValue

					window[markerAux].setZIndexOffset(prioritatZ);  // Augmentem la prioritat de l'ultim marker polsat

					//console.log(markerAux);

					prioritatZ ++;  // El nivel de prioritat s'incrementarar en cada seleccio per a aconseguir que l'ultim marker polsat aparegue per damunt dels altres
				}

				Tancar();  // Amaguem el sidebar

			}


			// Funcio que serveix per a mostrar / ocultar el Google Maps


			function ActivarGoogleViewd10(ipunto)   // Recordem que en "ipunto" tenim la referÃÂ¨ncia de l'index de l'opciÃÂ³ seleccionada desde el selector
			{
				if (GoogleViewActiu==false)  // Si el pano estÃÂ  inactiu
				{
					var elemento=document.getElementById("pano");  // Recollim la referÃÂ¨ncia del div "pano" (on es dibuixa l'Street View)
					elemento.style.display = "block";   //  Mostrem el div "pano" amb Street View
					GoogleViewActiu=true;
					//console.log("Dins del false -> true");
				}
				else    // Si el pano ja estÃÂ  actiu
				{
					var elemento=document.getElementById("pano");   // Recollim la referÃÂ¨ncia del div "pano" (on es dibuixa l'Street View)
					elemento.style.display = "none";     //  Mostrem el div "pano" amb Street View
					GoogleViewActiu=false;
					//console.log("Dins del true -> false");
				}

				if (ipunto==657){var fenway = new google.maps.LatLng(39.4784679419641, -0.35904755262301863); var panoramaOptions ={position: fenway,pov: {heading: 45, pitch: 0}};var panorama = new  google.maps.StreetViewPanorama(document.getElementById('pano'),panoramaOptions);}if (ipunto==658){var fenway = new google.maps.LatLng(39.4782018625977, -0.3593139976790741); var panoramaOptions ={position: fenway,pov: {heading: 45, pitch: 0}};var panorama = new  google.maps.StreetViewPanorama(document.getElementById('pano'),panoramaOptions);}if (ipunto==661){var fenway = new google.maps.LatLng(39.477986876752304, -0.35790458321571356); var panoramaOptions ={position: fenway,pov: {heading: 45, pitch: 0}};var panorama = new  google.maps.StreetViewPanorama(document.getElementById('pano'),panoramaOptions);}if (ipunto==663){var fenway = new google.maps.LatLng(39.48091947348641, -0.3568240467573026); var panoramaOptions ={position: fenway,pov: {heading: 45, pitch: 0}};var panorama = new  google.maps.StreetViewPanorama(document.getElementById('pano'),panoramaOptions);}if (ipunto==665){var fenway = new google.maps.LatLng(39.48147923044804, -0.35503417254176384); var panoramaOptions ={position: fenway,pov: {heading: 45, pitch: 0}};var panorama = new  google.maps.StreetViewPanorama(document.getElementById('pano'),panoramaOptions);}if (ipunto==666){var fenway = new google.maps.LatLng(39.481135, -0.355846); var panoramaOptions ={position: fenway,pov: {heading: 45, pitch: 0}};var panorama = new  google.maps.StreetViewPanorama(document.getElementById('pano'),panoramaOptions);}if (ipunto==669){var fenway = new google.maps.LatLng(39.4781226158567, -0.3578120001466461); var panoramaOptions ={position: fenway,pov: {heading: 45, pitch: 0}};var panorama = new  google.maps.StreetViewPanorama(document.getElementById('pano'),panoramaOptions);}    <!-- INYECTEM LA INFORMACIO DE L'STREET VIEW -->   <!--  infoGoogleView2 = infoGoogleView2 + "if (ipunto=="+contador+"){var fenway = new google.maps.LatLng("+lat