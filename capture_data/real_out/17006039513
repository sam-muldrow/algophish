			// FunciÃÂÃÂ³ per redibuixar el mapa segons l'espai seleccionat

			function Seleccionard9()
			{
				GoogleViewActiu=false;
				map.addLayer(d9);
				var selectBox = document.getElementById("selectord9");  // Recollim el ID del selector
				var selectedValue = selectBox.options[selectBox.selectedIndex].value;  // Recollim el ID del valor polsat

				// Extraem l'index de cadascun dels noms dels espais per redibuixar el mapa amb la localitzaciÃÂÃÂ³ i el titol adient

				if (selectedValue==653){map.setView(new L.LatLng(39.5156314,-0.4223676), 22,{animation: true});marker653.bindPopup('<img class="popupStyle" src="//www.uv.es/uwm/imatges/GMaps/logo_uv.png" width="50%" alt="LogoUV"/><div class="popupTitol">'+"Fundació Parc Científic"+'</div><div class="popupStyle"><p> <span class="fa fa-globe" aria-hidden="true"></span> &nbsp;<a href="https://www.pcuv.es/va/home">www.pcuv.es</a></p><p> <span class="fa fa-phone" aria-hidden="true"></span> &nbsp; <a href="tel:963 544 758">963 544 758</a> </p><p> <span class="fa fa-envelope" aria-hidden="true"></span> &nbsp;  <a href="mailto:parc.cientific@uv.es">parc.cientific@uv.es</a> </p></div><div></div>'+'<div id="pano" class="googleView"></div>'+'<button class="botoGoogle" onclick="ActivarGoogleViewd9(653);">Veure Street View</button>').openPopup();}   <!-- INYECTEM LA INFORMACIÃÂ? DE L'ESPAI -->      <!-- infoEspai = infoEspai + "if (selectedValue=="+contador+"){map.setView(new L.LatLng("+latlng+"), 22,{animation: true});marker"+contador+".bindPopup(\""+titol+"\").openPopup();}";-->

				if (selectedValue==653){var fenway = new google.maps.LatLng(39.5156314,-0.4223676); var panoramaOptions ={position: fenway,pov: {heading: 45, pitch: 0}};var panorama = new  google.maps.StreetViewPanorama(document.getElementById('pano'),panoramaOptions);}   <!-- INYECTEM LA INFORMACIO DE L'STREET VIEW -->    <!-- infoGoogleView = infoGoogleView + "if (selectedValue=="+contador+"){var fenway = new google.maps.LatLng("+latlng+"); var panoramaOptions ={position: fenway,pov: {heading: 45, pitch: 0}};var panorama = new  google.maps.StreetViewPanorama(document.getElementById('pano'),panoramaOptions);}"; -->


				// FunciÃÂ³ per a definir que l'ultim marker pulsat ocupe el focus principal

				if(selectedValue!==0)
				{

					var markerAux = "marker"+selectedValue

					window[markerAux].setZIndexOffset(prioritatZ);  // Augmentem la prioritat de l'ultim marker polsat

					//console.log(markerAux);

					prioritatZ ++;  // El nivel de prioritat s'incrementarar en cada seleccio per a aconseguir que l'ultim marker polsat aparegue per damunt dels altres
				}

				Tancar();  // Amaguem el sidebar

			}


			// Funcio que serveix per a mostrar / ocultar el Google Maps


			function ActivarGoogleViewd9(ipunto)   // Recordem que en "ipunto" tenim la referÃÂ¨ncia de l'index de l'opciÃÂ³ seleccionada desde el selector
			{
				if (GoogleViewActiu==false)  // Si el pano estÃÂ  inactiu
				{
					var elemento=document.getElementById("pano");  // Recollim la referÃÂ¨ncia del div "pano" (on es dibuixa l'Street View)
					elemento.style.display = "block";   //  Mostrem el div "pano" amb Street View
					GoogleViewActiu=true;
					//console.log("Dins del false -> true");
				}
				else    // Si el pano ja estÃÂ  actiu
				{
					var elemento=document.getElementById("pano");   // Recollim la referÃÂ¨ncia del div "pano" (on es dibuixa l'Street View)
					elemento.style.display = "none";     //  Mostrem el div "pano" amb Street View
					GoogleViewActiu=false;
					//console.log("Dins del true -> false");
				}

				if (ipunto==653){var fenway = new google.maps.LatLng(39.5156314,-0.4223676); var panoramaOptions ={position: fenway,pov: {heading: 45, pitch: 0}};var panorama = new  google.maps.StreetViewPanorama(document.getElementById('pano'),panoramaOptions);}    <!-- INYECTEM LA INFORMACIO DE L'STREET VIEW -->   <!--  infoGoogleView2 = infoGoogleView2 + "if (ipunto=="+contador+"){var fenway = new google.maps.LatLng("+latlng+"); var panoramaOptions ={position: fenway,pov: {heading: 45, pitch: 0}};var panorama = new  google.maps.StreetViewPanorama(document.getElementById('pano'),panoramaOptions);}";  -->
			}


