			// FunciÃÂÃÂ³ per redibuixar el mapa segons l'espai seleccionat

			function Seleccionard3()
			{
				GoogleViewActiu=false;
				map.addLayer(d3);
				var selectBox = document.getElementById("selectord3");  // Recollim el ID del selector
				var selectedValue = selectBox.options[selectBox.selectedIndex].value;  // Recollim el ID del valor polsat

