			// FunciÃÂÃÂ³ per redibuixar el mapa segons l'espai seleccionat

			function Seleccionard7()
			{
				GoogleViewActiu=false;
				map.addLayer(d7);
				var selectBox = document.getElementById("selectord7");  // Recollim el ID del selector
