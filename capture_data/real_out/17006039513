		function mostrarLista (n) {
			var visibility = $('.enlace-cab-'+n+' ul').css( "visibility" ); 
			console.log('primero: ');
			console.log();
			if(visibility != 'visible') {

				console.log('tercero: ');
				console.log();
				$('.enlace-cab-'+n+' ul').css( "opacity","1" ); 
				$('.enlace-cab-'+n+' ul').css( "visibility","visible" ); 
