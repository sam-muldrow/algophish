	<script type="text/javascript">

	var numImg = '0';
	
	var boxElement47720 = $('.callAction-complet.47720 .call-info');
	
	if (numImg > 1){
	    $('.callAction-complet.47720 .callBackgrounds').imgCrossfade({
	        interval: 10000    
	    });
	}

	$(window).scroll(function() {
	    //console.log("fire: "+'47720');
			var options = {};
		   var hT = $('.callAction-complet.47720').offset().top,
		       hH = $('.callAction-complet.47720').outerHeight(),
		       wH = $(window).height(),
		       wS = $(this).scrollTop();
		    
		   if (wS > (hT+hH-wH)){
			 
			   boxElement47720.removeClass(function (index, css) {
					return (css.match (/\bcallAction-animacio-\S+/g) || []).join(' '); // removes anything that starts with "page-"
				});
		   }
		});
	
