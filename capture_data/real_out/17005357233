					try { hopscotch.startTour({"id":"search-tour","i18n":{"nextBtn":"Weiter","prevBtn":"Zur\u00fcck","doneBtn":"Tour beenden","closeTooltip":" "},"steps":[{"placement":"bottom","width":250,"zindex":10001,"xOffset":"0","arrowOffset":"40","showCloseButton":true,"showPrevButton":true,"showNextButton":true,"target":"#sl-logo:visible","title":"Zur\u00fcck zur Startseite","content":"\u00dcber das Logo gelangen Sie jederzeit zur\u00fcck auf die Startseite."},{"placement":"bottom","width":250,"zindex":10001,"xOffset":"center","arrowOffset":"center","showCloseButton":true,"showPrevButton":true,"showNextButton":true,"target":".sl-form-row-field:visible","title":"Suchen in allen Diensten","content":"Suche im Web, Telefonbuch, Wetter, Karte, Fahrplan und Routenplaner."},{"placement":"bottom","width":250,"zindex":10001,"xOffset":"center","arrowOffset":"center","showCloseButton":true,"showPrevButton":true,"showNextButton":true,"target":".www-springboard-tile .sl-icon-service-tel:visible","title":"Dienstsuche","content":"Gezieltes Suchen innerhalb eines Dienstes."},{"placement":"bottom","width":250,"zindex":10001,"xOffset":"center","arrowOffset":"center","showCloseButton":true,"showPrevButton":true,"showNextButton":true,"target":".www-springboard-tile .sl-icon-service-map:visible","title":"Routenplaner in der Karte","content":"Der Routenplaner ist neu in der Karte integriert."},{"placement":"left","width":250,"zindex":10001,"xOffset":"0","arrowOffset":"-1","showCloseButton":true,"showPrevButton":true,"showNextButton":true,"target":"#sl-more-link:visible","title":"Mehr","content":"Websuche, Schneebericht und Informationen \u00fcber search.ch. Hier k\u00f6nnen Sie die Tour jederzeit wieder starten."}]}) } catch(e) {}
				};

				e.preventDefault();
				if ($(".sl-flyout-menu").hasClass("open"))
					$("#sl-more-link").trigger("click");	// Close menu

				if ("hopscotch" in window)
					startTour();
				else {
					$("<link>").appendTo("head").attr({
						type: "text/css",
						rel: "stylesheet",
						href: U("/itjs/hopscotch.css")
					});

					$.getScript(U("/itjs/hopscotch.min.js"), startTour);
				}

				new Image().src = "//search.ch/lib/tours.gif?service=www_full&t=" + Math.floor(Date.now() / 1000);
			});
			try {
				if (window.location.href.match(/(\?|&)tour=1($|&)/) || hopscotch.getState() !== null)
					$(".sl-start-tour").first().trigger("click");
			} catch(e) {}
		})</script>
