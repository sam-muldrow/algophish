        setTimeout(function () {
            if ($(".mw-80.C_AD").children().length > 1) {
                $("#C17").removeClass("d-none");
                $("#C17").addClass("d-flex");
            }
        }, 18000);
        $('.fa-window-close').click(function () {
            $("#C17").removeClass("d-flex");
            $("#C17").addClass("d-none");
        });
    </script>
