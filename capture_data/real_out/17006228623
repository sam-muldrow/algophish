<script>
  let userMenu = $("#user-menu")
  let st = {}
  $("#user-menu-button").hover(function () {
    clearTimeout(st)
    userMenu.show()
  },function () {
    st = setTimeout(function () {
      userMenu.hide()
    },200)
  })
  userMenu.hover(function () {
    clearTimeout(st)
    userMenu.show()
  },function () {
    st = setTimeout(function () {
      userMenu.hide()
    },200)
  })
</script>

