      target="_blank">点击此处申请开源</a> ｜
    <a href="https://beian.miit.gov.cn/" target="_blank">鄂ICP备18029942号-4</a> ｜ <a href="https://www.wpxap.com/report"
      class="font-bold text-black" target="_blank">联系站长</a> ｜ <a onclick="report(0,0)"
      class="link " href="javascript:;" >投诉/举报</a>
  </div>
  <div class="popup" style="display: none" id="popup-report" aria-labelledby="modal-title" role="dialog" aria-modal="true">
    <div class="popup-box">
        <div class="popup-bg" aria-hidden="true"></div>
        <!-- This element is to trick the browser into centering the modal contents. -->
        <span class="popup-center" aria-hidden="true">&#8203;</span>
        <div class="popup-main">
            <div class="popup-main-bg">
                <div class="sm:flex sm:items-start">
                    <div class="popup-icon">
                        <!-- Heroicon name: outline/exclamation -->
                        <svg class="h-6 w-6 text-red-600" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor" aria-hidden="true">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 9v2m0 4h.01m-6.938 4h13.856c1.54 0 2.502-1.667 1.732-3L13.732 4c-.77-1.333-2.694-1.333-3.464 0L3.34 16c-.77 1.333.192 3 1.732 3z" />
                        </svg>
                    </div>
                    <div class="popup-content">
                        
<h3 class="text-lg leading-6 font-medium text-gray-900" id="modal-title">
    投诉/举报/反馈
</h3>
<div class="mt-2 block">
    <label for="report_msg"></label>
    <div class="mt-2"><textarea id="report_msg" name="msg" rows="3"
            class=" shadow-sm focus:ring-indigo-500 focus:border-indigo-500 mt-1 block w-full sm:text-sm border border-gray-300 rounded-md"
            placeholder="请说明理由"></textarea>
    </div>
    <div id="email"><a class="link text-xs" href="mailto:admin@wpxap.com" target="_blank">无法提交？点此联系管理员</a></div>
</div>

                    </div>
                </div>
            </div>
            <div class="popup-foot">
                <button type="button" id="confirm" class="btn-lg btn sm:ml-3 sm:w-auto sm:text-sm">
                    确定
                </button>
                <button type="button" id="cancel" class="btn-lg btn-red mt-3 sm:mt-0 sm:ml-3 sm:w-auto sm:text-sm">
                    取消
                </button>
            </div>
        </div>
    </div>
</div>

<script>
    let popupReport = $("#popup-report")
    let reportPath = "";
    let reportMsg = "";
    let reportPid = 0;
    let reportTid = 0;
    function report(pid, tid) {
        reportPath = window.location.href + "#pid_" + pid;
        reportPid = pid
        reportTid = tid
        reportShow()
    }
    function reportShow() {
        popupReport.fadeIn(100)
    }
    function reportHide() {
        popupReport.fadeOut(100)
    }
    popupReport.find("#confirm").click(function () {
        let _this = $(this)
        _this.attr("disabled", true);
        reportMsg = $("#report_msg").val();
        $.ajax({
            type: 'post',
            url: '/api/v1/post_report',
            dataType: 'json',
            contentType: "application/json; charset=utf-8",
            data: JSON.stringify({ msg: reportMsg, path: reportPath, pid: reportPid, tid: reportTid }),
            success: function (res) {
                //alert("感谢您的反馈，已经提交给管理员处理！")
                reportHide()
                _this.attr("disabled", false);
                $("#report_msg").val("");
            },
            error: function (err) {
                console.log(err)
                alert(err.responseJSON.message)
                window.open("https://www.wpxap.com/report", "_blank");
                _this.attr("disabled", false);
            }
        })
    })
    popupReport.find("#cancel").click(function () {
        reportHide()
    })
</script>


