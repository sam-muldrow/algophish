<script>
  zhidingdo([20230410013],[20230411171911])
  function zhidingdo(b,c){
	  var Kda      = new Date();        var Kyear = Kda.getFullYear()
	  var Kmonth   = Kda.getMonth()+1;  if(Kmonth  <10){Kmonth  ='0'+Kmonth}
	  var Kdate    = Kda.getDate();     if(Kdate   <10){Kdate   ='0'+Kdate}
	  var Khours   = Kda.getHours();    if(Khours  <10){Khours  ='0'+Khours}
	  var Kminutes = Kda.getMinutes()+1;if(Kminutes<10){Kminutes='0'+Kminutes}
	  var Kseconds = Kda.getSeconds();  if(Kseconds<10){Kseconds='0'+Kseconds}
	  var Knowtime = [Kyear,Kmonth,Kdate,Khours,Kminutes,Kseconds].join('')
		var t1 = window.setTimeout(function() {
			for(var i=0;i<b.length;i++){
				if(Knowtime>c[i]){
					var lb=document.getElementById('li'+b[i])
					if(lb){lb.style.display="none";continue;}
				}
				for(var j = 1; j<= 5; j++){
					var a = document.getElementsByClassName('list-focus'+j)
					for(var k = 0; k< a.length; k++)
						if(a[k].outerHTML.indexOf(b[i])>-1 && a[k].outerHTML.indexOf('<!--zhiding-->')==-1) 
							{a[k].style.display='none';break}
				}
			}
		},500)
  }
