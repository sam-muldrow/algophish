	<script>
  $(function(){
    var cloudChinaVideoSwiper = new Swiper('#cloudChinaVideo-swiper', {
      loop: false,
      slidesPerView: 4
    })

    $('#cloudChina-arrowLeft').on("click", function () {
      cloudChinaVideoSwiper.swipePrev()
      return false;
    })
    $('#cloudChina-arrowRight').on("click", function () {
      cloudChinaVideoSwiper.swipeNext()
      return false;
    })



    var preview_img_url0;
    var url;
    var videoId;
    var player0;
    var $cloudChinaVideoList;
    var isAuto = false;
    var address = 'https://www.cnr.cn/yyzg/index.html?id='
    var $cloudChinaName = $('.cloudChina-name')
    //ÂýÖ±²¥
    $.ajax({
      "url": "http://icmspc.cnr.cn/page",
      "method": "POST",
      dataType: 'json',
      "headers": {
        "Content-Type": "application/json"
      },
      "data": JSON.stringify({
        "id": 1,
      }),
    }).done(function (response) {
      if (response && response.code == 200) {
        var datas = response.data;
        var len = datas.length;
        for (var i = 0; i < len; i++) {
          var str;
          if ( i == 0) {
            str = '<div class="cloudChinaItem isSelect" onClick="clickList(this)" data-info5="' + datas[i].detail[0].other_info5 + '" data-id="'+ datas[i].id +'" data-status="' + datas[i].detail[0].other_info7 + '" data-url="' + datas[i].detail[0].other_info9 + '" data-title="' + datas[i].detail[0].name + '"><img src="' + datas[i].detail[0].img + '"><div class="swiper_name">'+ datas[i].title +'</div></div>';
          } else {
            str = '<div class="cloudChinaItem" onClick="clickList(this)" data-info5="' + datas[i].detail[0].other_info5 + '" data-id="'+ datas[i].id +'" data-status="' + datas[i].detail[0].other_info7 + '" data-url="' + datas[i].detail[0].other_info9 + '" data-title="' + datas[i].detail[0].name + '"><img src="' + datas[i].detail[0].img + '"><div class="swiper_name">'+ datas[i].title +'</div></div>';
          }
          var newSlide = cloudChinaVideoSwiper.createSlide(str);
          newSlide.append(); 
          $cloudChinaVideoList = $('.cloudChinaItem');
        }
        
        $cloudChinaName.attr('href', address + datas[0].id).text(datas[0].detail[0].name);
        preview_img_url0 = datas[0].detail[0].other_info5;
        url = datas[0].detail[0].other_info9;

        buildVideo("", url);
      } else {
        console.log(response.code)
      }
    })
    window.clickList = function (a) {
      if ($(a).hasClass('isSelect')) {
        return false;
      }
      $('.cloudChina-poster').hide();
      $cloudChinaVideoList.removeClass('isSelect');
      $(a).addClass('isSelect');
      preview_img_url0 = $(a).attr('data-info5');
      url = $(a).attr('data-url');
      videoId = $(a).attr('data-id');
      $cloudChinaName.attr('href', address + videoId).text($('.cloudChinaItem.isSelect').attr('data-title'));
      buildVideo("", url);
    }
    function buildVideo(img, url) {
      var autoplayvalue0 = true;
      if (player0) {
        isAuto = true;
        player0.destroy();
        $('.vcp-bigplay').show();
        url = $('.cloudChinaItem.isSelect').attr("data-url");
        preview_img_url0 = $('.cloudChinaItem.isSelect').attr("data-info5");
        $cloudChinaName.attr('href', address + videoId).text($('.cloudChinaItem.isSelect').attr('data-title'));
      }
      player0 = new TcPlayer('cloudChinaVideo', {
        "m3u8": url,
        "autoplay": isAuto,
		"volume": 0,
        "poster": {
          "style": "cover",
          "src": preview_img_url0
        },
        "controls": 'none',
        "bigPlayButton": true,
        "listener": function (msg) {
          if (msg.type == 'play') {
            $('.vcp-bigplay').hide();
          } else if (msg.type == 'ended') {
            $('.vcp-bigplay').show();
          }
          if (msg.type == 'error') {
            $('.cloudChina-poster img').attr('src', $('.cloudChinaItem.isSelect').attr('data-info5'));
            $('.cloudChina-poster').show();
            player0.stop();
          }
        }
      });
	  player0.volume(0)
    }

  })
  


</script>
