	$(function(){
	  var locationNewsSwiper = new Swiper('#location-news-swiper', {
	      autoplay: 30000,
	      loop: true,
	      mode: 'vertical',
	      slidesPerView: 2,
	      speed: 1000
	    })
	    $('#location-news-swiper').on("mouseenter", function () {
	      locationNewsSwiper.stopAutoplay()
	    })
	    $('#location-news-swiper').on("mouseleave", function () {
	      locationNewsSwiper.startAutoplay()
	    })
	
	  $.ajax({
	    method: 'POST',
	    url: 'https://icmspc.cnr.cn/region',
	    data: JSON.stringify({}),
	    dataType: 'json',
	    success: function(res){
	      console.log(res)
	      if (res.code == 200) {
	        const data = res.data[0].detail
	        if (data.length >= 2) {
			  $('.location-news .swiper-container').css({"height": 96})
	          $('.wrapper .row2 .w420 .list li:last').remove()
	          $('.wrapper .row2 .w420 .list li:last').remove()
	          for (var i = 0; i < data.length; i++) {
	            var newSlide = locationNewsSwiper.createSlide('<div class="boder"><span><a href="'+ data[i].other_info2 +'" target="_blank">'+ data[i].name +'</a></span></div>');
	            newSlide.append(); 
	          }
	          
	        } else {
	          $('.location-news').hide()
	        }
	      } else {
	        $('.location-news').hide()
	        console.log(res.message)
	      }
	    },
	    fail: function(err){
	      $('.location-news').hide()
	      console.log(err)
	    }
	  })
	})
