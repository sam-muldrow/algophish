  <script>$(function () {
      // Í¼Æ¬ÀÁ¼ÓÔØ
      $.lazyImg()
  
  
      var headerSwiper = new Swiper('#header-swiper', {
        autoplay: 5000,
        loop: true,
        mode: 'vertical',
        autoplayDisableOnInteraction: false
      })
      $('#header-swiper').on("mouseenter", function () {
        headerSwiper.stopAutoplay()
        return false;
      })
      $('#header-swiper').on("mouseleave", function () {
        headerSwiper.startAutoplay()
        return false;
      })
  
  
      var fouceSwiper = new Swiper('#fouce-swiper', {
        pagination: '#fouce-swiper-pagination',
        autoplay: 8000,
        loop: true,
        paginationClickable: true,
        noSwiping: true,
        autoplayDisableOnInteraction: false
      })
      $('#fouce-swiper-prev').on("click", function () {
        fouceSwiper.swipePrev()
        return false;
      })
      $('#fouce-swiper-next').on("click", function () {
        fouceSwiper.swipeNext()
        return false;
      })
  
  
      var unleashList = {}
      var slide_duration = 3000;
      var timer = null;
      var currentUnleash = null;
      var unleashDom;
      var $localArrowLeft = $('#local-arrowLeft')
      var $localArrowRight = $('#local-arrowRight')
      var locationSwiper = new Swiper('#swiper-location', {
        autoplay: false,
        speed: 2000,
        loop: true,
        onInit: function (swiper) {
          //Swiper³õÊ¼»¯ÁË
          unleashDom = $('.unleash_slider');
          for (var i = 0; i < unleashDom.size(); i++) {
            var slide = unleashDom.eq(i).unleash({
              slide_width: "45.833%",
              duration: 700,
              slide_duration: slide_duration,
              slideshow: true,
              initially_open_slide: 0,
              hide_controls: true,
              pause_onmouseover: true,
              open_event: 'mouseover',
              onSlideOpen: onSlideOpen,
              //onSlideOpenEnd: onSlideOpenEnd
            }).data("plugin_unleash")
            unleashList[i] = slide
            unleashList[i].closeAll()
          }
          currentUnleash = unleashList[1]
          currentUnleash.OpenSlide(1)
        },
        onSlideChangeStart: function (swiper) {
          if (Object.keys(unleashList).length !== 0) {
            for (var index in unleashList) {
              unleashList[index].pause()
            }
      
            unleashList[swiper.activeIndex].OpenSlide(1)
          }
        },
        onSlideChangeEnd: function (swiper) {
          //unleashList[swiper.activeIndex].play()
        }
      })
      $localArrowLeft.on("click", function () {
        locationSwiper.swipePrev()
        return false;
      })
      $localArrowRight.on("click", function () {
        locationSwiper.swipeNext()
        return false;
      })
      
      function onSlideOpenEnd(number) {
        var current = unleashList[locationSwiper.activeIndex]
        var total = current.NumberOfSlides();
        if (number == total) {
          locationSwiper.swipeNext()
        }
      }
      
      function onSlideOpen(event) {
        currentUnleash = unleashList[locationSwiper.activeIndex]
        var slideNumber = currentUnleash.CurrentSlide();
        var total = currentUnleash.NumberOfSlides();
        if (slideNumber == total) {
          if (event == "mouseover") {
            timer = setTimeout(function () {
              locationSwiper.swipeNext()
              clearTimeout(timer)
            }, 2000)
          }
        }
      }
  
      $(document).on('mouseenter mouseleave', '.itemew', function () {
         $(this).toggleClass('hover')
      });
      $('.appplus').hover(function () {
        $(".tipp").show();
      }, function () {
        $('.tipp').hide();
      });
  
  
      $('.hide1,.close').click(function () {
        $('.mask').hide();
        $('.tip').hide();
        $('.listen_icon').show();
      })
      $('.listen_icon').click(function () {
        var offset = $(this).offset();
        $('.tip').css({
          'margin-top': 0,
          'left': offset.left - 220,
  
        }).show();
      })
  
  
      // ·µ»Ø¶¥²¿
      var $row1 = $('.row1').offset()
      var $floatNavWrapper = $('.floatNav-wrapper')
      var $goback = $('.goback')
      $(window).scroll(function () {
        var top = $(window).scrollTop()
        if (top > 50) {
          $goback.fadeIn(200);
        } else {
          $goback.fadeOut(200);
        }
  
        if (top > $row1.top) {
          $floatNavWrapper.css({
            visibility: "visible"
          })
        } else {
          $floatNavWrapper.css({
            visibility: "hidden"
          })
        }
      });
      $goback.click(function () {
        $('body,html').animate({
          scrollTop: 0
        },
          500);
      })
  
      //¶þÎ¬Âë·ÖÏí
      $(document).on('click', '.btn_towcode', function () {
        var number = $(this).children().length
        $('.btn_towcode').empty();
        if (number == 0) {
          $(this).append('<div class="towCode"></div>')
        }
  
        return false
      })
      $(document).on('click', function (evt) {
        $('.btn_towcode').empty();
      })
  
      //¶úÎÅ¶þÎ¬Âë·ÖÏí
      $(document).on('click', '.share', function () {
        var number = $(this).children().length
        $('.share').empty();
        if (number == 0) {
          $(this).append('<div class="towCode"></div>')
        }
  
        return false
      })
      $(document).on('click', function (evt) {
        $('.share').empty();
      })
  
      // ¹ú³±µã»÷
      $('#guochao_dianji').on('click', function () {
        $('html,body').animate({ scrollTop: $('#guochao').offset().top }, 1000);
        return false;
      })
  
  
      // ¸¡¶¯µ¼º½
      $('.openFloatNav-hidden').click(function () {
        $('.floatNav-hidden').show()
        $('.floatNav-default').hide()
      })
      $('.closeFloatNav-hidden').click(function () {
        $('.floatNav-hidden').hide()
        $('.floatNav-default').show()
      })
  
  
  
    })
