        <script>

            function customizeBgSize() {
                var customizeBgH = $('#bli_首頁大廣上').outerHeight() + $('#bli_Mobile八宮格').outerHeight() + $('#bli_新黃金看板').outerHeight() - 22;
                $("#ul-index-upper").css("background-size", "100% " + customizeBgH + "px");
            }

            (function () {
                var Order = "Mobile八宮格,海外直送,新黃金看板,黃金看版,今日秒殺,熱銷排行榜,官網六小格,發燒活動,超夯影音,大廣告New,大廣告版位,熱門標籤,品牌牆,官網三格";

                Order.split(",").reverse().forEach(function (value, index, ar) {
                    //console.log(value);

                    if ($("#bli_" + value).length > 0) {
                        $("#bli_" + value).insertAfter($("#bli_首頁大廣下"));
                    }

                    //bli_首頁大廣下
                });

                const MobileColorJson = "{\"HeadColor\":\"#ffffff\",\"TailColor\":\"#ffffff\",\"FontColor\":\"#000000\"}";
                const MobileColor = JSON.parse(MobileColorJson);
                const cssBackgroundImage = "linear-gradient(to bottom, " + MobileColor.HeadColor + " 40%, " + MobileColor.TailColor + " 70%)";
                $('#ul-index-upper').css("background-image", cssBackgroundImage);
                $('#bli_Mobile八宮格 .customize-bg-color').css("color", MobileColor.FontColor);
                $("#bli_首頁大廣上 img,#bli_Mobile八宮格 img, #bli_新黃金看板 img").each(function () {
                    $(this).bind('load', function () {
                        $(this).attr("data-isload", "1");
                        customizeBgSize();
                    });
                });
                window.setTimeout("customizeBgSize();", 100);

            })();

        </script>
        
        <script>

            // 下面這行 server取代
            var BestSellerSettings = [{"MyId":"HB662","BackgroundColor":"#c47045","BackgroundImage":"//photo.s3.com.tw/banner/block/HomeBanner20231110093353397.jpg","Is_ShowNumberFlag":"N","Link":"https://s3vip.tw/_aw5"},{"MyId":"HB304","BackgroundColor":"#e31836","BackgroundImage":"//photo.s3.com.tw/banner/block/HomeBanner20230727172009156.jpg","Is_ShowNumberFlag":"N","Link":"https://s3vip.tw/_9Dq"}];
            //--var BestSellerSettings = { "BackgroundColor": "#ffd966", "BackgroundImage": "//photo.s3.com.tw/banner/block/BillBoard20230407191213800.jpg", "Is_ShowNumberFlag": "Y" };

            $(function () {

                for (var i = 0; i < BestSellerSettings.length; i++) {
                    var BestSellerSetting = BestSellerSettings[i];
                    var $target = $('.bestseller[data-bsid="' + BestSellerSetting.MyId + '"]');
                    //console.log("BestSeller " + BestSellerSetting.MyId + " Start.");
                    $('.imgBestSellerBack', $target).attr('src', BestSellerSetting.BackgroundImage);
                    if (BestSellerSetting.Link && BestSellerSetting.Link != "") {
                        $('.imgBestSellerBack', $target).wrap("<a href='" + BestSellerSetting.Link + "'></a>");
                        $('.bestseller-swiper-block', $target).find("a.img").attr('href', BestSellerSetting.Link);
                    }
                    if (BestSellerSetting.BackgroundColor) {
                        $('.bestseller-swiper-block', $target).css('background', BestSellerSetting.BackgroundColor);
                    }

                    $(".bestseller-swiper-block .swiper-slide", $target).each(function (index, element) {
                        if (index < 3) {
                            $(this).find('.bestseller-tag').addClass('top3');
                        }
                        $(this).find('.bestseller-tag em').html(index + 1);
                        if (BestSellerSetting.Is_ShowNumberFlag == "Y") {
                            $(this).find('.bestseller-tag').show();
                        }
                    });

                    var $swiper = $target.find('.bestseller-swiper').get(0);
                    var $nextBtn = $target.find('.bestseller-action-next').get(0);
                    var $preBtn = $target.find('.bestseller-action-prev').get(0);
                    var swiperBestseller = new Swiper($swiper, {
                        slidesPerView: 3,
                        slidesPerGroup: 3,
                        spaceBetween: 14,
                        loop: true,
                        breakpoints: {
                            768: {
                                spaceBetween: 9,
                            },
                        },
                        autoplay: 3000,
                        nextButton: $nextBtn,
                        prevButton: $preBtn,
                    });
                    //console.log("BestSeller " + BestSellerSetting.MyId + " End.");
                }

                // 不會有小於三的情境
                //if ($(".bestseller-swiper .swiper-slide").length <= 3) {
                //    $(".bestseller-action-next, .bestseller-action-prev").hide();
                //}
            });
        </script>
        
        
        <script>

            // 下面這行 server取代
            var productsExposureArray = [{"MyId":"HB802","Link":"https://s3vip.tw/_ayx","IsTopMargin":"N","IsBottomMargin":"N"},{"MyId":"HB567","Link":"","IsTopMargin":"Y","IsBottomMargin":"Y"}];

            $(function () {
                //var productsExposureArray = [
                //    {
                //        MyId: '01',
                //        Link: 'https://s3vip.tw/_9D',
                //    }
                //];

                for (var i = 0; i < productsExposureArray.length; i++) {
                    let item = productsExposureArray[i];
                    var $target = $('.products-exposure[data-peid="' + item.MyId + '"]');

                    if ($('.block_title .name', $target).text() != '') {
                        $('.block_title', $target).show();
                    }

                    if (item.Link && item.Link != "") {
                        $('.products-exposure-swiper-block', $target).find("a.img").attr('href', item.Link);
                    }

                    // 設定上下邊界
                    if (item.IsTopMargin && item.IsTopMargin == "Y") {
                        $target.addClass("top-line-gap");
                    }
                    if (item.IsBottomMargin && item.IsBottomMargin == "Y") {
                        $target.addClass("bottom-line-gap");
                    }

                    var $swiper = $target.find('.products-exposure-swiper').get(0);
                    var $nextBtn = $target.find('.products-exposure-action-next').get(0);
                    var $preBtn = $target.find('.products-exposure-action-prev').get(0);
                    var swiperProductsExposure = new Swiper($swiper, {
                        slidesPerView: 'auto',
                        freeMode: true,
                        spaceBetween: 0,
                        slidesPerGroup: 4,
                        loop: true,
                        breakpoints: {
                            768: {
                                slidesPerGroup: 2,
                            },
                        },
                        autoplay: 5000,
                        nextButton: $nextBtn,
                        prevButton: $preBtn,
                    });
                }
            });
        </script>
        
