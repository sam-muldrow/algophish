<script type="text/javascript">
    var LittleBox = {
        GetPara: function (e) {
            var query = window.location.search.substring(1);
            var vars = query.split("&");
            for (var i = 0; i < vars.length; i++) {
                var pair = vars[i].split("=");
                if (pair[0] == e) {
                    return pair[1];
                }
            }
            return null;
        },
        IsBPage: function(){
            var IsBPagePgRegex = /\/BPage.aspx/i;
            if (IsBPagePgRegex.test(location.pathname)) {
                return true;
            } else {
                return false;
            }
        }
    }
    if (LittleBox.IsBPage()) {
        (window._bwtm = window._bwtm || []).push({
            "mid": 109,
            "pid": "promotion",
            "uid": ''
        });
        console.log(' LittleBox.IsBPage promotion ');
    }
    else {
        (window._bwtm = window._bwtm || []).push({
            "mid": 109
        });
    }
</script>
