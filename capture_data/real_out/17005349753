<script type="text/javascript">
/*global window */
/*jslint browser: true */
(function () {
    'use strict';

    var user;
    var el_recommendations;

    if (document.getElementsByClassName('user-name').length > 0 &&
            document.getElementsByClassName('user-name')[0].getElementsByTagName('span').length > 0 &&
            document.getElementById('kwp_seeRecommendations') &&
            document.getElementById('kwp_seeRecommendations').getElementsByTagName('span').length > 0) {

        user = document.getElementsByClassName('user-name')[0].getElementsByTagName('span')[0].innerHTML.trim();
        el_recommendations = document.getElementById('kwp_seeRecommendations').getElementsByTagName('span')[0];

        if (user.length <= 7) {
            el_recommendations.innerHTML = user + ', ' +
                    el_recommendations.innerHTML.charAt(0).toLowerCase() +
                    el_recommendations.innerHTML.slice(1);
        }
    }
}());
</script></div>
