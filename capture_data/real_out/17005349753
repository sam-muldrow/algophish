<script type="text/javascript" src="//cdn.kobo.com/merch-assets/magento/common/js/kwp_skinnyState.js"></script>
-->
<script>
/*global window */
/*jslint browser: true, for: true */
(function () {
    "use strict";
    
    var state;
    var event_skinnyBeforeLoaded = new CustomEvent("kwp_skinny_before_state_loaded", {
        detail: { },
        bubbles: true,
        cancelable: true
    });
    var event_skinnyAfterLoaded = new CustomEvent("kwp_skinny_after_state_loaded", {
        detail: { },
        bubbles: true,
        cancelable: true
    });
    var hasAuthenticated = function () {
        var result = false;
        var c_persistentTracking;

        if (document.cookie.indexOf('persistentTracking=') > -1 &&
                document.cookie.split('persistentTracking=').length > 1) {

            c_persistentTracking = document.cookie.split('persistentTracking=')[1].split(';')[0];

            if (c_persistentTracking.indexOf('hasPreviouslyAuthenticated=True') > -1) {
                result = true;
            }

        } else if (document.cookie.toLowerCase().indexOf("_speku") > -1) {
            result = true;
        }

        return result;
    };
    var isRMR = function () {
        var result = false;
        var c;
        var x;

        if (document.cookie &&
                document.cookie.toLowerCase().indexOf('session=') > -1) {

            c = document.cookie.toLowerCase().split('session=')[1].split(';')[0].split('&');

            for (x = 0; x < c.length; x = x + 1) {

                if (c[x].indexOf('ult=') > -1 &&
                        c[x].split('ult=')[1].indexOf('haspurchased') > -1) {

                    result = true;
                    x = c.length;

                }

            }
        }
        return result;
    };
    var initState = function (_s) {
        var s = ["kwp-skinny-state-anonymous", "kwp-skinny-state-unauthenticated", "kwp-skinny-state-non-rmr"];
        var el;
        var i;

        window.dispatchEvent(event_skinnyBeforeLoaded);

        if (_s === undefined) {
            _s = "kwp-skinny-state-anonymous";
        }

        for (i = 0; i < s.length; i = i + 1) {

            el = document.getElementById(s[i]);
            
            if (el) {
                if (_s.toLowerCase() !== s[i].toLowerCase()) {
                    el.parentNode.removeChild(el);
                } else {
                    el.classList.remove('kwp-hidden');
                }
            } 
        }

        // Fire Window 'kwp_multiSkinny_loaded' Event.
        window.dispatchEvent(event_skinnyAfterLoaded);
    };
    
    if (document.cookie) {
        
        if (document.cookie.toLowerCase().indexOf("uid=cc00ppxfsprpf7dxnsjqb6tb5eq=") > -1 ||
                document.cookie.toLowerCase().indexOf("uid=hipaj2rpgclii9zc1qp2ob3enr8=") > -1) {

            // Currently NOT Authenticated.
            if (hasAuthenticated()) {
                state = "kwp-skinny-state-unauthenticated";
            } else {
                state = "kwp-skinny-state-anonymous";
            }

        } else {

            // Currently Authenticated.
            if (isRMR() === true) {
                state = "kwp-skinny-state-rmr";
            } else {
                state = "kwp-skinny-state-non-rmr";
            }
        }
    } else {
        state = "kwp-skinny-state-anonymous";
    }

    // When HSBC cookie true, show RMR state.
    if (document.cookie) {
      if (document.cookie.toLowerCase().indexOf("utm_source=hsbc") > -1) {
        state = "kwp-skinny-state-unauthenticated"
      }
    }

    initState(state);

}());
</script>

<!-- B-START ::: skinny-template-js ::: skinny-template-js -->
<script type="text/javascript">
/*global window */
/*jslint browser: true, for: true */
(function () {
    "use strict";



    window.addEventListener('kwp_skinny_after_state_loaded', function () {

        var el_wrapper;
        var els_zblock;
        var i;
        var el_message;
        var el_prevArrow;
        var el_nextArrow;
        var prev_idx;
        var next_idx;
        var targetIdx;

        var createArrowElement = function (_class, _idx) {
            var _el = document.createElement('a');
            _el.setAttribute('class', _class);
            _el.href = "#target-message-" + _idx.toString();

            return _el;
        };

        if (document.getElementById('kwp-multiItemSkinnyWrapper')) {

            el_wrapper = document.getElementById('kwp-multiItemSkinnyWrapper');
            els_zblock = el_wrapper.querySelectorAll('.zblock');

            if (els_zblock.length > 0) {

                els_zblock[0].className = els_zblock[0].className + " kwp_firstMessage";

                for (i = 0; i < els_zblock.length; i = i + 1) {

                    targetIdx = i + 1;

                    el_message = document.createElement('span');
                    el_message.setAttribute('id', 'target-message-' + targetIdx);

                    els_zblock[0].parentNode.insertBefore(el_message, els_zblock[0]);

                    if ((i + 1) === els_zblock.length) {
                        next_idx = 1;
                    } else {
                        next_idx = i + 2;
                    }

                    if (i === 0) {
                        prev_idx = els_zblock.length;
                    } else {
                        prev_idx = i;
                    }

                    el_prevArrow = createArrowElement('arrow arrow-prev', prev_idx);
                    el_nextArrow = createArrowElement('arrow arrow-next', next_idx);

                    els_zblock[i].appendChild(el_prevArrow);
                    els_zblock[i].appendChild(el_nextArrow);
                }

            }


            var el_hpHero;
            var el_skinnyOriginal;
            var el_skinnyClone;

            if (document.body.className.toLowerCase().indexOf("store-homepage") > -1 &&
                    document.getElementsByClassName("HeroBannerWidget").length > 0) {

                el_hpHero = document.getElementsByClassName("HeroBannerWidget")[0];

                el_skinnyOriginal = document.getElementById("kwp-multiItemSkinnyWrapper");
                el_skinnyClone = el_skinnyOriginal.cloneNode(true);

                el_skinnyOriginal.parentNode.removeChild(el_skinnyOriginal);

                el_hpHero.parentNode.insertBefore(el_skinnyClone, el_hpHero);
            }
    
        }

    }, false);

}());
</script>
<!-- B-END ::: skinny-template-js ::: skinny-template-js -->

