        <script>
        function firstViewFull() {
            var wWidth = $(window).width();
            var wHeight = $(window).height();
            if (wWidth > 992 || wWidth < wHeight) {
                $("#age-first-view").css('height', wHeight + 'px');
            } else {
                $("#age-first-view").css('height', 'auto');
            }
        }
        firstViewFull();
        $(window).resize(function() {
            firstViewFull();
        });

        var isReadMore = false;
        $('#read-more').on('click', function() {
            // $(this).next().slideToggle();
            if (isReadMore) {
                $(this).next().slideUp();
                $(this).find('span').html('Read More');
                $(this).removeClass('active');
                isReadMore = false;
            } else {
                $(this).next().slideDown();
                $(this).find('span').html('Hide');
                $(this).addClass('active');
                isReadMore = true;
            }
        });
        </script>
