    <script>
    var last_visit_uri_cookie = docCookies.getItem("last_visit_uri");
    if (last_visit_uri_cookie != null && last_visit_uri_cookie != "") {
        const last_visit_uri = new URL(last_visit_uri_cookie);
        let hostname = last_visit_uri.hostname;
        let pathname = last_visit_uri.pathname;
        let redirect_uri = "";

        if (pathname.match(/channel/)) {
            redirect_uri = "https://" + hostname + "/monthly/channel/index.html";
        } else {
            redirect_uri = "https://" + hostname + "/index2.html";
        }

        window.location.href = redirect_uri;
    }
    </script>
