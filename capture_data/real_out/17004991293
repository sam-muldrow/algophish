<script>
    document.getElementById("defaultOpenWinner").click();
    function openTabWinner(evt, tabName) {
        var i, tabcontent, tablinks;
        tabcontent = document.getElementsByClassName("js-tabcontent-winner");
        for (i = 0; i < tabcontent.length; i++) {
            tabcontent[i].style.display = "none";
        }
        tablinks = document.getElementsByClassName("js-tablinks-winner");
        for (i = 0; i < tablinks.length; i++) {
            tablinks[i].className = tablinks[i].className.replace(" active", "");
        }
        document.getElementById(tabName).style.display = "block";
        evt.currentTarget.className += " active";
    }
</script>



            <div class="tab">
    <button class="tablinks js-tablinks-game" onclick="openTab(event, 'new')">–ù–æ–≤–∏–Ω–∫–∏</button>
    <button class="tablinks js-tablinks-game" onclick="openTab(event, 'slot')" id="defaultOpen">–°–ª–æ—Ç—ã</button>
    <button class="tablinks js-tablinks-game" onclick="openTab(event, 'roulette')">–†—É–ª–µ—Ç–∫–∏</button>
