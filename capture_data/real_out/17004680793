<script type="text/javascript">
        function googleTranslateElementInit() {
            new google.translate.TranslateElement({
                    pageLanguage: 'en',
                    layout: google.translate.TranslateElement.InlineLayout.HORIZONTAL
                },
                'google-translate');
            applyAriaLabel();
            removeGoogleTranslateAltTag()
        };
        function applyAriaLabel() {
            var gtSelect = document.getElementsByClassName('goog-te-combo')[0];
            var ariaLabel = document.createAttribute('aria-label');         
            ariaLabel.value = 'Translate this web page';
            gtSelect.setAttributeNode(ariaLabel);        
        };
        function removeGoogleTranslateAltTag() {
            // This function is a sticking plaster fixing a bigger issue with the Google Translate library used above.
            var altTag = document.getElementsByClassName('goog-logo-link')[0];
            altTag.childNodes[1].remove()
		}
    </script>

</body>
