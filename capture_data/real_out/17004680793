    var $L = $LAB;
    for (var i = 0, len = queue.length; i < len; i++) {
        if (typeof queue[i] == "string") {
            $L = $L.script(queue[i]);
        } else if (!queue[i]) {
            $L = $L.wait();
        } else if (typeof queue[i] == "function") {
            $L = $L.wait(queue[i]);
        }
    }
