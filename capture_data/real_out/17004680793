                    if (!!window.MSInputMethodContext && !!document.documentMode) {
                        var formInstance = document.getElementById('search-filter-form');
                        var tabFlag = false;
                        function checkTabPress(event) {
                            if (event.key === 'Tab') {
                                if (tabFlag) {  
