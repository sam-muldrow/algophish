    $(document).ready(function() {
        $(window).scroll(function(e){ 
            $el = $('.skyscraper-left, .skyscraper-right'); 

                if ($(this).scrollTop() > 237 && $el.css('position') != 'fixed'){ 
                    $('.skyscraper-left, .skyscraper-right').css({'position': 'fixed', 'top': '40px'}); 
                }
