$(document).ready(function() {
    //Home.init();
    //Ads.init();
    var t = '<div class="loading"><div class="contener_mixte"><div class="ballcolor ball_1"></div></div><div class="contener_mixte"><div class="ballcolor ball_2"></div></div><div class="contener_mixte"><div class="ballcolor ball_3"></div></div></div>';
