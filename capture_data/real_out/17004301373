<script type="text/javascript">
    var isAppForAdobe = ('false' === 'true');
    var adobeputil = {
        // Device Checks
        isAndroid: function () {
            return navigator.userAgent.match('Android');
        },
        isChrome: function () {
            return navigator.userAgent.match("Chrome");
        },
        isIPad: function () {
            return navigator.userAgent.match(/iPad/i) !== null;
        },
        isIPhone: function () {
            return navigator.userAgent.match(/iPhone/i) !== null;
        },
        isIOS: function () {
            return adobeputil.isIPhone() || adobeputil.isIPad();
        }
    };

    function getCookieAdobe(cName) {
        return $.cookie(cName);
    }

    var _adobeYn = getCookieAdobe('analytics_sp');
    var _pcId = getCookieAdobe('PCID');
    var _memberId = getCookieAdobe('MEMBER_ID');
    var _ckwhere = getCookieAdobe('CKWHERE');

    function isEmptyObject(param) {
        return Object.keys(param).length === 0 && param.constructor === Object;
    }

    function sendAdobeServer(_dl) {
        
        var scriptUrl = "//assets.adobedtm.com/c78a04c43d47/dedbe93b08df/launch-febc6f5bcace.min.js";
        

        $.getScript(scriptUrl, function () {
            if (typeof _satellite !== "undefined") {
                if (!isEmptyObject(_dl)) {
                    
                    var typeString = 'tracking_log';
                    if (_dl.log_type_flag == 'r') {
                        typeString = 'reacting_log';
                    }
                    _satellite.track(typeString, _dl);
                }
            }
        });
    }

    function isUseSessionStorage() {
        var ssFlag = false;
        if (settings.isMobile) {
            if (isAppForAdobe) {
                if (adobeputil.isAndroid) {
                    ssFlag = true;
                }
            } else {
                ssFlag = true;
            }
        } else {
            ssFlag = true;
        }

        return ssFlag;
    }

    function sendLoadDataToAdobe(type, tarea, tagType) {

        var tracking_dl = {
            pcid: _pcId,
            fsid: 's4e46y0311145wm5dqc4',
            pt: 'EM_SALE_STR_NO=' + '2037' + '&TR_SALE_STR_NO=' + '2154' + '&GM_SALE_STR_NO=' + '2449',
            siteno: '6005',
            log_type_flag: type,
            ckwhere: _ckwhere,
            memberid: _memberId,
            ga: '00',
            mbrgrdcd: '',
            search_fail: 'N'
        };

        

        if (type == 'r' && tarea != 'undefined' && tarea != '') {
            tracking_dl.tarea = tarea;
        }

        sendAdobeServer(tracking_dl);
    }

    function sendLoadDataToAdobeFromSession(_dl) {
        var _dlObj = JSON.parse(_dl);
        sendAdobeServer(_dlObj);
    }

    function getAdobeAnalytics() {
        $.ajax({
            type: "GET",
            url: "/targeting/idSampling.ssg",
            dataType: "json",
            success: function (data) {
                if (_adobeYn == 'Y') {
                    sendLoadDataToAdobe('t', '', 'x');
                }
            },
            error: function (data) {
            }
        });
    }

    if (typeof deferredObj != "undefined") {
        deferredObj.done(function () {
            // var adobeEmergencyFlag = settings && settings.emergency && settings.emergency.DISP_CONTENTS_LIMIT_EMERGENCY == 'Y';
            // if(!adobeEmergencyFlag) {
            if (_adobeYn == "Y") {
                sendLoadDataToAdobe('t', '', 'x');
            } else {
                getAdobeAnalytics();
            }
            // }
        });
    }
    // $(function(){
    // });

</script>
