<script type="text/javascript">
    $(function() {
        var isTargetDomain = ['www.ssg.com','emart.ssg.com'].filter(host => location.host.indexOf(host) >= 0).length > 0;
        if (isTargetDomain && $.cookie('lp_gnbTop_Banner') !== 'N') {
            $("#lp_gnbTopBanr").show();
        }

        $(".js_gnb_bnr_belt_close").on("click", function() {
            var now = new Date();
            var expire = new Date(now.getFullYear(), now.getMonth(), now.getDate(), 23, 59, 59);
            $.cookie("lp_gnbTop_Banner", "N", {expires: expire, path: '/', domain:'ssg.com'});

            $(".gnb_bnr_belt").addClass("is-active");
        });
    });
</script>

