        function SanitizerRedirectUrl(res) {
            var hrf = "";
            try {
                const myUrl = new URL(res);
                var host = myUrl.hostname;
                var str2 = "sharekhan.com";
                if (host.indexOf(str2) != -1) {
                    hrf = res;
                }
            }
            catch (err) {

            }

            return hrf;
        }
