<script>
    $(document).ready(function () {
        /*$(window).on('load', function () {*/
        changeDivHeight();
    });
    $(window).resize(function () {
        changeDivHeight();
    });
    function changeDivHeight() {
        var a = $("#div_a").innerHeight();
        var b = $("#div_b").innerHeight();
        if (a > b) {
            $("#div_b").height(a);
        } else {
            $("#div_a").height(b);
        }
    }
    $("#txtLoginMob,#txtLoginMobPopup").on("keypress", function (event) {

        if (event.which < 48 || event.which > 57) {
            event.preventDefault();
        }
        else {

            if ($(this).val().length >= 10) {
                event.preventDefault();
            }
            else {
                //console.log(validateMobile($(this).val()));
                // console.log(validateMobile());
            }
        }
    });

    $('.toggleTermsCondition').click(function () {
        if ($(this).is(':checked')) {

            $('.btnStartResume').removeAttr('disabled'); //enable input

        } else {
            $('.btnStartResume').attr('disabled', true); //disable input
        }
    });

    function validateMobile(desc) {
        const patern = /\b([0|+[0-9]{1,5})?([7-9][0-9]{9})\b/;
        var match = desc.match(patern);
        var contains = match != null;
        var exists = contains ? true : false;
        return exists;
    }


