<style>
  .total_entries_multiplier {
    padding: 4px 10px;
    background-color: #1e90ff;
    color: #fff;
    margin-right: 10px;
    font-weight: 700;
  }
</style>

<script type="text/javascript">
    $(document).ready(function(){
        
        //We need this to make sure that we dont run middleware requests when the page first loads.
        window.hasSlideCartBeenOpened = false;
        
        //This part allows the system to start running only when the user actually opens slidecart.
        window.SLIDECART_OPENED = function() {

            $('quick-add-modal[open] .quick-add-modal__toggle').click();
            
            window.hasSlideCartBeenOpened = true;
            window.SLIDECART_UPDATE();
            
            
        }

        window.SLIDECART_ADD_TO_CART = function({ id, quantity }) {
          // twq('event', 'tw-oebyx-oebz2', {
          //   conversion_id: id
          // });
        }
        
        
        window.SLIDECART_CLOSED = function() {

        }

        window.SLIDECART_REMOVED_FROM_CART = function() {
            window.renderPromotion(window.SLIDECART_STATE().cart.total_price / 100);
        }
        
        //This is the function that will allow us to grab if an item has a multiplier and if that multiplier is global.
        function middleWareItemTransform(item) {
            return $.ajax({
                type: "GET",
                async: false,
                url: 'https://app.speedsociety.com/get_product_override.php',
                data:  {
                    product_id: item.product_id,
                },
                success:function(data) {
                    if (data != '') {
                        data = $.parseJSON(data);
                        if (data.multiplier_value) {
                            item.multiplier_value = data.multiplier_value;
                        }
                        
                        if (data.global_multiplier) {
                            item.global_multiplier = data.global_multiplier;
                        }
                    }
                },
                error: function(errorThrown){
                    console.log(errorThrown);
                }
            });
        }        
        
        
        window.SLIDECART_LOADED = function(cart) {
            $( ".tier-spend-promo-bar" ).clone().insertAfter( ".slidecarthq header" );
            var items = cart.items;

            items.forEach(function (item, index) {
                if (item.discounts.length > 0 && item.discounts[0].title === "BFCM - Promo" ) {
                    window.selectedFreeGifts = window.selectedFreeGifts + 1 * item.quantity;
                }
            });     
        }
        
        window.SLIDECART_UPDATED = function(cart) {
            window.selectedFreeGifts = 0;
            //update item count
            $('.cart-btn span').text(cart.item_count);

            $('.variants-p.entries_for_item').remove();
            
            if (window.hasSlideCartBeenOpened) {
                //Initial Values that we need to keep track of different things.
                var mp = 40;
                var has_global_modifier = false;
                var global_modifier_value = 0;
                var items = cart.items;
                var total_entries_to_be_earned = 0;
                
                //Perform Housekeeping on any previous entries in the system.
                $('#total-entries-earned').remove();
                $('#current_multiplier').remove();
                $('.slidecarthq .entries_for_item').remove();
                
                //Add the override data to each item making a request for each middleware item.
                items.forEach(function (item, index) {
                    middleWareItemTransform(item);
                });               
                
                //Perform a quick search over the whole of the items array to find the maximum global override.
                items.find(function(item, index) {
                    if( item.global_multiplier == true ) {
                        has_global_modifier = true;
                        //Overwrite only if the previous value is less than the current.
                        if (global_modifier_value < item.multiplier_value) {
                            global_modifier_value = item.multiplier_value;
                        }
                    }
                });
                
                //Change the base multiplier.
                if (has_global_modifier) {
                    mp = global_modifier_value;
                }
                
                //Loop over each item and do its calculation.
                items.forEach(function (item, index) {
                    
                    console.log(item);

                    if (item.discounts.length > 0 && item.discounts[0].title === "BFCM - Promo" ) {
                        window.selectedFreeGifts = window.selectedFreeGifts + 1 * item.quantity;
                    }
                    
                    //This is the standard case that works for both global and regular items.
                    var entries_for_item = item.original_line_price / 100 * mp;
                    
                    //This is a special case where an item has an individual multiplier but no global.
                    if (item.multiplier_value && !has_global_modifier) { 
                        entries_for_item = item.original_line_price / 100 * item.multiplier_value; 
                    }
                    
                    //Round Up to the Nearest Full Number for entries.
                    entries_for_item = Math.ceil(entries_for_item);
                    
                    //Put the interface element for entries on the item.
                    //console.log(item.handle);
                    //console.log(item);
                    
                    if (item.handle == 'car-club-membership') {
                        $('.slidecarthq .item[data-line-item-id='+item.id+'] .title').after('<p class="variants-p entries_for_item">'+350+' Entries</p>');
                        entries_for_item = 350;
                    } else if (item.handle == 'car-club-membership-1') {
                        $('.slidecarthq .item[data-line-item-id='+item.id+'] .title').after('<p class="variants-p entries_for_item">'+55+' Entries</p>');
                        entries_for_item = 55;
                    } else if (item.handle == 'golden-ticket') {
                        $('.slidecarthq .item[data-line-item-id='+item.id+'] .title').after('<p class="variants-p entries_for_item">'+250 * item.quantity +' Entries</p>');
                        entries_for_item = 250;
                    } else {
                        $('.slidecarthq .item[data-line-item-id='+item.id+'] .title').after('<p class="variants-p entries_for_item">'+entries_for_item+' Entries</p>');
                    }
                    
                    //Increment the total entries that will be earned for each item.
                    total_entries_to_be_earned += entries_for_item;
                });

                window.renderPromotion((cart.total_price / 100));
                //Append the final interface element that shows the multiplier and the total number of entries.
                $('.slidecarthq .footer #total-entries-earned').remove();
                $('.slidecarthq .footer').prepend('<div id="total-entries-earned" class="footer-row"><span class="flex">Total Entries</span><span class="shake total_entries_multiplier">'+mp+'X</span>'+total_entries_to_be_earned+'</div>');
            }
        }    
    });
</script>

    
