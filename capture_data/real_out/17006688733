<script>
  $(document).ready(function () {
    var start = new Date('2023/11/17 23:55:00');
    var end = new Date('2023/11/24 23:55:00');
    var today = new Date();

    //use Math.abs to avoid sign
    var q = Math.abs(today - start);
    var d = Math.abs(end - start);
    var barPercentage = Math.round((q / d) * 100);

    $('#meter-template--16030007722179__a8fb20d3-7e64-4387-a785-25e18e59339c .pmeter span').css('width', barPercentage + '%');
  });
</script>


