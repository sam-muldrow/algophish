  <p class="free-gifts-notice t1-section">
    <img
      width="41"
      height="20"
      loading="lazy"
      src="https://cdn.shopify.com/s/files/1/0535/5470/8675/files/pointing-icon.png?v=1700524153"

    ><a class="select-gift-link" href="/pages/select-your-free-gifts">Select your free gift(s)</a>
  </p>
</div>

<script type="text/javascript">
  $(document).ready(function () {

    window.selectedFreeGifts = 0;

    window.explode = function(x, y, tierclass1, tierclass2) {
      var particles = 50,
      // explosion container and its reference to be able to delete it on animation end
      explosion = $('<div class="explosion"></div>');

      // put the explosion container into the body to be able to get it's size
      $('body').append(explosion);

      // position the container to be centered on click
      explosion.css('left', x - explosion.width() / 2);
      explosion.css('top', y - explosion.height() / 2);

      for (var i = 0; i < particles; i++) {
        if(i % 2 == 0) {
          var tierclass = tierclass2;
        } else {
          var tierclass = tierclass1;
        }
        // positioning x,y of the particle on the circle (little randomized radius)
        var x = (explosion.width() / 2) + rand(80, 150) * Math.cos(2 * Math.PI * i / rand(particles - 10, particles + 10));
        var y = (explosion.height() / 2) + rand(80, 150) * Math.sin(2 * Math.PI * i / rand(particles - 10, particles + 10));
        // particle element creation (could be anything other than div)
        elm = $('<div class="particle '+tierclass+' " style="' +
          'top: ' + y + 'px; ' +
          'left: ' + x + 'px"></div>');

          if (i == 0) { // no need to add the listener on all generated elements
            // css3 animation end detection
            elm.one('webkitAnimationEnd oanimationend msAnimationEnd animationend', function(e) {
              explosion.remove(); // remove this explosion container when animation ended
            });
          }
          explosion.append(elm);
        }
      }

      function rand(min, max) {
        return Math.floor(Math.random() * (max + 1)) + min;
      }

      window.threshold = 0;

      window.renderPromotion = function(cartTotal) {

        //The numerical float value of the goals.
        var T1SpendGoal = 100.00;
        var T2SpendGoal = 200.00;
        var T3SpendGoal = 300.00;

        var t1GoalPercentage = (100 * cartTotal) / T1SpendGoal;
        var t3GoalPercentage = (100 * cartTotal) / T3SpendGoal;

        if (cartTotal < T1SpendGoal) {
          $('.headline-notice').html('Spend <span class="spend-notice t1">'+(T1SpendGoal - cartTotal).toLocaleString('en-US', {style: 'currency', currency: 'USD'})+'</span> or more to <img width="auto" height="23" loading="eager" src="https://cdn.shopify.com/s/files/1/0535/5470/8675/files/lock_3ab27fd1-de13-4165-8840-7386d2f95c0d.svg?v=1700173710"> <span class="gift-count">1</span> free gift!');
          $('.meter.orthodox span.progress').animate({ width: t3GoalPercentage + '%' }, 'slow');
          $('.free-gifts-notice').hide();
        }

        if (cartTotal > T1SpendGoal) {
          $('.meter.orthodox span.progress').animate({ width: t3GoalPercentage + '%' }, 'slow');
          $('.free-gifts-notice').attr('class','free-gifts-notice t1-section');
          $('.free-gifts-notice').show();
        } else {
          $('.meter.orthodox span.progress').css({ width: '0%' });
        }

        if (cartTotal >= T1SpendGoal && cartTotal < T2SpendGoal) {
          $('.headline-notice').html('Spend <span class="spend-notice t2">'+(T2SpendGoal - cartTotal).toLocaleString('en-US', {style: 'currency', currency: 'USD'})+'</span> or more to <img width="auto" height="23" loading="eager" src="https://cdn.shopify.com/s/files/1/0535/5470/8675/files/lock_3ab27fd1-de13-4165-8840-7386d2f95c0d.svg?v=1700173710"> <span class="gift-count">2</span> free gifts!');
          $('.free-gifts-notice').html('<img width="41" height="20" loading="lazy" src="https://cdn.shopify.com/s/files/1/0535/5470/8675/files/pointing-icon.png?v=1700524153"> <a class="select-gift-link" href="/pages/select-your-free-gifts">Select your free gift</a>').show();
          $('.free-gifts-notice').attr('class','free-gifts-notice t1-section');
          if (window.selectedFreeGifts >= 1) {
            $('.free-gifts-notice').hide();
          }
          if (window.threshold == 0) {
            $(".t1-section .icon-present").addClass('spin');
            explode($(".t1-section .icon-present")[0].getBoundingClientRect().left + 25, window.scrollY + $(".t1-section .icon-present")[0].getBoundingClientRect().top + 25, 't1-section', 't2-section');
            if ($('.slidecarthq').hasClass('open')) {
              setTimeout(function () {
                explode($(".slidecarthq .t1-section .icon-present")[0].getBoundingClientRect().left + 15, window.scrollY + $(".slidecarthq .t1-section .icon-present")[0].getBoundingClientRect().top + 15, 't1-section', 't2-section');
              }, 700);
            }
            window.threshold++;
          }
        }

        if (cartTotal >= T2SpendGoal && cartTotal < T3SpendGoal) {
          $('.headline-notice').html('Spend <span class="spend-notice t3">'+(T3SpendGoal - cartTotal).toLocaleString('en-US', {style: 'currency', currency: 'USD' })+'</span> or more to <img width="auto" height="23" loading="eager" src="https://cdn.shopify.com/s/files/1/0535/5470/8675/files/lock_3ab27fd1-de13-4165-8840-7386d2f95c0d.svg?v=1700173710"> <span class="gift-count">3</span> free gifts!');
          $('.free-gifts-notice').html('<img width="41" height="20" loading="lazy" src="https://cdn.shopify.com/s/files/1/0535/5470/8675/files/pointing-icon.png?v=1700524153"> <a class="select-gift-link" href="/pages/select-your-free-gifts">Select your <span class="free-gift-count">'+(2 - window.selectedFreeGifts)+'</span> free gift(s)</a>').show();
          $('.free-gifts-notice').attr('class','free-gifts-notice t2-section');
          if (window.selectedFreeGifts >= 2) {
            console.log('iran');
            $('.free-gifts-notice').hide();
          }
          if (window.threshold == 1) {
            $(".t2-section .icon-present").addClass('spin');
            explode($(".t2-section .icon-present")[0].getBoundingClientRect().left + 25, window.scrollY + $(".t2-section .icon-present")[0].getBoundingClientRect().top + 25, 't2-section', 't3-section');
            if ($('.slidecarthq').hasClass('open')) {
              setTimeout(function () {
                explode($(".slidecarthq .t2-section .icon-present")[0].getBoundingClientRect().left + 15, window.scrollY + $(".slidecarthq .t2-section .icon-present")[0].getBoundingClientRect().top + 15, 't2-section', 't3-section');
              }, 700);
            }
            window.threshold++;
          }
        }

        if (cartTotal >= T3SpendGoal) {
          $('.headline-notice').html('You have <img width="auto" height="23" loading="eager" src="https://cdn.shopify.com/s/files/1/0535/5470/8675/files/lock_3ab27fd1-de13-4165-8840-7386d2f95c0d.svg?v=1700173710"> <span class="gift-count">3</span> free gifts!');
          $('.free-gifts-notice').html('<img width="41" height="20" loading="lazy" src="https://cdn.shopify.com/s/files/1/0535/5470/8675/files/pointing-icon.png?v=1700524153"> <a class="select-gift-link" href="/pages/select-your-free-gifts">Select your <span class="free-gift-count">'+(3 - window.selectedFreeGifts)+'</span> free gift(s)</a>').show();
          $('.free-gifts-notice').attr('class','free-gifts-notice t3-section');
          if (window.selectedFreeGifts >= 3) {
            $('.free-gifts-notice').hide();
          }
          if (window.threshold == 2) {
            $(".t3-section .icon-present").addClass('spin');
            explode($(".t3-section .icon-present")[0].getBoundingClientRect().left + 25, window.scrollY + $(".t3-section .icon-present")[0].getBoundingClientRect().top + 25, 't3-section', 't4-section');
            if ($('.slidecarthq').hasClass('open')) {
              setTimeout(function () {
                explode($(".slidecarthq .t3-section .icon-present")[0].getBoundingClientRect().left + 15, window.scrollY + $(".slidecarthq .t3-section .icon-present")[0].getBoundingClientRect().top + 15, 't3-section', 't4-section');
              }, 700);
            }
            window.threshold++;
          }
        }

      }

      window.renderPromotion( 0.00 );
    });
</script>


</div>
