    		c-21-2.9-52.6-7.2-63.1-11.1C276.5,156.2,273.8,152,272.5,148.9z M532.7,486.1H244V272h288.7V486.1z M557.1,251.6h-14.2H233.8
    		h-14.2v-53.9h337.5V251.6z"/>
    	<g>
    		<path class="st0" d="M333.1,404.7h41.6c0,9.5,3.5,14.2,10.4,14.2c3.5,0,6.3-1.3,8.5-4s3.3-6.2,3.3-10.6c0-11.1-5.8-16.7-17.4-16.7
