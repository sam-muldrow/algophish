    window.addEventListener("DOMContentLoaded", function() {
        var mainSwithFlag = $(".login-success-3rd").data('showPopup');
        if (mainSwithFlag) {
            var consentUserEmailKey = $(".consent-btn-login-success-3rd").data('email') + "-3rd-party-login-success";
            var isShownLoginSuccessPopup = window.localStorage && window.localStorage.getItem(consentUserEmailKey);
            if (!isShownLoginSuccessPopup || isShownLoginSuccessPopup !== "true") {
                $(".login-success-3rd").show();
            }
        }
        $(".consent-btn-login-success-3rd").click(function() {
            if ($('html').data('locale') === 'en_ZA') {
                $.ajax({
                    url: '/api/subscription/unsubcribeEmail',
                    type: 'POST',
                    data: {
                        email: $(".consent-btn-login-success-3rd").data('email'),
                        isUnsubscribed: !$("#register-consent-comfirm").prop("checked")
                    },
                    dataType: 'json'
                });
                $(".login-success-3rd").hide();
                if (!consentUserEmailKey) {
                    consentUserEmailKey = $(".consent-btn-login-success-3rd").data('email') + "-3rd-party-login-success";
                }
                window.localStorage && window.localStorage.setItem(consentUserEmailKey, "true");
            }
        });
