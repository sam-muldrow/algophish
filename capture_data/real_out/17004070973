            var bP = {
                accId: ('/7162/Gumtree_ZA/home').split(' ').join('_').replace(/\,|&_|'/g, '')
            };
            window.additionalParams = {};

            window.Bolt.BANNERS.length && window.Bolt.BANNERS.forEach(function(banner) {
                var bannerParams = Object.assign({}, bP, banner.params);
                if (window.hasOwnProperty('additionalParams') && window.additionalParams[banner.name]) {
                    bannerParams = Object.assign(bannerParams, window.additionalParams[banner.name]);
                }
                if (bannerParams.slotId && document.getElementById(bannerParams.slotId)) {
                    if (bannerParams.skin) {
                        if (window.matchMedia("(min-width:1024px)").matches) {
                            BOLT.defineLazyLoadSkinCampaign(bannerParams);
                        }
                    } else {
                        BOLT.defineLazyLoadBanner(bannerParams);
                    }
                }
            });

            window.addEventListener('DOMContentLoaded', function() {
                if (!window.Bolt.BANNERS.length) {
                    return;
                }

                var pageTargets = {
                    dc_ref: window.location.href,
                    ptype: 'home'
                };

                var pageTargetConfigs = [];
                pageTargetConfigs.forEach(function(pageTargetConfig) {
                    if (pageTargetConfig.value) {
                        pageTargets[pageTargetConfig.key] = pageTargetConfig.value;
                    }
                });
                window.Bolt.BANNERS.length && BOLT.requestBids(window.Bolt.BANNERS);
            });
