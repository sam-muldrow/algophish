<script language="JavaScript">
function set_vote(){
	var nvote = 0;
	if (gebi("vote").length > 0){
		for(var i=0; i<(gebi("vote").length-1);i++){
			if (gebi("vote").nvote[i].checked){
				nvote = gebi("vote").nvote[i].value;
			}
		}
	}
		
	if (nvote <= 0){
//		alert("Ошибка! Вы ничего не выбрали!");
//		return false;
	}
	
	openWindow("Результаты голосования", "html-addvote", "addvote", 0, 0, 0, 0, "vid="+gebi("vid").value+"&nvote="+nvote);
	gebi("addvote").innerHTML = "<p><b>Идет обработка результатов</b></p><p>Пожалуйста подождите...</p>";
//	ajax("html-addvote", [gebi("vid").value,nvote]);
	return false;
}
</script>
