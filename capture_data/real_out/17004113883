            function SetLocalStorage() {
                var IDs = [];
                $('.topblog-description input:checked').each( function(){
                    IDs.push( $(this).attr('id') )
                } );
                $.Storage.set('korr.main.lenta', IDs.join(';') + ";");               

                var rubrics = $.Storage.get('korr.main.lenta');
                if (rubrics != null && rubrics!= "" && rubrics!= ";") {
                    var href = "/ajax/module.aspx?spm_id=1055&type=%RUBRICID%&IsAjax=true";
                    ajaxNews1055(href.replace("%RUBRICID%", rubrics));
                }else{
                    ajaxNews1055('/ajax/module.aspx?spm_id=1055&type=&IsAjax=true');;                
                }
                return false;
            }
            

            $('.dropdown-menu .topblog-description').click(function (e) {
                e.stopPropagation();
            });
