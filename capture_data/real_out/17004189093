        function wdlclick2() { //2b è¥æªç»å½ï¼åæ£æµç»å½è´¦æ·
            $.ajax({
                type: "POST",
                dataType: "json",
                cache: false,
                async: true,
                url: serverPath + "ids/getUserInfo.do?jsoncallback=?",
                error: function (request, textStatus, errorThrown) {
                    //   alert("è·åç»å½ç¨æ·ä¿¡æ¯éè¯¯");

                },
                success: function (data) {
                    //ç¨æ·ç¶æä¸ºå·²ç»å½
                    if (data.username != undefined) {
                        //ç»å½è´¦æ·ï¼å·æ°é¡µé¢
                        //personList();
                        $('.guessFavorite_dl').hide();
                        window.location.reload();
                    }
                }
            })

        };

        //è°åæ°æ®
        function dengluF(username) {
            var url;
            if (username == "  " || username == '' || username == undefined) {
                url = 'https://siteapi.cdstm.cn/rec/index/indexlike-wcm-.json'
            } else {
                url = 'https://siteapi.cdstm.cn/rec/index/indexlike-wcm-' + username + '.json'
            }
            $.ajax({
                url: url,
                type: 'get',
                dataType: 'json',
                async: false, //  åæ­¥è¯·æ±
                success: function (res) {
                    var data = res.data;
                    var html = "";
                    $(".guessFavorite_lis").empty();
                    if (username == "  ") {
                        lis = 6;
                        $(".guessFavorite_dl").show();
                    } else {
                        lis = 8;
                        $(".guessFavorite_dl").hide();
                    }
                    for (var i = 0; i < lis; i++) {
                        var n = i + 1;
                        html += '<li class="clearfix">'
                            + '<div class="guessFavorite_one">' + n + '</div>'
                            + '<a href="' + data[i].DOCPUBURL + ' " title="' + data[i].DOCTITLE + '">' + data[i].DOCTITLE + '</a>'
                            + '</li>'
                    }
                    $("#jaza").hide();
                    $("#jazas").hide();
                    $(".guessFavorite_lis").html(html);

                },
                error: function () {

                    console.log("æ°æ®å è½½éè¯¯");
                }

            });
            /*  if(location.href.indexOf("#reloaded")==-1){
                            location.href=location.href+"#reloaded";
                            location.reload();
                        };*/
        }
	//dengluF(username);
    </script>
