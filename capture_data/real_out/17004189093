            var share_img2 = 'https://cdn.resources.cdstm.cn/statics/first_logo/logo.jpg'
            var shareimg = "";

            if (share_img == null || share_img == "" || share_img == undefined) {
                //if(share_img2 == null|| share_img2 =="" || share_img2 == undefined){
                shareimg = 'https://cdn.resources.cdstm.cn/statics/first_logo/logo.jpg';
                //}else{
                //shareimg = share_img2
                //}
            } else {
                shareimg = share_img;
            }
            var delBlank = $(".addText").text()
            console.log(delBlank)
            wx.ready(function () {
                wx.onMenuShareTimeline({
                    title: "ä¸­å½æ°å­ç§æé¦",
                    desc: "https://www.cdstm.cn",
                    link: "https://www.cdstm.cn",
                    imgUrl: shareimg,
