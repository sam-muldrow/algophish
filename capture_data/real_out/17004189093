    <script>
        var lis;
        //	ç»éæ£æµç¨æ·å
        var serverPath = "https://ids.cdstm.cn:8443/zgkjg/";
        $.ajax({
            type: "POST",
            dataType: "json",
            cache: false,
            async: true,
            url: serverPath + "ids/getUserInfo.do?jsoncallback=?",
            success: function (data) {
                //ç¨æ·ç¶æä¸ºå·²ç»å½
                if (data.username != undefined) {
                    username = data.username;
                    dengluF(username);
                    // clearInterval(realTimeClData);//å³é­å®æ¶å¨
                    // var realTimeClData = setInterval(wdlclick1, 10000);
