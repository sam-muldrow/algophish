        $(function () {
            // è½®æ­
            bannerRotate.bannerInit(5);
        });

        // banner rotating
        var bannerRotate = {
            _time: 3000,
            _fade: 200,
            _i: 0,

            _max: 0,
            _interval: null,
            _navId: "#mb-inav",
            _navBox: "#mb-ibox",
            _navTxt: "#mb-itxt",
            bannerShow: function () {
                $(this._navId).find("li a").removeClass("cur");
                $(this._navId).find("li:eq(" + this._i + ")").find("a").addClass("cur");

                $(this._navBox).find("a").fadeOut(this._fade);
                $(this._navBox).find("a:eq(" + this._i + ")").fadeIn(this._fade);

                $(this._navTxt).children("div").hide();
                $(this._navTxt).children("div:eq(" + this._i + ")").fadeIn(this._fade);
            },
            bannerStart: function () {
                var _this = this;
                _this._interval = setInterval(function () {
                    if (_this._i >= _this._max - 1) {
                        _this._i = 0;
                    }
                    else {
                        _this._i++;
                    }
                    _this.bannerShow();
                }, _this._time);
            },
            bannerInit: function (max) {
                var _this = this;
                _this._max = max;
                _this.bannerStart();

                $(_this._navId).find("li a").bind("mouseover", function () {
                    clearInterval(_this._interval);
                    _this._i = $(this).parent().index();
                    _this.bannerShow();
                    _this.bannerStart();
                });
            }
        };
        String.prototype.replaceAt = function (index, character) {
            if (!character)
                return this;
            return this.substr(0, index) + character + this.substr(index + character.length);
        };

