                } else {
                    username = "  ";
                    dengluF(username);
                    // clearInterval(realTimeClData);//å³é­å®æ¶å¨
                    // var realTimeClData = setInterval(wdlclick2, 10000);å®æ¶å¨
                    $('.guessFavorite_dl').show();
