                <script>
                    $.extend({
                        myTime: {
                            CurTime: function () {
                                return Date.parse(new Date()) / 1000;
                            },
                            DateToUnix: function (string) {
                                var f = string.split(' ', 2);
                                var d = (f[0] ? f[0] : '').split('-', 3);
                                var t = (f[1] ? f[1] : '').split(':', 3);
                                return (new Date(
                                    parseInt(d[0], 10) || null,
                                    (parseInt(d[1], 10) || 1) - 1,
                                    parseInt(d[2], 10) || null,
                                    parseInt(t[0], 10) || null,
                                    parseInt(t[1], 10) || null,
                                    parseInt(t[2], 10) || null
                                )).getTime() / 1000;
                            },
                            UnixToDate: function (unixTime, isFull, timeZone) {
                                if (typeof (timeZone) == 'number') {
                                    unixTime = parseInt(unixTime) + parseInt(timeZone) * 60 * 60;
                                }
                                var time = new Date(unixTime * 1000);
                                var ymdhis = "";
                                ymdhis += time.getUTCFullYear() + "-";
                                ymdhis += (time.getUTCMonth() + 1) + "-";
                                ymdhis += time.getUTCDate();
                                if (isFull === true) {
                                    ymdhis += "" + time.getUTCHours() + ":";
                                    ymdhis += time.getUTCMinutes() + ":";
                                    ymdhis += time.getUTCSeconds();
                                }
                                return ymdhis;
                            }
                        }
                    });

                    function testone(promo) {
                        var promo_link = $(promo).attr("href");
                        var promo_title = $(promo).children('img').attr("title");
                        var promo_position = $(promo).children('img').attr("alt");
                        var onlinedate = $(promo).children('div').attr("title");
                        var promo_onlinedate = $.myTime.DateToUnix(onlinedate)
                        //alert(promo_link)
                        //alert(promo_title)
                        //alert(promo_position)
                        //alert(promo_onlinedate)
                        //alert('ç¾åº¦ç»è®¡ï¼'+'promo_linkï¼    '+promo_link+'promo_titleï¼    '+promo_title+'promo_positionï¼    '+promo_position+'promo_onlinedateï¼    '+promo_onlinedate)

                        _hmt.push(['_trackCustomEvent', 'promo_click', { 'promo_link': promo_link, 'promo_title': promo_title, 'promo_position': promo_position, 'promo_onlinedate': promo_onlinedate }]);

                    }

                    function testtwo(promo) {
                        var promo_link = $(promo).attr("href");
                        var promo_title = $(promo).children('div').attr("title");
                        var promo_position = $(promo).attr("alt");
                        var onlinedate = $(promo).children('span').attr("title");
                        var promo_onlinedate = $.myTime.DateToUnix(onlinedate)


                        _hmt.push(['_trackCustomEvent', 'promo_click', { 'promo_link': promo_link, 'promo_title': promo_title, 'promo_position': promo_position, 'promo_onlinedate': promo_onlinedate }]);
                    }
                    function testthree(promo) {
                        var promo_link = $(promo).attr("href");
                        var promo_title = $(promo).parents(".card").children('div').children('h4').children('a').attr("title");
                        var promo_position = $(promo).parents(".card").attr("title");
                        var onlinedate = $(promo).parents(".card").children('div').children('.action-box').attr("title");
                        var promo_onlinedate = $.myTime.DateToUnix(onlinedate)


                        _hmt.push(['_trackCustomEvent', 'promo_click', { 'promo_link': promo_link, 'promo_title': promo_title, 'promo_position': promo_position, 'promo_onlinedate': promo_onlinedate }]);
                    }
