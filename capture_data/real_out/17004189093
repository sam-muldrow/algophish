                };
            },
            error: function (request, textStatus, errorThrown) {
                //	alert("è·åç»å½ç¨æ·ä¿¡æ¯éè¯¯");
                username = undefined;
                dengluF(username);
                //               $('.guessFavorite_dl').show();
