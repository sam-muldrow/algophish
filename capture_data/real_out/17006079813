<script>
$(window).ready(function() {
	$("#submitNewsletter").submit(function(e) {
		e.preventDefault();
		e.stopPropagation();
	}).validate({
		rules: {
			"cgv": {
				"required": true
			},
			"email": {
				"required": true,
				"email": true,
				"maxlength": 255
			}
		},
		messages: {
			email: {
				//required: ""
				required: Translator.getText("menu","footer.newsletter.alert.email","Cette adresse email est déjà enregistrée.")
			},
			cgv: {
				error: Translator.getText("menu","footer.newsletter.alert.conditions","Vous devez accepter les conditions")
			}
		}, 
		errorPlacement: function(error, element) {
			var placement = $(element).data('error');
			if (placement) {
				$(placement).append(error)
			} else {
				error.insertAfter(element);
			}
		},
		submitHandler: function(form) {
			$('.result-newsletter-failure').addClass('none');
			var form = $("#submitNewsletter")
			jQuery.ajax({
				type	: "post",
				url		: form.attr("action"),
				data	: form.serialize(),
				success : function(data) {
					var _res = $.parseJSON(data)
					console.log(_res.success)
					if(_res.success) {
						$('.form-newsletter').addClass('none');
						$('.result-newsletter').removeClass('none');
						
						window.dataLayer = window.dataLayer || [];
						dataLayer.push({
							 'event': 'NewsletterSubsciption',
							 'eventCategory': 'All templates',
							 'eventAction': 'Subscribe',
							 'eventLabel': 'Newsletter subscription',
						});
						
					} else {
						$('.result-newsletter-failure').removeClass('none');
					}
				  }
			});
			return false;
		}
	});
});
</script>
	</body>
</html>



<script>
var _loaded = false;
$('.dropdown').bind('mousemove', function(){
	var _obj = $('.dropdown-menu');
	if(!_loaded){
		$("ul.nav li.dropdown").trigger('mouseover');
		if($('.dropdown-menu').length >= 1){
			_loaded = true;
		}
	}
});
</script>
                    

