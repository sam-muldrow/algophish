    <script>
        $(function () {
            $(ClientCoupon).on("onEventAddedUI", function (event, Code, MarketId, OutcomeId, coupon, eventType) {
                $(".event .m" + Code + "_" + MarketId + "_" + OutcomeId).addClass("selected");
            });
            $(ClientCoupon).on("onEventRemovedUI", function (event, Code, MarketId, OutcomeId, coupon, eventType) {
                $(".event .m" + Code + "_" + MarketId + "_" + OutcomeId).removeClass("selected");
            });
            $(ClientCoupon).on("onEventAddedUI", function (event, Code, MarketId, OutcomeId, coupon, eventType) {
                $(".tab-pane .m" + Code + "_" + MarketId + "_" + OutcomeId).addClass("selected");

                if(!ProgramManager.IsMostPopularEventsMarket(Code,MarketId)){
                $(".tab-pane .m_p" + Code).addClass("selected"); //öne çıkan maçlar widget
                 $(".tab-pane .m_p_320_" + Code).addClass("active"); //öne çıkan maçlar widget
                }
            });
            $(ClientCoupon).on("onEventRemovedUI", function (event, Code, MarketId, OutcomeId, coupon, eventType) {
                $(".tab-pane .m" + Code + "_" + MarketId + "_" + OutcomeId).removeClass("selected");
                $(".tab-pane .m_p" + Code).removeClass("selected"); //öne çıkan maçlar widget
                $(".tab-pane .m_p_320_" + Code).removeClass("active"); //öne çıkan maçlar widget
            });
        });
    </script>
    


