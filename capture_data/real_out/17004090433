        function footerClickPushEvent(position, name, destination, section) {
            if (!window.gtmDataLayer) {
				console.warn('window.gtmDataLayer is null or undefined');
                return;
            }

            window.gtmDataLayer.push({
                "event": "footer_click",
                "eventParams": {
                    "location": "Footer",
                    "position": position,
                    "name": name,
                    "destination": destination,
                    "section": section
                }
            });
        }
    </script>
