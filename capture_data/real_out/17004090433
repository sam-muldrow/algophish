					{
						"platform": "play",
						"id": ""
					}
				]
			}

			var manifestJson = JSON.stringify(manifest);
			var manifestBlob = new Blob([manifestJson], { type: 'application/json' });
			var manifestUrl = URL.createObjectURL(manifestBlob);

			var androidBannerElement = document.createElement('link');
			androidBannerElement.setAttribute("rel", "manifest");
			androidBannerElement.setAttribute("href", manifestUrl);

			document.getElementsByTagName('head')[0].appendChild(androidBannerElement);
		}
