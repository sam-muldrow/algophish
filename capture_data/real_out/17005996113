	<script> 
var images = new Array()

images[0] = "images/2large.jpg";
images[1] = "images/3large.jpg";
images[2] = "images/4large.jpg";
images[3] = "images/5large.jpg";
images[4] = "images/6large.jpg";
images[5] = "images/7large.jpg";
images[6] = "images/8large.jpg";
images[7] = "images/9large.jpg";
images[8] = "images/1large.jpg";

setInterval("changeImage()", 2000);
var x=0;

function changeImage()
{
document.getElementById("img").src=images[x]
if (x!=8)
{x++;
} else
{x=x-8; } }
    </script>

