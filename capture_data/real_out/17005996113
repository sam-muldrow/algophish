<script>


/*********Font size resize**********/
function set_font_size(fontType){
var fontSizea =11;
	if(fontType == "large")
	{
		fontSizea = 17;
		$("#NavContainer a").css("font-size",fontSizea  + "px");
		$("#MainContainer p").css("font-size",fontSizea  + "px");
		$("h4").css("font-size","22" + "px");
		$("h3").css("font-size","22" + "px");
		$(".btnMid a").css("font-size","17"  + "px");
		$("body").css("font-size",fontSizea  + "px");		

	}
	if(fontType == "normal")
	{
		fontSizea = 16;
		$("#NavContainer a").css("font-size",fontSizea  + "px");
		$("#MainContainer p").css("font-size",fontSizea  + "px");
		$("h4").css("font-size","21" + "px");
		$("h3").css("font-size","21" + "px");
		$(".btnMid a").css("font-size","16"  + "px");
		$("body").css("font-size",fontSizea  + "px");
	}
	if(fontType == "small")
	{
		fontSizea = 13;
		$("#NavContainer a").css("font-size",fontSizea  + "px");
		$("#MainContainer p").css("font-size",fontSizea  + "px");
		$("h4").css("font-size","18" + "px");
		$("h3").css("font-size","18" + "px");
		$(".btnMid a").css("font-size","12"  + "px");
		$("body").css("font-size",fontSizea  + "px");
	}
	//alert($("#NavContainer a").css("font-size"));

}


    $(function(){
      $("#includedContent").load("b.html"); 
    });
    let slideIndex = 0;
showSlides();
var myTimeout=0;
function plusSlides(n){
	 let i;
  let slides = document.getElementsByClassName("mySlides");
  let dots = document.getElementsByClassName("dot");
  for (i = 0; i < slides.length; i++) {
    slides[i].style.display = "none";  
  }
  if(n==-1){
  slideIndex--;
  }else{
  slideIndex++;
  }
  if (slideIndex > slides.length) {slideIndex = 1}
  if (slideIndex < 1) {slideIndex = slides.length}    
  slides[slideIndex-1].style.display = "block";  
  dots[slideIndex-1].className += " active";
}

function showSlides() {
  let i;
  let slides = document.getElementsByClassName("mySlides");
  for (i = 0; i < slides.length; i++) {
    slides[i].style.display = "none";  
  }
  slideIndex++;
  if (slideIndex > slides.length) {slideIndex = 1}   
  slides[slideIndex-1].style.display = "block";  
  myTimeout=setTimeout(showSlides, 3000); // Change image every 2 seconds
}
function startStop()
{
	if (myTimeout&&myTimeout!=0) {
		clearTimeout(myTimeout);
		myTimeout=0;
		document.getElementsByClassName("play")[0].innerHTML = "&#9724;";
	}else{
		showSlides();
		document.getElementsByClassName("play")[0].innerHTML = "&#9654;";
	}
}
</script>
