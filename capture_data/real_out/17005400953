<style>.caro { height: 150px; }</style>

<script>
    function buildCarousel(visibleSlides) {
        $('.caro').cycle({
            fx: 'carousel',
            speed: 600,
            slides: 'div',
            carouselVisible: visibleSlides,
            carouselFluid: true
        });
        $('.caro img').css('opacity', '1');
    }

    function initCycle() {
        var width = $(document).width();
        var visibleSlides = 7;

        if (width < 400) {
            visibleSlides = 1
        }
        else if (width < 700) {
            visibleSlides = 3
        }
        else {
            visibleSlides = 7
        }

        buildCarousel(visibleSlides);
    }

    function reinit_cycle() {
        var width = $(window).width();

        var destroyCarousel = function() {
            // create a function
            $('.caro').cycle('destroy');
        }

        if (width < 400) {
            destroyCarousel(); // call the function
            reinitCycle(1);
        } else if (width > 400 && width < 700) {
            destroyCarousel();
            reinitCycle(3);
        } else {
            destroyCarousel();
            reinitCycle(7);
        }
    }

    function reinitCycle(visibleSlides) {
        buildCarousel(visibleSlides);
    }
    var reinitTimer;
    $(window).resize(function() {
        clearTimeout(reinitTimer);
        reinitTimer = setTimeout(reinit_cycle, 100);
    });

    initCycle();

</script>
    </div>
