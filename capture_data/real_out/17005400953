    <script>
      var menu_open = false;
      function menu(e) {
        //$('html, body').scrollTop(0);
        if (menu_open) {
          $('#submenu').hide();
          $('#menuOpenBtn').show();
          $('#menuCloseBtn').hide();
          menu_open = false;
        } else {
          $('#submenu').show();
          $('#menuOpenBtn').hide();
          $('#menuCloseBtn').show();
          menu_open = true;
        }
      }
    </script>

