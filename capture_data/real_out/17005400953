<script>
    $(document).ready(function () {

        var $container = $("#newsletter");

        $container.find('form').submit(function(event){
            event.preventDefault();

            var $form = $(this);

            $.ajax({
                url: $form.attr('action'),
                type: 'POST',
                data: $form.serialize(),
                success: function(data) {
                    $container.find(".alert-warning").hide()
                    $container.find(".alert-success").show().html(data['message']);
                    $form.find("input").not("input[type='hidden']").each(function (key, value) {
                        $(value).removeClass('is-invalid');
                        $(value).val('');
                    })
                },
                error: function(xhr, textStatus, error) {

                    if (xhr.status === 422) {
                        $.each(xhr['responseJSON']['errors'], function (key, value) {
                            $container.find(".alert-success").hide()
                            $container.find(".alert-warning").show().html(value[0]);
                            $form.find("input[name='" + key + "']").addClass('is-invalid');
                        });
                    }
                    else if (xhr.status === 400) {
                        $container.find(".alert-success").hide()
                        $container.find(".alert-warning").show().html(xhr['responseJSON']['message']);
                    }
                    else if (xhr.status === 500) {
                        console.log(xhr['responseJSON']['message'])
                    } else {
                        console.log(error)
                    }
                }
            });
        });
    });
</script>
