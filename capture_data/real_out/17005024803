    <script type="text/javascript">
        var logTiming = (function () {
            var startTime = new Date().getTime();
            var lastTime = startTime;
            var now = startTime;

            var log = function (options) {
                if (!options.timingValue) {
                    now = new Date().getTime();
                    options.timingValue = options.sinceStart ? now - startTime : now - lastTime;
                    lastTime = now;
                }
                
                    ga('send', 'timing', options.timingCategory, options.timingVar, options.timingValue, options.timingLabel);
                
            };
            return log;
        })();
    </script>
