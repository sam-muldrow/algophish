<div class="tabs__content tab-content">
<div class="tab-pane fade show active" id="v-pills-horizontal-tab-item-85522-1" role="tabpanel" aria-labelledby="v-pills-horizontal-tab-item-85522-1-tab">
<div>
<div id="paragraph-id-85522"></div>
<div class="entity entity-paragraphs-item paragraphs-item-entity-cards paragraph paragraph--type--entity-cards--homepage paragraph--view-mode--teaser">
<div class="field field-name-field-entity field--type-entity-reference field--label-hidden field--items">
<div class="field--item">
<a href="/company/goldman-sachs" class="company-teaser-card--homepage d-block">
<article data-history-node-id="122857" class="company teaser-homepage clearfix node node-company">
<div class="company-card__wrapper">
<div class="company__logo">
<div class="field field-name-field-company-logo-teaser field--type-image field--label-hidden field--item"> <img decoding="async" loading="lazy" src="/files/styles/medium/public/2023-10/goldman-sachs_3.png?itok=d_3X7r_A" width="215" height="220" alt="Goldman Sachs logo" />
</div>
</div>
<div class="company__info">
<div class="company__label">
