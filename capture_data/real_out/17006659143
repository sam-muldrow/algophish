function tampilcari(){
	var input = document.getElementById('cari');
	if(input.value==='Cari artikel, berita, tanya dokter, forum dan tentang asi'){
		input.value='';
		input.style.color='#000';
	}

}

function hilangcari(){
		var input = document.getElementById('cari');
		if(input.value===''){
			input.value ='Cari artikel, berita, tanya dokter, forum dan tentang asi';
			input.style.color='#ccc';
		}
	}

