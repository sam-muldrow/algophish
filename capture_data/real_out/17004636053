<h3 class="jeg_block_title"><span>COMMUNITY FORUMS</span></h3>
</div></div></div></div></div><div class="row vc_row wpb_row vc_row-fluid"><div class="jeg-vc-wrapper"><div class="wpb_column jeg_column vc_column_container vc_col-sm-8 jeg_main_content"><div class="jeg_wrapper wpb_wrapper">
<div class="wpb_raw_code wpb_content_element wpb_raw_html">
<div class="wpb_wrapper">
<div class="static-feature">
<div class="threads-left">
<div class="active-threads">Active Threads</div>
<div id="forum_active_topics_list">
</div>
</div>
<div class="threads-right">
<div class="new-threads">Latest Threads</div>
<div id="forum_latest_topics_list">
</div>
</div>
</div>
<div class="clear"></div>
<script type="text/javascript">
	/* TODO: move this string format into a js file */
	String.prototype.format = function() {
	  var args = arguments;
	  return this.replace(/{(\d+)}/g, function(match, number) { 
	    return typeof args[number] != 'undefined'
	      ? args[number]
	      : match
	    ;
	  });
	};
     
	function fetch_forum_topics() {
		jQuery.ajax({
			url: "//forum.lowyat.net/ajax.php?mode=latest_topics",
			dataType: "jsonp",
			cache: true,
			jsonpCallback: "fetch_forum_topics_callback",
			success: function(response) {

				var tmpl = "<div class='rowthis'><a href='{1}' class='forum-link thread-title' target='_blank'>{2}</a></div>";

				for (var type in response) {
					var target = "#forum_" + type + "_topics_list";
					jQuery(target).empty();
					for (var i in response[type]) {
						var topic = response[type][i];
						jQuery(target).append(tmpl.format(topic.forum, topic.url, topic.title, topic.author,topic.views));					
					}
				}
			}
		});
	}

	fetch_forum_topics();
	jQuery(window).focus(fetch_forum_topics);
</script>
</div>
</div>
</div></div><div class="wpb_column jeg_column vc_column_container vc_col-sm-4 jeg_sidebar"><div class="jeg_wrapper wpb_wrapper">
