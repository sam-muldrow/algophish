	function keydownVideoEstandar(e,firstFocusableElementEstandar,lastFocusableElementEstandar) {

	  if (e.shiftKey) { // if shift key pressed for shift + tab combination
		  if (document.activeElement === firstFocusableElementEstandar) {
			  lastFocusableElementEstandar.focus(); // add focus for the last focusable element
			  e.preventDefault();
			} 
	  } else { // if tab key is pressed
			if (document.activeElement === lastFocusableElementEstandar) { // if focused has reached to last focusable element then focus first focusable element after pressing tab
				firstFocusableElementEstandar.focus(); // add focus for the first focusable element
			  e.preventDefault();
			}
	  }
