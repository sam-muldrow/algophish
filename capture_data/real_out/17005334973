                                    messageName.find('.anxs-8qwe-attention').hide();
                                }

                            }
                        }
                    });
                },
                //è·åipçå¼
                isIp:function(nameData){
                    for(var prop in nameData){
                        if(prop == 'OnlyIPLogin'){
                            return nameData[prop];
                            break;
                        }
