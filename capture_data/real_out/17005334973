<script>
window.__loginCallback = function(data) {
  var lastLogin = ''
  var groupId = ''
  var personId = ''
  data.context.accountIds
    .map(function(item){return item.replace(/\./g,'_')})
    .forEach(function(item){
      if(item.indexOf('Group') === 0) {
        groupId = item
      } else if(item.indexOf('Person') === 0) {
        personId = item
        lastLogin = document.cookie.split('; ').filter(c=>c.indexOf(item)===0)[0]
        document.cookie = item + '=true;expires=${new Date(new Date().getTime()+7200000).toUTCString()};domain=.wanfangdata.com.cn'
      }
    })
  // 24 å°æ¶å ç»å½ ä¸ªäººè´¦å·ï¼è·³è½¬å° sns
  var pathname = window.location.pathname
  var paths = ['/', '/index.html']
  if(personId && !lastLogin && window.__isWFIndex && paths.some(function(path){return path===pathname})) {
    window.location = 'https://sns.wanfangdata.com.cn/'
  }
  // group å®å¶ Logo
  if(document.getElementById('anxs-logo_sns')) {
    document.getElementById('anxs-logo_sns').src = 'https://w.wanfangdata.com.cn/www/file/logo/' + groupId + '.png?default=image/' + window.__logo_theme_name
  }
  if(document.getElementById('anxs-logoName_sns')) {
    var initGroupName = document.head.appendChild(document.createElement('script'))
    initGroupName.src = 'https://w.wanfangdata.com.cn/www/file/logo/' + groupId + '.js'
    initGroupName.remove()
  }
}
