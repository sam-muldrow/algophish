                //è·åurlåæ°
                getRequest: function () {
                    var url = location.search; //è·åurlä¸­"?"ç¬¦åçå­ä¸²
                    if (!url) {
                        return '';
                    }
                    var theRequest = new Object();
                    if (url.indexOf("?") != -1) {
                        var str = url.substr(1);
                        var strs = str.split("&");
                        for (var i = 0; i < strs.length; i++) {
                            theRequest[strs[i].split("=")[0]] = unescape(strs[i].split("=")[1]);
                        }
                    }
                    return theRequest;
                },
                //è·åcookieåå­
                getCookies: function (cookieName) {
                    var strCookie = document.cookie;
                    var arrCookie = strCookie.split("; ");
                    for (var i = 0; i < arrCookie.length; i++) {
                        var arr = arrCookie[i].split("=");
                        if (cookieName == arr[0]) {
                            return arr[1];
                        }
                    }
                    return "";
                },
                //èµå¼ç»iframe
                getSrc: function (loginModel) {
                    this.layerAlert();
                    me.attr('src', '//my.wanfangdata.com.cn/auth/user/'+loginModel+'?login_mode=AJAX&service=' + encodeURIComponent(window.location.href));
                },
