            var certificationImg = grShow.find('.anxs-8qwe-certification-no');  //è®¤è¯å¾ç
            var jgLogin = $('.anxs-8qwe-login-jg');   //æºæç»éæé®
            var jgname = $('.anxs-8qwe-list-jg');
            var jgMain = jgname.find('.anxs-8qwe-nav-list-main');    //æºæä¸æåè¡¨
            var listJgName = jgname.find('.anxs-8qwe-login-jg');     //æºæä¸æåè¡¨ä¸­çæºæç»éæé®
            var registerName = $('.anxs-8qwe-register');             //æ³¨åæé®
            var personBack = $('.anxs-8qwe-back');                   //ä¸ªäººä¸æåè¡¨ä¸­çéåºæé®
            var messageName = $('.anxs-8qwe-list-message');          //æ¶æ¯æé®
            var messageNum = messageName.find('.anxs-8qwe-num');     //æ¶æ¯æé®ä¸æåè¡¨ä¸çæ¶æ¯æ°
            var castgc = '';
            var addPeople = $('.anxs-8qwe-addPeople');               //æ°å¢å³æ³¨

            //Binding mecFhanism variable
            var bindMe = $('.anxs-8qwe-list_bind');
            var bindMeWx = $('.anxs-8qwe-list_bind-wx');//æ«ç 
            var bindMeImg = $('.anxs-8qwe-list_bind-img');//å¾ç
            var bindMeTitle = bindMeWx.find('.anxs-8qwe-bind-title');//æå­æç¤º
            var bindTime = '';

            return {
                //ç»å®æºæçåºç°
                bindMeShow:function () {
                    var that = this;
                    $.ajax({
                        url: 'https://login.wanfangdata.com.cn/showBindTip',
                        dataType: 'jsonp',
                        jsonp: 'callback',
                        cache:false,
                        success:function(data){
                            that.cookieAjax();
                            if(data == 'true') {
                                bindMe.show();
                                that.bindMechanism();
                            }else {
                                bindMe.hide();
                                $('.anxs-8qwe-bind-layer').hide();
                            }
                        }
                    });
                },
                //ç»å®æºæ
                bindMechanism:function () {
                    var that = this;
                    bindMe.hover(function (e) {
                        bindMeWx.show();
                        that.getBindqcInfo();
                        that.refreshQrCode();
                    },function () {
                        bindMeWx.hide();
                        clearInterval(that.bindTime);
                        clearInterval(that.sweepCode);
                    });
                },
                //æ¿å°äºç»´ç ä¿¡æ¯
                getBindqcInfo:function () {
                    var that = this;
                    clearInterval(that.sweepCode);
                    $.ajax({
                        url:'https://login.wanfangdata.com.cn/getQRcodeMessage?time='+(+new Date()),
                        dataType:'jsonp',
                        jsonp:'callback',
                        success:function (data) {
                            if(data == 'isBind') {
                                window.location.reload();
                            }else if(data == 'error'){
                                window.location.reload();
                            }else {
                                var Ciphertext = data.ciphertext,
                                        CodeId = data.codeId;
                                bindMeImg.attr('src','https://login.wanfangdata.com.cn/createQRCode?ciphertext='+encodeURIComponent(Ciphertext))
                                that.sweepCodeSuccess(CodeId);
                                that.refreshsweepCodeSuccess(CodeId);
                            }
                        },
                        error:function() {
                            console.log('äºç»´ç è¯·æ±å¤±è´¥!');
                        }
                    });
                },
                //1ç§éèªå¨å·æ°æ«æç¶æè¯·æ±
                refreshsweepCodeSuccess:function (CodeId) {
                    var that = this;
                    clearInterval(that.sweepCode);
                    this.sweepCode = setInterval(function () {
                        that.sweepCodeSuccess(CodeId);
                    },1500)
                },
                //ä¸åéèªå¨å·æ°
                refreshQrCode:function () {
                    var that = this;
                    clearInterval(that.bindTime);
                    this.bindTime = setInterval(function () {
                        that.getBindqcInfo();
                    },60000)
                },
                //æ«ææå
                sweepCodeSuccess:function (CodeId) {
                    var that = this;
                    $.ajax({
                        url: 'https://login.wanfangdata.com.cn/getQRCodeStatus',
                        dataType:'jsonp',
                        jsonp:'callback',
                        data:{codeId:CodeId},
                        success:function(data){
                            if(data == 'scanned') {
                                clearInterval(that.sweepCode);
                                clearInterval(that.bindTime);
                                $('.anxs-8qwe-bind-layer').show();
                            }else if (data == 'available') {
                                $('.anxs-8qwe-bind-layer').hide();
                            }else{
                                $('.anxs-8qwe-bind-layer').hide();
                            }
                        },
                        error:function (err) {
                            console.log(err)
                        }
                    });
                },
                //å¼¹åºç»å½æ¡
                layerAlert:function(){
                    var winHeight = $(window).height();
                    if(winHeight<800){
                        layer.open({
                            offset: '40px',
                            type: 1, //pageå± 1divï¼2é¡µé¢
                            area: ['50%px'],
                            title: '',
                            shade: 0.6, //é®ç½©éæåº¦
                            moveType: 1, //ææ½é£æ ¼ï¼0æ¯é»è®¤ï¼1æ¯ä¼ ç»æå¨
                            shift: 1, //0-6çå¨ç»å½¢å¼ï¼-1ä¸å¼å¯
                            content: $("#anxs-8qwe-login"),
                            success:function(layero){
                                $(layero).css({'position':'absolute','top':'40px','left':'50%','marginLeft':'-350px','marginBottom':'40px'});
