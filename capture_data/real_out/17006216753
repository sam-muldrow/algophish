66}#sidebar li.item-chinese .lanText-cn,#sidebar li.item-chinese .lanText-tw{opacity:.5}#sidebar li.item-chinese .lanText-cn.active,#sidebar li.item-chinese .lanText-tw.active{opacity:1}#sidebar li.item-chinese .translateBtn{margin-left:-2.5px}#sidebar li a{color:#333;display:block;padding-left:20px;padding-right:20px;position:relative;text-decoration:none;white-space:nowrap}#sidebar li a.collapsed .fa-chevron-up{display:none}#sidebar li a .fa-chevron-up,#sidebar li a.collapsed .fa-chevron-down{display:inline}#sidebar li a .fa-chevron-down{display:none}#sidebar li a .icon{display:inline-block;margin-right:15px}#sidebar li a .icon.toggle{margin-right:0;position:absolute;right:10px}#sidebar li li a{padding-left:47px;transition:all .2s ease}#sidebar li li a:active,#sidebar li li a:focus,#sidebar li li a:hover{padding-left:52px}#footer{background:#f9f9f9;font-size:1em}#footer .info{border-bottom:1px solid #b3b3b3;color:#b3b3b3;font-size:medium;padding:40px 0}#footer .info a,#footer .info a a{color:#666}#footer .info a:active,#footer .info a:active a,#footer .info a:focus,#footer .info a:focus a,#footer .info a:hover,#footer .info a:hover a{color:#333}.mode-night #footer .info a,.mode-night #footer .info a a{color:#999}.mode-night #footer .info a:active,.mode-night #footer .info a:active a,.mode-night #footer .info a:focus,.mode-night #footer .info a:focus a,.mode-night #footer .info a:hover,.mode-night #footer .info a:hover a{color:#ccc}#footer .info-content{display:flex;justify-content:space-evenly;margin:0 auto;max-width:1024px;width:100%}@media (max-width:1199.98px){#footer .info-content{justify-content:center;width:100%}}@media (max-width:991.98px){#footer .info-content{flex-direction:column}}#footer .logos-wrapper{display:flex;flex-direction:column;row-gap:12px}@media (max-width:1199.98px){#footer .logos-wrapper{margin:0 auto}}#footer .logos-wrapper .logo{width:180px}#footer .logos-wrapper .logos-wrapper-info{-moz-column-gap:12px;column-gap:12px;display:flex;justif