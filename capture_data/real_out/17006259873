		<script>
			const morningTime = "9:00:00 AM";
			const eveningTime = "5:00:00 PM";
			const dateNow = new Date(Date.now());
	
			var currTimeString = dateNow.toLocaleTimeString('en-US', { timeZone: 'America/Chicago' });
			var morningCheck = morningTime.split(":");
			var userCheck = currTimeString.split(":");
			var eveningCheck = eveningTime.split(":");
			var morningNum = parseInt(morningCheck[0]);
			var eveningNum = parseInt(eveningCheck[0]);
			var userNum = parseInt(userCheck[0]);
			var userMinsNum = parseInt(userCheck[1]);
			var agentsAvailable = false;
	
			if((userNum >= morningNum && userNum < 12 && currTimeString.includes("AM")) || userNum == 12 && currTimeString.includes("PM") || userNum >= 1 && userNum <= eveningNum  && currTimeString.includes("PM")){
				agentsAvailable = true;
			 
			  if(userNum == 5 && userMinsNum > 0){
				agentsAvailable = false;
			  }
			  if(agentsAvailable){
				$(document).ready(function(){
				   setTimeout("_genesys.widgets.bus.command('WebChat.open');", 15000);
				});			
			  }
			}
		</script>
