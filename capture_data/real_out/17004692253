);

function observeTakeoverActive() {  const targetNode = document.querySelector('html');  
	const adSlotContainer = document.querySelector('.container-outer.top-ads-container-outer.remove-on-mobile');  
	const config = { attributes: true, attributeFilter: ['class'] };  
	
	const callback = (mutationsList) => {    
		for (const mutation of mutationsList) {      
			if (mutation.type === 'attributes') {        
				if (targetNode.classList.contains('takeover-active')) {          
					ramp.hideAdSlot('leaderboard_atf');          
					adSlotContainer.style.minHeight = 'unset';        
				}      
			}    
		}  
	};  

	const observer = new MutationObserver(callback);  
	observer.observe(targetNode, config); 
} 

observeTakeoverActive();
</script>
