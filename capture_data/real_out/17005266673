                             <script type='text/javascript'>
 function activateRegionCategory(catRegionName, obj) {
  cats = document.getElementsByClassName('category'); 
  for(var i = 0; i < cats.length; i++) 
    cats[i].style.display='none'; 
  cat = document.getElementById(catRegionName);
  cat.style.display='block';
  cats = document.getElementsByClassName('categorybutton'); 
  for(var i = 0; i < cats.length; i++) 
    cats[i].className='categorybutton button'; 
  obj.className='categorybutton button selectedbutton';
 }</script>
