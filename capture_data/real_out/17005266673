                                <script language="javascript" type="text/javascript">
                                    // carte obs animation 12h

                                    images_url = new Array();
                                    images_url[0] = "/cartes_obs/archives/20-11-2023/temp2_1h-01.png";
images_url[1] = "/cartes_obs/archives/20-11-2023/temp2_1h-02.png";
images_url[2] = "/cartes_obs/archives/20-11-2023/temp2_1h-03.png";
images_url[3] = "/cartes_obs/archives/20-11-2023/temp2_1h-04.png";
images_url[4] = "/cartes_obs/archives/20-11-2023/temp2_1h-05.png";
images_url[5] = "/cartes_obs/archives/20-11-2023/temp2_1h-06.png";
images_url[6] = "/cartes_obs/archives/20-11-2023/temp2_1h-07.png";
images_url[7] = "/cartes_obs/archives/20-11-2023/temp2_1h-08.png";
images_url[8] = "/cartes_obs/archives/20-11-2023/temp2_1h-09.png";
images_url[9] = "/cartes_obs/archives/20-11-2023/temp2_1h-10.png";
images_url[10] = "/cartes_obs/archives/20-11-2023/temp2_1h-11.png";
images_url[11] = "/cartes_obs/archives/20-11-2023/temp2_1h-12.png";
images_url[12] = "/cartes_obs/archives/20-11-2023/temp2_1h-13.png";
images_url[13] = "/cartes_obs/archives/20-11-2023/temp2_1h-14.png";
images_url[14] = "/cartes_obs/archives/20-11-2023/temp2_1h-15.png";
images_url[15] = "/cartes_obs/archives/20-11-2023/temp2_1h-16.png";
images_url[16] = "/cartes_obs/archives/20-11-2023/temp2_1h-17.png";
images_url[17] = "/cartes_obs/archives/20-11-2023/temp2_1h-18.png";
images_url[18] = "/cartes_obs/archives/20-11-2023/temp2_1h-19.png";
images_url[19] = "/cartes_obs/archives/20-11-2023/temp2_1h-20.png";
images_url[20] = "/cartes_obs/archives/20-11-2023/temp2_1h-21.png";
images_url[21] = "/cartes_obs/archives/20-11-2023/temp2_1h-22.png";
images_url[22] = "/cartes_obs/archives/20-11-2023/temp2_1h-23.png";
images_url[23] = "/cartes_obs/archives/21-11-2023/temp2_1h-00.png";
images_url[24] = "/cartes_obs/temp2_1h.png";

                                    var curImage = 24;
                                    var animated = 0;
                                    var numimages = 25;
                                    var precharge = 0;
                                    var speed = 500;
                                    var imageName = "obs_temps";
                                    var animLinkName = "animLink";

                                    images = new Array();

                                    function preloadImages()
                                    {
                                        precharge = 0;
                                        for(var i = 0; i < numimages; i++)
                                        {
                                            images[i] = new Image();
                                            images[i].onload = chargeFini;
                                            images[i].src = images_url[i];

                                        }
                                    }

                                    function chargeFini()
                                    {
                                        precharge++;
                                        document.getElementById("playText").innerHTML = "Chargement (" + precharge  + "/" + numimages + ")";

                                        if(precharge == numimages)
                                        {
                                            curImage = 0;
                                            startAnim();
                                            activateMap();

                                        }
                                    }

                                    function activateMap()
                                    {
                                        if(curImage >= 24)
                                        {
                                            document.getElementById(imageName).useMap = "#observ";
                                        }
                                        else
                                        {
                                            document.getElementById(imageName).useMap = "";                                  
                                        }
                                    }


                                    function animate()
                                    {
                                        if(!animated) return;
                                        
                                        curImage++;
                                        if(curImage == numimages+1) curImage = 0;
                                        
                                        if( curImage < numimages)
                                        {
                                            document.getElementById(imageName).src = images_url[curImage];
                                        }
                                        

                                        if(curImage < numimages)
                                        {
                                            anim = setTimeout("animate()", speed);
                                        }
                                        else
                                        {    
                                            anim = setTimeout("animate()", 1500);
                                        }
                                        activateMap();
                                    }


                                    function faster()
                                    {
                                        if(animated == 1)
                                        {
                                            speed = speed - 100;
                                            if(speed < 100) speed = 100;
                                        }
                                        else
                                        {
                                            curImage = curImage + 1;
                                            if(curImage >= numimages) curImage = numimages-1;
                                            document.getElementById(imageName).src = images_url[curImage];
                                        }
                                        activateMap();
                                        return false;
                                    }
                                    function slower()
                                    {
                                        if(animated == 1)
                                        {
                                            speed = speed + 100;
                                            if(speed > 2000) speed = 2000;
                                        }
                                        else
                                        {
                                            curImage = curImage - 1;
                                            if(curImage < 0) curImage = 0;
                                            document.getElementById(imageName).src = images_url[curImage];
                                        }
                                        activateMap();
                                        return false;
                                    }

                                    function startAnim()
                                    {
                                        if(animated == 0)
                                        {
                                            animated = 1;
                                            animate();
                                            var animPlayText = document.getElementById("playText");
                                            animPlayText.innerHTML = "Stopper l'animation";
                                            document.getElementById("playText").className = "pause";
                                            document.getElementById("animLink").onclick = stopAnim;
                                            document.getElementById("chevronOrMoins").className = "iconMoins";
                                            document.getElementById("next").onclick = faster;
                                            document.getElementById("next").title = "Plus rapide";
                                            document.getElementById("next").alt = "Plus rapide";
                                            document.getElementById("chevronOrPlus").className = "iconPlus";
                                            document.getElementById("prev").onclick = slower;
                                            document.getElementById("prev").title = "Plus lent";
                                            document.getElementById("prev").alt = "Plus lent";
                                        }
                                        activateMap();
                                        return false;
                                    }
                                    function stopAnim()
                                    {
                                        if(animated == 1)
                                        {
                                            animated = 0;
                                            document.getElementById("playText").innerHTML = "Animation 24h";
                                            document.getElementById("playText").className = "play";
                                            document.getElementById("animLink").onclick = startAnim;
                                            document.getElementById("prev").onclick = slower;
                                            document.getElementById("prev").title = "Image précédente";
                                            document.getElementById("prev").alt = "Image précédente";
                                            document.getElementById("chevronOrMoins").className = "chevronleft";
                                            document.getElementById("chevronOrPlus").className = "chevronright";
                                            document.getElementById("next").onclick = faster;
                                            document.getElementById("next").title = "Image suivante";
                                            document.getElementById("next").alt = "Image suivante";
                                            clearTimeout(anim);
                                        }
                                        return false;
                                    }

                                </script>


