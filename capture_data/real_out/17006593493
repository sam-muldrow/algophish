// when jquery loads
jQuery(document).ready(function($) {
  // track page views
  var pageProperties = {};
  if ($('.person-name').length) {
    pageProperties['Person Name'] = $('.person-name').text();
    pageProperties['Person Role'] = $('.person-role').text();
  } 
  mixpanel.track('View Page', pageProperties);

  // track FAQ clicks
  $('body').on('click', '.panel-group.faq.fadeIn', function() {
    var category = $(this).siblings('h2').text();
    var question = $(this).children('.faq-question').first().text();
    mixpanel.track('Click FAQ Question', {
      'Question': question,
      'Category': category
    });
  });
  
  // track talent application clicks
  $('body').on('click', 'a[href$="firstround.typeform.com/to/vR178S"]', function() {
    mixpanel.track('Click Talent Application');
  });
  
  // track external link clicks
  $('body').on('click', 'a', function() {
    var href = $(this).attr('href');
    if ((href.indexOf('#') === 0 || href.indexOf('/') === 0) && href.indexOf('review') < 0) {
      // internal link
      return;
    } else {
      mixpanel.track('Click External Link', {
        'Link URL': href,
        'Link Text': $(this).text()
      });
    }
  });
});
