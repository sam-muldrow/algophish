            <script type="text/javascript">
                function clearDims(url){
                 return url.slice(0,url.lastIndexOf("/dims"));
                }
                $(document).ready(function() {
                    $.ajax({
                        type: 'GET',
                        url: "https://rest.news1.kr/www/videos",
                        data:{},
                        cache: false,
                        success: function(data) {
                            var items = data.top_data;
                            if (typeof items !== "undefined" && items != null) {
                                _.each(items, function(item, idx) {
                                    if (idx === 0) {
                                        $("#videoC a").prop('href',"https://tv.news1.kr/video/"+item.id )
                                        $("#videoC img").prop('src',item.img_url).prop('srcset',item.img_url)
                                    }else{
                                        var row = '<a class="btn_avi" role="button" href="https://tv.news1.kr/video/?'+ item.id +'" target="_blank" data-idx="" data-poster="" data-title="" >'
                                            + '<img src="'+ item.img_url +'" srcset="' + item.img_url +'" alt="">'
                                            + '</a>';
                                        $(".side_avi_news > .link_avi > .btn_avi_area").append(row);

                                    }
                                });
                            }
                        }
                    });


                });
            </script>
            

