	// ê²ì
	function doSearchMain() {
		var searchForm = document.pront_top_search; 

		if (searchForm.query.value == "") {
			alert("ê²ìì´ë¥¼ ìë ¥íì¸ì.");
			searchForm.query.focus();
			return false;
		}

		searchForm.collection.value = "";
		searchForm.startDate.value = "";
		searchForm.endDate.value = "";
		searchForm.cal_range.value = "";
		searchForm.startCount.value = "";
		searchForm.searchField.value = "";
		searchForm.sort.value = "DATE";
		searchForm.listType.value = "";

		//doReset();
		return true;
	};

	function doSearch() {
		var searchForm = document.pront_top_search; 

		if (searchForm.query.value == "") {
			alert("ê²ìì´ë¥¼ ìë ¥íì¸ì.");
			searchForm.query.focus();
			return false;
		}
		
		searchForm.collection.value = "ALL";
		searchForm.startDate.value = "";
		searchForm.endDate.value = "";
		searchForm.cal_range.value = "A";
		searchForm.startCount.value = 0;
		searchForm.searchField.value = "ALL";
		searchForm.sort.value = "DATE";
		searchForm.listType.value = "listType1";

		searchForm.submit();
	}
</script>


