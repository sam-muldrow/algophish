        <button id="btn_top" class="top ir" roll="button">íë©´ ìµìë¨ ì´ëë²í¼</button>
        <script>
            $(document).ready(function (){
                function handleScrollTop(){
                    if($(this).scrollTop()>200 &&  !$("#btn_top").hasClass("fixed")){
                        $("#btn_top").addClass("fixed")
                    }else if ($(this).scrollTop() ==0){
                        $("#btn_top").removeClass("fixed")
                    }
                }
                $(this).on("scroll",function (e){
                    handleScrollTop()
                })
                handleScrollTop()
            })
        </script>
        <style>
            #btn_top.fixed{position: fixed}
