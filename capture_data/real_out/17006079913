<script>
    $(function(){
        $(window).scroll(function() {
            var scrollTop = $(document).scrollTop();
            var top=0;
            if (scrollTop < 490) {
                top = 0;
                $(".ad_wing_l").stop().animate({"top": top}, 500);
            }else{
                top = scrollTop-480;
                $(".ad_wing_l").stop().animate({"top": top}, 500);
            }
        });
    });
