    $(document).ready(function() {
        $(".list_01 .txt_area, .list_02 .txt_area, .list_03 .txt_area, .list_04 .txt_area, .list_05 .txt_area, .list_06 .txt_area, .list_07 .text").dotdotdot();
        $(".main_news .eli_02.text").dotdotdot();
        $(".middle_list .middle_news p").dotdotdot();
        // $(".common_list .common_news .eli_01").dotdotdot();
        $(".sl_card_area .tit_02").dotdotdot();
        $(".side_avi_news .txt, .side_topic h3, side_sl_list_section h5").dotdotdot(); // ì¤ëì íì , ë§ì´ ë³¸ ê¸°ì¬
        $(".side_topic_02 .text").dotdotdot(); // ì°ì¬ ê¸°ì¬
        $(".metro_list .tit_01").dotdotdot(); // ì¤í¬ì¸  ì°ì
        $(".side_pic_article .tit_03, .side_pic_article .text").dotdotdot(); // Book ë¦¬ë·°

        // ë§ì´ ë³¸ ê¸°ì¬
        $("#NewsHitsNav .page").click(function(e) {
            e.preventDefault();
            var cid = $(this).attr("data-cid");
            var $list = $("#NewsHitsList");
            for (var i=0; i<7; i++) {
                var url = _news_hits [cid] [i]['url'],
                    img_url = _news_hits [cid] [i]['img_url'],
                    title = _news_hits [cid] [i]['title'],
                    article_id = _news_hits [cid] [i]['article_id'];
                var $li = $list.find("li").eq(i);

                $li.find("a").attr("href", url);
                $li.find(".text").text(title);

                // ì¸ë¤ì¼ íì
                if (i >= 1) { continue; }
                if (typeof img_url === "undefined" || img_url == null || img_url === '') {
                    $li.removeClass("no_img");
                    $li.find("img").attr("src", 'https://image.news1.kr/system/resource/news1_www/images/noimage/no_image_175x110.jpg');
                } else {
                    $li.removeClass("no_img");
                    $li.find("img").attr("src", img_url);
                }
            }
        });

//     $(window).scroll(function(e){
//     	var st = $(this).scrollTop();
//     	if (st >= 475) { // ê¸°ì¤ ì¤í¬ë¡¤ y 180
//     		$(".ad_wing_l").addClass("sticky").wingLeft("#content .wrap_main_section");
//     		$(".ad_wing_r").addClass("sticky").wingSide("#content .wrap_main_section");
//     	} else {
//     		$(".ad_wing_l").removeClass("sticky").css({left:''});
//     		$(".ad_wing_r").removeClass("sticky").css({right:''});
//     	}
// //     	if (st > NEWS1.lastTop){ // down
// //     	} else { // up
// //     	}
//
// 		NEWS1.lastTop = st;
//     });

        $(".ad_wing_l").wingLeft("#content .wrap_main_section");
        $(".ad_wing_l").show();

        $(".ad_wing_r").wingSide("#content .wrap_main_section");
        $(".ad_wing_r").show();


        $(window).resize(function() {
            $(".ad_wing_l").wingLeft("#content .wrap_main_section");
            $(".ad_wing_r").wingSide("#content .wrap_main_section");
        });


    });

    /* jQuery íì¥í¨ì */
    $.fn.extend({
        wingLeft: function(content) {
            var $obj = $(this);
            if ($obj.hasClass("sticky")) {
                var offset = $(content).offset(),
                    wl_left = offset.left*1 - 105 + "px";
//NEWS1.log(offset.left + " " + offset.top + " " + wl_left);
                $obj.show().css({left: wl_left});
            }
        },

        wingLeftScroll: function() {
            var $obj = $(this);
            var st = $obj.scrollTop();

            if (st >= 180) { // ê¸°ì¤ ì¤í¬ë¡¤ y 180
                $obj.addClass("sticky").wingLeft("#content");
            } else {
                $obj.removeClass("sticky").css({left:''});
            }
//     	if (st > NEWS1.lastTop){ // down
//     	} else { // up
//     	}
            NEWS1.lastTop = st;
        },
        wingSide: function(content) {
            var $obj = $(this);
            if ($obj.hasClass("sticky")) {
                var offset = $(content).offset(),
                    wl_right = offset.left*1 - 105 + "px";
//NEWS1.log(offset.left + " " + offset.top + " " + wl_left);
                $obj.show().css({right: wl_right});
            }
        },

        wingRightScroll: function() {
            var $obj = $(this);
            var st = $obj.scrollTop();

            if (st >= 180) { // ê¸°ì¤ ì¤í¬ë¡¤ y 180
                $obj.addClass("sticky").wingSide("#content");
            } else {
                $obj.removeClass("sticky").css({right:''});
            }
//     	if (st > NEWS1.lastTop){ // down
//     	} else { // up
//     	}
//         NEWS1.lastTop = st;
        }

    });

    $(function() {
        $(".lazyimg").Lazy({
            attribute:"data-original",
            threshold: 20
            // afterLoad: function(element) {
            //           element.addClass('on')
            //        },
        });
    });

</script>
