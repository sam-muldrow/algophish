<!doctype html>
<head><meta name="Robots" content="noindex, nofollow"></head>
<script>
(function(localStorageKey, redirectLang, langs){
/* eslint-disable */
if (window.localStorage && window.localStorage.getItem(localStorageKey)) {
  const localStorageLang = window.localStorage.getItem(localStorageKey);
  if (langs.includes(localStorageLang)){ redirectLang = localStorageLang; }
}
window.location.replace(window.location.origin+'/'+redirectLang+window.location.search);

})('selected-language', 'en', ["de","en","sl","pl","hr"]);
</script>
</html>