<script src="https://cdnjs.cloudflare.com/ajax/libs/crypto-js/4.0.0/crypto-js.min.js"></script>
<script>
	var outEncoding="base64";
	var key ="SNdBtLxLpfHD7AAgxAYQGgmnpeWLpebT";
	key = CryptoJS.enc.Utf8.parse(key);

	var userInfo = JSON.stringify({
		"name" : "Anonymous" ,
		"surname" : "" ,
		"email" : "anonymous" ,
		"crmId" : "" ,
		"phone" : "+900000000000"
	});

	userInfo = CryptoJS.enc.Utf8.parse(userInfo);

	var encrypted = CryptoJS.AES.encrypt(userInfo, key, {
		mode: CryptoJS.mode.ECB
	});

	var cbotScript = document.getElementById("cbot-widget-script");

	cbotScript.setAttribute("data-user", encrypted);


	$(".cbot-close-icon-new").on("click", function(){
		var element = document.querySelector('.cbot-dialog-button-regular');
		var element2 = document.querySelector('.fsm');
		var existingStyles = element.getAttribute('style');
		setTimeout(function(){
			element.setAttribute('style', existingStyles + 'display:block !important; opacity: 1');
			element2.setAttribute('style', 'bottom:50px;');
		},250)
	});

</script>

