or","taxonomy":"post_tag","_links":{"self":[{"href":"https://sabor.eluniverso.com/wp-json/wp/v2/tags/187"}],"collection":[{"href":"https://sabor.eluniverso.com/wp-json/wp/v2/tags"}],"about":[{"href":"https://sabor.eluniverso.com/wp-json/wp/v2/taxonomies/post_tag"}],"wp:post_type":[{"href":"https://sabor.eluniverso.com/wp-json/wp/v2/posts?tags=187"}],"curies":[{"name":"wp","href":"https://api.w.org/{rel}","templated":true}]}},{"id":233,"link":"https://sabor.eluniverso.com/tag/gastronomia-local/","name":"GASTRONOMÍA LOCAL","slug":"gastronomia-local","taxonomy":"post_tag","_links":{"self":[{"href":"https://sabor.eluniverso.com/wp-json/wp/v2/tags/233"}],"collection":[{"href":"https://sabor.eluniverso.com/wp-json/wp/v2/tags"}],"about":[{"href":"https://sabor.eluniverso.com/wp-json/wp/v2/taxonomies/post_tag"}],"wp:post_type":[{"href":"https://sabor.eluniverso.com/wp-json/wp/v2/posts?tags=233"}],"curies":[{"name":"wp","href":"https://api.w.org/{rel}","templated":true}]}},{"id":169,"link":"https://sabor.eluniverso.com/tag/historia/","name":"HISTORIA","slug":"historia","taxonomy":"post_tag","_links":{"self":[{"href":"https://sabor.eluniverso.com/wp-json/wp/v2/tags/169"}],"collection":[{"href":"https://sabor.eluniverso.com/wp-json/wp/v2/tags"}],"about":[{"href":"https://sabor.eluniverso.com/wp-json/wp/v2/taxonomies/post_tag"}],"wp:post_type":[{"href":"https://sabor.eluniverso.com/wp-json/wp/v2/posts?tags=169"}],"curies":[{"name":"wp","href":"https://api.w.org/{rel}","templated":true}]}},{"id":229,"link":"https://sabor.eluniverso.com/tag/pasteles-de-carne/","name":"PASTELES DE CARNE","slug":"pasteles-de-carne","taxonomy":"post_tag","_links":{"self":[{"href":"https://sabor.eluniverso.com/wp-json/wp/v2/tags/229"}],"collection":[{"href":"https://sabor.eluniverso.com/wp-json/wp/v2/tags"}],"about":[{"href":"https://sabor.eluniverso.com/wp-json/wp/v2/taxonomies/post_tag"}],"wp:post_type":[{"href":"https://sabor.eluniverso.com/wp-json/wp/v2/posts?tags=229"}],"curies":[{"name":"wp","h