                            }
                        }
                    }
                }
            }
        }, true);
    });
  
    // Prebid Bidder Settings
    window.pbjs.que.push(function () {
        window.pbjs.bidderSettings = {
			criteo: {
				storageAllowed: true, 
			},
		  	adagio: {
				storageAllowed: true, 
			},
			caroda: { bidCpmAdjustment: function (bidCpm) { return bidCpm * 0.6336; } },
