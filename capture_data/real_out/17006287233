			databind : function(isInitial) {

				this.toggleLoadingComponent(true, 'bodyContainerpar_galleryphotogrid_0411fd43_323f_4879_a881_90dbab80fa63', 'loadingDivpar_galleryphotogrid_0411fd43_323f_4879_a881_90dbab80fa63', isInitial, );
				
