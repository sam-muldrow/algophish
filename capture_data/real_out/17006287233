						this.model.pages.push(i);
					}

					var controllerDivReady = setInterval(function(){
						if ($("#" + 'bodyContainerpar_molnewslisting_b7959dd3_66ea_4e50_b316_ca1b6dbaf0e2').length) {
							$("#" + 'bodyContainerpar_molnewslisting_b7959dd3_66ea_4e50_b316_ca1b6dbaf0e2').attr('data-copy-ready', '');
							clearInterval(controllerDivReady);
