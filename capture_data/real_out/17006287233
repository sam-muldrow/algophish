					this.toggleLoadingComponent(false, 'bodyContainerpar_molnewslisting_b7959dd3_66ea_4e50_b316_ca1b6dbaf0e2', 'loadingDivpar_molnewslisting_b7959dd3_66ea_4e50_b316_ca1b6dbaf0e2', isInitial, '');					
