					var controllerDivReady = setInterval(function(){
						if ($("#" + 'bodyContainerpar_galleryphotogrid_0411fd43_323f_4879_a881_90dbab80fa63').length) {
							$("#" + 'bodyContainerpar_galleryphotogrid_0411fd43_323f_4879_a881_90dbab80fa63').attr('data-copy-ready', '');
