					return;
				}

				this.isLoading = true;

				this.toggleLoadingComponent(true, 'bodyContainerpar_molnewslisting_b7959dd3_66ea_4e50_b316_ca1b6dbaf0e2', 'loadingDivpar_molnewslisting_b7959dd3_66ea_4e50_b316_ca1b6dbaf0e2', isInitial, '');
				
				let jsonUrl = this.jsonUrl;
				jsonUrl += "/" + this.limit;
				jsonUrl += "/" + this.pageSize;
				jsonUrl += "/" + pageIndex + "/news.json.html";
				
				fetch(jsonUrl)
				.then(function(response){
					return response.json();
				})
				.then((data) => {
					this.isLoading = false;
					this.pageIndex = pageIndex;
					
					if (clearData && this.model.newsitems) {
						this.model.newsitems = [];
					}
					
					if (this.model.newsitems) {
						this.model.newsitems = this.model.newsitems.concat(data.newsitems);
					} else {
						this.model = data;
					}
					
					this.model.pages = [];
