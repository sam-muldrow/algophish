  "sc": "yes"
};
oewa_data.cp = oewa_path + "/" + oewa_unique;
if (window.oewa_init) {
   window.oewa_init(oewa_data);
}
if (window.oewa_consentless_init) {
   window.oewa_consentless_init(oewa_data);
}
</script>
<!-- /OEWA -->



</body>
</html>

