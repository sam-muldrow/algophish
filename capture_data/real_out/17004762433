<!-- top40 vote start -->
<script>
function votehot(id){
    $.ajax({
		type: "GET",
		url: "addvote.php",
		data: "id="+id,
		success: function(result){
		alert("Thank you for voting.");
		$("#vote"+id).replaceWith('<span class="liked">You VOTED for this.</span>');
		return false;
		}
	});
   }
</script>
<!-- top40 vote end -->

<script>
$(document).ready(function () {
	var $container = $(".galHomeContainer").imagesLoaded( function() {
		// init
		$container.isotope({
		  // options
		  itemSelector: ".galHomeBox",
		  layoutMode: "fitRows"
		});
	});
	$(window).resize(function () {
		var $container = $(".galHomeContainer").imagesLoaded( function() {
			// init
			$container.isotope({
			  // options
			  itemSelector: ".galHomeBox",
			  layoutMode: "fitRows"
			});
		});
	});
});

</script>

<script src="//www.hirufm.lk/chat/js/cufon-yui.js"></script>
<script src="//www.hirufm.lk/chat/js/support.js"></script>

<script>
function updateShouts(){
	mydiv = document.getElementById("chatOutput");
	mydiv.scrollTop = mydiv.scrollHeight;
	
	$.ajax({
		url: "chat/ajax_feed.php",
		data: "option=chatConvos",
		cache: false,
		success: function(html){
			$("#chatResults").append(html);
			$("#chatLoading").fadeOut();
		}
	});
}

$(function() {
	setInterval("updateShouts()",30000);
});

</script>

