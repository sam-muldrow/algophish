    <script type="application/javascript">
        (function () {
            // testing css support
            var el = document.createElement('div'),
                    docEl = document.documentElement,
                    tests = {
                        'ms-flex': ['msFlex']
                    };

            function test(props) {
                var support = false;

                for (var p = 0, pLen = props.length; p < pLen; p++) {
                    if (props[p] in el.style) {
                        support = true;
                        break;
                    }
                }

                return support;
            }

            for (var module in tests) {
                if (tests.hasOwnProperty(module)) {
                    var support = test(tests[module]);
                    className = module + (support ? '--on' : '--off');
                    docEl.classList.add(className);
                }
            }
        })();
    </script>

