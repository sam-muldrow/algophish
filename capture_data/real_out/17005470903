    var initChatbox = function() {
        if (!window.embedded_svc) {
            var s = document.createElement('script');
            s.setAttribute('src', 'https://asiakastieto.my.salesforce.com/embeddedservice/5.0/esw.min.js');
            s.onload = function() {
                initESW(null);
            };
            document.body.appendChild(s);
        } else {
            initESW('https://service.force.com');
        }
    }

    function addConsentListener() {
        __tcfapi('addEventListener', 2, function(tcData, success) {
            if (success) {
                if (tcData.gdprApplies) {
                    if (tcData.eventStatus === 'tcloaded' && tcData.purpose.consents && Object.keys(tcData
                            .purpose
                            .consents).length > 0) {
                        initChatbox();
                        __tcfapi('removeEventListener', 2, () => {}, tcData.listenerId);
                    } else if (tcData.eventStatus === 'useractioncomplete' && tcData.purpose.consents && Object
                        .keys(tcData.purpose.consents).length > 0) {
                        initChatbox();
                        __tcfapi('removeEventListener', 2, () => {}, tcData.listenerId);
                    }
                }
            }
        });

    }
    var counter = 1;
    var consentLoadedTimer = window.setInterval(function() {
        if (typeof __tcfapi !== 'undefined' || __tcfapi !== null) {
            window.clearInterval(consentLoadedTimer);
            addConsentListener();
        } else if (counter >= 5) {
            window.clearInterval(consentLoadedTimer);
        }
    }, 100*counter);
</script>
