    <script type='text/javascript'>
        function initChatBoxBubble() {
            if (!document.cookie.match('(^|;) ?chatbox-bubble=([^;]*)(;|$)') &&
                window.innerWidth >= 768)
                setTimeout(() => {
                    jQuery('.chatbox-bubble').show();
                    jQuery(".chatbox-bubble").click((function() {
                            document.cookie = "chatbox-bubble=0; max-age=7776000;",
                                jQuery(".helpButton .uiButton").trigger("click"),
                                jQuery(".chatbox-bubble").remove()
                        })),
                        jQuery(".chat-bubble-close").click((function() {
                            document.cookie = "chatbox-bubble=0; max-age=7776000;",
                                jQuery(".chatbox-bubble").remove()
                        }))
                }, 3e3);
        }

        function addConsListener() {
            __tcfapi('addEventListener', 2, function(tcData, success) {
                if (success) {
                    if (tcData.gdprApplies) {
                        if (tcData.eventStatus === 'tcloaded' && tcData.purpose.consents && Object.keys(tcData
                                .purpose
                                .consents).length > 0) {
                            initChatBoxBubble();
                            __tcfapi('removeEventListener', 2, () => {}, tcData.listenerId);
                        } else if (tcData.eventStatus === 'useractioncomplete' && tcData.purpose.consents && Object
                            .keys(tcData.purpose.consents).length > 0) {
                            initChatBoxBubble();
                            __tcfapi('removeEventListener', 2, () => {}, tcData.listenerId);
                        }
                    }
                }
            });
        }

        var counter = 1;
        var consentLoadTimer = window.setInterval(function() {
            if (typeof __tcfapi !== 'undefined' || __tcfapi !== null) {
                window.clearInterval(consentLoadTimer);
                addConsListener();
            } else if (counter >= 5) {
                window.clearInterval(consentLoadedTimer);
            }
        }, 100 * counter);
    </script>
                    
