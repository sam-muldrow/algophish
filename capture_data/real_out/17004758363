<!-- yyyy.timer -->
<script
  src="//code.jquery.com/jquery-3.6.0.slim.min.js"
  integrity="sha256-u7e5khyithlIdTpu22PHhENmPcRdFiHRjhAuHcs05RI="
  crossorigin="anonymous"
></script>
<script src="//static.musiciansfriend.com/static/mf/Libs/Count-Down/v2/jquery.countdown.min.js"></script>
<script src="//static.musiciansfriend.com/static/mf/Libs/Count-Down/Moment/moment.min.js"></script>
<script src="//static.musiciansfriend.com/static/mf/Libs/Count-Down/Moment/moment-timezone.min.js"></script>
<script src="//static.musiciansfriend.com/static/mf/Libs/Count-Down/Moment/moment-timezone-with-data-2012-2022.min.js"></script>

<script>
  $(document).ready(function () {
    var pstDate = moment.tz("2023-11-24 23:59:59", "America/Los_Angeles");

    $(".js-countdown")
      .countdown(pstDate.toDate())
      .on("update.countdown", function (event) {
        // var format = '%DD : %HH : %MM : %SS';
        var format = "%-D";

        // if (event.offset.totalDays > 0) {
        //   format = '%-D day%!D ' + format;
        // }
        $(this).html(event.strftime(format));
      })
      .on("finish.countdown", function (event) {
        $(this).html("00");
        // $('#heroText').css("display", "none");
      });
  });
</script>
