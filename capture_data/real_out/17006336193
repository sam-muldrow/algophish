<script>
      function vcvSetCookie(cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays*24*60*60*1000));
        var expires = "expires="+ d.toUTCString();
                    baseDomain = 'wpbakery.com';
                document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/;domain=" + baseDomain;
      }
      function vcvGetCookie(cname) {
        var name = cname + "=";
        var decodedCookie = decodeURIComponent(document.cookie);
        var ca = decodedCookie.split(';');
        for(var i = 0; i <ca.length; i++) {
          var c = ca[i];
          while (c.charAt(0) == ' ') {
            c = c.substring(1);
          }
          if (c.indexOf(name) == 0) {
            return c.substring(name.length, c.length);
          }
        }
        return "";
      }
      document.addEventListener('DOMContentLoaded', () => {
        initVcvBanner();
      });
      function initVcvBanner() {
		if ( vcvGetCookie('vcv-gdpr-banner-hidden') ) {
		  document.querySelector('#vcv-top-banner').remove();
		}
		var vcvBannerClose = document.querySelector('#vcv-close-top-message');
		vcvBannerClose.addEventListener('click', function (e) {
			vcvSetCookie('vcv-gdpr-banner-hidden', 1, 365);
			document.querySelector('#vcv-top-banner').remove();
			e.preventDefault();
		});
	  }
    </script>
