    <script>
	var data = null;

	for(var key in serviceCategory){
			data = getServiceCategory(key);
			if(data != null){
				var serviceDiv = document.createElement('li');
				var anchortag = document.createElement('a');
				var image = document.createElement('img');
				var text = document.createElement('div');
				var hoverdiv = document.createElement('div');
				var descriptionParent = document.createElement('span');
				var description = document.createElement('p');
				
				serviceDiv.className = "servicesBlock";
				serviceDiv.id = key;
				text.className = "linkText";
				descriptionParent.className = "descrp";
				hoverdiv.id = "hov";
				description.className = "serviceDescrp";
				
				image.src='/static/start/view/img/2017landing/'+data.image;
				text.innerHTML=data.title;
				description.innerHTML = data.description;
				descriptionParent.appendChild(description);
				hoverdiv.appendChild(descriptionParent);
				hoverdiv.appendChild(image);
				anchortag.appendChild(hoverdiv);
				anchortag.appendChild(text);
				
				if(key == 'MBB'){
					anchortag.href="https://www.netzero.net"+data.uri;
					
				}
				else{
				anchortag.href="https://store.netzero.net"+data.uri;
				}
				
				serviceDiv.appendChild(anchortag);
				document.getElementById('services').appendChild(serviceDiv);
			}
		}
	</script> 
