        })
        // Shares popup
        $(document).on("click", ".ir-share-fb", function(e) {
            e.preventDefault();
            window.open($(this).attr('href'), 'fbShareWindow', 'height=450, width=550, top=' + ($(window).height() / 2 - 275) + ', left=' + ($(window).width() / 2 - 225) + ', toolbar=0, location=0, menubar=0, directories=0, scrollbars=0');
            return false;
        });
        $(document).on("click", ".ir-share-tw", function(e) {
            e.preventDefault();
            window.open($(this).attr('href'), 'twShareWindow', 'height=350, width=550, top=' + ($(window).height() / 2 - 175) + ', left=' + ($(window).width() / 2 - 225) + ', toolbar=0, location=0, menubar=0, directories=0, scrollbars=0');
            return false;
        });
        $(document).on("click", ".ir-share-tg", function(e) {
            e.preventDefault();
            window.open($(this).attr('href'), 'tgShareWindow', 'height=350, width=550, top=' + ($(window).height() / 2 - 175) + ', left=' + ($(window).width() / 2 - 225) + ', toolbar=0, location=0, menubar=0, directories=0, scrollbars=0');
            return false;
        });

        $(document).on("click", "a.FlvipScrollNextPostItem", function(e) {
            e.preventDefault();
            var url = this.getAttribute('href');
            gtag('event', 'Переход скролл', {
                event_category: 'Читайте также',
                event_action: 'click',
                event_callback: function() {
                    window.location = url;
                }
