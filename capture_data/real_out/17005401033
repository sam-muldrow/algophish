<script>
    jQuery(document).ready(function($) {
        var push_notificationBlock = jQuery('#push_notificationBlock');
        push_notificationBlock.hide()

        push_notificationBlock.find('.rightBlk').on('click', function() {
            window.open(push_notificationBlock.data('link'));
            push_notificationBlock.slideUp(300);
        });

        push_notificationBlock.find('.close').click(function() {
            push_notificationBlock.remove();
            jQuery.cookie('notification2_removed', 1);
        })

        if (!jQuery.cookie('notification2')) {
            jQuery.cookie('notification2', 1, {
                expires: 3,
                path: '/'
            });
        } else {
            var notification2 = parseInt(jQuery.cookie('notification2'));
            jQuery.cookie('notification2', notification2 + 1, {
                expires: 3,
                path: '/'
            });
        }
        // if(navigator.userAgent.match(/chrome/i))
        if (!jQuery.cookie('notification2') || (jQuery.cookie('notification2') < 3 && !jQuery.cookie('notification2_removed'))) {
            push_notificationBlock.show();
        }
    });
</script>
