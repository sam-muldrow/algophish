<script>
        jQuery(document).ready(function($) {
            function getCookie(name) {
                const value = `; ${document.cookie}`;
                const parts = value.split(`; ${name}=`);
                if (parts.length === 2) return parts.pop().split(';').shift();
            }

            if (document.cookie.indexOf('ir_lang') !== -1 && getCookie('ir_lang') === 'ua' && window.location.href.split('?')[0] === 'https://inforesist.org/') {
                window.location.href = "https://inforesist.org/ua/"
            }
            if (document.cookie.indexOf('ir_lang_popup_shown') == -1 && $('html').attr('lang') == 'ru-RU') {
                $('#js-lang-panel').addClass('is--open')
                $('#lang-modal').show()
            }

            $(document).on('click', '.flap__btn', function(e) {
                e.preventDefault()
                let days = 31
                let date = new Date(Date.now() + days * 86400e3)
                date = date.toUTCString()
                document.cookie = "ir_lang_popup_shown=1; expires=" + date
                if ($(this).attr('href') == 'https://inforesist.org/') {
                    document.cookie = "ir_lang=ru; expires=" + date
                    $('#lang-modal').hide()
                    window.location.href = $(this).attr('href')
                } else {
                    document.cookie = "ir_lang=ua; expires=" + date
                    window.location.href = $(this).attr('href')
                }
            })
        })
    </script>
