		SETUPS.set('nemo.fefa.scOneFootnoteJsonUrl', '');
					SETUPS.set('navigationServiceOptions', '{"schema":"oneshopmapping.schema.json","pages":{"product-details":{"featureApps":{"details":"audi:oneshop-frontend-product-detail"},"path":"/de/brand/de/shop/produktdetails.html"},"product-list":{"featureApps":{"details":"audi:oneshop-frontend-product-list"},"path":"/de/brand/de/shop.html"},"checkout":{"featureApps":{"details":"audi:oneshop-frontend-checkout"},"path":"/de/brand/de/shop/checkout.html"},"cart":{"featureApps":{"details":"audi:oneshop-frontend-cart"},"path":"/de/brand/de/shop/warenkorb.html"}},"primaryConsumerId":"audi:integrator:csr"}');
					SETUPS.set('gfaServiceConfig', '{}');
				SETUPS.set('nemo.audicode.rescue.messages', {"NEW_MODELYEAR": "Engine","BASE_MODEL_CHANGE": "Engine","EXTCOLOR_CHANGED": "Exterior colour","TRIMLINE_CHANGED_EXTENSION": "Equipment","INTCOLOR_CHANGED": "Equipment","OPTION_DROPPED": "Equipment","OPTION_DROPPED_DUE_TO_CONFLICT": "Equipment","OPTION_ADDED_DUE_TO_CONFLICT": "Equipment","AOZ_CHANGED": "Audi Genuine Accessory","AOZ_DROPPED": "Audi Genuine Accessory","MODEL_CHANGED_PRICE": "Price","OPTION_CHANGED_PRICE": "Price","CARLINE_NAME_CHANGE": "Price","EXTCOLOR_CHANGED_PRICE": "Price","AOZ_PRICE_CHANGED": "Price",});
				SETUPS.set('nemo.FefaFeatureAppVersion', 'https://fa-footnote-engine.cdn.prod.arcade.apps.one.audi/v2.2.2/fh/app.js');
				SETUPS.set('scopes.nemo.SHOW_EU_TYRE_LABEL', false);
	
	SETUPS.set('nemo.market.homepage', '/us/web/en');
</script>
