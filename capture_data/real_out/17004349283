<script>
;(() => {
  const buy_plus = document.getElementById('buy_plus');
  if (buy_plus) {
    buy_plus.addEventListener('click', () => {
      if (window.longboat && window.longboatVersion === 1) {
        window.longboat.queue.push({
          data:{topmenu: 'koeb-plus'},
          event: 'topmenu',
          once: false,
        });
      } else {
        ebTracking.queryBuilder('topmenu', {topmenu: 'koeb-plus'});
      }
    });
  }

  const buy_paper = document.getElementById('buy_paper');
  if (buy_paper) {
    buy_paper.addEventListener('click', () => {
      if (window.longboat && window.longboatVersion === 1) {
        window.longboat.queue.push({
          data:{topmenu: 'koeb-avisen'},
          event: 'topmenu',
          once: false,
        });
      } else {
        ebTracking.queryBuilder('topmenu', {topmenu: 'koeb-avisen'});
      }
    });
  }

  const buy_epaper = document.getElementById('buy_epaper');
  if (buy_epaper) {
    buy_paper.addEventListener('click', () => {
      if (window.longboat && window.longboatVersion === 1) {
        window.longboat.queue.push({
          data:{topmenu: 'koeb-e-avisen'},
          event: 'topmenu',
          once: false,
        });
      } else {
        ebTracking.queryBuilder('topmenu', {topmenu: 'koeb-e-avisen'});
      }
    });
  }

})();
</script>
</div>
