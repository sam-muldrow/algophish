<script>
  ;(() => {
    const tipssheet = document.getElementById('tipssheet');
    const listItems = tipssheet.querySelectorAll('.card');

    listItems.forEach((listItem) => {
      listItem.addEventListener('click', async (ev) => {
        const article = listItem.href;
        if (await window.eb.ready('ebTracking')) {
          window.ebTracking.queryBuilder('lists', {
            cmsaid: '',
            ld: '',
            ln: 'tipsbladet_tip',
          });
        }
      });
    });
  })();
</script>
