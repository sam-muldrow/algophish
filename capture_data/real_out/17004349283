<script>
  (() => {
    const notificationNewsOptions = {
      device: "desktop",
      environment: "PRODUCTION",
    }
    const notificationTarget = document.getElementById('notification-news');
    const menuItems = document.querySelectorAll('.navmenu-container .menu-item');
    if (window.navigationmenu) {
      window.navigationmenu.init(notificationNewsOptions, notificationTarget, menuItems, false, 'desktop');
    }
  })()
</script>
