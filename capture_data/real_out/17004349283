                  <use xlink:href="#cog-solid"></use>
                </svg>
              </span>
            </div>
          <div id="ebbanner_monster5" class="eb-placement">
          </div>
        </div>
        <script>
          ;(async () => {
            if (await window.eb.ready('ebBanners')) {
              window.ebComponents.ebBanners.addPlacement('monster5');
            }
          })();
        </script>
