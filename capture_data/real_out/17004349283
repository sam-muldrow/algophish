<script>
(function() {
  let cacheBuster = "";

  try {
    const regexpCachebuster = new RegExp(/\.css\?v\=(\d+\.\d+)/, "gm");
    const match = regexpCachebuster.exec(document.head.innerHTML);

    if (!match.length) {
      throw "Cannot find cachebuster string";
    }
    cacheBuster = match[1]
  } catch (error) {
    console.error(error);
  }

  const link = document.createElement("link");
  const script = document.createElement("script");

  link.href = "/assets/svelte/videouniverse.css?v=t" + cacheBuster;
  link.type = "text/css";
  link.rel = "stylesheet";

  script.src = "/assets/svelte/videouniverse.js?v=t" + cacheBuster;

  document.head.appendChild(link);
  document.head.appendChild(script);

  const videouniverseOptions = {
    pageType: "teaser",
    listId: 9394752,
    targetId: "videouniverse-teaser"
  }

  script.addEventListener("load", () => {
    videouniverse.teaserList(videouniverseOptions);
  })
})();
</script></div>
