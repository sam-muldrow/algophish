<script>
    events = ""; localeParam = ""; pageLinkType = "0"; ashuraTitle = 'Day of Ashura 2017, 10 Muharram 1439';
    ramadanTitle = 'Ramadan 2017 - Special Month of Fasting'; laylatulqadrTitle = "Laylatul Qadr – Blessed Nights of Ramadan";
    eidulfitrTitle = 'Eid al Fitr – Ramadan Festival'; var monthNamesArray = [ "Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec" ];
    $('[accordian-target-1]').click(function() { var clikedA = $(this).attr('accordian-target-1'); $('[accordian-toggle-1="' + clikedA + '"]').slideToggle(); $('[accordian-target-1="' + clikedA + '"]').toggleClass('active'); });
    $(window).resize(function() { if ($(window).width() < 768) { $('[accordian-toggle-1]').addClass('accordian'); $('[accordian-target-1]').removeClass('hide');
    } else { $('[accordian-toggle-1]').removeClass('accordian'); $('[accordian-target-1]').addClass('hide'); } });
    jQuery(document).ready(function() {
            row = $(".upcoming-event-row"); $(".upcoming-event-row").remove(); $("#upcoming-event").html(row);
            if ($(window).width() < 768) { $('[accordian-toggle-1]').addClass('accordian'); $('[accordian-target-1]').removeClass('hide');
            } else { $('[accordian-toggle-1]').removeClass('accordian'); $('[accordian-target-1]').addClass('hide'); }

            $(".ico-du").click(function() { if ($(".ico-du").hasClass("fa-angle-down")) { $("#acc").removeClass("accordian"); $(".ico-du").removeClass("fa-angle-down"); $(".ico-du").addClass("fa-angle-up"); } else { $("#acc").addClass("accordian"); $(".ico-du").addClass("fa-angle-down"); $(".ico-du").removeClass("fa-angle-up"); } });
    });
</script>
