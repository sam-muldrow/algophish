<script>
    var url = window.location.pathname; var origin = window.location.origin + "/quran/";
    function scrollToTop() { $('html, body').animate({ scrollTop : 0 }, 800); }

     $(document).ready(function() {
        $('[accordian-target]').click(function() { var clikedA = $(this).attr('accordian-target'); console.log(clikedA);
        $('[accordian-toggle="' + clikedA + '"]').slideToggle(); $('[accordian-target="' + clikedA + '"]').toggleClass('active'); });

        if ($(window).width() < 768) { $('[accordian-toggle]').addClass('accordian'); $('[accordian-target]').removeClass('hide');
        } else { $('[accordian-toggle]').removeClass('accordian'); $('[accordian-target]').addClass('hide'); }

        $(window).resize(function() { if ($(window).width() < 768) { $('[accordian-toggle]').addClass('accordian'); $('[accordian-target]').removeClass('hide');
        } else { $('[accordian-toggle]').removeClass('accordian'); $('[accordian-target]').addClass('hide'); } });
    });

    $(document).ready(function() {
            setTimeout(function() { $('html, body').animate({ scrollTop : $(this).scrollTop() - 52 }, 800); }, 50);

            $('.dua-menu a[href="' + url + '"]').addClass('active'); $('.dua-sub-category').html($('.dua-menu a.active').text());

            var elem = new Foundation.Equalizer(jQuery('#eq-row')); elem.applyHeight();
            $('.dua-menu a').on('click', function(e) { if ($('.toggle-menu').hasClass('active')) { e.preventDefault(); $('.dua-menu a').removeClass('active'); $(this).addClass('active'); 
            $('.dua-sub-category').html($(this).text()); } }); $('.toggle-menu').on('click', function(e) { e.preventDefault(); $('.dua-menu').slideToggle(); $('.dua-sub-category').slideToggle(); });
            $('.dua-sub-category').on('click', function(e) { $(this).slideUp(); $('.dua-menu').slideToggle(); });
    });

    function copyDua(source) { var clipboard = $('#clipboard'); var arabic = $(source).closest(".dua").find(".arabic").text();
        var transliteration = $(source).closest(".dua").find(".transliteration").text();
        var translation = $(source).closest(".dua").find(".translation").text();
        var reference = $(source).closest(".dua").find(".reference").text();
        var duaUrl = window.location.href + "#" + $(source).closest(".dua").attr("id");
        clipboard.html(''); clipboard.append($.trim(arabic) + '&#13;&#10;' + '&#13;&#10;').append( $.trim(transliteration) + '&#13;&#10;' + '&#13;&#10;').append( $.trim(translation) + '&#13;&#10;' + '&#13;&#10;').append( $.trim(reference) + '&#13;&#10;' + '&#13;&#10;').append( $.trim(duaUrl));
        clipboard.select(); var successful = document.execCommand('copy'); var msg = successful ? 'Text copied to cliboard' : 'Error copying to clipboard'; showToast('Copied to clipboard'); }

    $(window).scroll(function() { var position = $(this).scrollTop(); $('.dua').each(function() {var target = $(this).offset().top;
    if (position >= target - 150) { if (!$(this).hasClass('active')) { $('.dua').removeClass('active'); $(this).addClass('active'); } } else { $(this).removeClass('active'); } }); });

    var addthis_config = { services_exclude : 'print, printfriendly' };
    var addthis_share = { url : "", title : "Dua by IslamicFinder", description : "Dua by IslamicFinder" };
</script>
