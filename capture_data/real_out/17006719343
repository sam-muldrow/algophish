	<script>
	function check_testimonials()
	{
		if(window.jQuery){
			jQuery('.testimonial .play').each(function(i,element){
				href=jQuery(element).attr('href');
				if(href=="#" || href==""){
					jQuery(element).css('visibility','hidden');
				}
			});
		}else{
			setTimeout(function(){check_testimonials();}, 50);
		}
	}
	check_testimonials();
	</script>
	<!--- END HOMEPAGE TESTIMONIALS -->
    

