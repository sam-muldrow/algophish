<script>
(function() {
  var api_pressrelease = '/newsroom/pressrelease/api/v2/top_list.html'
  _ajax(api_pressrelease)
    .done(function(response) {
      if (_isResult(response)) {
        $('#js-custom-news-pressreleaseList').html(response);
      }
    });
  function _ajax(url) {
    return $.ajax({
        url: url,
        type: 'GET',
        dataType: 'html',
        cache: false
      })
  }
  function _isResult(response) {
    return String(response).indexOf('<!-- ok -->') > -1;
  }
})();
</script>

