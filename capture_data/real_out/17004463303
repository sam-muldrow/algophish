    function wait(time) {
        const MAX_TIME = 5000;
        const INTERVAL = 100;

        const timeout = setTimeout(function() {
            if (!getMyCookie('_fbp') || !getMyCookie('_fbc') || !getQS('bdk')) {
                let timePassed = time + INTERVAL;
                if (timePassed >= MAX_TIME) {
                    clearTimeout(timeout);
                    return initAssertive();
                }
                // else
                return wait(timePassed);
            }
            // else
            clearTimeout(timeout);
            return initAssertive();
        }, INTERVAL);
    }

	const ayConfigWasSet = new Event('ayConfigWasSet');
  
    if (getQS("as").includes("6da")) {
        if (getQS('fbclid')) {
            wait(0);
        } else {
            const timeout = setInterval(function() {
                if (!getQS("bdk")) { return; }
                clearInterval(timeout);
                return initAssertive();
            }, 100);
        }
    }else {
        //all traffic

          var checkExist = setInterval(function() {
          if (getQS("bdk") !== '') {
           clearInterval(checkExist);
           initAssertive();
          }
         }, 100);

    }

