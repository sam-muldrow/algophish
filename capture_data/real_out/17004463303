</script><script name="script-group-widget-html-3-head-1" type="text/javascript">
  function extractIIQ_ID(json) {
  try {
    const eids = json.eids;
    for (let i = 0; i < eids.length; i++) {
      const eid = eids[i];
      if (eid.source === "intentiq.com") {
        const uids = eid.uids;
        for (let j = 0; j < uids.length; j++) {
          const uid = uids[j];
          if (uid.ext.stype === "ppuid") {
            return uid.id;
          }
        }
      }
    }
    return null; // Return null if no matching "id" value is found
  } catch (e) {
    return null;
  }
}

const getBiddingPlatformID = (bidderCode) => {
  switch (bidderCode) {
    // Amazon platform
    case "amazon":
      return 2;
    // Google patform
    case "adx":
      return 3;
    // Prebid platform
    default:
      return 1;
  }
};

let isPrebidInited = false;
const setAssertive = () => {
  window.assertive.analytics.custom.custom_8 = "intentIq";
};
const iiqCallbackMethod = () => {
  if (isPrebidInited === true) return;
  isPrebidInited = true;

  if (window.assertive) {
    setAssertive();
  } else {
    document.addEventListener("ayConfigWasSet", setAssertive);
  }
};

const maximumPrebidDelayTimeout = 2000;
const initIntentIq = () => {
  if (!IntentIqObject) {
    console.error("IntentIQ loader not found... exiting!");
    return;
  }

  var intentIq_928146862 = new IntentIqObject({
    partner: 928146862,
    pbjs,
    callback: iiqCallbackMethod,
    timeoutInMillis: maximumPrebidDelayTimeout,
    ABTestingConfigurationSource: "percentage",
    abPercentage: 95,
    manualWinReportEnabled: true,
    domainName: window.location.host,
    browserBlackList: "Chrome",
  });
  
  //console.log("IntentIQ object is created ", intentIq_928146862, 'pbjs', pbjs);

  // ADX-PPID
  const ppid = extractIIQ_ID(intentIq_928146862.getIntentIqData());
  googletag.pubads().setPublisherProvidedId(ppid);
  // ADX-PPID end

  // GAM integration
  try {
    googletag.cmd.push(function () {
      googletag
        .pubads()
        .setTargeting(
          "intent_iq_group",
          intentIq_928146862.intentIqConfig.abTesting.currentTestGroup || "U"
        );
    });
  } catch (e) {
    googletag.cmd.push(function () {
      googletag.pubads().setTargeting("intent_iq_group", "N");
    });
  }
  // GAM integration end

  // RTPO
  const reportToIntentIQ = (e) => {
    if (!e?.data?.meta) return;
    // We are not reporting video, CR and PV without ADS
    const mediaType = e.data.meta.mediaType;
    if (mediaType !== null && mediaType !== 'banner') return;

    let bidderCode = "amazon";
    const dfp = e.data.meta.dfpResponseInformation;
    if (dfp && dfp.isBackfill) {
      bidderCode = "adx";
    }
    if (e.data.meta.preBidWon) {
      bidderCode = e.data.meta.highestPreBid_partner;
    }
    const biddingPlatformId = getBiddingPlatformID(bidderCode);

    const reportData = {
      biddingPlatformId,
      bidderCode,
      prebidAuctionId: biddingPlatformId === 1 ? e.data.meta.auctionId : undefined,
      cpm: e.data.predictedRevenueCPM.impression,
      currency: "USD",
      placementId: e.data.meta.slotId,
    };

    console.log("[IntentIQ script] predicted revenue report", reportData.bidderCode, reportData, e);
    intentIq_928146862.reportExternalWin(reportData);
  };

  window.addEventListener("assertive_predictedRevenue", reportToIntentIQ);
  // RTPO end
};

window.addEventListener("intentIQ", initIntentIq, { once: true });
</script><script>
          (function(domain) {
            
  const getParamFromUrl = ({ name }) => {
    if (typeof window !== 'undefined') {
      const url = new window.URL(window.location.href);
      const params = url.searchParams;
      return params.get(name);
    } else {
      return '';
    }
  };

  const getThisCookie = cname => {
    var name = cname + '=';
    var decodedCookie = decodeURIComponent(document.cookie);
    var ca = decodedCookie.split(';');
    for (var i = 0; i < ca.length; i++) {
      var c = ca[i];
      while (c.charAt(0) == ' ') {
        c = c.substring(1);
      }
      if (c.indexOf(name) == 0) {
        return c.substring(name.length, c.length);
      }
    }
    return '';
  };

  const generateSessionId = () => {
    let uuid = '',
      ii;

    for (ii = 0; ii < 32; ii += 1) {
      switch (ii) {
        case 8:
        case 20:
          uuid += '-';
          uuid += ((Math.random() * 16) | 0).toString(16);
          break;
        case 12:
          uuid += '-';
          uuid += '4';
          break;
        case 16:
          uuid += '-';
          uuid += ((Math.random() * 4) | 8).toString(16);
          break;
        default:
          uuid += ((Math.random() * 16) | 0).toString(16);
      }
    }
    return uuid;
  };


  const setSessionIdCookieAndGlobal = sessionId => {
    window.clientSessionId = sessionId;
    document.cookie = 'clientSessionId=' + sessionId + '; path=/';
  };

  const toMillis = min => min * 60 * 1000;

  let clientSessionId = null;
  let startTime = Date.now();
  const qaTimeout = getParamFromUrl({ name: 'stout' });

  // If we have existing session for this domain we use the same session
  if (sessionStorage.domain === domain) {
    clientSessionId = sessionStorage.getItem('clientSessionId');
    Boolean(qaTimeout) && console.log('SESSION GET FROM LAST SESSION -- POST FROM SAME DOMAIN', clientSessionId);
  } else {
    // Set current session domain
    sessionStorage.setItem('domain', domain);
  }

  if (!clientSessionId) {
    clientSessionId = generateSessionId();
    sessionStorage.setItem('clientSessionId', clientSessionId);
  }

  setSessionIdCookieAndGlobal(clientSessionId);

  Boolean(qaTimeout) && console.log('INIT SESSION ID IN SESSION STORAGE', clientSessionId);

  // Method to get cookie value from document.cookie string;
  window.getThisCookie = getThisCookie;


  const userActiveEventListener = () => {
    window.removeEventListener('click', userActiveEventListener);
    window.removeEventListener('scroll', userActiveEventListener);
  };

  const handleElapsedSessionTimeout = () => {
    startTime = Date.now();
    clientSessionId = generateSessionId();
    sessionStorage.setItem('clientSessionId', clientSessionId);
    setSessionIdCookieAndGlobal(clientSessionId);

    Boolean(qaTimeout) && console.log('UPDATE SESSION ID IN SESSION STORAGE', clientSessionId);

    window.addEventListener('click', userActiveEventListener);
    window.addEventListener('scroll', userActiveEventListener);
  }

  // Default session timout is 30 minutes
  const sessionTimeout = Number(qaTimeout) || 30;
  let remainingTimeout = Number(sessionStorage.getItem('remainingSessionTimeout')) || sessionTimeout;

  Boolean(qaTimeout) && console.log('SESSION - REMAINING TIMEOUT IN MINUTES:', remainingTimeout, ' FROM ',  sessionTimeout);

  setTimeout(() => {
    handleElapsedSessionTimeout();
    remainingTimeout = sessionTimeout;

    // Session timeout passed, so we need to create a new sessionId
    setInterval(() => {
      handleElapsedSessionTimeout();
    }, toMillis(sessionTimeout));

  }, toMillis(remainingTimeout));

  // Subscribe to unload event to set the remaining session timeout
  window.addEventListener('beforeunload', () => {
    const endTime = Date.now();

    const remainingTime = remainingTimeout - (endTime-startTime) / 60 / 1000;
    if (remainingTime > 0.1) {
      sessionStorage.setItem('remainingSessionTimeout', remainingTime);
    } else {
      sessionStorage.removeItem('clientSessionId');
      sessionStorage.removeItem('remainingSessionTimeout');
    }
  });

          })('boredomtherapy.com');
