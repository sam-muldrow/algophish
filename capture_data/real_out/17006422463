$(document).ready(function (){
		loadRecentPosts();
		$(".cta-download-btn").click(function () {
			pats('lp_click', 'download');
		});		
	})	

	function loadRecentPosts() {		
		$.ajax({
			url: "https://www.ytddownloader.com/blog/wp-json/wp/v2/posts/?page=1&per_page=6&order=desc",
			type: "GET",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            cache: false,
			success: function(result){								
				for (var i = 0; i < result.length; i++) {
			  		$(".recentposts").append('<div class="recentpost"><div class="recentp-title"><a target="_blank" href="' + result[i].link + '">' + result[i].title.rendered + '</a></div><div class="recentp-content">' + result[i].excerpt.rendered + '</div><div class="recentp-link"><a target="_blank" href="' + result[i].link + '" title="' + result[i].title.rendered + '">Read post &raquo;</a></div></div>');
		  		}
			},
			complete: function(done) {},
			error: function(err) {console.log(err)}}
		)
	}	

	window.flow = "index-default";
    window.cc = "US";

	
	let starDate = Date.UTC(2021, 10, 01);	
	let td = new Date();
	let todayDate = Date.UTC(td.getFullYear(), td.getMonth(), td.getDate());	
	const oneDay = 1000 * 60 * 60 * 24;	
	const diffInTime = todayDate - starDate;	
	let diffInDays = Math.round(diffInTime / oneDay);
	var initialnumber = 1121000000 + (diffInDays * 17000);
	
	let alrky = getCookie("cntr");	
	if(alrky != '') {
		initialnumber = Number(alrky);
	}
    incrementNo();
    function incrementNo() {
        initialnumber += Math.floor(Math.random() * 3) + 1;
        let arr = Array.from(String(initialnumber), Number);
        $('.d1 span').html(arr[0]);
        $('.d2 span').html(arr[1]);
        $('.d3 span').html(arr[2]);
        $('.d4 span').html(arr[3]);
        $('.d5 span').html(arr[4]);
        $('.d6 span').html(arr[5]);
        $('.d7 span').html(arr[6]);
        $('.d8 span').html(arr[7]);
        $('.d9 span').html(arr[8]);		
		$('.d10 span').html(arr[9]);
        document.cookie = 'cntr=' + initialnumber.toString() + '; SameSite=None; Secure';		
    }
    setInterval(function () {
    	incrementNo();
    }, 1000);
	
//Offer countdown script
    var remaining = 35999;
    var x = setInterval(function() {
        remaining--;
        var days = Math.floor( remaining / ( 60 * 60 * 24));
        var hours = Math.floor((remaining % ( 60 * 60 * 24)) / ( 60 * 60));
        var minutes = Math.floor((remaining % ( 60 * 60)) / ( 60));
		var seconds = Math.floor((remaining % 60) / 1);
		document.getElementById('countdowndays').innerHTML =  ('0' + days).slice(-2);
		document.getElementById('countdownhours').innerHTML = ('0' + hours).slice(-2);
		document.getElementById('countdownminutes').innerHTML = ('0' + minutes).slice(-2);
		document.getElementById('countdownseconds').innerHTML = ('0' + seconds).slice(-2);
				if (remaining > 0 ) {
			document.getElementById('plantimer').style = 'display:block';
		}
		else {
			clearInterval(x);
			document.getElementById('plantimer').style = 'display:none';		
		}
				}, 1000);

		function hidePopup(target) {
			if (document.getElementById(target) != null) {
				document.getElementById(target).style.display = 'none';
			}
		}
		function showPopup(target) {
			if (document.getElementById(target) != null) {
				document.getElementById(target).style.display = 'block';
									}
		}
