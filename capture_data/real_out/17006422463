<script type="text/javascript" data-categories="essential">
	let slideIndex = 0;
	let slides = document.getElementsByClassName("socialmedia-carousel-item");
	let dots = document.getElementsByClassName("dot");
	let timeoutHandler = setInterval(showSlides, 5000);

showSlides();

function showSlides(index = null) {
	
	let i;

  	for (i = 0; i < slides.length; i++) {
    	slides[i].style.display = "none";  
  	}

  	if (index != null) {				
		clearTimeout(timeoutHandler);
		slideIndex = index;
		if (slideIndex > slides.length) {slideIndex = 1}    
  		for (i = 0; i < dots.length; i++) {
    		dots[i].className = dots[i].className.replace(" active", "");
  		}
  		slides[slideIndex-1].style.display = "block";  
  		dots[slideIndex-1].className += " active";
  		timeoutHandler = setInterval(showSlides, 5000);

  } else {
  	slideIndex++;
  	if (slideIndex > slides.length) {slideIndex = 1}    
  	for (i = 0; i < dots.length; i++) {
    	dots[i].className = dots[i].className.replace(" active", "");
  	}
  	slides[slideIndex-1].style.display = "block";  
  	dots[slideIndex-1].className += " active";  	
  }    
}

const set = document.querySelector('.all-dots');
set.addEventListener('click', handleClick, false);

function handleClick(e) {  
  if (e.target.matches('span')) {	
	showSlides(e.target.getAttribute("name"))	   
  }
}

