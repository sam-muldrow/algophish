      <script>

     	//FC dark mode
		(function () {
			//cookie shared same cookie on domain name ".caradidiac.com" (https://www.caradisiac.com/, https://forum-auto.caradisiac.com/)
			var cookieDomainName = '.caradisiac.com';
			loadFcDarkMode();
		})();     
		function loadFcDarkMode() {
			var fcDarkMode = getCookie('fc_dark_mode');
			var el = document.querySelector('.ipsApp');
            if (fcDarkMode == 'on') {
            	el.classList.add('isDarkMode');
            } else {
            	el.classList.remove('isDarkMode');
            }
		}
        
		//Set Cookies
		function setCookie(cname, cvalue, exdays, cookieDomainName = null) {
			var d = new Date();
			d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
			var expires = "expires=" + d.toUTCString();
			var domain = cookieDomainName != null ? ";domain=" + cookieDomainName : '';
			document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/" + domain + ';secure';
		}
		
        //Get Cookies
		function getCookie(cname) {
			var name = cname + "=";
			var decodedCookie = decodeURIComponent(document.cookie);
			var ca = decodedCookie.split(';');
			for (var i = 0; i < ca.length; i++) {
				var c = ca[i];
              
				while (c.charAt(0) == ' ') {
					c = c.substring(1);
				}

				if (c.indexOf(name) == 0) {
					return c.substring(name.length, c.length);
				}
			}
			return "";
		}
        
        // Variable pour le target (N ou S) des liens du megamenu
		var megamenuTarget_xiti = 'N';
      </script>
      
