  } else if (simpleURL.match('login')){
    x10 = 'Profils_login';
    x20 = 'Profils_login';
    x16 = 'Profils_login';
    x19 = 'privé';
    xtn2 = '87';
  } else if (simpleURL.match('register')){
    x10 = 'Profils_inscription';
    x20 = 'Profils_inscription';
    x16 = 'Profils_inscription';
    x19 = 'privé';
    xtn2 = '87';
  }
}

//Permet de ne pas afficher la pub video sur les pages d'inscrition et de login
if (x16=='Profils_login' || x16=='Profils_inscription'){
	$('.habillage2').css( "display", "none" );
}
  
//affectation de la valeur de la N2 en fonction du type de forum affiché, si on est sur le front du forum
if (xtn2=='') {
  var testTypeN2 = '';
  if (testTypeN2=='Marques' || testLevel0JSON=='Marques'){xtn2 = 82;}
  else if (testTypeN2=='Technique' || testLevel0JSON=='Technique'){xtn2 = 83;}
  else if (testTypeN2=='Pratique' || testLevel0JSON=='Pratique'){xtn2 = 84;}
  else if (testTypeN2=='Collection' || testLevel0JSON=='Collection'){xtn2 = 85;}
  else if (testTypeN2=='Motos' || testLevel0JSON=='Motos'){xtn2 = 86;}
  else if (testTypeN2=='Tuning' || testLevel0JSON=='Tuning'){xtn2 = 88;}
  else if (testTypeN2=='Les Clubs' || testLevel0JSON=='Les Clubs'){xtn2 = 89;}
  else if (testTypeN2=='Scooters' || testLevel0JSON=='Scooters'){xtn2 = 90;}
  else if (testTypeN2==''){xtn2 = 91;} //Home
  else if (testTypeN2=='Sport' || testLevel0JSON=='Sport'){xtn2 = 92;}
  else if (testTypeN2=='Bateaux' || testLevel0JSON=='Bateaux'){xtn2 = 93;}
  else if (testTypeN2=='saxovts' || testLevel0JSON=='saxovts'){xtn2 = 96;}
  else if (testTypeN2=='Les marques vous répondent' || testLevel0JSON=='Les marques vous répondent'){xtn2 = 97;}
  else if (testTypeN2=='ClubHome' || testLevel0JSON=='ClubHome'){xtn2 = 98;}
  else if (testTypeN2=='Aide' || testLevel0JSON=='Aide'){xtn2 = 99;}
  else if (testTypeN2=='ClubBlog' || testLevel0JSON=='ClubBlog'){xtn2 = 100;}
  else if (testTypeN2=='ClubPage404' || testLevel0JSON=='ClubPage404'){xtn2 = 101;}
  else if (testTypeN2=='Index' || testLevel0JSON=='Index'){xtn2 = 102;}
  else if (testTypeN2=='Loisirs' || testLevel0JSON=='Loisirs'){xtn2 = 103;}
  else if (testTypeN2=='ClubGroupe' || testLevel0JSON=='ClubGroupe'){xtn2 = 104;}
  else if (testTypeN2=='Peugeot205' || testLevel0JSON=='Peugeot205'){xtn2 = 105;}
  else if (testTypeN2=='SalonAuto' || testLevel0JSON=='SalonAuto'){xtn2 = 107;}
  else{xtn2 = 106;} //Non classés
}
  
//Mise en forme des variables à envoyer
var page = {
  name     : xtTraiter(x10),
  chapter1 : '',
  chapter2 : '',
  chapter3 : '',
  level2   : xtn2, //a verifier quand on aura les vrais libeles du forum
};

// Identification du user connecte
var identifiedVisitor = {
  id       : user_id,
  category : 1
};

// Données custom (x12 et autres)
var customVars = {
  site: {
    1   : '['+x1+']',
    10 	: '['+x10+']',
    20 	: '['+x20+']',
    21 	: '['+x21+']',
    12 	: '['+x12+']',
    14 	: '['+x14+']',
    15 	: '['+x15+']',
    16 	: '['+x16+']',
    17 	: '['+x17+']',
    18 	: '['+x18+']',
    19 	: '['+x19+']',
    
  }
};

//atPrivacy pour exempt au 1er hit - CARA-7082
var atPrivacy = lit_cook('atPrivacy');
//console.log('ICIPRIV:'+atPrivacy);
if (atPrivacy === '' || atPrivacy === 'exempt') {    
	window.ATTag.privacy.setVisitorMode('cnil', 'exempt');
	if (atPrivacy === '') {
		setCook('atPrivacy', 'exempt', 182, '/');
	}
}
if (atPrivacy === 'optout') {
    window.ATTag.privacy.setVisitorOptout();
}

// Envoi des données de la page
ATTag.page.set(page);
ATTag.customVars.set(customVars);
//ATTag.tags.set(keywords);
if ( user_id != '' ) {
	ATTag.identifiedVisitor.set(identifiedVisitor);
} else {
	ATTag.identifiedVisitor.unset();
}

// Envoi des données
ATTag.dispatch();
// Fin données xiti concernant la page
  
/* Fix to scroll navigation mobile */
if (screen && screen.width < 768 && ajoutSujet != 'add') {
    function fixNav() {
        var cache2 = $('.mobileNav');
        if (stopScrollHiding == true) {
          return
        }
        if ($(window).scrollTop() > 100) {
            cache2.addClass('stickyNav2');
          	$('.stickyNav2').slideDown('slow'); 
        }
        else {
          	$('.stickyNav2').slideUp('slow');
            cache2.removeClass('stickyNav2');
        }
    }
    $(window).scroll(fixNav);
    //fixNav();
}
  
/* Bouton scroll to top */
$(window).scroll(function() {
    var height = $(window).scrollTop();
	if (screen && screen.width < 768) {
      if(height  > 100) {
          $('.cara_scrollTo_top').show();
      }else{
          $('.cara_scrollTo_top').hide();
      }
    }else{
    	if(height  > 300) {
          $('.cara_scrollTo_top').show();
      }else{
          $('.cara_scrollTo_top').hide();
      }
    }
});

$('.cara_scrollTo_top').bind("click", function () {
  $('html, body').animate({ scrollTop: 0 }, 1200);
  return false;
});

/* Masquage nav forum mobile au scroll down - FC-529 */
if (screen && screen.width < 992 && ajoutSujet != 'add') {
  var lastScrollTop = 0;
  var stopScrollHiding = false;
  $(window).scroll(function() {
    var st = $(this).scrollTop();
    if (stopScrollHiding == true) {
      return
    }
    if (st > lastScrollTop){
        $('.mobileList').hide();
    } else {
       $('.mobileList').slideDown('fast');
    }
    lastScrollTop = st;
  });
}
  
/* Masquage des 2 navs sur mobile sur les formulaires de création de sujet - FC-529 */
if (screen && screen.width < 992 && ajoutSujet == 'add') {
	$('.mobileList').slideUp('slow');
	$('.stickyNav2').slideUp('slow'); 
}
  
/* Masquage des 2 navs sur mobile lors d'une réponse à un sujet ou d'un edition - FC-529 & FC-535 */
if (screen && screen.width < 992 && ajoutSujet != 'add') {
	$('[data-role="editorComposer"]').click(function() {
		$('.mobileList').slideUp('slow');
		$('.stickyNav2').slideUp('slow');
      //Variable servant a arreter les scripts d'affichage des menus au scroll
      	stopScrollHiding = true;
    });
  	$('[data-action="editComment"]').click(function() {
		$('.mobileList').slideUp('slow');
		$('.stickyNav2').slideUp('slow');
      //Variable servant a arreter les scripts d'affichage des menus au scroll
      	stopScrollHiding = true;
      //apres 3s (et l'ajout du formulaire d'edition) au clic sur un bouton, on reactive les menus au scroll
      	setTimeout(function(){
        	$('.ipsButton').click(function() {
                stopScrollHiding = false;
            });
        }, 3000);
    });
  	//Au clic sur le submit de la reponse, on reactive les menus au scroll
  	$('#replyArea button[type="submit"]').click(function() {
      	stopScrollHiding = false;
    });
}
  
/* Ancre fluide lien repondre */
$('a[href^="#replyArea"]').click(function(){
	var replyArea = $(this).attr("href");
	if (replyArea === '#') {
		return;
	}
	$('html, body').animate({
		scrollTop:$(replyArea).offset().top -100
	}, 'slow');
  	$( ".ipsComposeArea_dummy" ).click();
	return false;
});
  
/* Liste depliable - FC-288 */
$(document).ready(function () {

  if (x16=='ListeDesSujetsCat' && $(window).width()<='992'){
    $('*[id*=JSblocMarker]:visible').each(function() {
        if ($(this).height() > '110' ){
            $(this).next('.readMoreContainerCateg').show();
            $(this).addClass('bigListCateg'); 
        }
    });
  }

  $('.readMore').click(function(){
    $(this).parent().prev('.ipsDataList').toggleClass('bigListCateg');
    if($(this).parent().hasClass('checked')) {
      $(this).parent().removeClass('checked');
    } else {
      $(this).parent().addClass('checked');
    }       
  });

});
  
/* Tooltip - FC-436 */
  $(document).ready(function() {
    var testBreadcrumb = false;
    $('.ipsBreadcrumb_top .cara-breadcrumbList li').each(function(index) {
      	var breadcrumbList = $( this ).text();
      	breadcrumbList = $.trim(breadcrumbList);
      	if(breadcrumbList == 'Mécanique / Électronique'){
        	 testBreadcrumb = true;
        }
	})
 	if (testBreadcrumb == true) {
      $(".cara-tooltipBtn").hover(function() {
        $(".cara-tooltiptext").toggle("fast");
      });
      $( ".cara-field input" ).focus(function() {
        $(".cara-tooltiptext").show("fast");
      });
      $( ".cara-field input" ).focusout(function() {
        $(".cara-tooltiptext").hide("fast");
      });
      $( ".cara-field input" ).keyup(function() {
        $(".cara-tooltiptext").hide("fast");
      });
     }else{
     	$(".cara-tooltipBtn").css("display", "none");
     }
   });
  
  /* Tooltip 2 - FC-510 */
  $(document).ready(function() {

      $(".cara-tooltipBtn2").hover(function() {
        $(".cara-tooltiptext2").toggle("fast");
      });
    
      var textAreaFocusAlreadyChanged = false;
      setTimeout(textAreaFocus, 1000, textAreaFocusAlreadyChanged);
   });
  
  function textAreaFocus(textAreaFocusAlreadyChanged){ /* Fonction vérifiant que l'editeur (asynchrone) a été injecté dans le bloc de la page */ 
    $("#topic_content_editor").bind("DOMSubtreeModified", function() {
      if(textAreaFocusAlreadyChanged !== true){
        textAreaFocusAlreadyChanged = true;

        $( ".cke_wysiwyg_div").focus(function() {
          $(".cara-tooltiptext2").show("fast");
        });
        $( ".cke_wysiwyg_div").focusout(function() {
          $(".cara-tooltiptext2").hide("fast");
        });
        $( ".cke_wysiwyg_div" ).keyup(function() {
          $(".cara-tooltiptext2").hide("fast");
        });
      }
    });
  }
</script>

