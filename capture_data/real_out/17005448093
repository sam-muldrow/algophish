<script type="text/javascript" src="https://static.caradisiac.com/javascript/forum/tc_ForumAuto_3.js?v=20221212b"></script>
<script type="text/javascript">
/* Fonctions d'ecriture & lecture de cookie */
function setCook(name, value, expires, path, domain, secure) {
	var exdate = new Date();
    exdate.setDate(exdate.getDate()+expires);
    //var myReg = new RegExp(/\.caradisiac\.[a-z]{3}/);
    //var domain = myReg.exec(window.location);
    host = location.host;
    if (host.split('.').length === 1)
          {
             // no "." in a domain - it's localhost or something similar
             domain='';
          }
          else
          {

             domainParts = host.split('.');
             domainParts.shift();
             domain = '.'+domainParts.join('.');
    }
     document.cookie = name + '=' + escape(value)
		+ ((expires) ? '; expires=' + exdate.toGMTString() : '')
		+ ((path) ? '; path=' + path : '; path=/')
		+ ((domain.length) ? '; domain=' + domain : '')
		+ ((secure) ? '; secure' : '');

}

function lit_cook(name) {
    var deb,fin;
    deb = document.cookie.indexOf(name + "=");
    if (deb >= 0) {
       	deb += name.length + 1;
       	fin = document.cookie.indexOf(";",deb);
       	if (fin < 0) {
       		fin = document.cookie.length;
       	}
		return unescape(document.cookie.substring(deb,fin));
    }
    return "";
}  

function getCurrentHost() {
  var currentHost = window.location.hostname;
  return currentHost;
}
function getCookieDomainWWW() {
  var cookieDomain = getCurrentHost();
  if (!cookieDomain || !cookieDomain.match(/www/)) {
    cookieDomain = 'www.caradisiac.com';
  }
  return cookieDomain;
}
</script>
