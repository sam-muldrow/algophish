  	var sNew = document.createElement("script");
	sNew.async = true;
	sNew.src = "https://static.caradisiac.com/javascript/forum/sticky.js?v=20220914";
	var s0 = document.getElementsByTagName('script')[0];
	s0.parentNode.insertBefore(sNew, s0);
}
  
/* Injection des adsense entre les posts - FC-423*/ 
  $(function(){
    var adsenseContent='<div class="ipsComment"><div class="ipsBox"><scr'+
        'ipt src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></scr'+
        'ipt><ins class="adsbygoogle" style="display:block" data-ad-client="ca-pub-2451155287677968" data-ad-slot="1807547185" data-ad-format="auto"></ins><scr'+
        'ipt>(adsbygoogle = window.adsbygoogle || []).push({});</scr'+
        'ipt></div></div>';
    console.log('adsenseBlacklist :', adsenseBlacklist);
    if (adsenseBlacklist == 'false'){
      $('.adsenseContainer1').append(adsenseContent);
      $('.adsenseContainer2').append(adsenseContent);
    }
  });
</script>

