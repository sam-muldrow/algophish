 <!-- Hotjar - nb de visites -->
function getCookieHotjar(cname) {
  var name = cname + "=";
  var decodedCookie = decodeURIComponent(document.cookie);
  var ca = decodedCookie.split(';');
  for (var i = 0; i < ca.length; i++) {
    var c = ca[i];
    while (c.charAt(0) == ' ') {
      c = c.substring(1);
    }
    if (c.indexOf(name) == 0) {
      return c.substring(name.length, c.length);
    }
  }
  return "";
}
   var hotjar_number_visits = getCookieHotjar('hotjar_number_visits');
   var pageVues = 1;
   if(hotjar_number_visits > 1) {
     pageVues = Number(hotjar_number_visits);
   }
 
