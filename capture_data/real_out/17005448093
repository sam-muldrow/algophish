	<script type="text/javascript">
      	var quicktestURL = window.location.href.replace('https://forum-auto.caradisiac.com/',''); 
		if (!(quicktestURL.match('register'))&&!(quicktestURL.match('login'))&&!(quicktestURL.match('lostpassword'))){
      		document.write("<aside id='Habillage2' class='habillage2 containeraPepette' data-emplacement='Interstitiel'></aside>");
        }
	</script>


 <script type="text/javascript">
    // Fonction de conversion du tracking via xtcore en tracking smartTag
    function xt_click(element, c, xtn2, label, typeClick) {
      var type = typeClick == 'N' ? 'navigation' : typeClick == 'S' ? 'exit' : typeClick;

      if (typeClick == 'A') {
        // Pour le moment on bloque les "anciens" click action sur cara à cause de certains effets de bord
        // Pour tracker un click action il faut utiliser 'action' et plus 'A'
      } else {
        var xiti_chapters = label.split('::');
        ATTag.click.send({
          elem:element, 
          name:xiti_chapters.pop(), 
          chapter1:xiti_chapters[0], 
          chapter2:xiti_chapters[1], 
          chapter3:xiti_chapters[2], 
          level2:xtn2, 
          type:type
        });
      }
    }
   
   //affichage de la recherche sur mobile
   if (screen && screen.width < 768) {
      	const searchBtn = document.getElementById("elMyCaraForumSearch");
      	const target = document.getElementsByClassName("mobSearch")[0];
      	
      	searchBtn.addEventListener("click", (event) => {
          
          //console.log(searchBtn.dataset.clicked);
          if(searchBtn.dataset.clicked=='false'){
	         searchBtn.setAttribute("data-clicked", "true");
             setTimeout(() => {
             	const child = document.getElementById("elMyCaraForumSearch_menu");             	             	
               	child.classList.add("caraForumSearchMob");
	            target.appendChild(child);
               
              	document.getElementById("elMyCaraForumSearch_menu").style.display = "none";
               	document.getElementsByClassName("caraForumSearchMob")[0].style.display = "block";
            }, 10);
	      }else{            
            searchBtn.setAttribute("data-clicked", "false");
            
            document.getElementsByClassName("caraForumSearchMob")[0].style.display = "none";
            
           	const child = document.getElementById("elMyCaraForumSearch_menu");
            child.classList.remove("caraForumSearchMob");
	      }
		});      	
    }
</script>


		
