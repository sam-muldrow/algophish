//CMP source Point  
  $(function(){
    $.ajax({
      url: 'https://cdn.privacy-mgmt.com/unified/wrapperMessagingWithoutDetection.js',
      dataType: "script",
      async: true
    });
    var dismissCMPCookie= getCookie("cw");
    window._sp_queue = [];
    window._sp_ = {
      config: {
        accountId: 1348,
        baseEndpoint: 'https://cdn.privacy-mgmt.com',
        gdpr:{
          	targetingParams:{cookiew:dismissCMPCookie},
        },
        events: {
          onMessageChoiceSelect: function (placeholder,choice_id, choice_type_id) {
            console.log('onMessageChoiceSelect choice_id: ', choice_id);
            console.log('onMessageChoiceSelect choice_type_id: ', choice_type_id);
            //cookie de session si clic sur 'continuer sans accepter'
            if (choice_type_id == 15){
              document.cookie = "cw=dismissed;domain=.caradisiac.com;path=/;";
            }
          },
          onPrivacyManagerAction: function (message_type, pmData) {
            //console.log('onPrivacyManagerAction message_type:', message_type);
            //console.log('onPrivacyManagerAction', pmData)
            if(pmData.purposeConsent ==  "none" && pmData.vendorConsent == "none") {
              document.cookie = "cw=dismissed;domain=.caradisiac.com;path=/;";
            }
          },
          onConsentReady : function(){
            // Lancement pub Appnexus
            try {
              throwAppNexus();
            } catch(e){
              console.log('Error : lib is not ready', e);
            }
            // Lancement de Numerly/MilleMercis
            window.__tcfapi('addEventListener', 2, window.tcfApiEventListenerForMMTRO);
          }
        },

      }
    }

  });

  
</script>

