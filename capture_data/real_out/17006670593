    <script>
  // PARDOT FORM HANDLER
  const getTrackingInfo = () => {
    const params = {};
    params["queryString"] = window.location.search.substring(1);
    params["webFormUrl"] = window.location.href.split(/[?#]/)[0];
    // hiddenCountryField
    params["countryCode"] = 'US';
    // Currency Dropdown
    params["localizedCurrency"] = 'USD';

    return params;
  };

  const sendPostMessage = (message) => {
    if (document.getElementById("iframe")) {
      document
        .getElementById("iframe")
        .contentWindow.postMessage(message, "https://pypd.paypal-mktg.com");
    }

    if (document.getElementById("iframe1")) {
      document
        .getElementById("iframe1")
        .contentWindow.postMessage(message, "https://pypd.paypal-mktg.com");
    }
  };

  window.addEventListener("message", (event) => {
    const { data, origin } = event;
    if (origin === "https://pypd.paypal-mktg.com") {
      if (data.frameHeight) {
        if (document.getElementById("iframe")) {
          document.getElementById("iframe").style.height = `${data.frameHeight}px`;
        }
        if (document.getElementById("iframe1")) {
          document.getElementById("iframe1").style.height = `${data.frameHeight}px`;
        }
        sendPostMessage(getTrackingInfo());
      }
    }
  });
</script>

