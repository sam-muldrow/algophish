  <script>
  function LocaleRedirect(){
    // Remove any lingering locales saved in local storage
    window.localStorage.removeItem('locale');
    var storedLocale = Cookies.get('locale');

    var targetLocale = window.location.href.match(/locale=(.{2})/) ? window.location.href.match(/locale=(.{2})/)[1] : storedLocale;
    var validLocale = (window.braintreeConstants.supportedLocales.indexOf(targetLocale) !== -1);
    if (!validLocale){
      //Invalid locale
      return;
    }
    if (targetLocale) {
      Cookies.set('locale', targetLocale, {expires: 7});
    }

    var storedLocale = Cookies.get('locale');
    var currentLocale = 'us';

    function localizedPathFor(locale) {
      var path = window.location.pathname.slice(1);
      var segments = path.split('/');
      var nonLocalizedSegments = segments.filter(function (segment) { return !segment.match(/^\w{2}$/); });
      var nonLocalizedPath = nonLocalizedSegments.join('/');
      var localizedSegments = [];

      if (locale !== 'us') {
        localizedSegments.push(locale);
      }

      if (nonLocalizedPath && window.braintreeConstants.supportedLocales.indexOf(storedLocale) !== -1) {
        localizedSegments.push(nonLocalizedPath);
      }

      return '/' + localizedSegments.join('/');
    }

    if (storedLocale && storedLocale !== currentLocale && true) {
      path = localizedPathFor(storedLocale);

      if (window.location.search) {
        path += window.location.search;
      }

      if ('referrer' in document && document.referrer && !document.referrer.match(/braintreepayments\.com/)) {
        path += window.location.search ? '&' : '?';
        path += 'referrer=' + encodeURIComponent(document.referrer);
      }

      if (window.location.hash) {
        path += window.location.hash;
      }

      window.location = path
    }
  }
  LocaleRedirect();
</script>

