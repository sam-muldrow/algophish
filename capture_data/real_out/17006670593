<script>
    const mediaQuery = window.matchMedia("(prefers-reduced-motion: reduce)");
    const vidButton = document.getElementById("vid-button");
    const vid = document.getElementById("vid");

    if(mediaQuery.matches) {
        vid.removeAttribute("autoplay");
    }

    mediaQuery.addListener(function handleReduceMotionChanged() {
        (mediaQuery.matches) ? vid.pause() : vid.play();
    }); // Note: https://webkit.org/b/168491

    document.addEventListener("keydown", function(e) {
        if(e.keyCode === 27) {
            vid.pause();

            if (vidButton.className !== "play") {
                vidButton.className = "play";
            }
        }
    });

  if (vidButton) {
    vidButton.addEventListener("click", function(e) {
          const el = e.target;

      if (el.classList.contains("play")) {
        el.classList.remove("play");
        vid.play();
      } else {
        el.classList.add("play");
        vid.pause();
      }
    });
  }
</script>
