	<script type='text/javascript'>
	var commonSrc = "https://i2.plug.it/wp/siviaggia/js/common.js?,iolam,lazyload,pink";

	function downloadJSAtOnload(src, callback) {
		var element = document.createElement("script");
		element.src = src;
		document.body.appendChild(element);
		element.onload = function() {
			callback();
			if(typeof(runLazyLoad) == 'function') {
				runLazyLoad();
			}
			if(typeof(runPgCall) == 'function') {
				runPgCall();
			}
		}
	}
	if (window.addEventListener) {
		window.addEventListener("load", downloadJSAtOnload(commonSrc,function(){}), false);
	} else if (window.attachEvent) {
		window.attachEvent("onload", downloadJSAtOnload(commonSrc,function(){}));
	} else {
		window.onload = downloadJSAtOnload(commonSrc,function(){});
	}
	</script>
