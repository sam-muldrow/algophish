   <script type="text/javascript">
   (function() {
	  var cwv_site = "dilei", cwv_page;
	  try { var x = iolam_com_ns.split("/"); cwv_page = x[3]; }catch(e){}
	  var iol_track = function(l,v,atr){
		  if(typeof atr === 'undefined') atr = "";
		  if(!cwv_page) cwv_page = 'unknown';
		  if(typeof v === 'undefined') return;
		  var u ="https://evnt.iol.it/v2?&pgnf="+cwv_site+"%2C"+cwv_page+"%2C%2C%2C%2C0%2C1192%2C0%2C%2C0%2C0%2C"+atr+"%2C"+l+"%2C"+v+"&nc="+Date.now();
		  var i = new Image();
		  i.src = u;
	  }
	  var cls_trk = false, lcp_trk = false, fid_trk = false, cwv_once = true, lcp_data = false, ttfb_trk = false, fcp_trk = false;
	  var ct = document.createElement('script');
	  ct.src = 'https://unpkg.com/web-vitals/dist/web-vitals.iife.js';
	  ct.type = 'text/javascript';
	  ct.onload = function(){
 	  	  var adv_sizes = [];
		  var check_adv_size = function(frcd){
			  var sizeWP = 0, sizeMPU = 0, mobile = (window.innerWidth < 768);
			  if (document.getElementById('page').offsetTop) sizeWP = document.getElementById('page').offsetTop;
			  if (sizeWP>30 && !mobile) sizeWP-=30;
			  if (mobile) sizeWP-=10;
			  var o = document.getElementById((mobile ? 'admputop2' : 'admputop'));
			  if (o) if (o.clientHeight) sizeMPU = o.clientHeight;
			  if ((sizeMPU > 310)&&(sizeWP > 90)) adv_sizes['wp600']=1;
			  else {
				  if (sizeWP > 90) adv_sizes['wp']=1;
				  if (sizeMPU > 310) adv_sizes['h600']=1;
			  }
			  if (document.getElementById('adv_left_div')) adv_sizes['skin']=1;
			  if (document.getElementById('eyeDiv')) adv_sizes['eyediv']=1;
			  if (document.getElementById('ayads-html')) adv_sizes['ayads']=1;
			  if (document.getElementById('ayads-video')) adv_sizes['ayadsvideo']=1;
			  if (typeof frcd === 'undefined') setTimeout(check_adv_size, 8000);
		  }
		  setTimeout(check_adv_size, 2000);
		  webVitals.getCLS(function(d){ cls_trk = Math.floor(d.value * 1000); });
		  webVitals.getLCP(function(d){ lcp_trk = Math.floor(d.value); lcp_data = d; });
		  webVitals.getTTFB(function(d){ ttfb_trk = Math.floor(d.value); });
		  if (document.addEventListener) document.addEventListener("visibilitychange", function() {
			  if (document.visibilityState === 'hidden') {
				  if (!cwv_once) return;
				  cwv_once = false;
				  if (!(cls_trk === false)) {
					  var labs = '';
						  try {
								check_adv_size(true);
								if (adv_sizes['eyediv']) labs = 'eyediv';
								else if (adv_sizes['ayadsvideo']) labs = 'ayadsvideo';
								else if (adv_sizes['ayads']) labs = 'ayads';
								else if (adv_sizes['wp600']) labs = 'masthead_600';
								else if (adv_sizes['wp']) labs = 'masthead';
								else if (adv_sizes['h600']) labs = '300x600';
								else if (adv_sizes['skin']) labs = 'skin';
								else labs = 'none';
						 } catch(e){ labs = ''; }
					 iol_track("CLS", cls_trk, labs);
				  }
				  if (!(lcp_trk === false)) {
					  var lcp_org = '', oo = '';
					  try {
						  if (lcp_data.entries) if (lcp_data.entries[0]) {
							  if (lcp_data.entries.length>1) {
								  lcp_org = "MULTI";
							  } else {
								  var o = lcp_data.entries[0].element;
								  if (o.nodeName) oo += o.nodeName;
								  else  oo += "NONE";
								  if (o.classList) if (o.classList[0])  oo += encodeURIComponent("." + o.classList[0]);
								  if (o.id) oo += encodeURIComponent("#" + o.id);
								  lcp_org = oo;
							  }
						  }
					  } catch(e) {
						  lcp_org = '';
					  }
					  iol_track("LCP", lcp_trk, lcp_org);
				  }
				  if (!(ttfb_trk === false)) iol_track("TTFB", ttfb_trk);
			  }
		  });
	  }
	  setTimeout(function(){
		  var s = document.getElementsByTagName('script')[0];
		  s.parentNode.insertBefore(ct, s);
	  }, 5000);
	  	/* ADV Size Tracking */
		var justOne = true;
		var track_ads_size = function(){
			try {
				if (!justOne) return;
				justOne = false;
				var sizeWP = 0, sizeMPU = 0, mobile = (window.innerWidth < 768);
				if (document.getElementById('page').offsetTop) sizeWP = document.getElementById('page').offsetTop;
				if (sizeWP>30 && !mobile) sizeWP-=30;
				var o = document.getElementById((mobile ? 'admputop2' : 'admputop'));
				if (o) if (o.clientHeight) sizeMPU = o.clientHeight;
				iol_track("WPSize", sizeWP);
				iol_track("MPUSize", sizeMPU);
			} catch(e){}
		}
		if (document.addEventListener) document.addEventListener("visibilitychange", function() {
			if (document.visibilityState === 'hidden') {
				track_ads_size();
			}
		});
   })();
   </script>
