  //イベントトラッキング送信(action=hrefの不要なパラメータ除去したもの, label=コンテンツ名）
  function sendEvent(action, label) {
      if (action && label) {
          var myPath = document.location.pathname;
          gtag('event', action, {
              'event_category': myPath,
              'event_label': label
          });
      }
  }

  /**
   * ポイント数計測用 イベントトラッキング送信
   * path   : 掲載ページURLのルート相対パス
   * label  : コンテンツ名
   * value  : ポイント数
   */
  function sendEventValue(path, label, value) {
      if (path && label) {
          var action = document.location.href.replace('https://dietnavi.com', '');
          gtag('event', action, {
              'event_category': path,
              'event_label': label,
              'value': value
          });
      }
  }
</script>
