        <script type="text/javascript">
            (function(){
                var $category4 = $('.slider_category4');
                var $category4_pager = $('.category4_pager');
                var activeClass = 'active-slide';
                $category4.bxSlider({
                  slideWidth: 235,
                  minSlides: 4,
                  maxSlides: 4,
                  moveSlides: 4,
                  auto: true,
                  autoHover: true,
                  pause: 5500,
                  speed: 1200,
                          pager: true,
                  pagerCustom: '.category4_pager',
                  touchEnabled:false,
                  onSliderLoad: function() { $category4_pager.children('li').eq(0).addClass(activeClass);},
                  onSlideAfter: function( $slideElement, oldIndex, newIndex) {
                  $category4_pager.children('li').removeClass(activeClass).eq(newIndex + 0).addClass(activeClass);
                  $category4.startAuto();
                  }
                });
            })();
        </script>
