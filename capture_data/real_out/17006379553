         // TODO : 텐텐세일 기간에만 아이콘 노출
        function tentenSaleEmblem() {
            const today = new Date();
            const startDate = new Date( 2023, 10, 6, 10);
            const endDate = new Date( 2023, 10, 15, 23,59,59);
            let iconEmblem = document.getElementsByClassName('tentensale-emblem');

            for(let i = 0; i < 15; i++) {
                if (today > startDate && today < endDate) {
                    iconEmblem[i].style.display = 'block';
                } else {
                    iconEmblem[i].style.display = 'none';
                }
            }

        }
        tentenSaleEmblem();
	</script>
