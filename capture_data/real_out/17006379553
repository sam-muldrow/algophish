							} else if(dv==""&&md=="U") {
								document.searchForm.sTtxt.value="띠부씰 포토후기 이벤트! 잊지 말아요!";
								document.searchForm.extUrl.value = "/shopping/category_prd.asp?itemid=5274320";
							}
						}
