<script>
  $('form#exitOverlayForm').on('submit', function(){
    var emptyForm = false;
    var email = $('#email').val();
    $('#locality').val(email);
    $('form#exitOverlayForm input').each(function(){
      if($(this).val() == ''){
        emptyForm = true;
      }
    });
    if (emptyForm) {
      return false;
    }
    //$('form#exitOverlayForm').submit();
  });
</script>
