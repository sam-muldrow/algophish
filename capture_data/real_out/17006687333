<script>
	$(document).on('click', '.menu_item', function () {
		var verticalSlug = $(this).data('vertical-slug');
		$('.nav > li > a').removeAttrs('class');
		$('#menu-vertical-' + verticalSlug).addClass('active');
		$('.tab-content').find('.active').removeClass('active');
		$('#categories-'+verticalSlug).addClass('active');
	});
</script>
