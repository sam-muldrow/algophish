<script>

	// GA4 :: add_to_wishlist
	function dataLayerAddToWishlist(params) {
		$(function(){
			var objDealOptions = $('.stepSpinnerDealOption');
			var arrItems = [];
			var currency = 'AED';
			var totalPrice = 0;

			// Fetch Data Loop
			objDealOptions.each(function(i, obj) {
				if (i == 0) {
					arrItems.push({
						item_id: $(obj).attr('data-id').toString(),
						item_name: $(obj).attr('data-slug') + '-' + $(obj).attr('data-id'),
						affiliation: "Cobone Web Store",
						discount: parseInt($(obj).attr('data-oldprice')) - parseFloat($(obj).attr('data-price')),
						index: i,
						item_brand: $(obj).attr('data-brand'),
						item_category: $(obj).attr('data-category'),
						location_id: $(obj).attr('data-city'),
						price: parseFloat($(obj).attr('data-price')),
						quantity: 1
					});
					currency = $(obj).attr('data-currency');
					totalPrice = totalPrice + parseFloat($(obj).attr('data-price'));
				}
				else {
					return;
				}
			});

			// Push Data
			if (typeof arrItems[0] !== 'undefined') {
				dataLayer.push({
					event: "add_to_wishlist",
					ecommerce: {
						currency: currency,
						value: totalPrice,
						items: arrItems
					}
				});
			}
		});
	}

	// GA4 :: begin_checkout
	function dataLayerBeginCheckout() {
		$(function(){
			var lang = $('#cartCityLang').data('cart-lang');
			var citySlug = $('#cartCityLang').data('cart-city');
			var cartName = 'cb-' + citySlug + '-' + lang + '-' + 'cart';
			var items = JSON.parse(Cobone.store.getItem(cartName));
			var arrItems = [];
			var currency = 'AED';
			var totalPrice = 0;
			var promoCodeApplied = '';

			// Check
			if ($('.js-promocode-applied').length) {
				promoCodeApplied = $('.js-promocode-applied').attr('data-promocode-applied');
			}

			// Fetch Data Loop
			for (var i=0; i<items.items.length; i++) {
				var item = items.items[i];
				arrItems.push({
					item_id: item.id.toString(),
					item_name: item.slug + '-' + item.id.toString(),
					affiliation: "Cobone Web Store",
					coupon: promoCodeApplied,
					discount: parseFloat(item.oldprice) - parseFloat(item.price),
					index: i,
					item_brand: item.brand,
					item_category: item.category,
					location_id: item.location,
					price: parseFloat(item.price),
					quantity: item.qty
				});
				currency = item.currency;
				totalPrice = totalPrice + parseFloat(item.price) * item.qty;
			}

			// Push Data
			if (typeof arrItems[0] !== 'undefined') {
				dataLayer.push({
					event: "begin_checkout",
					ecommerce: {
						currency: currency,
						value: totalPrice,
						coupon: promoCodeApplied,
						items: arrItems
					}
				});
			}
		});
	}

	// GA4 :: view_cart
	function dataLayerViewCart() {
		$(function(){
			var lang = $('#cartCityLang').data('cart-lang');
			var citySlug = $('#cartCityLang').data('cart-city');
			var cartName = 'cb-' + citySlug + '-' + lang + '-' + 'cart';
			var items = JSON.parse(Cobone.store.getItem(cartName));
			var arrItems = [];
			var currency = 'AED';
			var totalPrice = 0;

			// Fetch Data
			for (var i=0; i<items.items.length; i++) {
				var item = items.items[i];
				arrItems.push({
					item_id: item.id.toString(),
					item_name: item.slug + '-' + item.id.toString(),
					affiliation: "Cobone Web Store",
					discount: parseInt(item.oldprice) - parseFloat(item.price),
					index: i,
					item_brand: item.brand,
					item_category: item.category,
					location_id: item.location,
					price: parseFloat(item.price),
					quantity: item.qty
				});
				currency = item.currency;
				totalPrice = totalPrice + parseFloat(item.price) * item.qty;
			}

			// Push Data
			if (typeof arrItems[0] !== 'undefined') {
				dataLayer.push({
					event: "view_cart",
					ecommerce: {
						currency: currency,
						value: totalPrice,
						items: arrItems
					}
				});
			}
		});
	}

	// GA4 :: add_to_cart
	// params = Object();
	// params.currency = currency;
	// params.price = price;
	// params.dealId = dealId;
	// params.dealTitle = dealTitle;
	// params.discount = oldprice;
	// params.brand = brand;
	// params.category = category;
	// params.location = location;
	// params.quantity = quantity;
	function dataLayerAddToCart(params) {
		dataLayer.push({
			event: "add_to_cart",
			ecommerce: {
				currency: params.currency,
				value: parseFloat(params.price) * parseInt(params.quantity),
				items: [
					{
						item_id: params.dealId.toString(),
						item_name: params.slug + '-' + params.dealId,
						affiliation: "Cobone Web Store",
						discount: parseInt(params.discount) - parseFloat(params.price),
						index: 0,
						item_brand: params.brand,
						item_category: params.category,
						location_id: params.location,
						price: parseFloat(params.price),
						quantity: parseInt(params.quantity)
					}
				]
			}
		});
	}

	// GA4 :: remove_from_cart
	// params = Object();
	// params.currency = currency;
	// params.price = price;
	// params.dealId = dealId;
	// params.dealTitle = dealTitle;
	// params.discount = oldprice;
	// params.brand = brand;
	// params.category = category;
	// params.location = location;
	// params.quantity = quantity;
	function dataLayerRemoveFromCart(params) {
		dataLayer.push({
			event: "remove_from_cart",
			ecommerce: {
				currency: params.currency,
				value: parseFloat(params.price) * parseInt(params.quantity),
				items: [
					{
						item_id: params.dealId.toString(),
						item_name: params.slug + '-' + params.dealId,
						affiliation: "Cobone Web Store",
						discount: parseInt(params.discount) - parseFloat(params.price),
						index: 0,
						item_brand: params.brand,
						item_category: params.category,
						location_id: params.location,
						price: parseFloat(params.price),
						quantity: parseInt(params.quantity)
					}
				]
			}
		});
	}
	function dataLayerRemoveFromCartPage(obj) {
		var lang = $('#cartCityLang').data('cart-lang');
		var citySlug = $('#cartCityLang').data('cart-city');
		var cartName = 'cb-' + citySlug + '-' + lang + '-' + 'cart';
		var items = JSON.parse(Cobone.store.getItem(cartName));
		var category = '';

		// Fetch Data from Localhost
		for (var i=0; i<items.items.length; i++) {
			var item = items.items[i];
			if (parseInt(item.id) == parseInt(obj.attr('data-deal-id'))) {
				category = item.category;
				break;
			}
		}

		// GA4 Push
		dataLayer.push({ ecommerce: null });  // Clear the previous ecommerce object.
		dataLayer.push({
			event: "remove_from_cart",
			ecommerce: {
				currency: obj.attr('data-currency'),
				value: parseFloat(obj.attr('data-price')) * parseInt(obj.attr('data-quantity')),
				items: [
					{
						item_id: obj.attr('data-deal-id').toString(),
						item_name: obj.attr('data-slug') + '-' + obj.attr('data-deal-id'),
						affiliation: "Cobone Web Store",
						discount: parseFloat(obj.attr('data-old-price')) - parseFloat(obj.attr('data-price')),
						index: 0,
						item_brand: obj.attr('data-brand'),
						item_category: category,
						location_id: citySlug,
						price: parseFloat(obj.attr('data-price')),
						quantity: parseInt(obj.attr('data-quantity'))
					}
				]
			}
		});
	}

	// GA4 :: view_item
	function dataLayerViewItem() {
		$(function(){
			var lang = $('#cartCityLang').data('cart-lang');
			var citySlug = $('#cartCityLang').data('cart-city');
			var arrItems = [];
			var currency = 'AED';
			var totalPrice = 0;

			// Fetch Data
			$('.stepSpinnerDealOption').each(function(i, obj) {
				if (i == 0) {
					arrItems.push({
						item_id: $(obj).attr('data-id'),
						item_name: $(obj).attr('data-slug') + '-' + $(obj).attr('data-id'),
						affiliation: "Cobone Web Store",
						discount: parseFloat($(obj).attr('data-oldprice')) - parseFloat($(obj).attr('data-price')),
						index: i,
						item_brand: $(obj).attr('data-brand'),
						item_category: $(obj).attr('data-category'),
						location_id: citySlug,
						price: parseFloat($(obj).attr('data-price')),
						quantity: 1
					});
					currency = $(obj).attr('data-currency');
					totalPrice = totalPrice + parseFloat($(obj).attr('data-price')) * 1;
				}
				else {
					return;
				}
			});

			// Push Data
			if (typeof arrItems[0] !== 'undefined') {
				dataLayer.push({
					event: "view_item",
					ecommerce: {
						currency: currency,
						value: totalPrice,
						items: arrItems
					}
				});
			}
		});
	}


	// Old Functions
	function addToCartSynerise(dealId, name, price, saleprice, image, url, shortdescription, brand, category, qty, currency, language, city)
	{
			dataLayer.push({
					'event': 'addToCart',
					'ecommerce': {
							'currencyCode': '' + currency + '',
							'language':'' + language + '',
							'add': {
									'products': [{
											'name': '' + name + '',
											'id': '' + dealId + '',
											'price': '' + saleprice + '',
											'brand': '' + brand + '',
											'category': '' + category + '',
											'shortdescription': '' + shortdescription + '',
											'image':'' + image + '',
											'url':'' + url + '',
											'quantity': qty,
											'location':'' + city + '',
											'language':'' + language + ''
									}]
							}
					}
			});


	}

	function removeFromCartSynerise(dealId, name, price, saleprice, image, url, shortdescription, brand, category, qty, currency, language, city)
	{
			dataLayer.push({
					'event': 'removeFromCart',
					'ecommerce': {
							'currencyCode': '' + currency + '',
							'language':'' + language + '',
							'remove': {
									'products': [{
											'name': '' + name + '',
											'id': '' + dealId + '',
											'price': '' + saleprice + '',
											'brand': '' + brand + '',
											'category': '' + category + '',
											'shortdescription': '' + shortdescription + '',
											'image':'' + image + '',
											'url':'' + url + '',
											'quantity': qty,
											'location':'' + city + '',
											'language':'' + language + ''
									}]
							}
					}
			});

	}
</script>
