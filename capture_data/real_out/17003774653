function checkForm(obj,wd){
var oks=$(obj).parent();
var input=$(".inpTxt",obj);
if($(oks).hasClass('ks-btn')){
$(oks).removeClass('ks-btn');
$(input).show();
$(input).animate({'width' : wd},300);
input.focus();
return false;
}
else{
$(oks).addClass('ks-btn');
$(input).animate({'width' : 0},300,function(){
$(this).hide();
});
return true;
}
}
window.checkForm=checkForm;
