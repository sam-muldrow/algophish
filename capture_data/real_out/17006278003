$(document).ready(function(){
	var bannerNum = $('#bannerArea ul#bannerSlide li').length;//件数
	var dNum = 3;//表示の数
	var bPa=dNum-(bannerNum%dNum) + 1;//最後が半個なので+1
	for (i=bPa; 0<i; i--){
		$('#bannerArea ul#bannerSlide').append('<li>&nbsp;</li>');
	}
	var bannerNumPage = bannerNum / dNum - 1;//ボタンでめくりたい数
	//
	var bannerHei = bannerNum * 57;
	if (bannerHei<194){bannerHei=194;}
	$('#bannerArea ul#bannerSlide').css({'height':bannerHei+'px'});//縦幅
	slideChange('#obt #bannerSlide',171,'#obt #btnBnUp a','#obt #btnBnDown a','top',bannerNumPage);//実行
});

$(document).ready(function(){
	$('#newsBox dl a').hover(function(){
		$(this).parent().prev().css('backgroundPosition','8.5em bottom');
	},function(){
		$(this).parent().prev().css('backgroundPosition','8.5em top');
	});
});

$(document).ready(function(){
	var bgImg=['new','info','event','mainte','up','fix'];
	var imgLoad=[];
	for (var i=0; i<bgImg.length; i++){
		imgLoad[i]=new Image();
		imgLoad[i].src='https://cache.sengokuixa.jp/www/150220/img/s3/s3_news_bg_'+bgImg[i]+'.gif';
	}
	$('#newsNavi a').click(function(){
		var newsId = $(this).parent().attr('id');
		var mStr = newsId.replace('nBtn','');
		$('[id^=newsBody]').css('display','none');
		$('#newsBody'+mStr).css('display','block');
		return false;
	});
	$('[id^=newsBody]').css('display','none');
	$('#newsBodyNew').css('display','block');
});

$(document).ready(function(){//カードポップアップ
	var cardPopNone = function(){//カード非表示
		$('.popCardIn').css('display','none');
	};
	var cardPopNum = $('.popCardIn').length - 1;
	$('.popCardIn .number p').each(function(index){//カードページ表示
		var idx = index;
		var idxp = index + 1;
		var cpNum = cardPopNum + 1
		$(this).append(idxp + '/' + cpNum);
	});
	$('.btnRightAr a').each(function(index){//右ボタン
		var idx = index + 1;
		if (idx > cardPopNum) idx = 0;
		$(this).click(function(){
			cardPopNone();
			$('.popCardIn').eq(idx).css('display','block');
			return false;
		});
	});
	$('.btnLeftAr a').each(function(index){//左ボタン
		var idx = index - 1;
		if (idx < 0) idx = cardPopNum;
		$(this).click(function(){
			cardPopNone();
			$('.popCardIn').eq(idx).css('display','block');
			return false;
		});
	});
	cardPopNone();
	$('.popCardIn').eq(0).css('display','block');
});
//]]>
</script>

