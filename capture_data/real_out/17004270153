<script type="text/javascript">
	$(document).ready(function(){

		var slidesCount = (location.origin.indexOf("verslas") != -1 || location.pathname.indexOf("verslas") != -1) ? 5.5 : 7;

		const storiesSwiper = new Swiper('.swiper', {
		  direction: 'horizontal',
		  loop: false,
		  slidesPerView: 2.5,

		  spaceBetween: 10,

		  // Navigation arrows
		  navigation: {
		    nextEl: '.swiper-button-next',
		    prevEl: '.swiper-button-prev',
		    enabled: false
		  },

		  // And if we need scrollbar
		  scrollbar: {
		    el: '.swiper-scrollbar',
		  },

		  breakpoints: {
		    675: {
		      slidesPerView: slidesCount,
		      navigation: {
		      	enabled: true
		      }
		    },
		  },

		});
		
		$('[data-fancybox="storries"]').fancybox({
			smallBtn: false,
			baseClass: "fancy-story",
			defaultType: "inline",
			toolbar: true,
			buttons: [
			  "close"
			],
			slideShow: {
			    autoStart: true,
			    speed: 20000
			},
			afterLoad : function( instance, current ) {

	  			setTimeout(function(){

					if ($('.story.full #stories-navigation').length > 0) {
						$('.story.full #stories-navigation').remove();
					}

					var $storiesNavigation = $('#stories-navigation').clone();

		  			$('.fancybox-slide--current .story.full').prepend($storiesNavigation);
	  				
	  				var bar = current.src.replace('#','') + "-bar";

	  				$("#" + bar).parent().prevAll().children().css("width", "100%");

		  			move(bar);

		  			// analytics 
	  				var aricleId = current.src.replace('#story-','');
	  				var articleTitle = $("#story-" + aricleId).find(".story-info .title")[0].innerText;
	  				var articleUrl = $("#story-" + aricleId).find(".story-info")[0].getAttribute('data-url');

		  			// delfi cookie 
		  			_dct({a:aricleId});
		  			// google analytics
		  			ga('send',{hitType:'pageview', articleUrl, dimension4:aricleId, title:articleTitle});
		  			window.ga('send', 'event', 'VersloPulsasClick', 'click', aricleId);

		  			// $(".story.full .story-info .lead").perfectScrollbar({
					// 	wheelSpeed: 2,
					// 	suppressScrollX: true,
					// 	wheelPropagation: true
					// });


		  		}, 500);	  			

			},
			clickContent: function(current, e) {

			    var pWidth = $(window).innerWidth();
			    e.stopPropagation();
			    e.preventDefault();

				if(e.type == 'touchstart' || e.type == 'touchmove' || e.type == 'touchend' || e.type == 'touchcancel'){
			        var touch = e.originalEvent.touches[0] || e.originalEvent.changedTouches[0];
			        x = touch.pageX;
			    } else if (e.type == 'mousedown' || e.type == 'mouseup' || e.type == 'mousemove' || e.type == 'mouseover'|| e.type=='mouseout' || e.type=='mouseenter' || e.type=='mouseleave') {
			        x = e.clientX;
			    } else {
			    	var touch = e.originalEvent.touches[0] || e.originalEvent.changedTouches[0];
			        x = touch.pageX;
			    }

			   var instance = $.fancybox.getInstance();
			   if(pWidth/2 > x) {
			   		var jumpTo = current.index - 1;
			   		instance.jumpTo( jumpTo, 100 );
			   } else {
			   		var jumpTo = current.index + 1;
			   		instance.jumpTo( jumpTo, 100 );
			   }				
			},

			// needed for mobile ios devices
			mobile: {
				clickContent: function(current, e) {

				    var pWidth = $(window).innerWidth();
				    e.stopPropagation();
				    e.preventDefault();

					if(e.type == 'touchstart' || e.type == 'touchmove' || e.type == 'touchend' || e.type == 'touchcancel'){
				        var touch = e.originalEvent.touches[0] || e.originalEvent.changedTouches[0];
				        x = touch.pageX;
				    } else if (e.type == 'mousedown' || e.type == 'mouseup' || e.type == 'mousemove' || e.type == 'mouseover'|| e.type=='mouseout' || e.type=='mouseenter' || e.type=='mouseleave') {
				        x = e.clientX;
				    } else {
				    	var touch = e.originalEvent.touches[0] || e.originalEvent.changedTouches[0];
				        x = touch.pageX;
				    }

				   var instance = $.fancybox.getInstance();
				   if(pWidth/2 > x) {
				   		var jumpTo = current.index - 1;
				   		instance.jumpTo( jumpTo, 100 );
				   } else {
				   		var jumpTo = current.index + 1;
				   		instance.jumpTo( jumpTo, 100 );
				   }				
				}
			}

		});

		var i = 0;
		function move(bar) {
		
		  if (i == 0) {
		    i = 1;
		    var elem = document.getElementById(bar);
		    var width = 0;
		    var id = setInterval(frame, 180);

		    function frame() {
		      if (width >= 100) {
		        clearInterval(id);
		      } else {
		        width++;
		        elem.style.width = width + "%";
		      }
		      i = 0;
		    }
		  }
		}

		/* native share */
		if (navigator.share) {

			$(".story.full .share-native").css("display", "inline-block");
			$(".story.full .share-native").prevAll().hide();

			$(".story.full .share-native a").click(function(event){
				event.preventDefault();

				var shareData = {
				  title: 'Dalintis',
				  text: $(this).data("title"),
				  url: $(this).data("url")
				}

				navigator.share(shareData);
			});
		}
		// $("story-info")
		// element.scrollHeight > element.clientHeight || element.scrollWidth > element.clientWidth

	});

</script></div></div></div><div class="container bb2" id="toc-verslas" data-side-menu data-title="Verslas">
