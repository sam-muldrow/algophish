    #askme-form input[type=text]{
		padding: 10px;
		border: 1px solid #e9e9e9;
		box-sizing: border-box;
		width: 49%;
		font-size: 14px;
		color: #686868;
		border-radius: 10px;
		outline: none;
		display: inline-block
    }
    #askme-form .submit{
	    margin: 10px 0;
		padding: 10px 25px;
		color: #FFF;
		background: #3858f5;
		border: 0;
		font-size: 16px;
		border-radius: 27px;
		outline: none;
    }
     #askme-form .ask-report-message{
	     font-size: 11px;
     }
     #askme .success-report-message{
         text-align: center;
		 font-weight: 700;
    }
    
</style>

<script>
	
	$(function() {
		if (_dlf.is_user_customer){
			$('#iscustomer').val('TAIP');
		}
	});
	
	$(function() {	
		$(document).on('click', '#__asksb', function() {
			var isFormValid = true;
			isFormValid = isEmpty();
			if (isFormValid ){
					$.ajax({
						url: 'https://www.delfi.lt/plius/misc/askme.php',
						type: 'POST',
						data: new FormData($('#askme-form')[0]),
						contentType: false,       
						cache: false,             
						processData:false,  
						dataType: 'json',
							success: function(response) {
								if (response.status == '1') {
									$('#askme-form').slideUp(function() {
									$('.success-report-message').css('padding-top','15px');
									$('.success-report-message').text(response.message).slideDown();
								});
								} else {
									$('.ask-report-message').text(response.message).addClass('error').slideDown();
								}
							}
						});
				}					
			return false;
		})
	});

function isEmpty(){ 
	var isFormValid = true; 
	if($('#__mrct').val() == "")
    {
	    $('.ask-report-message').css('color', '#dd5353')
		$('.ask-report-message').text('Įveskite klausimą')
		isFormValid = false
    }
   return isFormValid;
}
</script>


