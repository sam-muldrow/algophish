	}
	.video-stars-content .col-xs-3{
	    padding-left: 7px;
	    padding-right: 7px;
height: 200px;
    overflow: hidden;
	}
	#video-stars-container a {
		color: #FFF;
		font-size: 15px;
 		font-weight: 700;
	}
	#video-stars-container .fa-youtube {
		position: absolute;
	    font-size: 42px;
	    left: calc(50% - 21px);
	    top: calc(50% - 21px);
	    color: #ea3223;
	}
	#video-stars-container .duration {
	    position: absolute;
	    bottom: 5px;
	    right: 5px;
	    background: rgba(0, 0, 0, 0.5);
	    padding: 3px 7px;
	    border-radius: 3px;
	}
	#video-stars-container .carousel-left {
		display: none !important;
	}
	#video-stars-container .carousel-right {
		display: block;
	    top: 45px;
	    padding: 10px 13px;
	    right: -12px;
