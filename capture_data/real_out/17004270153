<script type="text/javascript">
	
	$(function(){
		if (_dlf.is_user_customer) {
			$("#delfi-adx-siteheader").remove();
		}

		setTimeout(function(){
			if (!!$("#delfi-adx-siteheader").find("iframe")[0]) {
				$("body").addClass("sh-inited");
				$(window).on('scroll', function() {
					if (!!$("#delfi-adx-siteheader").length) {
						if ($(window).scrollTop() < 300) {
				            var windowScrollTop = 400 - $(window).scrollTop();
				            $("html")[0].setAttribute("style", "--delfi-topbar-height:" + windowScrollTop + "px");
				        } else {
				            $("html")[0].setAttribute("style", "--delfi-topbar-height:100px");
				        }
					}
		        });

			    var resize_ob = new ResizeObserver(function(entries) {
			    	$("html")[0].setAttribute("style", "--delfi-topbar-height:400px");
					resize_ob.unobserve(document.querySelector("#delfi-adx-siteheader"));
				    $("#siteheader-close-button .closeButton ").show();
				});

				resize_ob.observe(document.querySelector("#delfi-adx-siteheader"));
			} else {
				// init plius promotion line
				_dlf.adxtag(1024268)
			}

		}, 500);

		

        $("#siteheader-close-button").click(function(){
        	$("#delfi-adx-siteheader").remove();
            $("html")[0].setAttribute("style", "--delfi-topbar-height:0px");
        })

	});

</script>
