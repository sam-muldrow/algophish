<script type="text/javascript">
    $(function() {

        
    });

        
        $("#rusijos-agresija-block-v2 nav ul li.tab-switcher").click(function(){
            if ($(this).hasClass("active")) {
                return;
            } else {
                var tab = $(this).data("tab");
                $("#rusijos-agresija-block-v2 nav ul li").removeClass("active");
                $(this).addClass("active");
                $("#rusijos-agresija-block-v2 .content-tab").removeClass("active");
                $("#rusijos-agresija-block-v2 #" + tab).addClass("active");
            }
            
            ga('send', 'event', 'Ru agression navigation block', $(this).text());

        });

</script><div id="aktualu" class="mb20">
