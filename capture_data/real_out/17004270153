    #rusijos-agresija-block-v3 .message a {
        font-size: 14px;
        width: 420px;
        float: left;
    }
    #rusijos-agresija-block-v3 .col-xs-4 {width: 300px;}
    #rusijos-agresija-block-v3 .col-xs-8 {width: calc(100% - 320px); margin-left: 20px;}
    #rusijos-agresija-block-v3 ul { list-style: disc; overflow: hidden; padding-left: 20px; margin: 0;}
    #rusijos-agresija-block-v3 li { color:#fa044f; font-size: 15px; font-weight: 500; margin-bottom: 10px; }
    #rusijos-agresija-block-v3 li a {font-size: 14px; font-weight: 400;}
    #rusijos-agresija-block-v3 li a.commentCount { color:#fa044f;}
    #rusijos-agresija-block-v3 .headline {margin-bottom: 0;}
    #rusijos-agresija-block-v3 .headline-title {padding-bottom: 0; margin-top: 7px;}
    #rusijos-agresija-block-v3 .date-from {color: #727272; font-size: 12px; margin: 0; font-weight: 400;}
    #rusijos-agresija-block-v3 li.chronologija a {display: none;}
