<script type="text/javascript">
	$(function() {

		var resize_ob = new ResizeObserver(function(entries) {
			var rect = entries[0].contentRect;
			var height = rect.height;
			var bodyPaddingTop = 115 + height;

			$("body")[0].setAttribute("style", "padding-top:" + bodyPaddingTop + "px");

		});
		resize_ob.observe(document.querySelector("#plius-promotion-line"));

		var promotionLine  = new Headroom(document.getElementById("plius-promotion-line"), { offset: 94 });
		promotionLine.init()
	});
</script>
<style type="text/css">
body {
    padding-top: 115px;
}
#plius-promotion-line {
    position: fixed;
    top: 115px;
    width: 100%;
    z-index: 5;
}
#plius-promotion-line.headroom.headroom--not-bottom.headroom--pinned.headroom--top {
	transform: translateY(0px);
}
#plius-promotion-line.headroom--unpinned {
    transform: translateY(-43px);
}
#plius-promotion-line.headroom--pinned {
    transform: translateY(-43px);
}
</style>
