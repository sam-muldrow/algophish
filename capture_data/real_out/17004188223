            <script>
                window.addEventListener('message', function (e) {
                    if (e.data.type === "video-gtm-flow-player-plugin") {
                        function checkEvents() {
                            if (typeof window.dataLayer.push === 'function') {
                                if (e.data.event !== 'trigger_pause') {
                                    window.dataLayer.push({
                                        'event': e.data.event,
                                        'gaEventCategory': 'video',
                                        'gaEventAction': e.data.description,
                                        'gaEventLabel': e.data.title
                                    });
                                }
                                if (e.data.event === 'trigger_play') {
                                    document.querySelector('.news__carousel-top').classList.add('news__carousel-top--hidden')
                                }
                                if (e.data.event === 'trigger_pause') {
                                    document.querySelector('.news__carousel-top').classList.remove('news__carousel-top--hidden')
                                }
                            } else {
                                setTimeout(checkEvents, 500);
                            }
                        }
                        checkEvents();
                    }
                }, false);
            </script>

