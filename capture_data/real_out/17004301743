<script>
// mobile view accordian view for footer-title
document.addEventListener('DOMContentLoaded', function () {
    var accordions = document.querySelectorAll('.title-news, .title-policy, .title-about, .title-edition');

    for (var i = 0; i < accordions.length; i++) {
        accordions[i].addEventListener('click', function () {
            var isActive = this.classList.contains('active-acc-title');

            for (var j = 0; j < accordions.length; j++) {
                accordions[j].classList.remove('active-acc-title');
                var subscriptionpanel = accordions[j].nextElementSibling;
                subscriptionpanel.style.display = 'none';
            }

            if (!isActive) {
                this.classList.add('active-acc-title');
                var subscriptionpanel = this.nextElementSibling;
                subscriptionpanel.style.display = 'block';
            }
        });
    }
});
</script>

