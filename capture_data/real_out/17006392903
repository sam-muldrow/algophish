<script async="async" src="https://relevantmagazine.com/wp-content/themes/theissue-child/assets/jquery.cookie.js?v=3"></script>
<script type="text/javascript">

 	

	jQuery(window).on('load', function() {
		jQuery(".popmake-240906").trigger("click");
	});
	jQuery(document).ready(function($) {
		function getCookie(cname) {
		  let name = cname + "=";
		  let ca = document.cookie.split(';');
		  for(let i = 0; i < ca.length; i++) {
		    let c = ca[i];
		    while (c.charAt(0) == ' ') {
		      c = c.substring(1);
		    }
		    if (c.indexOf(name) == 0) {
		      return c.substring(name.length, c.length);
		    }
		  }
		  return "";
	}

	let hide_banner = getCookie("hide_banner");
	  if (hide_banner != "") {
	    //alert("hide_banner " + hide_banner);
	    $('.ad-supported').addClass('hide');
	  } else {
	  	$('.ad-supported').removeClass('hide');
	  }

		$(document).on('click','#usr_acunt',function (e) {
		  $(this).parent().toggleClass('active');
		  e.stopPropagation();
		});

		window.almDestroyed = function(alm){
			jQuery('.show_pop_after_destroy').show();
			jQuery('body').addClass('stopscrolling');
			console.log("Ajax Load More functionality has been destroyed!");
		};


		$('.hide_ad').click(function(){
			$('.ad-supported').addClass('hide');
			$.cookie('hide_banner', 'yes', {
			   expires: 1,
			   path: '/',
			});
		});
	});

</script>
</body>
</html>

