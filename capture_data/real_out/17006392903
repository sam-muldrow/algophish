ite-title):not(.featured-style):not(.white-post-content) .post-category a { color:#e9c46a !important; }.footer .widget a { color:#909090 !important; }.footer .widget a:hover { color:#222222 !important; }.subfooter a { color:#909090 !important; }.subfooter a:hover { color:#222222 !important; }.subfooter .thb-full-menu>li a { color:#909090 !important; }.subfooter .thb-full-menu>li a:hover { color:#222222 !important; }.page-id-1544265 #wrapper div[role="main"],.postid-1544265 #wrapper div[role="main"] {}#mobile-menu {background-color:#1a1a1a !important;background-position:center center !important;}.footer {background-color:#f5f5f5 !important;}.subfooter {background-color:#1a1a1a !important;}.footer {padding-top:80px;padding-bottom:10px;}.subfooter {padding-bottom:30px;}.footer .footer-logo-holder .footer-logolink .logoimg {max-height:30px;}.row {max-width:1300px;}.row.max_width {max-width:1300px !important;}.fb-comments {background:#fff;}aside.ad_container_bottom.cf {text-align:center;}aside.ad_container_bottom.cf a img {padding-top:20px;width:auto;}.adcnt-center {text-align:center;}.adcnt-center a img {width:auto;}.footer .widget {color:rgba(255,255,255,.8);}body:not(.thb-dark-mode-on) .footer-logolink .logoimg {filter:brightness(1) invert(1);}.footer a:not(.post-social-share).thb_title.style8 h2 {color:#000000;}.header.style5 .thb-navbar {border:0;}.post .post-excerpt {font-size:16px;}p.thb-newsletter-warning-text {display:block !important;}.post .thb-post-bottom ul li+li:before{display:none;}.category-subscribe {margin:0 auto;}.category-subscribe form.mailster-form {max-width:410px;display:inline-flex;width:100%;}.category-subscribe form.mailster-form input[type="email"] {height:46px;padding:17px 20px;}.category-subscribe form.mailster-form input[type="submit"] {height:46px;line-height:46px;padding:0 30px;font-size:12px;font-weight:600;}.category-subscribe .mailster-form-fields .form-submit {margin-top:0;margin-bottom:0;}.mailster-wrapper.mailster-email-wrapper labe