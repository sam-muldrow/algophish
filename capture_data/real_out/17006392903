        function playpod3(id)
        {

        if (!jQuery('.play' + id).hasClass("mksingliclick") ) {

jQuery('.play' + id).addClass("mksingliclick") ;

                setTimeout(function () {  
jQuery('.play' + id).removeClass("mksingliclick") ;

                       }, 1000);


            jQuery('.custom-play-' + id).trigger('click');

        //jQuery('.cstmplay').removeClass('pause_active');


        if (jQuery('.play' + id).hasClass("pause_active")) {
            //jQuery('.play'+id).removeClass('pause_active');
            jQuery('.cstmplay').removeClass('pause_active');
        } else {
            jQuery('.cstmplay').removeClass('pause_active');
            jQuery('.play' + id).addClass('pause_active');
        }

        }



        //jQuery(this).toggleClass('pause_active');
