<script type="text/javascript">
	var recaptchaOnload = function() {
		grecaptcha.execute('6Ld9gXsmAAAAAE_tgSNL-WBZ35ORXclvMLwwGtXZ', {action: '/index_do'}).then(function(token) {
			jQuery.ajax({
				url: "/validation_recaptcha.do?method=v3",
				dataType: "json",
				type: "POST",
				data: {
					token: token,
				},
			}).done(function(data) {
				result = data[0];
				if (result.success == true) {
					location.reload();
				} else {
					if (confirm('잠시후 다시 시도해주세요.')) {
						location.reload();
					}
				}
			});
		});
	};
</script>

