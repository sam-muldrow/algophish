	<script>

		/**
		 * This function makes sure that menu items that are out of the .im-primary-nav__list element are hidden,
		 * otherwise the menu item is shown.
		 * 
		 * The idea is to hide menu item when the screen width gets smaller (mobile and tablet screens) and show them when the screen gets bigger (desktop).
		 * 
		 * The script is placed here so it gets executed inmediately after the menu is rendered.
		 *
		 * @return void
		 */
		function menuResponsive() {
			const menuList = document.querySelector( '.im-primary-nav__list' );

			const menuItemIsInside = function( menuItem ) {
				const parentCoords = menuList.getBoundingClientRect();
				const childCoords = menuItem.getBoundingClientRect();

				return ! ( childCoords.bottom > parentCoords.bottom );

			};

			const menuItems = menuList.querySelectorAll( 'li.menu-item.im-primary-nav__item' );

			menuItems.forEach( function( menuItem ) {
				if ( menuItemIsInside( menuItem ) ) {
					menuItem.classList.remove( 'out-of-bounds' );
				} else {
					menuItem.classList.add( 'out-of-bounds' );
				}
			} );
		}

		menuResponsive();

		window.addEventListener('resize', menuResponsive);
	</script>
