if(substrEd == "/editorial/"){     
    var elements = document.getElementsByClassName("viafoura vf-wrapper");
    elements[0].parentNode.removeChild(elements[1]);
 }
    
}
$( document ).ready(function() {
removeElementsByClassEditorial();
    
});</script>
