              }
            });
          } else {
            _comscore.push({
              c1: "2", c2: "32666898", cs_ucfr: "0",
              options: {
                enableFirstPartyCookie: true
              }
            });
          }
        });
        customize.addEventListener("click", function() {
          if(document.cookie.includes("cookielawinfo-checkbox-analytics=yes;")) {
            _comscore.push({
              c1: "2", c2: "32666898", cs_ucfr: "1",
              options: {
                enableFirstPartyCookie: true
              }
            });
          } else {
            _comscore.push({
              c1: "2", c2: "32666898", cs_ucfr: "0",
              options: {
                enableFirstPartyCookie: true
              }
            });
          }
        });
      }, 1000);
      </script>

