<script>
    function checkRadio(id) {
        $('#' + id).attr('checked', true);
    }

    var opt_clicked = false;
    var load_poll = '';
    $(document).ready(function() {
        if (load_poll) {
            var file = '/_ajax/anket_form.php';
            $.ajax({
                type: 'GET',
                url: file,
                success: function(ajaxData) {
                    $('#anket_main').removeClass('ajax_loading');
                    $('#anket_main').css('height','');
                    $('#anket_main').html(ajaxData);
                }
            });
        }

        $('.options_form').each(function(i) {
            /*
            $(this).mouseover( function(){
                id = $(this).attr('id').substr(7);
                $('#opt_img_'+id).removeClass('options_img');
                $('#opt_img_'+id).addClass('options_img_selected');
            })

            $(this).mouseout( function(){
                id = $(this).attr('id').substr(7);
                $('#opt_img_'+id).removeClass('options_img_selected');
                $('#opt_img_'+id).addClass('options_img');
            })
            */

            $(this).click( function(){
                id = $(this).attr('id').substr(7);
                qId = $(this).attr('qId');
                $('.options_img').removeClass('options_img_selected');
                $('#opt_img_'+id).addClass('options_img_selected');
                opt_clicked = true;
                checkRadio(id,qId);
            })
        });

    });
    function checkRadio(id,qId) {
        //alert(qId+" "+id);
        document.getElementById('frm_option').value = id;
        $('#opt_img_'+id).addClass('options_img_selected');
        //submitPoll(qId)
    }


    function submitPoll(qId) {
        //alert('tiklandi');
        frmName = 'frmAnket'+qId;
        if (!$("#frm_option").val()){
            alert("Lutfen bir secim yapiniz!");
            return false;
        }
        else
            document.forms[frmName].submit();
    }

</script>
