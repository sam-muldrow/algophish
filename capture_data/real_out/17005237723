<script>
    sxhaber_load(1)
    function sxhaber_load(id){
        if(id == 1) {
            $("#sporxnews-head").load("/_txt/sporx_haberler1.inc");
            $(".bold1").removeClass('regular');
            $(".bold1").addClass('bold');
            $(".bold2").removeClass('bold');
            $(".bold2").addClass('regular');
            $(".bold3").removeClass('bold');
            $(".bold3").addClass('regular');
        }
         if(id == 2) {
            $("#sporxnews-head").load("/_txt/sporx_haberler2.inc");
            $(".bold2").removeClass('regular');
            $(".bold2").addClass('bold');
            $(".bold1").removeClass('bold');
            $(".bold1").addClass('regular');
            $(".bold3").removeClass('bold');
            $(".bold3").addClass('regular');
        }
         if(id == 3) {
            $("#sporxnews-head").load("/_txt/sporx_haberler3.inc");
            $(".bold3").removeClass('regular');
            $(".bold3").addClass('bold');
            $(".bold2").removeClass('bold');
            $(".bold2").addClass('regular');
            $(".bold1").removeClass('bold');
            $(".bold1").addClass('regular');
        }
    }
</script>
