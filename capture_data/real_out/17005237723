<script>
var AktifManset = 1;
var mansetTimerId = 0;

function manset_degistir(pos){

	if(pos == '+'){
		AktifManset += 1;
		if(AktifManset>8)AktifManset = 1;
	}
	else if(pos == '-'){
		AktifManset -= 1;
		if(AktifManset<1)AktifManset = 8;
	}
	else if(pos > 0 && pos < 9){
		AktifManset = pos;
	}

	for(k=1;k<=8;k++){
		if(k == AktifManset){
			manset_div = $("#micerik"+k).html();
			$("#mth"+k).show();
			$("#mctr").html(manset_div);
		}
		else $("#mth"+k).hide();
	}

	clearTimeout(mansetTimerId);
	mansetTimerId = setTimeout("manset_degistir('+')",10000);
}

function mnstop(){
	clearTimeout(mansetTimerId);
}
function mnstart(){
	mansetTimerId = setTimeout("manset_degistir('+')",10000);
}

mansetTimerId = setTimeout("manset_degistir('+')",10000);

</script>
