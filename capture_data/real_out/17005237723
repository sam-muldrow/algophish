<script language="JavaScript">
var current_page = 1;
$(document).ready(function() {

    $('.liglr').each(function(i) {
        $(this).mouseover(function(){
            $(this).css('background','url(https://cdn.webaslan.com/_img/foto_galeri/foto_cerceve.png) no-repeat');
        })

        $(this).mouseout( function(){
            $(this).css('background','');
        })

    });
});

function goGlrPage(no) {
    var gp = no - current_page;
    if (gp == 0) return false;
    if (gp > 0) {
        for (i=1;i<=gp;i++)
            $('.next_ei').trigger('click');
    }
    else {
        for (i=1;i>=gp;i--)
            $('.prev_ei').trigger('click');
    }
    current_page = no;
}

$(function() {
    $("#anasayfa-galeri").jCarouselLite({
        btnNext: ".next_ei",
		btnPrev: ".prev_ei",
        visible: 5,
        scroll: 5,
        //circular: false,
        mouseWheel: false,
        afterEnd: function(a) {
            gsayfa2 = a[4].className;
            //alert(gsayfa2)  ;
            gsayfa2 = Math.round(gsayfa2 / 5);
            //alert(gsayfa2)  ;

            for(i=1;i<=5;i++){
                $(".extCglr .s"+i).html('<img src="https://cdn.webaslan.com/_img/foto_galeri/gri_nokta.png" border="0">');
            }
            $(".extCglr .s"+gsayfa2).html('<img src="https://cdn.webaslan.com/_img/foto_galeri/kirmizi_nokta.png" border="0">');
        },
		btnGo:[]
    });
});
