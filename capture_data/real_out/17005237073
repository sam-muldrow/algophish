<script>
  var 
    nav_newmedia = document.getElementById("nav-newmedia"),
	nav_newmedia2 = document.getElementById("nav-newmedia2"),

    viewportHeight = Math.max(window.innerHeight || 0),
    nav_newmediaOffsetTop = nav_newmedia.offsetTop;
	nav_newmedia2OffsetTop = nav_newmedia2.offsetTop;

    var hasScrolled = function() {

        var fromTop = document.body.scrollTop || document.documentElement.scrollTop || 0;

        if (nav_newmediaOffsetTop < fromTop) {
            nav_newmedia.classList.add('fixed-position');
			 nav_newmedia2.classList.add('fixed-position');
        } else {
            nav_newmedia.classList.remove('fixed-position');
			nav_newmedia2.classList.remove('fixed-position');
        }
    }

    window.addEventListener('scroll', hasScrolled);
