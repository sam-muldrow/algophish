            site = "www";
            access = "user_access";
            email_cookie = "user_email";
            mobile_cookie = "user_mobile";
            ajaxPath = "https://"+Host+".clickindia.com/cimem/";
            functionParam = "FormName";
            login = "login";
          }

	$("form#loginform :input").each(function()
  {
    var input = $(this);
    $(input).keyup(function(event)
    {
        if (event.keyCode === 13)
        {
            if($('#login:hidden').length==0)
            {
                $("#login_sub").click();
            }
            else
            {
                $("#ads_submit").click();
            }
        }
    });
  });
