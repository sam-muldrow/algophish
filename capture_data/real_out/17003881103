<script>
/* ^uØèÖ¦ */
function changeTab (pos,num,cnt) {
	if(!pos || !num) return false;
	var target = document.getElementById(pos);
	var targetNodes = target.childNodes;
	var selector = document.getElementById(pos + 'Selector');
	var selectorTag = selector.getElementsByTagName('li');
	for(var i = 0; i < targetNodes.length; i++) {
		if(!targetNodes[i].className) continue;
		if(!targetNodes[i].className.match(/colBoxTab/)) {
			continue;
		} else if(targetNodes[i].className.match('colBoxTab' + num)) {
			targetNodes[i].style.display = 'block';
		} else {
			targetNodes[i].style.display = 'none';
		}
	}
	for(var i = 0; i < selectorTag.length; i++) {
		if(selectorTag[i].title.match('colBoxTab' + num)) {
			if(!selectorTag[i].className) selectorTag[i].className += 'colBoxLlistActive';
		} else {
			selectorTag[i].className = '';
		}
	}
	return true;
}
</script>


