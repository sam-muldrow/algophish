  var btabshow = "";
  
  showTab = function () {
    
    if (window.innerWidth < 420) {
        for (let i = 1; i <= numbtabs; i++) {
        console.log('btabshow is '+btabshow+'a');
        $('.'+xbtab+i+'a').hide();
        //console.log(xbtab+i);
        $('.'+btabshow+'a').show();
        $('.'+btabshow).show();
      }
    }
    
    else {
      for (let i = 1; i <= numbtabs; i++) {
        console.log('btabshow is '+btabshow);
        $('.'+xbtab+i).hide();
        //console.log(xbtab+i);
        $('.'+btabshow).show();
      }
    }
    
  }
    
  $(".btab").click(function() {
    $(".btab").removeClass("clicked");
    console.log('clicked');
    $(this).addClass("clicked");
    btabshow = $(this).attr("btabname");
    console.log(btabshow);
    
    showTab();
    
  });
  
  
