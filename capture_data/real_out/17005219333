  <script>
    // Intentionally run this immediately rather than waiting for document ready.
    // Document ready can block for a couple seconds if there is a synchronous <script> taking a while to load,
    // which may be reaching out to advertisers or other telemtry things.
    // Maybe this is paranoid, but I want this to be an ultra reliable thing to use.
    $('#skip-link').click(function() {
      // Jump to the element #main-content if present
      // Otherwise jump to the overall main content area from the layout
      var skipTargets = $('#main-content');
      if(skipTargets.length) {
        $(skipTargets[0]).attr('tabindex','-1');
        $(skipTargets[0]).focus();
      } else {
        $('div.main').attr('tabindex','-1');
        $('div.main').focus();
      }
    });
  </script>

