    woopra.track("jobs_click", {
    });
  });
})
jQuery( ".top-search-form input" ).keyup(function() {
  if(jQuery(this).val() != '') {
    jQuery('.top-search-form a img').show();
  } else {
    jQuery('.top-search-form a img').hide();
  }
});
jQuery('.top-search-form a').on('click', function(e) {
  e.preventDefault();
  jQuery( ".top-search-form input" ).val('');
  jQuery('.top-search-form a img').hide();
})</script><script id="cornerstone-custom-js" type="text/javascript">(function($) {
  $('a[href*=\\#]:not([href=\\#])').click(function() 
  {
    if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') 
        || location.hostname == this.hostname) 
    {
      
      var target = $(this.hash),
      headerHeight =  $(".x-bar-is-sticky").height() + 5; // Get fixed header height
            
      target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
              
      if (target.length) 
      {
        // $('html,body').animate({
        //   scrollTop: target.offset().top - headerHeight
        // }, 500);
        return false;
      }
    }
  });
  
  $('.x-menu-collapsed a[href*=\\#]:not([href=\\#])').on("click", function(event) {
		$('html').removeClass('js-no-scroll');
		$('.js-burger-btn').click();
	});
  
  
  //trigger Resources filtres//
  const resourcesFilterLink = document.querySelectorAll('#menu-resources-filter a');
	if (resourcesFilterLink) {
    Array.from(resourcesFilterLink).forEach(el => {
      el.addEventListener('click', function() {
        woopra.track("resource_filter", {
          category : el.innerText
        });
      });
  	})
  }
  
  //trigger datasets download //
  const datasetDownloadBtn = document.querySelectorAll('.download-links a:not(.open-dataset-info');
	if (datasetDownloadBtn) {
    Array.from(datasetDownloadBtn).forEach(el => {
      el.addEventListener('click', function() {
        woopra.track("dataset_download", {
        });
      });
  	})
  }
  
 
})(jQuery);


jQuery(document).ready(function() {
  if (jQuery(window).width() <= 767) {
    jQuery('.vid_wrap').each(function() {
      var width = jQuery(this).width();
      var height = jQuery(this).height();
      var content = jQuery(this).html();
      var img = jQuery(this).attr('data-placeholder');
      jQuery(this).html('<div class="placeholder" style="width: '+width+'px; height: '+height+'px;"><img src="'+img+'" width="'+width+'"></div>');
      jQuery(this).css('visibility', 'visible');
      jQuery(this).on('click', function(e) {
        e.preventDefault();
        jQuery(this).find('.placeholder').remove();
        jQuery(this).html(content);
        jQuery(this).addClass('loaded');
        jQuery(this).find('video.x-mejs[autoplay]').attr('playsinline', 'true');
        jQuery(this).find('video.x-mejs[autoplay]').trigger('play');
        jQuery(this).find('.mejs-layers .mejs-poster').hide();
        /*video.setAttribute('playsinline', 'true');
        video.play();*/
      })
    })
  }
})


/* Machine Learning Platform Tour - Take a tour button track */
jQuery(document).ready(function($) {
  $('a.btn_mlp_tour').on('click', function(e) {
    var link = window.location.href;
    if ("ga" in window) {
        tracker = ga.getAll()[0];
        if (tracker){
            tracker.send("event", "Machine Learning Platform Tour", 'click', link);
        }
    }
    woopra.track("mlpt_click", {
          category: "Machine Learning Platform Tour",
          action: "Click",
          label: link
    });
  });
  
});

/* Real World AI Book - woopra track */
jQuery(document).ready(function($) {
  $('a.real_world_ai_book').on('click', function(e) {
    var link = $(this).attr("href");
      	woopra.track("mlpt_click", {
          category: "Real World AI Book",
          action: "click",
          label: link
      });
  });
  
  /*$('.scrollspy-navbar ul.nav li:last-child a, #selections ul.dropdown-menu li:nth-child(6) a').on('click', function() {
    var myLink = $(this).attr("href");
    window.location.href = myLink;
    //window.open(myLink, '_BLANK');
  });*/
  
  $('a.get-in-touch-link').on('click', function(e) {
    jQuery("#tab-A").click();
  });
  
  $('li#menu-item-35372 a.ubermenu-target').on('click', function(e) {
    $('html, body').animate({
        scrollTop: $("#get_in_touch").offset().top
    }, 2000);
  });
  
  const swiper = new Swiper('.news-slider', {
    speed: 600,
    slidesPerView: 3,
    spaceBetween: 10,
    navigation: {
        nextEl: ".latest-next-btn",
        prevEl: ".latest-prev-btn"
    },
    // Responsive breakpoints
    breakpoints: {
      // when window width is >= 320px
      320: {
        slidesPerView: 1,
        spaceBetween: 20
      },
      // when window width is >= 480px
      480: {
        slidesPerView: 3,
        spaceBetween: 30
      },
      // when window width is >= 640px
      640: {
        slidesPerView: 3,
        spaceBetween: 40
      },
      1200: {
          slidesPerView: 3,
          spaceBetween: 28,
      },
    }
  });
  
});</script>
