<script>
	jQuery(function($){
		$('.js-burger-btn').on('click', function() {
			setTimeout(function(){
			if($(".x-off-canvas").hasClass('x-active')){
				$("body").addClass("show_lang_menu");	
			}else{
				$("body").removeClass("show_lang_menu");
			}
			},100);
		});
		$(".lang-select").click(function(e){
			$("#amp-lang-menu").toggleClass("menu-active");
		})
	})
</script>
<script id="x-header-custom-scripts" type="text/javascript">const clientLoginBtn = document.querySelector('.client_login a');
const jobsBtn = document.querySelectorAll('a[href="/jobs/"]');

