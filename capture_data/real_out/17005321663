	<script>
		var btn = jQuery('#back_to_top');
		jQuery(window).scroll(function() {
		  if (jQuery(window).scrollTop() > 300) {
			btn.addClass('show');
		  } else {
			btn.removeClass('show');
		  }
		});

		btn.on('click', function(e) {
		  e.preventDefault();
		  jQuery('html, body').animate({scrollTop:0}, '300');
		});	
	</script>
