$(document).ready(function() {    

       if ($(window).width() <= 600) { 

        var bgArray = ['1.jpg', '2.jpg', '3.jpg', '4.jpg', '5.jpg'];
        var bg = bgArray[Math.floor(Math.random() * bgArray.length)];
        var path = 'https://www.bodybuilding.com/images/2023/homepage/headers/hp-banner-mobile-';
$('#welcome-to-bbcom').css('background-image', 'url(' + path+bg + ')');
        
        } else {

        var bgArray2 = ['1.jpg', '2v2.jpg', '3v2.jpg', '4v2.jpg', '5v2.jpg'];
        var bg2 = bgArray2[Math.floor(Math.random() * bgArray2.length)];
        var path = 'https://www.bodybuilding.com/images/2023/homepage/headers/hp-banner-desktop-';
$('#welcome-to-bbcom').css('background-image', 'url(' + path+bg2 + ')');

        }

$(function() {
    var header = $("#welcome-to-bbcom");
    $(window).scroll(function() {
        var scroll = $(window).scrollTop();

        if (scroll >= 450) {
            header.removeClass('clearHeader').addClass("fixHeader");
        } else {
            header.removeClass("fixHeader").addClass('clearHeader');
        }
    });
});


    
});

</script>

