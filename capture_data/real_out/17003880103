        var date = new Date();
        var yearDate = date.getFullYear();
        document.getElementById("this-year").innerText = yearDate;

        this.video = $("#video")[0],
            this.video.volumn = .8,
            this.volumeBtn = $("#volumn");
        var n = this;
        this.volumeBtn.click(function() {
            n.video.muted ? n.volumeBtn.removeClass("muted").addClass("voice") : n.volumeBtn.removeClass("voice").addClass("muted"),
                n.video.muted = !n.video.muted
        })
