    function RedirectZoneF()
    {

        var ddl = document.getElementById("ctl00_ctl00_ddlRegion_firstpage");

        location.href = location.href.replace("/rn77/", "/" + ddl.options[ddl.selectedIndex].value + "/")
        
    }

