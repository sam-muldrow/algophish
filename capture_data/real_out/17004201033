                            },{    "title": "Dua Pesawat Tempur TNI AU Jatuh di Pasuruan Jatim",
                                "image": "https://cdn01.metrotvnews.com/dynamic/content/2023/11/16/N6GCgLjJ/t_6555bccad0d4c.jpg?w=300",
                                "link": "/play/N6GCgLjJ-dua-pesawat-tempur-tni-au-jatuh-di-pasuruan-jatim",
                                "date_published": "16 November 2023 12:58"
                            },{    "title": "Drama Sungkeman Gibran ke Megawati",
                                "image": "https://cdn01.metrotvnews.com/dynamic/content/2023/11/15/bJECa55A/t_6554bf1639691.jpg?w=300",
                                "link": "/play/bJECa55A-drama-sungkeman-gibran-ke-megawati",
                                "date_published": "15 November 2023 19:20"
                            },{    "title": "Biaya Haji Rp105 Juta Baru Usulan Awal",
                                "image": "https://cdn01.metrotvnews.com/dynamic/content/2023/11/14/NP6Cpm4A/t_6553046c46ae8.jpg?w=300",
                                "link": "/read/NP6Cpm4A-biaya-haji-rp105-juta-baru-usulan-awal",
                                "date_published": "14 November 2023 12:19"
                            }
                    ]
        </script>    <script type="text/javascript">
        const loadBtn = document.querySelector('.latest-paging .load-more');
        const uri_more = "https://www.metrotvnews.com/more_channel/3/";
        const options = {
            root: null,
            rootMargin: "0px",
            threshold: 0.1
        };

        const observer = new IntersectionObserver((entries) => {
            entries.forEach((entry) => {
                if (entry.isIntersecting) {
                    let dateSel = document.querySelectorAll('.latest-paging .item-1');
                    let node = dateSel[dateSel.length - 1];
                    let dt = dateSel.length > 0 ? parseInt(node.getAttribute('data-page')) + 1 : 1;

                    if (dt > 10) observer.disconnect();
                    let tpl = null;
                    let img = null;

                    fetch(uri_more + dt)
                        .then(response => response.json())
                        .then(res => {
                            rows = res;
                            for (v of rows) {
                                tpl = document.querySelectorAll('.latest-video .item-1')[0].cloneNode(true);
                                tpl.querySelectorAll('img').forEach(e => e.remove());
                                tpl.querySelector('.channel').remove();
                                // tpl.querySelector('.channel a').setAttribute('href', v.program_loc);
                                // tpl.querySelector('.channel a').setAttribute('title', v.program_title);
                                // tpl.querySelector('.channel a').innerHTML = v.program_title;

                                tpl.querySelector('.video-text a').setAttribute('href', v.loc);
                                tpl.querySelector('.video-text a').setAttribute('title', v.title);
                                tpl.querySelector('.video-text a').innerHTML = v.title;

                                img = document.createElement("img");
                                img.src = v.image;
                                img.alt = v.title;
                                onErrorImgLoad(img);

                                tpl.querySelector('.image-container').append(img);

                                tpl.setAttribute('data-date_published', v.published_at);
                                tpl.setAttribute('data-page', dt);

                                document.querySelector('.latest-paging .video-flex').append(tpl);
                            }
                        });
                }
            });
        }, options);

        observer.observe(loadBtn);
    </script>

    <script type="text/javascript">
        function closeAdsLeft() {
            document.getElementById("rails_left").style.display = "none";
        }

        function closeAdsRight() {
            document.getElementById("rails_right").style.display = "none";
        }

        function closeAds() {
            document.getElementById("footer_sticky").style.display = "none";
