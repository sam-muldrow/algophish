    ((w) => {
        const gseaFrom = '';
        const hostName = 'www.warthunder.com';

        w['GSEA_init'] = {
            debug: false,

            loginURI: 'https://login.gaijin.net',
            embedURI: 'https://embed.gaijin.net',

            embed: {
                template: 'default',
                page: 'default',
                language: undefined,
            },

            autoLogin: true,
            sessionPolling: true,
            sessionPollingInterval: 10 * 60000,
            uid: '',
            jwt: '',
            queryContext: 'https://login.gaijin.net/en/sso/reLogin/?return_url=aHR0cHM6Ly93d3cud2FydGh1bmRlci5jb20vZW4%3D&crc=0548238201d50267c97a31a99c2c1016&public_key=hoCqVTL6XOSaCR8dsMwD&domain=www.warthunder.com&base_return_url=1&refresh_token=1',
            onLoad: (client) => {
                client.on('change', (newSession, oldSession) => {
                    if (newSession.uid !== oldSession.uid) {
                        if (newSession.rt) {
                            window.location = `https://${hostName}/extLogin?refresh_token=${newSession.rt}${gseaFrom}`
                            return;
                        } else if (!newSession.uid) {
                            window.location = `https://${hostName}/logout`
                            return;
                        }
                    }
                })
            }
        }
    })(window);
</script>
