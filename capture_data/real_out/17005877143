		if (getId() == "") {
			$(".box_login").show();
			$(".box_logout").hide();
		} else {
			$(".box_login").hide();
			$(".box_logout").show();
		}

        //ì ì²´ë©ë´
		$('.btn_allmenu').click(function () {
			$('.search_box, .btn_search').removeClass('open');
			$('.allmenu_box, .btn_allmenu').toggleClass('open');
        });

        //ê²ìíì´ì§
		$('.btn_search').click(function () {
			$('.allmenu_box, .btn_allmenu').removeClass('open');
			$('.search_box, .btn_search').toggleClass('open');
        });

		//ìë¨ ì¤íìì´ë²¤í¸ ë«ê¸°
		$("#layerClose").click(function () { $(".special_area").slideUp(); });

		// ì¤í°í¤
		$(".gnb").sticky({ topSpacing: -95 });

		var ieChk = navigator.userAgent.toLowerCase();
		if ((navigator.appName == 'Netscape' && ieChk.indexOf('trident') != -1) || (ieChk.indexOf("msie") != -1)) {
			console.log("IE!!!")
        } else {
			fetch("/Home/_SpecialYoutube").then(function (resp) {
				if (resp) {
					resp.text().then(function (inner) {
						if (inner && inner.length > 100) {
							$(".youtube_target").after(inner);
                            $(".youtube_target").remove();
                            $(".government").hide();
                            $(".btn_elx_sub").hide();
						}
					});
				}
			}).catch(function (err) { console.log("ì¤í¸ë¦¬ë° íì´ì§ í¸ì¶ ì¤í¨", err); });
		}
	});

	$(document).bind('keydown', function (e) {
		if (e.which === 13) {
			$('#btn_headSearch').trigger('click');
		}
	});
</script>

