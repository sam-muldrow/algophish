	// Borderfree global objects for country and currency
	var localizeCurrent = {
  "currency": {
    "code": "USD",
    "symbol": "USD",
    "value": 1.0,
    "roundMethod": 2
  },
  "country": {
    "countryCode": "US",
    "countryCodeThreeChar": "USA",
