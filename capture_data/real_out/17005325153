      <script>
        var info = {}
        function get_playurl () {
          console.log(123);

          const previewBox = document.getElementById('preview');
          $.ajax({

            url: '/inc/interface/lv_data.php',
            type: 'POST',
            data: {
              act: 'getlv_adg',

              industry: 11
            },
            dataType: 'json',
            success: function (data) {
              console.log('11', data);
              if (data.error_code == 0) {
                info = data.info
                if (!info.id) {
                  previewBox.style.display = 'none';
                  return
                }

                console.log('info11', info)
                let lid = info.id

                let getlv_state = document.getElementById("getlv_state");
                getlv_state.innerText = info.state == 1 ? '直播中' : '预约';
                let divElement = document.getElementById("getlv_title");

                divElement.innerText = info.title;
                const cover_photo = info.cover_photo;

                let styles = {
                  "background-image": "url(" + cover_photo + ")",
                  "z-index": "999",
                  "background-size": "cover",
                  "background-repeat": "no-repeat",
                  "background-position": "center center"
                };
  previewBox.style.display = 'block';
                Object.assign(previewBox.style, styles);
              } else {
                previewBox.style.display = 'none';
              }
            }
          })
        };




        get_playurl()
        setInterval(function () {
          get_playurl();
        }, 6000);


        // const ua = window.navigator.userAgent;
