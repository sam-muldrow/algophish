<script>
    $(function(){
        $.ajax({
            type: "POST",
            url: "/xihuan/gt_job",
            dataType: "json",
            success: function(data){
                if(data.code == 1){
                    $('#show_xihuan').show()
                    $(".like").html(data.str);
                }else{
                    $('#show_xihuan').hide()
                }
            },error:function(data){
            }
        });
    });
</script>

