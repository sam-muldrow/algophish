    <script defer src="https://ocdn.eu/account-login-sdk/v2.min.js" onload="accountLoginSdkRunner()"></script>
    <script>
        const articleUrl = window.location?.href || "";
        document.querySelector('#subscribeLink').href += `?src=topBar&tpcc=topBar&articleuuid=&articleUrl=` + encodeURIComponent(articleUrl)
        function accountLoginSdkRunner() {
            const accountLoginSdk = new AccountLoginSDK({
                clientID: 'www.komputerswiat.pl.okonto.front.onetapi.pl',
                userSessionProxyEndpoint: '/user-session-proxy',
                paywall: true,
                premiumCookies: true
            });
            const accountBtn = document.getElementById('btnAccount');
            const accountPopup = document.getElementById("popupAccount");
            const loginPopup = document.getElementById("popupLogin");
            const loginLink = document.querySelector('.loginLink');
            const subscribeLink = document.getElementById('subscribeLink');
            let user = null;
    
            loginLink.setAttribute('href', accountLoginSdk.getLoginUrl())
    
            const registerLink = document.querySelector('.registerLink');
            registerLink.setAttribute('href', accountLoginSdk.getRegisterUrl())
    
            const logoutLink = document.getElementById('logoutLink');
            logoutLink.setAttribute('href', accountLoginSdk.getLogoutUrl())
    
            // user login and ad grid update
            function areAdsHidden() {
                const premiumCookies = accountLoginSdk.getPremiumCookies();
                return Boolean(premiumCookies) && premiumCookies.indexOf('prm_no_ads') > -1
            }
    
            //autologin
            const adsHiddenBeforeLogin = areAdsHidden();
            accountLoginSdk.autoLogin()
                .then(function (result) {
                    if (result.redirect) {
                        // rediection by SSO in a moment, so just wait...             
                        return;
                    }
                    if (areAdsHidden() !== adsHiddenBeforeLogin) {
                        // prm_no_ads cookie value has changed, reload the page to generate new grid
                        window.location.reload();
                        return;
                    }
                    if (result.session) {
                        // user logged     
                        user = true
                        accountBtn.classList.add('red');       
                    } else {
                        // user logged out    
                        subscribeLink.classList.remove('hidden');        
                    }
                    accountBtn.classList.remove('loading');
                    accountBtn.addEventListener('click', (event => user ? accountPopup.classList.toggle('hidden') : loginPopup.classList.toggle('hidden')));
                    logoutLink.addEventListener('click', (event => document.cookie = "login_status=;path=/;expires = Thu, 01 Jan 1970 00:00:00 GMT"));
                });
        }
    </script>
