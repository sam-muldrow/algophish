    $(document).ready(function () {
        $(".footer-extender .btn-arrow").on('click', function () {
            if (!$(this).hasClass('active')) {
                $(this).removeClass('active');
                $(this).parent().next('.footer-extend').slideDown({
                    complete: function () {
                        $(this).parent().find('.wrap.box-type02.footer-extender .title02').html('ë´ì¤ ëª©ë¡ ë³´ê¸°');
                    }
                });
            } else {
                $(this).addClass('active');
                $(this).parent().next('.footer-extend').slideUp({
                    complete: function () {
                        $(this).parent().find('.wrap.box-type02.footer-extender .title02').html('ë´ì¤/TV ëª©ë¡ ë³´ê¸°');
                    }
                });
            }
        });
    });
</script>

