<script type="text/javascript">
    
    var isPlayCdn = false;
    function setLive(YouTubeUrl, KakaoUrl) {
        var agent = navigator.userAgent.toLowerCase();
        if ((navigator.appName == 'Netscape' && navigator.userAgent.search('Trident') !== -1) || (agent.indexOf("msie") !== -1)) {
            // ì¸í°ë· ìµì¤íë¡ë¬ ë¸ë¼ì°ì 
            $('#tvPlayerSwfContainerFrame').html(KakaoUrl);
        } else {
            // ê¸°í
            $('#tvPlayerSwfContainerFrame').html(YouTubeUrl);
        }

        
        $('#tvPlayerSwfContainerFrame').css('display', '');
        $('#wrap-vod').hide();
    }
    var LiveTvPlayer = {
        Width: 731,
        Height: 409,
        OpenCover: false,
        Play: function () {
            
            try {
                TvPlayer.StopVod();
            } catch (e) {
            }
            $.ajax({
                type: 'POST',
                url: "/component/TvPlayer/GetLivePlayerInfo",
                data: { "serviceCode": "PC-HK-Main" },
                success: function (data) {
                    isCDN = data.IsCdn;
                    if (data.IsSuccess) {
                        if (isCDN != true) {
                            YouTubeUrl = data.YoutubePlayer == "" ? data.KakaoPlayer : data.YoutubePlayer;
                            KakaoUrl = data.KakaoPlayer == "" ? data.YoutubePlayer : data.KakaoPlayer;
                            setLive(YouTubeUrl, KakaoUrl);
                        } else {
                            LiveTvPlayer.PlayCDN();
                            isPlayCdn = true;
                        }
                        
                    } else {
                        LiveTvPlayer.PlayCDN();
                        isPlayCdn = true;
                    }
                },
                error: function () {
                    alert('ì¤ë¥ê° ë°ìíììµëë¤.');
                }
            });
        },
        PlayCDN: function () {
            $.ajax({
                type: 'GET',
                url: "/Component/TvPlayer/GetLiveTvInfo",
                success: function (data) {
                    $("#tvPlayerSwfContainerFrame").html("<div id=\"tvPlayerSwfContainer\" style=\"width:731px; height:409px; position:relative; bottom:0px; left:0px; background-color:black; display:none;\"><a href=\"http://www.macromedia.com/go/getflashplayer\" target=\"_blank\" style=\"color: white;\">Flash Playerë¥¼ ì¤ì¹ íìê¸° ë°ëëë¤.(í´ë¦­)</a>.</div>");

                    $("#tvPlayerSwfContainer").css("display", "");
                    $("#tvPlayerSwfContainerCover").css("display", "none");
                    if (data.WithAD == true) {
                        LiveTvPlayer.OpenCover = false;
                        /*
                        LiveTvPlayer.OpenCover = true;
                        setTimeout(function () {
                            var memberGrade = 2;

                            if (IsLogin() == true) {
                                memberGrade  = ConfirmMemGrade();
                            }

                            if ( data.memberGrade != 0 && data.memberGrade != 1 && LiveTvPlayer.OpenCover == true) {
                                LiveTvPlayer.ChangeToCover();
                            }

                        }, AdTerm);
                        */
                    }
                    var swfPlayer = "https://img.wowtv.co.kr/PcScript/Component/KinxPlayer/wowtv.swf";
                    var idPlayer = "tvPlayerSwfContainer";
	                var swfVariables = {
		                urlStream: "rtmp://cdnlive.wowtv.co.kr/wowtv",
                        nameStream: "livestream",
                        wmode: "transparent"
	                };
                    var swfParameters = {
                        allowscriptaccess: "always",
                        allowfullscreen: "true",
                        wmode: "transparent"
                    };
                    var swfAttributes = {
                        wmode: "transparent"
                    };
                    swfobject.embedSWF(swfPlayer, idPlayer, LiveTvPlayer.Width, LiveTvPlayer.Height, "10.1.0", null, swfVariables, swfParameters, swfAttributes);
                }
            });
        },
        Stop: function () {
            
            if (isPlayCdn) {
                $("#tvPlayerSwfContainer").remove();
                LiveTvPlayer.OpenCover = false;
                $("#tvPlayerSwfContainer").css("display", "none");
                $("#tvPlayerSwfContainerCover").css("display", "none");
            } else {
                $('#tvPlayerSwfContainerFrame').html('');
            }
        },
        ChangeToCover: function () {
            $("#tvPlayerSwfContainer").remove();
            LiveTvPlayer.OpenCover = false;
            $("#tvPlayerSwfContainer").css("display", "none");
            $("#tvPlayerSwfContainerCover").css("display", "");
        },
        Resize: function (width, height) {
            $("#tvPlayerSwfContainer").css("width", width);
            $("#tvPlayerSwfContainer").css("height", height);
            $("#tvPlayerSwfContainerCover").css("width", width);
            $("#tvPlayerSwfContainerCover").css("height", height);
            LiveTvPlayer.Width = width;
            LiveTvPlayer.Height = height;
            LiveTvPlayer.Play();
        },
        FullScreen: function () {
            tvPlayerSwfContainer.webkitRequestFullScreen();
        },
        ConfirmMemGrade: function () {
            var memberGrade = 2;
            $.ajax({
                method: 'POST',
                url: "/Component/TvPlayer/ConfirmMemGrade",
                success: function (data) {
                    memberGrade = data.memberGrade;
                }
            });
            return memberGrade;
        }
    }

    try {
        TvPlayer.StopVod();
    } catch (e) {
    }
    var player_new = null;
    function init_Player() {
        //$("#mediaplayer2").html('');
        try {
            if (player_new.isReady()) {
                
                return;
            }
            var D = widnow.getDframework();
            console("D = " + D);
        } catch (e) {
            (function () {
                var attr = {
                    playerType: '',
                    width: '',
                    height: '',
                    isLive: false,
                    debug: { use: false },
                    autoplay: true,
                    module: {
                        openLayer: { use: true }
                        , wide: { hideButton: true }
                        , repeat: { repeatBtn: { hideButton: true }, type: 2 }
                        , repmark: { menuBtn: { hideButton: true }, addBtn: { hideButton: true } }
                        , bookmark: { menuBtn: { hideButton: true }, addBtn: { hideButton: true } }
                        //, playlist: { previousBtn: {hideButton: true}, nextBtn: {hideButton: true}, menuBtn: { hideButton: true }, addBtn: {hideButton:true } }
                        , rate: { rateBtn: { hideButton: false } }
                        , config: { configBtn: { hideButton: true } }
                        , progress: {
                            previousSkipBtn: { hideButton: true },
                            nextSkipBtn: { hideButton: true }
                        }
                    }
                }

                player_new = window.createMediaPlayer('#mediaplayer2', null, attr, function (e) {
                    switch (e.type) {
                        case this.EVENT.ON_READY:
                            break;
                        case this.EVENT.CAN_PLAY:
                            //console.log(e.type);
                            //this.play();
                            break;
                        case this.EVENT.ON_BEFORE_PLAY:
                            //this.mute();
                            break;
                        case this.EVENT.STATE_CHANGE:
                            if (e.data == this.STATE.PLAYING) {
                            } else if (e.data == this.STATE.SETSOURCE) {
                            } else if (e.data == this.STATE.PAUSED) {
                            } else if (e.data == this.STATE.ENDED) {
                            }
                            break;
                    }
                });
            })();
        }
    }

    function stopplayer() {
        player_new.stop();
        player_new.destroy();
    }

    function playplayer(source, islive) {
        $('#wrap-vod').show();
        $("#tvPlayerSwfContainerFrame").html("");
        init_Player();
        player_new.source(source);
    }

    var TvPlayer = {
        PlayLiveTv: function () {
            
            try {
                $('#wrap-vod').hide();
                TvPlayer.StopVod();
            } catch (e) {
            }
            $.ajax({
                type: 'POST',
                url: "//www.wowtv.co.kr/component/TvPlayer/GetLivePlayerInfo",
                data: { "serviceCode": "PC-HK-Main" },
                success: function (data) {
                    isCDN = data.IsCdn;
                    if (data.IsSuccess) {
                        if (isCDN != true) {
                            YouTubeUrl = data.YoutubePlayer == "" ? data.KakaoPlayer : data.YoutubePlayer;
                            KakaoUrl = data.KakaoPlayer == "" ? data.YoutubePlayer : data.KakaoPlayer;
                            setLive(YouTubeUrl, KakaoUrl);
                        } else {
                            LiveTvPlayer.PlayCDN();
                            isPlayCdn = true;
                        }
                        
                    } else {
                        LiveTvPlayer.PlayCDN();
                        isPlayCdn = true;
                    }
                },
                error: function () {
                    alert('ì¤ë¥ê° ë°ìíììµëë¤.');
                }
            });
        },
        PlayTvReplay: function (num) {
            $.ajax({
                type: 'GET',
                url: "/Component/TvPlayer/GetTvReplayInfo",
                data: { num: num },
                success: function (data) {
                    //VodPlayer.Url = data.Url;
                    if (data.IsSuccess == true) {
                        //AdCate = data.tvProgramGubunCode;
                        //VodPlayer.Play(VodPlayer.Url, data.WithAD);
                        var vodUrl = data.Url;
                        vodUrl = vodUrl.replace("http://cdnvod.wowtv.co.kr:8080/", "https://cdnvod.wowtv.co.kr/wowtvhome/_definst_/mp4:") + "/playlist.m3u8";;
                        
                        playplayer(vodUrl, false);
                    }
                    else {
                        if (data.ReturnCode == "LOGIN_ERROR") {
                            alert("ì ë£ê²°ì¬ê° íìí ììì¼ë¡ ë¡ê·¸ì¸ì´ íìí©ëë¤.");
                        }
                        else if (data.ReturnCode == "NUM_ERROR") {
                            alert("ìëª»ë ì ê·¼ìëë¤.");
                        }
                        else if (data.ReturnCode == "INFO_ERROR") {
                            alert("ë°©ì¡ì ë³´ê° ììµëë¤.");
                        }
                        else if (data.ReturnCode == "USER_INFO_NOT_EXISTS") {
                            alert("ì¬ì©ì ì ë³´ê° ììµëë¤.");
                        }
                        else if (data.ReturnCode == "NO_AUTHORYT") {
                            if (data.HasPoint == true) {
                                if (confirm("ìì°ìºì ê²°ì ê° íìí©ëë¤. ê²°ì íìê² ìµëê¹?") == true) {
                                    var billingDomainUrl = "https://billing.wownet.co.kr";
                                    var billingPurchasePath = "/fillup/pop_purchase_select.asp";
                                    var billingParameters = "?priceid=" + data.PriceId + "&itemorpack=1&price=" + data.TvProgramPoint + "&cpitemid=VOD_" + num;
                                    var billUrl = billingDomainUrl + billingPurchasePath + billingParameters;
                                    window.open(billUrl, "CashPayment", "status=no,toolbar=no,scrollbars=auto,resizable=no,width=600,height=535");
                                }
                            }
                            else {
                                if (confirm("ìì°ìºì ê²°ì ê° íìí©ëë¤. ìì°ìºì ì¶©ì  íì´ì§ë¡ ì´ëíìê² ìµëê¹?") == true) {
                                    window.open("https://billing.wownet.co.kr/fillup/pop_payment_select.asp", "cash_add", "status=no,toolbar=no,scrollbars=auto,resizable=no,width=600,height=608");
                                }
                            }
                        }
                    }
                }
            });
        },
        PlayVod: function (num) {
            $.ajax({
                type: 'GET',
                data: { num: num },
                url: "/Component/TvPlayer/GetVodInfo",
                success: function (data) {
                    if (data.IsSuccess == true) {
                        var vodUrl = data.Url;
                        vodUrl = vodUrl.replace("http://cdnvod.wowtv.co.kr:8080/", "https://cdnvod.wowtv.co.kr/wowtvhome/_definst_/mp4:") + "/playlist.m3u8";;
                        
                        playplayer(vodUrl, false);
                    }
                    else {
                        if (data.ReturnCode == "NUM_ERROR") {
                            alert("ìëª»ë ì ê·¼ìëë¤.");
                        }
                    }
                }
            });
        },
        StopVod: function () {
            try {
                player_new.stop();
                player_new.closeAllMenu();
                player_new.destroy();
                player_new = null;
            } catch (e) {
            }
        },
        PlayLiveAudio: function () {
            $("#tvPlayerSwfContainerFrame").html("");
            TvPlayer.StopVod();
            $("#audioPlayerSwfContainer").show();
            LiveAudioPlayer.Play();
        }
    }

    $(document).keydown(function (e) {
        switch (e.keyCode) {
            case 37:
                $('.mpv-previous-skip-btn.mpv-button.mpv-bctrl-btn').click()
                // Previous
                return false;
            case 39:
                // Next
                $('.mpv-next-skip-btn.mpv-button.mpv-bctrl-btn').click()
                return false;
        }
    });
</script>



