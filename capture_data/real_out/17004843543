</table></td></tr><tr><!-- 首頁 -自訂廣告圖框架 2 --><td height="50" colspan="5" align="center" valign="bottom" bgcolor="#FFFFFF" class="w03"><table width="100%" border="0" cellspacing="0" cellpadding="0"><tr><td></td></tr></table></td></tr></table><script type="text/javascript">var jssor_1_slider
var jssor_1_slider_init = function() {
    var jssor_1_SlideshowTransitions = [{$Duration : 1200, $Opacity : 2}];
    var jssor_1_options = {
        $AutoPlay: true,
        $SlideshowOptions : {
            $Class : $JssorSlideshowRunner$,
            $Transitions : jssor_1_SlideshowTransitions,
            $TransitionsOrder : 1
        },
        $ArrowNavigatorOptions : {
            $Class : $JssorArrowNavigator$
        },
        $BulletNavigatorOptions : {
            $Class : $JssorBulletNavigator$
        }
    };

    jssor_1_slider = new $JssorSlider$('jssor_1', jssor_1_options);

    function ScaleSlider() {
        var refSize = jssor_1_slider.$Elmt.parentNode.clientWidth;
        if (refSize) {
            refSize = Math.min(refSize, 1200);
            jssor_1_slider.$ScaleWidth(refSize);
        }else{
            window.setTimeout(ScaleSlider, 60);
        }
    }
    ScaleSlider();
    $Jssor$.$AddEvent(window, "load", ScaleSlider);
    $Jssor$.$AddEvent(window, "resize", $Jssor$.$WindowResizeFilter(window, ScaleSlider));
    $Jssor$.$AddEvent(window, "orientationchange", ScaleSlider);

    jssor_1_slider.$On($JssorSlider$.$EVT_PARK, SlideshowEndEventHandler);

    const videoIndex = $('.slides').toArray()
        .map(function (element, index) {
            return element.innerHTML.includes('video') ? index : -1;
        })
        .filter(function (item) {
            return item >= 0
        })

    const youtubeIndexies = $('.slides').toArray()
        .map(function (element, index) {
            return element.innerHTML.includes('iframe') ? index : -1;
        })
        .filter(function (item) {
            return item >= 0
        })

    function SlideshowEndEventHandler(position) {
        if (videoIndex.includes(position)) {
            $('video').toArray().forEach(function (player) {
                player.currentTime = 0
                player.play()
            });

            jssor_1_slider.$Pause();

            return
        }

        if (youtubeIndexies.includes(position)) {
            const player = $('.slides [data-slide-youtube]')
                .eq(youtubeIndexies.findIndex((x) => x == position))
                .data('slide-youtube')

            player.seekTo(0)
            player.playVideo()

            jssor_1_slider.$Pause();

            return
        }

        jssor_1_slider.$Play();
    }

    $("#jssor_1 video").on("ended", function(event) {
        if ($(event.target).parents('.slides').data('slide-index') == jssor_1_slider.$CurrentIndex()) {
            jssor_1_slider.$Next();
        }
    });
};

$(function() {

jssor_1_slider_init();


});
</script><script>var tag = document.createElement('script');
tag.src = "https://www.youtube.com/iframe_api";
var firstScriptTag = document.getElementsByTagName('script')[0];
firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
function onYouTubeIframeAPIReady() {
$('.slides [data-slide-youtube] iframe').each((key, value) => {
$(value).parent().data('slide-youtube', new YT.Player($(value)[0], {
width: '100%',
height: '100%',
playerVars: {
playsinline: 1,
controls: 0,
autoplay: 0,
showinfo: 0,
autohide: 1,
loop: 0,
modestbranding: 1
},
events: {
'onReady': onPlayerReady,
'onStateChange': onPlayerStateChange
}
}))
});
}
// 4. The API will call this function when the video player is ready.
function onPlayerReady(event) {
event.target.mute();
}
function onPlayerStateChange(event) {
if ([YT.PlayerState.ENDED].includes(event.data)) {
event.target.seekTo(event.target.getDuration() - 1)
event.target.pauseVideo()
if ($(event.target.i).parent().data('slide-index') == jssor_1_slider.$CurrentIndex()) {
jssor_1_slider.$Next();
}
}
}
