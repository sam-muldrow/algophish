<style>
    .primary-header {
        position: relative;
    }

        .primary-header .fontsize {
            position: absolute;
            right: 32px;
            top: 12px;
        }

            .primary-header .fontsize a {
                color: #fff;
            }

    @media only screen and (max-width: 768px) {

        .primary-header .fontsize {
            display: none;
        }
    }
</style>


<script>
    $(document).ready(function () {

        $("#btninc").click(function () {
            var fontSize = "16px";
            $("a").css({ 'font-size': fontSize });
            $("p").css({ 'font-size': fontSize });
            $("li").css({ 'font-size': fontSize });
            $("span").css({ 'font-size': fontSize });
            $("div").css({ 'font-size': fontSize });
            $("body").css({ 'font-size': fontSize });
            $("td").css({ 'font-size': fontSize });
            $("select").css({ 'font-size': fontSize });
        });

        $("#btnnrml").click(function () {
            var fontSize = "";
            $("a").css({ 'font-size': fontSize });
            $("p").css({ 'font-size': fontSize });
            $("li").css({ 'font-size': fontSize });
            $("span").css({ 'font-size': fontSize });
            $("div").css({ 'font-size': fontSize });
            $("body").css({ 'font-size': fontSize });
            $("td").css({ 'font-size': fontSize });
            $("select").css({ 'font-size': fontSize });
        });

        $("#btndec").click(function () {
            var fontSize = "12px";
            $("a").css({ 'font-size': fontSize });
            $("p").css({ 'font-size': fontSize });
            $("li").css({ 'font-size': fontSize });
            $("span").css({ 'font-size': fontSize });
            $("div").css({ 'font-size': fontSize });
            $("body").css({ 'font-size': fontSize });
            $("td").css({ 'font-size': fontSize });
            $("select").css({ 'font-size': fontSize });
        });

    });
</script>

                    
