    <a id="btndec" href="javascript:void(0);" title="Decrease text size" style="outline: none;">A-</a> &nbsp; 
                    <a id="btnnrml" href="javascript:void(0);" title="Normal text size" style="outline: none;">A</a> &nbsp;
                     <a id="btninc" href="javascript:void(0);" title="Increase text size" style="outline: none;">A+</a>
