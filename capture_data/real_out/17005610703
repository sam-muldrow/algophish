<script>
    (function() {
        function initOneTrustCss() {
            var link = document.createElement("link");
            link.type = "text/css";
            link.rel = "stylesheet";
            link.href = "/on/demandware.static/Sites-dsglcom_prod_us-Site/-/en_US/v1700547785231/css/oneTrust.css";
            document.head.appendChild(link);
        }
        function showCookieBanner() {
            var banner = document.getElementById("onetrust-banner-sdk");
            if (banner) {
                banner.style.display = 'block';
                var acceptButton = document.getElementById("onetrust-accept-btn-handler");
                if (acceptButton) {
                    acceptButton.onclick = function() {
                        banner.style.display = 'none';
                        if (window.consentTracking) {
                            window.consentTracking(window.OptanonActiveGroups, false);
                        }
                    };
                }
            }
        }
        var isShowCookieBanner = !window.getCookie('ACCEPTED_COOKIES');
        if (isShowCookieBanner) {
            initOneTrustCss();
            showCookieBanner();
        }
    })();
</script>
