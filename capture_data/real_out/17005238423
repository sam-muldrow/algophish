    .pt_storefront .mini-cart-header {
      color: #000;
    }
  
    /* removing gray borders around menu items */
    .l-header-inner,
    .b-menu_bar-tab {
      border-color: black
    }
  
    @media screen and (min-width: 1025px) {
  
      .b-menu_bar-tab_content_wrapper,
      .b-menu_bar-tab_content_item:not(.m-offers),
      .b-menu_bar-tab_content_item:not(.m-offers):hover,
      .b-menu_bar-tab_content_link:not(.m-offers).m-has-submenu::after,
