        <script>
            
            var consentStatus = 0;
            
            function StartPGTracker(consentStatus){
                var metadata = {
                    gdpr: consentStatus,
                    gdpr_consent: consentStatus == 1 ? "BOzs9x4Ozs9x4AcABBTRDJ-AAAAvh7_______9______9uz_Ov_v_f__33e8__9v_l_7_-___u_-33d4u_1vf99yfm1-7etr3tp_87ues2_Xur__71__3z3_9pxP78k89r7337Ew_v-_v-b7BCPN9Y3v-8K4"  : "",
                    ccpa: null,
                    bp_id: "foxtv",
                    
                };

                var config = {
                    name: "P&G",
                    pixelUrl: "https://pandg.tapad.com/tag"
                };

                var tagger = Tapad.init(metadata, config);


                var data = {
                    category : 'Anasayfa'
                };

                tagger.sync(data);
            }

            $(document).ready(function(){
                if(Cookies.get('cookiepolicy') == 'true')
                {
                    consentStatus = 1;
                    $('.consent-banner').remove();

                    StartPGTracker(1)
                }else{                    
                    $('.consent-banner .icon').on('click', function(e){
                        $('.consent-banner').remove();
                        Cookies.set('cookiepolicy', 'true', { expires: 365 });
                        consentStatus = 1;       
                        StartPGTracker(1)               
                    })

                    StartPGTracker(0)
                }           
            });   
        </script>     
    </body>
</html>