        <script>
            var trgt = 'Anasayfa';
            var size = window.innerWidth;
            var slots = [];
            var ad = {};
            var adUnitPrefix = (mobilecheck() == true ? "Foxtv_Mobile" : "Foxtv_desktop");
            var adUnitCategory = '';
            var adUnitContentType = '';

                                     
            adUnitCategory = 'Dizi';
                        

                        adUnitContentType = 'Diger';
            
            
            
                 

                       
            googletag.cmd.push(function () {


                

                
                /*
                
                ad = {};
                ad.id = 'div-gpt-ad-promoted-banner';
                ad.slot = googletag.defineSlot('/113421212/Foxtv_Desktop/Anasayfa/Genel/Promoted_Banner',  ['fluid', [336, 280], [970, 250], [1600, 320]], 'div-gpt-ad-promoted-banner').addService(googletag.pubads());
                ad.refresh = false;
                ad.refresh_time = 20;
                ad.max_refresh_count = 10;
                slots[slots.length] = ad;  
                
                ad = {};
                ad.slot.defineSizeMapping(googletag.sizeMapping()
                .addSize([1920, 500], [1600, 320])
                .addSize([1200, 400], [970, 250])
                .addSize([640, 300], [336, 280])
                .addSize([320, 300], [336, 280])
                .addSize([0, 0], [])
                .build());

                */

                
                var blockStatus = 'false';
                
                

                
                googletag.pubads().enableSingleRequest();
                googletag.pubads().collapseEmptyDivs(true);
                googletag.pubads().disableInitialLoad();
                googletag.pubads().setTargeting('Block-Status', [blockStatus]);

                googletag.pubads()
                                .setTargeting('Fox_Kategori', ['Anasayfa'])
                                .setTargeting('Environment', ['staging'])
                                .setTargeting('Video_Tur', ['']);

                googletag.enableServices();                                            

                var adTimer = setInterval(function(){

                    if($('.video-js.vjs-fullscreen').length > 0)
                    {

                    }else{
                        for(var s = 0; s < slots.length; s++){
                        var diff = Math.abs(new Date() - slots[s].started_at);
                        var seconds = Math.floor((diff/1000));
                        
                        if(slots[s].inited == true && slots[s].force_refresh){
                            if(slots[s].refresh_count < slots[s].max_refresh_count && slots[s].refresh == true && seconds > slots[s].refresh_time){
                                slots[s].started_at  = new Date();
                                slots[s].refresh_count++;
                                googletag.pubads().refresh([slots[s].slot]);
                            }

                        }else{
                            if(slots[s].inited == true && (slots[s].in_view_percentage >= 75 || slots[s].in_view_percentage == null) && slots[s].refresh_count < slots[s].max_refresh_count && slots[s].refresh == true && seconds > slots[s].refresh_time){
                                slots[s].started_at  = new Date();
                                slots[s].refresh_count++;
                                googletag.pubads().refresh([slots[s].slot]);
                            }
                        }
                    }                   
                    }                                   
                }, 1000);

                googletag.pubads().addEventListener('slotVisibilityChanged', function (event) {           
                    var id = event.slot.getSlotElementId();
                    for (var i = 0; i < slots.length; i++) {
                        if(slots[i].id == id)
                        {      
                            slots[i].in_view_percentage = event.inViewPercentage;
                        }
                    }        
                });    

                setTimeout(function(){
                    for(var s = 0; s < slots.length; s++){
                        slots[s].started_at  = new Date();            
                        slots[s].refresh_count  = 0;                               
                        slots[s].in_view_percentage  = null;  

                        if(typeof slots[s].force_refresh === 'undefined'){
                           slots[s].force_refresh  = false;   
                        }

                        if(typeof slots[s].max_refresh_count === 'undefined'){
                           slots[s].max_refresh_count  = 3;   
                        }

                        if(typeof slots[s].refresh === 'undefined'){
                           slots[s].refresh  = false;   
                        }
                    }
                }, 5000);                
                
            });



            function removeElement(id) {
                var elem = document.getElementById(id);
                return elem.parentNode.removeChild(elem);
            }

            
        </script>
