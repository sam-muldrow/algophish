        var nextVideoTimer = setInterval(function(){
        if($('.adm-player-container').length > 0)
        {           
            var nextVideoCancelled = false;
            var sidebarPlayer = $('.sidebar-player-container .sidebar-player');
            if(sidebarPlayer.length > 0 && typeof fxPlayer != "undefined"){
                clearInterval(nextVideoTimer);
                var nextVideoImageUrl = $('.sidebar-player-container .sidebar-player img').attr('src');
                var nextVideoUrl = $('.sidebar-player-container a').attr('href') + '?utm_source=gospock&utm_medium=web&utm_content=widget&utm_campaign=next-video-recommendation&utm_id=WIDGET_NEXT_VIDEO'
                var nextVideoProgramName = "";
                if(GOSpockShared.program != null){
                    nextVideoProgramName = GOSpockShared.program.name;
                }
                var nextVideoTitle = $('.sidebar-player-container .sidebar-player-meta strong').text();
                $('.adm-player-container').not('.live-player-container').not('.mini-player-container').find('.video-js').prepend("<div class='gospock-widget-player-next-video'/>")
                $('.gospock-widget-player-next-video').append('<div class="gospock-widget-player-next-video-inner"><div class="gospock-widget-player-next-video-content"><h2>SIRADAKİ İÇERİK (<span></span>)</h2><a href="'+ nextVideoUrl +'"><div class="gospock-widget-player-next-video-image"><div class="icon-play"><div class="icon-icon-play"></div></div><img src="'+ nextVideoImageUrl+'"/></div><div class="gospock-widget-player-next-video-meta"><div class="gospock-widget-player-next-video-program-name"></div><div class="gospock-widget-player-next-video-episode-number">'+ nextVideoTitle +'</div></div></a><div class="gospock-widget-player-next-video-buttons"><div class="gospock-widget-player-next-video-button-cancel btn outlined">VAZGEÇ</div><div class="gospock-widget-player-next-video-button-playnow btn bg-red"><a href="'+ nextVideoUrl +'">İZLE</a></div></div></div></div>')

                $('.gospock-widget-player-next-video-button-cancel').on('click', function(){
                    nextVideoCancelled = true;
                    $('.gospock-widget-player-next-video').hide()
                });

                $('.gospock-widget-player-next-video-button-playnow').on('click', function(){
                    window.location = nextVideoUrl;
                });

                var nextVideoCountdownStarted = false
                var nextVideoTimeLeft = 10;
                var nextVideoCountdownTimer = setInterval(function(){
                    if(fxPlayer.ads != null && !ADMPlayer.players[0].isAdDisplayed && ADMPlayer.players[0].instance.currentTime() >  ADMPlayer.players[0].instance.duration() - 12){                        
                        if(nextVideoCountdownStarted && !nextVideoCancelled && nextVideoTimeLeft >= 0){
                            $('.gospock-widget-player-next-video .gospock-widget-player-next-video-content span').html(nextVideoTimeLeft)                                                
                            if(nextVideoTimeLeft == 0){
                                window.location = nextVideoUrl;
                            }else{
                                nextVideoTimeLeft--;
                            }
                        }  
                    }
                }, 1000);
                
                fxPlayer.on('ended', function(){
                    if(fxPlayer.ads != null && !ADMPlayer.players[0].isAdDisplayed){
                        nextVideoCountdownStarted = true;
                        $('.gospock-widget-player-next-video').show();
                        $('.gospock-widget-player-next-video').css('height', '100%');              
                    }                    
                });
            }
        }
    }, 250);        

    $.ajax({
        type: "GET",
        dataType: "json",
        url: 'https://w2epsrhsbnit.merlincdn.net/api/gateway',
        success: function (response) {
            GOSpockShared.banners.loadedItems = response.payload.banners;

            for(var i = 0; i < GOSpockShared.banners.loadedItems.length; i++){    

                var allowed = true;
                var cookie = Cookies.get('X_SPOCK_BANNERS') ;
                if(typeof cookie != "undefined")
                {                                  
                    var banners = JSON.parse(cookie);
                    var key = false;
                    banners.forEach(function(key, value){                        
                        if(key == GOSpockShared.banners.loadedItems[i].id){
                            allowed = false                            
                        }
                    });                 
                } 

                if(GOSpockShared.program != null && GOSpockShared.banners.loadedItems[i].visibility_filters != null)
                {
                    if(GOSpockShared.banners.loadedItems[i].visibility_filters.excluded_programs != null && GOSpockShared.banners.loadedItems[i].visibility_filters.excluded_programs.length > 0)
                    {
                        GOSpockShared.banners.loadedItems[i].visibility_filters.excluded_programs.forEach(function(id){
                            if(GOSpockShared.program.id == id){
                                allowed = false;
                            }
                        })                        
                    }
                }

                if(allowed){
                    if(typeof GOSpockShared.banners.groupedItems[GOSpockShared.banners.loadedItems[i].type_system_name] == "undefined"){
                        GOSpockShared.banners.groupedItems[GOSpockShared.banners.loadedItems[i].type_system_name] = new Array();
                    }

                    GOSpockShared.banners.groupedItems[GOSpockShared.banners.loadedItems[i].type_system_name].push(GOSpockShared.banners.loadedItems[i]);
                }                                
            }   
            
            for (var key in GOSpockShared.banners.groupedItems) {
                var index = Math.floor(Math.random() * GOSpockShared.banners.groupedItems[key].length);
                GOSpockShared.banners.create(GOSpockShared.banners.groupedItems[key][index]);
            }
        }
    });

</script>
   
        <script>
            $(function() {

                
                var isMobile = window.mobilecheck();

                $.fn.visible = function(){

                    var win = $(window);

                    var viewport = {
                        top : win.scrollTop(),
                        left : win.scrollLeft()
                    };
                    viewport.right = viewport.left + win.width();
                    viewport.bottom = viewport.top + win.height();

                    var bounds = this.offset();
                    bounds.right = bounds.left + this.outerWidth();
                    bounds.bottom = bounds.top + this.outerHeight();

                    var avgHeight = parseInt(this.attr('data-avg-height'));

                    if(isNaN(avgHeight)){
                        avgHeight = 100;
                    }
                    

                    var topPercent = ((win.height() - (this.offset().top - $(window).scrollTop())) / avgHeight) * 100;                

                    return topPercent >= 50 && (!(viewport.right < bounds.left || viewport.left > bounds.right || viewport.bottom < bounds.top || viewport.top > bounds.bottom));

                };
                
                $('.lazyload-ads').each(function(inx,elm){

                    $(elm).height(1).show();

                    var id = $(elm).attr('id');
                    var type = $(elm).attr('data-type');

                    if($(elm).visible())
                    {
                        var initable = true;
                        
                        if($('body').hasClass('has-video'))
                        {
                            if(typeof fxPlayer == 'undefined')
                            {
                                initable = false;
                            }                            
                        }

                        for (var i = 0; i < slots.length; i++) {
                            if(slots[i].id == id)
                            {
                                $(elm).css('height', 'auto').removeClass('lazyload-ads').addClass('ad-inited');
                                if($(elm).hasClass('require-closer')){
                                    setTimeout(function(){                                        
                                        $(elm).prepend('<a href="#" class="ad-closer"><span class="icon-icon-close"></span> REKLAMI GİZLE</a>');
                                        $('.ad-closer').on('click', function(e){
                                            e.preventDefault();
                                            $(this).parent('.ad-inited').slideUp(250);
                                        });
                                    }, 1000);
                                }

                                slots[i].inited = true;
                                googletag.cmd.push(function(){
                                    googletag.display(id);
                                    googletag.pubads().refresh([slots[i].slot]);
                                });

                            }
                        }
                    }
                });

                setTimeout(function(){
                    $('.none-lazyload-ads').each(function(inx,elm){
                        var id = $(elm).attr('id');
                        for (var i = 0; i < slots.length; i++) {
                            if(slots[i].id == id)
                            {                            
                                $(elm).removeClass('none-lazyload-ads').addClass('ad-inited');
                                if($(elm).hasClass('require-closer')){
                                    setTimeout(function(){
                                        $(elm).prepend('<a href="#" class="ad-closer"><span class="icon-icon-close"></span> REKLAMI GİZLE</a>');
                                        $('.ad-closer').on('click', function(e){
                                            e.preventDefault();
                                            $(this).parent('.ad-inited').slideUp(250);
                                        });
                                    }, 1000);
                                }

                                slots[i].inited = true;
                                googletag.cmd.push(function(){
                                    googletag.display(id);
                                    if(slots[i].id != 'div-gpt-ad-1576136486311-0'){
                                        googletag.pubads().refresh([slots[i].slot]);
                                    }                                    
                                });

                            }
                        }
                    });
                }, 2000);
                
                setInterval(function(){
                    
                    if($('#div-gpt-ad-1638436733135-0').length > 0){
                        if($('#div-gpt-ad-1638436733135-0').hasClass('ad-inited') && $('#div-gpt-ad-1638436733135-0').height() > 0){
                            if(!$('#div-gpt-ad-1638436733135-0').hasClass('shown')){
                                $('#div-gpt-ad-1638436733135-0').addClass('shown');
                                $('body').addClass('interstitial-showing');
                                $('body').prepend('<div class="interstitial-overlay"></div>');
                                setTimeout(function(){
                                    $('#div-gpt-ad-1638436733135-0').prepend('<div class="interstitial-close-wrap"><div class="interstitial-close"><img src="/img/icons/icon-close.svg"></div></div>');
                                    $('.interstitial-close-wrap .interstitial-close').on('click', function(){
                                        $('body').removeClass('interstitial-showing');
                                        $('#div-gpt-ad-1638436733135-0').fadeOut(250, function(){
                                            $('#div-gpt-ad-1638436733135-0').html('');
                                        });
                                    });
                                }, 2500)
                            }    
                        }
                    }

                    $('.lazyload-ads').each(function(inx,elm){

                            var id = $(elm).attr('id');
                            var type = $(elm).attr('data-type');

                            if($(elm).visible())
                            {                                    
                                for (var i = 0; i < slots.length; i++) {
                                    if(slots[i].id == id)
                                    {
                                        $(elm).css('height', 'auto').removeClass('lazyload-ads').addClass('ad-inited');                                            

                                        slots[i].inited = true;
                                        googletag.cmd.push(function(){
                                            googletag.display(id);
                                            googletag.pubads().refresh([slots[i].slot]);
                                        });

                                    }
                                }
                            }

                    });  
                }, 1000);

                setTimeout(function(){
                    $(window).on('load resize scroll', function() {
                        if($('.footer-logo').visible() && !$('.footer-logo').hasClass('shown') ){
                            $('.footer-logo').addClass('shown');
                            ga('send', 'event', 'FOOTER_LOGO', 'VIEWED');
                        }
                    }); 
                }, 3000);
            });
