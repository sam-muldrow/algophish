<script>    

    var GOSpockShared = {
        program : null,
        banners : {
            types : [
                'BANNER_TYPE_STANDARD_TOP_ALIGNED',
                'BANNER_TYPE_STANDARD_CENTER_ALIGNED_MODAL_BOX',
                'BANNER_TYPE_COUNTDOWN_TOP_ALIGNED',
                'BANNER_TYPE_COUNTDOWN_CENTER_ALIGNED_MODAL_BOX',
                'BANNER_TYPE_WIDGET_TOP_RIGHT_ALIGNED',
                'BANNER_TYPE_WIDGET_CENTER_RIGHT_ALIGNED',
                'BANNER_TYPE_WIDGET_BOTTOM_RIGHT_ALIGNED'
            ],
            loadedItems : [],
            groupedItems : [],
            onViewed : function(item){
                var allowed = true;
                var cookie = Cookies.get('X_SPOCK_BANNERS');
                if(typeof cookie != "undefined")
                {                                  
                    var banners = JSON.parse(cookie);
                    var key = false;
                    banners.forEach(function(key, value){                        
                        if(key == item.id){
                            allowed = false                            
                        }
                    });

                    if(allowed){
                        banners.push(item.id);
                        Cookies.set('X_SPOCK_BANNERS', JSON.stringify(
                            banners
                        ), {
                            expires : 1
                        });       
                    }                    
                }else{
                    var banners = new Array();
                    var key = false;
                    banners.push(item.id);
                    Cookies.set('X_SPOCK_BANNERS', JSON.stringify(
                        banners
                    ), {
                        expires : 1
                    });          
                }
            },
            create : function(item){
                var ww = $(window).width();
                var allowed = true;
                var cookie = Cookies.get('X_SPOCK_BANNERS') ;
                if(typeof cookie != "undefined")
                {                                  
                    var banners = JSON.parse(cookie);
                    var key = false;
                    banners.forEach(function(key, value){                        
                        if(key == item.id){
                            allowed = false                            
                        }
                    });                 
                } 

                if(allowed){
                    if(item.type_system_name == 'BANNER_TYPE_STANDARD_TOP_ALIGNED'){
                        $('body').prepend('<div id="gospock-banner-' + item.id +'" class="gospock-banner gospock-banner-standard-top-aligned"/>')
                        $('#gospock-banner-' + item.id).append('<div class="gospock-banner-inner"><div class="gospock-banner-image-container"><a href="'+ item.button_target_url + '?utm_source=gospock&utm_medium=web&utm_content=banner&utm_campaign='+ item.slug +'&utm_id=BANNER_TYPE_STANDARD_TOP_ALIGNED"><img src="'+ item.cdn_image_url +'"/></a></div><div class="gospock-banner-content-container"> <div class="gospock-banner-content-message"><strong>'+ item.title +'</strong> <br> ' + item.body +' </div> <div class="gospock-banner-cta"><a href="'+ item.button_target_url + '?utm_source=gospock&utm_medium=web&utm_content=banner&utm_campaign='+ item.slug +'&utm_id=BANNER_TYPE_STANDARD_TOP_ALIGNED">' + item.button_label + '</a></div>  </div></div> <div class="gospock-banner-closer" id="gospock-banner-closer-'+ item.id +'"><img src="/img/icons/icon-close.svg"></div>');                    
                        setTimeout(function(){
                            $('#gospock-banner-' + item.id).animate({
                                'height' : '100px',                        
                            })

                            $('#gospock-banner-closer-' + item.id).on('click', function(){
                                
                                $('#gospock-banner-' + item.id).animate({
                                    'height' : '0',                        
                                }, function(){
                                    $('#gospock-banner-' + item.id).remove();
                                });

                                GOSpockShared.banners.onViewed(item);
                                  
                            });

                            if(item.timeout > 0){
                                setTimeout(function(){
                                    $('#gospock-banner-' + item.id).animate({
                                        'height' : '0',                        
                                    }, function(){
                                        $('#gospock-banner-' + item.id).remove();
                                    });
                                }, item.timeout * 1000)
                            }

                            $(window).on('scroll', function(){
                                if($('#gospock-banner-' + item.id).length > 0)
                                {
                                    if(window.scrollY > 90){
                                        $('body.menu-fixed header.main').css({
                                            'margin-top' : '0'
                                        })
                                    }else if(window.scrollY > 0 ){
                                        $('body.menu-fixed header.main').css({
                                            'margin-top' : (95 - parseInt(window.scrollY)) + 'px'
                                        })
                                    }else{
                                        $('body header.main').css({
                                            'margin-top' : '0px'
                                        })
                                    }     
                                }else{
                                    $('body header.main').css({
                                        'margin-top' : '0px'
                                    })
                                }                                                   
                            })

                        }, item.delay * 1000)
                    }

                    if(item.type_system_name == 'BANNER_TYPE_STANDARD_CENTER_ALIGNED_MODAL_BOX'){
                        $('body').prepend('<div id="gospock-banner-' + item.id +'" class="gospock-banner gospock-banner-center-top-modal-box"/>')
                        if(item.title != null && item.title != "" && item.body != null && item.body != "")
                        {
                            $('#gospock-banner-' + item.id).append('<div class="gospock-banner-inner"><div class="gospock-banner-inner-relative"><div class="gospock-banner-image-container"><a href="'+ item.button_target_url + '?utm_source=gospock&utm_medium=web&utm_content=banner&utm_campaign='+ item.slug +'&utm_id=BANNER_TYPE_STANDARD_CENTER_ALIGNED_MODAL_BOX"><img src="'+ item.cdn_image_url +'"/></a></div><div class="gospock-banner-content-container"> <div class="gospock-banner-content-message"><strong>'+ item.title +'</strong> <br> ' + item.body +' </div> <div class="gospock-banner-cta"><a href="'+ item.button_target_url + '?utm_source=gospock&utm_medium=web&utm_content=banner&utm_campaign='+ item.slug +'&utm_id=BANNER_TYPE_STANDARD_CENTER_ALIGNED_MODAL_BOX">' + item.button_label + '</a></div> </div><div class="gospock-banner-closer" id="gospock-banner-closer-'+ item.id +'"><img src="/img/icons/icon-close.svg"></div></div></div>');
                        }else{
                            $('#gospock-banner-' + item.id).append('<div class="gospock-banner-inner"><div class="gospock-banner-inner-relative"><div style="width:100%!important;max-width:100%!important" class="gospock-banner-image-container"><a href="'+ item.button_target_url + '?utm_source=gospock&utm_medium=web&utm_content=banner&utm_campaign='+ item.slug +'&utm_id=BANNER_TYPE_STANDARD_CENTER_ALIGNED_MODAL_BOX"><img src="'+ item.cdn_image_url +'"/></a></div><div class="gospock-banner-closer" id="gospock-banner-closer-'+ item.id +'"><img src="/img/icons/icon-close.svg"></div></div></div>');
                        }
                        

                        $('#gospock-banner-' + item.id).on('click', function(e){
                            GOSpockShared.banners.onViewed(item);
                        })

                        if(item.on_scrolled == 1){
                            setTimeout(function(){
                                $(window).on('scroll', function(){
                                    if(window.scrollY > parseInt(item.scroll_offset)){
                                        $('#gospock-banner-closer-' + item.id).on('click', function(){
                                            $('#gospock-banner-' + item.id).remove();
                                            GOSpockShared.banners.onViewed(item);
                                        });

                                        if(item.timeout > 0){
                                            setTimeout(function(){
                                                $('#gospock-banner-' + item.id).fadeOut(250, function(){
                                                    $('#gospock-banner-' + item.id).remove();
                                                });
                                            }, item.timeout * 1000)
                                        }

                                        $('#gospock-banner-' + item.id).fadeIn(250);
                                    }
                                });
                            }, 1000)
                        }else{
                            setTimeout(function(){                        
                                $('#gospock-banner-closer-' + item.id).on('click', function(){
                                    $('#gospock-banner-' + item.id).remove();
                                    GOSpockShared.banners.onViewed(item);
                                });

                                if(item.timeout > 0){
                                    setTimeout(function(){
                                        $('#gospock-banner-' + item.id).fadeOut(250, function(){
                                            $('#gospock-banner-' + item.id).remove();
                                        });
                                    }, item.timeout * 1000)
                                }

                                $('#gospock-banner-' + item.id).fadeIn(250);

                            }, item.delay * 1000)
                        }                                            
                    }

                    if(item.type_system_name == 'BANNER_TYPE_WIDGET_TOP_RIGHT_ALIGNED'){

                        if(typeof item.message != "undefined" && item.message != null && item.message.length > 0){
                            message = '<br>' + item.message;
                        }

                        $('body').prepend('<div id="gospock-banner-' + item.id +'" class="gospock-banner gospock-banner-widget-top-right-aligned"/>')
