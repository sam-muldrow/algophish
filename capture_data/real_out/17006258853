        $(window).on('resize', function(){
            var ww = $(window).width();
            $('.slider-wrapper .slides .slide .slide-bg').each(function(index, element){
                var src = $(element).attr('data-src');
                var mobileSrc = $(element).attr('data-mobile-src');                         

                if(ww < 768){
                    $(element).attr('style', 'background-image: url('+ mobileSrc +')');
                }else{
                    $(element).attr('style', 'background-image: url('+ src +')');
                }
            });

            $('.promoted-content .p-content .p-content-bg').each(function(index, element){
                var src = $(element).attr('data-src');
                var mobileSrc = $(element).attr('data-mobile-src');
                if(ww < 768){
                    $(element).attr('style', 'background-image: url('+ mobileSrc +')');
                }else{
                    $(element).attr('style', 'background-image: url('+ src +')');
                }
            });
