    <script>
        // display ad if one exisits
        $(document).ready(function() {

                AdvertisementUtils.showAdAbovePlannerBlockIfNeeded();
        });

        $.fn.isInViewport = function() {
            var elementTop = $(this).offset().top;
            var elementBottom = elementTop + $(this).outerHeight();
            var viewportTop = $(window).scrollTop();
            var viewportBottom = viewportTop + $(window).height();
            return elementBottom > viewportTop && elementTop < viewportBottom;
        };

        $(window).on('load resize scroll',
            function() {
                if ($('#contentSection').isInViewport()) {
                    if (!$("#homeContent").length) {

                        var url = " /GenerateHomeContentBlockViewModelPost";
                        $.get(url)
                            .done(function(data) {
                                $('#contentCol').html(data);
                            })
                            .fail(function() {
                                $('#contentCol').html($(this).data('modal-error'));
                            })
                            .always(function() {
                                //do nothing
                            });
                    };
                };

                var header = $(".newMenu");
                var scroll = $(window).scrollTop();

                if (scroll >= 200) {
                    header.addClass("scrolled");
                } else {
                    header.removeClass("scrolled");
                };

            });
    </script>


