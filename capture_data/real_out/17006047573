<script>
    $(function () {

      $('#date').val('21-11-2023');

      $('#date').attr({
        "max": '2023-12-23',
        "min": '2023-11-20'
      });

      $('#datepicker').datetimepicker({
        viewMode: 'days',
        locale:  moment().local('NL'),
        format: 'DD-MM-YYYY',
        minDate: '2023-11-20',
        maxDate: '2023-12-23',
        keepInvalid: false,
        widgetPositioning: {
          horizontal: 'auto',
            vertical: 'auto'
        },
        allowInputToggle: true,
        inline: false
      });

      $('#timepicker').datetimepicker({
        format: 'LT',
        locale:  moment().local('NL'),
        widgetPositioning: {
            horizontal: 'auto',
            vertical: 'auto'
        },
        allowInputToggle: true,
        inline: false
      });

      combineAndSwapDateTime();

    });

    $("#datepicker, #timepicker").on("dp.show", function (e) {
        $('.tooltip').tooltip('hide');
        return true;
    });
</script>
