    var getAdmanagerPPID = function() {
        var cookies = document.cookie.split(";");
        var ppid = "";

        for (var i = 0; i < cookies.length; i++) {
            var cookie = cookies[i].trim();
            var kv = cookie.split("=");
            if (kv.length !== 2) {
                continue;
            }

            if (kv[0] === "gam_ppid") {
                ppid = kv[1];
                break;
            }
        }

        return ppid;
    };
    
