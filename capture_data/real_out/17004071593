        <script>
        var timeGap = [1, 2, 4, 8];
        var intervalId;
        var workerIntervalId = 'metricsInterval';
        var worker;
        var isError = false;

        function sendMetrics(n) {
            var payload = {
                metricName: 'full_page_load',
                data: Date.now(),
                device: 'desktop',
                isModernBrowser: false,
                city: 'Москва',
                url: '/'
            };

            fetch("https://alfabank.ru" + '/conductor/metrics', {
                method: 'POST',
                credentials: 'include',
                headers: { "Content-Type": "application/json" },
                body: JSON.stringify(payload)
            })
            .then(function(res) {
                if (res.status !== 200) {
                    throw new Error();
                }
                res.json();
                if (isError) {
                    isError = false;
                    if (worker) {
                        worker.postMessage({
                            command: 'setInterval',
                            id: workerIntervalId,
                            delay: 118000,
                        });
                    } else {
                        intervalId = setInterval(sendMetrics, 118000, 0);
                    }
                }
            })
            .catch(function() {
                isError = true;
                if (worker) {
                    worker.postMessage({
                        command: 'clearInterval',
                        id: workerIntervalId,
                    });
                } else {
                    clearInterval(intervalId); 
                }
                if (n >= timeGap.length) {
                    return;
                }
                if (worker) {
                    worker.postMessage({
                        command: 'setTimeout',
                        id: 'errorTimeout',
                        delay: timeGap[n] * 1000,
                    });
                    worker.addEventListener('message', function (e) {
                        if (e.data === 'errorTimeout') {
                            sendMetrics(n + 1);
                        }
                    }, { once: true });
                } else {
                    setTimeout(sendMetrics, timeGap[n] * 1000, n + 1);
                }
            });
        }

        sendMetrics(0);

        if (typeof(Worker) !== 'undefined') {
            worker = new Worker('data:text/javascript;base64,');
            worker.postMessage({
                command: 'setInterval',
                id: workerIntervalId,
                delay: 118000,
            });
            worker.addEventListener('message', function (e) {
                if (e.data === workerIntervalId) {
                    sendMetrics(0);
                }
            });
        } else {
            intervalId = setInterval(sendMetrics, 118000, 0);
        }
    </script>
    </head>
