            $('#ilmeteonews,.home-news-top,#news-topbox-container-masthead,#box-special-title-masthead').css('display','none');
            _paq.push(['trackEvent', 'news','homepage-news-nascoste']); //su mobile non invio l'evento sul click essendo già inviato dopo il refresh

            $('#buttonHideNews a').text('Mostra news');
            $.cookie("news nascoste", 1, { expires : 30 });
        }

    }
