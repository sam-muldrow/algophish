<script type="text/javascript">
var newsTopBoxScrollingmasthead=false;
$(document).ready(function()
{
	$('#news-topbox-right-masthead').click(function(){
		if(newsTopBoxScrollingmasthead) return;
		newsTopBoxScrollingmasthead = true;
		var news = $('#news-topbox-container2-masthead').find('.singlenews:first');
		news.animate({marginLeft: -news.outerWidth()}, 400, 'swing',
			function(){
				$(this).insertAfter($('#news-topbox-container2-masthead').find('.singlenews:last'));
				$(this).css('marginLeft', 0);
				newsTopBoxScrollingmasthead = false;
			}
		);
	});

	$('#news-topbox-left-masthead').click(function(){
		if(newsTopBoxScrollingmasthead) return;
		newsTopBoxScrollingmasthead = true;
		var news = $('#news-topbox-container2-masthead').find('.singlenews:last');
        news.css('marginLeft', -news.outerWidth());
        news.insertBefore($('#news-topbox-container2-masthead').find('.singlenews:first'));
		news.animate({marginLeft:0}, 400, 'swing', function(){newsTopBoxScrollingmasthead=false});
	});

});
</script>
