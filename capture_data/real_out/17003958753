            try{console.log('uc - begin')}catch(e){}

            adv_load_frame_targeting();
            ilmeteo_dfp_reload(false);

            if(isHome && h<0)checkTabCart(h);
            try{console.log('uc - end')}catch(e){}
            return true;
        }

