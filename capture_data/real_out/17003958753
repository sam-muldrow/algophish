<script type="text/javascript">
    function ilmeteo_sticky_pos()
    {
        try
        {
            sticky_offset_x = 20;
            sticky_offset_y = $('#page-menu').height() + $('#page-submenus').height();
            //	o = $('#page-body').offset();
            //	o = $('#page-shadow').offset();
            //	o = $('#banner-mnz-topleft').offset();
            o = $('#page-menu').offset();
            x = (o.left - $('#div-dfp-sticky').width() - sticky_offset_x);
            y = o.top + sticky_offset_y;
            if(y<0) y=50; //tmp
            $('#div-dfp-sticky').offset({left:x, top:y});

            // mostra l'adv solo se non c'è la skin già caricata
            if ($('#div-dfp-sticky').hasClass('with-skin')) {
                $('#div-dfp-sticky').hide();
            }
            else {
                $('#div-dfp-sticky').show();
            }
        }catch(e){}
    }
    $(window).resize(function(){ilmeteo_sticky_pos()});
</script>

