    function checkNewsHomepage(){
        if (typeof $.cookie('news nascoste') === 'string'){

            $('#ilmeteonews,.home-news-top,#news-topbox-masthead, #news-topbox-container-masthead,#box-special-title-masthead').css('display','none');
            _paq.push(['trackEvent', 'news','homepage-news-nascoste']);
            $('#buttonHideNews a').text('Mostra news');
        }
    }

    function hideShowNews(event) {

        event.preventDefault();

        if (typeof $.cookie('news nascoste') === 'string'){

            $('#ilmeteonews,.home-news-top,#news-topbox-masthead,#news-topbox-container-masthead,#box-special-title-masthead').show();

            $('#buttonHideNews a').text('Chiudi news');
