<script type="text/javascript">
var newsTopBoxScrolling=false;
$(document).ready(function()
{
	var contClass = '.home-news-top-big ';
	$(contClass + '#news-topbox-right').click(function(){
		if(newsTopBoxScrolling) return;
		newsTopBoxScrolling = true;
		var news = $(contClass + '#news-topbox-container2').find('.singlenews:first');
		news.animate({marginLeft: -news.outerWidth()}, 400, 'swing',
			function(){
				$(this).insertAfter($(contClass + '#news-topbox-container2').find('.singlenews:last'));
				$(this).css('marginLeft', 0);
				newsTopBoxScrolling = false;
			}
		);
	});

	$(contClass + '#news-topbox-left').click(function(){
		if(newsTopBoxScrolling) return;
		newsTopBoxScrolling = true;
		var news = $(contClass + '#news-topbox-container2').find('.singlenews:last');
        news.css('marginLeft', -news.outerWidth());
        news.insertBefore($(contClass + '#news-topbox-container2').find('.singlenews:first'));
		news.animate({marginLeft:0}, 400, 'swing', function(){newsTopBoxScrolling=false});
	});
});
</script>

<script type="text/javascript">
	$(document).ready(function(){//google tag assistant
		let event_name = 'open_news_from_home';
		$(".hnt-big, .singlenews > a").unbind('click').click(function(e){
			// e.preventDefault(); // enable for debug

			let _ = $(this);
			let sending = $(this).data('gtag-sending');

			if(! sending) {
				_.data('gtag-sending', 1);
				var _paq = _paq || [];
				_paq.push(['trackEvent', 'news', event_name, $(this).attr('title')]);
			}
		});
	});
</script>

</div><!-- news-topbox -->
                    

