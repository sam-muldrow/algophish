<script type="text/javascript">
let advTopLeftInterval = 0;
$(document).ready(function(){
    advTopLeftInterval = setInterval(function(){ // Nel caso di AD del masthead che copre il carosello imposta lo sfondo
		let advTopLeft = $('#adv-TopLeft');
		let h = advTopLeft.innerHeight();
		if (h>0) {
			let bg = (h > 168) ? 'white' : 'transparent';
			advTopLeft.css('background',bg);
		    clearTimeout(advTopLeftInterval);
		}
    },500);
});
</script><div class="topnews-home"; id="news-topbox-masthead">
