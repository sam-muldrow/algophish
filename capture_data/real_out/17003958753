    <script type="text/javascript">
<!--
	var channels=13;  // numero canali
	var nextTab=null;  // menu canale in fase di apertura
	var defTab=1;  // menu canale di default
	var currTab=defTab;  // menu canale attivo
	var hiding=false;  // menu in fase di chiusura (annullabile)
	var timerMenu=null;
	var timerSubMenu=null;

	var isHome = true;
	//var nextTab=null;var defTab=1
	//function mcanOver(tab){if(nextTab!=tab){nextTab=tab;setTimeout("mcanChange()", 150)}}
	//function mcanOut(tab){nextTab=null}
	function mcanChange()
	{//console.log('mcanChange()');
		if( typeof page_rating_toggle === 'function' ) page_rating_toggle(false);
		if( nextTab == true ) return;

		if( nextTab != null )
		{
			$('#page-menu .tab').attr('class', 'tab');
			nextTab.attr('class', 'tab tab-on');
			try{n = nextTab.attr('id').substr(3);}catch(e){n=0;}
			currTab = n;
			showSubmenu(n);
			if(isHome) closeSearchMenu();
		}
	//	nextTab=null;
	}
	// mostra l'i-esimo sottomenu. se i=0 nasconde tutto (con hiding=true)
	function showSubmenu(i)
	{//console.log('showSubmenu('+i+')');
		if( (i == 0 && hiding) || i > 0 )
			$(isHome ? '.page-submenu-home' : '.page-submenu').hide();  // nasconde i sottomenu
		if( i > 0 && i <= 14 && $('#submenu'+i).length && $('#tab'+i).length )
		{   // mostra il sottomenu selezionato
			o = $('#tab'+i).position();
			submenu = $('#submenu'+i);
			if( isHome )
			{
				if( o.left + submenu.outerWidth() < 990 )
					submenu.css('left', parseInt(o.left) +'px');
				else  // allineamento a destra... occhio allo spigolo arrotondato
					submenu.css('left', parseInt(o.left) - submenu.outerWidth() + $('#tab'+i).outerWidth() +'px');
				//console.log(parseInt(o.left) + submenu.outerWidth() );
			}
			submenu.show();
		}
		hiding = false;
	}
	function hideSubmenu()
	{//console.log('hideSubmenu()');
		if( timerMenu != null ) clearTimeout(timerMenu);
		if( timerSubMenu != null ) clearTimeout(timerSubMenu);
		if( nextTab == null ) return;
		nextTab=null;
		hiding=true;
		timerMenu=setTimeout("showSubmenu(0)", 1000); // era 50
		timerSubMenu=setTimeout("showDefaultMenu()", 150)
	}
	function showDefaultMenu()
	{
		if( nextTab == null )
		{
	//console.log('showDefaultMenu() nextTab==null');
			nextTab = $('#tab'+defTab);
			mcanChange();
		}
	}

//-->
</script>

            <style type="text/css">
