>
<label for="searchInput--1452" class="hidden">suchfeld</label>
<input
@input.debounce="getSuggestionsForInput();cursorIndex=-1"
@click="active=true"
name="query"
x-model="query"
placeholder='Ort oder Thema suchen'
class="cy-quick-search w-full px-2 py-2 placeholder-search-header-text text-search-header-text  text-base leading-4 placeholder-opacity-100 rounded-l appearance-none lg:top-10 placeholder-text-xs focus:outline-none"
