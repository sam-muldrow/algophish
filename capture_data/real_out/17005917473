<script type="text/javascript" src="https://www.wisconsin.edu/wp-includes/js/jquery/jquery.min.js?ver=3.7.1" id="jquery-core-js"></script>
<script type="text/javascript" src="https://www.wisconsin.edu/wp-includes/js/jquery/jquery-migrate.min.js?ver=3.4.1" id="jquery-migrate-js"></script>
<script type="text/javascript" src="https://www.wisconsin.edu/wp-content/plugins/uwsa-cfct-cache-js-css/classes/../js/cfct.js?ver=6.4.1" id="cfct-build-js-js"></script>
<script type="text/javascript" src="https://www.wisconsin.edu/wp-content/plugins/uwsa-carousel-image/public/includes/slick/slick.min.js?ver=1.8.0" id="slick-carousel-js"></script>
<script>function initiate_slides( $ ) {
	"use strict";

	$(function () {

		const carousel = document.getElementById('header-carousel');
        const slides = carousel.getElementsByClassName('slide');
        const goalWidth = 3000;

        function sizeSlides( goalWidth , slides) {
            const containerWidth = carousel.offsetWidth;
            if ( goalWidth > containerWidth ) {
                const goalHeight = 3000;
                let actualHeight = ( goalHeight * containerWidth ) / goalWidth;
                                actualHeight = Math.max(actualHeight , 300)
                actualHeight = Math.min(actualHeight , 600)
                                for (var i = 0; i < slides.length; i++) {
                    slides[i].style.height = actualHeight + 'px';
                }
            }
        }
        sizeSlides( goalWidth , slides );
        window.addEventListener("resize", resizeThrottler, false);

        var resizeTimeout;
        function resizeThrottler() {
            // ignore resize events as long as an actualResizeHandler execution is in the queue
            if ( !resizeTimeout ) {
                resizeTimeout = setTimeout(function() {
                    resizeTimeout = null;
                    sizeSlides( goalWidth , slides );
                                    }, 66);
            }
        }

        const useDots = false ;

        const $carousel = $(carousel);

		$carousel.slick({
			accessibility: true,
			adaptiveHeight: false,
			slidesToShow: 1,
			slidesToScroll: 1,
			arrows: true,
			prevArrow: '<button class="carousel-nav uw-left-open"><span class="sr-only">Previous</span></button>',
			nextArrow: '<button class="carousel-nav uw-right-open"><span class="sr-only">Next</span></button>',
			dots:true,
			mobileFirst:true,
			draggable: false,
			swipe: false,
            responsive: [
                {
                    breakpoint: 769,
                    settings: {
                        dots: useDots
                    }
                }
            ]
		});

                //The rest of this is JS specifically for the thumbnails

        const $thumbnails = $('#header-carousel-thumbnails').find('.thumb');

		//set active class to first thumbnail slides
        $thumbnails.eq(0).addClass('thumb-current');

		// On before slide change match active thumbnail to current slide
        $carousel.on('beforeChange', function (event, slick, currentSlide, nextSlide) {
			const mySlideNumber = nextSlide;
			const slides = jQuery('.carousel-thumbs .thumb');
			slides.removeClass('thumb-current');
			slides.eq(mySlideNumber).addClass('thumb-current');
		});

        const keys = {
            end: 35,
            home: 36,
            left: 37,
            up: 38,
            right: 39,
            down: 40,
			space: 32,
			enter: 13
        };

        $thumbnails.on('click', function(event) {
            changeSlide( this.dataset.slideIndex );
        });

        $thumbnails.on('keydown', function( event ) {
			const key = event.keyCode;
			const currentIndex = this.dataset.slideIndex;

            switch (key) {
                case keys.space:
                case keys.enter:
                    changeSlide( currentIndex );
                	event.preventDefault();
                    break;
                case keys.left:
                case keys.up:
                    thumbFocusSwitch( this , -1 );
                    event.preventDefault();
                    break;
				case keys.right:
                case keys.down:
                    thumbFocusSwitch( this , 1 );
                    event.preventDefault();
                    break;
            }

        });

		function changeSlide( index ) {
            $carousel.slick('slickGoTo' , index );
		}

		function thumbFocusSwitch( currentElement , direction ) {
			var newFocusedElement = null;
			if ( 1 == direction ) {
				newFocusedElement = currentElement.nextElementSibling;
			} else if ( -1 == direction ) {
				newFocusedElement = currentElement.previousElementSibling;
			}
			if ( newFocusedElement ) {
                newFocusedElement.focus();
			}
		}
		

	});

}

jQuery(document).ready(initiate_slides(jQuery));</script>    </body>
</html>
