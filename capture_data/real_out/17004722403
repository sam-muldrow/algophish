<script>
  (function () {
    const [key, value] = location.hash
      .substring(1) // remove #
      .split('=');

    if (key === 'error_code') {
      TD.trackEvent('login_failure');
    } else {
      TD.trackEvent('login_open');
    }
    const emailError = document.querySelector('#error-element-username');
    if (emailError) {
      TD.trackEvent('login_warning_email');
    }
    const helpLinkAnchor = document.querySelector('.help-link');
    if (helpLinkAnchor) {
      helpLinkAnchor.addEventListener('click', _ev => TD.trackEvent('login_click_help_link'));
    }
    const googleButton = document.querySelector('button[data-provider="google"]');
    if (googleButton) {
      googleButton.addEventListener('click', _ev => TD.trackEvent('login_click_google'));
    }
    const appleButton = document.querySelector('button[data-provider="apple"]');
    if (appleButton) {
      appleButton.addEventListener('click', _ev => TD.trackEvent('login_click_apple'));
    }
  })();
</script>




<script>
  (function () {
    const privacyPolicyAnchor = document.querySelector('.privacy-policy');
    if (privacyPolicyAnchor) {
      privacyPolicyAnchor.addEventListener('click', _ev => TD.trackEvent('login_click_privacy_policy'))
    }

    const loginHelpAnchor = document.querySelector('.login-help');
    if (loginHelpAnchor) {
      loginHelpAnchor.addEventListener('click', _ev => TD.trackEvent('login_click_help_login'));
    }
  })();
</script>


  
  <script>
    const [key, value] = location.hash
      .substring(1) // remove #
      .split('=');

    if (key === 'error_code') {
      
