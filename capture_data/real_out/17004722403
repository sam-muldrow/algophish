  var TD = window.TD || {};

  (function () {
    const IS_PRODUCTION = 'true' === 'true';
    const DATABASE = IS_PRODUCTION ? 'chatwork' : 'test_chatwork';
    const TD_TABLE = 'chatwork_login_events';

    var td = new Treasure({
      host: 'in.treasuredata.com',
      writeKey: '8125/e009a25851366477cb6884a9958f630709fe9602',
      database: DATABASE
    });

    // Enable cross-domain tracking
    td.set('$global', 'td_global_id', 'td_global_id');
    td.setSignedMode();

    TD.trackEvent = function (event) {
      options = {
        cw_event: event,
        cw_client_type: 'web',
        td_title: 'title_dummy',
        td_uuid: ''
      };
      if (!IS_PRODUCTION) {
        console.log('Send to td: [database_name] -> ' + DATABASE + ' [table_name] -> ' + TD_TABLE + ' [value] -> ' + JSON.stringify(options));
      }
      td.trackEvent(TD_TABLE, options);
    };
  })();
