<script>
    jQuery('.pl-plus-link a.clickmenu').on("click", function(e){
        jQuery(this).next('ul').toggle();
        if(jQuery(this).children('span').hasClass('icon-down')){
            jQuery(this).children('span').removeClass('icon-down');
            jQuery(this).children('span').addClass('icon-up');
        }else{
            jQuery(this).children('span').removeClass('icon-up');
            jQuery(this).children('span').addClass('icon-down');
        }
        e.stopPropagation();
        e.preventDefault();
    });
</script>
