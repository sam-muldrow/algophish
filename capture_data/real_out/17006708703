<script>
	jQuery('.sub-menu').addClass('sub-menu-collapse');
	jQuery('.menu-item-has-children').addClass('list');
	if(window.matchMedia("(max-width: 767px)").matches){
		jQuery( '.list > a' ).append( "<span class='icon icon-down float-ight white-color'></span>" );
		jQuery(".list > a").click(function(e) {
			if(jQuery(this).next().is(":visible") === true ){
				jQuery("span",this).removeClass('icon-up').addClass('icon-down');
			}else {
				jQuery("span",this).removeClass('icon-down').addClass('icon-up');
			}
			jQuery(".sub-menu-collapse").slideUp(), jQuery(this).next().is(":visible") || jQuery(this).next().slideDown(),
				e.stopPropagation()
		})
	}
	const list = document.querySelectorAll('.list');
	let btn_click = false;
	jQuery(document).on('click', '.btn-hambur', () => {
		if (jQuery('.display-hambur').hasClass('icon-menu-plus collapse')) {
			jQuery('.display-hambur').removeClass('icon-menu-plus collapse');
			jQuery('.display-hambur').addClass('icon-Cerrar-X-Button expand');
			jQuery("#lgMenu").addClass("enter");
			jQuery("#site-navigation").addClass("enter");
			btn_click = true;
		} else {
			jQuery('.display-hambur').removeClass('icon-Cerrar-X-Button expand');
			jQuery('.display-hambur').addClass('icon-menu-plus collapse');
			jQuery("#lgMenu").removeClass("enter");
			jQuery("#site-navigation").removeClass("enter");
			btn_click = false;
		}
	});

	jQuery(document).click(function(event) {
		if (!(jQuery(event.target).closest(".btn-hambur,#site-navigation").length) && ( btn_click === true )) {
			jQuery('.display-hambur').removeClass('icon-Cerrar-X-Button expand');
			jQuery('.display-hambur').addClass('icon-menu-plus collapse');
			jQuery("#lgMenu").removeClass("enter");
			jQuery("#site-navigation").removeClass("enter");
			jQuery("#site-navigation").removeClass("is-active");
			btn_click = false;
		}
	});
</script>

