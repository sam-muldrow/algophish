</link>

<script type="text/javascript"
    src="/wcm/connect/resources/userstories/swiperjs/swiper8.0.6.js?subtype=javascript"></script> -->

<!-- 
MicroComp.Referencia.SwiperJs
 -->


<link rel="stylesheet" type="text/css"
    href="/wcm/connect/resources/libraries/swiperjs/swiperjs.css?subtype=css">
    <script src="/wcm/connect/resources/libraries/swiperjs/swiperjs.js?subtype=javascript">
</script>
<script>

    const bannerHome = document.querySelector('.bcp_banner_home');
    const btnBanner = document.querySelector('.btn_btn_banner');
    const imagenBanner = bannerHome.querySelector('.bcp_titulo');
    const tiempoCambioSlideSegundos = 5;
    const cantidadSlidebannerHome = bannerHome.querySelectorAll('.swiper-slide').length;
    let tiempMilesegundosCambioSlide = 8000;
    const slidersTaggeadosBannerHome = [];

    let swipperBannerHomeLoop = true;
    const btnPausarPlay = document.querySelector('.bcp_pause_play');

    if (Number.isInteger(tiempoCambioSlideSegundos)) {
        tiempMilesegundosCambioSlide = tiempoCambioSlideSegundos * 1000;
    }
    if (cantidadSlidebannerHome == 1) {
        bannerHome.classList.add('bcp_un_slider');
        swipperBannerHomeLoop = false;
    }

    const taggeoPromotionViewBannerHome = () => {
        try {
            const sliderActivo = document.querySelector('.swiper-slide-active');
            const tituloBanner = sliderActivo.querySelector('.bcp_titulo').innerText;
            const idBanner = sliderActivo.getAttribute('id');
            if (slidersTaggeadosBannerHome.some(bannerTaggeado => bannerTaggeado == idBanner) == true) return;
            digitalData.push({
                "promotion": {
                    "name": `Home - ${cleanLabel(tituloBanner)}`,
                    "creative": "Banner",
                    "position": "Banner Principal"
                },
                "event": "trackPromotionView"
            });
            slidersTaggeadosBannerHome.push(idBanner);
        } catch (error) {
            console.error(error);
        }
    }

    let swiper_bannerHome = new Swiper('.bcp_banner_home .swiper-container', {
        loop: swipperBannerHomeLoop,
        speed: 1100,
        autoplayDisableOnInteraction: false,
        watchSlidesProgress: true,
        effect: 'slide',
        init: true,
        autoplay: {
            delay: tiempMilesegundosCambioSlide,
            disableOnInteraction: false
        },
        pagination: {
            el: '.bcp_banner_home .bcp_paginacion',
            clickable: true,
        },
        navigation: {
            nextEl: '.bcp_banner_home .bcp_boton_next',
            prevEl: '.bcp_banner_home .bcp_boton_prev',
        },
        on: {
            init: function () {
                bannerHome.querySelectorAll('.swiper-pagination-bullet').forEach(pagina => {
                    pagina.addEventListener("click", (eventPagina) => {
                        btnPausarPlay.classList.remove('activo');
                    });
                });

                taggeoPromotionViewBannerHome();
                agregarEventoClickBotonBannerHome();
                bannerHome.classList.add('bcp_animar_entrada');
                fnlazy_loader_backgroundImage_comp_demanda(bannerHome.querySelectorAll('.swiper-slide-active .lazy_loader_backgroundImage_comp_demanda'))

            },
            autoplayTimeLeft: function (s, time, progress) {
                const avanceBanner = (1 - progress) * 100;
                const paginaActiva = bannerHome.querySelector('.bcp_banner_home .swiper-pagination-bullet-active');
                if (avanceBanner > 28) {
                    paginaActiva.style.setProperty("--progress", avanceBanner + '%');
                } else {
                    paginaActiva.style.setProperty("--progress", '28%');
                }
            },
            slideChangeTransitionStart: function () {
                fnlazy_loader_backgroundImage_comp_demanda(bannerHome.querySelectorAll('.swiper-slide-active .lazy_loader_backgroundImage_comp_demanda'))
                bannerHome.classList.remove('bcp_animar_entrada');
                btnPausarPlay.classList.remove('activo');
            },
            slideChangeTransitionEnd: function () {
                bannerHome.classList.add('bcp_animar_entrada');
                taggeoPromotionViewBannerHome();

            },
            slideChange: function () {
            }
        },
    });

    const estaDentroVistaContenidoBannerHome = () => {
        const heightHeader = 112;
        const topBanner = bannerHome.getBoundingClientRect().top - heightHeader;
        const bottomBanner = bannerHome.getBoundingClientRect().bottom - heightHeader;
        const viewHeight = window.innerHeight - heightHeader;

        if (topBanner <= viewHeight && bottomBanner >= 0 && getComputedStyle(bannerHome).display !== "none") {
            return true;
        }
        return false;
    }

    btnPausarPlay.addEventListener("click", (event) => {
        if (event.currentTarget.classList.contains('activo')) {
            swiper_bannerHome.autoplay.resume();
            btnPausarPlay.classList.remove('activo');
        } else {
            swiper_bannerHome.autoplay.pause();
            btnPausarPlay.classList.add('activo')
        }
    })

    function agregarEventoClickBotonBannerHome() {
        try {
            const contBanner = bannerHome.querySelectorAll('.bcp_grupo_contenido');

            contBanner.forEach((element) => {
                const tituloBn = element.querySelector('.bcp_titulo').innerText;
                const btnBanner = element.querySelector('.btn_btn_banner');
                const parametros = `pcid=viabcp:home:${prepareString(tituloBn)}:masivo:banner-principal`;
                agregarParemetrosGet(btnBanner, parametros, 'BannerHome');

                btnBanner.addEventListener('click', () => {
                    digitalData.push({
                        "promotion": {
                            "name": `Home - ${cleanLabel(tituloBn)}`,
                            "creative": "Banner",
                            "position": "Banner Principal"
                        },
                        "event": "trackPromotionClick"
                    });
                })
            });
        } catch (error) {
            console.error(error);
        }
    }

</script>
        
        
    

    
        





        







    
<div class="wpthemeClear"></div>
	</div>
