<!-- 
MicroComp.Referencia.SwiperJs
 -->


<link rel="stylesheet" type="text/css"
    href="/wcm/connect/resources/libraries/swiperjs/swiperjs.css?subtype=css">
    <script src="/wcm/connect/resources/libraries/swiperjs/swiperjs.js?subtype=javascript">
</script>
<script>
    const carrouselBoxesEducacion = document.querySelector('.bcp_carousel_boxes_educacion');

    let swiperCarouseBoxesEducacion = new Swiper('.bcp_carousel_boxes_educacion .swiper-container', {
        direction: 'horizontal',
        breakpoints: {
            1: {
                slidesPerView: 'auto',
                spaceBetween: 16
            },
            1280: {
                slidesPerView: 3,
                spaceBetween: 12
            }
        }
    });


    carrouselBoxesEducacion.querySelectorAll('.bcp_item_box_educacion').forEach((box)=>{
        box.addEventListener('click',(event)=>{
            let boxSeleccionado = event.currentTarget;
            let category = cleanLabel(carrouselBoxesEducacion.querySelector('.bcp_titulo_lista').innerText);
            let tipoCard = cleanLabel(boxSeleccionado.querySelector('.bcp_box_tag').innerText);
            let nombreCard = cleanLabel(boxSeleccionado.querySelector('.bcp_box_titulo').innerText);
            let group = carrouselBoxesEducacion.dataset.taggeo;

            digitalData.push({
                "action": {
                    "group": group,
                    "category": category,
                    "name": "Click",
                    "label": `${tipoCard}-${nombreCard}`
                },
                "event": "trackAction"
            })
        })
    })

    carrouselBoxesEducacion.querySelectorAll('.btnseccion').forEach((btn)=>{
        btn.addEventListener('click',()=>{
            let btnSeleccionado = event.currentTarget;
            let category = cleanLabel(carrouselBoxesEducacion.querySelector('.bcp_titulo_lista').innerText);
            let group = carrouselBoxesEducacion.dataset.taggeo;

            digitalData.push({
                "element": {
                    "container": `${group}-${category}`,
                    "type": "Link",
                    "name": cleanLabel(btnSeleccionado.innerText)
                },
                "event": "trackElement"
            })
        })
    })

</script>
        
        
    

    
        





        







    
