<script>

    let politicaCookieAdobe = document.querySelector('.bcp_politica_uso_cookieAdobe');

    setTimeout(() => {
        MostrarCookiePoliticaPrivacidadAdobe(true);

        let validarPersonzalizacion = getCookie("politica_privacidad_personalizacion");
        let validarMarketing = getCookie("politica_privacidad_marketing_analitica");
        let validarEscenciales = getCookie("politica_privacidad_escenciales");

        if( validarPersonzalizacion == '' && validarMarketing == '' && validarEscenciales == '' ){
            positionTaggeo = 'Mostrar Modal';
            //promotionViewModalCookies(positionTaggeo);
        }else{
            positionTaggeo = 'Ocultar Modal';
            //promotionViewModalCookies(positionTaggeo);
        }

    }, 100);

    politicaCookieAdobe.querySelector('.bcp_btn_configurar').addEventListener('click', () => {
        //detectarBotonTGAB('Configurar', 'Modal', false);
        fnMostrarDetalleCookiePoliticaPrivicidadAdobe(true);
    })

    politicaCookieAdobe.querySelector('.bcp_cerrar_modal').addEventListener("click", () => {
        //detectarBotonTGAB('Cerrar','Modal', false);
        fnMostrarDetalleCookiePoliticaPrivicidadAdobe(false);
        politicaCookieAdobe.querySelector('.bcp_btn_configurar').focus();
    })

    politicaCookieAdobe.querySelector('.bcp_btn_aceptar').addEventListener("click", () => {
        fnAceptaPoliticaPrivicidadAdobe(false);
        aceptarCookiesTGAB(true);
        detectarBotonTGAB('Aceptar','Modal',true);

        //Taggeo Adicional
        dataLayer.push({'event': 'cookieSet'});
    })

    politicaCookieAdobe.querySelector('.bcp_guardar_preferencia').addEventListener("click", (btn) => {
        fnAceptaPoliticaPrivicidadAdobe(true);
        aceptarCookiesTGAB(false);
        detectarBotonTGAB('Guardar preferencias','Configurar',true);
        
        //Taggeo Adicional
        dataLayer.push({'event': 'cookieSet'});
    })

    politicaCookieAdobe.querySelectorAll('.bcp_mostrar_ocultar_lista').forEach(ele => {
        ele.addEventListener("click", (btn) => {
            slideToggle(btn.currentTarget.nextElementSibling);

            if (btn.currentTarget.className.indexOf('down') == -1) {
                btn.currentTarget.classList.add('down');
            } else {
                btn.currentTarget.classList.remove('down');
            }
        })
    })

    //Taggeo Provisional
    politicaCookieAdobe.querySelectorAll('input[type="checkbox"]').forEach((checked)=>{
        checked.addEventListener('click',(event)=>{
            let selectorChk = event.currentTarget;
            let selectorTitulo = fnGetParent(selectorChk,'.bcp_tipo_titulo');
            let modoONOFF = 'OFF';

            if( selectorChk.checked == true ){
                modoONOFF = 'ON';
            }

            let categoryTaggeo = `Configurar Switch ${modoONOFF}`;

            digitalData.push({
                "action": {
                    "group": "Politicas de Cookies",
                    "category": categoryTaggeo,
                    "name": "Click",
                    "label": selectorTitulo.getAttribute('attr-label')
                },
                "event": "trackAction"
            });
        });
    });


    function MostrarCookiePoliticaPrivacidadAdobe(boolMostrar = true) {
        let politica = politicaCookieAdobe;

        let pathnameActual = window.location.pathname;
        let enlacePrivacidadPoliticas = `${pathnameActual}#protecciondedatos`
        let positionTaggeo = '';

        if (boolMostrar == true && getCookie('politica_privacidad_escenciales') == '') {

            if( enlacePrivacidadPoliticas == '/transparencia/#protecciondedatos' ){
                politica.classList.remove('mostrar');
                politica.setAttribute('style', 'display:none');
                document.body.classList.remove('bcp_overflow');
            }else{
                politica.classList.add('mostrar');
                politica.removeAttribute('style');
                document.body.classList.add('bcp_overflow');
            }

        } else {
            politica.classList.remove('mostrar');
            politica.setAttribute('style', 'display:none');
            document.body.classList.remove('bcp_overflow');
        }

    }

    function fnMostrarDetalleCookiePoliticaPrivicidadAdobe(boolMostrar = true) {
        let DetallePolitica = politicaCookieAdobe.querySelector('.bcp_configuracion_politica_cookie');
        let bntConfigurar = politicaCookieAdobe.querySelector('.bcp_btn_configurar');
        let btnAceptar = politicaCookieAdobe.querySelector('.bcp_btn_aceptar');
        if (boolMostrar) {
            DetallePolitica.style.display = "block";
            setTimeout(() => {
                DetallePolitica.classList.add('mostrar');
                document.body.classList.add('bcp_overflow');
                btnAceptar.style.display = "none"
            }, 100);

        } else {
            DetallePolitica.classList.remove('mostrar');
            document.body.classList.remove('bcp_overflow');
            btnAceptar.style.display = "block"
            setTimeout(() => {
                DetallePolitica.style.display = "none"
            }, 500);
        }
    }

    function setCookieAllDomain(strNombreCookie, strValorCookie, tiempoExperirar) {
        let date = new Date;
        date.setTime(date.getTime() + tiempoExperirar);
        let tiempoExpirar = "; expires=" + date.toGMTString();
        document.cookie = strNombreCookie + "=" + strValorCookie + ";" + tiempoExpirar + ";path=/;domain=.viabcp.com;";
    }

    function fnAceptaPoliticaPrivicidadAdobe(boolConfiguropoliticaCookieAdobe = false) {

        let cookie_personalizacion = politicaCookieAdobe.querySelector('#cookie_personalizacion').checked;
        let cookie_marketing_analitica = politicaCookieAdobe.querySelector('#cookie_marketing_analitica').checked;
        let cookie_escenciales = politicaCookieAdobe.querySelector('#cookie_escenciales').checked;

        if (boolConfiguropoliticaCookieAdobe == true) {

                setCookieAllDomain("politica_privacidad_personalizacion", cookie_personalizacion, (1000 * 60 * 60 * 24 * 265));//365 dias
                setCookieAllDomain("politica_privacidad_marketing_analitica", cookie_marketing_analitica, (1000 * 60 * 60 * 24 * 265));//365 dias
                setCookieAllDomain("politica_privacidad_escenciales", cookie_escenciales, (1000 * 60 * 60 * 24 * 265));//365 dias

        } else {

            if (boolConfiguropoliticaCookieAdobe == false) {
                setCookieAllDomain("politica_privacidad_personalizacion", true, (1000 * 60 * 60 * 24 * 265));//365 dias
                setCookieAllDomain("politica_privacidad_marketing_analitica", true, (1000 * 60 * 60 * 24 * 265));//365 dias
                setCookieAllDomain("politica_privacidad_escenciales", true, (1000 * 60 * 60 * 24 * 265));//365 dias
            }

        }

        setCookieAllDomain("politica_privacidad_configurado_x_usuario", boolConfiguropoliticaCookieAdobe, (1000 * 60 * 60 * 24 * 265));//365 dias        
        MostrarCookiePoliticaPrivacidadAdobe(false);

        //Update de las políticas
        // fnUpdatePoliticas();
        updatePoliticaPrivacidadGoogle();

        //lanzar evento de adobe 
        var event = new Event('adobeDataConsent');
        window.dispatchEvent(event);

        // console.log('Cookies seteadas::::');
        // console.log('personaliacion: ',getCookie("politica_privacidad_personalizacion"));
        // console.log('marketing: ',getCookie("politica_privacidad_marketing_analitica"));
        // console.log('escenciales: ',getCookie("politica_privacidad_escenciales"));
        // console.log('politica_privacidad_configurado_x_usuario: ',getCookie("politica_privacidad_configurado_x_usuario"));
        

    }

    function setPrimeraCargaGoogleExpAdobe() {
        if (getCookie('politica_privacidad_escenciales') == '') {//cuando se va mostrar el inicio de politica de privacidad
            window.dataLayer = window.dataLayer || [];

            function gtag() {
                dataLayer.push(arguments);
            }

            gtag('consent',
                'default',
                {
                    'personalization_storage': 'denied',
                    'analytics_storage': 'denied'
                }
            );
            dataLayer.push({ 'event': 'default_consent' });
        }

    }

    function updatePoliticaPrivacidadGoogle() {
        let strEstPrivacidadPersonalizacion = getCookie('politica_privacidad_personalizacion');
        let strEstMarketingAnalitica = getCookie('politica_privacidad_marketing_analitica');


        let strEstado_personalization_storage = 'denied';
        let strEstado_analytics_storage = 'denied';

        if (strEstPrivacidadPersonalizacion == 'true') {
            strEstado_personalization_storage = 'granted';
        }

        if (strEstMarketingAnalitica == 'true') {
            strEstado_analytics_storage = 'granted';
        }

        window.dataLayer = window.dataLayer || [];

        function gtag() {
            dataLayer.push(arguments);
        }

        gtag('consent',
            'default',
            {
                'personalization_storage': strEstado_personalization_storage,
                'analytics_storage': strEstado_analytics_storage
            }
        );

        dataLayer.push({ 'event': 'default_consent' });

        // console.log('updatePoliticaPrivacidadGoogle')
    }

    setPrimeraCargaGoogleExpAdobe();

    //Taggeos
    function aceptarCookiesTGAB(actionBtnAceptar=false) {
        let cookie_personalizacion = '';
        let cookie_marketing_analitica = ''
        let cookie_escenciales = '';
        
        if( actionBtnAceptar == true ){
            cookie_personalizacion = true;
            cookie_marketing_analitica = true;
            cookie_escenciales = true;
        }else{
            cookie_personalizacion = politicaCookieAdobe.querySelector('#cookie_personalizacion').checked;
            cookie_marketing_analitica = politicaCookieAdobe.querySelector('#cookie_marketing_analitica').checked;
            cookie_escenciales = politicaCookieAdobe.querySelector('#cookie_escenciales').checked;
        }
        const estadoCookie = (cookie) => { return cookie ? 'Activo' : 'Desactivo' };

        console.log(`Personalizacion=>${estadoCookie(cookie_personalizacion)}|Analitica=>${estadoCookie(cookie_marketing_analitica)}|Esenciales=>${estadoCookie(cookie_escenciales)}`)
    }

    function detectarBotonTGAB(btn, category, permiso) {


        let jsonPermisos = adobe.optIn.permissions;
        let aa = jsonPermisos.aa;
        let aam = jsonPermisos.aam;
        let target = jsonPermisos.target;
        let ecid = jsonPermisos.ecid;

        let permisos = '';
        if( permiso == true ){
            permisos = ` - permisos: aa:${aa}-aam:${aam}-target:${target}-ecid:${ecid}`;
        }

        digitalData.push({
            "action": {
                "group": "Politicas de Cookies",
                "category": "Modal",
                "name": "Click",
                "label": `${cleanLabel(btn)}${permisos}`
            },
            "event": "trackAction"
        });

    }

    function promotionViewModalCookies(positionTaggeo){

        digitalData.push({
            "promotion":{
                "name":"Politicas de Cookies",
                "creative":"Personalización",
                "position": positionTaggeo
            },
            "event":"trackPromotionView"
        });


    }

</script>
<!-- FIN POliticas de cookies -->

