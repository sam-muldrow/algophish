<script>
    const selectorAyudaRapida = document.querySelector('.bcp_ayuda_buscador');

    selectorAyudaRapida.querySelector('#buscadorayuda').addEventListener('click',()=>{
        document.querySelector('.bcp_header .bcp_buscar').click();
    })

    selectorAyudaRapida.querySelectorAll('.bcp_bloque_ayuda_incrustado .bcp_opcion_enlace').forEach((btnEnlace) => {
        btnEnlace.addEventListener('click',(event)=>{

            let category = selectorAyudaRapida.querySelector('.bcp_bloque_ayuda_incrustado .bcp_titulo_opciones').innerText;
            let label = event.currentTarget.innerText
            
            digitalData.push({
                "action":{
                    "group":"Home",
                    "category": cleanLabel(category),
                    "name": "Click",
                    "label": cleanLabel(label)
                },
                "event":"trackAction"
            })

        })
    })

    selectorAyudaRapida.querySelectorAll('.bcp_bloque_ayuda_incrustado .bcp_enlace_seccion').forEach((enlace) => {
    
        enlace.addEventListener('click',(event)=>{

            let container = cleanLabel(selectorAyudaRapida.querySelector('.bcp_bloque_ayuda_incrustado .bcp_titulo_opciones').innerText);
            let btn = cleanLabel(event.currentTarget.innerText);

            digitalData.push({
                    "element":{
                        "container": `Home-${container}`,
                        "type":"Link",
                        "name": btn
                    },
                    "event":"trackElement"
                })

        
        })
    })

</script>
        
        
    

    
        





        







    
