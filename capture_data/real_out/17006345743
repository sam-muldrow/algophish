<script>

    const explorarMejoresBeneficios = document.querySelector('.bcp_explorar_mejores_beneficios');
    explorarMejoresBeneficios.querySelectorAll('.bcp_benficio ').forEach(box => {

        box.addEventListener("click", (event) => {
            const tituloBox = cleanLabel(event.currentTarget.querySelector('.bcp_titulo').innerText.trim())
            digitalData.push({
                "action": {
                    "group": "Home",
                    "category": "Explora los Mejores Beneficios con BCP",
                    "name": "Click",
                    "label": tituloBox
                },
                "event": "trackAction"
            });
        })
    })

</script>
        
        
    

    
        





        







    
