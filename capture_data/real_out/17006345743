        .bcp_entry_point_mi_espacio_bcp[data-origen-producto="CT"] .bcp_grupo_entry_point_img .entry_point_img {
            width: 560px;
            height: 354px;
        }

        .bcp_entry_point_mi_espacio_bcp .bcp_legal,
        .bcp_entry_point_mi_espacio_bcp form-captcha .bcp_legal_captcha {
            text-align: left;
        }

        .bcp_entry_point_mi_espacio_bcp .bcp_grupo_documento_btndescubrir {
            flex-direction: row;
            align-items: flex-start;
            gap: 20px;
        }

        .bcp_entry_point_mi_espacio_bcp .bcp_btn_enviar_form {
            margin-top: 0px;
        }

        .bcp_entry_point_mi_espacio_bcp[data-origen-producto="HOME"] .bcp_titulo.bcp_desktop {
            display: block;

        }

        .bcp_entry_point_mi_espacio_bcp[data-origen-producto="HOME"] .bcp_titulo.bcp_mobile {
            display: none;
        }

    }
</style>


<section id="bcp-form-entry-point-mi-espacio-bcp" class="bcp_entry_point_mi_espacio_bcp  bcp_formulario  cargando"
data-translate-comp="true"
    data-origen-producto="HOME">
    <div class="container">
        <div class="bcp_antetitulo_entrypoint" data-translate="true">Productos para ti</div>
        <h2 class="bcp_titulo bcp_mobile" data-translate="true">
            Mi Espacio BCP
        </h2>
        <div class="bcp_grupo_entry_point_img">
            <form id="formSolicitalo" class="bcp_form_integrado">
                <h2 class="bcp_titulo bcp_desktop" data-translate="true">
                    Mi Espacio BCP
                </h2>
                <div class="bcp_descripcion" data-translate="true">
                    Ingresa tu DNI y descubre tu producto especial
                </div>

                <div class="bcp_grupo_documento_btndescubrir">
                    <form-cbodocumento>
                    </form-cbodocumento>
                    <button class="bcp_btn_enviar_form bcp_boton_naranja large" type="button" data-translate="true"
                        disabled>Descubrir</button>

                </div>
                <div class="bcp_legal" data-translate="true">
                    <p dir="ltr">(*)Al enviarnos esta información usted autoriza al BCP el tratamiento de sus datos personales. Conoce más&nbsp;<a href="https://www.viabcp.com/transparencia/#protecciondedatos" target="_blank" >aquí.</a></p>

                </div>
                <form-captcha id="recaptchanformgeneral" datacallback="verificarLeadsMiEspacioBCP">

                </form-captcha>
            </form>
            <div id="af3d43df-f75e-427a-aa12-d1cd02c5f21b" class="entry_point_img lazy_loading lazy_loading_backgroundImage"
                data-src-m="/wcm/connect/82664216-ddd8-4b7a-b3bb-eacf9d986ad6/desktop-home.svg?MOD=AJPERES&amp;CACHEID=ROOTWORKSPACE-82664216-ddd8-4b7a-b3bb-eacf9d986ad6-ozl.-ia"
                data-src-t="/wcm/connect/05bade65-b760-4441-be24-4be0c4eaac8f/desktop.svg?MOD=AJPERES&amp;CACHEID=ROOTWORKSPACE-05bade65-b760-4441-be24-4be0c4eaac8f-ozl.-ia"
                data-src-d="/wcm/connect/05bade65-b760-4441-be24-4be0c4eaac8f/desktop.svg?MOD=AJPERES&amp;CACHEID=ROOTWORKSPACE-05bade65-b760-4441-be24-4be0c4eaac8f-ozl.-ia">
            </div>
            <div class="bcp_animacion_ruta">
                /wcm/connect/8b59b8d7-9e4f-4ed0-9f57-06ac5310c80e/lottie-mi-espacio-bcp-mobile.json?MOD=AJPERES&amp;CACHEID=ROOTWORKSPACE-8b59b8d7-9e4f-4ed0-9f57-06ac5310c80e-ozl.-ia
