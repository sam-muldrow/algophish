<script>
    //CONSTANTES GLOBALES
    const PAGE_PCID = (data)=> {
        switch (data) {
            case 'BCP Online':
                    return 'bcp-online'
                break;
            case 'Abrir Cuenta BCP':
                    return 'abrir-cuenta'
                break;
            default:
                    return 'home'
                break;
        }
    }
    function obtenerGroup(titulo) {
        const bloque = titulo.split("|");
        let str = bloque[0];
        return str.trim();
    }
    //FUNCIóN QUE ELIMINA TILDE Y CARACTERES EXTRAÑOS
    function cleanLabel(str,limite=0){
        if(str){
            //Variables
            let contador = 0;
            let newStr = str;
            const simbolos = [/¿|¡/g,/[\u0021-\u002C]/g,/[\u002E-\u002F]/g,/[\u003A-\u0040]/g];
            const cambios = [{r1:/ñ/g,r2:'ni'},{r1:'y/o',r2:'zoz'}];
            const excluir = ['de','la','un','una','por','a','mi','mis','y','que','o','te','son','las','en','el','tu','tus','al','del','y/o','con','los','sin'];
            //Funciones
            function reemplazar(data) {
                if(contador == 0){
                    data = data.replace(/^\w/, (c) => c.toUpperCase())
                }
                else {
                    (excluir.indexOf(data) != -1) ? data = data : data = data.replace(/^\w/, (c) => c.toUpperCase());
                }
                contador++;
                return data;
            }
            //Procesando String
            newStr =  newStr.replace(/&nbsp;/g," ");//Elimina el espacio con codigo HTML
            if(/ /.test(str)) {
                //console.log(`Varias palabra: ${newStr}`)
                limite != 0 ? newStr = newStr.substr(0, limite).trim() : newStr = newStr;
                newStr = newStr.toLocaleLowerCase();
                cambios.forEach((element)=>{newStr = newStr.replace(element.r1,element.r2)});//Reemplazando valores
                simbolos.forEach((element)=>{newStr = newStr.replace(element,"")});//Eliminando simbolos
                newStr = newStr.normalize("NFD").replace(/[\u0300-\u036f]/g, "");//Elimina Tildes
                newStr = newStr.replace(cambios[1].r2,cambios[1].r1);//Retornar algunos valores reemplazados
                newStr = newStr.trim().replace(/\w\S*/g, reemplazar);//Convierte texto en Alta y bajas
                return newStr;
            }
            else {
                //console.log(`una palabra ${newStr}`)
                limite != 0 ? newStr = newStr.substr(0, limite).trim() : newStr = newStr;
                cambios.forEach((element)=>{newStr = newStr.replace(element.r1,element.r2)});//Reemplazando valores
                simbolos.forEach((element)=>{newStr = newStr.replace(element,"")});//Eliminando simbolos
                newStr = newStr.normalize("NFD").replace(/[\u0300-\u036f]/g, "");//Elimina Tildes
                newStr = newStr.replace(cambios[1].r2,cambios[1].r1);//Retornar algunos valores reemplazados
                newStr = newStr.replace(/^\w/, (c) => c.toUpperCase())
                return newStr;
            }
        }
        else {
            console.log(`cleanLabel => Hubo problemas al formatear el texto`);
        }
    };
    //FUNCIóN QUE AGREGA PARAMETROS GET PARA TAGGEO ADOBE
    function agregarParemetrosGet(link,parametros,section='Sin info',attribute='attr-href') {
        try {
            if(link&&parametros) {
                const atributo = link.tagName !='BUTTON'? 'href': attribute;
                const urlOriginal = link.getAttribute(atributo);
                const validarExisteTaggeo = (enlace,section) => {
                    let resultado = true;
                    const codigo = [/rfid/i,/pcid/i];
                    const message = ['Ya existe un pcid ó rfid en el enlace','href es null'];
                
                    if(enlace){ 
                        codigo.forEach(element => { 
                            if(enlace.search(element) >= 0) {resultado = false;mostrarMensajeConsola('A',`${message[0]} (${section}) `)}
                        });
                    }
                    else {
                        resultado = false;mostrarMensajeConsola('A',`${message[1]} (${section}) `);
                    }
                    return resultado;
                }
    
                if (validarExisteTaggeo(urlOriginal,section)) {
                    
                    parametros = parametros.toLowerCase();
                    const urlCopia = urlOriginal.trim();
                    const conector =  urlCopia.indexOf("?") >= 0 ? '&' : '?';
                    if(urlCopia!=="https://www.proyectosinmobiliariosbcp.com/"){
                        const result = `${ urlCopia}${conector}${parametros}`;
                        link.setAttribute(atributo,result);
                    }
                    
                    
                }
            }
            else {
                mostrarMensajeConsola('A',`agregarParemetrosGet=>(${link},${parametros}) Valores enviados en null o undefined (${section})`);
            }
        } catch (error) {
            console.log(error);
        }
    }
    //FUNCIÓN ELIMINA TILDE Y ESPACIO EN BLANCO
    function prepareString(str,section='Sin Info',limite=30){
        if(typeof str == 'string'){
            if(str.length > 0){
                let newStr = str.toLocaleLowerCase().trim();
                const letras = {exprInit:/ñ/g,changeInit:'ni'};
                const simbolos = [/¿|¡/g,/[\u002D]/g,/[\u0021-\u002F]/g,/[\u003A-\u0040]/g];
                const buscarGuion = (str)=>{return str.slice(-1) == '-' ? str.substr(0, (str.length - 1)) : str};
    
                newStr =  newStr.replace(/&nbsp;/g," ");//Elimina el espacio con codigo HTML
                newStr = buscarGuion(newStr.substr(0, limite));
                simbolos.forEach((e)=>{newStr = newStr.replace(e,"")});//Buscando simbolos para eliminarlos
                newStr = newStr.replace(letras.exprInit,letras.changeInit);//Reemplazo la ñ
                newStr = newStr.normalize("NFD").replace(/[\u0300-\u036f]/g, "");//Elimina tildes
                newStr = newStr.replace(/  /g, " ");
                newStr = newStr.replace(/ /g, "-");//Reemplazo espacio en blanco con guión
                return newStr;
            }
            else {
                mostrarMensajeConsola('A',`prepareString => el string esta vacio.(${section})`); 
            }
        }
        else {
            mostrarMensajeConsola('A',`prepareString => No es un string.(${section})`);
        }
    }
    
    
    function validarDataTGAB(d,m){
        let resultado = true;
        const tipoError = ['null','undefined'];
        const mostrarError = (m,tipo)=>{
            resultado = false;
            mostrarMensajeConsola('E',`El valor de ${m} es ${tipo}`);
        }
    
        if(typeof d !== 'undefined'){if(d === null){mostrarError(m,tipoError[0])}}
        else {mostrarError(m,tipoError[1])};
        return resultado;
    }
    function mostrarMensajeConsola(tipo,mensaje){
        const tipoMensaje = [
            ['Error:','background: #ff6961; color: white;'],
            ['Advertencia:','background: #fdfd96; color: #505050;'],
            ['Completado:','background: #77dd77; color: #505050;']
        ]
    
        if(tipo === 'E'){
            console.log(`%c ${tipoMensaje[0][0]} ${mensaje} `, tipoMensaje[0][1]);
        }
        else if(tipo === 'A'){
            console.log(`%c ${tipoMensaje[1][0]} ${mensaje} `, tipoMensaje[1][1]);
        }
        else {
            console.log(`%c ${tipoMensaje[2][0]} ${mensaje} `, tipoMensaje[2][1]);
        } 
    }
    function eventPushTGAB(tipo,data){
        switch (tipo) {
            case 'A':
                digitalData.push({"action":data,"event":"trackAction"});
                break;
            case 'F':
                digitalData.push({"flow":data,"event":"trackFlow"});
                break;
            case 'E':
                digitalData.push({"element":data,"event":"trackElement"});
                break;
            case 'ER':
                digitalData.push({"form":data,"event":"trackFormError"});
                break;
            case 'M':
                digitalData.push({"metadata":data,"event":"trackMetadata"});
                break;
            case 'P':
                digitalData.push({"popup":data,"event":"trackPopup"});
                break;
            case 'S':
                digitalData.push({"form":data,"event":"trackFormSubmit"});
                break;
            case 'V':
                digitalData.push({"form":data,"event":"trackFormView"});
                break;
            default:
                mostrarMensajeConsola('E',`eventPushTGAB => (${tipo}) parametro enviado no encontrado`);
                break;
        };
    }

    function urlUTMTGAB() {
        let detener = true;
        let result = '';
        const simbolos = [/¿|¡/g,/[\u0021-\u002C]/g,/[\u002E-\u002F]/g,/[\u003A-\u0040]/g];
        const eliminarSimbol = (s)=>{simbolos.forEach((e)=>{s = s.replace(e,"")}); return s.trim()}//Eliminando simbolos
        let url = window.location.pathname;
        url = url.replace('/','');
        const urlseparada = url.split("/");
        
        const armar = (lista)=>{lista.forEach((e,i)=>{result += `${eliminarSimbol(e)}${i!=(lista.length - 1)?'-':''}`})}

        /* if(urlseparada.length > 7) {
            const contenedor = [];
            urlseparada.forEach((e,i)=>{
                if(e =='!ut'){detener=false}
                if(i > 3 && detener){contenedor.push(e)}
            })
            armar(contenedor);
        }
        else {
            armar(urlseparada);
        } */
        armar(urlseparada);
        return result;
    }

    function titleUTMTGAB(tipo ='UTM') {

        const titulo = document.title;
        const bloque = (s)=>{const b = s.split("|");return b[0]}
        const titleClean = titulo.search(/\u007C/)!= -1 ? bloque(titulo): titulo;

        return tipo == 'UTM' ? prepareString(titleClean) : cleanLabel(titleClean);

    }
    function TGABTaggeoForm(form='vacio',tipo='submit'){
        try {
            if(form){
                const event = tipo == 'submit' ? 'trackFormSubmit':'trackFormError';
                let nameForm = titleUTMTGAB('LABEL');
                if(form !='vacio') {
                    nameForm = document.getElementById(form) ? document.getElementById(form).getAttribute('nomproducto'):titleUTMTGAB('LABEL');
                }
                digitalData.push({
                    "form":{
                        "channel":"ViaBCP",
                        "name":cleanLabel(nameForm),
                        "version":"",
                        "product":""
                    },
                    "event":event
                });
                }
            else {
                mostrarMensajeConsola('A','Verificar los parametros de TGABTaggeoForm');
            }
        } catch (error) {
            console.log(error.message);
        }
    }
class TaggeoAdobeV1 {
    constructor(contenedor,parametros) {
        this.contenedor = contenedor;
        this.label = parametros.label;
        this.data =  parametros.data;
        this.event = parametros.event ? parametros.event : "trackAction";
    }
    init(){
        
        if(Object.keys(this.data).length && this.contenedor.length > 0){
            this.contenedor.forEach((contenedor)=>{
                document.querySelectorAll(contenedor).forEach((btn)=>{this.taggear(btn)})
                console.log(`Se agregó taggeo ${this.event}`)
            })
        }
        else {
            console.log('No se encontro el contenedor')
        }
    }
    armarData(e){
        let envio = this.data;
        let txt = [];
        let result =  null;
        this.label.forEach((element)=>{
            if (typeof element == 'string'){
                if(element.length == 0) txt.push(cleanLabel(e.innerText))
                else if(element[0] == '.') txt.push(cleanLabel(e.querySelector(element)?.innerText))
                else txt.push(element)
            }
            else {
                const nodo = this.subirNodos(e,element.nivel)
                txt.push(cleanLabel(nodo.querySelector(element.clase)?.innerText))
            } 
        })
        result = txt.includes(undefined) ? null : txt.toString().replace(/\u002c/g, " - ");
        if(result) envio.label = result
        else envio = null;
        return envio;
    }
    taggear(btn){
        btn.addEventListener('click',(event)=>{
            console.log(this.data);
            const activo =  event.currentTarget;
            const data = this.label ? this.armarData(activo):this.data;

            if(data) {
                console.log(data)
                digitalData.push({"action":data,"event":this.event})
            }
            else {console.log('Hubo un error al generar la data')}
        })
    }
    subirNodos(inicio,nivel){
        let node = inicio
        for (let i = 0; i < nivel; i++) {node = node.parentNode}
        return node
    }
}
</script>

<!-- FIN Funciones para taggeo adobe -->

