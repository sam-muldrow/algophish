<!-- INICIO validar soporte de navegador -->
<script>
    function verificarNavegador() {
        var isIE = /Trident|MSIE/.test(window.navigator.userAgent);
        var isEdge = /Edge/.test(window.navigator.userAgent) && !isIE;

        if (isIE || isEdge) {
            window.location = '/navegador-soportado';
        }
    }

    verificarNavegador();
</script>
<!-- FIN validar soporte de navegador -->

