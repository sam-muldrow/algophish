            'https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);
            })(window,document,'script','dataLayer','GTM-WZTD854')
            ;`
    
        if (id_contenedor == 'GTM-5TZQ3DT') {//no es contenedor
            // console.log('normal');
            //codigo antiparpadeo
            let script_1 = document.createElement("script");
            script_1.text = asynchide_optimize;
            document.getElementsByTagName('head')[0].appendChild(script_1);
    
            //Etiqueta Anlytics-Optimize sin Contenedor
            let script_2 = document.createElement("script");
            script_2.text = antiguo_optimize;
            document.getElementsByTagName('head')[0].appendChild(script_2);
    
        } else {//contenedor
            // console.log('Contenedor');
            //codigo antiparpadeo
            let script_3 = document.createElement("script");
            script_3.text = asynchide_optimize;
            document.getElementsByTagName('head')[0].appendChild(script_3);
    
            //Etiqueta Anlytics-Optimize Contenedor
            let script_4 = document.createElement("script");
            script_4.src = `https://www.googleoptimize.com/optimize.js?id=${id_contenedor}`;
            document.getElementsByTagName('head')[0].appendChild(script_4);
        }
    
        // // //tag manager
        let script_5 = document.createElement("script");
        script_5.text = tag_manager;
        document.getElementsByTagName('head')[0].appendChild(script_5);
    
