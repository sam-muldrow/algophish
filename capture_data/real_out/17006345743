<script src="/wcm/connect/resources/userstories/Lottiejs/lottie.js?subtype=javascript"></script>
<script>

    const selectorTiendaOnline = document.querySelector('.bcp_tienda_online_home_v4');
    let boxPosicionTiendaOnline = selectorTiendaOnline.querySelectorAll('.bcp_comp_tienda_online_v4')

    for (i = 0; i < boxPosicionTiendaOnline.length; i++) {
        boxPosicionTiendaOnline[i].setAttribute("attr-position", i + 1);
    }

    //Crear Lotties
    let tiendaOnlineBox0 = '';
    let tiendaOnlineBox1 = '';
    let tiendaOnlineBox2 = '';
    let tiendaOnlineBox3 = '';
    let tiendaOnlineBox4 = '';
    let tiendaOnlineBox5 = '';

    selectorTiendaOnline.querySelectorAll('.bcp_comp_tienda_online_v4').forEach((item, index) => {
        //Setear Index
        item.querySelector(`.bcp_box .bcp_hover_animacion`).setAttribute('attr-id', index);
        //Extraer rurta de lottie por box
        let rutaLottie = item.querySelector('.bcp_animacion_ruta').innerText;
        item.querySelector('.bcp_animacion_ruta').remove();

        let dataLottie = {
            container: item.querySelector(`.bcp_box .bcp_hover_animacion`),
            renderer: 'svg',
            loop: false,
            autoplay: false,
            path: rutaLottie
        };

        switch (index) {
            case 0: tiendaOnlineBox0 = bodymovin.loadAnimation(dataLottie); break;
            case 1: tiendaOnlineBox1 = bodymovin.loadAnimation(dataLottie); break;
            case 2: tiendaOnlineBox2 = bodymovin.loadAnimation(dataLottie); break;
            case 3: tiendaOnlineBox3 = bodymovin.loadAnimation(dataLottie); break;
            case 4: tiendaOnlineBox4 = bodymovin.loadAnimation(dataLottie); break;
            case 5: tiendaOnlineBox5 = bodymovin.loadAnimation(dataLottie); break;
        }

    });

    //taggeo -  promotionImpression al cargar
    function fnTaggeoPromotionImpression() {

        let DatosDatalayer = null;
        selectorTiendaOnline.querySelectorAll('.bcp_comp_tienda_online_v4[tag-tageeado-promotionimpression="false"]').forEach(function (box, index) {

            let id = box.getAttribute('contenido-id');
            let name = box.querySelector('.bcp_titulo').innerText.trim();
            let creative = box.querySelector('a').getAttribute('href');
            let position = 'Tienda Online - ' + box.getAttribute('attr-position');
            DatosDatalayer = {
                'event': 'promotionImpression',
                'ecommerce': {
                    'promoView': {
                        'promotions': [{
                            'id': id,
                            'name': name,
                            'creative': creative,
                            'position': position
                        }]
                    }
                }
            };

            if ((box.getBoundingClientRect().top <= window.innerHeight && box.getBoundingClientRect().bottom >= 0)) {
                dataLayer.push(DatosDatalayer);

                box.removeAttribute('tag-tageeado-promotionImpression');
            }

        });

    };

    function fnAgregarBoxesClickTiendaOnline() {
        let btn = this;
        let componente = fnGetParent(btn, '.bcp_comp_tienda_online_v4');
        let strTextoCard = componente.querySelector('.bcp_titulo').innerText.trim();
        if(componente.dataset.isExperimento=='true'){
            strTextoCard=strTextoCard+'-XT';
        }
       
        let posicion = componente.getAttribute('attr-position');
        let id = componente.getAttribute('contenido-id');
        let creative = componente.querySelector('a').getAttribute('href');

        let taggeoboton = {
            'clickType': 'Botón',
            'event': 'promotionClick',
            'ecommerce': {
                'promoClick': {
                    'promotions': [{
                        'id': id,
                        'name': strTextoCard,
                        'creative': creative,
                        'position': `Tienda Online - ${posicion}`

                    }]
                }
            }
        };

        dataLayer.push(taggeoboton);

        let taggeobotonClick = {
            'event': 'virtualEvent',
            'category': 'Tienda Online',
            'action': 'Seleccionar elemento',
            'label': strTextoCard

        };
        dataLayer.push(taggeobotonClick);

        //TAGGEO ADOBE
        digitalData.push({
            "action": {
                "group": "Home",
                "category": "Que Hacemos Hoy",
                "name": "Click",
                "label": cleanLabel(strTextoCard)
            },
            "event": "trackAction"
        });

    }

    function fnPlayLottieBox(index) {
        switch (index) {
            case '0':
                tiendaOnlineBox0.stop();
                tiendaOnlineBox0.play();
                break;
            case '1':
                tiendaOnlineBox1.stop();
                tiendaOnlineBox1.play();
                break;
            case '2':
                tiendaOnlineBox2.stop();
                tiendaOnlineBox2.play();
                break;
            case '3':
                tiendaOnlineBox3.stop();
                tiendaOnlineBox3.play();
                break;
            case '4':
                tiendaOnlineBox4.stop();
                tiendaOnlineBox4.play();
                break;
            case '5':
                tiendaOnlineBox5.stop();
                tiendaOnlineBox5.play();
                break;
        }
    }

    document.addEventListener("DOMContentLoaded", function () {

        //Hover Lottie
        function fnAgregarHover(box) {
            box.querySelector('.bcp_img_tienda').classList.add('hover');
            box.querySelector('.bcp_hover_animacion').classList.add('hover');
        }

        function fnRetirarHover(box) {
            box.querySelector('.bcp_img_tienda').classList.remove('hover');
            const eleHoverAnimation=box.querySelector('.bcp_hover_animacion');
            if(eleHoverAnimation){
                box.querySelector('.bcp_hover_animacion').classList.remove('hover');
            }
        }

        selectorTiendaOnline.querySelectorAll('.bcp_comp_tienda_online_v4').forEach((item) => {

            item.querySelector('.bcp_box').addEventListener('mouseenter', (event) => {
                let selector = event.currentTarget;
                const eleHoverAnimation=selector.querySelector('.bcp_hover_animacion');
                if(eleHoverAnimation){
                    let selectorIndex = eleHoverAnimation.getAttribute('attr-id');
                    fnPlayLottieBox(selectorIndex);
                    fnAgregarHover(selector);
                }
            });

            item.querySelector('.bcp_box').addEventListener('mouseleave', (event) => {
                let selector = event.currentTarget;
                fnRetirarHover(selector);
            });
        })

        //Taggeo click enlace
        try {
            selectorTiendaOnline.querySelectorAll('.bcp_acceso_tienda.bcp_btn_ver_todos').forEach((btn) => {
                btn.addEventListener("click", function () {
                    let datosTaggeo = {
                        'event': 'virtualEvent',
                        'category': 'Tienda Online',
                        'action': 'Clic enlace',
                        'label': btn.innerText.trim()
                    };
                    dataLayer.push(datosTaggeo);
                });
            });

        } catch (error) {
        };

        //Modal Ventas
        selectorTiendaOnline.querySelectorAll('.bcp_comp_tienda_online_v4 .bcp_box').forEach((btn) => {
            btn.addEventListener('click', fnAgregarBoxesClickTiendaOnline, false);
        });

        window.addEventListener("resize", function () {
            fnTaggeoPromotionImpression();
        });

        window.addEventListener("scroll", () => {
            fnTaggeoPromotionImpression();
        });
        window.addEventListener("orientationchange", () => {
            fnTaggeoPromotionImpression();
        });


    });

    window.addEventListener("load", () => {
        fnTaggeoPromotionImpression();
    });

   
    function IntCmpBox() {
        selectorTiendaOnline.querySelectorAll('.bcp_comp_tienda_online_v4').forEach((btn) => {
            const link = btn.querySelector('a');
            const posicion = btn.getAttribute('attr-position');
            const titulo = prepareString(btn.querySelector('.bcp_titulo').innerText);
            const parametros = `pcid=viabcp:home:${titulo}:masivo:que-hacemos-hoy${posicion}`;
            agregarParemetrosGet(link, parametros, 'Boxes Tienda Online');
        })
    }
    IntCmpBox();

    selectorTiendaOnline.querySelector('.bcp_enlace_componente_compartido').addEventListener('click', (event) => {
        let claseScroll = event.currentTarget.dataset.clase;

        if (claseScroll != '' && document.querySelector(`.${claseScroll}`)) {

            let topSeccion = document.querySelector(`.${claseScroll}`).offsetTop - 150;
            let container = cleanLabel(selectorTiendaOnline.querySelector('.bcp_titulo_principal').innerText);
            let btn = cleanLabel(event.currentTarget.innerText);

            digitalData.push({
                "element": {
                    "container": `Home-${container}`,
                    "type": "Link",
                    "name": btn
                },
                "event": "trackElement"
            })

            window.scroll({
                top: topSeccion,
                left: 0,
                behavior: 'smooth'
            })
        }
    })


    if (selectorTiendaOnline.querySelector('.bcp_componente_compartido_incrustado')) {

        selectorTiendaOnline.querySelectorAll('.bcp_bloque_ayuda_incrustado .bcp_opcion_enlace').forEach((btnEnlace) => {
            btnEnlace.addEventListener('click', (event) => {

                let category = selectorTiendaOnline.querySelector('.bcp_bloque_ayuda_incrustado .bcp_titulo_opciones').innerText;
                let label = event.currentTarget.innerText

                digitalData.push({
                    "action": {
                        "group": "Home",
                        "category": cleanLabel(category),
                        "name": "Click",
                        "label": cleanLabel(label)
                    },
                    "event": "trackAction"
                })

            })
        })

        selectorTiendaOnline.querySelectorAll('.bcp_bloque_ayuda_incrustado .bcp_enlace_seccion').forEach((enlace) => {

            enlace.addEventListener('click', (event) => {

                let container = cleanLabel(selectorTiendaOnline.querySelector('.bcp_bloque_ayuda_incrustado .bcp_titulo_opciones').innerText);
                let btn = cleanLabel(event.currentTarget.innerText);

                digitalData.push({
                    "element": {
                        "container": `Home-${container}`,
                        "type": "Link",
                        "name": btn
                    },
                    "event": "trackElement"
                })


            })
        })
    }

    const generaSaludoTiendaOnline = () => {

        if (selectorTiendaOnline.querySelector('.bcp_saludo_generado')) {
            let tiempo = new Date();
            let hora, horaActual;
            let saludoSeccion = '';

            with (tiempo) {
                hora = getHours();
                horaActual = hora + ":" + getMinutes() + ":" + getSeconds();
            }

            if (hora < 12) {
                saludos = "Buenos días";
            } else if (hora < 18) {
                saludos = "Buenas tardes";
            } else {
                saludos = "Buenas noches";
            }

            selectorTiendaOnline.querySelector('.bcp_saludo_generado').innerHTML = saludos;
        }
    }

    generaSaludoTiendaOnline();


    const aplicarPersonalizacionMBox=(boxExperimento)=>{
        if(!boxExperimento) return;
        
        const boxes= selectorTiendaOnline.querySelectorAll('.bcp_comp_tienda_online_v4');
        if(boxes){
            const boxSinPersonalizar=boxes[boxExperimento.orden];
            if(boxSinPersonalizar){
              
                boxSinPersonalizar.querySelector('.bcp_titulo').innerText=boxExperimento.titulo;
                boxSinPersonalizar.querySelector('.bcp_img_tienda img').setAttribute('src',boxExperimento.imagen);
                boxSinPersonalizar.querySelector('.bcp_img_tienda img').setAttribute('data-src',boxExperimento.imagen);
                boxSinPersonalizar.querySelector('a').setAttribute('href',boxExperimento.enlace);
                boxSinPersonalizar.querySelector('.bcp_box').style.background=boxExperimento.colorfondo;
                boxSinPersonalizar.querySelector('.bcp_titulo').style.color=boxExperimento.colortexto;
                boxSinPersonalizar.querySelector('.bcp_hover_animacion').remove();
                boxSinPersonalizar.dataset.isExperimento=true;
            }
        }

    }


    const mostrarExperimentoTiendaOnline = (intervaloRenderExperimentoCintillo) => {

        if (typeof adobe === 'undefined') return;
        if (!adobe.target) return;

        adobe.target.getOffer({
            "mbox": "viabcp-home-mbox-tiendaonline",
            "success": function (offer) {
                if (!offer) return;
                if (!offer[0]) return;

                if (!existeEnListaNegraCintillo) {
                  
                    offer[0].content[0].forEach(box=>{
                        aplicarPersonalizacionMBox(box);
                    })
                }

            },
            "error": function (status, error) {
                console.log('Error', status, error);
            }
        });
        clearInterval(intervaloRenderExperimentoCintillo)
    }

        const intervaloRenderExperimentoCintillo = setInterval(() => {

            if(window.location.pathname.indexOf('pymes')!=-1 ) {
                clearInterval(intervaloRenderExperimentoCintillo)
                return;
            }

            if(window.location.pathname.indexOf('empresas')!=-1 ) {
                clearInterval(intervaloRenderExperimentoCintillo)
                return;
            }
            mostrarExperimentoTiendaOnline(intervaloRenderExperimentoCintillo)
        }, 100);
    

</script>
        
        
    

    
        





        







    
