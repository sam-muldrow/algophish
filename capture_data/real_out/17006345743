<link rel="stylesheet" type="text/css" href="/wcm/connect/resources/userstories/css/formulario.css?subtype=css&v=92614">    

<script src="/wcm/connect/resources/userstories/js/formulario.js?subtype=javascript&v=213158"></script>
    
<script src="/wcm/connect/resources/userstories/modal/bcp-modal.js?subtype=javascript"></script>
<script>

    const redireccionarPaginaError = (codigoError, error) => {
        const LINK_REDIRECT_ERROR_APP = '/pagina-error'
        console.log('error', error);
        const codigoErrorEnvar = 'ERR' + codigoError;
        try {
            // Taggeo Error
            digitalData.push({
                "error": {
                    "code": codigoErrorEnvar,
                    "message": error
                },
                "event": "trackError"
            });
        } catch (error) {

        }

        window.location = `${LINK_REDIRECT_ERROR_APP}?error=${codigoErrorEnvar}`;
    }


    let formEntryPointMiEspacioBCP = new Formulario(document.querySelector('.bcp_entry_point_mi_espacio_bcp'), { geolocation: false });
    formEntryPointMiEspacioBCP.elementForm.querySelector('#bcp_tipo_nro_documento button').setAttribute('disabled', '');

    const origenProducto = formEntryPointMiEspacioBCP.elementForm.dataset.origenProducto;

    let leadsPortafolioTc = {};

    function verificarLeadsMiEspacioBCP(tokenCaptcha) {
        try {

            leadsPortafolioTc = {
                'origen': origenProducto,
                'tc': false,
                'ads': false,
                'cef': false,
                "ando": false,
                "al": false,
                "ptc": false,
                "cuotealo": false,
                'portafoliotc': false,
                'ctpymes': false,
                'ctconsumo': false,
                'yape': false,
                'seg': '',
                'ofertaMitigacion': false
            };

            const dni = formEntryPointMiEspacioBCP.getNroDocumento();
            const dniHash = fnsha256(dni);

            validarLeadsTcMiEspacioBCP(dniHash.toUpperCase(), tokenCaptcha);

        } catch (error) {
            console.error('error callback recaptcha', error);
            redireccionarPaginaError('005', error);
            resetearCaptchaMiEspacioBCP();
        }
    }

    const validarLeadsTcMiEspacioBCP = (dniHash, tokenCaptcha) => {
        try {
            




const URL_LEADS = 'https://www.viabcp.com/apis/public/miespaciobcp/leads-oferta-mitigacion';


            let requestOpcionsLeads = {
                method: 'POST',
                headers: {
                    "accept": "application/json",
                    "content-type": "application/json"
                },
                body: JSON.stringify({
                    "numeroDNI": dniHash,
                    "tipoFormulario": "MITIGACION",
                    "tipoProducto": "OFERTA MITIGACION",
                    "captchaToken": tokenCaptcha
                }),
                redirect: 'follow'
            };

            fetch(URL_LEADS, requestOpcionsLeads)
                .then((response) => {

                    if (response.status == '200') {
                        response.text().then((result) => {
                            piorizarProductoLeads(JSON.parse(result));

                        }).catch((error) => {
                            console.error('Error promesa: ', error);
                            redireccionarPaginaError('002', error);
                        });
                    } else {
                        console.error('Error status: ', response.status);
                        redireccionarPaginaError('001', response.status);
                    }

                }).catch((error) => {
                    console.error('Error fetch: ', error);
                    redireccionarPaginaError('003', error);
                });
        } catch (error) {
            console.error('Error funcion validarLeadsTcMiEspacioBCP: ', error);
            redireccionarPaginaError('004', error);
        }
    }

    const resetearCaptchaMiEspacioBCP = () => {
        try {
            grecaptcha.reset(formEntryPointMiEspacioBCP.Captcha.getAttribute('index'));
            Formulario.prototype.Procesando(false, formEntryPointMiEspacioBCP.BotonEnviar);
        } catch (error) {
            console.error('Error al resetear recapctha', error);
            redireccionarPaginaError('009', error);
        }
    }

    const isLeadMiEspacioBcp = () => {
        if ((leadsPortafolioTc.tc)
            || (leadsPortafolioTc.ads)
            || (leadsPortafolioTc.cef)
            || (leadsPortafolioTc.yape)
            || (leadsPortafolioTc.ando)
            || (leadsPortafolioTc.seg.toLocaleLowerCase() == 'soat')
            || (leadsPortafolioTc.al)
            || (leadsPortafolioTc.ptc)
            || (leadsPortafolioTc.cuotealo)
            || (leadsPortafolioTc.ctconsumo)
            || (leadsPortafolioTc.ctpymes)) {
            return true;
        }
        return false;
    }

    const redireccionarMiEspacioBCP = () => {

        const isLead = isLeadMiEspacioBcp() ? 'Lead' : 'No Lead';
        digitalData.push({
            "action": {
                "group": "Mi Espacio BCP",
                "category": origenProducto,
                "name": 'Descubrir',
                "label": 'Botón'
            },
            "event": "trackAction"
        });

        const urlOrigen = window.location.pathname.split('/').slice(-1)[0];
        let textOrigen = urlOrigen;
        if (urlOrigen == '') {
            textOrigen = 'Home'
        }

        const tituloMiEspacioBcp = cleanLabel(document.querySelector('.bcp_entry_point_mi_espacio_bcp .bcp_titulo').innerText.trim());
        const textTituloMiEspacioBcp = tituloMiEspacioBcp.split(' ').join('-');
        const textBottonDescubrir = formEntryPointMiEspacioBCP.elementForm.querySelector(".bcp_btn_enviar_form").innerText;
        const nuevoLink = '/mi-espacio-bcp?' + 'rfid=' + textOrigen + ':' + textTituloMiEspacioBcp + ':form:mebcp-' + textBottonDescubrir;

        window.location = nuevoLink.toLocaleLowerCase();
        setTimeout(() => {
            resetearCaptchaMiEspacioBCP();
        }, 500);
    }

    const piorizarProductoLeads = (result) => {
        try {
            if (!result) return;
            if (!result[0]) return;

            const leads = result[0].leadResponse;
            if (leads) {
                leads.forEach(lead => {
                    const producto = lead.producto;
                    const tipoLead = lead.tipoLead;

                    if (producto == 'TC') leadsPortafolioTc.tc = true;
                    if (producto == 'ADS') leadsPortafolioTc.ads = true;
                    if (producto == 'CEF') leadsPortafolioTc.cef = true;
                    if (producto == 'ANDO') leadsPortafolioTc.ando = true;
                    if (producto == 'AL') leadsPortafolioTc.al = true;
                    if (producto == 'PTC') leadsPortafolioTc.ptc = true;
                    if (producto == 'CUOTEALO') leadsPortafolioTc.cuotealo = true;
                    if (producto == 'YAPE') leadsPortafolioTc.yape = true;
                    
                    if (producto == 'CT') {
                        if (lead.tipoLead == 'Q1N') {
                            leadsPortafolioTc.ctpymes = true;
                        } else {
                            leadsPortafolioTc.ctconsumo = true;
                        }
                    }
                   
                    if (producto == 'SEG') {
                        if (tipoLead == '3') leadsPortafolioTc.seg = 'SOAT';
                    }
                })
            }

            const portafoliogt = result[0].promocionTCResponse;
            if (portafoliogt) {
                portafoliogt.forEach(portafolio => {
                    if (portafolio.producto == 'PORT' || portafolio.producto == 'GT') {
                        leadsPortafolioTc.portafoliotc = true;
                        leadsPortafolioTc.portafoliotcDetalle = portafolio;
                    }
                })
            }

            const ofertaMitigacion = result[0].ofertaMitigacionResponse;
            if (ofertaMitigacion) {
                const valorOfertaMitigacion = ofertaMitigacion[0];
                if (valorOfertaMitigacion) {
                    if (valorOfertaMitigacion.leadsOfertaMitigacion) {
                        leadsPortafolioTc.ofertaMitigacion = true;
                    }
                }
            }
            console.log('leadsPortafolioTc',leadsPortafolioTc);
            setCookieAllDomain("leadMiEspacioBCP", JSON.stringify(leadsPortafolioTc), (1000 * 60 * 60 * 1));//1 hora

            //Ejecutando pixel para pauta
            if (formEntryPointMiEspacioBCP.elementForm.getAttribute('attr-setpauta') != null &&
                (formEntryPointMiEspacioBCP.elementForm.getAttribute('attr-setpauta') == true
                    || formEntryPointMiEspacioBCP.elementForm.getAttribute('attr-setpauta') == 'true')) {
                let urlPagina = '/mi-espacio-bcp';

                try {
                    fbq('track', 'CompleteRegistration', { "content_name": "boton-dni" });
                    gtag_report_conversion(urlPagina);
                } catch (error) {
                    redireccionarPaginaError('006', error);
                    redireccionarMiEspacioBCP();
                }

            } else {
                redireccionarMiEspacioBCP();
            }
        } catch (error) {
            redireccionarPaginaError('007', error);
        }
    }

    formEntryPointMiEspacioBCP.elementForm.querySelector(".bcp_btn_enviar_form").addEventListener('click', (event) => {
        event.preventDefault();
        try {
            const titleForm = formEntryPointMiEspacioBCP.elementForm.querySelector('.bcp_titulo').innerText.trim();
            // document.querySelector('.bcp_formulario .bcp_form_integrado .bcp_titulo').innerText;
            let inputDocumento = formEntryPointMiEspacioBCP.elementForm.querySelector('form-cbodocumento');
            if (formEntryPointMiEspacioBCP.Validar.TipoNroDocumento(inputDocumento)) {

                Formulario.prototype.Procesando(true, formEntryPointMiEspacioBCP.BotonEnviar);
                grecaptcha.execute(formEntryPointMiEspacioBCP.Captcha.getAttribute('index'));
                //TAGGEO ADOBE
                digitalData.push({
                    "form": {
                        "channel": "ViaBCP",
                        "name": cleanLabel(titleForm)
                    },
                    "event": "trackFormSubmit"
                });
            }
        } catch (error) {
            redireccionarPaginaError('008', error);
            resetearCaptchaMiEspacioBCP();
        }

    });



</script>
<script src="/wcm/connect/resources/userstories/Lottiejs/lottie.js?subtype=javascript"></script>
<script>
  
        let rutaLottie = document.querySelector('.bcp_animacion_ruta').innerText;
        document.querySelector('.bcp_animacion_ruta').innerHTML = ""
        let dataLottie = {
            container: document.querySelector(`.entry_point_img_mobiles`),
            renderer: 'svg',
            loop: true,
            autoplay: true,
            path: rutaLottie
        };
        bodymovin.loadAnimation(dataLottie);
    

</script>
        
        
    

    
        





        







    
