		function setupSTNApi(getApi) {
			const stnPlayer = document.querySelector('div[data-stn-player]');
			const stnApi = getApi(stnPlayer);

			if (!stnApi) {
				console.error("Failed to retrieve STN API. Please reach out to your STN Video contact for troubleshooting.");
				return;
			}

			const alertBarEl = document.getElementById('global-alert');
			const hasAlertBar = alertBarEl && alertBarEl.innerHTML.trim() !== '';

			stnApi.floatApi.verticalTranslate = hasAlertBar ? 100 : 50;
		}
