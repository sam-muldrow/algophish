    <script>
        document.querySelectorAll('#list_match_tab a').forEach(function(element) {
            element.addEventListener('click', function(e) {
                e.preventDefault();
                document.querySelectorAll('#list_match_tab a').forEach(function(el) {
                    el.classList.remove('active');
                });
                this.classList.add('active');
                var id = this.dataset.id;
                document.querySelectorAll('#list_match_content .list_match_content_item').forEach(function(item) {
                    item.style.display = 'none';
                });
                document.getElementById(id).style.display = 'block';
            });
        });
    </script>
                                                

