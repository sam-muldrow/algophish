<script type="text/javascript">
	/*
	document.onreadystatechange = subSomething;
	function subSomething() {
		if (document.readyState == "complete")
			aaccTag();
	}
	
	function aaccTag() {
		var z = 350;
		$('#NewsContainer').find('a,input,select,textarea,button,object').not('.pane a,select,input').each(function(i, n) {
			var dvName = "";
			switch ($(n).attr("id")) {
			case "ashow1":
				dvName = "showdiv0";
				break;
			case "ashow2":
				dvName = "showdiv1";
				break;
			case "ashow3":
				dvName = "showdiv2";
				break;
			case "ashow4":
				dvName = "showdiv3";
				break;
			case "ashow5":
				dvName = "showdiv4";
				break;
			case "ashow6":
				dvName = "showdiv5";
				break;			
			default:
				break;
			}
			z++;
			$(this).attr("tabindex", z);
			//$(this).attr("ddd", z);
			
			if (dvName.length > 0) {
				$.each($("#" + dvName + " a," + "#" + dvName + " select," + "#" + dvName + " input"), function(j, k) {
					//z++;
					$(this).attr("tabindex", z);
				});
			}
		});
	}
	*/
	
	/*
	    概念：在頁籤標題 focus 的時候 賦予其他頁籤 tabindex = -1 
		      然後在頁籤 blur 的時候 清除。
			  
			  頁籤內容 each 聆聽 keydown 事件 
			  keyCode==9 (tab) 就 focus 至下個頁籤
			  event.shiftKey && event.keyCode==9 (shift + tab) 則 focus 至原本頁籤
	*/
	
	//A:頁籤 focus 的時候 其他頁籤設定 tabindex = -1
	//B:頁籤 blur 的時候 其他頁籤清除 tabindex = -1
	//C:頁籤按下 shift + tab 時 focus 至上個頁籤內容的最後一個
	
	$(document).ready(function(){
	
	$('#MainNews').find("#menu1 a").each(function(i,n){
	    //A
		$(this).focus(function(){
			$(this).addClass("onActive");
			$('#MainNews').find("#menu1 a").not(".onActive").each(function(j,m){
			    $(this).attr("tabindex" , "-1");
			});
		});
		//B
		$(this).blur(function(){
			$(this).removeClass("onActive");
			$('#MainNews').find("#menu1 a").each(function(j,m){
			    $(this).removeAttr("tabindex");
			});
		});
		//C
		var lastPaneName = "";
		var z = "0";
		switch ($(n).attr("id")) {
			case "ashow1":
				break;
			case "ashow2":
				lastPaneName = "showdiv0";
				z = "0";
				break;
			case "ashow3":
				lastPaneName = "showdiv1";
				z = "1";
				break;
			case "ashow4":
				lastPaneName = "showdiv2";
				z = "2";
				break;
			case "ashow5":
				lastPaneName = "showdiv3";
				z = "3";
				break;
			case "ashow6":
				lastPaneName = "showdiv4";
				z = "4";
				break;			
			default:
				break;
			}
		$(this).on("keydown",function(e){
		    var item = "";
			if(lastPaneName!=""){
			    item = $('#MainNews').find("#"+lastPaneName+" a:last-child ");
			}
		    var code = (e.keyCode ? e.keyCode : e.which);
			if((e.shiftKey) && (e.keyCode==9)){
			    if(item!=""){
			        e.preventDefault();//停止默認的 tab 行為
					setTab(1,z);
			        item.focus();
				}
			}
		});
	});
	
	//A:頁籤內容最後一個按下 tab 時 focus 至下個頁籤
	//B:頁籤內容第一個按下 shift + tab 時 focus 至原本頁籤
	
	$('#MainNews').find(".pane").each(function(i,n){
	    var tabsName = "";
		var nextTabsName = "";
				
		switch ($(n).attr("id")) {
			case "showdiv0":
				tabsName = "ashow1";
				nextTabsName = "ashow2";
				break;
			case "showdiv1":
				tabsName = "ashow2";
				nextTabsName = "ashow3";
				break;
			case "showdiv2":
				tabsName = "ashow3";
				nextTabsName = "ashow4";
				break;
			case "showdiv3":
				tabsName = "ashow4";
				nextTabsName = "ashow5";
				break;
			case "showdiv4":
				tabsName = "ashow5";
				nextTabsName = "ashow6";
				break;
			case "showdiv5":
				tabsName = "ashow6";
				break;			
			default:
				break;
		}
		//A
		$(this).find("a:last").on("keydown",function(e){
			//alert("last");
			var tab = "";
			if(nextTabsName!=""){
			    tab = $('#MainNews').find("a[id='"+nextTabsName+"']");
			}
			var code = (e.keyCode ? e.keyCode : e.which);
			if(code == 9 && !e.shiftKey){
				if(tab!=""){
				    e.preventDefault();//停止默認的 tab 行為
			        tab.focus();
				}
			}
		});
		//B
		$(this).find("a:first").on("keydown",function(e){
			//alert("first");
			var tab = $('#MainNews').find("a[id='"+tabsName+"']");
			var code = (e.keyCode ? e.keyCode : e.which);
			if((e.shiftKey) && (e.keyCode==9)){
			    e.preventDefault();//停止默認的 tab 行為
			    tab.focus();
			}
		});
	});
	
	});
	
	/*
	document.onreadystatechange = subSomething;
	function subSomething() {
		if (document.readyState == "complete")
			aacc();
	}
	
	function aacc() {
		var z = 1;
		$.each($('a,input,select,textarea,button,object').not('.pane a'), function(i, n) {
			var dvName = "";
			switch ($(n).attr("id")) {
			case "ashow1":
				dvName = "showdiv0";
				break;
			case "ashow2":
				dvName = "showdiv1";
				break;
			case "ashow3":
				dvName = "showdiv2";
				break;
			case "ashow4":
				dvName = "showdiv3";
				break;
			case "ashow5":
				dvName = "showdiv4";
				break;
			case "ashow6":
				dvName = "showdiv5";
				break;			
			default:
				break;
			}
			z++;
			$(this).attr("tabindex", z);
			if (dvName.length > 0) {
				$.each($("#" + dvName + " a"), function(j, k) {
					z++;
					$(this).attr("tabindex", z);
				});
			}
		});
	}
	*/
</script>

<script type="text/javascript">
	
	function setTab(m, n) {
		var tli = document.getElementById("menu" + m).getElementsByTagName("a");

		for (var i = 0; i < tli.length; i++) {
			tli[i].className = i == n ? "current" : "";
			document.getElementById("showdiv" + i).style.display = i == n ? "block" : "none";
			
		}
	}
	
</script>

<div id="NewsContainer">
	<div id="NewsInner">
		<div class="Header">
			<div class="acc-key" style="position:absolute;">
				<a accesskey="L" id="accesskey-l" href="#accesskey-l" title="左方區塊">:::</a>
			</div>
			<img src="/post/internet/Group/images/LatestNewsHeader.png" alt="最新消息" border="0" class="PC_pic"/>
			<img src="/post/internet/Group/images/RWDLatestNewsHeader.png" alt="最新消息" border="0" class="RWD_pic"/>
		</div>
		<div id="MainNews" class="news_tabs_wrap">
			<ul id="menu1" class="tabs clear">
				<li><a id="ashow1" href="#A-1" class="current" onfocus="setTab(1,0);return false;">全部</a></li>
				<li><a id="ashow2" href="#B-1" onfocus="setTab(1,1);return false;">郵務</a></li>
				<li><a id="ashow3" href="#C-1" onfocus="setTab(1,2);return false;">儲匯</a></li>
				<li><a id="ashow4" href="#D-1" onfocus="setTab(1,3);return false;">壽險</a></li>
				<li><a id="ashow5" href="#E-1" onfocus="setTab(1,4);return false;">集郵</a></li>
				<li class="last"><a id="ashow6" href="#G-1" onfocus="setTab(1,5);return false;">房地產出租</a></li>
			</ul>

			<!--全部-->			
			<div id="showdiv0" class="pane" data-newsCollect="0" >
				







 



	
	
	
		
