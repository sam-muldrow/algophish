	<div class="DirectLink_2">
		<a id="DirectLink_right_arr" class="Link_Second FloatLeft" href="javascript:void(0)" onclick="$('#main-slider_new').cycle('goto',1);"  onkeypress="$('#main-slider_new').cycle('goto',1);" >代售、房地產、理財、企業社會責任</a>
		<div class="CLR"></div>
	</div>

		
<script type="text/javascript">
$(document).ready(function(){
  $('#main-slider_new').cycle();
  $('#main-slider_new').find('a,input').each(function(){$(this).on("blur",function(){$('#main-slider_new').cycle('resume');});});
  $('#main-slider_new').find('a,input').each(function(){$(this).on("focus",function(){$('#main-slider_new').cycle('pause');});});
  $('#main-slider_new').find('a,input').each(function(){$(this).keydown(function(e){if(e.keyCode == '37'){
	if($('#main-slider_new').data("cycle.opts").currSlide==1){
	$('#main-slider_new').cycle('goto',0);
	setTimeout(function(){$('.ServiceLinkContainerWrap[data-cycle-my-index="0"]').find('a,input').last().focus();},800);}
  }});});
  $('#main-slider_new').find('a,input').each(function(){$(this).keydown(function(e){if(e.keyCode == '39'){
	if($('#main-slider_new').data("cycle.opts").currSlide==0){
	$('#main-slider_new').cycle('goto',1);
	setTimeout(function(){$('.ServiceLinkContainerWrap[data-cycle-my-index="1"]').find('a,input').first().focus();},800);}
  }});});
  $('.ServiceLinkContainerWrap[data-cycle-my-index="0"]').find('a,input').last().keydown(function(e){if(e.keyCode=='9' && !e.shiftKey){
	$('#main-slider_new').cycle('goto',1);
	setTimeout(function(){$('.ServiceLinkContainerWrap[data-cycle-my-index="1"]').find('a,input').first().focus();},800);}});
  $('.ServiceLinkContainerWrap[data-cycle-my-index="1"]').find('a,input').first().keydown(function(e){if(e.keyCode=='9' && e.shiftKey){
	$('#main-slider_new').cycle('goto',0);
	setTimeout(function(){$('.ServiceLinkContainerWrap[data-cycle-my-index="0"]').find('a,input').last().focus();},800);}});
});
</script>
	
