			$('.bxslider').slick({
				dots: true,
				infinite: true,
				speed: 1000,
				slidesToShow: 1,
				slidesToScroll: 1,
				autoplay: true,
				autoplaySpeed: 3000,
				accessibility: true
			});
			
			 $('.marquee-box').slick({
				 slidesToShow: 1,
				 slidesToScroll: 1,
				 vertical: true,
				 autoplay: true,
				 speed: 1000,
				 dots: false,
				 autoplaySpeed: 3000,
				 accessibility: true
			 });
			
			
			var bxslider_small_1 = $('.bxslider_small').bxSlider({
				auto: true,
				autoHover: true,
				tickerHover:true,
				onSlideAfter: function (slide, oldIndex , newIndex){
				  bxslider_small_1.stopAuto();
				},
				minSlides: 0,
				maxSlides: 2,
				slideWidth: 191,
				slideMargin: 0,
				infiniteLoop: false,
				pager: false
			});
