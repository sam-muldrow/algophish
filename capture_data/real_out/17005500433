                slideWidth: 191,
                slideMargin: 10,
				infiniteLoop: false,
                pager: false
			});
			setTimeout(function(){
    			$('.bxslider_small_3').find('a').each(function(){$(this).on("focus",function(){bxslider_small_3.stopAuto();});});
			},1000);
		});
	</script>
        
