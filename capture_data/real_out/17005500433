		//setTimeout("set_index_ad_setH()",500);
	});	

	function set_index_ad_setH()
	{
		var tablew = $('.bx-viewport').width();
		//alert("tablew:"+tablew);		
		var tableh = $('.bx-viewport').height();
		//alert("tableh:"+tableh);			
		$('.bx-viewport').css("height","100%"); 
	}
</script>
	
			
