<script>
    function goToComments(url) {
        window.location = `${url}#comments`;
        const getMeTo = document.getElementById("comments");
        const elementRect = getMeTo.getBoundingClientRect().top - 100;
        window.scrollTo({
            top: elementRect,
            behavior: 'smooth'
        });
    }
</script>

<script>
    document.addEventListener('DOMContentLoaded', function() {
        const url = "https://next.ink/wp-admin/admin-ajax.php";
        const user_is_logging = "";
        const postsId = JSON.parse("[48655,36322,36328,36324,683,679]");
        
        if (user_is_logging === "1") {
            const data = {
                action: 'get_comment_diff',
                post_ids: postsId
            }
            $.ajax({
                url: url,
                type: 'POST',
                data: data,
                success: function (data) {
                    if (data.length !== 0){
                        data.forEach((element) => {
                            $("#list-heart .new-comment-" + element.post_id).append(`<span class="separator-point"></span><span>${element.new_comment}</span>`);
                        })
                    }
                },
                error: function (error) {
                    console.log(error)
                }
            })
        }
    })
