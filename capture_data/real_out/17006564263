<script>
    window.addEventListener('resize', function(event) {
        if (window.innerWidth > 900) {
            var dropdownMobileBurger = document.getElementById("mobile-burger-container");

            dropdownMobileBurger.classList.remove("mobile-burger-container-show");

            var burgerMenu = document.getElementById("burger-menu");

            burgerMenu.classList.remove("open-mobile-burger-container");

        }
    }, true);

    function toggleMobileBurger() {
        var dropdownMobileBurger = document.getElementById("mobile-burger-container");

        dropdownMobileBurger.classList.toggle("mobile-burger-container-show");

        var burgerMenu = document.getElementById("burger-menu");

        burgerMenu.classList.toggle("open-mobile-burger-container");

        document.getElementsByTagName('body')[0].classList.toggle('blocked-body-header');
    }
</script>


<script defer>
    function active() {
        const search = document.getElementById("search");
        search.classList.remove("inactive");
        search.classList.add("active");
    }

    function inactive() {
        const search = document.getElementById("search");

        search.classList.add("inactive");
        search.classList.remove("active");
    }
</script>

<script>
    function destroy_session_user(url) {
        $.ajax({
            type: 'POST',
            url: url,
            dataType: 'json',
            data: {
                action: 'get_ajax_deco',
            },
            hearders: {
                'Content-Type': 'application/x-www-form-urlencoded',
                'Cache-Control': 'no-cache',
            },
            success: function(res) {
                window.location = `${window.origin}/`;
            }
        })
    }
</script>

<script>
    window.addEventListener('click', function(event) {
        const element = document.getElementById('menu_profil');
        if (element !== null) {
            if (document.getElementById('nav-right').contains(event.target) !== false) {
                return "";
            } else {
                element.classList.add("menu_profil_close");
            }
        }
    });

    function open_profil_menu() {
        const element = document.getElementById('menu_profil');
        element.classList.toggle("menu_profil_close");
    }
</script>

