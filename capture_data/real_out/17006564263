        //                     document.getElementById("previous").setAttribute("data-date", previous.toISOString().split('T')[0]);
        //                     const urlParams = new URLSearchParams(window.location.search);
        //                     urlParams.set("begin", previous.toISOString().split('T')[0]);
        //                     urlParams.set("end", nextDate.toISOString().split('T')[0]);
        //                     const newUrl = `${window.location.protocol}//${window.location.hostname}:${window.location.port}?${urlParams.toString()}`;
        //                     getPostScroll(previous.toISOString().split('T')[0], nextDate.toISOString().split('T')[0]);
        //                 // }
        //             }
        //         }
        //     }
        // });

        $("#list-content").scroll(function () {
            if (window.innerWidth < 1100) {
                if ($(this).scrollTop() + $(this).innerHeight() + 60 >= $(this)[0].scrollHeight) {
                    // if (canBeLoaded === true) {
                        if ($("#list-content").scrollTop() > ($("#list-content").height() - bottomOffset - $(window).height())) {
                            // console.log($("#list-content").scrollTop());
                            // console.log(($("#list-content").height() - bottomOffset - $(window).height()));
                            const elementDomPrevious = new Date(document.getElementById("previous").getAttribute('data-date'));
                            const nextDate = new Date(elementDomPrevious);
                            document.getElementById("next-date").setAttribute("data-date", elementDomPrevious.toISOString().split('T')[0]);
                            const previous = new Date(elementDomPrevious.setDate(elementDomPrevious.getDate() - 7));
                            document.getElementById("previous").setAttribute("data-date", previous.toISOString().split('T')[0]);
                            const urlParams = new URLSearchParams(window.location.search);
                            urlParams.set("begin", previous.toISOString().split('T')[0]);
                            urlParams.set("end", nextDate.toISOString().split('T')[0]);
                            const newUrl = `${window.location.protocol}//${window.location.hostname}:${window.location.port}?${urlParams.toString()}`;
                            getPostScroll(previous.toISOString().split('T')[0], nextDate.toISOString().split('T')[0]);
                        // }
                    }
                }
            }
        });
    })

    document.addEventListener('DOMContentLoaded', function() {
        if (window.innerWidth < 1100) {
            if ($("#list-content").height() > $("#home-article-reload").height()) {
                let urlParams = new URLSearchParams(window.location.search);
                let newUrl = `${window.location.protocol}//${window.location.hostname}:${window.location.port}?${urlParams.toString()}`;
                // while ($("#list-content").height() > $("#home-article-reload").height()) {
                    const elementDomPreviousSave = new Date(document.getElementById("previous").getAttribute('data-date'));
                    const nextDate = new Date();
                    const elementDomPrevious = new Date(document.getElementById("previous").getAttribute('data-date'));
                    const previous = new Date(elementDomPrevious.setDate(elementDomPrevious.getDate() - 7));
                    urlParams.set("begin", previous.toISOString().split('T')[0]);
                    urlParams.set("end", elementDomPreviousSave.toISOString().split('T')[0]);
                    newUrl = `${window.location.protocol}//${window.location.hostname}:${window.location.port}?${urlParams.toString()}`;
                    getPostScroll(previous.toISOString().split('T')[0], nextDate.toISOString().split('T')[0]);
                // }
            }
        }
    });
