<script>
    function loadCommentBrief() {
        const url = "https://next.ink/wp-admin/admin-ajax.php";
        const user_is_logging = "";
        const postsId = JSON.parse("[117194,117204,117209,117081,117200,117198,117241]");
        if (user_is_logging === "1") {
            const data = {
                action: 'get_comment_diff',
                post_ids: postsId
            }
            $.ajax({
                url: url,
                type: 'POST',
                data: data,
                success: function (data) {
                    if (Array.isArray(data) && data.length > 0){
                        data.forEach((element) => {
                            $(".new-comment-" + element.post_id).append(`<span class="separator-point"></span><span>${element.new_comment}</span>`);
                        })
                    }
                },
                error: function (error) {
                    console.log(error)
                }
            })
        }
    }
    document.addEventListener('DOMContentLoaded', function() {
        loadCommentBrief();
    })
</script>
    </div>
</div>

<script>
    function get_posts_by_date(url, e) {
        const urlParams = new URLSearchParams(window.location.search);
        urlParams.set("brief", e.value);
        urlParams.toString();
        const newUrl = `${window.location.protocol}//${window.location.hostname}:${window.location.port}?${urlParams.toString()}`;
        history.pushState(null, null, newUrl);
        $("#test").load(location.href + " #test", function () {
            loadCommentBrief();
        });
    }
</script>                </div>
            </div>
            <div id="list-hearts" class="list-hearts">
