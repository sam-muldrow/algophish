                <span onclick="categoryRedirect('droit', 3)" class="categories-button footer-categories-droit" ><span class="categories-puce categories-puce-droit"></span><span>Droit</span></span><span onclick="categoryRedirect('économie', 4)" class="categories-button footer-categories-economie" ><span class="categories-puce categories-puce-economie"></span><span>Économie</span></span><span onclick="categoryRedirect('flock', 13)" class="categories-button footer-categories-flock" ><span class="categories-puce categories-puce-flock"></span><span>Flock</span></span><span onclick="categoryRedirect('hardware', 9)" class="categories-button footer-categories-hardware" ><span class="categories-puce categories-puce-hardware"></span><span>Hardware</span></span><span onclick="categoryRedirect('ia-et-algorithmes', 6)" class="categories-button footer-categories-ia" ><span class="categories-puce categories-puce-ia"></span><span>IA et algorithmes</span></span><span onclick="categoryRedirect('internet', 7)" class="categories-button footer-categories-web" ><span class="categories-puce categories-puce-web"></span><span>Internet</span></span><span onclick="categoryRedirect('logiciel', 8)" class="categories-button footer-categories-logical" ><span class="categories-puce categories-puce-logical"></span><span>Logiciel</span></span><span onclick="categoryRedirect('next', 14)" class="categories-button footer-categories-next" ><span class="categories-puce categories-puce-next"></span><span>Next</span></span><span onclick="categoryRedirect('réseaux-sociaux', 5)" class="categories-button footer-categories-social" ><span class="categories-puce categories-puce-social"></span><span>Réseaux sociaux</span></span><span onclick="categoryRedirect('science-et-espace', 10)" class="categories-button footer-categories-space" ><span class="categories-puce categories-puce-space"></span><span>Science et espace</span></span><span onclick="categoryRedirect('sécurité', 12)" class="categories-button footer-categories-security"><span class="categories-puce categories-puce-security"></span><span>Sécurité</span></span><span onclick="categoryRedirect('société-numérique', 11)" class="categories-button footer-categories-numerique" ><span class="categories-puce categories-puce-numerique"></span><span>Société numérique</span></span><script>
   function categoryRedirect(slug, id) {
    var url = "/category-page?category=" + slug + "&id=" + id;
    window.location.href = url;
}
</script>
<script defer>
    function filterCategories(url, event) {
        if (event.parentElement.id === "category-filter") {
            event.classList.toggle("active-filter");
            const category = document.getElementById('category-filter');
            const categoriesElementDom = Array.from(category.getElementsByClassName('active-filter'));
            const categoriesId = categoriesElementDom.map((element) => element.getAttribute('data-id'));
            const urlParams = new URLSearchParams(window.location.search);
            $.ajax({
                type: 'POST',
                url: url,
                dataType: 'json',
                data: {
                    action: 'filter_by_categories',
                    categories: categoriesId,
                    paged: urlParams.get('paged') || undefined
                },
                hearders: {
                    'Content-Type': 'application/x-www-form-urlencoded',
                    'Cache-Control': 'no-cache',
                },
                success: function(res) {
                    history.pushState(null, null, res.data.base);
                    $("#test-category").load(location.href + " #test-category", function(event) {
                        if (res.data.ids.length !== 0 && res.data.ids !== null) {
                            const user_is_logging = "";
                            if (user_is_logging === "1") {
                                const data = {
                                    action: 'get_comment_diff',
                                    post_ids: res.data.ids
                                }
                                $.ajax({
                                    url: url,
                                    type: 'POST',
                                    data: data,
                                    success: function (data) {
                                        if (data.length !== 0){
                                            data.forEach((element) => {
                                                $("#home-category .new-comment-" + element.post_id).append(`<span class="separator-point"></span><span>${element.new_comment}</span>`);
                                            })
                                        }
                                    },
                                    error: function (error) {
                                        console.log(error)
                                    }
                                })
                            }
                        }
                    });
                }
            })
        }
    }
</script>
