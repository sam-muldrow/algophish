    <script>
        function toggleFooterMessage() {
            var footerMojiMessage = document.querySelector(".footer-moji-message");
            var seeMoreLess = document.querySelector('.footer-see-more-less');
            var footerMojiMessageToggleSvg = document.querySelector(".footer-moji-message-toggle>svg");

            footerMojiMessage.classList.toggle("footer-moji-mini");
            if (seeMoreLess.textContent === 'Voir plus') {
                seeMoreLess.textContent = 'Voir moins';
            } else {
                seeMoreLess.textContent = 'Voir plus';
            }
            footerMojiMessageToggleSvg.classList.toggle("see-less")
        }
    </script>

<!-- <script>
    var listBrief = document.getElementById('list-brief');
    var footer = document.getElementById('footer');
    window.addEventListener('scroll', function() {
        console.log(window.innerHeight);
        let footerClientY = footer.getBoundingClientRect();
        let listBriefClientY = listBrief.getBoundingClientRect();
        console.log(footerClientY);
        if (window.innerHeight > footerClientY.top) {
            listBrief.style.top = `${window.innerHeight - footerClientY.bottom}px`;
            // listBrief.style.bottom = `${footerClientY.bottom - footerClientY.height}px`;
            console.log(footerClientY.top - footerClientY.height);
            console.log(window.innerHeight - footerClientY.bottom + footerClientY.height);
        }
    })
</script> -->

<script>

    var brief = document.getElementById('list-brief');
    var continu = document.getElementById('list-continu');
    var content = document.getElementById('list-content');

    window.onresize = function(event) {
        let positionClientY = continu.getBoundingClientRect().top - 50;
        if (window.innerWidth < 1100) {
            brief.classList.remove("fixed-brief");
        }
        if (window.innerWidth > 1100 && window.innerWidth < 1400) {
            if (positionClientY < 0) {
                brief.style.right = content.getBoundingClientRect().left - 10 + "px";
            }
        }
        if (window.innerWidth > 1400) {
            brief.style.right = content.getBoundingClientRect().left - 10 + "px";
        }
    };

    window.addEventListener('scroll', function() {
        let positionClientY = continu.getBoundingClientRect().top - 50;
        if (window.innerWidth > 1100) {
            if (positionClientY < 0) {
                brief.classList.add("fixed-brief");
                brief.style.right = content.getBoundingClientRect().left - 10 + "px";
            } else {
                brief.classList.remove("fixed-brief");
            }
        }
    })

    function navTab(name) {
        if (name !== null) {
            if (name === 'continu') {
                const hearts = document.getElementById("hearts");
                hearts.classList.add("not-actif");
                hearts.classList.remove("actif");
                const listHearts = document.getElementById("list-hearts");
                listHearts.classList.add("list-hearts");
                listHearts.classList.remove("list-hearts-actif");
                const brief = document.getElementById("brief");
                brief.classList.remove("actif");
                brief.classList.add("not-actif");
                const listBrief = document.getElementById("list-brief");
                listBrief.classList.remove("list-brief-actif");
                listBrief.classList.add("list-brief");
                const continu = document.getElementById("continu");
                continu.classList.remove("not-actif");
                continu.classList.add("actif");
                const listContinu = document.getElementById("list-continu");
                listContinu.classList.remove("list-continu");
                listContinu.classList.add("list-continu-actif");
            } else if (name === 'brief') {
                const hearts = document.getElementById("hearts");
                hearts.classList.add("not-actif");
                hearts.classList.remove("actif");
                const listHearts = document.getElementById("list-hearts");
                listHearts.classList.add("list-hearts");
                listHearts.classList.remove("list-hearts-actif");
                const continu = document.getElementById("continu");
                continu.classList.remove("actif");
                continu.classList.add("not-actif");
                const listContinu = document.getElementById("list-continu");
                listContinu.classList.remove("list-continu-actif");
                listContinu.classList.add("list-continu");
                const brief = document.getElementById("brief");
                brief.classList.remove("brief");
                brief.classList.remove("not-actif");
                brief.classList.add("actif");
                const listBrief = document.getElementById("list-brief");
                listBrief.classList.remove("list-brief");
                listBrief.classList.add("list-brief-actif");
            } else {
                const continu = document.getElementById("continu");
                continu.classList.remove("actif");
                continu.classList.add("not-actif");
                const listContinu = document.getElementById("list-continu");
                listContinu.classList.remove("list-continu-actif");
                listContinu.classList.add("list-continu");
                const brief = document.getElementById("brief");
                brief.classList.remove("actif");
                brief.classList.add("not-actif");
                const listBrief = document.getElementById("list-brief");
                listBrief.classList.add("list-brief");
                listBrief.classList.remove("list-brief-actif");
                const hearts = document.getElementById("hearts");
                hearts.classList.remove("not-actif");
                hearts.classList.add("actif");
                const listHearts = document.getElementById("list-hearts");
                listHearts.classList.remove("list-hearts");
                listHearts.classList.add("list-hearts-actif");
            }
        }
    }
</script>