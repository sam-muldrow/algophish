<script>
    function goToComments(url) {
        window.location = `${url}#comments`;
        const getMeTo = document.getElementById("comments");
        const elementRect = getMeTo.getBoundingClientRect().top - 100;
        window.scrollTo({
            top: elementRect,
            behavior: 'smooth'
        });
    }
</script>
