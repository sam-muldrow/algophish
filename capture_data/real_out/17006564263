<script>
    function loadCommentPost(ids) {
        const url = "https://next.ink/wp-admin/admin-ajax.php";
        const user_is_logging = "";
        const postsId = ids !== undefined ? ids : JSON.parse("[117070,117059,117247,117055,117054,48655,687,676,682,677,678,36303,693,679,680,686,681,685,683,690,684,36311,688,689,36316,692,695,691,36323,698]");
        if (user_is_logging === "1") {
            const data = {
                action: 'get_comment_diff',
                post_ids: postsId
            }
            $.ajax({
                url: url,
                type: 'POST',
                data: data,
                success: function (data) {
                    if (data.length !== 0){
                        data.forEach((element) => {
                            if ($("#test-category .new-comment-" + element.post_id).children().length === 0) {
                                $("#test-category .new-comment-" + element.post_id).append(`<span class="separator-point"></span><span>${element.new_comment}</span>`);
                            }
                        })
                    }
                },
                error: function (error) {
                    console.log(error)
                }
            })
        }
    }

    function previous(event) {
        const elementDomPrevious = new Date(event.getAttribute('data-date'));
        const nextDate = new Date(elementDomPrevious);
        const previousDate = new Date(elementDomPrevious);
        previousDate.setDate(previousDate.getDate() - 7);
        const urlParams = new URLSearchParams(window.location.search);
        urlParams.set("begin", previousDate.toISOString().split('T')[0]);
        urlParams.set("end", nextDate.toISOString().split('T')[0]);
        urlParams.toString();
        const newUrl = `${window.location.protocol}//${window.location.hostname}:${window.location.port}?${urlParams.toString()}`;
        history.pushState(null, null, newUrl);
        $("#test-category").load(location.href + " #test-category", function(event) {
            loadCommentPost();
        })
        $('html, body').animate({ scrollTop: 0 }, 'fast');
        // smooth
    }

    function next(event) {
        const elementDomNext = new Date(event.getAttribute('data-date'));
        const previousDate = new Date(elementDomNext);
        const nextDate = new Date(elementDomNext);
        nextDate.setDate(previousDate.getDate() + 7);
        const urlParams = new URLSearchParams(window.location.search);
        urlParams.set("begin", previousDate.toISOString().split('T')[0]);
        urlParams.set("end", nextDate.toISOString().split('T')[0]);
        urlParams.toString();
        const newUrl = `${window.location.protocol}//${window.location.hostname}:${window.location.port}?${urlParams.toString()}`;
        history.pushState(null, null, newUrl);
        $("#test-category").load(location.href + " #test-category", function(event) {
            loadCommentPost();
        })
        $('html, body').animate({ scrollTop: 0 }, 'fast');
        // smooth
    }
    const canBeLoaded = true;
    const bottomOffset = 10; // Distance depuis le bas de la page pour déclencher le chargement

    function getPostScroll(begin, end) {
        const url = "https://next.ink/wp-admin/admin-ajax.php";
        $.ajax({
            url: url,
            type: 'POST',
            data: {
                action: "mobile_scroll_loading",
                begin: begin,
                end: end
            },
            success: function (res) {
                $('#home-article-reload').append(res.data.response);
                loadCommentPost(res.data.ids);
            },
            error: function (error) {
                console.log(error)
            }
        })
    }

    let count = 0;
    $(document).ready(function($) {
        $( window ).on( "resize", function() {
            if (window.innerWidth < 1100) {
                if (count === 0) {
                    const nextDate = new Date();
                    const elementDomPrevious = new Date(document.getElementById("previous").getAttribute('data-date'));
                    const urlParams = new URLSearchParams(window.location.search);
                    urlParams.set("begin", elementDomPrevious.toISOString().split('T')[0]);
                    urlParams.set("end", nextDate.toISOString().split('T')[0]);
                    const newUrl = `${window.location.protocol}//${window.location.hostname}:${window.location.port}?${urlParams.toString()}`;
                    history.pushState(null, null, newUrl);
                    $("#test-category").load(location.href + " #test-category", function(event) {
                        loadCommentPost();
                    })
                }
                count = 1;
            } else {
                if (count === 0) {
                    const elementDomPrevious = new Date(document.getElementById("previous").getAttribute('data-date'));
                    const previous = elementDomPrevious;
                    const nextDate = new Date(document.getElementById("next-date").getAttribute('data-date'));
                    const urlParams = new URLSearchParams(window.location.search);
                    urlParams.set("begin", previous.toISOString().split('T')[0]);
                    urlParams.set("end", nextDate.toISOString().split('T')[0]);
                    const newUrl = `${window.location.protocol}//${window.location.hostname}:${window.location.port}?${urlParams.toString()}`;
                    history.pushState(null, null, newUrl);
                    $("#test-category").load(location.href + " #test-category", function(event) {
                        loadCommentPost();
                    })
                }
                count = 0;
            }
        })
    })
    
    $(document).ready(function($) {
        // $("#list-continu").scroll(function () {
        //     if (window.innerWidth < 1100) {
        //         if ($(this).scrollTop() + $(this).innerHeight() >= $(this)[0].scrollHeight) {
