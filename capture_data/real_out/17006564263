    <span data-id="3" onclick="filterCategories('https://next.ink/wp-admin/admin-ajax.php', this)" id="droit" class="categories-button "><span class="categories-puce categories-puce-droit"></span><span>Droit</span></span><span data-id="4" onclick="filterCategories('https://next.ink/wp-admin/admin-ajax.php', this)" id="economie" class="categories-button "><span class="categories-puce categories-puce-economie"></span><span>Économie</span></span><span data-id="13" onclick="filterCategories('https://next.ink/wp-admin/admin-ajax.php', this)" id="flock" class="categories-button "><span class="categories-puce categories-puce-flock"></span><span>Flock</span></span><span data-id="9" onclick="filterCategories('https://next.ink/wp-admin/admin-ajax.php', this)" id="hardware" class="categories-button "><span class="categories-puce categories-puce-hardware"></span><span>Hardware</span></span><span data-id="6" onclick="filterCategories('https://next.ink/wp-admin/admin-ajax.php', this)" id="ia" class="categories-button "><span class="categories-puce categories-puce-ia"></span><span>IA et algorithmes</span></span><span data-id="7" onclick="filterCategories('https://next.ink/wp-admin/admin-ajax.php', this)" id="web" class="categories-button "><span class="categories-puce categories-puce-web"></span><span>Internet</span></span><span data-id="8" onclick="filterCategories('https://next.ink/wp-admin/admin-ajax.php', this)" id="logical" class="categories-button "><span class="categories-puce categories-puce-logical"></span><span>Logiciel</span></span><span data-id="14" onclick="filterCategories('https://next.ink/wp-admin/admin-ajax.php', this)" id="next" class="categories-button "><span class="categories-puce categories-puce-next"></span><span>Next</span></span><span data-id="5" onclick="filterCategories('https://next.ink/wp-admin/admin-ajax.php', this)" id="social" class="categories-button "><span class="categories-puce categories-puce-social"></span><span>Réseaux sociaux</span></span><span data-id="10" onclick="filterCategories('https://next.ink/wp-admin/admin-ajax.php', this)" id="space" class="categories-button "><span class="categories-puce categories-puce-space"></span><span>Science et espace</span></span><span data-id="12" onclick="filterCategories('https://next.ink/wp-admin/admin-ajax.php', this)" id="security" class="categories-button "><span class="categories-puce categories-puce-security"></span><span>Sécurité</span></span><span data-id="11" onclick="filterCategories('https://next.ink/wp-admin/admin-ajax.php', this)" id="numerique" class="categories-button "><span class="categories-puce categories-puce-numerique"></span><span>Société numérique</span></span><script>
   function categoryRedirect(slug, id) {
    var url = "/category-page?category=" + slug + "&id=" + id;
    window.location.href = url;
}
</script>
<script defer>
    function filterCategories(url, event) {
        if (event.parentElement.id === "category-filter") {
            event.classList.toggle("active-filter");
            const category = document.getElementById('category-filter');
            const categoriesElementDom = Array.from(category.getElementsByClassName('active-filter'));
            const categoriesId = categoriesElementDom.map((element) => element.getAttribute('data-id'));
            const urlParams = new URLSearchParams(window.location.search);
            $.ajax({
                type: 'POST',
                url: url,
                dataType: 'json',
                data: {
                    action: 'filter_by_categories',
                    categories: categoriesId,
                    paged: urlParams.get('paged') || undefined
                },
                hearders: {
                    'Content-Type': 'application/x-www-form-urlencoded',
                    'Cache-Control': 'no-cache',
                },
                success: function(res) {
                    history.pushState(null, null, res.data.base);
                    $("#test-category").load(location.href + " #test-category", function(event) {
                        if (res.data.ids.length !== 0 && res.data.ids !== null) {
                            const user_is_logging = "";
                            if (user_is_logging === "1") {
                                const data = {
                                    action: 'get_comment_diff',
                                    post_ids: res.data.ids
                                }
                                $.ajax({
                                    url: url,
                                    type: 'POST',
                                    data: data,
                                    success: function (data) {
                                        if (data.length !== 0){
                                            data.forEach((element) => {
                                                $("#home-category .new-comment-" + element.post_id).append(`<span class="separator-point"></span><span>${element.new_comment}</span>`);
                                            })
                                        }
                                    },
                                    error: function (error) {
                                        console.log(error)
                                    }
                                })
                            }
                        }
                    });
                }
            })
        }
    }
</script>
</div>
