<script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script defer>
    function activeMobile() {
        const searchMobile = document.getElementById("search-mobile");
        searchMobile.classList.remove("inactiveMobile");
        searchMobile.classList.add("activeMobile");
    }

    function inactiveMobile() {
        const searchMobile = document.getElementById("search-mobile");
        searchMobile.classList.add("inactiveMobile");
        searchMobile.classList.remove("activeMobile");
    }
</script>

<script>
    const burgerMenuCategories = document.getElementById('burger-menu-categories');
    const burgerMenuElementFollowUs = document.getElementById('burger-menu-element-follow-us');
    const burgerMenuElementAboutUs = document.getElementById('burger-menu-element-about-us');

    const burgerCategoryTitleCategory = document.getElementById('burger-category-title-category');
    const burgerCategoryTitleFollow = document.getElementById('burger-category-title-follow');
    const burgerCategoryTitleAbout = document.getElementById('burger-category-title-about');


    function toggleBurgerMenuElement1() {
        burgerCategoryTitleCategory.classList.add('burger-category-title-rotate');
        burgerCategoryTitleFollow.classList.remove('burger-category-title-rotate');
        burgerCategoryTitleAbout.classList.remove('burger-category-title-rotate');

        burgerMenuCategories.classList.replace('burger-menu-element-hide', 'burger-menu-element-show');
        burgerMenuElementFollowUs.classList.replace("burger-menu-element-show", "burger-menu-element-hide");
        burgerMenuElementAboutUs.classList.replace("burger-menu-element-show", "burger-menu-element-hide");
    }

    function toggleBurgerMenuElement2() {
        burgerCategoryTitleCategory.classList.remove('burger-category-title-rotate');
        burgerCategoryTitleFollow.classList.add('burger-category-title-rotate');
        burgerCategoryTitleAbout.classList.remove('burger-category-title-rotate');

        burgerMenuCategories.classList.replace('burger-menu-element-show', 'burger-menu-element-hide');
        burgerMenuElementFollowUs.classList.replace("burger-menu-element-hide", "burger-menu-element-show");
        burgerMenuElementAboutUs.classList.replace("burger-menu-element-show", "burger-menu-element-hide");
    }

    function toggleBurgerMenuElement3() {
        burgerCategoryTitleCategory.classList.remove('burger-category-title-rotate');
        burgerCategoryTitleFollow.classList.remove('burger-category-title-rotate');
        burgerCategoryTitleAbout.classList.add('burger-category-title-rotate');

        burgerMenuCategories.classList.replace('burger-menu-element-show', 'burger-menu-element-hide');
        burgerMenuElementFollowUs.classList.replace("burger-menu-element-show", "burger-menu-element-hide");
        burgerMenuElementAboutUs.classList.replace("burger-menu-element-hide", "burger-menu-element-show");
    }
</script>

