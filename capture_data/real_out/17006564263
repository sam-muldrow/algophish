                            <span onclick="categoryRedirect('hardware', 9)" class="categories-button footer-categories-hardware" ><span class="categories-puce categories-puce-hardware"></span><span>Hardware</span></span><script>
   function categoryRedirect(slug, id) {
    var url = "/category-page?category=" + slug + "&id=" + id;
    window.location.href = url;
}
</script>
<script defer>
    function filterCategories(url, event) {
        if (event.parentElement.id === "category-filter") {
            event.classList.toggle("active-filter");
            const category = document.getElementById('category-filter');
            const categoriesElementDom = Array.from(category.getElementsByClassName('active-filter'));
            const categoriesId = categoriesElementDom.map((element) => element.getAttribute('data-id'));
            const urlParams = new URLSearchParams(window.location.search);
            $.ajax({
                type: 'POST',
                url: url,
                dataType: 'json',
                data: {
                    action: 'filter_by_categories',
                    categories: categoriesId,
                    paged: urlParams.get('paged') || undefined
                },
                hearders: {
                    'Content-Type': 'application/x-www-form-urlencoded',
                    'Cache-Control': 'no-cache',
                },
                success: function(res) {
                    history.pushState(null, null, res.data.base);
                    $("#test-category").load(location.href + " #test-category", function(event) {
                        if (res.data.ids.length !== 0 && res.data.ids !== null) {
                            const user_is_logging = "";
                            if (user_is_logging === "1") {
                                const data = {
                                    action: 'get_comment_diff',
                                    post_ids: res.data.ids
                                }
                                $.ajax({
                                    url: url,
                                    type: 'POST',
                                    data: data,
                                    success: function (data) {
                                        if (data.length !== 0){
                                            data.forEach((element) => {
                                                $("#home-category .new-comment-" + element.post_id).append(`<span class="separator-point"></span><span>${element.new_comment}</span>`);
                                            })
                                        }
                                    },
                                    error: function (error) {
                                        console.log(error)
                                    }
                                })
                            }
                        }
                    });
                }
            })
        }
    }
</script>
