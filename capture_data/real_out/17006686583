<script> 

(function($) {
	//testimonials
	$('#dw-testimonials .image-item').hover(
       function(){ 
		$('#dw-testimonials .image-item').removeClass('active');
			$(this).addClass('active');
			var quote = $(this).data('quote');
			$('#dw-testimonials .testimonial-quote .quote-item').removeClass('active');
			$('#dw-testimonials .testimonial-quote .quote-item').each(function(){
			if($(this).data('quote')== quote){
				$(this).addClass('active');
			}
		});
	})
})(jQuery);
</script>

