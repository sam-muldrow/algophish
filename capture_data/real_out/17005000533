        function BeaconPrefill(formObject) {
            var formObject = formObject || {};

            if (window.BeaconDefaultCustomFields) {
                formObject.fields = window.BeaconDefaultCustomFields;
            }

            window.Beacon('prefill', formObject);
        }

        document.onreadystatechange = function () {
            if (document.readyState == "complete") {
                var logout_link = document.querySelectorAll('a[href*="/my-account/customer-logout/"]');
                if (logout_link) {
                    for (var i = 0; i < logout_link.length; i++) {
                        logout_link[i].addEventListener("click", function () {
                            window.Beacon('logout');
                        });
                    }
                }
            }
        };
    </script>
