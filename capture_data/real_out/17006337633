    <!-- subpixel -->
                <script type="text/javascript">
                    var _paq = _paq || [];
                    var matomoURL="https://subpixel.4players.de/";
                        // require user tracking consent before processing data
                        _paq.push(['requireConsent']);                                    
                    _paq.push(["setTrackerUrl", matomoURL+"abba/deff.php"]);
                    _paq.push(["setSiteId", 1]);

                    function startMatomo() {
                        _paq.push(["setDomains", ["*.www.4players.de"]]);
                        _paq.push(["enableLinkTracking"]);
                        _paq.push(["enableHeartBeatTimer"]);
                        _paq.push(["trackVisibleContentImpressions"]);
                        _paq.push(["setCustomDimension",
                            2,
                            "Allgemein",
                        ]);
                    _paq.push(["setCustomDimension",
                        5,
                        "GUEST"
                    ]);
                        _paq.push(["trackPageView"
                        ]);}
                    (function() {
                        var d=document, g=d.createElement("script"), s=d.getElementsByTagName("script")[0];
                        g.type="text/javascript"; g.async=true; g.defer=true; g.src=matomoURL+"5678/8765.js"; s.parentNode.insertBefore(g,s);
                    })();
                </script>
                <!-- /subpixel -->

    <!-- AWStats -->
        <script>
        startAWStats = function() {
            var awStatsScript = document.createElement("script");
            awStatsScript.src = "https://www.4players.de/javascript/awstats_misc_tracker.js";
            document.body.appendChild(awStatsScript);
        }
        </script><!-- /AWStats -->

    
