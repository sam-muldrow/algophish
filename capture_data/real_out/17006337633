</script>

    <script>

    var debug = true;
    var lstAllAdTagNames = 'banner,sky,rectangle,rectangle2,rectangle3,rectangle4,out-of-page',
        lstAllAdTagIDs = 'spk_banner,spk_sky,spk_rectangle,spk_rectangle2,spk_rectangle3,spk_rectangle4,spk_outofpage';
    var arrAllAdTagNames = lstAllAdTagNames.split(','),
        arrAllAdTagIDs = lstAllAdTagIDs.split(',');
    var arrSlots = [];
    
    arrSlots['spk_banner'] = {"name":"sb1","id":"spk_banner","sizes":[[728,90],[800,250],[970,250],[700,90],[1280,90]],"targetings":{"pos":"sb1"}};

    arrSlots['spk_sky'] = {"name":"sky1","id":"spk_sky","sizes":[[120,600],[160,600],[200,600],[300,600],[500,1000]],"targetings":{"pos":"sky1"}};

    arrSlots['spk_rectangle'] = {"name":"mr1","id":"spk_rectangle","sizes":[[300,250],[300,600]],"targetings":{"pos":"mr1"}};

    arrSlots['spk_rectangle2'] = {"name":"mr2","id":"spk_rectangle2","sizes":[[300,250],[300,600]],"targetings":{"pos":"mr2"}};

    arrSlots['spk_rectangle3'] = {"name":"mr3","id":"spk_rectangle3","sizes":[[300,250],[300,600]],"targetings":{"pos":"mr3"}};

    arrSlots['spk_rectangle4'] = {"name":"mrn","id":"spk_rectangle4","sizes":[[300,250],[300,600]],"targetings":{"pos":"mrn"}};

    arrSlots['spk_outofpage'] = {"name":"outofpage","id":"spk_outofpage","targetings":{"pos":"outofpage"}};

    // primary ads
    if (debug) {	
        console.log('------ display ads ------');	
        console.log('zone: homepage');
    }
 
    jQuery.each(arrAllAdTagNames, function(index, value) {

        var adTagName = value,
            adTagID = arrAllAdTagIDs[jQuery.inArray(adTagName, arrAllAdTagNames)],

        adContainerElement = jQuery('#' + adTagID);
        if (debug) { console.log('adContainerElement', adContainerElement); }
        if(adContainerElement === 'undefined' || adContainerElement.length === 0) {
            return;
        }
        
        var currentSlot = arrSlots[adTagID];
        var doDefine = true;
        var doShow = true;
        if(currentSlot.name == 'mr1') {
            if(typeof rectangleHPA === 'undefined') {
                if(debug)console.log('mr1 not defined, skipping');
                doDefine = false;
                doShow = false;
            } else if(!rectangleHPA) {
                currentSlot.sizes = [[300,250]];
            }
        }
        else if(currentSlot.name == 'mr2') {
            if(typeof rectangle2HPA === 'undefined') {
                if(debug)console.log('mr2 not defined, skipping');
                doDefine = false;
                doShow = false;
            } else if(!rectangle2HPA) {
                currentSlot.sizes = [[300,250]];
            }
        }
        else if(currentSlot.name == 'mr3') {
            if(typeof rectangle3HPA === 'undefined') {
                if(debug)console.log('mr3 not defined, skipping');
                doDefine = false;
                doShow = false;
            } else if(!rectangle3HPA) {
                currentSlot.sizes = [[300,250]];
            }            
        }
        else if(currentSlot.name == 'mrn' && currentSlot.id === 'spk_rectangle4') {
            doDefine = true;
            if(typeof rectangle4HPA === 'undefined') {
                if(debug)console.log('rectangle4HPA not defined, skipping');
                doDefine = false;
                doShow = false;
            } else if(!rectangle4HPA) {
                currentSlot.sizes = [[300,250]];
            }            
        }
        else if(currentSlot.name == 'mrn' && currentSlot.id === 'spk_rectangle5') {
            doDefine = true;
            if(typeof rectangle5HPA === 'undefined') {
                if(debug)console.log('rectangle5HPA not defined, skipping');
                doDefine = false;
                doShow = false;
            } else if(!rectangle5HPA) {
                currentSlot.sizes = [[300,250]];
            }            
        }
        else if(currentSlot.name == 'po1') {
            if(typeof interscrollerHPA === 'undefined') {
                if(debug)console.log('interscrollerHPA not defined, skipping');
                doDefine = false;
                doShow = false;            
            }
        }
        else if(currentSlot.name == 'mrn' && currentSlot.id === 'spk_posterad2') {
            doDefine = true;
            if(typeof interscroller2HPA === 'undefined') {
                if(debug)console.log('interscroller2HPA not defined, skipping');
                doDefine = false;
                doShow = false;
            }      
        }
        else {
            // other slot types are already defined
            doDefine = false;
        }
        
        if(doShow || doDefine) {
        
            Spark.cmd.push (function () {
                if(doDefine) {
                  if (debug) { console.log('defineSlot', currentSlot); }
                  Spark.definitionService.defineSlot (currentSlot);
                }
                
                if(doShow) {
                  if (debug) { console.log('displaySlot', adTagID); }
                  Spark.definitionService.displaySlot (adTagID);
                }
            });
            
        }
        
    });
    </script>
