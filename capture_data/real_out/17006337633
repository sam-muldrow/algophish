<script>
toggleBerichteboxListe = function(area)
{
    if ( area == 'arbeit' )
    {
        addClassName(document.querySelector('#index-berichtebox-berichte-liste-aktuell'),'hide');
        addClassName(document.querySelector('#index-berichtebox-berichte-liste-wertungstipp'),'hide');
        removeClassName(document.querySelector('#index-berichtebox-berichte-liste-arbeit'),'hide');
        addClassName(document.querySelector('#index-berichteboxnavi-rechts-arbeit'),'aktiv');
        removeClassName(document.querySelector('#index-berichteboxnavi-rechts-akt'),'aktiv');
        removeClassName(document.querySelector('#index-berichteboxnavi-rechts-wert'),'aktiv');
    }
    else if ( area == 'akt' )
    {
        addClassName(document.querySelector('#index-berichtebox-berichte-liste-arbeit'),'hide');
        addClassName(document.querySelector('#index-berichtebox-berichte-liste-wertungstipp'),'hide');
        removeClassName(document.querySelector('#index-berichtebox-berichte-liste-aktuell'),'hide');
        addClassName(document.querySelector('#index-berichteboxnavi-rechts-akt'),'aktiv');
        removeClassName(document.querySelector('#index-berichteboxnavi-rechts-arbeit'),'aktiv');
        removeClassName(document.querySelector('#index-berichteboxnavi-rechts-wert'),'aktiv');
    }
    else if ( area == 'wertungstipp' )
    {
        addClassName(document.querySelector('#index-berichtebox-berichte-liste-arbeit'),'hide');
        addClassName(document.querySelector('#index-berichtebox-berichte-liste-aktuell'),'hide');
        removeClassName(document.querySelector('#index-berichtebox-berichte-liste-wertungstipp'),'hide');
        addClassName(document.querySelector('#index-berichteboxnavi-rechts-wert'),'aktiv');
        removeClassName(document.querySelector('#index-berichteboxnavi-rechts-akt'),'aktiv');
        removeClassName(document.querySelector('#index-berichteboxnavi-rechts-arbeit'),'aktiv');
    }
    //ivw tagging here
    
<!-- START new IVW measurement manager -->

if(typeof IOMm !== "undefined")
{
    IOMm('configure', { st: '4playde', dn: 'data-b204b9f978.4players.de', mh: 0b101 });
}
        
var iomm_data = {
    "st":"4playde",
    "cp":"a_4p_4players_Start_aktuelles",
    "sv":"ke",
    "co":"4players/index/aktuelles",
    "sc":"yes"
}
console.log('iomm_data', iomm_data);

if(typeof IOMm !== "undefined")
{
    IOMm('pageview', { cp: iomm_data['cp'] });
}
<!-- END new IVW measurement manager -->

}

document.getElementById('index-berichteboxnavi-rechts-akt').addEventListener('mouseover', function() {
    toggleBerichteboxListe('akt');
}, true);
document.getElementById('index-berichteboxnavi-rechts-arbeit').addEventListener('mouseover', function() {
    toggleBerichteboxListe('arbeit');
}, true);
document.getElementById('index-berichteboxnavi-rechts-wert').addEventListener('mouseover', function() {
    toggleBerichteboxListe('wertungstipp');
}, true);

</script>

