<script>
    var searchClient = algoliasearch('00WA5BNCKP', 'a341077d0e1915ac3316b8e2c749f0d7');
    //var spielindex = searchClient.initIndex('Games');
    var spielindex = 'Games';
    var lastSearchText = '';
    var advancedSearchPlaceholderText = '---- Erweiterte Suche ----';
    if (typeof $ == 'undefined') {
        var $ = jQuery;
    }
    //console.log($);


    const iSAutocomplete = instantsearch.connectors.connectAutocomplete(
        ({ indices, refine, widgetParams }, isFirstRendering) => {
            const { container, onSelectChange } = widgetParams;

            if (isFirstRendering) {
                container.html('<select id="ais-autocomplete"></select>');

                container.find('select').selectize({
                    options: [],
                    valueField: 'id',
                    labelField: 'name',
                    highlight: false,
                    persist: true,
                    openOnFocus: true,
                    allowEmptyOption: true,
                    //onType: refine,
                    onBlur() {
                        console.log('onBlur', this.getValue());
                        refine(lastSearchText);
                    },
                    score() {
                        return function() {
                            return 1;
                        };
                    },
                    onChange(value) {
                        // newSearchText = jQuery('select#ais-autocomplete option:checked')[0].textContent;
                        // console.log('onChange', newSearchText);
                        //
                        // if(newSearchText && newSearchText !== advancedSearchPlaceholderText)
                        // {
                        //     lastSearchText = newSearchText;
                        //     console.log('now lastSearchText=', lastSearchText);
                        // }

                        onSelectChange(value);
                        refine(value);
                    },
                    // onDropdownClose(event) {
                    //
                    //
                    //     newSearchText = jQuery('select#ais-autocomplete option:checked')[0].textContent;
                    //
                    //     console.log('onDropdownClose', newSearchText);
                    //     if(newSearchText && newSearchText !== advancedSearchPlaceholderText)
                    //     {
                    //         lastSearchText = newSearchText;
                    //         console.log('now lastSearchText=', lastSearchText);
                    //     }
                    // },
                    onType(str){
                        //console.log('onType', str);

                        if(str && str !== advancedSearchPlaceholderText)
                        {
                            lastSearchText = str;
                            //console.log('now lastSearchText=', lastSearchText);
                        }

                        refine(str);
                    }
                });

                return;
            }

            const [select] = container.find('select');

            indices.forEach(index => {
                select.selectize.clearOptions();
                select.selectize.addOption({name: advancedSearchPlaceholderText, id: 0, css: "color:red;"});
                index.results.hits.forEach(h => select.selectize.addOption(h));
                select.selectize.refreshOptions(select.selectize.isOpen);
            });
        }
    );

    const suggestions = instantsearch({
        indexName: spielindex,
        searchClient,
    });

    suggestions.addWidget(
        instantsearch.widgets.configure({
            hitsPerPage: 5,
        })
    );

    suggestions.addWidget(
        iSAutocomplete({
            container: $('#autocomplete'),
            onSelectChange(value) {

                if(value === 0 || value === '0')
                {
                    //console.log('lastSearchText', lastSearchText);
                    window.location.href = 'https://www.4players.de/4players.php/suchergebnis/Allgemein/4players/?s='+encodeURI(lastSearchText);
                }
                else
                {
                    //console.log('value', value);

                    //search.helper.setQuery(value).search();
                    window.location.href = 'https://www.4players.de/4players.php/spielinfo/Allgemein/'+value+'/index.html';
                }
            },
        })
    );

    console.log('navigation->', 'https://www.4players.de/');

    //const search = instantsearch({
    //    indexName: spielindex,
    //    searchClient,
    //});
    //
    //search.addWidget(
    //    instantsearch.widgets.configure({
    //        hitsPerPage: 5,
    //    })
    //);
    //
    //search.addWidget(
    //    instantsearch.widgets.hits({
    //        container: '#hits',
    //        templates: {
    //            item: '<div class="hit-content"><a href="/4players.php/spielinfo/Allgemein/{{id}}/index.html" class="hit-link"><img class="hit-image" src="{{coverImageUrl}}"><header class="hit-name">{{#helpers.highlight}}{ "attribute": "name" }{{/helpers.highlight}}</header><p class="hit-description">{{#helpers.highlight}}{ "attribute": "description" }{{/helpers.highlight}}</p></a></div>',
    //        },
    //    })
    //);

    var suggestionsInitialized = false;

    searchField = document.getElementById('suchfeld');

    var searchFieldListener = function onclick(event) {
        searchField.removeEventListener("click", searchFieldListener);
        initSuggestions();
        //event.preventDefault();

        return true;
    }

    searchField.addEventListener("click", searchFieldListener);

    function initSuggestions()
    {
        console.log('initSuggestions');

        if(!suggestionsInitialized)
        {
            suggestionsInitialized = true;

            suggestions.start();

            // show menu after init
            if($('.selectize-dropdown .single')[0])
            {
                $('.selectize-dropdown .single')[0].style.display = "block";
            }

            if($('.selectize-control .single')[0])
            {
                $('.selectize-control .single')[0].style.display = "block";
            }

        }

        $('#ais-autocomplete-selectized')[0].focus();

        return false;
    }

    //search.start();

</script>
    
    </div>
