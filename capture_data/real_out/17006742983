{;if(page>=1&&page<=ff_lastpage&&page!=ff_currentpage){vis='visible';if(page==1||ff_currentpage==1){if(page==1)vis='visible';else vis='hidden';document.getElementById('ff_div239').style.visibility=vis;document.getElementById('ff_div242').style.visibility=vis;
