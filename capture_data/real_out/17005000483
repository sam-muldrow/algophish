<script>
    const experimentElements = Array.from(
        document.querySelectorAll('.article-teaser[data-experiment-id]')
    );
    const experiments = experimentElements.reduce((acc, el) => {
        if (!acc[el.dataset.experimentId]) {
            acc[el.dataset.experimentId] = [];
        }

        acc[el.dataset.experimentId].push(el);

        return acc;
    }, {});

    Object.values(experiments).forEach((teasers) => {
        const selectedIndex = Math.floor(Math.random() * teasers.length);
        teasers.forEach((teaser, i) => {
            if (i !== selectedIndex) {
                teasers[i].style.display = 'none';
            };
        });
    });
</script>

