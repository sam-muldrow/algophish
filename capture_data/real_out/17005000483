<script>
    const prefersDark = window.matchMedia('(prefers-color-scheme: dark)');

    function setLocalTheme(theme) {
        localStorage.setItem('e24-theme', theme);
    }

    function setThemeByPrefers() {
        const html = document.documentElement

        html.setAttribute('data-theme', prefersDark.matches ? 'dark' : 'light')
    }

    function getStoredTheme() {
        try {
            return localStorage.getItem('e24-theme');
        } catch (_) {
            return undefined;
        }

    }

    function setTheme(currentTheme) {
        if (currentTheme) {
            const html = document.documentElement;
            html.setAttribute('data-theme', currentTheme);
        } else {
            setThemeByPrefers();
        }
    }

    function darkMode(){
        const currentTheme = getStoredTheme();
        setTheme(currentTheme);
    }

    darkMode();

    if (prefersDark.addEventListener) {
        prefersDark.addEventListener('change', darkMode);
    } else {
        prefersDark.addListener(darkMode);
    }
</script>
