    width:16%;
}


     div.fat-footer .ff-links ul li {
    text-align: left;
}
.ff-accessibility {
    width: 100%;
    margin: 0 0 2rem 0;
  padding-top:0;
  float:right;
}
div.fat-footer .ff-links ul {
    padding: 0;
    margin:0;
}
     div.fat-footer {
    margin: 1.5rem auto;
    text-align: left;
}
  .ff-accessibility ul li {
    margin-top: 1rem;
    text-align: left;
}
div.fat-footer .ff-logo,
div.fat-footer .ff-address{
    margin-top:2px;
}


    }

