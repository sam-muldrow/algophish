<script type="text/javascript">
(function(jQuery){
jQuery.fn.extend({
        Scroll:function(opt,callback){
                //åæ°åå§å
                if(!opt) var opt={};
                var _btnUp = jQuery("#"+ opt.up);//Shawphy:åä¸æé®
                var _btnDown = jQuery("#"+ opt.down);//Shawphy:åä¸æé®
                var timerID;
                var _this=this.eq(0).find("ul:first");
                var     lineH=_this.find("li:first").height(), //è·åè¡é«
                        line=opt.line?parseInt(opt.line,10):parseInt(this.height()/lineH,10), //æ¯æ¬¡æ»å¨çè¡æ°ï¼é»è®¤ä¸ºä¸å±ï¼å³ç¶å®¹å¨é«åº¦
                        speed=opt.speed?parseInt(opt.speed,10):1200; //å·å¨éåº¦ï¼æ°å¼è¶å¤§ï¼éåº¦è¶æ¢ï¼æ¯«ç§ï¼
                        timer=opt.timer //?parseInt(opt.timer,10):7200; //æ»å¨çæ¶é´é´éï¼æ¯«ç§ï¼
                if(line==0) line=1;
                var upHeight=0-line*lineH;
                //æ»å¨å½æ°
                var scrollUp=function(){
                        _btnUp.unbind("click",scrollUp); //Shawphy:åæ¶åä¸æé®çå½æ°ç»å®
                        _this.animate({
                                marginTop:upHeight
                        },speed,function(){
                                for(i=1;i<=line;i++){
                                        _this.find("li:first").appendTo(_this);
                                }
                                _this.css({marginTop:0});
                                _btnUp.bind("click",scrollUp); //Shawphy:ç»å®åä¸æé®çç¹å»äºä»¶
                        });
                }
                //Shawphy:åä¸ç¿»é¡µå½æ°
                var scrollDown=function(){
                        _btnDown.unbind("click",scrollDown);
                        for(i=1;i<=line;i++){
                                _this.find("li:last").show().prependTo(_this);
                        }
                        _this.css({marginTop:upHeight});
                        _this.animate({
                                marginTop:0
                        },speed,function(){
                                _btnDown.bind("click",scrollDown);
                        });
                }
               //Shawphy:èªå¨æ­æ¾
                var autoPlay = function(){
                        if(timer)timerID = window.setInterval(scrollUp,timer);
                };
                var autoStop = function(){
                        if(timer)window.clearInterval(timerID);
                };
                 //é¼ æ äºä»¶ç»å®
                _this.hover(autoStop,autoPlay).mouseout();
                _btnUp.css("cursor","pointer").click( scrollUp ).hover(autoStop,autoPlay);//Shawphy:åä¸åä¸é¼ æ äºä»¶ç»å®

                _btnDown.css("cursor","pointer").click( scrollDown ).hover(autoStop,autoPlay);
        }       
})
})(jQuery);
jQuery(document).ready(function(){
        jQuery("#news_rolling").Scroll({line:1,speed:1200,timer:7200,up:"news_rolling_up",down:"news_rolling_down"});

		//å¯å®ä¹ä¸æ¬¡æ»å¨çè¡æ° æ»å¨éåº¦ æ¶é´å»¶è¿ è¿æåä¸æ»å¨æé® åä¸æ»å¨æé®
});

</script>
