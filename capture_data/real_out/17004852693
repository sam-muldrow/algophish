//ææ²³ååä¼
function SetMenuds11(theid){
	if(theid){
		for (i = 1; i< 3; i++) {
			document.getElementById("ds11_link_"+i).style.display="none";
			if(i==theid){
				document.getElementById("ds11_link_"+i).style.display="block";                    
			}
		}
	}
}

