       	function tab_2(x){
			for (var i = 1; i <= 3; i++) {
				if (i == x) {
					document.getElementById("wrap_img" + i).style.display = "block";
					document.getElementById("ts_round" + i).className = "ts_select";
				}
				else {
					document.getElementById("wrap_img" + i).style.display = "none";
					document.getElementById("ts_round" + i).className = "";
				}
			}
		};	
