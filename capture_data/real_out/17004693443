    <script>
        $(document).ready(function () {

            $('#navDomainName').bind("enterKey", function (e) {
            });
            $('#navDomainName').keyup(function (e) {
                if (e.keyCode == 13) {
                    submitNavForQueryDomain();
                }
            });


        });

        function submitNavForQueryDomain() {

            if ($("#navDomainName").val().trim() !== "") {

                var checked = [];
                $("input[name=chkNav]:checked").each(function (i, o) {
                    checked.push(o.value);
                });

                document.location = "/domain-sorgulama-sonuc.aspx?domainName=" + $("#navDomainName").val() + "&checkedTlds=" + checked;


            }

        }


        function updateCartCount() {

            $.ajax({
                type: "POST",
                url: "default.aspx/GetCartCount",
                contentType: 'application/json; charset=utf-8',
                dataType: "json",
                beforeSend: function () {

                },
                success: function (data) {
                    $("#spnCount").html(data.d);
                },
                error: function (request, status, error) {

                    alert("Cevap alınamadı,tekrar Deneyiniz.");
                }
            });

        }


        function addCartCount(count) {

            if (count == undefined || count == "" || parseInt(count) == NaN) {
                count = 1;
            }

            var cartCount = $("#spnCount").html();
            $("#spnCount").html(parseInt(cartCount) + parseInt(count));

        }


        function removeCartCount(count) {

            if (count == undefined || count == "" || parseInt(count) == NaN) {
                count = 1;
            }

            var cartCount = $("#spnCount").html();

            if (parseInt(cartCount) > 0) {
                $("#spnCount").html(parseInt(cartCount) - parseInt(count));
            }

        }

        function showLoading() {
            $(".loading").css("display", "block");
            $("[class*='button']").css('pointer-events', 'none');
        }

        function hideLoading() {
            $(".loading").css("display", "none");
            $("[class*='button']").css('pointer-events', 'all');
        }

        function showBlackLoading() {
            $("body").append("<div id='loading-black' class='loading black'><img src='/content/assets/img/loading-black.svg'/></div>");
            $(".black-layer").addClass("active");
        }

        function hideBlackLoading() {
            $("#loading-black").remove();
            $(".black-layer").removeClass("active");
        }

    </script>

    





    <script src="https://cdn.jsdelivr.net/npm/es6-promise@4/dist/es6-promise.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/es6-promise@4/dist/es6-promise.auto.min.js"></script>
    <script src="/content/assets/js/min/vue.min.js"></script>


    <script type="text/javascript">

        $(document).ready(function () {

            $('#domainName').bind("enterKey", function (e) {

            });
            $('#domainName').keyup(function (e) {
                if (e.keyCode == 13) {
                    if ($("#domainName").val().trim() !== "") {

                        document.getElementById("aspnetForm").action = "/domain-sorgulama-sonuc.aspx";
                        document.getElementById("aspnetForm").submit();
                    }
                }
            });

        });

        ES6Promise.polyfill();
        new Vue({
            el: '#domain-search',
            data: {
                items: [],
                type: 'Hizli-Sorgula',
                isLoading: false,
                isTransfer: false,
                checkedTlds: [],
                domainNamePlaceholder: 'yeni bir alan adı sorgulayın'
            },
            created: function created() {
                $("#tlds").show();
                $("#moreTlds").show();
                this.fillTab(this.type);
            },
            methods: {
                fillCheckDefault: function fillCheckDefault() {

                    if (this.type == 'Hizli-Sorgula') {
                        this.checkedTlds = [1, 2, 3, 460, 680, 397, 537]
                        this.domainNamePlaceholder = 'yeni bir alan adı sorgulayın';
                    }
                    else if (this.type == '.COM.TR') {
                        this.checkedTlds = [10, 131, 11]
                        this.domainNamePlaceholder = 'yeni bir tr alan adı sorgulayın';
                    }
                    else if (this.type == 'Turkce-Karakterli') {
                        this.checkedTlds = [121, 10, 131]
                        this.domainNamePlaceholder = 'yeni bir türkçe karakterli alan adı sorgulayın';
                    }
                    else if (this.type == 'Ad-Soyad') {
                        this.checkedTlds = [122, 123]
                        this.domainNamePlaceholder = 'yeni bir ad soyad alan adı sorgulayın';
                    }
                    else if (this.type == 'Kampanyali') {
                        this.checkedTlds = [121, 122, 460, 680]
                        this.domainNamePlaceholder = 'yeni bir kampanyalı alan adı sorgulayın';
                    }
                    else if (this.type == 'Ulke-Uzantilari') {
                        this.checkedTlds = [121, 195, 155, 4]
                        this.domainNamePlaceholder = 'yeni bir ülke uzantılı alan adı sorgulayın';
                    }
                    else if (this.type == 'Yeni-Domainler') {
                        this.checkedTlds = [460, 680, 420, 832]
                        this.domainNamePlaceholder = 'yeni bir alan adı sorgulayın';
                    }


                },
                fillTab: function fillTab(tname, o) {
                    var _this = this;

                    this.isLoading = true;

                    if (tname != '') {
                        this.type = tname;

                        if (tname == "Transfer") {
                            this.isTransfer = true;
                            this.domainNamePlaceholder = 'transfer için alan adı sorgulayın';
                        } else {
                            this.isTransfer = false;
                            this.fillCheckDefault();
                        }
                    }

                    if (o !== undefined) {
                        $("#tabs > .active").removeAttr("style");
                        $("#tabs > .active").removeClass("active")
                        $(o.target).attr("style", "pointer-events: none;");
                        $(o.target).addClass("active");
                    }

                    var obj = { 'tab': this.type };

                    $.ajax({
                        type: "POST",
                        url: "/default.aspx/FillTab",
                        contentType: 'application/json; charset=utf-8',
                        data: JSON.stringify(obj),
                        dataType: "json",
                        success: function success(res) {
                            _this.items = JSON.parse(res.d);
                            _this.isLoading = false;
                        },
                        error: function error(request, status) {
                            alert("Cevap alınamadı,tekrar Deneyiniz.");
                        }
                    });

                }
            }
        });



