<script>

    function addCartHostingProduct(productId, page) {

        if (page != 'main') {

            var os = $('#hostingType' + productId).val();
            if (productId == 198)
                os = $('#hostingType171').val();


            var osInt = os == "linux" ? 2 : 1;

            if (productId != 171)
                showLoading();

            $.ajax({
                cache: false,
                url: "/hosting/paketleri.aspx?type=addSepet&productId=" + productId + "&opr=" + osInt,
                success: function (data) {
                    if (data === "1") createScript(setTimeout(function () { location.href = "/ShoppingCart.aspx" }), 2000);
                },
                complete: function () { hideLoading(); }
            });

        } else {

            $.ajax({
                cache: false,
                url: "/hosting/paketleri.aspx?type=addSepet&productId=" + productId + "&opr=" + 2,
                success: function (data) {
                    if (data === "1") createScript(setTimeout(function () { location.href = "/ShoppingCart.aspx" }), 2000);
                },
                complete: function () { hideLoading(); }
            });

        }

        return false;
    }

</script>

