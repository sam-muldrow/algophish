tNodes(elements[i], 0);\n      if (childSections === null) {\n        // ignore section completly\n      } else if (childSections.length > 0) {\n        sections = sections.concat(childSections);\n      } else {\n        sections.push(elements[i]);\n      }\n    }\n    sections = getSectionsAfterCleanup(sections);\n    var ads = [];\n    for (var i = 0; i < sections.length; i++) {\n      var pos = i + 1;\n      var newNodeElement = null;\n      var ad = adPlacements[siteType][pos];\n      if (contentType === 'forumThread') {\n        if (siteType === 'mobile') {\n          if (pos >= 18 && (pos - 18) % 6 === 0) {\n            ad = 'mobile_2_placeholder';\n          } else if (pos === 12 || pos >= 21 && (pos - 21) % 3 === 0) {\n            ad = 'mobile_1_placeholder';\n          }\n        }\n      }\n\n      // check, if next section is available\n      var hasNextSection = sections.hasOwnProperty(i + 1);\n      if (debug) {\n        if (ad === 'video' && hasVideoInSite()) {\n          ad = false;\n        }\n        newNodeElement = getDebugElement(ad ? hasNextSection ? 'green' : 'pink' : 'red');\n        newNodeElement.innerHTML = (ad && !hasNextSection ? 'Blocked - last section<br />' : '') + (ad ? 'Ad #' + (i + 1) + '<br />' + ad : 'ABSATZ #' + (i + 1));\n      } else if (ad && (hasNextSection || contentType === 'forumThread')) {\n        if (ad === 'video') {\n          if (!hasVideoInSite()) {\n            newNodeElement = document.createElement('div');\n            newNodeElement.id = 'brightcoveDistribution';\n            newNodeElement.innerHTML = '<figure class=\"video article__lead-content video--lead-content\"><div style=\"--aspect-ratio: 1.7778;--video-player-margin: 0 calc(var(--space-3) * -1) var(--space-1);margin: var(--video-player-margin);position: relative;\"><div style=\"padding-top: 56.25%;background-color:#efefef\"></div></div></figure>';\n          }\n        } else if (ad === 'outstream') {\n          var wrapperInner = document.createElement('div');\n          wrapperInner.id = 'outstream_div';\n          wrapperInner.className = 'gujAd';\n          newNodeElement = document.createElement('div');\n          newNodeElement.className = 'ad-slot outstream_div_1';\n          newNodeElement.appendChild(wrapperInner);\n        } else if (ad === 'apester') {\n          var wrapperInner = document.createElement('interaction');\n          wrapperInner.setAttribute('data-token', '64aea1d6420f1e00122badaa');\n          wrapperInner.setAttribute('data-context', 'true');\n          wrapperInner.setAttribute('data-tags', '');\n          wrapperInner.setAttribute('data-fallback', 'true');\n          newNodeElement = document.createElement('div');\n          newNodeElement.className = 'apester-widget-embed__wrapper';\n          newNodeElement.appendChild(wrapperInner);\n          addApesterEventListener();\n        } else if (ad === 'carpetAd') {\n          newNodeElement = document.createElement('x-carpet-ad');\n        } else {\n          newNodeElement = getAdElement(ad);\n          if (contentType === 'search' || contentType === 'forumThread' && ad !== 'mobile_1_placeholder' && ad !== 'mobile_2_placeholder') {\n            ads.push(ad);\n          }\n        }\n      }\n      if (newNodeElement) {\n        if (contentType === 'search') {\n          var tr = document.createElement('tr');\n          tr.className = 'tr-ad';\n          var td = document.createElement('td');\n          td.setAttribute('colspan', '4');\n          td.appendChild(newNodeElement);\n          tr.appendChild(td);\n          sections[i].parentNode.insertBefore(tr, sections[i].nextSibling);\n        } else if (contentType === 'forumThreadList') {\n          var li = document.createElement('li');\n          li.className = 'li-ad';\n          li.appendChild(newNodeElement);\n          sections[i].parentNode.insertBefore(li, sections[i].nextSibling);\n        } else if (contentType === 'forumThread') {\n          if (ad !== undefined && ad && hasNextSection && document.querySelector('.js-div-ad[data-pos=\"' + pos + '\"]') === null) {\n            var div = document.createElement('div');\n            div.className = \"js-div-ad ad-slot \".concat(ad.replace('_placeholder', ''), \"_1\");\n            div.setAttribute('data-pos', pos);\n            div.setAttribute('data-ad', ad);\n            if (debug || !(ad === 'mobile_2' || ad === 'mobile_1_placeholder' || ad === 'mobile_2_placeholder')) {\n              div.appendChild(newNodeElement);\n            }\n            if (!debug && (ad === 'mobile_2' || ad === 'mobile_1_placeholder' || ad === 'mobile_2_placeholder')) {\n              threadAdObserver.observe(div);\n            }\n            sections[i].parentNode.insertBefore(div, sections[i].nextSibling);\n          }\n        }\n        // else if (contentType === 'article' && document.getElementsByClassName('grouped-tabledata').length > 0)\n        // {\n        //     var tr = document.createElement('tr');\n        //     tr.className = 'tr-ad';\n        //\n        //     var td = document.createElement('td');\n        //\n        //     td.setAttribute('colspan', '4');\n        //     td.appendChild(newNodeElement);\n        //     tr.appendChild(td);\n        //     console.log('doing tr stuff');\n        //     console.log('sections[i].nextSibling', sections[i].childNodes)\n        //     sections[i].parentNode.insertBefore(tr, sections[i].nextSibling);\n        // }\n        else {\n          sections[i].parentNode.insertBefore(newNodeElement, sections[i].nextSibling);\n        }\n      }\n    }\n    if (ads.length) {\n      MoltenBundle.cmd.push(function () {\n        if (MoltenBundle.adsAllowedChecked) {\n          showAds(ads);\n        } else {\n          MoltenBundle.registerAdsAllowedCheckedHandler(function () {\n            showAds(ads);\n          });\n        }\n      });\n    }\n  }\n\n  /**\n   * Show ads, if allowed\n   *\n   */\n  function showAds(ads) {\n    for (var i = 0; i < ads.leng