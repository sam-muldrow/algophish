<script>
  function maxInnerWidthLowerThan(value) {
    return window.innerWidth < value || (window.innerWidth > window.innerHeight && window.innerHeight < value);
  }
  var aspFile = 'desktop';

  if ((!BrowserDetection.deviceDetection.isMobile && window.innerWidth < 641) || (BrowserDetection.deviceDetection.isMobile && window.innerWidth < 1001)) {
    aspFile = 'mobile';
  }
  if (BrowserDetection.deviceDetection.isInAppIOS) {
    if (maxInnerWidthLowerThan(641)) {
      aspFile = 'phone_ios';
    } else {
      aspFile = 'tablet_ios';
    }
  }
  if (BrowserDetection.deviceDetection.isInAppAndroid) {
    if (maxInnerWidthLowerThan(641)) {
      aspFile = 'phone_android';
    } else {
      aspFile = 'tablet_android';
    }
  }
  if (BrowserDetection.deviceDetection.isSBKApp) {
    aspFile = 'sbk_' + aspFile;
  }
  if (BrowserDetection.deviceDetection.isUrbiaESKApp) {
    aspFile = 'eisprung_' + aspFile;
  }

  document.getElementsByTagName('html')[0].classList.add('asp_' + aspFile);
</script>
