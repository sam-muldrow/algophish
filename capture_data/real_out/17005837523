n  /**\n   * Get contentType based on pageKeywords\n   *\n   * @returns {String}\n   */\n  function getContentType() {\n    var contentType;\n    if (pageKWs.indexOf(',ct_video,') >= 0) {\n      contentType = 'video';\n\n      // remove impossible ads from article - the rest is equal\n      adPlacements.desktop[2] = 'outstream';\n      adPlacements.tablet[2] = 'outstream';\n      adPlacements.mobile[2] = 'outstream';\n    } else if (pageKWs.indexOf(',ct_search,') >= 0) {\n      // check has Google search\n      var googleElement = document.querySelectorAll('gcse\\\\:search');\n      if (googleElement.length === 0) {\n        // table tr/td logic\n        contentType = 'search';\n      } else {\n        // normal logic\n        contentType = 'article';\n      }\n    } else if (pageKWs.indexOf(',ct_bookmarks,') >= 0) {\n      // check has a result\n      var nameTable = document.querySelectorAll('.nametable');\n      if (nameTable.length > 0) {\n        // table tr/td logic\n        contentType = 'search';\n      } else {\n        // normal logic\n        contentType = 'article';\n      }\n    } else if (pageKWs.indexOf(',ct_gallery,') >= 0) {\n      contentType = 'gallery';\n      adPlacements = adPlacementsGalerie;\n    } else if (pageKWs.indexOf(',ct_article,') >= 0) {\n      contentType = 'article';\n    } else if (window.location.pathname.indexOf('/forum') === 0) {\n      var folderCount = window.location.pathname.split('/').length;\n      if (folderCount < 3 || window.location.pathname.indexOf('/forum/expertenforen') === 0) {\n        contentType = 'forumOverview';\n        adPlacements = adPlacementsForumOverview;\n      } else {\n        if (folderCount === 3) {\n          contentType = 'forumThreadList';\n          adPlacements = adPlacementsForumOverview;\n        } else {\n          contentType = 'forumThread';\n          adPlacements = adPlacementsForumThread;\n        }\n      }\n    }\n\n    // remove carpetAd, if search page -> problem with table\n    if (contentType == 'search') {\n      removeObjectByValue(adPlacements.desktop, 'carpetAd');\n      removeObjectByValue(adPlacements.tablet, 'carpetAd');\n      removeObjectByValue(adPlacements.mobile, 'carpetAd');\n    }\n    if (debug) {\n      if (contentType === null) {\n        console.error('No contentType identified');\n      } else {\n        console.log('contentType: ', contentType);\n      }\n    }\n    return contentType || '';\n  }\n\n  /**\n   * Check possible HTML nodes to add as section\n   *\n   * @param {HTMLElement}     element\n   * @param {Number}          level - node deep\n   *\n   */\n  function checkElementNodes(element, level) {\n    var childNodes = element.childNodes;\n    var elements = [];\n    var childNodesLength = childNodes.length;\n    for (var i = 0; i < childNodesLength; i++) {\n      var node = childNodes[i];\n\n      // cut nodes, if commentbox\n      // if (node.id === 'comment')\n      // {\n      //     childNodesLength = i;\n      // }\n      //FAMILY-4178 viebr2 not excluding\n\n      // remove first nodes, if element is summary\n      if (node.className === 'summary') {\n        elements = [];\n        continue;\n      }\n\n      // ignore embeded video player if is in second DOM level\n      if (level === 0 && node.nodeType === Node.ELEMENT_NODE && node.className.indexOf('paragraph video') !== -1) {\n        return null;\n      }\n      var hasGroupedTableData = document.getElementsByClassName('grouped-tabledata').length > 0;\n\n      // An element node like <p> or <div>\n      if (node.nodeType === Node.ELEMENT_NODE && node.nodeName !== 'A' && node.nodeName !== 'BR' && node.nodeName !== 'LI' && node.nodeName !== 'PICTURE' && node.nodeName !== 'FORM' && node.nodeName !== 'H1' && node.nodeName !== 'STRONG' && node.nodeName !== 'H2' && node.nodeName !== 'H3' && node.nodeName !== 'H4' && node.nodeName !== 'H5' && node.nodeName !== 'SCRIPT' && node.nodeName !== 'STYLE' && node.nodeName !== 'LINK' && node.nodeName !== 'IMG' && node.nodeName !== 'X-THIRDPARTY' && (hasGroupedTableData === true && node.nodeName !== 'THEAD' || hasGroupedTableData === false && node.nodeName !== 'TBODY') && node.className.indexOf('title__kicker') === -1 && node.className !== 'button' && node.className !== 'read-more-btn' && node.className !== 'recoBoxTitle' && node.className !== 'caption' && node.className !== 'source' && node.className.indexOf('comments') === -1 && node.className.indexOf('box-group') === -1 //FAMILY-4134\n      && node.className.indexOf('special-teaser') === -1 //FAMILY-4134 in Absprache mit evers6 hat viebr2 das hier eingesetzt, damit da keine Werbung in die Boxen kommt\n      && node.className.indexOf(' special-teaser') === -1 && node.className.indexOf('para-video') === -1 && node.className.indexOf('paragraph gallery') === -1 && node.className.indexOf('media ') === -1 && node.className.indexOf('head') === -1 && node.className.indexOf('quizArea') === -1 && node.className.indexOf('sub-nav') === -1 && node.className.indexOf('member-user-list') === -1 && node.id !== 'meta' && node.id !== 'toolbar' && node.id !== 'about'\n      //&& node.className.indexOf('video__player') > -1\n      ) {\n        elements.push(node);\n      }\n    }\n    if (!elements) {\n      return [];\n    }\n    var sections = [];\n    if (level < domDeep) {\n      for (var i = 0; i < elements.length; i++) {\n        var childSections = [];\n        if (elements[i].className.indexOf('a-image__position--left') !== -1) {\n          //FAMILY-4182 do not ad tags where images are inside of texts\n          continue;\n        }\n\n        // skip pager elements, forms, x-gallery\n        if (elements[i].className !== 'pager js-pager' /*&& elements[i].nodeName !== 'FORM'*/ && elements[i].nodeName !== 'X-GALLERY-SCROLLER') {\n          childSections = checkElementNodes(elements[i], level + 1);\n        }\n        if (childSections.length > 0) {\n          sections = sections.concat(childSections);\n        } else {\n          sections.push(elements[i]);\n        }\n      }\n    }\n    return sections;\n  }\n\n  /**\n   * Show ad placeholders before an ad will be displayed\n   *\n   */\n\n  function checkAdPlaceholders() {\n    var placeholders = document.querySelectorAll('.ad-slot[data-fallback]');\n    for (var i = 0; i < placeholders.length; i++) {\n      var placeholder = placeholders[i];\n      var fallback = placeholder.getAttribute('data-fallback');\n      var adIdElement = placeholder.id;\n      if (adPlaceholders.hasOwnProperty(fallback)) {\n        placeholder.appendChild(new DOMParser().parseFromString(getAdPlaceholder(adPlaceholders[fallback], adIdElement), 'text/html').body.firstChild);\n      } else if (debug) {\n        console.log('AdPlaceholder missed: ', fallback);\n      }\n    }\n  }\n  function getAdPlaceholder(data, adIdElement) {\n    return \"\\n        <article class=\\\"teaser teaser--list group-teaserblock__item js-fallback-\".concat(adIdElement, \"\\\">\\n                <figure class=\\\"teaser__figure\\\">\\n                    <div class=\\\"teaser__image-clip-path\\\" style=\\\"--aspect-ratio:1.5;background-color:#f0ecf3;\\\">\\n                        <picture class=\\\"image teaser__image lazy-picture\\\">\\n                            <img data-src=\\\"\").concat(data.img, \"\\\" src=\\\"data:image/gif;base64,R0lGODlhAQABAIAAAP///wAAACH5BAEAAAAALAAAAAABAAEAAAICRAEAOw==\\\"  alt=\\\"\\\" />\\n                        </picture>\\n                    </div>\\n                </figure>\\n                <div class=\\\"teaser__text-content\\\">\\n                  <a class=\\\"teaser__link teaser__link--overlay teaser--list-card__link\\\" href=\\\"\").concat(data.link, \"\\\" title=\\\"\").concat(data.headline, \"\\\" target=\\\"_self\\\">\\n                      <h3 class=\\\"teaser__headline-wrapper\\\">\\n                          <span class=\\\"teaser__kicker u-typo u-typo--kicker-tg4\\\">\").concat(data.kicker, \"</span>\\n                          <span clas