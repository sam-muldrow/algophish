        $('.catlink').click(function (e) {
        var $this = e.target,
            evtobj = window.event ? event : e,
            go_to_url = catlink[$this.getAttribute('data-cat-id')]
        ;

        if (evtobj.ctrlKey || evtobj.metaKey || evtobj.which == 2) {
            window.open(go_to_url);
