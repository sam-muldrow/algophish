  function OptanonWrapper() {
    o_global.eventLoader.onAllScriptsExecuted(11, function () {
      OneTrust.OnConsentChanged(function () {
        if (cmp) {
          cmp.archiveReceipt(getConsentId(), "OneTrust-CB-Web");
          cmp.setLegacyCBCookie().then(
            () => emitConsentChangeEvent(),
            () => emitConsentChangeEvent()
          );
        } else {
          o_util.cookie.remove("OptanonAlertBoxClosed");
          console.error("CMP not available");
        }
      });
    });

    function emitConsentChangeEvent() {
      // deprecated, replaced by eprivacy.cmp.consentChanged
      window.o_global.eventQBus.emit("ft4.cookie-banner.consentAccepted");
      window.o_global.eventQBus.emit("eprivacy.cmp.consentChanged");
    }
  }

  function getConsentId() {
    let optanonConsentCookie = o_util.cookie.get("OptanonConsent");
    if (!optanonConsentCookie) {
      console.warn("OptanonConsent cookie not found");
      return null;
    }
    let cookieValuesArray = optanonConsentCookie.split("&");
    let targetValueArray = cookieValuesArray.find((element) => element.startsWith("consentId="));
    if (!targetValueArray) {
      console.warn("'consentId' not found in OptanonConsent cookie");
      return null;
    }
    return decodeURIComponent(targetValueArray.split("=")[1]);
  }

  function showConsentBanner() {
    document.body.classList.remove(HIDE_OT_CONSENT_BANNER_CLASS);
  }
  function hideConsentBanner() {
    document.body.classList.add(HIDE_OT_CONSENT_BANNER_CLASS);
  }

  function showConsentBannerIfNotInAppOrWhenAppDecidesTo() {
    if (!document.cookie.includes("app=true")) {
      console.debug("No App cookie found - OneTrust-Consent-Banner is shown");
      showConsentBanner();
    } else {
      // make sure apps api is loaded
      o_global.eventLoader.onAllScriptsExecuted(11, () => {
        if (o_apps.shouldShowWebCookieBanner()) {
          showConsentBanner();
        }
      });
    }
