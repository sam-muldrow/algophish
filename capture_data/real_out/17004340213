/*<![CDATA[*/
var MiniShoppingCart = (function() {
  "use strict";
  var self = {};
  var cartTriggerUpdate;
  if (typeof(Event) === 'function') {
    cartTriggerUpdate = new Event("cartTriggerUpdate");
  } else {
    cartTriggerUpdate = document.createEvent('Event');
    cartTriggerUpdate.initEvent('cartTriggerUpdate', true, true);
  }
  self.update = function() {
    document.dispatchEvent(cartTriggerUpdate);
  };
  return self;
})();

/*]]>*/
