<script type="text/javascript" src="/newmedia/globalCssJs/script/64ca7faa16d4c7c2.js"></script><script type="text/javascript" >
/*<![CDATA[*/
/* global $, fdc */
/* eslint no-use-before-define: warn */

var fdc = fdc || {};
fdc.ctracking = fdc.ctracking || {};
fdc.ctracking.ga = fdc.ctracking.ga || {};
fdc.ctracking.ga.pagedata = fdc.ctracking.ga.pagedata || {};
fdc.ctracking.historyDataForProducts = {"dimension81":"none","dimension80":"none","dimension76":"none","dimension75":"none","dimension77":"none","dimension72":"none","price":"none","dimension74":"homepage_de","dimension73":"global","variant":"none","id":"none","dimension59":"none","pvaci":"eyJzdCI6IklOVEVSTkFMX1NVQlNIT1AiLCJzbiI6Ikdsb2JhbCIsImNuIjoiaG9tZXBhZ2UtZGUi\nLCJxcyI6Ik5PIiwid3MiOiJOTyIsIm9zIjoiTk8iLCJzZiI6IlVOS05PV04iLCJkbCI6IlVOS05P\nV04iLCJzaSI6NDAwMDQ3MTUyfQ==","quantity":"none","coupon":"none","dimension70":"false","dimension65":"none","dimension64":"none","dimension67":"none","dimension66":"none","dimension83":"none","dimension60":"none","dimension82":"none","dimension63":"none","dimension85":"none","dimension62":"none","dimension84":"none","name":"none","dimension69":"none","dimension68":"none","dimension48":"false","metric7":"none"};
fdc.ctracking.historyFieldInLocalStorage = 'HISTORY_FOR_PRODUCT';
fdc.ctracking.useLocalStorageHistoryForProducts = false;

// Empty function kept for compatibility, should anyone still call it. Exactag tracking is now done in the CollectOnTrack library.
fdc.ctracking.initExactagAndTrackPageView = function () {};

fdc.ctracking.fillLifecycleInfo = function (eventProductData, isQuickShopping) {
    if (eventProductData === undefined
        || eventProductData.products === undefined
        || eventProductData.products[0] === undefined) {
        return;
    }
    if (!fdc.ctracking.useLocalStorageHistoryForProducts) {
        return;
    }

    var webshopStyle = eventProductData.products.length > 1 ? 'true' : 'false';
    // fill lifecycle info for each product
    eventProductData.products.forEach(function(product) {
        product.dimension48 = isQuickShopping ? 'true' : 'false';
        product.dimension70 = webshopStyle;

        // try the source-product
        productHistoryDataAsString = fdc.hunter.getLocalStorageEntry(
            fdc.ctracking.historyFieldInLocalStorage,
            product.dimension68);
        if (!productHistoryDataAsString) {
            // try the webshop-product
                    var productHistoryDataAsString = fdc.hunter.getLocalStorageEntry(
                        fdc.ctracking.historyFieldInLocalStorage,
                        product.id);
            if (!productHistoryDataAsString) {
                // at this point there is no data in the local storage
                // the PDS has been opened without a labelled link
                var referrer = document.referrer;
                if (referrer === undefined || referrer === "") {
                    product.dimension73 = 'direkt';
                    product.dimension74 = 'direkt';
                    // fill pvaci
                    var pvaci = JSON.parse(atob(fdc.ctracking.historyDataForProducts.pvaci));
                    pvaci.sn = 'direkt';
                    pvaci.cn = 'direkt';
                    fdc.ctracking.handClientsideInfoToServer(btoa(JSON.stringify(pvaci)));
                } else {
                    var absoluteUrl = h_conf.getAbsoluteUrl();
                    var absoluteUrlTrimmed = absoluteUrl.substring(0, absoluteUrl.length - 1);
                    var referrerTrackingName = referrer.includes(absoluteUrlTrimmed) ? 'intern' : 'extern';
                    product.dimension73 = referrerTrackingName;
                    product.dimension74 = referrerTrackingName;
                }
                return;
            }
        }

        var productHistoryData = JSON.parse(productHistoryDataAsString);

        product.dimension73 = productHistoryData.dimension73;
        product.dimension74 = productHistoryData.dimension74;
        product.dimension75 = productHistoryData.dimension75;
        product.dimension76 = productHistoryData.dimension76;
        product.dimension77 = productHistoryData.dimension77;

        product.dimension80 = productHistoryData.dimension80; // shop feature

        if (eventProductData.actionField !== undefined) {
            eventProductData.actionField.list =
                [productHistoryData.dimension73,
                 productHistoryData.dimension74,
                 productHistoryData.dimension75,
                 productHistoryData.dimension76,
                 productHistoryData.dimension77
                ].join('/') ;
        }

        // fill the form field to get the PVACI back to the server
        fdc.ctracking.handClientsideInfoToServer(productHistoryData.pvaci);
    });
}

fdc.ctracking.handClientsideInfoToServer = function (dataForServer) {
    // hand info only known in the client to the server for the addProduct-event
    var hiddenFormField = $('[id^=pHistory]');
    hiddenFormField.val(dataForServer);
}

fdc.ctracking.saveProductHistory = function (event) {
    if (fdc.ctracking.useLocalStorageHistoryForProducts) {
        var linkNode = event.target.closest('[data-pds-link]');
        var productIds = linkNode.attributes['data-pds-link'].value.split(',');

        // find-nearest sourceTrackingData
        var sourceTrackingData= sourceFinderUtils.getSourceTrackingInfoFromLink($(linkNode));
        var featureAttribute = linkNode.attributes['data-qs-feature'];
        var feature = (featureAttribute && featureAttribute.value) ? featureAttribute.value : null;

        fdc.ctracking.saveProductHistoryWithGivenLinkData(productIds, sourceTrackingData, feature);
    }
}

fdc.ctracking.saveProductHistoryWithGivenLinkData = function (productIds, sourceTrackingData, feature) {
    if (fdc.ctracking.useLocalStorageHistoryForProducts) {
        var historyDataForProducts = fdc.ctracking.historyDataForProducts;

        if (feature) {
            sourceTrackingData.feature = feature;
        }

        // remember subshop from where the pds was opened
        var subshop = $(document.documentElement).attr('data-subshop');
        if (subshop === undefined) {
            subshop = 'intern';
        }
        historyDataForProducts.dimension73 = subshop.toLowerCase();

        fdc.ctracking.fillFeatureRelatedFields(historyDataForProducts, sourceTrackingData);

        productIds.forEach(function(productId) {
            // remember from where the PDS is opened
            if (productId !== null && fdc.hunter && fdc.hunter.createOrUpdateLocalStorageEntry) {
               fdc.hunter.createOrUpdateLocalStorageEntry(
                   fdc.ctracking.historyFieldInLocalStorage,
                   productId,
                   JSON.stringify(historyDataForProducts));
            } else {
               console.error('could not push history for product');
            }
        });
    }
}

fdc.ctracking.fillFeatureRelatedFields = function (historyDataForProducts, sourceTrackingData) {
        var feature = sourceTrackingData['feature'];
        if (feature) {
            // fill other fields
            var featureTrackingString = fdc.ctracking.shopFeatureMap[feature.toUpperCase()] || feature.toUpperCase();
            historyDataForProducts.dimension80 = featureTrackingString.toLowerCase().replace(/ /g, '_').replace(/-/g,'_');

            // fill pvaci
            var pvaci = JSON.parse(atob(historyDataForProducts.pvaci));
            // these key values are taken from initSourceOnClick.js
            // TODO(OÖ): make the values in initSourceOnClick.js accessible globally
            pvaci.sf = feature;
            historyDataForProducts.pvaci = btoa(JSON.stringify(pvaci));

            if(feature === 'SEARCH') {
                // node levels 1-5
                historyDataForProducts.dimension73 = 'search';
                historyDataForProducts.dimension74 = 'search';
                historyDataForProducts.dimension75 = 'none';
                historyDataForProducts.dimension76 = 'none';
                historyDataForProducts.dimension77 = 'none';
            }
            if(feature === 'SUGGEST_PRODUCT_RESULT') {
                // node levels 1-5
                historyDataForProducts.dimension73 = 'search';
                historyDataForProducts.dimension74 = 'suggested_search_products';
                historyDataForProducts.dimension75 = 'none';
                historyDataForProducts.dimension76 = 'none';
                historyDataForProducts.dimension77 = 'none';
            }
        }
}


// Track events, either directly with the page-load, or with additional data handed in on the client-side to
// overwriting properties of the page-view event.
fdc.ctracking.initGAAndTrackPageView = function (overridesFromBaristaIncludedPageContent) {

    var trackEvents = function () {
        fdc.ctracking.trackPageViewWithCoT();

        dataLayer = (typeof(dataLayer) === 'undefined' || dataLayer === null) ? [] : dataLayer;

        var eventsDealtWithByCoT = $('html').attr('data-events-pushed-to-live');

        // prepend the client-side events (e.g. view product event) to the events
        dataLayerEvents = [];
        if (fdc.ctracking.ga.events !== null && fdc.ctracking.ga.events !== undefined) {
            fdc.ctracking.ga.events.forEach(function(eventData) {
                console.log('EVENT: ' + eventData.event)
                if (eventData.event === 'productDetails'
                    && eventData.ecommerce !== undefined
                    && eventData.ecommerce.detail !== undefined) {
                    fdc.ctracking.fillLifecycleInfo(eventData.ecommerce.detail, false);
                }
                dataLayerEvents.unshift(eventData);
            })
        }
        // now push all events to the dataLayer
        dataLayerEvents.forEach(function (eventData) {
            if (eventData.event === 'checkout') {
                fdc.ctracking.trackCheckoutWithCoT(eventData);
                if ( eventsDealtWithByCoT.indexOf('checkout') === -1) {
                    fdc.hunter.pushToDataLayer(eventData);
                }
            } else if (eventData.event === 'purchase') {
                fdc.ctracking.trackPurchaseWithCoT(eventData);
                if ( eventsDealtWithByCoT.indexOf('purchase') === -1) {
                    fdc.hunter.pushToDataLayer(eventData);
                }
            } else if (eventData.event === 'addToCart') {
                fdc.ctracking.trackAddToCartWithCoT(eventData);
                if ( eventsDealtWithByCoT.indexOf('add_to_cart') === -1) {
                    fdc.hunter.pushToDataLayer(eventData);
                }
            } else if (eventData.event === 'productDetails') {
                fdc.ctracking.trackViewItemWithCoT(eventData);
                if ( eventsDealtWithByCoT.indexOf('item_view') === -1) {
                    fdc.hunter.pushToDataLayer(eventData);
                }
            } else {
                fdc.hunter.pushToDataLayer(eventData); 
            }
        });
    };

    fdc.hunter.withPersonalData(
        function (jsonPsdata) {
            if (jsonPsdata.ga_psdata) {

                var overrideWith = function(override) {
                    if (override) {
                        for (var key in override) {
                            if (override.hasOwnProperty(key)) {
                                fdc.ctracking.ga.pagedata[key] = override[key];
                            }
                        }
                    }
                };
                // add personal info to pagedata
                overrideWith(jsonPsdata.ga_psdata);
                overrideWith(overridesFromBaristaIncludedPageContent);

                if ($('html').attr('data-cot-included') === 'true' && window.CotClient === undefined) {
                    // we have to wait for the Cot library to be finished loading
                    document.addEventListener('cotLibraryLoaded', function() {
                        trackEvents();
                    });
                } else {
                    // without Cot library, we can immediately track to the dataLayer.
                    trackEvents();
                }

            }
        }
    );
}

fdc.ctracking.trackPageViewWithCoT = function() {
    var pagedata = fdc.ctracking.ga.pagedata
    if (window.CotClient != undefined) {
        window.CotClient.pushInitialPageData({
            ab_tests: pagedata.abTests,
            exactag_site_group: fdc.ctracking.exactag.sitegroup,
            internal_search_campaign: pagedata.internalSearchCampaign,
            internal_search_fallback: pagedata.internalSearchFallback,
            lead_id: fdc.ctracking.exactag.orderid,
            language: pagedata.language,
            exactag_level: fdc.ctracking.exactag.level,
            node_level_1: pagedata.nodeLevel1,
            node_level_2: pagedata.nodeLevel2,
            node_level_3: pagedata.nodeLevel3,
            node_level_4: pagedata.nodeLevel4,
            node_level_5: pagedata.nodeLevel5,
            number_of_available_product_results: pagedata.numberOfAvailableProductResults || pagedata.numberOfAvailableSearchResults,
            number_of_product_results: pagedata.numberOfProductResults || pagedata.numberOfSearchResults,
            page_name: pagedata.pageName,
            page_quality_score: pagedata.pageQualityScore,
            page_type: pagedata.pageType,
            platform: pagedata.platform,
        });
    }
}

fdc.ctracking.trackCheckoutWithCoT = function(eventData) {
    if (window.CotClient != undefined) {
        window.CotClient.pushEcommerceEventData({
            event: 'begin_checkout',
            avl_payment_types: eventData.dimension39,
            payment_type: eventData.dimension40,
            avl_shipping_methods: eventData.dimension41,
            shipping_method: eventData.dimension42,
            ecommerce: {
                checkout_step: eventData.ecommerce.checkout.actionField.step,
                checkout_option: eventData.ecommerce.checkout.actionField.option,
                currency: eventData.ecommerce.currencyCode,
                items: eventData.ecommerce.checkout.products.map((product) => {
                    return {
                        item_id: product.id,
                        item_name: product.name,
                        item_name_local: product.dimension64,
                        product_order_id: product.dimension67,
                        source_product_id: product.dimension68,
                        product_id: product.dimension69,
                        price: Number(product.price),
                        quantity: Number(product.quantity),
                        item_variant: product.variant,
                        item_brand: product.dimension72,
                        commodity_group: product.dimension81,
                        assortment_level_1: product.dimension82,
                        assortment_level_2: product.dimension83,
                        assortment_level_3: product.dimension84,
                        assortment_level_4: product.dimension85,
                        online_exclusive: product.dimension59,
                        product_sale: product.dimension62,
                        scout_price: product.dimension63,
                        list_name: "none",
                    }
                })
            }
        })
    }
}

fdc.ctracking.trackAddToCartWithCoT = function(eventData) {
    if (window.CotClient != undefined) {
        window.CotClient.pushEcommerceEventData({
            event: 'add_to_cart',
            ecommerce: {
                currency: eventData.ecommerce.currencyCode,
                items: eventData.ecommerce.add.products.map((product) => {
                    return {
                        item_id: product.id,
                        item_name: product.name,
                        item_name_local: product.dimension64,
                        product_order_id: product.dimension67,
                        source_product_id: product.dimension68,
                        product_id: product.dimension69,
                        price: Number(product.price),
                        quantity: Number(product.quantity),
                        item_variant: product.variant,
                        item_brand: product.dimension72,
                        commodity_group: product.dimension81,
                        assortment_level_1: product.dimension82,
                        assortment_level_2: product.dimension83,
                        assortment_level_3: product.dimension84,
                        assortment_level_4: product.dimension85,
                        quickbuy: "false",
                        webshop_style: product.dimension70,
                        online_exclusive: product.dimension59,
                        product_sale: product.dimension62,
                        scout_price: product.dimension63,
                        list_name: "none",
                    }
                })
            }
        })
    }
}

fdc.ctracking.trackViewItemWithCoT = function(eventData) {
    if (window.CotClient != undefined) {
        window.CotClient.pushEcommerceEventData({
            event: 'view_item',
            ecommerce: {
                currency: eventData.ecommerce.currencyCode,
                items: eventData.ecommerce.detail.products.map((product) => {
                    return {
                        item_id: product.id,
                        item_name: product.name,
                        item_name_local: product.dimension64,
                        product_order_id: product.dimension67,
                        source_product_id: product.dimension68,
                        product_id: product.dimension69,
                        price: Number(product.price),
                        quantity: Number(product.quantity),
                        item_variant: product.variant,
                        item_brand: product.dimension72,
                        commodity_group: product.dimension81,
                        assortment_level_1: product.dimension82,
                        assortment_level_2: product.dimension83,
                        assortment_level_3: product.dimension84,
                        assortment_level_4: product.dimension85,
                        quickbuy: "false",
                        webshop_style: product.dimension70,
                        online_exclusive: product.dimension59,
                        product_sale: product.dimension62,
                        scout_price: product.dimension63,
                        list_name: "none",
                    }
                })
            }
        })
    }
}

fdc.ctracking.trackPurchaseWithCoT = function(eventData) {
    if (window.CotClient != undefined) {
        window.CotClient.pushEcommerceEventData({
            event: 'purchase',
            avl_payment_types: eventData.dimension39,
            payment_type: eventData.dimension40,
            avl_shipping_methods: eventData.dimension41,
            shipping_method: eventData.dimension42,
            guest_order: eventData.dimension52,
            coupon_types: eventData.couponType,
            coupon_campaigns: eventData.couponCampaign,
            coupon_codes: eventData.couponCode,
            coupon_values: eventData.couponValue,
            ecommerce: {
                transaction_id: eventData.ecommerce.purchase.actionField.id,
                value: Number(eventData.ecommerce.purchase.actionField.revenue),
                tax: 0,
                shipping: Number(eventData.ecommerce.purchase.actionField.shipping),
                currency: eventData.ecommerce.currencyCode,
                coupon: eventData.ecommerce.purchase.actionField.coupon,
                affiliation: eventData.ecommerce.purchase.actionField.affiliation,
                items: eventData.ecommerce.purchase.products.map((product) => {
                    return {
                        item_id: product.id === undefined || product.id == null ? '' : product.id,
                        item_name: product.name,
                        item_name_local: product.dimension64,
                        product_order_id: product.dimension67,
                        source_product_id: product.dimension68 === undefined || product.dimension68 == null ? '' : product.dimension68,
                        product_id: product.dimension69,
                        price: Number(product.price),
                        quantity: Number(product.quantity),
                        item_variant: product.variant,
                        item_brand: product.dimension72,
                        commodity_group: product.dimension81,
                        assortment_level_1: product.dimension82,
                        assortment_level_2: product.dimension83,
                        assortment_level_3: product.dimension84,
                        assortment_level_4: product.dimension85,
                        online_exclusive: product.dimension59,
                        product_sale: product.dimension62,
                        scout_price: product.dimension63,
                        coupon: product.coupon,
                        coupon_code: product.dimension66,
                        coupon_value: isNaN(Number(product.metric1)) || product.metric1 === '' || product.metric1 === null ? 0 : Number(product.metric1),
                        item_coupon_value: isNaN(Number(product.metric7)) || product.metric7 === '' || product.metric7 === null ? 0 : Number(product.metric7),
                        list_name: "none",
                    }
                })
            }
        })
    }
}

// track events unless tracking should be delayed
if ($('html').attr('data-delay-t-init') !== 'true') {
    fdc.ctracking.initGAAndTrackPageView();
}

/*]]>*/
</script>
