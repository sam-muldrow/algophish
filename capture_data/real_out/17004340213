var h_conf = new function() {

    var baseUrl = 'https://www.tchibo.de/';

    var resourceBaseUrl = '/st/static/';

    var jSessionIdInUrl = '';

    var countryCode = 'DE';

    var isAffiliateDevice = 'false' === "true";

    var productSeoPopupDelay = '500';

    var isproductSeoPopupEnabled = 'false';

    var orderHistoryImportTimeout = '45000';

    var accountStatementImportTimeout = '45000';

    var mediaServerRegex = 'www\.tchibo\.de';

    var isSearchtrackingActive = 'true' === "true";

    var appPromotionNextReminderOffsetDays = '${appPromotionNextReminderOffsetDays}';

    var resourceMappings = JSON.parse('{"freiheit/img/gmaps/transit.svg":"/newmedia/st/img/4aa4447f0b0c7df2.svg","freiheit/img/gmaps/driving.svg":"/newmedia/st/img/4b1696aebb0c7df2.svg","freiheit/img/gmaps/my_location.svg":"/newmedia/st/img/433056a69abc7df2.svg","freiheit/img/gmaps/cancel.svg":"/newmedia/st/img/4aa695caab0c7df2.svg","freiheit/img/gmaps/walking.svg":"/newmedia/st/img/4aa699f87b0c7df2.svg","freiheit/img/match/affiliatefinder-marker-sprite.png":"/newmedia/st/img/40572849cb0c7df2.png","freiheit/img/gmaps/bicycling.svg":"/newmedia/st/img/46928719cb0c7df2.svg"}');

    var quickshoppingAsVueApp = 'true' === "true";

    var affiliateAvailabilityUseStoreFinderService = 'true' === "true";

    this.getCountryCode = function() { return countryCode; }

    this.isAffiliateDevice = function() { return isAffiliateDevice; }

    this.getMediaServerRegex = function(){ return mediaServerRegex;}

    this.isproductSeoPopupEnabled = function(){ return isproductSeoPopupEnabled;}

    this.getProductSeoPopupDelay =  function(){ return productSeoPopupDelay;}

    this.getOrderHistoryImportTimeout = function(){ return orderHistoryImportTimeout;}

    this.getAccountStatementImportTimeout = function(){ return accountStatementImportTimeout;}

    this.quickshoppingAsVueApp = function(){ return quickshoppingAsVueApp; }

    this.affiliateAvailabilityUseStoreFinderService = function(){ return affiliateAvailabilityUseStoreFinderService; }

    this.getAppPromotionNextReminderOffsetDays = function(){ return appPromotionNextReminderOffsetDays;}

    this.getAbsoluteUrl = function( urlPath ) {
        var res = [baseUrl, urlPath];
        if ( jSessionIdInUrl != null && jSessionIdInUrl.length > 0 ) {
            res.push( ";jsessionid=" + jSessionIdInUrl );
        }
        return res.join("");
    }

    this.getAbsoluteUrlWithoutSessionId = function( urlPath ) {
        return baseUrl + (typeof urlPath != "undefined" ? urlPath : "");
    }

    this.getDynamicResourceUrl = function( url ) {
        return resourceMappings[url];
    }

    this.getResourceUrl = function( urlPath ) {
        return resourceBaseUrl + urlPath;
    }

    this.isSearchTrackingActive = function () {
        return isSearchtrackingActive;
    }

    this.presentation = 'WEBSHOP';

    this.uuid = 'ce899d58abb37b82f598d1131c4adfcc';

}();

