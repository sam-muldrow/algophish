<script>

var isTabActive;
isTabActive = true;
window.onfocus = function () {
  isTabActive = true;
};

window.onblur = function () {
  isTabActive = false;
};

function GetCookie(c_name)
{
   var i,x,y,ARRcookies=document.cookie.split(";");
   for (i=0; i<ARRcookies.length; i++)
   {
      x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
      y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
      x=x.replace(/^\s+|\s+$/g,"");
      if (x==c_name)
      {
        return unescape(y);
      }
   }
}



var getRandomItem = function (list) {
  return Math.floor(Math.random()*list.length);
}

var Notification = function(data) {
  this.id = data.id;
  this.title = data.title;
  this.price = data.price;
  this.picture = data.thumbnail;
  this.url = data.url	;
  this.elem;
  this.create();
  this.show = function() {
    //var audio = new Audio('https://www.autobazar.eu/images/notify/light.mp3');
    //audio.play();
    $(this.elem).animate({
        opacity: 1,
        marginRight: 20
      }, 400);
    this.hide();
  }

  this.hide = function() {
    $(this.elem).delay(4600).queue(function(next){
      //$('.n-full').hide();
	   //$(this.elem).hide();
	   //$('.n-box-mini').show();
      next();
    })
  }
};

Notification.prototype.create = function() {
  var box = document.createElement('div');
    box.classList.add('n-box');
  var picturebox = document.createElement('div');
    picturebox.classList.add('n-picobal');
  var loader = document.createElement('div');
    loader.classList.add('n-loader');
  var pictureround = document.createElement('div');
    pictureround.classList.add('n-pictureround');
  var center =  document.createElement('center');

  var picture =  document.createElement('img');
    picture.classList.add('n-picture');
    picture.src = this.picture;
	picture.alt = 'picture';
  var body = document.createElement('div');
    body.classList.add('n-body');
  var link = document.createElement('div');
	 //<a target='_blank' class='n-full-back' onclick='ga(\"send\", \"event\", \"notifikacie\", \"zobrazit\",  \"homepage\"); $(this).parent().parent().hide(); setTimeout(function(){ $(\".n-box\").last().show();$(\".n-full\").last().show() }, 100); setTimeout(function(){ $(\".n-full\").hide()},4600); '></a>
    link.innerHTML = "<a target='_blank' onclick=' if($(\".n-box\").last().width()<80){$(\".n-box\").last().hide(); setTimeout(function(){ $(\".n-box\").last().show()},10); ga(\"send\", \"event\", \"notifikacie\", \"zobrazit\",  \"homepage\"); return false;}else{ SetCookie(\"addnotify\","+this.id+",30); ga(\"set\", \"dimension3\", \""+this.url+"\"); ga(\"send\", \"event\", \"notifikacie\", \"detail\",  \"homepage\");}' class='n-full' title='Zobraziť: "+this.title+"' rel='nofollow noopener' href='"+this.url+"'></a>";
  var message = document.createElement('span');
    message.classList.add('n-message');
    message.innerHTML = "<b class='n-teraz' >PRÁVE PRIDANÉ</b><br/>"
	+"<span class='title'>"+this.title+"<span class='price'> / "+this.price+"</span></span><br/> <a onclick=\"ga('send', 'event', 'notifikacie', 'rychlo-pozriet', 'homepage');\" rel='nofollow noopener' href='"+this.url+"'>Rýchlo pozrieť</a>"
	+"<div class='n-close' onclick='$(this).parent().parent().hide(); $(\".n-box-mini\").removeClass(\"alwaysphone\"); SetCookie(\"notifytype\",2,365); ga(\"send\", \"event\", \"notifikacie\", \"zatvorit\", \"homepage\")'></div>"
	+"<div class='n-dont'  onclick='$(this).parent().parent().hide(); $(\".n-box-mini\").removeClass(\"alwaysphone\"); SetCookie(\"notifytype\",2,365); ga(\"send\", \"event\", \"notifikacie\", \"zatvorit\", \"homepage\")'>Nezobrazovať</div>";//+this.message;
	box.appendChild(pictureround);
	box.appendChild(loader);
	box.appendChild(picturebox);
    picturebox.appendChild(center);
	center.appendChild(picture);
	box.appendChild(message);
	box.appendChild(link);
  var wrapper = document.getElementById('n-wrapper');

    wrapper.appendChild(box); this.elem = box;

	 $('.n-box-mini').html('<div class="n-loader"></div><div class="n-picobal" ><img class="n-picture" src="'+this.picture+'" alt="image of vehicle" /></div>');

	if(window.nfirst==1){
		if(GetCookie('notifycar')!=undefined){ $('.n-box').hide(); $('.n-box-mini').removeClass("alwaysphone");}
	 }
	else if(GetCookie('notifytype')==2){
		//nastavim loader
		$('.n-box').hide();
		$('.n-box-mini').removeClass("alwaysphone");
		$('.n-loader').addClass("run-spin").show();
		setTimeout(function(){ $('.n-loader').removeClass("run-spin");}, 1000);

	 }else{

	 }

}

function checkcars(){
	  //zobrazim loader na zaciatku

	  var cars = {};
	  //console.log(isTabActive);
	  if(isTabActive){
		  var request = $.getJSON('/storage/autobazar.eu/data/car-ticker/cars.json?i='+Math.floor(Math.random() * 10000)+'&j='+Math.floor(Math.random() * 10000));
		  $.when( request ).done(function() {
			cars = jQuery.parseJSON(request.responseText);
			var notifications = [];
			var pushNotif = function(){
			  var key=Math.floor(Math.random()*cars.length);
			  var car = cars[key];
			  var car = cars[0];


			   if(/*GetCookie('notifycar')==undefined || */GetCookie('notifycar')<car.id || window.nfirst==1)
				{
				$('.n-box').hide(); //$('.n-box').fadeOut();
				$('.n-box-mini').addClass("alwaysphone");
				var n = new Notification(car);
				n.show();
				notifications.push(n);
				SetCookie('notifycar',car.id,365);
				}

				window.nfirst=0;
			};
			setTimeout(function(){ pushNotif(); }, 100);
			});
		}
	    window.setTimeout(function(){ checkcars()}, 15000);
}


function prepare_ticker(){
	$(function(){
		window.onresize = function() {
		$('.n-box').hide();
		}

		window.nfirst=1;
	  if($( window ).width()>599) window.setTimeout(function(){    checkcars();   }, 3000);
		else window.setTimeout(function(){    checkcars();   }, 1000);
		if($( window ).width()>1015 && $( window ).width()<9440){
			newright=($( window ).width()-1015)/2+0;
			$('#n-wrapper').css('right',newright);
		}

	});



	$(window).scroll(function(){
		if($( window ).width()>1015){
			var distFromBottom = $(document).height() - $(window).scrollTop() - $(window).height();
			$('#n-wrapper').css('bottom','6px');
			if(distFromBottom<330){
				if ($("#m_top.compact").length){
					$('#n-wrapper').css('bottom',(120-distFromBottom));
				} else {
					$('#n-wrapper').css('bottom',(330-distFromBottom));
				}
			}
		}
	});
}

function SetCookie(cookieName,cookieValue,nDays) {
  var today = new Date();
  var expire = new Date();
  if (nDays==null || nDays==0) nDays=1;
  expire.setTime(today.getTime() + 3600000*24*nDays);
  document.cookie = cookieName+"="+escape(cookieValue)+";path=/"+";domain=.autobazar.eu"+";expires="+expire.toGMTString();
}

