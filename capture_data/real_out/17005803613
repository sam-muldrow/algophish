	<script>
function changeaction(){
	var sub1='', sub2='';
	if($("#BrandID :selected").length==1 && $("#BrandID").val()>0){
		var sub1=$("#BrandID  option:selected").text().replace('č','c').replace('Š','s').replace('ü','u').replace('ö','o').replace('ä','a').replace('@','a').replace(' ','-').replace(' ','-').replace(' ','-').replace(/[^a-z0-9-]/gi,'').toLowerCase();
		//alert($("#CarModelID").val().substr(-2, 2));
		if($("#CarModelID :selected").length==1){
			if(($("#CarModelID").val()[0].substr(-2, 2)!='|0' && Array.isArray($("#CarModelID").val())) || ( !Array.isArray($("#CarModelID").val()))&& $("#CarModelID").val().substr(-2, 2)!='|0')
				if($("#CarModelID").val().indexOf('a')==-1) //vypnutie pre vsetky moduly
			var sub2=$("#CarModelID  option:selected").text().replace('+','-plus').replace('Cee’d','Cee-d').replace('@','a').replace('Pro_Cee’d / ProCeed','proceed').replace(' (Combi)','').replace(' (Combi)','').replace(' (Sports Tourer)','').replace('č','c').replace('Š','s').replace('á','a').replace('é','e').replace('é','e').replace('é','e').replace('ľ','l').replace('í','i').replace('  ','').replace(' ','-').replace(' ','-').replace(' ','-').replace(/[^a-z0-9-]/gi,'').toLowerCase();
		}
	}
	var kam='www.autobazar.eu/hladaj/';
	if(sub1!='' && sub2!='' ) kam=sub1+'-'+sub2+'.autobazar.eu/';   //hladaj/
	else if(sub1!='') kam=sub1+'.autobazar.eu/';    //hladaj/
	kam=kam.replace('---','-');
	$('#searcher').attr('action', '//'+kam);
	window.sub1=sub1;
	window.sub2=sub2;
}

function load_values(vals){
  $('#BrandID').val(vals);
  $("#BrandID").multiselect("refresh");
  $("#BrandID").multiselect("widget").find("input:checkbox:not(:checked)").prop( "disabled", true );
  ajaxFunctionSerialize();
}

function loadfulltext(){
	if($("#qsearchformdata").html()=="")
	{
		sHladat = "Hľadať";
		$('#qsearchformdata').append('<script src="//www.autobazar.eu/upsearch/tagsinput/jquery.tagsinput.js?v=2023-05-11A"><\/script>');
		$('#qsearchformdata').append('<script src="//www.autobazar.eu/upsearch/tagsinput/jquery.tagsinit.js?v=2023-05-11A"><\/script>');
	}
}
function prepare_models(){
		chooseModel(document.searcher);
	


if($("#CarModelID").attr('multiple')=='multiple'){
	$("#CarModelID").multiselect("refresh");
	multibind(); advanced_search();
	}else{

	advanced_search();
	}
//advanced_search();


changeaction();
};

function page_ready(){

$(function(){
	if($("select[name=BrandID]").val()>0){
		$("select[name=BrandID]").trigger('change');
	}
})

	$(function(){
		
		$("#regionsk").multiselect({
			checkAllText: '',  /*Vybrať všetko*/
			uncheckAllText: 'Odznačiť všetko',
			noneSelectedText: 'Všetky kraje',
			selectedText: 'Vybrané: #',
			minWidth:188,
			click: function(event, ui){
				if($('#checkcountry1').prop('checked')==false) $('#checkcountry1').click();
			},
			selectedList: 1
		});

		$("#regioncz").multiselect({
			checkAllText: '',  /*Vybrať všetko*/
			uncheckAllText: 'Odznačiť všetko',
			noneSelectedText: 'Všetky kraje',
			selectedText: 'Vybrané: #',
			minWidth:188,
			click: function(event, ui){
				if($('#checkcountry2').prop('checked')==false) $('#checkcountry2').click();
			},
			selectedList: 1
		});

		$(" #regionpl, #regionuk").multiselect({
			checkAllText: '',  /*Vybrať všetko*/
			uncheckAllText: 'Odznačiť všetko',
			noneSelectedText: 'Všetky kraje',
			selectedText: 'Vybrané: #',
			minWidth:188,
			selectedList: 1
		});
	});

	$(function() {
		loadfulltext();
		SetCookie('resX',screen.width,365);
		SetCookie('resY',screen.height,365);
		SetCookie('resAR',window.devicePixelRatio,365);
	});
	$(window).resize(function () {
		loadfulltext();
	});

	$("#zip").autocomplete({
		source: function (request, response) {

			jQuery.get("https://www.autobazar.eu/ajax/zip.php", {
				query: request.term
			}, function (data) {
				// assuming data is a JavaScript array such as
				// ["one@abc.de", "onf@abc.de","ong@abc.de"]
				// and not a string
				 data = JSON.parse(data);
				response(data);
			});
		},
		appendTo: "#zipform",

		select: function(event, ui){
			if (ui.item && ui.item.value){
				titleinput = ui.item.value;
				ui.item.value= ui.item.value.substring(0, 5);
				 $('#countries').hide();  $('#kmvzdialenost').show();
			}
		},
		minLength: 1,

	});
 }
 function trackform(){
	colectdata();
	var kam=$("#searcher").attr('action');
	if(kam2) kam2 = kam2.replace("&BrandID%5B%5D=0","");
	if(kam2) kam=kam+'hladaj/'+'?'+kam2;
	kam=kam.replace('hladaj/hladaj/','hladaj/');
	kam=kam.replace(new RegExp('%5B%5D', 'g'), '[]');
	document.cookie = "fromsearch=1;path=/;domain=.autobazar.eu;";

	var catArr = {0 : 'osobne-vozidla',2 : 'suv-terenne-vozidla',1 : 'uzitkove-vozidla-do-3.5t',17 : 'nakladne-vozidla-3.5-7.5t',16 : 'nakladne-vozidla-nad-7.5t',10 : 'tahace-a-kamiony',18 : 'autobusy',3 : 'motocykle',4 : 'vodne-snezne-skutre-stvorkolky',5 : 'obytne-privesy-autokaravany-bufety',8 : 'veterany',6 : 'polnohospodarske-vozidla-stroje',7 : 'stavebne-stroje-nakladace-sklapace',21 : 'hydraulicke-ruky-autozeriavy',20 : 'vysokozdvizne-voziky',19 : 'specialy-nosice-kontajnerov',22 : 'navesy-privesy-nadstavby',9 : 'privesne-voziky',11 : 'lode-clny-lietadla-vznasadla',12 : 'nahradne-diely',13 : 'pneumatiky-disky',14 : 'prislusenstvo-oblecenie',15 : 'kupim',};
	catFound = kam.match(new RegExp('cat=[0-9]{1,2}', 'g'));
	if(catFound === null){
			catFound = '0';

	} else {
		catFound = catFound[0].split("=");
		catFound = catFound[1];
	}

	if (catFound !== null){
		if (kam.match(new RegExp('//www.', 'g')) === null) {
			var reg=/hladaj\/\?cat=[0-9]{1,2}/g;
							if (catFound == 1) {
										kam = kam.replace(reg,"uzitkove-vozidla/");
				} else {
					kam = kam.replace(reg,catArr[catFound]+"/");
				}
			        }
		if (kam.match(/cat=17/)) {
			var match = {};
			if (match = kam.match(/plustype=(\d?\d?)/)) {
				var aPlustypes = {};
				aPlustypes[0] = 'nevybrane';
aPlustypes[2] = 'skrinove-auta';
aPlustypes[3] = 'chladiarenske-auta';
aPlustypes[4] = 'izotermicke-auta';
aPlustypes[5] = 'mraziarenske-auta';
aPlustypes[6] = 'valnik';
aPlustypes[7] = 'valnik-s-plachtou';
aPlustypes[8] = 'odtahove-auta';
aPlustypes[9] = 'ostatne-auta';
				kam = kam.replace(/cat=17&*/, '');
				kam = kam.replace(/plustype=\d?\d?&*/, '');
				kam = kam.replace(/plustype=\d?\d?&*/, '');
				kam = kam.replace(/(autobazar...)\/hladaj/, '$1/nakladne-vozidla-3.5-7.5t/'+aPlustypes[match[1]*1]);
				if (kam.match(/\/\?$/)) {
					kam = kam.replace(/\?$/, '');
				}
			}
		}
        
        if ($("#catmain").val() == 13 && $('select[name="plustype"]').val()!='') {
            var kamdal='';
            var pneuArr = {'': 'hladaj', '0': 'pneumatiky', '5': 'puklice'};
            var pneuArrSub = {
                '0': 'hladaj',
                '1': 'celorocne',
                '2': 'letne',
                '3': 'zimne',
                '4': 'uzitkove-celorocne',
                '5': 'uzitkove-letne',
                '6': 'uzitkove-zimne',
                '7': 'off-road',
                '8': 'nakladne',
                '9': 'pre-motocykle',
                '10': 'stvorkolkove'
            };
            if ($('select[name="plustype"]').val() == 0 || $('select[name="plustype"]').val() == 5) {
                kam = (pneuArr[$('select[name="plustype"]').val()]);

                if (pneuArrSub[$('select[name="fuel"]').val()]) kam += '/' + (pneuArrSub[$('select[name="fuel"]').val()]);
                    if ($('select[name="pistonDisplacement"]').val() == 0 && $('select[name="powerKW"]').val()  == 0 && $('select[name="modelyear"]').val() > 0) {
                        kamdal = '/priemer-r' +  $('select[name="modelyear"]').val();

                    }
                    else if ($('select[name="pistonDisplacement"]').val() > 0 && $('select[name="powerKW"]').val() > 0 && $('select[name="modelyear"]').val() > 0) {
                        kamdal = '/' + $('select[name="pistonDisplacement"]').val() + '-' + $('select[name="powerKW"]').val() + '-r' + $('select[name="modelyear"]').val();

                    }

                    var hlp = $('form[name="searcher"]').find(':input').filter(function () {
                        return $.trim(this.value).length > 0 && (this.value != '') && (this.value != '0') && (this.value != '0|0' )
                    }).serialize();
                    if (hlp){
                        hlp = hlp.replace('page=1', '').replace('&fuel=' + $('select[name="fuel"]').val(), '').replace('cat=' + $('select[name="cat"]').val(), '').replace('&plustype=' + $('select[name="plustype"]').val(), '');
                                                if ($('select[name="pistonDisplacement"]').val() > 0 && $('select[name="powerKW"]').val() > 0 && $('select[name="modelyear"]').val() > 0)
                            hlp = hlp.replace('&powerKW=' + $('select[name="powerKW"]').val(), '').replace('&pistonDisplacement=' + $('select[name="pistonDisplacement"]').val(), '').replace('&modelyear=' + $('select[name="modelyear"]').val(), '')
                        if ($('select[name="pistonDisplacement"]').val() == 0 && $('select[name="powerKW"]').val() == 0 && $('select[name="modelyear"]').val() > 0)
                            hlp = hlp.replace('&modelyear=' + $('select[name="modelyear"]').val(), '');
                        if (hlp) kamdal += '/?' + hlp;
                    }


                if(kam || kamdal){
                    var newurl = '/' + kam + kamdal;

                    kam = newurl.replace('?&', '?'); //dam prec cat
                }
                //return false;
            }
			if ($('select[name="plustype"]').val() > 0 && $('select[name="plustype"]').val() < 5) {
				function inArray(needle, haystack) {
					var length = haystack.length;
					for(var i = 0; i < length; i++) {
						if (haystack[i] == needle) {
							return true;
						}
					}
					return false;
				}
				pneuArr = {'1': 'alu-disky', '2': 'alu-disky-pluspneu', '3': 'plechove-disky', '4': 'plechove-disky-pluspneu'};
				kam = (pneuArr[$('select[name="plustype"]').val()]);

				var vynechat = [];
				if (inArray($('select[name="plustype"]').val(), [1, 3])) {
					if ($('#DriveLine').val() > 0 && $('#TransmissionType').val() > 0 && $('#modelmodel').val().length > 0) {
						vynechat = ['DriveLine', 'TransmissionType', 'modelmodel'];
						kam += '/' + $('#modelmodel').val() + '-' + $('#TransmissionType').val() + '-' + $('#DriveLine').val();
					} else if ($('#TransmissionType').val() > 0 && $('#modelmodel').val().length > 0) {
						vynechat = ['TransmissionType', 'modelmodel'];
						kam += '/' + $('#modelmodel').val() + '-' + $('#TransmissionType').val();
					} else if ($('#TransmissionType').val() > 0) {
						vynechat = ['TransmissionType'];
						kam += '/priemer-' + $('#TransmissionType').val();
					} else if ($('#modelmodel').val().length > 0) {
						vynechat = ['modelmodel'];
						kam += '/roztec-' + $('#modelmodel').val();
					} else if ($('#modelmonth').val().length > 0) {
						vynechat = ['modelmonth'];
						kam += '/et-' + $('#modelmonth').val();
					}
				}
				vynechat.push('plustype');

				var hlp = $('form[name="searcher"]').find(':input').filter(function () {
					if (inArray(this.id, vynechat)) {
						return false;
					}
					return $.trim(this.value).length > 0 && (this.value != '') && (this.value != '0') && (this.value != '0|0' )
				}).serialize();

				if (hlp) {
					hlp = hlp.replace('page=1', '').replace('cat=' + $('select[name="cat"]').val(), '');
					if (hlp) {
						kamdal += '/?' + hlp;
					}
				}

				if (kam || kamdal) {
					var newurl = '/' + kam + kamdal;

                    kam = newurl.replace('?&', '?'); //dam prec cat
                }
			}
        }
        
        kam = kam.replace("/&","/?");

    }

     var term = $("input[name=type]").val();
     if (term != '') {
         var results = $("#searchgo").val().split(":")[1].trim().replace(" ", "") * 1;
         var resultsGaString = "over 30";
         if (results === 0) {
             resultsGaString = "0";
         } else if (results < 4) {
             resultsGaString = "1-3";
         } else if (results < 10) {
             resultsGaString = "4-9";
         } else if (results < 31) {
             resultsGaString = "10-30";
         }

         dataLayer.push({
             "event": "search",
             "gtm": {
                 "search_term": term,
                 "q_search_result_range": resultsGaString,
                 "q_search_redirect": 'true'
             }
         });
     }

    //alert(kam);
	window.location.href=kam;
	return false;
}
</script>
