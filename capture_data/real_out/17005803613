<script>

function multify(){
	$("#BrandID").multiselect({
		checkAllText: '',
		uncheckAllText: 'Resetovať',
		noneSelectedText: 'všetky značky',
		selectedText: 'Vybrané: #',
		open: function () { $("input[type='search']").focus(); },
		uncheckAll:function () { $('#znackacounter').html(''); clearModel(document.searcher); $("#CarModelID").multiselect("refresh"); $(this).multiselect("widget").find("input:checkbox:not(:checked)").prop( "disabled", false );},
		click: function(e){
		$('#znackacounter').html(' '+$(this).multiselect("widget").find("input:checked").length+'/5');
        if( $(this).multiselect("widget").find("input:checked").length >=5 ){
           //alert("You can only check two checkboxes!");
		   $(this).multiselect("widget").find("input:checkbox:not(:checked)").prop( "disabled", true );
		   if($(this).multiselect("widget").find("input:checked")>5)
           return false;
		   else
		   return true;
		   } else {
			   $(this).multiselect("widget").find("input:checkbox:not(:checked)").prop( "disabled", false );
		   }
		  //
	    },
		minWidth:230,
		selectedList: 1
	}).multiselectfilter({
      label: '',
      placeholder: 'hľadať značku',
      autoReset:true,
    }
	);
	$("#BrandID").multiselect("widget").find("a.ui-multiselect-close").append('&check; Potvrdiť');
	$("#BrandID").multiselect("widget").find(".ui-helper-reset:first").after('<div class="clear"></div><div class="selects"><span class="select_de" onclick="load_values([5,44,37,7,65,47])"></span><span class="select_it" onclick="load_values([30,1,14,29,95])"></span><span class="select_jp" onclick="load_values([57,32,103,101,61,20,42,36,40,58])"></span><span class="select_fr" onclick="load_values([10,237,45])"></span></div>');

	$("#CarModelID").multiselect({
		checkAllText: '',
		uncheckAllText: 'Resetovať',
		noneSelectedText: 'Všetky modely',
		selectedText: 'Vybrané: #',
		open: function () { $("input[type='search']").focus(); },
		click: function(e,ui){
			//$(e.target.nodeName).nextAll( ".odsad" ).click();
	    },
		minWidth:230,
		selectedList: 1
	}).multiselectfilter({
      label: '',
      placeholder: 'hľadať model',
      autoReset:true,
    }
	);
	$("#CarModelID").multiselect("widget").find("a.ui-multiselect-close").append('&check; Potvrdiť');

//nacitame vsetky modely pri prechode z jednoducheho searchu na multi
var temp=[];
temp.push($("#CarModelID").val());
$("#moremodels  input").each(function(){
        temp.push($(this).val());
});

$("#CarModelID").val(temp);
$("#CarModelID").multiselect("refresh");
$("#moremodels").html('');

}



