	<a class="btn btn-outline" id="toggleCats">Všetky kategórie</a>
</section>
<script>
	document.getElementById('toggleCats').onclick = function changeVisibility(e) {
		document.querySelectorAll('.items .item--hidden').forEach(function(item) {
			  item.classList.toggle('hidden')
		});
		$(".hp__categories__wrapper").toggleClass('toggled');
		var text = document.getElementById('toggleCats').textContent === 'Všetky kategórie' ? 'Menej kategórií' : 'Všetky kategórie';
		document.getElementById('toggleCats').textContent = text;
		if(text=='Všetky kategórie') { $('html,body').animate({scrollTop: ($('.hp__categories').offset().top-55)}); ga('send', 'event', 'homepage', 'menej-kategorii', '');} else{ga('send', 'event', 'homepage', 'vsetky-kategorie', '');}
	}
</script>

