					<script>
function setCookie(cname, cvalue, exhours) {
	var d = new Date();
	d.setTime(d.getTime() + (exhours*60*60*1000));
	var expires = "expires="+d.toUTCString();
	document.cookie = cname + "=" + cvalue + "; " + expires;
}

function getCookie(cname) {
	var name = cname + "=";
	var ca = document.cookie.split(';');
	for(var i=0; i<ca.length; i++) {
		var c = ca[i];
		while (c.charAt(0)==' ') c = c.substring(1);
		if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
	}
	return "";
}

</script>
					
		<script type="text/javascript">
					if(getCookie("poll")!="poll162562" ){
					
					document.getElementById("pollbtn162562").disabled = false;

					}

					function addPoll(poll_id) {
	
	var choice = 0;
				var optionlength = document.getElementsByName('poll-radioG1').length;
				for(var i=0; i<optionlength; i++){
					if(document.getElementsByName('poll-radioG1')[i].checked) {
						answer_id = document.getElementsByName('poll-radioG1')[i].value;
					}
				}
	
	var dataurl = 'poll_id='+poll_id+'&answer_id='+answer_id;
	var xhttp = new XMLHttpRequest();
	xhttp.onreadystatechange = function() {
		if (this.readyState == 4 && this.status == 200) {
			var messageType = this.responseText.split('####')[0];
			
			
			if(messageType == 'success') {
			
				setCookie("poll", "poll162562", 50000);
				document.getElementById("pollbtn162562").disabled = true;
			drawChart1(answer_id);
			 $('#message').html('<div class="alert alert-success">You have Successfully voted</div>');
				//$('#thumbs-ups-'+comment_id).html(thumbs_ups);
				//$('#thumbs-downs-'+comment_id).html(thumbs_downs);
			}
		}
	};
	xhttp.open("POST", static_url()+"staticfeeds/addPoll", true);
	xhttp.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
	xhttp.send(dataurl);
}
			
		</script>   
	
    <script type="text/javascript">

      // Load the Visualization API and the corechart package.
      google.charts.load('current', {'packages':['corechart']});

      // Set a callback to run when the Google Visualization API is loaded.
      google.charts.setOnLoadCallback(drawChart);

      // Callback that creates and populates a data table,
      // instantiates the pie chart, passes in the data and
      // draws it.

      
      function drawChart1(answerid) {
      var ansid=answerid;
		 var data = new google.visualization.DataTable();
        data.addColumn('string', 'Topping');
        data.addColumn('number', 'Slices');
	
        data.addRows([

				
          ['Yes', 126],
		
				
          ['No', 195],
		
				
          ['Do not Know', 17],
		
		         
         
        ]);


if(ansid==1102){
data.setCell(0, 1, 127);

}


if(ansid==1103){
data.setCell(1, 1, 196);

}


if(ansid==1104){
data.setCell(2, 1, 18);

}


        // Set chart options
        var options = {'title':'',
                       'width':300,
                       'height':300};

        // Instantiate and draw our chart, passing in some options.
        var chart = new google.visualization.PieChart(document.getElementById('chart_div'));
        chart.draw(data, options);
	
      }
      function drawChart() {

        // Create the data table.
        var data = new google.visualization.DataTable();
        data.addColumn('string', 'Topping');
        data.addColumn('number', 'Slices');
        data.addRows([
		          [' Yes', 126],
		          [' No', 195],
		          [' Do not Know', 17],
		         
         
        ]);

        // Set chart options
        var options = {'title':'',
                       'width':300,
                       'height':300};

        // Instantiate and draw our chart, passing in some options.
        var chart = new google.visualization.PieChart(document.getElementById('chart_div'));
        chart.draw(data, options);
      }
