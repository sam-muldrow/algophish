    <script type="text/javascript">
        const currentDate = new Date();
                                    
        // Contenido
        dataLayer.push({
            contentgroup1: 'elDiario.es',
            contentgroup2: '',
            contentgroup3: '',
            contentgroup4: '',
            contentgroup5: ''
        });

        // Ensure app is defined
        var timeout = 100000;
        function ensureAppIsSet(timeout) {
            var start = Date.now();
            return new Promise(waitForApp);

            function waitForApp(resolve, reject) {
                if (typeof app === 'object')
                    resolve(app);
                else if (timeout && (Date.now() - start) >= timeout)
                    reject(new Error("timeout"));
                else
                    setTimeout(waitForApp.bind(this, resolve, reject), 30);
            }
        }

        ensureAppIsSet(timeout).then(function(){
            // Usuario
            dataLayer.push({
                userID: app.user.id || '',
                userPerfil: app.user.profile || '',
                userSexo: app.user.sex || '',
                userProvincia: app.user.province || '',
                userAnoNacimiento: app.user.birthYear || '',
                userAltaUsuario: app.user.registerDate || '',
                userLogin: app.user.loginDate || '',
                userLastAccess: currentDate.getUTCFullYear() + "-" + parseInt(parseInt(currentDate.getUTCMonth()) + 1) + "-" + currentDate.getUTCDate(),
                userAgent: app.user.userAgent || '',
                userState: app.user.state || '',
                userStatus: app.user.status || '',
                userAutentification: app.user.userAuthentication || '',
                userFormaPago: app.user.userPaymentMethod || '',
                userNumeroRenovaciones: app.user.userRenovationCount || '',
                userModalidadSubscripcion: app.user.userSubscriptionMode || '',
                userCampaign: app.user.userCampaign || '',
                userSource: app.user.userSource || '',
                userMedium: app.user.userMedium || '',
                userCarrito: app.user.carrito || '',
                userTarjetaCaducada: app.user.tarjetaCaducada || '',
                userPeriodoGracia: app.user.periodoGracia || ''
            });

            // Viewport
            dataLayer.push({
                isMovil: app.user.isMobile
            });

            window.dataLayerUserLoaded = true;
        });

        // Page
        dataLayer.push({
            pageTemplate: 'home',
            pageFechaPublicacion: '2020-03-24',
            pageFechaModificacion: '2023-11-20',
            pageRedactorName: '',
            pageSection: 'elDiario.es',
            pageFirstTitle: "",
