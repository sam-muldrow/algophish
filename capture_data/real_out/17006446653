//<![CDATA[
function fnSuggestion() {
        if($('#suggestion_text').val()){
            myRet = confirm("ご意見を投稿してよろしいですか？");
            if ( myRet == true ){
                send_suggestion('',$('#suggestion_text').val());
                $('#suggestion_text').val('');
                $('#suggestion_main').hide()
                $('#suggestion_complete').show()
