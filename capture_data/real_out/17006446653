<script>
jQuery(document).ready(function($) {
    $("#category-items").menuAim({
        activate: function(a){
            var idx = $(a).index();
            $('.category-sub-panel').eq(idx).show();
        },
        deactivate: function(a){
            var idx = $(a).index();
            $('.category-sub-panel').eq(idx).hide();
        }
    });
    $('#form_search').submit(function(){
        if( $('#search_multi_text').val() == '' ){
            alert('キーワードを入力してください');
            return false;
        }else{
            return true;
        }
    })
});
</script>

