<script >

var e=document.createElement('div'); 
e.id='qvMym2bCOpKBX'; 
e.style.display='none'; 
document.body.appendChild(e);

function checkingjava()
{ 

  
	if(!adsbygoogle.loaded){

	 document.getElementById('40de41a9').style.visibility='visibile';
	}
}

function checkingjavaagain()
{ 
	
	if(!adsbygoogle.loaded){

	 document.getElementById('40de41a9').style.visibility='visible';

	}
	else if(adsbygoogle.loaded)
	{
		 document.getElementById('40de41a9').style.visibility='hidden';
	}
}

function checkingjavatest()
{ 
	if(document.getElementsByClassName('adsbygoogle')[0]==null)
	{

	 document.getElementById('40de41a9').style.visibility='visible';
	}
	else if ( document.getElementsByClassName('adsbygoogle')[0].innerHTML === "") 
	{
    document.getElementById('40de41a9').style.visibility='visible';
  }
  
}

if(!adsbygoogle.loaded)
{
	 
	 setTimeout(checkingjava, 250);
	 setTimeout(checkingjavaagain, 500);
	 
}

setTimeout(checkingjavatest, 1500);


</script>


