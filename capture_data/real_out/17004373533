      <script>
        var mobilePopupOnUserLeavingMediaQuery = window.matchMedia("(min-width: 961px)"),
            popupOnUserLeavingShown = false,
            $popupOnUserLeaving = $('#popup_on_user_leaving');

        function closePopup() {
            $popupOnUserLeaving.empty();
            $.magnificPopup.close();
        }

        function bindMouseLeave() {
            $(document).on("mouseleave", function (e) {
                if (!popupOnUserLeavingShown && e.pageY - $(window).scrollTop() <= 1 && !$.magnificPopup.instance.isOpen) {
                    $.magnificPopup.open({
                        tClose: 'Chiudi (ESC)',
                        items: {
                            src: '#popup_on_user_leaving',
                            type: 'inline'
                        },
                        callbacks: {
                            open: function () {
                                popupOnUserLeavingShown = true;
                                $.get('/' + 'popu' + 'p_on_' + 'user_' + 'leavi' + 'ng_sh' + 'own?t' + 'empla' + 'te=ho' + 'mepag' + 'e');
                            },
                            close: function () {
                                closePopup();
                            }
                        },
                        alignTop: true,
                        mainClass: 'mfp-fade'
                    });
                }
            });
        }

        function loadPopupContent() {
            if ($popupOnUserLeaving.html().length == 0) {
                $.ajax({
                    url: '/' + 'codi' + 'ci-sc' + 'onto-' + 'coupo' + 'n/pop' + 'up_on' + '_user' + '_leav' + 'ing?t' + 'empla' + 'te=ho' + 'mepag' + 'e'
                }).done(function () {
                    $('body').on('click', '.coupon_popup_cta', function () {
                        ga('send', {
                            hitType: 'event',
                            eventCategory: 'Coupon Popup ClickOut',
                            eventAction: 'homepage',
                            eventLabel: $('.claim').data('vendor')
                        });

                        var closing_delay = ($(this).attr('id') == 'copy-code') ? 1000 : 0;
                        setTimeout(function () {
                            closePopup();
                        }, closing_delay);
                    });
                }).fail(function () {
                    unbindMouseLeaveIfNoCoupons();
                });
            }
        }

        function unbindMouseLeaveIfNoCoupons() {
            $popupOnUserLeaving.empty();
            $(document).off("mouseleave");
        }

        function loadPopupOnUserLeaving(mobileMediaQuery) {
            if (mobileMediaQuery.matches) {
                bindMouseLeave();
                loadPopupContent();
            }
        }

        loadPopupOnUserLeaving(mobilePopupOnUserLeavingMediaQuery);
        if (mobilePopupOnUserLeavingMediaQuery.addEventListener === undefined) {
            mobilePopupOnUserLeavingMediaQuery.addListener(loadPopupOnUserLeaving);
        } else {
            mobilePopupOnUserLeavingMediaQuery.addEventListener('change', loadPopupOnUserLeaving, false);
        }

    </script>
  <script>

      (function ($) {
          $(async function () {

              try {
                  var $chronology = $('[data-component="chronology-unlogged"]');
                  var history = await chronologyGetNItems(50);

                  if (history.length === 0) {
                      return false;
                  }

                  $chronology.removeClass("invisible");

                  $.ajax({
                      method: "POST",
                      contentType: "application/json",
                      url: "/chronology_render",
                      data: JSON.stringify({history: history})
                  }).done(function (data) {
                      $chronology.find('.chronology-carousel > .carousel').html(data);
                      var carousel_slider = window.carousels.filter(function (item) {
                          return item[0].match(/.chronology-carousel/)
                      });
                      tp_style.carousel(carousel_slider);
                      chronologyComponent.unlogged.bindImagesLoadEvents($chronology);
                  });
              } catch {
                  $chronology.addClass("invisible");
              }
          });
      })(jQuery);
  </script>
  <script>
      (function ($) {
          $(function () {
              $.widget("custom.search_selectmenu", $.ui.selectmenu, {
                  _renderMenu: function (ul, items) {
                      var that = this;
                      $.each(items, function (index, item) {
                          that._renderItemData(ul, item);
                      });
                      $(ul).addClass("search_category_filter_menu");
                  },
                  _renderItem: function (ul, item) {
                      var li = $("<li>"),
                        wrapper = $("<div>", {
                              title: item.element.attr("title")
                          });

                      if (item.disabled) {
                          this._addClass(li, null, "ui-state-disabled");
                      }
                      if (item.value === '-1') {
                          $("<strong>", {html: item.label}).appendTo(wrapper);
                      } else {
                          this._setText(wrapper, item.label);
                      }

                      return li.append(wrapper).appendTo(ul);
                  }
              });

              $('.search_category_filter').search_selectmenu();
          });
      })(jQuery);
  </script>
