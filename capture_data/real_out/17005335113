<div class="targetprehiding">
<!-- Pre-hide the homepage hero until Adobe Target finishes loading the A/B tests -->
<div id="aem-target-prehiding">
    <style id="alloy-prehiding">
        body {
            opacity: 0 !important;
        }
    </style>
    <script type="text/javascript" id="adobe-target-prehiding-snippet">
        // Show the page when Target library loads OR after a set duration, whichever comes first.
        let duration = 5000;
        setTimeout(function() {
            document.querySelector("style#alloy-prehiding")?.remove();
        }, duration);

        const isVisible = function(el) {
            return !!(el.offsetWidth || el.offsetHeight || (el.getClientRects() && el.getClientRects().length));
        }
        function showHeroVideos() {
            var videos = document.querySelectorAll(".aem-hero video");
            if (videos) {
                Array.from(videos).map(video => {
                    if (isVisible(video)) {
                        Array.from(video.children).map(child => {
                            if (child.tagName === "SOURCE" && !child.src) {
                                child.src = child.dataset.src;
                                video.load();
                            }
                        });
                    }
                })
            }
        }
        // Watch the alloy-prehiding style-tag to trigger video loading event
        let targetNode = document.getElementById("aem-target-prehiding");
        // Options for the observer (which mutations to observe) 
        const config = {
            attributes: false,
            childList: true,
            subtree: false
        };
        // Callback function to execute when mutations are observed
        const callback = (mutationList, observer) => {
            for (let mutation of mutationList) {
                if (mutation.removedNodes.length && mutation.removedNodes[0].id === "alloy-prehiding") {
                    observer.disconnect();
                    showHeroVideos();
                }
            }
        };
        // Create an observer instance linked to the callback function 
        let observer = new MutationObserver(callback);
        // Start observing the target node for configured mutations
        observer.observe(targetNode, config);
    </script>
