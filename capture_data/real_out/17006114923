  fcmsJs.currentUser.onLoad(function(data) {

    var userStatus = "0";

    if (data.user && data.user.permissions) {

      if (data.user.permissions.indexOf('apur') !== -1) {
        userStatus = "3";
      } else {
        userStatus = "2";
      }

      window.marfeel.cmd.push(['compass', function(compass) {
        compass.setUserType(userStatus);
      }]);
    }
  });
</script>

