<script type="text/javascript" src="https://cds.loteriasyapuestas.es/cms/js/jquery.bxslider.min.4.2.15.js"></script>
<script type="text/javascript">
	var randomStart = false;
	var speed = 1200;
	var timeout = 4000;
	var pause = true;
	var isNumberEmpty = false;
	
		isNumberEmpty = true;
	</script>
<script type="text/javascript">var jqBannerSlideShow = jQuery.noConflict();

function BannerSlideShow(isNumberEmpty) {
	this.slideShowParams = {
		auto: true,
		randomStart: randomStart,
		infiniteLoop: true,
		adaptiveHeight: true,
		speed: speed,
		autoHover: pause,
		pager: true,
		pagerSelector: "#pages",		
		prevSelector: "#prev",
		nextSelector: "#next",

		onSliderLoad: function(){
			console.log("Header > bxSlider onSliderLoad()");				
			jqBannerSlideShow("div.bx-viewport").height("auto");
			jqBannerSlideShow("div.bx-controls").remove();
			jqBannerSlideShow("a#aPrev").click(function(e){
				e.preventDefault();
				jqBannerSlideShow("#prev a").click();
			});
			  
			jqBannerSlideShow("a#aNext").click(function(e){
				e.preventDefault();
				jqBannerSlideShow("#next a").click();
			});
		}
	};

	if (isNumberEmpty) {
		this.slideShowParams.pause = timeout;
	}
}

BannerSlideShow.prototype.main = function(){
	this.removeBannersOutOfDate();
	this.setUpSrcImg("img.img-mobile-tablet");
	this.setUpHref("a.link-mobile-pc");
	this.setUpHtml("a.enlace");
	jqBannerSlideShow("ul.banner").bxSlider(this.slideShowParams);
};

BannerSlideShow.prototype.createDateFromString = function(str){
	var result = null;
	try {
		if (str) {
			if (new Date(str).getTime()) {
				result = new Date(str);
			} else {
				var dateSrt = str.split(" ")[0] + "T" + str.split(" ")[1];
				result = new Date(dateSrt);
			}
		}
	} catch (err) {
		console.error("ERROR en createDateFromString: " + err);
		result = null;
	}
	return result;
};

BannerSlideShow.prototype.removeBannersOutOfDate = function(){
	var cont = 0;
	try{
		var actualDate = new Date();
		actualDate.setSeconds(0);
		actualDate.setMilliseconds(0);
		
		var that = this;
	
		jqBannerSlideShow("li.item-banner").each(function(i, e) {
			var activation;
			var deactivation;
			var activationStr = jqBannerSlideShow(e).attr("data-activation");
			var deactivationStr = jqBannerSlideShow(e).attr("data-deactivation");
			var removed = false;
	
			if (activationStr && !deactivationStr) {
				activation = that.createDateFromString(activationStr);
				if (activation > actualDate) {
					jqBannerSlideShow(e).remove();
					console.debug("Element: " + jqBannerSlideShow(e).attr("id") + " - REMOVE");
					removed = true;
				}
			}
			if (!activationStr && deactivationStr && !removed) {
				deactivation = that.createDateFromString(deactivationStr);
				if (deactivation < actualDate) {
					jqBannerSlideShow(e).remove();
					console.debug("Element: " + jqBannerSlideShow(e).attr("id") + " - REMOVE");
					removed = true;
				}
			}
			if (activationStr && deactivationStr && !removed) {
				activation = that.createDateFromString(activationStr);
				deactivation = that.createDateFromString(deactivationStr);
				if (!(activation <= actualDate && deactivation >= actualDate)) {
					jqBannerSlideShow(e).remove();
					console.debug("Element: " + jqBannerSlideShow(e).attr("id") + " - REMOVE");
					removed = true;
				}
			}
			if (!removed) {
				console.debug("Element: " + jqBannerSlideShow(e).attr("id") + " - OK");
				cont++;
			}
		});
	} catch(err){
		console.error("Error en removeBannersOutOfDate(): " + err);
	}
	return cont;
};

BannerSlideShow.prototype.setUpHref = function(selector){
	if (!isMobileByWidth()) {
		jqBannerSlideShow(selector).each(function(i, e) {
			if (jqBannerSlideShow(e).attr("data-href-pc")) {
				jqBannerSlideShow(e).attr("href", jqBannerSlideShow(e).attr("data-href-pc"));
			}
		});
	}
};

BannerSlideShow.prototype.setUpHtml = function(selector){
	if (!isMobileByWidth()) {
		jqBannerSlideShow(selector).each(function(i, e) {
			if (jqBannerSlideShow(e).attr("data-html-pc")) {
				jqBannerSlideShow(e).html(jqBannerSlideShow(e).attr("data-html-pc"));
			}
		});
	}
};

BannerSlideShow.prototype.setUpSrcImg = function(selector){
	jqBannerSlideShow(selector).each(function(i, e) {
		var srcPC = jqBannerSlideShow(e).attr("data-src-pc");
		var srcTablet = jqBannerSlideShow(e).attr("data-src-tablet");
		var srcMobile = jqBannerSlideShow(e).attr("data-src-mobile");

		if (!srcPC) {
			console.log("- No existe imagen de PC");
			return false;
		} else {
			if (isPCForImg()) { // PC
				jqBannerSlideShow(e).attr("src", srcPC);
				console.log("--Elto: " + i + " --> actualizamos a srcPC");
			} else {
				if (isTabletForImg()) { // Tablet
					if (srcTablet) {
						jqBannerSlideShow(e).attr("src", srcTablet);
						console.log("--Elto: " + i + " --> actualizamos a srcTablet");
					} else {
						jqBannerSlideShow(e).attr("src", srcPC);
						console.log("--Elto: " + i + " --> actualizamos a srcPC");
					}
				} else { // Mobile
					if (!srcMobile) {
						if (srcTablet) {
							jqBannerSlideShow(e).attr("src", srcTablet);
							console.log("--Elto: " + i + " --> actualizamos a srcTablet");
						} else {
							jqBannerSlideShow(e).attr("src", srcPC);
							console.log("--Elto: " + i + " --> actualizamos a srcPC");
						}
					} else {
						jqBannerSlideShow(e).attr("src", srcMobile);
						console.log("--Elto: " + i + " --> actualizamos a srcMolvil");
					}
				}
			}

		}
	});
};


var bss = new BannerSlideShow(isNumberEmpty);
bss.main();
</script>
<!-- /carrusel --></div>
<div class="portal-seccion-promociones-home-nd" >

			    <div class="r-elementos-enlazables">
		<div class="c-elementos-enlazables__2-elementos c-2-elementos-enlazables">
					<a title="" href="https://www.loteriasyapuestas.es/es/registrate-y-juega" class="c-2-elementos-enlazables__enlace" id="qa_home_enlazable1">
										<img alt="" class="c-2-elementos-enlazables__enlace--imagen" src="/f/loterias/imagenes/Destacados/DESTACADOSND/ACCESO-REGISTRO-2.gif">
									</a>
								<a title="ElementoEnlazableAvisoExplotacion" href="https://www.loteriasyapuestas.es/es/pago-de-premios" class="c-2-elementos-enlazables__enlace" id="qa_home_enlazable2">
										<img alt="ElementoEnlazableAvisoExplotacion" class="c-2-elementos-enlazables__enlace--imagen" src="/f/loterias/imagenes/Banners/BANNERSND/como-cobrar-premios.jpg">
									</a>
								</div>
			</div>
