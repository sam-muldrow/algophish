	</div>
	<script type="text/javascript">
		var topazLiteral = '_topaz';
		var jqGameGrid = jQuery.noConflict();

var gameLinksMobileGrid = {
	currentLanguage : "es",
	lnacSpecialPrizeLabel : 'Premio Especial',
	lnacFirstPrizeLabel : 'Primer Premio',
	euroSymbolLabel : '&#x20ac;',
	millionsLabel : 'millones',
	
	formatThousandsNumber: function (euros, isEnglish) {
		var number = parseFloat(euros);
		var roundNumber = null;
		if (number != null && number != undefined && !isNaN(number)) {
			if(!isEnglish){
				roundNumber = number.toLocaleString("es-ES");
			}
			try {
				var numberAux = '' + roundNumber;
				numberAux = numberAux.trim();
				if (hasWhiteSpace(numberAux)) {
					var numberAux2 = numberAux.split(/\s/g);
					roundNumber = numberAux2.join(".");
				}
			} catch (error) {
				console.log('error ' + error.message);
			}
		}
		return roundNumber;
	},

	printFormatedPrizeAmountInEuros : function (target, prizeAmountInEuros, gameId){
		var formatedValue = null;
		var html = '';
		if(prizeAmountInEuros != null){
			var isEnglish = 'EN' === this.currentLanguage || 'en' === this.currentLanguage;
			if (Math.abs(Number(prizeAmountInEuros)) >= 1010000) {
				var formatedValueAux = Number(prizeAmountInEuros) / 1.0e+6;
				var re = new RegExp('^-?\\d+(?:\.\\d{0,2})?');
		        formatedValue = formatedValueAux.toString().match(re)[0];
				if(!isEnglish){
					formatedValue = Number(formatedValue).toLocaleString("es-ES");
					html += '<p class="c-parrilla-juegos__elemento-cantidad-millones--' + gameId 
					+ topazLiteral + '" id="qa_home_parrilla_bote_' + gameId.toLowerCase() + '">';
					html += formatedValue;
					html += '</p>';
					html += '<p class="c-parrilla-juegos__elemento-millones--' + gameId + topazLiteral + '">' + this.millionsLabel + '</p>';
				}
			} else {
				formatedValue = this.formatThousandsNumber(prizeAmountInEuros, isEnglish);
				html += '<p class="c-parrilla-juegos__elemento-cantidad--' + gameId + topazLiteral +'" id="qa_home_parrilla_bote_' 
				+ gameId.toLowerCase() + '">';
				html += formatedValue;
				html += '<span class="c-parrilla-juegos__elemento-simbolo-euro--' + gameId + topazLiteral + '">';
				html += this.euroSymbolLabel;
				html += '</span>';
				html += '</p>';
			}
		}
		target.after(html);
	},

	printJackpot : function (target, jackpotInEuros, gameId){
		if(jackpotInEuros){
			this.printFormatedPrizeAmountInEuros(target, jackpotInEuros, gameId);
		}
	},
	
	printJackpots : function (draws){
		if (draws) {
			var size = draws.length;
			console.log("JSON draws size: " + size);
			var jackpotAlreadyAddedForGameBONO = false;
			var jackpotAlreadyAddedForGameEMIL = false;
			var jackpotAlreadyAddedForGameLOTU = false;
			var jackpotAlreadyAddedForGameLAPR = false;
			var jackpotAlreadyAddedForGameELGR = false;
			var jackpotAlreadyAddedForGameLAQU = false;
			var jackpotAlreadyAddedForGameQGOL = false;
			var jackpotAlreadyAddedForGameQUPL = false;
			var jackpotAlreadyAddedForGameLNAC = false;
			var jackpotBono = jqGameGrid('#qa_home_parrilla_logo_divtopaz_bono');
			var jackpotEmil = jqGameGrid('#qa_home_parrilla_logo_divtopaz_emil');
			var jackpotLotu = jqGameGrid('#qa_home_parrilla_logo_divtopaz_lotu');
			var jackpotLapr = jqGameGrid('#qa_home_parrilla_logo_divtopaz_lapr');
			var jackpotElgr = jqGameGrid('#qa_home_parrilla_logo_divtopaz_elgr');
			var jackpotLaqu = jqGameGrid('#qa_home_parrilla_logo_divtopaz_laqu');
			var jackpotQgol = jqGameGrid('#qa_home_parrilla_logo_divtopaz_qgol');
			var jackpotQupl = jqGameGrid('#qa_home_parrilla_logo_divtopaz_qupl');
			var jackpotLnac = jqGameGrid('#qa_home_parrilla_logo_divtopaz_lnac');
			
			for (var i = 0; i < size; i++) {
				var draw = draws[i];
				var gameId = draw.game_id;
				var celebrationTimeStrJson = draw.fecha;
				var openingTimeStrJson = draw.apertura;
				var closingTimeStrJson = draw.cierre;
				var openingTimeDate = createDateFromString(openingTimeStrJson);
				var closingTimeDate = createDateFromString(closingTimeStrJson);
				var jackpot = draw.premio_bote;
				console.log("-JSON draw data: celebrationTimeStrJson=" + celebrationTimeStrJson + "; gameId=" 
						+ gameId + "; openingTimeStrJson=" + openingTimeStrJson + "; closingTimeStrJson="
						+ closingTimeStrJson + "; jackpot=" + jackpot);
				
				if (gameId && openingTimeDate && closingTimeDate) {
					var nowTimeDate = new Date();
					if (openingTimeDate <= nowTimeDate && nowTimeDate <= closingTimeDate) {
						switch (gameId) {
						case 'BONO':
							if (!jackpotAlreadyAddedForGameBONO && jackpotBono.length) {
								this.printJackpot(jackpotBono, jackpot, gameId);
								jackpotAlreadyAddedForGameBONO = true;
							}
							break;
						case 'EMIL':
							if (!jackpotAlreadyAddedForGameEMIL && jackpotEmil.length) {
								this.printJackpot(jackpotEmil, jackpot, gameId);
								jackpotAlreadyAddedForGameEMIL = true;
							}
							break;
						case 'LOTU':
							if (!jackpotAlreadyAddedForGameLOTU && jackpotLotu.length) {
								this.printJackpot(jackpotLotu, jackpot, gameId);
								jackpotAlreadyAddedForGameLOTU = true;
							}
							break;
						case 'LAPR':
							if (!jackpotAlreadyAddedForGameLAPR && jackpotLapr.length) {
								this.printJackpot(jackpotLapr, jackpot, gameId);
								jackpotAlreadyAddedForGameLAPR = true;
							}
							break;
						case 'ELGR':
							if (!jackpotAlreadyAddedForGameELGR && jackpotElgr.length) {
								this.printJackpot(jackpotElgr, jackpot, gameId);
								jackpotAlreadyAddedForGameELGR = true;
							}
							break;
						case 'LAQU':
							if (!jackpotAlreadyAddedForGameLAQU && jackpotLaqu.length) {
								this.printJackpot(jackpotLaqu, jackpot, gameId);
								jackpotAlreadyAddedForGameLAQU = true;
							}
							break;
						case 'QGOL':
							if (!jackpotAlreadyAddedForGameQGOL && jackpotQgol.length) {
								this.printJackpot(jackpotQgol, jackpot, gameId);
								jackpotAlreadyAddedForGameQGOL = true;
							}
							break;
						case 'QUPL':
							if (!jackpotAlreadyAddedForGameQUPL && jackpotQupl.length) {
								this.printJackpot(jackpotQupl,jackpot, gameId);
								jackpotAlreadyAddedForGameQUPL = true;
							}
							break;
						case 'LNAC':
							if (!jackpotAlreadyAddedForGameLNAC && jackpotLnac.length) {
								var lnacPrizeType = jqGameGrid('#qa_home_parrilla_tipo_premio_LNAC');
								var specialPrize = draw.premio_especial;
								var firstPrize = draw.primer_premio;
								if (specialPrize != null && specialPrize != undefined){
									this.printJackpot(jackpotLnac,specialPrize, gameId);
									lnacPrizeType.html(this.lnacSpecialPrizeLabel);
								}else if (firstPrize != null && firstPrize != undefined){
									this.printJackpot(jackpotLnac,firstPrize, gameId);
									lnacPrizeType.html(this.lnacFirstPrizeLabel);
								} else {
									lnacPrizeType.hide();
								}
								jackpotAlreadyAddedForGameLNAC = true;
							}
							break;
						}
					}
				}
			}
		} else {
			console.debug('ERROR: no existen sorteos futuros que mostrar.');
		}
	},
	
	printJackpotsFromExternalJson : function (){
		var that = this;
		var jsonProximosv3Url = '//' + window.location.host + '/servicios/proximosv3?game_id=TODOS&num=2';

		jqGameGrid.getJSON(jsonProximosv3Url, function(data) {
			if (data) {
				if (data != null && data != undefined && typeof data != 'string') {
					that.printJackpots(data);
				} else {
					console.log('ERROR: ' + data);
				}
			} else {
				console.log('ERROR: no se han cargado los datos de sorteos futuros');
			}
		}).fail(function(jqxhr, textStatus, error) {
			var err = textStatus + ', ' + error;
			console.log('Request Failed: ' + err);
		});
	},
	
	printJackpotsFromJson : function (drawsData){
		if (drawsData === undefined) {
			this.printJackpotsFromExternalJson();
		} else {
			this.printJackpots(drawsData);
		}
	}
};



jqGameGrid(document).ready(function() {
	var drawsData=undefined; // should be defined later when creating the new header
	gameLinksMobileGrid.printJackpotsFromJson(drawsData);
});</script>
	
</div>
