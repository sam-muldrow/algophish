	<script type="text/javascript">var jqHLH = jQuery.noConflict();

var horizontalLinksHome = {
	currentLanguage : "es",
	Label : '',
	
	removeLinksOutOfDate : function (){
		var activation, deactivation;
		var actualDate = new Date();
		actualDate.setSeconds(0);
		actualDate.setMilliseconds(0);

		jqHLH('div.soon-to-be-deleted').each(function(i, e) {
			var activationStr = jqHLH(e).attr('data-activation');
			var deactivationStr = jqHLH(e).attr('data-deactivation');
			var remove = false;

			if (activationStr && !deactivationStr && !remove) {
				activation = createDateFromString(activationStr);
				if (activation > actualDate) {
					jqHLH(e).remove();
					console.debug('Element ' + jqHLH(e).attr('id') + ' was removed');
					remove = true;
				}
			}
			if (!activationStr && deactivationStr && !remove) {
				deactivation = createDateFromString(deactivationStr);
				if (deactivation < actualDate) {
					jqHLH(e).remove();
					console.debug('Element ' + jqHLH(e).attr('id') + ' was removed');
					remove = true;
				}
			}
			if (activationStr && deactivationStr && !remove) {
				activation = createDateFromString(activationStr);
				deactivation = createDateFromString(deactivationStr);
				if (!(activation <= actualDate && deactivation >= actualDate)) {
					jqHLH(e).remove();
					console.debug('Element ' + jqHLH(e).attr('id') + ' was removed');
					remove = true;
				}
			}
			if (!remove) {
				console.debug('Element ' + jqHLH(e).attr('id') + ' remains');
			}
		});
	}, 
	
	createDateFromString : function (str){
		try {
			if (str) {
				if (new Date(str).getTime()) {
					return (new Date(str));
				} else {
					var dateSrt = str.split(' ')[0] + 'T' + str.split(' ')[1];
					return (new Date(dateSrt));
				}
			} else {
				return null;
			}
		} catch (err) {
			console.error('ERROR en createDateFromString: ' + err);
			return null;
		}
	}, 
	
	printRemainingFirstTwoLinksAndRemoveTheRest : function (length) {
		var first, title1, href1, firstImage, src1;
		var html = '';
		switch(length) {
			case 0:
				break;
			case 1:
				first = jqHLH( "div.soon-to-be-deleted:nth-child(1) > a" );
				title1 = first.attr( 'title' );
				href1 = first.attr( 'href' );
				firstImage = jqHLH( "div.soon-to-be-deleted:nth-child(1) img" );
				src1 = firstImage.attr( 'data-src' );
				html += '<div class="c-destacados-home c-1-destacado">';
				html += '<a href="' + href1 + '" title="' + title1 + '" class="c-1-destacado" id="qa_home_destacado1">';
				html += '<img src="' + src1 + '" class="c-1-destacado__enlace-imagen">';
				html += '</a>';
				html += '</div>';
				break;
			default:
				first = jqHLH( "div.soon-to-be-deleted:nth-child(1) > a" );
				title1 = first.attr( 'title' );
				href1 = first.attr( 'href' );
				firstImage = jqHLH( "div.soon-to-be-deleted:nth-child(1) img" );
				src1 = firstImage.attr( 'data-src' );
				var second = jqHLH( "div.soon-to-be-deleted:nth-child(2) > a" );
				var title2 = second.attr( 'title' );
				var href2 = second.attr( 'href' );
				var secondImage = jqHLH( "div.soon-to-be-deleted:nth-child(2) img" );
				var src2 = secondImage.attr( 'data-src' );
				html += '<div class="c-destacados-home c-2-destacados">';
				html += '<a href="' + href1 + '" title="' + title1 + '" class="c-2-destacados__enlace" id="qa_home_destacado1">';
				html += '<img src="' + src1 + '" class="c-2-destacados__enlace-imagen">';
				html += '</a>';
				html += '<a href="' + href2 + '" title="' + title2 + '" class="c-2-destacados__enlace" id="qa_home_destacado2">';
				html += '<img src="' + src2 + '" class="c-2-destacados__enlace-imagen">';
				html += '</a>';
				html += '</div>';
		}
		jqHLH('#highlightedLinksRegion').append(html);
	}, 

};

jqHLH(document).ready(function() {
	horizontalLinksHome.removeLinksOutOfDate();
	var length = jqHLH('div.soon-to-be-deleted').length;
	horizontalLinksHome.printRemainingFirstTwoLinksAndRemoveTheRest(length);
	jqHLH('#soon-to-be-deleted-div').remove();
});</script>
</div>
