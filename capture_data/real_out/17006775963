<script>
    
    window.addEventListener('load', function(e){
    
        $('#formSMSSignup').on('input', 'input:text', function(e){
            $('#formSMSSignup :button').attr('disabled', formEmpty);
        });
    
        function formEmpty(){
            return ($('#formSMSSignup [name=signupEmail]').val().length === 0 && $('#formSMSSignup [name=phoneNumber]').val().length ===0);
    
        }
    });
    
    </script>
