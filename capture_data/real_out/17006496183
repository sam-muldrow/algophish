        const buildLinkTrackingData = function ($item) {
            const zoneId = RTSClickTracker.getClosestAttributeValue($item, RTSClickTracker.DataAttributes.zoneId, null);
            if (!zoneId) return null;

            const areaId = RTSClickTracker.getClosestAttributeValue($item, RTSClickTracker.DataAttributes.areaId, null);
            if (!areaId) return null;

            const itemId = RTSClickTracker.getItemId($item);
            if (!itemId) return null;

            const $area = $item.closest(RTSClickTracker.Selectors.areaId);

            const trackingData = {};
            trackingData.areaId = areaId;
            trackingData.areaTitle = RTSClickTracker.getAreaTitle($area);
            trackingData.areaIndex = parseInt(RTSClickTracker.getAreaIndex(zoneId, $area));
            trackingData.itemId = itemId;
            trackingData.itemTitle = RTSClickTracker.getItemTitle($item);

            return trackingData;
        }

        boreas('newTracker', 'boreas', collectorUrl, {
            postPath: '/boreas/collect',
            appId: appId,
            discoverRootDomain: true,
            cookieSameSite: 'Lax',
            cookieSecure: false,
            session: true,
            contexts: {
                webPage: true,
                session: true,
                browser: true
            }
        });

        boreas('enableActivityTracking', {
            minimumVisitLength: 30,
            heartbeatDelay: 10
        });

        if (articleDetails) {
            boreas("addGlobalContexts", [{
