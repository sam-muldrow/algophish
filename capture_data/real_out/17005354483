<script>

$(function(){


$("#circles").change(function(){
if($(this).val()!=""){
window.location=$(this).val();
}
});


$(".portal").click(function(){
//window.location=$(this).attr("data-url");
  var url = $(this).attr("data-url");
  window.open(url, "_blank", "location=yes,height=570,width=1020,scrollbars=yes,status=yes");
});

$(".portal").hover(
function(){
var cls=$(this).prev().children("i").attr("class").split(" ");
cls=cls[cls.length-1];
$(this).addClass(cls);
},
function(){

var cls=$(this).prev().children("i").attr("class").split(" ");
cls=cls[cls.length-1];
$(this).removeClass(cls);

});

});
  </script>
