<script type="text/javascript" >
    document.getElementById('loginForm-captcha-button').onclick = function(event) {
        event.preventDefault();
        if (window.grecaptcha) { // check if captcha disabled or user is blocking google
            grecaptcha.ready( function() {
                grecaptcha.execute(
                    "6LceAcEZAAAAAK9LjsJrTYcI-7RSvlTaO2B8axUG",
                    { action: 'login' },
                ).then( function(token) {
                    document.getElementById('loginForm-captcha-token').value = token;
                    document.getElementById('loginForm').submit();
                })
            });
        } else {
            document.getElementById('loginForm').submit();
        }
    }
</script>
