                                            '' + headNewsValue2.title + '</a>';
                                        $("#singleRowScrollContent1").append(htmlThree);
                                    }
                                    if(headNewsI2 == 11 || headNewsI2 == 12 || headNewsI2 == 13 || headNewsI2 == 14 || headNewsI2 == 15){
                                        $("#singListTwo").css('display','block');
                                        var htmlThree = '<span style="padding-right: 5px;">Â·</span><a target="_blank" href="' + urlhttp(headNewsValue2.titleurl) + '">' +
                                            '' + headNewsValue2.title + '</a>';
                                        $("#singleRowScrollContent2").append(htmlThree);
                                    }
                                    if(headNewsI2 == 16 || headNewsI2 == 17 || headNewsI2 == 18 || headNewsI2 == 19 || headNewsI2 == 20){
                                        $("#singListThree").css('display','block');
                                        var htmlThree = '<span style="padding-right: 5px;">Â·</span><a target="_blank" href="' + urlhttp(headNewsValue2.titleurl) + '">' +
                                            '' + headNewsValue2.title + '</a>';
                                        $("#singleRowScrollContent3").append(htmlThree);
                                    }
                                    if(headNewsI2 == 21 || headNewsI2 == 22 || headNewsI2 == 23 || headNewsI2 == 24 || headNewsI2 == 25){
                                        $("#singListFour").css('display','block');
                                        var htmlThree = '<span style="padding-right: 5px;">Â·</span><a target="_blank" href="' + urlhttp(headNewsValue2.titleurl) + '">' +
                                            '' + headNewsValue2.title + '</a>';
                                        $("#singleRowScrollContent4").append(htmlThree);
                                    }
                                }
                                if (headNewsI == 'firsttitle') {
                                    var firstHtml = '<div class="swiper-slide news-slide-item">' +
                                        '<a target="_blank" href="' + urlhttp(headNewsValue2.titleurl) + '">' +
                                        '<img src="' + headNewsValue2.headpic1 + '" /></a>' +
                                        '<div class="bot-desc"><span>' + headNewsValue2.title + '</span></div></div>';
                                    $("#first-title").append(firstHtml);
                                }
                                if (headNewsI == 'times') {
                                    var timesHtml = '<li class="scroll-item scroll-item-hover">' +
                                        '<a href="' + urlhttp(headNewsValue2.titleurl) + '">' +
                                        '<div class="syc_right_lb">' +
