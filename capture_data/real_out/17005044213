<script src="/cache/Purple.notification.v1699937533.js"></script><script type="text/javascript">
    (function () {
        var isSmallScreen = window.innerWidth < 992;

        if(isSmallScreen) {
            var _scrl = window.scrollY;
            $.each(["acc-provinces"],function (k,v) {
                var $a = $('a[href="#'+v+'"]');
                var $c =  $('#'+v);
                $c.find('a').addClass('filter active no-before no-bg pt-15 pb-15 ').removeClass('btn');

                $a.on('click', function (e) {
                    e.preventDefault();
                    e.stopPropagation();
                    _scrl = window.scrollY;
                    $c.addClass('in').eeSlideUp('open');
                });
                $c.eeSlideUp({
                    headerClassName: 'ac--heading',
                    contentClassName: 'ac--body',
                    //fixHeight: true,
                    force: true
                });
                $c.find('.cross').on('click',function (e) {
                    e.preventDefault();
                    $c.eeSlideUp('close');
                });
                $c.eeSlideUp().on('ee.close',function (e) {
                    setTimeout(function () {
                        window.scrollTo(0, _scrl);
                    },100)
                });
            });
        }
        else{
            $.each(["acc-provinces","acc-positions"],function (k,v) {
                var $a = $('a[href="#'+v+'"]:first');
                var $c =  $('#'+v);
                $c.on('show.bs.collapse', function () {
                    $a.hide();
                })
                $c.on('hide.bs.collapse', function () {
                    $a.show();
                })
            })
        }

        $.ajax({
            url: '/suggestions/homeFastSearchLinks',
            type: 'get',
            dataType: 'json',
            success: function (data) {
                if (data && data.result) {
                    if (data.result.length){
                        $("#search-fast-access").find(".skeleton-container").remove();
                        var res = data.result;
                        for (var i=0; i < res.length; i++){
                            var template = $("#search-fast-access-link-template").html();
                            template = template.replace("__url__",res[i].link);
                            template = template.replace("__title__",res[i].linkTitle);
                            $("#search-fast-access").append(template);
                        }
                    }
                }
            },
        });
    })();
</script>
<script type="text/javascript">
    (function(){
        var $c = $('#frm595408');

        $c.find('.btnPasswordToggle').on('click',function (e) {
            e.preventDefault();
            var $t = $(this);
            var $inp = $t.closest('.form-group').find('input')
            if($inp.attr('type') === 'text'){
                $inp.attr('type','password');
                $t.removeClass('active');
            }else{
                $inp.attr('type','text');
                $t.addClass('active');
            }
        });

        $c.find("a.sms-login").on('click',function (e){
            e.preventDefault();
            $c.find('input.inp-username').attr('readonly',true);
            $c.find('.captcha-group').show();
            $c.find('.password-group').hide();
            $c.find('.captcha-group').find('.form-control').focus();
            $c.find('form').attr('action','/login-mobile?redirect=%2F')
            $c.find('.btnSubmit').val("دریافت پیامک");
            $c.find('.t1').hide();
            $c.find('.t2').show();
            $c.find('.btnCaptchaRefresh').trigger('click');
        })
        $c.find('.btnCaptchaRefresh').on('click',function (e) {
            e.preventDefault();
            $c.find('.captcha > img').attr('src',"/captcha?"+Date.now())
        })
        $c.find('.btnBackToPassword').on('click',function (e) {
            $c.find('form').attr('action','/login?redirect=%2F')
            $c.find('input.inp-username').attr('readonly',false);
            $c.find('.captcha-group').hide();
            $c.find('.password-group').show();
            $c.find('.password-group').find('.form-control').focus();
            $c.find('.btnSubmit').val("ورود");
            $c.find('.t1').show();
            $c.find('.t2').hide();
        })

        function controlPasswordField($elm){
            var val = $elm.val();
            val = ar_numbers(val);

            if (val && val.match(/^(00[1-9]|\+[1-9])[0-9]+$|(^0{1}((((910)|(911)|(912)|(913)|(914)|(915)|(916)|(917)|(918)|(919)|(990)|(991)|(900)|(901)|(902)|(903)|(904)|(905)|(930)|(933)|(934)|(935)|(936)|(937)|(938)|(939)|(940)|(941)|(932)|(920)|(921)|(922)|(923)){1}[0-9]{7})|(9981[0-9]{6})|(99[0123469][0-9]{7}))$)/) ){
                $c.find(".password-control").addClass('mobile-mode').find('.form-control').attr('required',false);
            }else{
                $c.find(".password-control").removeClass('mobile-mode').find('.form-control').attr('required',true);
            }
        }
        $c.find('.inp-username').off('click keypress keydown').on('input change', function (e) {
            e.preventDefault();
            controlPasswordField($(this));
        });
        controlPasswordField($c.find('.inp-username'));

        $c.find('.cs-value').val($.fn.isCookieEnabled()?1:0);
        try{
            var l = localStorage.getItem('t')
        }catch (e) {
            $c.find('.rp-value').val(e && e.message);
        }
        /*$c.closest(".modal").on('shown.bs.modal', function(){
            $c.find('.inp-username').focus();
        });
        $c.closest(".ee-slideup").on('ee.opened', function(){
            $c.find('.inp-username').focus();
        });*/
    })();

</script>
    <script>
        $(document).ready(function () {
            var scrollInterval;
            function setIsLoadingInboxMessage($b){
                isLoadingInboxMessage = $b;
                if (isLoadingInboxMessage){
                    $("#tinbox-spinner").removeClass("hidden");
                }
                else{
                    $("#tinbox-spinner").addClass("hidden");
                }
            }
            var isLoadingInboxMessage = false;
            var lastInboxMessageId = '0';
            var isInboxTabActive = true;
            var isRelateJobTabActive = false;
            var notLoadInboxMessage = false;
            var notSendRelatedJobLastDisplayed = true;
            var prevSendDisplayedUuids = [];
            if ($("#notif-dropdown").find(".inbox-message-container").length === 0){
                notLoadInboxMessage = true;
            }


            var $window = $(window);
            var width = $window.width();
            var height = $window.height();
            if (width < 768){
                $('#notif-dropdown').parent().on('show.bs.dropdown', function () {
                    window.history.pushState({notifications:1},'',null);
                    // lock scroll position, but retain settings for later
                    var scrollPosition = [
                        self.pageXOffset || document.documentElement.scrollLeft || document.body.scrollLeft,
                        self.pageYOffset || document.documentElement.scrollTop  || document.body.scrollTop
                    ];
                    var html = jQuery('html'); // it would make more sense to apply this to body, but IE7 won't have that
                    html.data('scroll-position', scrollPosition);
                    html.data('previous-overflow', html.css('overflow'));
                    html.css('overflow', 'hidden');
                    window.scrollTo(scrollPosition[0], scrollPosition[1]);
                    scrollInterval = setInterval(function (){
                        if (!$("#notif-dropdown").parent().hasClass("open")){
                            var html = jQuery('html');
                            html.css('overflow', html.data('previous-overflow'));
                            clearInterval(scrollInterval);
                        }
                    },200);
                    sendDisplayed();
                    setTimeout(function (){
                        var id = $("#notif-nav-parent").find(".tab-pane.active").attr("id");
                        if ( id === 'tinbox' ){
                            isInboxTabActive = true;
                            isRelateJobTabActive = false;
                            $("#tinbox-link").find(".new").removeClass("new");
                        }
                        else if(id === 'tjobs'){
                            isInboxTabActive = false;
                            isRelateJobTabActive = true;
                            $("#tjobs-link").find(".new").removeClass("new");
                            if (!notSendRelatedJobLastDisplayed){
                                notSendRelatedJobLastDisplayed = true;
                                $.ajax({
                                    url: "/panel/user-interactions/relatedJobsDisplayed",
                                    type: "post",
                                    dataType: 'json',
                                    data: {},
                                    beforeSend: function(xhr) {},
                                    success: function(data) {},
                                    fail:function(result){},
                                    error:function (result) {}
                                });
                            }
                        }
                        if ($("#tjobs-link").find(".new").length == 0 && $("#inbox-link").find(".new").length == 0){
                            $("#notif-dropdown-trigger").find(".dot").removeClass("dot");
                        }
                    },500);

                });
                $('#notif-dropdown').parent().on('hide.bs.dropdown', function () {
                    window.history.replaceState({notifications:0},'',null);
                    // un-lock scroll position
                    var html = jQuery('html');
                    var scrollPosition = html.data('scroll-position');
                    html.css('overflow', html.data('previous-overflow'));
                    window.scrollTo(scrollPosition[0], scrollPosition[1]);
                });
                $('#user-panel-dropdown').parent().on('show.bs.dropdown', function () {
                    window.history.pushState({panelMenu:1},'',null);
                    // lock scroll position, but retain settings for later
                    var scrollPosition = [
                        self.pageXOffset || document.documentElement.scrollLeft || document.body.scrollLeft,
                        self.pageYOffset || document.documentElement.scrollTop  || document.body.scrollTop
                    ];
                    var html = jQuery('html'); // it would make more sense to apply this to body, but IE7 won't have that
                    html.data('scroll-position', scrollPosition);
                    html.data('previous-overflow', html.css('overflow'));
                    html.css('overflow', 'hidden');
                    window.scrollTo(scrollPosition[0], scrollPosition[1]);
                    scrollInterval = setInterval(function (){
                        if (!$("#user-panel-dropdown").parent().hasClass("open")){
                            var html = jQuery('html');
                            html.css('overflow', html.data('previous-overflow'));
                            clearInterval(scrollInterval);
                        }
                    },200);

                });
                $('#user-panel-dropdown').parent().on('hide.bs.dropdown', function () {
                    window.history.replaceState({panelMenu:0},'',null);
                    // un-lock scroll position
                    var html = jQuery('html');
                    var scrollPosition = html.data('scroll-position');
                    html.css('overflow', html.data('previous-overflow'));
                    window.scrollTo(scrollPosition[0], scrollPosition[1]);
                });
                window.addEventListener("popstate", function (event){
                    if( (!event.state || event.state.notifications !== 1) && $("#notif-dropdown-trigger").parent().hasClass("open")){
                        $("#notif-dropdown-trigger").trigger("click");
                    }
                    if( (!event.state || event.state.panelMenu !== 1) && $("#user-panel-dropdown-trigger").parent().hasClass("open")){
                        $("#user-panel-dropdown-trigger").trigger("click");
                    }
                });
                $("#notif-dropdown .tab-content").css({'height':(height-100)+"px"});
            }
            else {
                $("#notif-dropdown .tab-content").css({'height':parseInt(2*height/3)+"px"});
            }

            $("#main-dropdown-usermenu").css({height:"100%",overflowY:'scroll'});
            if (width < 768){
                $("#user-panel-dropdown").css({'height':(height)+"px"});
                $("#main-dropdown-usermenu").css({paddingBottom:"150px"});
            }
            else {
                $("#user-panel-dropdown").css({'max-height':parseInt(9*height/10)+"px",overflowY:'auto'});
            }

            $('#notif-nav a').on('click', function (e) {
                e.preventDefault();
                e.stopPropagation();
                $(this).closest('ul').find("li").removeClass("active");
                $(this).closest("li").addClass("active");
                var id = $(this).attr("data-target");
                if (!id){
                    return;
                }
                var $tabContent = $(this).closest("#notif-nav-parent").find(".tab-content");
                $tabContent.find(".tab-pane").each(function (idx){
                    var elm = $(this);
                    if (elm.attr("id")=== id){
                        elm.addClass("active");
                    }
                    else {
                        elm.removeClass("active");
                    }
                });
                if ( id === 'tinbox' ){
                    isInboxTabActive = true;
                    isRelateJobTabActive = false;
                    $("#tinbox-link").find(".new").removeClass("new");
                }
                else {
                    isInboxTabActive = false;
                    isRelateJobTabActive = true;
                    $("#tjobs-link").find(".new").removeClass("new");
                    if (!notSendRelatedJobLastDisplayed){
                        notSendRelatedJobLastDisplayed = true;
                        $.ajax({
                            url: "/panel/user-interactions/relatedJobsDisplayed",
                            type: "post",
                            dataType: 'json',
                            data: {},
                            beforeSend: function(xhr) {},
                            success: function(data) {},
                            fail:function(result){},
                            error:function (result) {}
                        });
                    }
                }
                if ($("#tjobs-link").find(".new").length == 0 && $("#inbox-link").find(".new").length == 0){
                    $("#notif-dropdown-trigger").find(".dot").removeClass("dot");
                }
            });

            $("#notif-dropdown .tab-content").scroll(function(e) {
                e.stopPropagation();
                var $this = $(this);
                if (!notLoadInboxMessage && isInboxTabActive && !isLoadingInboxMessage && ($this.scrollTop() + $this.innerHeight() >= $this[0].scrollHeight) ) {
                    setIsLoadingInboxMessage(true);
                    $.ajax({
                        url: "/panel/user-inbox-messages/get",
                        type: "post",
                        dataType: 'json',
                        data: {
                            lastId: lastInboxMessageId,
                        },
                        beforeSend: function(xhr) {},
                        success: function(data) {
                            var content = data && data.content || '';
                            if (content){
                                $this.find(".inbox-message-container").append(content);
                            }
                            var newLastId = data.newLastId || lastInboxMessageId;
                            if (!content && newLastId == lastInboxMessageId){
                                notLoadInboxMessage = true;
                            }
                            lastInboxMessageId = newLastId;
                            setIsLoadingInboxMessage(false);
                            sendDisplayed();
                        },
                        fail:function(result){
                            setIsLoadingInboxMessage(false);
                        },
                        error:function (result) {
                            setIsLoadingInboxMessage(false);
                        }

                    });
                }
            });
            function sendDisplayed(){
                var uuids = [];
                $("#notif-dropdown").find(".displayed-item").each(function (){
                    var uid = $(this).data("display-uuid");
                    if (uid && prevSendDisplayedUuids.indexOf(uid) < 0){
                        uuids.push(uid);
                    }
                });
                if (uuids.length){
                    prevSendDisplayedUuids = prevSendDisplayedUuids.concat(uuids);
                    $.ajax({
                        url: "/panel/user-inbox-messages/displayed",
                        type: "post",
                        dataType: 'json',
                        data: {
                            uuids: uuids,
                        },
                        beforeSend: function(xhr) {},
                        success: function(data) {},
                        fail:function(result){},
                        error:function (result) {}
                    });
                }
            }
            $("#notif-dropdown .tab-content").find('*[data-toggle="collapse"]').on("click",function(e){
                var id = $(this).data("target");
                if (id){
                    $(id).addClass("in");
                }
            });
        });
    </script>
<script>
    jQuery(document).ready(function(){
        var activeHREF= window.location.pathname;
        $('a.btn.btn-xs.mobile-button').removeClass('active');
        $('a[name="'+activeHREF+'"]').addClass('active');
        if (activeHREF.includes("/config") || activeHREF.includes("/panel/users")) {
            $('a[name="/app-profile"]').addClass('active');
        }
        if (activeHREF.includes("/search")) {
            $('a[name="/search"]').addClass('active');
        }

        function clearPageContent(offlineHash='') {
            window.stop();
            window.scrollTo(0, 0);
            $("#loading-indicator").remove();
            $('html').removeClass('sidebar-mode');
            $('div#content').remove();
            $('div.company').remove();
            $('div.options-app-offline').remove();
            $('div.home-app-offline').remove();
            $('div.notifications-app-offline').remove();
            $('div.search-app-offline').remove();
            $('div.profile-app-offline').remove();
            $('div.header-after-section-rm').remove();
            history.pushState(null, null, offlineHash.replace('#', ''));
        }

        function appendScriptToDocument(loggined=true) {
            if (loggined == 'notif') {

                let notifScriptCode = `var scrollInterval;
            function setIsLoadingInboxMessage($b){
                isLoadingInboxMessage = $b;
                if (isLoadingInboxMessage){
                    $("#tinbox-spinner").removeClass("hidden");
                }
                else{
                    $("#tinbox-spinner").addClass("hidden");
                }
            }
            var isLoadingInboxMessage = false;
            var lastInboxMessageId = '0';
            var isInboxTabActive = true;
            var isRelateJobTabActive = false;
            var notLoadInboxMessage = false;
            var notSendRelatedJobLastDisplayed = true;
            var prevSendDisplayedUuids = [];
            if ($("#notif-dropdown").find(".inbox-message-container").length === 0){
                notLoadInboxMessage = true;
            }


            var $window = $(window);
            var width = $window.width();
            var height = $window.height();
            if (width < 768){
