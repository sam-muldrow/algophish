            <script async src="https://www.googletagmanager.com/gtag/js?id=G-99K4T4K9JX"></script>
    <script>
        var gTagConfig = {};
                            gTagConfig['dimension1'] = 'guest';
                window.dataLayer = window.dataLayer || [];
        function gtag(){
            dataLayer.push(arguments);
        }
        gtag('js', new Date());
        gtag('config', 'G-99K4T4K9JX',gTagConfig);
    </script>
<script>
    if (document.querySelectorAll && gtag){
        //var els = document.querySelectorAll("a[href='https://www.instagram.com/e_estekhdam_official/']");
        function prepareGtagFollowEvent(){
            var gtagFollowEventElements = document.querySelectorAll(".gtag-follow-event");
            if (gtagFollowEventElements && gtagFollowEventElements.length){
                for (var i=0;i<gtagFollowEventElements.length;i++){
                    var elm = gtagFollowEventElements[i];
                    elm.addEventListener("click",function(e){
                        var action = this.getAttribute("data-action");
                        var category = this.getAttribute("data-category");
                        var label = this.getAttribute("data-label");
                        var value = this.getAttribute("data-value");
                        if (action && category){
                            gtag('event',action,{
                                event_category: category,
                                event_label: label,
                                data_value: value || ''
                                // 'event_callback': function() {
                                //     if (this.getAttribute("target") !== '_blank'){
                                //         window.location.href = this.getAttribute('href')
                                //     }
                                // }
                            });
                        }
                        if (this.getAttribute("target") !== '_blank'){
                            e.preventDefault();
                            var href = this.getAttribute('href');
                            setTimeout(function (){
                                window.location.href = href;
                            },1000);
                        }
                    });
                }
            }
        }
        prepareGtagFollowEvent();
    }
</script>
</body>
</html>
