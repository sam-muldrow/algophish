<script type="text/javascript">

function changeText(){
var list=document.querySelector('.message-content')
if(window.location.href == "https://www.dpd.com/de/de/empfangen/sendungsverfolgung-und-live-tracking/"){if(list.innerHTML=='Chat'){list.innerHTML='Wo ist mein Paket?';}}
}

function hideelement() {
  const box = document.querySelector('.greeting-message-box');
const box2 = document.querySelector('.rf-chat-message-footer');
  box.style.visibility = 'hidden'; 
box2.style.visibility = 'hidden';
}
function checkcookies(){
if (String(sessionStorage.getItem("cookie")) != String(dpdAllowCookies.youtube)){
    sessionStorage.setItem("reset", "true");
    window['BOT_WEB_INTERFACE_SETTINGS'].setEntitiesOnAllMessages.youtubecookies=dpdAllowCookies.youtube;
    var script = document.createElement('script');
    script.src = "https://chatbot.alturing.eu/de/JavaHookServer/static/index.js";
    document.getElementsByTagName('body')[0].appendChild(script);
}else{
    sessionStorage.setItem("reset", "false");
}
}
function checkIfBotReady() {
  const el = document.querySelector('.rf-chat-box-container')
  if(el !== null){changeText(); setTimeout(hideelement,10000); setTimeout(checkcookies,3000)}else{setTimeout(checkIfBotReady,300);}
}
checkIfBotReady();


</script>
