<script>
//vdo zone slide
    function setCurrentTab(ele,index){
      $(".vdoclip_tab").removeClass("selected");
      ele.addClass("selected");
    }

    window.onload = function() {
      $('.vdoclip_tab').on("click",function(){
        var index = $('.vdoclip_tab').index(this)
        console.log(index)
        swiperClip.slideTo(index, 500, false);
        $(".vdoclip_tab").removeClass("selected");
        $(this).addClass("selected");
      });
    }

    var swiperClip = new Swiper ('.swiper-clip', {
      direction: 'horizontal',
      loop: false,
      autoHeight: true,
      pagination: '.swiper-pagination-clip',
      nextButton: '.vdoclip .swiper-button-next',
      prevButton: '.vdoclip .swiper-button-prev',
      paginationClickable: true,
      onSlideChangeEnd: function(swiper){
        var n=swiper.activeIndex;
        // console.log(n)
        setCurrentTab($(".vdoclip_tab").eq(n),n);
      }
    });
  //End vdo zone slide 
    var swiper1 = new Swiper('.swiper1', {
        pagination: '.swiper-pagination1',
        nextButton: '.swiper-button-next1',
        prevButton: '.swiper-button-prev1',
        paginationClickable: true,
        slidesPerView: 4.5,
        spaceBetween:0,
    });
    var swiper2 = new Swiper('.swiper2', {
        pagination: '.swiper-pagination2',
        nextButton: '.swiper-button-next2',
        prevButton: '.swiper-button-prev2',
        paginationClickable: true,
        slidesPerView: 4.5,
        spaceBetween:0,
    });
    var swiper3 = new Swiper('.swiper3', {
        pagination: '.swiper-pagination3',
        nextButton: '.swiper-button-next3',
        prevButton: '.swiper-button-prev3',
        paginationClickable: true,
        slidesPerView: 4.5,
        spaceBetween:0,
    });
    var swiper4 = new Swiper('.swiper4', {
        pagination: '.swiper-pagination4',
        nextButton: '.swiper-button-next4',
        prevButton: '.swiper-button-prev4',
        paginationClickable: true,
        slidesPerView: 3.5,
        spaceBetween:0,
        observer: true,
        observeParents: true,
    });
    var swiper5 = new Swiper('.swiper5', {
        pagination: '.swiper-pagination5',
        nextButton: '.swiper-button-next5',
        prevButton: '.swiper-button-prev5',
        paginationClickable: true,
        slidesPerView: 3.5,
        spaceBetween:0,
        observer: true,
        observeParents: true,
    });    
</script>

    
    
