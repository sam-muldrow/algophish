<script type="text/javascript">
    $(document).ready(function(){
        $(".hightlight .tabs li").on("click", function(){ 
            $(this).addClass("active").siblings().removeClass("active")
            $(this).closest(".tabs").siblings("div.tab-contents").find("li.section").eq($(this).index()).addClass("active").siblings().removeClass("active")
        })
    });
</script>
    
