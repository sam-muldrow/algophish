var mobileKeyWords = new Array('iPhone', 'Android');
for (var word in mobileKeyWords)
{
	if (document.referrer.indexOf('m.kmib.co.kr') == -1 && navigator.userAgent.match(mobileKeyWords[word]) != null)
	{
		location.href = "https://m.kmib.co.kr";
		break;
	}
}

$(document).ready(function(){


//¼Óº¸
	var hotTimer;
	var hotObj = $("div.hot_nws>div.hot_nws_lst>ul>li");
	hotObj.bind("mouseover", function(){clearInterval(hotTimer);});
	hotObj.bind("mouseout", function(){hotTimer = setInterval(function(){rollingHot()}, 3000);});
	$("div.hot_nws>div.btn_area>a.up").click(function(){
		$("div.hot_nws>div.hot_nws_lst>ul>li").eq(hotObj.length-1).prependTo($("div.hot_nws>div.hot_nws_lst>ul"));
	});
	$("div.hot_nws>div.btn_area>a.down").click(function(){
		$("div.hot_nws>div.hot_nws_lst>ul>li").eq(0).appendTo($("div.hot_nws>div.hot_nws_lst>ul"));
	});
	if (hotObj.length>1)
	{
		hotTimer = setInterval(function(){rollingHot()}, 3000);
	}
	function rollingHot()
	{
		$("div.hot_nws>div.hot_nws_lst>ul>li").eq(0).slideUp( function () { $(this).appendTo($("div.hot_nws>div.hot_nws_lst>ul")).slideDown(); });
	}



//¿¬Àç½Ã¸®Áî
	var serTimer, serCnt, serRolIdx;
	var serObj = $("div.mn_series .s_bnr>ul>li");
	serCnt = serObj.length;
	serRolIdx = 0;
	var serhtml = "";
	for (var i=0;i<serCnt;i++)
	{
		serhtml = serhtml + "<a href=\"javascript:;;\"></a>"
	}
	$("div.mn_series p.dott_area").html(serhtml);
	$("div.mn_series p.dott_area>a").eq(0).addClass("on");
	$("div.mn_series .s_bnr>ul>li:gt(0)").hide();
	$("div.mn_series p.dott_area>a").click(function(index){
		clearInterval(serTimer);
		serRolIdx = $(this).index();
		$(this).addClass("on").siblings().removeClass("on");
		$("div.mn_series .s_bnr>ul>li").eq($(this).index()).show().siblings().hide();
		serTimer = setInterval(function(){rollingSer()}, 3000);
	});
	serObj.bind("mouseover", function(){clearInterval(serTimer);});
	serObj.bind("mouseout", function(){serTimer = setInterval(function(){rollingSer()}, 3000);});
	if (serCnt>1)
	{
		serTimer = setInterval(function(){rollingSer()}, 3000);
	}
	function rollingSer()
	{
		serRolIdx = serRolIdx + 1;
		if (serRolIdx==serCnt)
		{
			serRolIdx = 0;
		}
		$("div.mn_series .s_bnr>ul>li").eq(serRolIdx).show().siblings().hide();
		$("div.mn_series p.dott_area>a").eq(serRolIdx).addClass("on").siblings().removeClass("on");
	}

//¸¸Æò tab
	$(".mn_cartoon .tab_mnu li").mouseover(function(){
		$(".mn_cartoon .tab_mnu li a").removeClass("on");
		$(this).children("a").addClass("on");
		$(".mn_cartoon .mn_cartoon_inner div:eq("+$(this).index()+")").show().siblings().hide();
	});

// ¿µ»ó ½½¶óÀÌµå
	$('.mn_tv_inner ul').slick({
	  slidesToShow: 5,
	  slidesToScroll: 1,
	  autoplay: false

	});

	// ±âÈ¹±â»ç
	$('.mn_plan').slick({
	  dots: true,
	  arrows: false,
	  slidesToShow: 1,
	  slidesToScroll: 1,
	  autoplay: false
	});

	// ¹è³Ê01
	$('.mn_kmbnr01 ul').slick({
	  dots: false,
	  arrows: true,
	  autoplay: true
	});

	// ¹è³Ê02
	$('.mn_kmbnr02').slick({
	  dots: false,
	  fade:true,
	  infinite: true,
	 speed: 500,
	  arrows: false,
	  autoplay: true
	});

	onScrollDirection();
	onScrollAdjustEndLine('.scroll_start01',  '#topSc1',  '.scroll_start01_in');
	onScrollAdjustEndLine('.scroll_start02',  '#topSc1',  '.scroll_start02_in');
	onScrollAdjustEndLine('.scroll_start03',  '#topSc1',  '.scroll_start03_in');
});

var	top_last_scroll		= 0;
var	diff_scroll			= 0;

function	onScrollDirection(){
	var	function_direction	= function(){
	var	top_scroll	= $(document).scrollTop();
	window.diff_scroll		= top_scroll - window.top_last_scroll;
	window.top_last_scroll	= top_scroll;

		$(window).trigger("onScrollAdjustEndLine");
	};

	$(window).scroll(function_direction);
	$(window).resize(function_direction);

	setTimeout(function(){window.diff_scroll=-1;$(window).trigger("onScrollAdjustEndLine");}, 200);
};

function	onScrollAdjustEndLine(query_start, query_end, query_in){
	$(window).on("onScrollAdjustEndLine",function(){
			var	obj_start	= $(query_start);
			var	obj_end	= $(query_end);
			var	obj_in		= $(query_in);

			if ( obj_start && obj_end && obj_in ){
				var	top_in		= obj_in.offset().top;
				var	top_win		= $(document).scrollTop();
				var	top_start		= obj_start.offset().top;
				var	top_end		= obj_end.offset().top;
				var	left_start		= obj_start.offset().left;
				var	width_start	= obj_start.width();
				var	height_win	= $(window).height();
				var	height_end	= obj_end.height();
				var	height_in		= obj_in.height();
				var	bot_win		= top_win + height_win;
				var	bot_end		= top_end + height_end;
				var	bot_in		= top_in + height_in;
				var	gap_max		= top_end + height_end - top_start - height_in;

				obj_start.css({"height":height_in+"px"});
				obj_in.css({"height":height_in+"px"});

				if ( window.diff_scroll == 0 ){
					if ( obj_in.css("position") == "fixed" ){
						obj_in.css({"left":left_start+"px"});
					}
				}
				if ( height_in >= bot_end - top_start ){
					obj_in.css({"position":"relative","top":"0px","left":"0px","width":""});
				}
				else if ( height_win > top_end + height_end - top_start ){
					obj_in.css({"position":"relative","top":"0px","left":"0px","width":""});
				}
				else if ( height_win > height_in ){
					if ( top_start >= top_win ){
						obj_in.css({"position":"relative","top":"0px","left":"0px","width":""});
					}
					else if ( top_start + gap_max <= top_win ){
						obj_in.css({"position":"relative","top":gap_max+"px","left":"0px","width":""});
					}
					else{
						obj_in.css({"position":"fixed","top":"0px","left":left_start+"px","width":width_start+"px"});
					}
				}
				else{
					if ( window.diff_scroll < 0 )	{
						if ( top_start >= top_win ){
							obj_in.css({"position":"relative","top":"0px","left":"0px","width":""});
						}
						else if ( top_in >= top_win ){
							obj_in.css({"position":"fixed","top":"0px","left":left_start+"px","width":width_start+"px"});
						}
						else if ( obj_in.css("position") == "fixed" ){
							obj_in.css({"position":"relative","top":(top_in-top_start-diff_scroll)+"px","left":"0px","width":""});
						}
					}
					if ( window.diff_scroll > 0 )	{
						if ( bot_end <= bot_win ){
							obj_in.css({"position":"relative","top":gap_max+"px","left":"0px","width":""});
						}
						else if ( bot_in <= bot_win ){
							obj_in.css({"position":"fixed","top":(height_win-height_in)+"px","left":left_start+"px","width":width_start+"px"});
						}
						else if ( obj_in.css("position") == "fixed" ){
							obj_in.css({"position":"relative","top":(top_in-top_start-diff_scroll)+"px","left":"0px","width":""});
						}
					}
				}
			}
		}
	);
};


</script>
