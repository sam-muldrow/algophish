									'mediaTypes': {
										'banner': {
											'sizes': [[728, 90]],
										},
									},
								}, {
									'code': 'nesn_rect_mid_2',
									'divId': 'mid_2_home_bets',
									'adUnitId': '/3884310/nesn_rect_mid_2',
									'adUnitIndex': '0',
									'mediaTypes': {
										'banner': {
											'sizes': [[728, 90]],
										},
									},
								}, {
									'code': 'mid_1_home',
									'divId': 'mid_1_home',
									'adUnitId': '/3884310/nesn_rect_mid_1',
