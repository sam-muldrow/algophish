						'https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);
					})(window,document,'script','dataLayer', 'GTM-P4LWZHH' );
		</script>
		<!-- Resolute temporarily hard coded tag wrapping datalayer context to their account. -->
		<script async src="https://www.googletagmanager.com/gtag/js?id=AW-780576397"></script>
		<script>
			window.dataLayer = window.dataLayer || [];
			function gtag(){
				dataLayer.push(arguments);
			}
			gtag('js', new Date());
			gtag('config', 'AW-780576397');

			if ( window.localStorage.getItem( 'dtcOrderID' ) && window.localStorage.getItem( 'dtcOrderAmount' ) ) {
              window.dataLayer.push( {
                'orderID': window.localStorage.getItem( 'dtcOrderID' ) ?? 'n/a',
                'purchaseSubtotal': window.localStorage.getItem( 'dtcOrderAmount' ) ?? 'n/a'
              } );
			}
		</script>
