									},
								}, {
									'code': 'mid_3_home',
									'divId': 'mid_3_home',
									'adUnitId': '/3884310/nesn_rect_mid_3',
									'adUnitIndex': '0',
									'mediaTypes': {
										'banner': {
											'sizes': [[728, 90]],
										},
									},
								}, {
									'code': 'masthead_home',
									'divId': 'masthead_archive',
									'adUnitId': '/3884310/nesn_masthead',
									'adUnitIndex': '0',
									'mediaTypes': {
										'banner': {
											'sizes': [[970, 250], [970, 90], [728, 90]],
										},
									},
								}, {
									'code': 'footer',
									'divId': 'footer',
									'adUnitId': '/3884310/nesn_footer',
									'adUnitIndex': '0',
									'mediaTypes': {
										'banner': {
											'sizes': [[728, 90], [970, 90], [970, 250]],
										},
									},
								}, {
									'code': 'footer',
									'divId': 'prefooter',
									'adUnitId': '/3884310/nesn_footer',
									'adUnitIndex': '0',
									'mediaTypes': {
										'banner': {
											'sizes': [[970, 90], [970, 250], [728, 90]],
										},
									},
								},
							];

							openwrap_units = openwrap_units.concat(openwrap_units_by_default);
						}
					}

					if ( typeof window.PWT.requestBids === 'function' ) {
						const start_time = new Date().getTime();

						window.PWT.requestBids(
							openwrap_units,
							function ( adUnitsArray ) {
								const end_time = new Date().getTime();
								const diff = end_time - start_time;

								console.debug( 'Recieved OpenWrap bids', adUnitsArray );
								pwtBids.push ( adUnitsArray );
								pwtFinished = true;
								initAdServer();
							},
						);
					} else {
						console.log("Something went wrong when trying to load OpenWrap, so we are ignoring bids and letting GPT continue");
						pwtFinished = true;
						initAdserver();
					}
				} );
			};

			PWT.jsLoaded = function () {
				pwtLoaded = true;
				window.nesnOpenWrap();
			};

			function initAdServer() {
				if (initAdserverSet) return;
				// Either gpt or OpenWrap can be ready first.  We can not proceed until
				//  both are complete.  When both are complete, we add OpenWrap bids into
				//  targeting keys and then display the ad units

				if (gptLoaded && pwtFinished) {
					initAdserverSet = true;
					let au;

					while (au = pwtBids.shift()) {
						PWT.addKeyValuePairsToGPTSlots(au);
						window.openw_key_apply = true;

						au.forEach( adslot => {
							googletag.cmd.push(function() {
								if ( document.getElementById( adslot.divId ) ) {
									console.log("Calling Core display() for ",adslot.divId);
									googletag.display( adslot.divId );
								}
							});
						})
					}
				}

				// GAM Ad Targeting based on BlueConic profiles/segments
				if (typeof Storage !== "undefined") {
					const targetingParamStr = localStorage.getItem("bcDFPTargetingParams");

					if (targetingParamStr) {
						const targetingParameters = JSON.parse(targetingParamStr);

						// set page-level targeting parameters
						jQuery.each(targetingParameters, function (index, param) {
							window.googletag.pubads().setTargeting(param.key, param.value);
						});
					}
				}
			}
		</script>
				<!-- Google Tag Manager -->
		<script>
					window.dataLayer = window.dataLayer || [];
					(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':
