<section data-analytics-datalayer-category="MainBlock: Boston Celtics" class="card card--image-left">
	<div class="card__header ">
	<h2 class="card__header__title card__header__title--icon">
		<img class="nesn-icon" aria-hidden="" width="30" height="30" src="https://nesn.com/wp-content/plugins/nesn-images/dist/svg/boston-celtics.svg" />		<span class="card__header__title-text">Boston Celtics</span>
			</h2>
	<a href="http://nesn.com/boston-celtics" class="card__header__link" data-analytics-datalayer-category-type="ViewMore">View More</a></div>
