<script>
	//open middle page by mp=cdx
	var isopen = getcookie('isOpen');
	$(function(){
		changeHref();
	});
	function changeHref() {
		if (isopen == 1) {
			$('.changeUrl').each(function (n) {
				$(this).attr('href',$(this).attr('href').replace(/\/game\//,"/middle/"));
			});
		}else{
			$('.changeUrl').each(function (n) {
				$(this).attr('href',$(this).attr('href').replace(/\/middle\//,"/game/"));
			});
		}
	}

	/*lang chioce*/
	var  langAlias = ["en","ar","es","ru","pt","tr","fr"];
	function selectlang(lang){
		var myURL = parseURL(window.location.href);
		setcookie("langtype",lang,60*60*24*365,'/');
		var nowlang =  myURL.segments[0];
		var nowcat =  myURL.segments[1];
		var nowcatname =  myURL.segments[2];
		if (nowlang === 'accounts' || nowlang === 'favorite') {
			location.reload();
		}
		if ($.inArray(nowlang,langAlias) < 0) {
			if (nowlang == '') {
				if(lang == deflang){
					location.href = "http://"+window.location.host;
				}else{
					location.href = "http://"+window.location.host+"/"+lang;
				}
			} else if (nowlang == 'cat' || nowlang == 'tag') {
                cat = nowlang;
                catname = nowcat;
	                if(lang == deflang){
	                    location.href = "http://"+window.location.host+"/"+cat+"/"+catname;
	                }else{				
	                    location.href = "http://"+window.location.host+"/"+lang+"/"+cat+"/"+catname;
	                }
			} else if (nowlang == 'history' || nowlang == 'hot' || nowlang == 'newgames' || nowlang == 'recommend') {				
				catname = nowlang;
					if(lang == deflang){
						location.href = "http://"+window.location.host+"/"+catname;
					}else{
						location.href = "http://"+window.location.host+"/"+lang+"/"+catname;	
				}
			} else if (nowlang == 'middle' || nowlang == 'game') {
				cat = nowlang;
				for (var i = 0; i < alllangname.length; i++) {
					if(alllangname[i].game_lang == lang){
						var pathname = alllangname[i].game_url; 	
					}	
				};
				if(pathname == undefined || pathname == ''){
					for (var i = 0; i < alllangname.length; i++) {
						if(alllangname[i].game_lang == deflang){
							var pathname = alllangname[i].game_url; 	
						}
					}	
				};
					if(lang == deflang){
						location.href = "http://"+window.location.host+"/"+cat+"/"+pathname;
					}else{
						location.href = "http://"+window.location.host+"/"+lang+"/"+cat+"/"+pathname;		
					}
			} else if (nowlang == 'search') {
				var search_path = myURL.path;
				if(lang == deflang){
					location.href = "http://"+window.location.host+search_path;
				}else{
					location.href = "http://"+window.location.host+"/"+lang+search_path;		
				}
			} 
		} else {
			if (nowcat == undefined || nowcat == '') {
				if(lang == deflang){
					location.href = "http://"+window.location.host;
				}else{
					location.href = "http://"+window.location.host+"/"+lang;
				}
			} else if (nowcat == 'cat' || nowcat == 'tag') {
                cat = nowcat;
                catname = nowcatname;
	            if(lang == nowlang){
	                self.location.reload();
	            }else{
	                if(lang == deflang){
	                    location.href = "http://"+window.location.host+"/"+cat+"/"+catname;
	                }else{					        
	                    location.href = "http://"+window.location.host+"/"+lang+"/"+cat+"/"+catname;	      
	                }
	            }
			} else if (nowcat == 'history' || nowcat == 'hot' || nowcat == 'newgames' || nowcat == 'recommend') {
				catname = nowcat;
				if(lang == nowlang){
					self.location.reload();
				}else{
					if(lang == deflang){
						location.href = "http://"+window.location.host+"/"+catname;
					}else{		
						location.href = "http://"+window.location.host+"/"+lang+"/"+catname;
					}
				}
			} else if (nowcat == 'middle' || nowcat == 'game') {
				cat = nowcat;
				catname = nowcatname;
				for (var i = 0; i < alllangname.length; i++) {
					if(alllangname[i].game_lang == lang){
						var pathname = alllangname[i].game_url; 	
					}	
				}
				if(pathname == undefined || pathname == ''){
					for (var i = 0; i < alllangname.length; i++) {
						if(alllangname[i].game_lang == deflang){
							var pathname = alllangname[i].game_url; 	
						}
					}	
				}
				if(lang == nowlang){
					self.location.reload();
				}else{
					if(lang == deflang){
						location.href = "http://"+window.location.host+"/"+cat+"/"+pathname;
					}else{
						location.href = "http://"+window.location.host+"/"+lang+"/"+cat+"/"+pathname;
					}
				}
			} else if(nowcat == 'search') {
				var search_path = myURL.path;
				var pattern_path = new RegExp(search_path.substring(1,3));
				var redirect_path = search_path.replace(pattern_path, lang, search_path);
				var en_path = search_path.substring(3);
				if(lang == nowlang){
					self.location.reload();
				}else{
					if(lang == deflang){
						location.href = "http://"+window.location.host+en_path;
					}else{
						location.href = "http://"+window.location.host+redirect_path;
					}
				}		
			}
		}
	}
</script>

</html>