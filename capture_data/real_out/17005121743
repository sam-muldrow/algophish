		//self search submit
		function sub_mit() {
			var form_value = $("#search-keywords").val();
			if(form_value != "" && form_value != null && form_value != undefined){
				window.location.href=cookielang+"/search/q/"+form_value;
			}
		}
	</script>

	
	
