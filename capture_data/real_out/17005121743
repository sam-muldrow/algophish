<script>
	function myEvent(obj,ev,fn){
		if(obj.attachEvent){
			obj.attachEvent('on'+ev,fn);
		}else{
			obj.addEventListener(ev,fn,false);
		}
	}
	myEvent(window,'load',function(){
		var oRTT = document.getElementById('to_top');
		var pH = 100;/*document.documentElement.clientHeight*/
		var timer = null;
		var scrollTop;
		window.onscroll = function(){
			scrollTop=document.documentElement.scrollTop||document.body.scrollTop;
			if(scrollTop>=pH){
				oRTT.style.display='block';
			}else{
				oRTT.style.display='none';
			}
			return scrollTop;
		};
		oRTT.onclick=function(){
			clearInterval(timer);
			timer=setInterval(function(){
				var now=scrollTop;
				var speed=(0-now)/5;
				speed=speed>0?Math.ceil(speed):Math.floor(speed);
				if(scrollTop==0){
					clearInterval(timer);
				}
				document.documentElement.scrollTop=scrollTop+speed;
				document.body.scrollTop=scrollTop+speed;
			}, 30);
		}
	});
	$('.single-click').click(function(){
		$('.footer li').find('.single-click').removeClass('current-single-click');
		$(this).addClass('current-single-click');
	});
	/*contact us*/
	var contactArr = {'contact':"Contact Us",'tos':"Terms of Use",'priv':"Privacy Policy"};
	function showDiv(str){
		$("#poptop").show(800);
		for(var i in contactArr){
			if (i == str){
				var title=contactArr[i];
			}
		};
		$.ajax({
			url:"/common/"+str+"",
			type: "POST",
			dataType: "html",
			success: function(data) {
				$("#cont").html(data);
				$("#pop>.titleName>p").html(title);
			}
		});
	}
	function colse_pop(){
		$("#poptop").fadeOut(200);
		$("#cont").html('');
		$('.footer li').find('.single-click').removeClass('current-single-click');
	}
	$(function() {
		var height_val = $(window).height() - $("body").height();
		if (height_val > 0) $(".hover-footer").height(height_val + $(".bottom").height());
	});

</script>

