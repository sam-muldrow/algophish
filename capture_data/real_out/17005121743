		.stars-mask{vertical-align: middle;text-align: center;line-height:135px;}
		.stars-outer{display: inline-block;width:130px;height:24px;background:url(http://static.cdnpoint.com/k7x.com/Public/img/star-empty.png) no-repeat;background-size: cover;}
		.stars-inner{height:100%;background:url(http://static.cdnpoint.com/k7x.com/Public/img/star-full.png) no-repeat;background-size: cover;}
	</style>
	<script type="text/javascript">
		//Picture size mixing
		(function($) {	
			$.fn.synchysis = function(options) {
				var obj=this;
				$.fn.synchysis.defaults = {
					container: obj.selector,
					element:"",
					width:"",
					height:"",
					particular:{},
					bottom:10,
				};
				var opts = $.extend({}, $.fn.synchysis.defaults, options);

				var container_width=$(opts.container).width();
				var maxColumn=0;
				var lists=[];
				var elementIndex={};
				var elementLen=0;
				obj.init=function(){
					lists=[];
					elementIndex={};
					elementLen=$(opts.element).length;
					container_width=$(opts.container).width();
					maxColumn=obj.computeMaxColumn();
					$(opts.element).each(function(index, element) {
						coordinates=obj.coordinate(index,element);
						$(element).css({position:'absolute',top:coordinates.top,left:coordinates.left});
					});
					$(opts.container).css({height:lists.length*opts.height+opts.bottom});
				}
				obj.coordinate=function(index,element){
					var elementName=$(element).attr("data-type");
					if(elementName==undefined || elementName==''){elementName="base";}
					var compare=[1,1];
					if(elementName!="base"){
						compare=opts.particular[elementName];
					}
					if(elementIndex[elementName]===undefined){elementIndex[elementName]=0;}
					if(index>elementIndex[elementName]){index=elementIndex[elementName];}
					do{
						elementIndex[elementName]=index;
						indexRow=Math.floor(index/maxColumn);
						indexColumn=index%maxColumn;
						index++;
					}while(!obj.detection([indexRow,indexColumn],compare))
					obj.setlists([indexRow,indexColumn],compare);
					return {top:indexRow*opts.width,left:indexColumn*opts.height}
				}

				obj.detection=function(n,c){
					for(var i=0;i<c[1];i++){
						if(lists[n[0]+i]===undefined){lists[n[0]+i]=[];}
						for(var j=0;j<c[0];j++){
							if(n[1]+j>=maxColumn && j<maxColumn || lists[n[0]+i][n[1]+j] !==undefined && lists[n[0]+i][n[1]+j]!==0){	
								return false;
							}
						}
					}
					return true;
				}
				obj.setlists=function(n,c){
					for(var i=0;i<c[1];i++){
						if(lists[n[0]+i]===undefined){lists[n[0]+i]=[];}
						for(var j=0;j<c[0];j++){
							lists[n[0]+i][n[1]+j]=1;
						}
					}
					return true;
				}

				obj.computeMaxColumn=function(){
					return Math.floor(container_width/opts.width);
				}	
				obj.init();
				return obj;
			}
		})(jQuery);
		
		//AJax load page
		var total = 15;//total page num
		var error_falg = false;
		var isLoaderWaterfall = "";
		$(function() {
			urlGetGames();
			resetWidth();
			bigSmaimg();
			$("#page_nav").val(2);
			resizeInit();
			//judge yes or not load waterfall
			if (isLoaderWaterfall === '' || isLoaderWaterfall === 1) {
				$(window).scroll(function() {
					ajax_pinterest();
				});
			}			
		});
		function getBaseObj(datas){
			var prepareStr = Waterfall(datas);
			$("#container").prepend(prepareStr);
			distinctGames();
			resetWidth();
			bigSmaimg();
		}

		//get url
		function urlGetGames(){
			var id = getPer('id');
			if(id==false){
				return;
			}
			var ajaxUrl = cookielang + '/ajaxUrlGetGames/id/' + id;
			$.ajax({
				url: ajaxUrl,
				type: 'GET',
				success: function(data) {
					if(data){
						urlAjaxReturnData = data;
						getBaseObj(data);
					}
				},
			});
		}
		//get url
		function getPer(per){
			var local_url = document.location.href;
			var get = local_url.indexOf(per +"=");
			if(get == -1){
				return false;
			}
			var get_par = local_url.slice(per.length + get + 1);
			var nextPar = get_par.indexOf("&");
			if(nextPar != -1){
				get_par = get_par.slice(0, nextPar);
			}
			return get_par;
		}
		function distinctGames () {
			if(urlAjaxReturnData){
				var data1 = urlAjaxReturnData;
				var length = Object.keys(data1).length;
				$('.url-base').each(function(){
					var num = $(this).index();
					var id = $(this).attr('data-id');
					if(num >= length){
						for (var i = 0; i< length; i++) {
							if(data1[i]!=undefined && data1[i].id == $(this).attr('data-id')){
								$('.url-base').eq(num).remove();
								break;
							}
						};
					}
				})
			}
		}

		function resizeInit() {
			var nextNum = parseInt($("#page_nav").val());;
			var w_heigth = $(window).height();
			var d_heigth = $(document).height();
			while (d_heigth <= w_heigth && nextNum <= total && error_falg === false) {
				ajax_pinterest(false);
				beforeNum = nextNum;
				nextNum = parseInt($("#page_nav").val());
				if (beforeNum == nextNum) break;
			}
		}
		function ajax_pinterest(is_async) {
			var loading = $("#loading");
			var page_nav = $("#page_nav");
			var cat_now = "";
			var tag = "";
			var hot = "Array";
			var newgames = "";
			var search_name = "";
			var search_keys = "";
			var recommend_field = "";
			var w_heigth = $(window).height();
			var d_heigth = $(document).height();
			var d_scrolltop = $(document).scrollTop();
			if (d_scrolltop + w_heigth > d_heigth - 3) {
				if (loading.val() == "on") return false;
				loading.val("on");
				var nextNum = parseInt(page_nav.val());
				if(cat_now !=''){
					var ajaxHref = cookielang+"/cat/"+cat_now+"/page/"+nextNum;
				}else if(tag != ''){
					var ajaxHref = cookielang+"/tag/"+tag+"/page/"+nextNum;
				}else if(hot == 'hot') {
					var ajaxHref = cookielang+"/hot/page/"+nextNum;
				}else if(newgames != '') {
					var ajaxHref = cookielang+"/newgames/page/"+nextNum;
				}else if(search_name != '') {
					var ajaxHref = cookielang+"/search/q/"+search_keys+"/page/"+nextNum;
				}else if(recommend_field != '') {
					var ajaxHref = cookielang+"/recommend/page/"+nextNum;
				}else{
					var ajaxHref = cookielang+"/page/"+nextNum;
				}
				if (is_async == undefined) is_async = true;
				if (nextNum <= total) {
					$.ajax({
						url: ajaxHref,
						type: 'GET',
						async: is_async,
						//dataType: 'html',
						success: function(renData) {
							if (renData != 0) {
								$("#container").append(Waterfall(renData));
								$("a.changeUrl").on("mouseover mouseout",function(){
									var evt = this.event || arguments[0];
									if (evt.type == "mouseover") {
										$(".div-floadlogo-text").html($(this).attr('title'));
									};
									if (evt.type == "mouseout") {
										$(".div-floadlogo-text").html("");
									};
								});
								if(urlAjaxReturnData){
									distinctGames();
								}
								changeHref();
								page_nav.val(nextNum + 1);
								loading.val("off");
								showloading("infscr");
								bigSmaimg();
							} else {
								loading.val("off");
								showloading("end");
							}
						},
						error: function(XMLHttpRequest, textStatus, errorThrown) {
							error_falg = true;
							loading.val("off");
							showloading("error");
						}
					});
				} else {
					loading.val("off");
					showloading("end");
				}
			}
		}

		function showloading(result) {
			$("#infscr-loading").stop(true, true).hide();
			$("#error-loading").stop(true, true).hide();
			$("#end-loading").stop(true, true).hide();
			if (result == undefined) result = 'infscr';
			$('#' + result + "-loading").fadeIn();
			$('#' + result + "-loading").fadeOut(3000);
		}

		//Waterfall loading
		var urlAjaxReturnData;
		var langurl="";
		var staticPath="static.cdnpoint.com";
		var redirectMid="middle";
		var tag_page = null;
		function Waterfall(allData){
			var eveData = '';
			for (var i in allData) {
				if(allData[i]['type'] == 'ad-game'){
					eveData += "<div class='game-item bigimgtitle' data-type='big'>"+
							"<a href='"+allData[i]['gameurl'][0]+"' class='changeUrl' title='"+allData[i]['name']+"' target='_blank'>"+
															"<div class='icon bigicon'>"+
									"<img src='http://"+staticPath+"/game_img/"+allData[i]['img']+" ' class='bigimg game-frame'/>"+
									"<div class='title mask GnameBg'>"+
										"<div class='stars-mask'>"+
											"<div class='stars-outer'>"+
												"<div class='stars-inner' style='width:"+allData[i]['rate']+"%;'></div>"+
											"</div>"+
										"</div>"+
									"</div>"+
								"</div>"+
							"</a>"+
						"</div>";
				}else{
					eveData += "<div class='game-item smallimgtitle url-base' data-type='base' data-id='"+ allData[i]['id'] +"'>"+
							"<a href='"+langurl+"/"+redirectMid+"/"+ allData[i]['url']+"' class='changeUrl' title='"+allData[i]['name']+"' target='_blank'>"+
								"<div class='icon'>"+
									"<img "+"class='game-frame'"+"src='http://"+staticPath+"/game_img/"+allData[i]['img']+" '/>"+
									"<div class='title mask GnameBg'>"+
										"<div class='stars-mask'>"+
											"<div class='stars-outer'>"+
												"<div class='stars-inner' style='width:"+allData[i]['rate']+"%;'></div>"+
											"</div>"+
										"</div>"+
									"</div>";
					if (allData[i]['mark']!=0) {
						eveData += '<div class="special-mark" style="background: url(http://static.cdnpoint.com/k7x.com/Public/img/'+allData[i]['mark']+'.png) no-repeat;"></div>';
					};
					eveData += "</div>"+
							"</a>"+
						"</div>";
				}
			}
			return eveData;
		}

		//big img small img
		function bigSmaimg(){
			var synchysis=$('#container').synchysis({element:".game-item",width:145,height:145,particular:{"big":[2,2],"jsbg":"[3,2]"}});
			$(window).resize(function(){synchysis.init();});
		}
		function resetWidth(){
			 $('#container').width(Math.floor($(".game-wrap").width()/ 145) * 145); 
		}

		$(window).resize(function() {
			resetWidth();
			resizeInit();
			ChangeFloatDivPosition(0);
		});

		//float LOGO
		var FloatDiv_Ori_Top;
		var FloatDiv_Ori_Left;
		$(document).ready(function(){
			FloatDiv_Ori_Top=$('.div-floadlogo').offset().top;
			FloatDiv_Ori_Left=$('.div-floadlogo').offset().left;
			$('.div-floadlogo').mouseover(function(){
				ChangeFloatDivPosition(0);
			});	
			ChangeFloatDivPosition(1);
		});
		function ChangeFloatDivPosition(Position){
			var Width=$(window).width();
			var Height=$(window).height()-60;
			var floadlogo_width_ori=400;
			var floadlogo_height_ori=300;
			var floadlogo_width=floadlogo_width_ori;	//$('#floadlogo').width();
			var floadlogo_height=floadlogo_height_ori;	//$('#floadlogo').height();
			if(Width<3*floadlogo_width){
				floadlogo_width=Width/3;
				floadlogo_height=floadlogo_height_ori*floadlogo_width/floadlogo_width_ori;
			}
			if(Height<3*floadlogo_height){
				floadlogo_height=Height/3;
				floadlogo_width=floadlogo_width_ori*floadlogo_height/floadlogo_height_ori;
			}
			$('.div-floadlogo').width(parseInt(floadlogo_width));
			$('.div-floadlogo').height(parseInt(floadlogo_height));
			$('.div-floadlogo-image').width(parseInt(floadlogo_width));
			$('.div-floadlogo-image').height(parseInt(floadlogo_height));
			$('#floadlogo_image').width(parseInt(floadlogo_width));
			$('#floadlogo_image').height(parseInt(floadlogo_height));

			var css = {
				'width' :parseInt( 260*floadlogo_width/floadlogo_width_ori),
				'height' : parseInt(48*floadlogo_height/floadlogo_height_ori),
				'line-height' : parseInt(48*floadlogo_height/floadlogo_height_ori)+"px",
				'top' : parseInt(220*floadlogo_height/floadlogo_height_ori),
				'left': parseInt(60*floadlogo_width/floadlogo_width_ori),
				//'font-size': parseInt(14*floadlogo_width/floadlogo_width_ori)
			};
			$(".div-floadlogo-text").css(css);

			Top=FloatDiv_Ori_Top;
			Left=FloatDiv_Ori_Left;
			//if(Width<990) Width=990;
			//if(Height<500) Height=500;
			//if(Width<2*floadlogo_width||Height<2*floadlogo_height){
			if(floadlogo_width<200){
				$('.div-floadlogo').hide();
				return;
			}

			while(Top==FloatDiv_Ori_Top && Left==FloatDiv_Ori_Left){
				if(Position==1){
					Top=Height/2+(Height/2-floadlogo_height)/2;
					Left=Width/2+(Width/2-floadlogo_width)/2;
				}else{
					Top=parseInt(Math.random()+0.5)*Height/2+(Height/2-floadlogo_height)/2;
					Left= parseInt(Math.random()+0.5)*Width/2+(Width/2-floadlogo_width)/2;
				}
				if(Top<75) Top=75;
				if(Left<0) Left=0;
			}
			$('.div-floadlogo').hide();
			$('.div-floadlogo').css({"top":Top,"left":Left});
			$('.div-floadlogo').fadeIn('slow');
			////console.log("Top",Top,"Left",Left);
			/*
			$('.div-floadlogo').fadeOut(400, function() {
				//console.log("Top",Top,"Left",Left);
				$('.div-floadlogo').css({"top":Top,"left":Left});
				$('.div-floadlogo').fadeIn();
				$('#floadlogo').show();
			})
			*/
			////console.log("floatlogo:",floadlogo_width,floadlogo_height,"screen:",Width,Height,"origin:",FloatDiv_Ori_Top,FloatDiv_Ori_Left,"final:",Top,Left);

			FloatDiv_Ori_Top=Top;
			FloatDiv_Ori_Left=Left;

			if(floadlogo_width<200){
				$('.div-floadlogo').hide();
				return;
			}
		};

		$("a.changeUrl").on("mouseover mouseout",function(){
			var evt = this.event || arguments[0];
			if (evt.type == "mouseover") {
				$(".div-floadlogo-text").html($(this).attr('title'));
			};
			if (evt.type == "mouseout") {
				$(".div-floadlogo-text").html("");
			};
		});

	</script>

