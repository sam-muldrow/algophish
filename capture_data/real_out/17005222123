<script>
   (function ($) {
   //$(document).ready(function (e) {
        window.rj_podcast = window.rj_podcast || {};
        window.rj_podcast.mobbedup = {
          'title': 'Mobbed Up: The Fight for Las Vegas',
          'description': 'The true-crime series chronicles the mob’s rise and fall in Las Vegas through the eyes of those who lived it.',
          'image': 'https://res.cloudinary.com/review-journal/image/upload/f_auto,q_auto,c_scale,w_100/v1588965207/webdev/MOBBEDUPwithlogos.jpg',
          'url': '/podcasts/mobbedup/'
        };
        window.rj_podcast.vegas_nation = {
          'title': 'Vegas Nation',
          'description': 'Fast takes and smart coverage of the Las Vegas Raiders every Monday, Wednesday and Friday.',
          'image': 'https://res.cloudinary.com/review-journal/image/upload/c_scale,w_100/v1591135774/webdev/vegas-nation-new-logo.jpg',
          'url': '/podcasts/podcast-vegas-nation/'
        };
        window.rj_podcast.golden_edge = {
          'title': 'Golden Edge',
          'description': 'Ben Gotz and David Schoen provide the inside scoop on all things Golden Knights.',
          'image': 'https://res.cloudinary.com/stephens-media/image/upload/c_scale,w_100/v1574116983/rj-project/podcasts/GOLDENEDGEbluewire.jpg',
          'url': '/podcasts/podcast-golden-edge/'
        };
       
      
        custom_add();


        function custom_add() {
          var url, cover, txt, elem;
          elem = $('.rj-widget.podcast');

          $(elem).append(
             $('<div/>',{'class':'rj-widget-title'}).append($('<a/>',{'href':window.rj_podcast.domain + window.rj_podcast.podcast_url, 'text':'PODCASTS'})))
          .append(
             $('<div/>',{'class':'field-list'}))
          .append(
             $('<div/>',{'class':'field-more-small'}).append(
                $('<a/>',{'href':'/podcasts/', 'text':'MORE PODCASTS'})
             )
          );

          url = window.rj_podcast.mobbedup.url;
          cover = window.rj_podcast.mobbedup.image;
          txt = '<span>'+window.rj_podcast.mobbedup.title+'</span>&nbsp; '+window.rj_podcast.mobbedup.description;
          $(elem).find('.field-list').append($('<div/>',{'class':'rj-podcast'})
            .append($('<div/>',{'class':'field-image'})
              .append($('<a/>',{'href':url})
                .append($('<img/>',{'class':'field-img rj-lazy-load lazy-image', 'width':'100', 'height':'100', 'src':cover, 'alt':'podcast mobbedup', 'data-lazy-src':cover, 'srcset':'data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7'}))
                .append($('<div/>',{'class':'play-button pb-bot-left'}))
              )
            )
            .append($('<div/>',{'class':'field-info'})
              .append($('<div/>',{'class':'field-title'})
                .append($('<a/>',{'href':url,'html':txt}))
              )
            )
          );

          url = window.rj_podcast.vegas_nation.url;
          cover = window.rj_podcast.vegas_nation.image;
          txt = '<span>'+window.rj_podcast.vegas_nation.title+'</span>&nbsp; '+window.rj_podcast.vegas_nation.description;
          $(elem).find('.field-list').append($('<div/>',{'class':'rj-podcast'})
            .append($('<div/>',{'class':'field-image'})
              .append($('<a/>',{'href':url})
                .append($('<img/>',{'class':'field-img rj-lazy-load lazy-image', 'width':'100', 'height':'100', 'src':cover, 'alt':'podcast vegas nation', 'data-lazy-src':cover, 'srcset':'data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7'}))
                .append($('<div/>',{'class':'play-button pb-bot-left'}))
              )
            )
            .append($('<div/>',{'class':'field-info'})
              .append($('<div/>',{'class':'field-title'})
                .append($('<a/>',{'href':url,'html':txt}))
              )
            )
          );

          $(document.body).trigger('post-load', {});
        }

        function resize_image(img_url, resize_str) {
            if (img_url.indexOf('res.cloudinary.com') > -1) {
               img_url = img_url.replace('/image/upload/v', '/image/upload/'+resize_str+'/v');
            }
            return img_url;
        }

    //});
    })(jQuery);
</script>
