<script>
    var $ = jQuery;
    var current_page, current_date, current_ut, steps, steps_c, bolv_clock, bolv_tmr, np_banner;
    var q_str, dt; 
    var vb_data = {};

    setup_vertical_banner();

    function setup_vertical_banner() {
        if (window.hide_def_banner) {
            $('.vertical-banner').hide();
            return;
        }
        current_page = 'not-set';
        if (window.dataLayer && window.dataLayer[0].metrics.pageType == 'homepage') {
            current_page = 'Homepage';
        }
        if (window.dataLayer && window.dataLayer[0].metrics.pageType == 'section') {
            current_page = window.dataLayer[0].metrics.name;
        }
        var vn_gameday = ['Homepage', 'Sports', 'Raiders'];
        if (window.dataLayer &&  window.fuel_vn_gameday_live && vn_gameday.includes(current_page)) {
            $('.vertical-banner').hide();
            return;
        }

        current_date, current_ut, steps, steps_c, bolv_clock, bolv_tmr, np_banner;

        q_str = new URLSearchParams(window.location.search);
        dt = q_str.get('dt');
        if (dt) {
            current_date = new Date(dt);
        } else {
            current_date = new Date();
        }
        current_ut = current_date.getTime();

        setup_bolv_clock();

        if ($('.vertical-banner .items .item').length == 1 ) {
            return;
        }

        vb_data = {};
        vb_data.headline_duration_time = 4 * 1000; //milli second
        vb_data.headline_transition_time = 1 * 1000;
        vb_data.height = '';
        vb_data.current = 0;
        vb_data.total = 0;
        vb_data.timer_duration;
        vb_data.timer_win_resize;
        vb_data.items_height;
        vb_data.item = [];
        
        init_vertical_banner();
    }

    function init_vertical_banner() {
        var item;

        vb_data.height = $('.vertical-banner .embed-responsive-item').height();
       
        item = $('.vertical-banner .items .item').first().clone();
        $('.vertical-banner .items').append(item);
        
        vb_data.total = $('.vertical-banner .items .item').length;
        
        vb_data.current = 1;
        $('.vertical-banner').attr('data-current',vb_data.current);
        
        $('.vertical-banner .item').mouseover(function() {
            if (vb_data.timer_duration) {
                clearTimeout(vb_data.timer_duration);
            }
        });

        $('.vertical-banner .item').mouseout(function() {
            on_duration();
        });

        $(window).resize(function(e) {
            if (vb_data.timer_win_resize) {
                clearTimeout(vb_data.timer_win_resize); 
            }
            vb_data.timer_win_resize = setTimeout(on_win_resize, 200);
        });

        on_duration();
    }

    function on_win_resize() {
        on_reset();
    }

    function on_duration() {
        if (vb_data.timer_duration) {
            clearTimeout(vb_data.timer_duration);
        }

        var current_i = vb_data.current - 1;
        if ($('.vertical-banner .items .item:eq('+current_i+')').hasClass('more-time')) {
            var t = vb_data.headline_duration_time * 2.5; //(vb_data.total - 2) * (vb_data.headline_duration_time * 2.5); //71.4%
            vb_data.timer_duration = setTimeout(on_transition, t);
        } else {
            vb_data.timer_duration = setTimeout(on_transition, vb_data.headline_duration_time);
        }
    }

    function on_transition() {
        
        vb_data.current++;
        $('.vertical-banner').attr('data-current',vb_data.current);
        var pos = vb_data.height;
        pos = '-='+pos+'px';
        if (vb_data.current < vb_data.total) {
            $('.vertical-banner .items').animate({
                'top': pos,
            },vb_data.headline_transition_time, on_duration);
        } else {
            $('.vertical-banner .items').animate({
                'top': pos,
            },vb_data.headline_transition_time, on_reset);
        }

    }
    function on_reset() {
        vb_data.height = $('.vertical-banner .embed-responsive-item').height();
        
        $('.vertical-banner .items').css({'top':'0'});
        vb_data.current = 1;
        $('.vertical-banner').attr('data-current',vb_data.current);
        vb_data.total = $('.vertical-banner .items .item').length;
        on_duration();
    }

    function setup_bolv_clock() {
        
        var o = get_time_offset(current_date); //-08:00
        //console.log('rjb_offset:'+o); 

        steps = [
            {'text':'Nominations Open In', 'dt':'2023-07-31T12:00:00'+o, 'url':'https://www.bestoflasvegas.com/?utm_source=lvrj&utm_medium=countdown+clock&utm_campaign=BOLV+2023+Pre-Nom', 'ut':0},
            {'text':'', 'dt':'2023-08-17T00:00:00'+o, 'url':'#', 'ut':0},
            {'text':'Voting Begins In', 'dt':'2023-08-28T12:00:00'+o, 'url':'https://www.bestoflasvegas.com/?utm_source=lvrj&utm_medium=countdown+clock&utm_campaign=BOLV+2023+Pre-Vote', 'ut':0},
            {'text':'', 'dt':'2023-11-27T00:00:00'+o, 'url':'#', 'ut':0},
            {'text':'Winners Announced In', 'dt':'2023-12-10T00:00:00'+o, 'url':'https://www.bestoflasvegas.com/?utm_source=lvrj&utm_medium=countdown+clock&utm_campaign=BOLV+2023+Pre-Winner+Results', 'ut':0},
        ];
        steps_c = 0;
        var i;
        
        var target_dt;

        for (i = 0; i < steps.length; i++) {
            steps[i].ut = new Date(steps[i].dt).getTime();
            if (current_ut >= steps[i].ut) {
                steps_c = i+1;
            }
        }
        //console.log('rjb_step_c:'+steps_c);
        if ('' == steps[steps_c].text) {
            return; 
        }
        if (steps_c >= steps.length) {
            return; 
        }
        
        bolv_clock = get_bolv_clock();
        $('.vertical-banner .items').append(bolv_clock);

        bolv_tmr = setInterval(check_time, 1000);

    }

    function get_bolv_clock() {
        var html;
        var img_arr = [];
        img_arr[0] = {'pc':'','tablet':'', 'phone':''};
        img_arr[1] = {'pc':'','tablet':'', 'phone':''};
        img_arr[2] = {
            'pc' : 'https://res.cloudinary.com/review-journal/image/upload/f_auto,q_auto,c_scale,w_1200/v1691612721/webdev/bolv_2023_1500_1_v0.jpg',
            'tablet' : 'https://res.cloudinary.com/review-journal/image/upload/f_auto,q_auto/v1691612721/webdev/bolv_2023_768_1_v0.jpg',
            'phone' : 'https://res.cloudinary.com/review-journal/image/upload/f_auto,q_auto/v1691612721/webdev/bolv_2023_620_1_v0.jpg'
        };
        img_arr[3] = {'pc':'','tablet':'', 'phone':''};
        img_arr[4] = {
            'pc' : 'https://res.cloudinary.com/review-journal/image/upload/f_auto,q_auto,c_scale,w_1200/v1691612721/webdev/bolv_2023_1500_2_v0.jpg',
            'tablet' : 'https://res.cloudinary.com/review-journal/image/upload/f_auto,q_auto/v1691612721/webdev/bolv_2023_768_2_v0.jpg',
            'phone' : 'https://res.cloudinary.com/review-journal/image/upload/f_auto,q_auto/v1691612721/webdev/bolv_2023_620_2_v0.jpg'
        };


        bolv_clock = $('<div/>',{'class':'item bolv-banner-clock set-h'});
        html = '<a href="'+steps[steps_c].url+'" target="_blank">';
        html += '<picture>';
        html += '<source media="(min-width: 992px)" srcset="'+img_arr[steps_c]['pc']+'" />';
        html += '<source media="(min-width: 420px)" srcset="'+img_arr[steps_c]['tablet']+'" />';
        html += '<image alt="bolv" width="620" height="133" src="'+img_arr[steps_c]['phone']+'" />';
        html += '</picture>';

        //html += '<div class="f-text">'+steps[steps_c].text+'</div>';
        
        html += '<div class="f-time">';
        html += '<div class="time-elem time-day">';
        html += '<div class="time-value">&nbsp;</div>';
        html += '<div class="time-unit">DAYS</div>';
        html += '</div>';
        html += '<div class="time-elem time-hr">';
        html += '<div class="time-value">&nbsp;</div>';
        html += '<div class="time-unit">HRS</div>';
        html += '</div>';
        html += '<div class="time-elem time-min">';
        html += '<div class="time-value">&nbsp;</div>';
        html += '<div class="time-unit">MINS</div>';
        html += '</div>';
        html += '<div class="time-elem time-sec">';
        html += '<div class="time-value">&nbsp;</div>';
        html += '<div class="time-unit">SEC</div>';
        html += '</div>';
        html += '</div>';
        html += '<img class="v-logo skip-lazy" alt="naqvi" src="https://res.cloudinary.com/review-journal/image/upload/f_auto,q_auto,c_scale,w_300/v1691612721/webdev/naqvi_logo.png"><noscript><img class="v-logo skip-lazy" alt="naqvi" src="https://res.cloudinary.com/review-journal/image/upload/f_auto,q_auto,c_scale,w_300/v1691612721/webdev/naqvi_logo.png" /></noscript>';
        html += '</a>';

        bolv_clock.append(html);
        return bolv_clock;
        
    }

    function check_time() {
        current_ut = current_ut + 1000;
        if ('' == steps[steps_c].text) {
            bolv_clock.remove();
            clearInterval(bolv_tmr);
            on_reset();
            return;
        }

        if (current_ut < steps[steps_c].ut) {
            var date_dist = steps[steps_c].ut - current_ut;

            var d = Math.floor(date_dist / (1000 * 60 * 60 * 24));
            var h = Math.floor((date_dist % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
            var m = Math.floor((date_dist % (1000 * 60 * 60)) / (1000 * 60));
            var s = Math.floor((date_dist % (1000 * 60)) / 1000);
            if (s < 10) {s = '0'+s;}
            if (m < 10) {m = '0'+m;}
            if (h < 10) {h = '0'+h;}
            if (d < 10) {d = '0'+d;}

            bolv_clock.find('.time-day .time-value').text(d);
            bolv_clock.find('.time-hr .time-value').text(h);
            bolv_clock.find('.time-min .time-value').text(m);
            bolv_clock.find('.time-sec .time-value').text(s);
            
            return;
        }
        steps_c++;
        //console.log('rjb_step_c:'+steps_c);
        if (steps_c >= steps.length) {
            //console.log('rjb_remove bolv clock');
            bolv_clock.remove();
            clearInterval(bolv_tmr);
            on_reset();
            return;
        }
    }

    function get_date_Ymd_His() {
        var y = current_date.getFullYear();
        var m = current_date.getMonth();
        var d = current_date.getDate();
        var h = current_date.getHours();
        var i = current_date.getMinutes();
        var s = current_date.getSeconds();
        
        m++;
        m = add_pre_zero(m);
        d = add_pre_zero(d);
        h = add_pre_zero(h);
        i = add_pre_zero(i);
        s = add_pre_zero(s);
        return y+'-'+m+'-'+d+'_'+h+':'+i+':'+s; 
    }

    function add_pre_zero(n) {
        if (n < 10) {
            n = '0' + n;
        } 
        return n;
    }

    function get_time_offset(d) {
        var t = d.getTimezoneOffset(); //480
        var s = '-';
        if (t < 0) {
            s = '+';
        }
        t = Math.abs(t);
        var h = Math.floor(t / 60);
        var m = t % 60;
        h = h.toString();
        m = m.toString();
        if (h.length < 2) {
            h = '0'+h;
        }
        if (m.length < 2) {
            m = '0'+m;
        }
        var o = s+h+':'+m
        return o;
    }
</script></div>
		</div>				</div>
