<script>
(function ($) {
//$(document).ready(function (e) {
    var page_path = window.location.pathname; // hard to detect /sports/rodeo/ or /rodeo/
    var page_type = '';
    var page_name = '';
    var page_type_name = '';

    var prefid = '';
    var alert_id = '';
    var title = '';
    var subtitle = '';

    if (window.dataLayer[0].metrics) {
        if (window.dataLayer[0].metrics.pageType) {
            page_type = window.dataLayer[0].metrics.pageType;
        }
        if (window.dataLayer[0].metrics.name) {
            page_name = window.dataLayer[0].metrics.name;
        }
       
    }

    if ('page' == page_type) {
        page_name = page_path;
    }

    page_type_name = page_type + '_' + page_name;
    

    switch(page_type_name) {
        case 'section_Sports':
            prefid = 'sports';
            title = 'NEWSLETTER SIGN UP';
            subtitle = 'Your Daily Update on the Latest Scores, Stats and More.';
        break;

        /*
        case 'section_Entertainment':
            prefid = 'entertainment';
            title = 'NEWSLETTER SIGN UP';
            subtitle = 'Keep up with what\'s happening in Vegas!';
        break;
        */
        case 'section_Business':
            prefid = 'business';
            title = 'NEWSLETTER SIGN UP';
            subtitle = 'Your Daily Briefing from the Financial Front.';
        break;

        case 'section_Golden Knights':
            prefid = '';
            alert_id = 'golden_knights';
            title = 'GOLDEN KNIGHTS EMAIL ALERTS';
            subtitle = 'Your source for the latest news about the Vegas Golden Knights.';
        break;

        case 'section_Raiders News':
            prefid = 'vegasnation';
            title = 'VEGAS NATION NEWSLETTER';
            subtitle = 'Your source for the latest news about the Oakland Raiders.';
        break;

        default:
            prefid = 'starting_point,pm_update';
            title = '<a href="/email-alerts">NEWSLETTER SIGN UP</a>';
            subtitle = 'Quality journalism sent to your inbox <br> each morning and afternoon.';
        break;

    }

    if (!prefid && !alert_id) {
        return;
    }
    var html = '';

    html += '<div class="rj-newsletter-signup nlsm-widget rj-gutter-mobile" data-prefid="'+prefid+'" data-alert-id="'+alert_id+'">';
    html += '<div class="rj-widget-title">'+title+'</div>';
    html += '<div class="field-sub-title">'+subtitle+'</div>';
