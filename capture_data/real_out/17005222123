					<div id="rjth-region-footer-row-3" class="widget-area rj-container footer_copyright">
				<div id="rj_widget_text-753" class="rj-widget-num-1 rj-widget-first rj-widget-odd  rj-widget rj_widget_text">		<div class="textwidget"><script>
    var i;
    var lazy_elem;
    var lazy_elems = document.querySelectorAll('div.rj-lazy-script');
    var lazy_elems_count = lazy_elems.length;
    var observer;

    if (!( 'IntersectionObserver' in window )) {
        for (i = 0; i < lazy_elems.length; i++) {
            lazy_elem = lazy_elems[i];
            load_lazy_script(lazy_elem);
        }
    } else {
        
        observer = new IntersectionObserver(onIntersection, {'rootMargin':'200px 0px', 'threshold': 0.01});

        for (i = 0; i < lazy_elems.length; i++) {
            lazy_elem = lazy_elems[i];
            if (lazy_elem.classList.contains('rj-lazy-load--handled')) {
                continue;
            }
            observer.observe(lazy_elem);
        }
    }

    function onIntersection(entries) {
        var i, entry, src;
        
        if (lazy_elems_count === 0) {
            observer.disconnect();
        }

        // Loop through the entries
        for (i = 0; i < entries.length; i++) {

            entry = entries[i];

            // Are we in viewport?
            if (entry.intersectionRatio > 0) {
                lazy_elems_count--;

                // Stop watching and load the image
                observer.unobserve(entry.target);

                entry.target.classList.add('rj-lazy-load--handled');
                load_lazy_script(entry);
            }
        }
    }

    function load_lazy_script(entry) {
        var src = entry.target.getAttribute('data-lazy-src');

        var lazy_sc =  document.createElement('script');
        lazy_sc.type = 'text/javascript';
        lazy_sc.src = src;
        lazy_sc.async = true;

        if (entry.target.classList.contains('rj-lazy-stn')) {
            lazy_sc.setAttribute('data-type', 's2nScript'); 
        }
        
        if (entry.target.classList.contains('rj-lazy-flipp')) {
            lazy_sc.onload = function() {
                window.flippxp = window.flippxp || {run: []};
                window.flippxp.run.push(function() {
                   window.flippxp.registerSlot("#flipp-ux-slot-83hdf2", "LasVegasJournalReview", 1252968, [ 292025 ]);
                });
            };
        }

        entry.target.parentNode.insertBefore(lazy_sc, entry.target.nextSibling);
    }
</script></div>
