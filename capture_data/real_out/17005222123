		</div><div id="rj_widget_text-632" class="rj-widget-num-3 rj-widget-odd  rj-widget rj_widget_text">		<div class="textwidget"><script>
(function ($) {
var fuel_arr;
var fid = '';
var fid_77 = 0;
var fid_vn_blitz = 1;
var fid_vn_gameday = 2;
var is_prepend = true;

$(document).on('rj_fuel_ready', function(e) {
    fuel_arr = window.fuel_arr;
    if ($('.rj-fuel-77.custom-field').length >= 1) {
        return;
    }
    set_up_fuel();
});
    

function set_up_fuel() {
    
    var current_page = 'not-set';
    if (window.dataLayer[0].metrics.pageType == 'homepage') {
        current_page = 'Homepage';
    }
    if (window.dataLayer[0].metrics.pageType == 'section') {
        current_page = window.dataLayer[0].metrics.name;
    }

    if (window.fuel_is_vn_blitz_enable) {
        if (current_page == 'Raiders News' || current_page == 'Sports' ) {
            fid = fid_vn_blitz;
        }
    }
    
    if (window.fuel_is_vn_gameday_enable) {
        fid = fid_vn_gameday;
    }

    //77 has high priority
    if (window.fuel_is_77_enable) {
        if (current_page != 'Raiders News' && current_page != 'Sports' ) {
            fid = fid_77;
        }
    }

    if (fid === '') {
        return;
    }
    
    move_story_to_latest();
       
    var click_url = fuel_arr[fid].url;
    switch(fid) {
        case 0:
            click_url = click_url + '?utm_campaign=7at7&utm_medium=topnews_widget&utm_source='+current_page;
            break;

        case 1:
            click_url = click_url + '?utm_campaign=vn_blitz&utm_medium=topnews_widget&utm_source='+current_page;
            break;

        case 2:
            click_url = click_url + '?utm_campaign=vn_gameday&utm_medium=topnews_widget&utm_source='+current_page;
            break;
    }

    var ww = $(window).width();
    var pos = 'mobile';
    if (ww >= 768) {
        pos = 'pc';
    }
    var cls = 'rj-fuel-77';
    if (!is_prepend) {
        cls += ' last-child';
    }

    var fuel = 
    $('<div/>',{'class':cls,'data-pos':pos})
    .append(
        $('<div/>',{'class':'rj-fuel rj-lazy-fuel embed-responsive embed-responsive-16by9'})
        .append($('<img/>',{'class':'rj-fuel-thumb','width':'400','height':'225','alt':window.fuel_arr[fid].name,'src':window.fuel_arr[fid].thumbnail}))
        .append($('<div/>',{'class':'rj-fuel-play-icon'}).append($('<i/>',{'class':'fa fa-youtube-play', 'aria-hidden':'true'})))
    ).append(
        $('<div/>',{'class':'f-title'})
        .append(
            $('<a/>',{'href':click_url, 'alt':'7at7', 'text':fuel_arr[fid].title})
        )
    ).append(
        $('<div/>',{'class':'f-desc','html':fuel_arr[fid].description})
    );

    if (is_prepend) {
        if ($('.rj-widget.top-news .field-col.left .special-article').length == 1) {
            fuel.insertAfter('.special-article');
        } else {
            fuel.prependTo('.rj-widget.top-news .field-col.left');
        }
    } else {
        fuel.appendTo('.rj-widget.top-news .field-col.left');
    }

    $('.rj-lazy-fuel').click(function(e) {
        if ($(this).hasClass('rj-lazy-load--handled')) {
            return;
        }

        $(this).addClass('rj-lazy-load--handled');

        if (window.fuel_arr[fid].youtube_vdo_id) {
            $('.rj-lazy-fuel').html('').append($('<iframe/>',{'src':'https://www.youtube.com/embed/'+window.fuel_arr[fid].youtube_vdo_id+'?rel=0&autoplay=1','frameborder':'0','allow':'autoplay;','allowfullscreen':''}));
        } else {

            var sc = document.getElementById('fuel-player-script');
            if(!sc) {
                sc = document.createElement('script');
                sc.type = 'text/javascript';
                sc.src = window.fuel_sc;
                sc.async = true;
                sc.setAttribute('id', 'fuel-player-script');
                sc.onload = function() {
                    $('.rj-lazy-fuel').html('').attr({'itemscope':'','itemprop':'VideoObject','itemtype':'https://schema.org/VideoObject'})
                    .append($('<meta/>',{'itemprop':'description','content':window.fuel_arr[fid].description}))
                    .append($('<meta/>',{'itemprop':'name','content':window.fuel_arr[fid].name}))
                    .append($('<meta/>',{'itemprop':'thumbnailUrl','content':window.fuel_arr[fid].thumbnail}))
                    .append($('<meta/>',{'itemprop':'uploadDate','content':'2021-01-18T00:00:00+00:00'}))
                    .append($('<meta/>',{'itemprop':'contentUrl','content':'https://fuel-streaming-prod01.fuelmedia.io/v1/sem/'+window.fuel_arr[fid].channel_id+'.m3u8'}))
                    .append($('<fuel-video/>',{
                        'data-autoplay':true, 'data-channel':window.fuel_arr[fid].channel_id, 'data-poster-image':window.fuel_arr[fid].thumbnail
                    })); 
                };
                document.head.appendChild(sc); 
            }
        }

        if ('undefined' !== typeof s2nVideo) {
            try {
                s2nVideo.resetPlayer();
            } catch (err) {
                console.log(err.message);
            }
        }

       
    });

}

function move_story_to_latest() {
    if ($('.latest-news').length < 1) {
        return;
    }
    var elem, tmp, limit;
    limit = 3;
    if ($('.rj-widget.top-news').hasClass('top-news-1-4')) {
        limit = 4;
    }
    if ($('.rj-widget.top-news .field-col.left .rj-story').length < limit) {
        return;
    }
    elem = $('.rj-widget.top-news .field-col.left .rj-story:last');
    
    if (!elem) {
        return;
    }
    
    elem.addClass('rj-thumb-list has-image');
    elem.wrapInner($('<div/>',{'class':'field-col right'}));
    elem.prepend($('<div/>',{'class':'field-col left'}));
    tmp = elem.find('.field-col.left');
    elem.find('.field-image').appendTo(tmp);
    elem.insertBefore('.latest-news .rj-story-latest-news-wrap:eq(0)').wrap($('<div/>',{'class':'rj-story-latest-news-wrap'}));
}

})(jQuery);
</script></div>
		</div><div id="rj_widget_text-731" class="rj-widget-num-4 rj-widget-last rj-widget-even default-banner rj-widget rj_widget_text">		<div class="textwidget"><style>
