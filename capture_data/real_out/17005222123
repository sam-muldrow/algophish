//https://fuel-streaming-prod01.fuelmedia.io/player/1.0/player.min.js';
window.fuel_sc = 'https://fuel-streaming-prod01.fuelmedia.io/player/v3/fuel.js'; 

window.bn_story_live = false;
window.fuel_date_obj = get_date_obj();
window.fuel_cdt = get_current_dt();
window.fuel_is_77_enable = is_77_enable();
window.fuel_is_vn_blitz_enable = is_vn_blitz_enable();
window.fuel_is_vn_gameday_enable = false //time vary each week have to update google sheet
window.fuel_vn_gameday_live = false;

//console.log('rj_load_fuel_setting:'+ window.fuel_cdt +'_day:'+fuel_cday);
load_fuel_setting();


function get_date_obj() {
    var q_str = new URLSearchParams(window.location.search);
    var dt = q_str.get('dt');
    var cdate;
    if (dt) {
        cdate = new Date(dt);
    } else {
        cdate = new Date();
    }
    return cdate;
}

function get_current_dt() {
    var cdate = window.fuel_date_obj;
    //d.toLocaleString(); //"2/1/2013 7:37:08 AM"
    var y = cdate.getFullYear();
    var m = cdate.getMonth();
    var d = cdate.getDate();
    var h = cdate.getHours();
    var i = cdate.getMinutes();
    var s = cdate.getSeconds();
    window.fuel_cday  = cdate.getDay(); //0-6 | Sun-Sat
    window.fuel_chour  = h;
    window.fuel_cminute  = m;
    
    m++;
    m = add_pre_zero(m);
    d = add_pre_zero(d);
    h = add_pre_zero(h);
    i = add_pre_zero(i);
    s = add_pre_zero(s);
    return y+'-'+m+'-'+d+'_'+h+':'+i+':'+s; 
}
function add_pre_zero(n) {
    if (n < 10) {
        n = '0' + n;
    } 
    return n;
}
function resize_image(img_url, resize_str) {
    if (img_url.indexOf('res.cloudinary.com') > -1) {
        img_url = img_url.replace('/image/upload/v', '/image/upload/'+resize_str+'/v');
    }
    return img_url;
}//resize_image

function load_fuel_setting() {
    var i;  
    var _setting;
    var fid;
   
    var u = '/wp-json/rj/v2/api?name=spreadsheetsv4&end_point=/1jVoUBFY2Xpt9g_eSOhoUipSA_OOh7hMbPDYAqYWx3nI/values/sheet1&param=alt%3Djson';
    //console.log('rj_u:'+u);
    $.ajax({
      type: 'GET',
      url: u,
      cache: true,
      dataType: 'json',
      timeout: 5000,
      success: function (response) {
        if ( response.status == 1 && response.response && response.response.values  ) { //response.status == 1 && response.response //response.feed
            _setting = response.response.values; //response.response.feed.entry; //response.feed.entry; //response.response.resp
            for (i=1; i<_setting.length; i++ ) {
                if (!window.fuel_arr[i-1]) {
                    continue;
                }
                window.fuel_arr[i-1].thumbnail = resize_image(_setting[i][1], 'f_auto,q_auto,c_scale,w_400'); //response.feed.entry[2]['gsx$imageurl']['$t'];
                window.fuel_arr[i-1].title = _setting[i][2]; //response.feed.entry[2]['gsx$title']['$t'];
                window.fuel_arr[i-1].description = _setting[i][3]; //response.feed.entry[2]['gsx$description']['$t'];

                if (_setting[i][4]) {
                    window.fuel_arr[i-1].start = _setting[i][4];  
                }
                if (_setting[i][5]) {
                    window.fuel_arr[i-1].end = _setting[i][5];
                } 
                if ( i==10 && _setting[i][6]) {
                    window.fuel_arr[i-1].url = _setting[i][6];
                }   
            }

            //check vn_gameday
            fid = 2;
            if (fuel_arr[fid].start && fuel_arr[fid].end) {
                if (window.fuel_cdt >= fuel_arr[fid].start && window.fuel_cdt <= fuel_arr[fid].end) {
                    window.fuel_is_vn_gameday_enable  = true;
                }
            }

            //check vn_gameday_live
            fid = 3;
            if (fuel_arr[fid].start && fuel_arr[fid].end) {
                if (window.fuel_cdt >= fuel_arr[fid].start && window.fuel_cdt <= fuel_arr[fid].end) {
                    window.fuel_vn_gameday_live = true;
                }
            }

            //check bn_story
            fid = 9;
            if (fuel_arr[fid].start && fuel_arr[fid].end) {
                if (window.fuel_cdt >= fuel_arr[fid].start && window.fuel_cdt <= fuel_arr[fid].end) {
                    window.bn_story_live = true;
                }
            }

            var try_max = 100, try_count = 0;
            var try_timer = setInterval(function () {
                try_count++;
                if (try_count < try_max) {
                    if ( $('.rj-widget.top-news').length == 1 ) {
                        clearInterval(try_timer);
                        window.fuel_arr[0].ready = true;
                        $(document).trigger('rj_fuel_ready');
                        if ($('.rj-fuel-77.custom-field').length == 1) {
                            set_up_77_custom_field();
                        }
                    }
                } else {
                    clearInterval(try_timer);
                    $('.rj-fuel-77.custom-field').remove();
                }
            }, 200);


        } else {
            $('.rj-fuel-77.custom-field').remove();
        }
        
      },
      error: function (xhr, ajaxOptions, thrownError) {
        console.log('_xhr.status:' + xhr.status + '_error:' + thrownError);
        $('.rj-fuel-77.custom-field').remove();
      }
    });
}

function is_77_enable() {
    //Mon 07:00:00 to Sat 07:00:00

    //var tz = c.getTimezoneOffset(); //420 or 480
    var day = window.fuel_cday; //0-6 | Sun-Sat
    var h = window.fuel_chour;//0-23
    var i = window.fuel_cminute;
   
    if (day == 1 && h >= 7) { //monday after 07:00:00
        return true;
    }
    if (day > 1 && day < 6) { //tue - fri
        return true;
    }
    if (day == 6 && h < 7) { //sat before 07:00:00
        return true;
    }
    return false;
}

function is_vn_blitz_enable() {
    //Wed 17:00:00 to Sat 06:00:00  
    var day = window.fuel_cday; //0-6 | Sun-Sat
    var h = window.fuel_chour;//0-23
    var i = window.fuel_cminute;
    var cdt = window.fuel_cdt;

    if (cdt >= '2023-11-29_00:00:00' && cdt <= '2023-12-03_00:00:00') {
        return false;
    }
    if (cdt >= '2023-12-13_07:00:00' && cdt <= '2023-12-16_07:00:00') {
        if (cdt <= '2023-12-14_06:00:00') {
            return true;
        } 
        return false;
    }

    if (day == 3 && h >= 17) { //wed after 17:00:00
        return true;
    }
    if (day == 4 || day == 5) {
        return true;
    }
       
    if (day == 6 && h < 6) { //sat before 06:00:00
        return true;
    }
    return false;
}

function set_up_77_custom_field() {
    var fid_vn_blitz = 1;
    var fid_vn_gameday = 2;
    
    var elem = $('.rj-fuel-77.custom-field');
    
    var fid = elem.attr('data-fid');
    fid = parseInt(fid);
    if (isNaN(fid)) {
        fid = 0;
    }

    if ('1' == elem.attr('data-vn-blitz-overwrite') && window.fuel_is_vn_blitz_enable ) {
        fid = fid_vn_blitz;
    }
    if ('1' == elem.attr('data-vn-gameday-overwrite') && window.fuel_is_vn_gameday_enable) {
        fid = fid_vn_gameday;
    }
    
    var current_page = 'not-set';
   
    if (window.dataLayer[0].metrics.pageType == 'homepage') {
        current_page = 'Homepage';
    }
    if (window.dataLayer[0].metrics.pageType == 'section') {
        current_page = window.dataLayer[0].metrics.name;
    }
    
    var click_url = fuel_arr[fid].url;
    switch(fid) {
        case 0:
            click_url = click_url + '?utm_campaign=7at7&utm_medium=topnews_widget&utm_source='+current_page;
            break;

        case 1:
            click_url = click_url + '?utm_campaign=vn_blitz&utm_medium=topnews_widget&utm_source='+current_page;
            break;

        case 2:
            click_url = click_url + '?utm_campaign=vn_gameday&utm_medium=topnews_widget&utm_source='+current_page;
            break;
    }
    
    $('.rj-fuel-77.custom-field .f-title').html('')
    .append(
        $('<a/>',{'href':click_url, 'alt':'7at7', 'text':window.fuel_arr[fid].title})
    );

    $('.rj-fuel-77.custom-field .f-desc').html(window.fuel_arr[fid].description);


    $('.rj-lazy-fuel')
    .append($('<img/>',{'class':'rj-fuel-thumb','width':'400','height':'225','alt':window.fuel_arr[fid].name,'src':window.fuel_arr[fid].thumbnail}))
    .append($('<div/>',{'class':'rj-fuel-play-icon'}).append($('<i/>',{'class':'fa fa-youtube-play', 'aria-hidden':'true'}))); 
    
    $('.rj-lazy-fuel').click(function(e) {
        if ($(this).hasClass('rj-lazy-load--handled')) {
            return;
        }

        $(this).addClass('rj-lazy-load--handled');
        
        if (window.fuel_arr[fid].youtube_vdo_id) {
            $('.rj-lazy-fuel').html('').append($('<iframe/>',{'src':'https://www.youtube.com/embed/'+window.fuel_arr[fid].youtube_vdo_id+'?rel=0&autoplay=1','frameborder':'0','allow':'autoplay;','allowfullscreen':''}));
        } else {

            var sc = document.getElementById('fuel-player-script');
            if(!sc) {
                sc = document.createElement('script');
                sc.type = 'text/javascript';
                sc.src = window.fuel_sc;
                sc.async = true;
                sc.setAttribute('id', 'fuel-player-script');
                sc.onload = function() {
                    $('.rj-lazy-fuel').html('').attr({'itemscope':'','itemprop':'VideoObject','itemtype':'https://schema.org/VideoObject'})
                    .append($('<meta/>',{'itemprop':'description','content':window.fuel_arr[fid].description}))
                    .append($('<meta/>',{'itemprop':'name','content':window.fuel_arr[fid].name}))
                    .append($('<meta/>',{'itemprop':'thumbnailUrl','content':window.fuel_arr[fid].thumbnail}))
                    .append($('<meta/>',{'itemprop':'uploadDate','content':'2021-01-18T00:00:00+00:00'}))
                    .append($('<meta/>',{'itemprop':'contentUrl','content':'https://fuel-streaming-prod01.fuelmedia.io/v1/sem/'+window.fuel_arr[fid].channel_id+'.m3u8'}))
                    .append($('<fuel-video/>',{
                        'data-autoplay':true, 'data-channel':window.fuel_arr[fid].channel_id, 'data-poster-image':window.fuel_arr[fid].thumbnail
                    })); 
                };
                document.head.appendChild(sc); 
            }

            
        }

        if ('undefined' !== typeof s2nVideo) {
            try {
                s2nVideo.resetPlayer();
            } catch (err) {
                console.log(err.message);
            }
        }

       
    });

}

})(jQuery);
</script>
