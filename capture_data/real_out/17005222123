(function ($) {
//$(document).ready(function (e) {
    var resp, url, story;
    var confirm_age = get_save_data('rj_confirm_age');
    confirm_age = parseInt(confirm_age);
    if (isNaN(confirm_age)) {
        confirm_age = 0;
    }
    console.log('rj_confirm_age:'+confirm_age);

    $('.confirm-age-18 a').click(function(e) {
        if (confirm_age < 18) {
            resp = confirm('Are you over 18?');
            if (resp) {
                set_save_data('rj_confirm_age', 18);
            } else {
                e.preventDefault();
            }
        }
    });

    $('.rj-story-thumb-list a').click(function(e) {
        story = $(this).closest('.rj-story-thumb-list');
        if (story.hasClass('sponsored_category-adult-health')
            || story.hasClass('sponsored_category-casinos')
            || story.hasClass('sponsored_category-dating')
            || story.hasClass('sponsored_category-supplements')
            //|| story.hasClass('sponsored_category-pets')
        ) {
            if (confirm_age < 18) {
                resp = confirm('Are you over 18');
                if (resp) {
                    set_save_data('rj_confirm_age', 18);
                } else {
                    e.preventDefault();
                }
            }
        }
    });

    if ($('.rj-story-full').length == 1) {
        story = $('.rj-story-full');
        if (story.hasClass('sponsored_category-adult-health')
            || story.hasClass('sponsored_category-casinos')
            || story.hasClass('sponsored_category-dating')
            || story.hasClass('sponsored_category-supplements')
            //|| story.hasClass('sponsored_category-pets')
        ) {
            if (confirm_age < 18) {
                resp = confirm('Are you over 18');
                if (resp) {
                    set_save_data('rj_confirm_age', 18);
                } else {
                    //url = $('.breadcrumbs li:last-child a').attr('href');
                    window.location.href = '/bp';
                }
            }
        }
    }

    $('.rj-story-thumb-list.tag-confirm-age-21 a').click(function(e) {
        if (confirm_age < 21) {
            resp = confirm('Are you over 21');
            if (resp) {
                set_save_data('rj_confirm_age', 21);
            } else {
                e.preventDefault();
            }
        }
    });

    if ($('.rj-story-full.tag-confirm-age-21').length == 1) {
        if (confirm_age < 21) {
            resp = confirm('Are you over 21');
            if (resp) {
                set_save_data('rj_confirm_age', 21);
            } else {
                url = $('.breadcrumbs li:last-child a').attr('href');
                window.location.href = url;
            }
        }
    }

    function get_save_data(cname) {
        var v;
        //v = sessionStorage.getItem(cname);
        v = getCookie(cname);
        return v;
    }

    function set_save_data(cname, cvalue) {
        //sessionStorage.setItem(cname, cvalue);
        setCookie(cname, cvalue, 7); //7days
    }

    function setCookie(cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays*24*60*60*1000));
        var expires = 'expires='+ d.toUTCString();
        document.cookie = cname + '=' + cvalue + ';' + expires + ';path=/';
    }

    function getCookie(cname) {
        var name = cname + '=';
        var decodedCookie = decodeURIComponent(document.cookie);
        var ca = decodedCookie.split(';');
        for(var i = 0; i <ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return '';
    }
     
//});
})(jQuery);
</script></div>
