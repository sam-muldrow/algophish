<style>.grecaptcha-badge { visibility: hidden; }</style>

<script>
(function ($) {
$(document).ready(function (e) {
    var user_token_n = '';
    var user_token_a = '';

    $('.nlsm-widget .user-email').one('focus', function() {
        console.log('rj-load-gcaptcha');
        var head = document.getElementsByTagName('head')[0]
        var script = document.createElement('script')
        script.type = 'text/javascript';
        script.src = 'https://www.google.com/recaptcha/api.js?render=6LfbFwknAAAAAOYHjyIUb8rynrIx-LJmJFjz3Jhe'
        head.appendChild(script);
    });

    $('.nlsm-widget .btn-signup').click(function(e) {
        e.preventDefault();
        var nwrapper = $(this).parent().parent();
        if (nwrapper.length === 0) {
            return;
        }
        var pref_id = nwrapper.attr('data-prefid');
        var alert_id = nwrapper.attr('data-alert-id');
        var user_email = nwrapper.find('.user-email').val();
        var err_elem = nwrapper.find('.err-msg');
        if (!pref_id && !alert_id) {
            err_elem.text('Missing settings!');
            return;
        }
       
        if (!validateEmail(user_email)) {
            err_elem.text('Invalid Email address!');
            return;
        }
        
        grecaptcha.ready(function() {
            if (pref_id) {
                pref_id = pref_id.split(',');
                grecaptcha.execute('6LfbFwknAAAAAOYHjyIUb8rynrIx-LJmJFjz3Jhe', {'action': 'rj_newsletters_widget_n'}).then(function(token) {
                    //console.log('rj_urser_token_n:'+token);
                    user_token_n = token;

                    set_email_pref_sf(user_email, nwrapper, pref_id, err_elem); 
                });

            }
            if (alert_id) {
                alert_id = alert_id.split(',');
                grecaptcha.execute('6LfbFwknAAAAAOYHjyIUb8rynrIx-LJmJFjz3Jhe', {'action': 'rj_newsletters_widget_a'}).then(function(token) {
                    //console.log('rj_urser_token_a:'+token);
                    user_token_a = token;

                    set_alerts_sf(user_email, nwrapper, alert_id, err_elem); 
                });
            }
        });  
                       
    });

   
    function set_email_pref_sf(user_email, nwrapper, sub_ids, err_elem) { 
        var dt = new Date().getTime();

        var param = 'email='+user_email;
        param += '&user_token='+user_token_n;
        param += '&sub_ids='+sub_ids.join(',');
        
        var url;
        url = '/wp-json/rj/v2/api?name=rjnewsletters';
        url += '&end_point=/api/salesforce/upsert_newsletters_subscriber';
        url += '&param='+encodeURIComponent(param);
        url += '&dt='+dt;
        
        //url = 'https://rjnewsletters.reviewjournal.com/api/salesforce/upsert_newsletters_subscriber?'+param;
        err_elem.text('Processing...');
        $.ajax({
            'method':'GET',
            'dataType': 'json',
            'cache': false,
            'url': url,
            'success': function (response) {
                console.log(response);
                if (response.status == '1' && response.response.data) {
                    err_elem.text('Thank you.');
                    console.log('rj_sf:'+sub_ids.join(','));
                    mather_event_complete(user_email, nwrapper, sub_ids);    
                } 
            },
            'error': function (xhr, ajaxOptions, thrownError) {
                console.log('_xhr.status:' + xhr.status + '_error:' + thrownError);
                //$('.newsletter-page .err-msg').text('Oops something went wrong, please try again later');
            }
        });
    }

    function set_alerts_sf(user_email, nwrapper, sub_ids, err_elem) {
        var dt = new Date().getTime();

        var param = 'email='+user_email;
        param += '&user_token='+user_token_a;
        param += '&sub_ids='+sub_ids.join(',');
        
        var url;
        url = '/wp-json/rj/v2/api?name=rjnewsletters';
        url += '&end_point=/api/salesforce/upsert_news_alert_subscriber';
        url += '&param='+encodeURIComponent(param);
        url += '&dt='+dt;
        //url = 'https://rjnewsletters.reviewjournal.com/api/salesforce/upsert_news_alert_subscriber?'+param;
        
        err_elem.text('Processing...');
        $.ajax({
            'method':'GET',
            'dataType': 'json',
            'cache': false,
            'url': url,
            'success': function (response) {
                console.log(response);
                if (response.status == '1' && response.response.data) {
                    err_elem.text('Thank you');   
                }
            },
            'error': function (xhr, ajaxOptions, thrownError) {
                console.log('_xhr.status:' + xhr.status + '_error:' + thrownError);
                //$('.newsletter-page .err-msg').text('Oops something went wrong, please try again later');
            }
        });
    }

    function mather_event_complete(user_email, nwrapper, pref_id) {
        var title = nwrapper.find('.rj-widget-title').text();
        var offers = [];
        for (var i=0; i<pref_id.length; i++) {
           offers.push({"offerName":title, "offerId": pref_id[i], "campaign": "News Blast"});
        }

        var event_obj = {"action": "signup_completed", "offers": offers};
        var options = {"email": user_email};
        mather_send_event(event_obj, options);
    }

    function mather_send_event(event_obj, options) {
        console.log('matherq');
        var m = ['newsletterEvent', event_obj, options];
        console.log(m);
        (window._matherq = window._matherq || []).push(m);
    }
});
})(jQuery);
</script>
