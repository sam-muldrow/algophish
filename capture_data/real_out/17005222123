	<!-- begin region topsponsor -->
			<div id="rjth-region-topsponsor">
			<div class="topsponsor-container">
				<div id="rj_widget_text-75" class="rj-widget-num-1 rj-widget-first rj-widget-last rj-widget-odd  rj-widget rj_widget_text">		<div class="textwidget"><div class="summit-media" id="ad_summit" data-offset-mobile="0" data-offset-tablet="40" data-offset-pc="40"></div>
<style>
body.rj-summit-ready #paid-post-rail.paid-post-rail-fixed {
    position: relative;
    margin-bottom: 0;
}
body.rj-summit-ready .top-rail-default .top-rail-separator {
    height: 0 !important;
}
#ad_summit {width:100%; height:100%;}
#ad_summit > div {width:100%; height:100%;}
#ad_summit > div > iframe {position: relative; left: 50%; transform: translate(-50%,0%); -webkit-transform: translate(-50%,0%); -ms-transform: translate(-50%,0%);}
.summit-media .vdo-player .jw-wrapper {background-color:#fff;}
</style>
<script>
var summit_doc = document;
var summit_window = window;
var open_time = 3*1000;
var lock_time = 2*1000;
var page_id = 'page';
var region_id = 'rjth-region-topsponsor';
var region_container_class = 'topsponsor-container';
summit_window.summit_ads = {};
var player_cont = 'vdo-player';
   
var jwvdo_cont = '';
var vdo_preview = '';
var  $ = summit_window.jQuery;
var timer_check;

function summit_exec() {
    console.log('rj_summit_exec');
    var summit_info = $('#ad_summit iframe').contents().find('#summit_info');
    var summit_type = summit_info.attr('data-type');
    switch(summit_type) {
        case 'jw':
            render_jw(summit_info);
        break;
    }
}
function render_jw(summit_info) {
    var player_id = 'wWWqYven' //prod = wWWqYven, dev = 3Lqr8DUD (with control)

    var vdo_id = summit_info.find('input[name="vdo_id"]').val();
    var click_through_url = summit_info.find('input[name="click_through_url"]').val();
    var hover_text = summit_info.find('input[name="hover_text"]').val();
    var preview_image = summit_info.find('input[name="preview_image"]').val();
    var preview_image_url = summit_info.find('input[name="preview_image_url"]').val();
    var impression_tracker = summit_info.find('input[name="impression_tracker"]').val();
    var click_tracker = summit_info.find('input[name="click_tracker"]').val();
    summit_window.summit_ads.is_360 = summit_info.find('input[name="is_360"]').val();

    jwvdo_cont = 'botr_'+vdo_id+'_'+player_id+'_div';

    var sc = document.createElement('script');
    sc.type = 'text/javascript';
    sc.src = 'https://cdn.jwplayer.com/libraries/'+player_id+'.js';

    $('.summit-media > div > iframe').css({'position':'absolute'});
    $('.summit-media > div')
    .append(
        $('<div/>',{'class':'vdo-cont','style':'position: relative; width: 100%; height: 100%; margin: 0 auto; display:none;'})
        .append(
            $('<div/>',{'class':'vdo-preview','style':'position: absolute; width:100%; height:100%; background-repeat:no-repeat; background-position:center center; background-size:contain;'})
        )
        .append(
            $('<div/>',{'class':'vdo-player','style':'height:100%; margin: 0 auto 0; opacity: 0;'})
            .append(
                sc
            )
            .append(
                $('<div/>',{'id':jwvdo_cont})
            )
        )
        .append(
            $('<a/>',{'class':'vdo-link', 'target':'_blank', 'href':click_through_url, 'title':hover_text})
        )
    );

    console.log('rj_summit_render_jw v2: '+vdo_id);

    //Impression Tracker
    if (impression_tracker) {
        document.write('<img src="&#039;+impression_tracker+&#039;" border="0" height="1" width="1" alt="Advertisement" data-lazy-src="http://&#039;+impression_tracker+&#039;?is-pending-load=1" srcset="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7" class=" rj-lazy-load lazy-image"><noscript><img src="'+impression_tracker+'" border="0" height="1" width="1" alt="Advertisement" /></noscript>');
    }

    //Click Tracker
    if (click_tracker) {
        document.write('<sc'+'ript type="text/javascript" src="'+click_tracker+'"></scr'+'ipt>');
    } 

    
    if (preview_image) {
        vdo_preview = preview_image;
    }
    if (preview_image_url) {
        vdo_preview = preview_image_url;
    }
    if (vdo_preview) {
        $('.summit-media .vdo-preview').css({'background-image':'url("'+vdo_preview+'")'});
    } else {
        $('.summit-media .vdo-player').css({'opacity':'1'});
        $('.summit-media .vdo-preview').hide();
    }

    summit_window.summit_ads.jw_player_state = 'unknown';
    if ( summit_window.summit_ads.play_pause_elem != null && typeof(summit_window.summit_ads.play_pause_elem) != 'undefined') {
        summit_window.summit_ads.play_pause_elem.classList.remove('playing');
    }
    if (typeof(summit_window.summit_ads.yt_player) == 'object') {
        summit_window.summit_ads.yt_player.destroy();
    }
    summit_window.summit_ads.yt_player = '';

    var timer_count = 500;
    timer_check = setInterval(function () {
        timer_count--;
        //console.log('rj_summit_t:'+timer_count);
        if ( 'undefined' !== typeof(jwplayer) ) {
            clearInterval(timer_check);
            jwplayer(jwvdo_cont).setup({
                'playlist': 'https://cdn.jwplayer.com/v2/media/'+vdo_id+'?max_resolution=1280'
            });

            jwplayer(jwvdo_cont).on('ready', function(e) {
                jwvdo_setup();
                summit_ready();
            });
        }
        if (timer_count == 0) {
             clearInterval(timer_check);
        }
    }, 100);
}
function jwvdo_setup() {
    console.log('rj_summit_jwvdo_setup');
    $('.summit-media .vdo-cont').show();
    summit_window.summit_ads.jw_first_play = false;

    jwplayer(jwvdo_cont).on('play', function() {
        summit_window.summit_ads.play_pause_elem.classList.add('playing');
        if (!summit_window.summit_ads.jw_first_play) {
            summit_window.summit_ads.jw_first_play = true;
            $('.summit-media .vdo-player').css({'opacity':'1'});
            $('.summit-media .vdo-preview').hide();
        }
    });

    jwplayer(jwvdo_cont).on('pause', function() {
        summit_window.summit_ads.play_pause_elem.classList.remove('playing');
    });

    jwplayer(jwvdo_cont).on('complete', function() {
        jwplayer(jwvdo_cont).play(true);
    });
}

function summit_ready() {
    console.log('rj_summit_ready'); 
    $('body').addClass('rj-summit-ready');

    if ( $('#'+page_id).length > 0 &&  $('#'+region_id).length > 0 ) {
        summit_setup();
        /*
        if (! summit_window.summit_ads.summit_setup_already) {
            summit_window.summit_ads.summit_setup_already = true;
            summit_setup();
        }
        */

        var vdo_cont_elem = $('.summit-media .vdo-cont'); 

        var vdo_w = $('#ad_summit iframe').width(); 
        var vdo_h = $('#ad_summit iframe').height(); 
        if ( vdo_w >  $('.'+region_container_class).outerWidth() ) {
            vdo_w = $('.'+region_container_class).outerWidth(); 
            vdo_cont_elem.css({'width':vdo_w +'px'});
        }
        if ( vdo_h > $('.'+region_container_class).outerHeight() ) {
            vdo_h = $('.'+region_container_class).outerHeight();
            vdo_cont_elem.css({'height':vdo_h +'px'});
        }
      
        //style_link_to_cover_vdo
        var link_elem = $('.summit-media .vdo-link');
        link_elem.css({'position':'absolute','display':'block','top':'0'});
        
        if (summit_window.summit_ads.is_360 =='1') {
            link_elem.css({'right':'20px','width':'60px','height':'30px','background':'url("https://res.cloudinary.com/stephens-media/image/upload/c_scale,w_100/v1569353499/RJ/manual/360-icon-red.png") no-repeat center center', 'background-size':'cover', 'background-color':'#fff'});    
        } else {
            link_elem.css({'left':'0','width':'100%','height':'100%'});
        }
        
        summit_setup_vdo();
        /*
        if (!summit_window.summit_ads.summit_setup_vdo_already) {
            summit_window.summit_ads.summit_setup_vdo_already = true;
            summit_setup_vdo();
        } else {
            $('.'+region_container_class+' .vdo-play-pause').show();
            $('.'+region_container_class+' .vdo-vol').show();
            jwplayer(jwvdo_cont).pause(true);
            jwplayer(jwvdo_cont).setVolume(summit_window.summit_ads.vdo_vol);
        }
        */
    }
}
    
function summit_setup() {
    console.log('rj_summit_setup'); 
    summit_window.summit_ads.offset_top = $('#'+region_id).css('padding-top');
    summit_window.summit_ads.offset_top.replace('px', '');
    summit_window.summit_ads.offset_top = parseInt( summit_window.summit_ads.offset_top);
        
    summit_window.summit_ads.summit_set_margin_top = function() {
   
        var win_h = $( summit_window ).height(); 
        var margin_top = win_h -  summit_window.summit_ads.offset_top;
        $('#'+page_id).css({'margin-top': margin_top + 'px'}); 
        $('.'+region_container_class).height(margin_top); 

        var vdo_cont_elem = $('.summit-media .vdo-cont'); 
        var vdo_w = $('#ad_summit iframe').width(); 
        var vdo_h = $('#ad_summit iframe').height(); 
        if ( vdo_w >  $('.'+region_container_class).outerWidth() ) {
            vdo_w = $('.'+region_container_class).outerWidth(); 
            vdo_cont_elem.css({'width':vdo_w +'px'}); 
        }
        if ( vdo_h > $('.'+region_container_class).outerHeight() ) {
            vdo_h = $('.'+region_container_class).outerHeight(); 
            vdo_cont_elem.css({'height':vdo_h +'px'}); 
        }
        margin_top = 0;
        var vdo_player_elem = $('.summit-media .vdo-player'); 
        var vdo_cont_w = vdo_cont_elem.outerWidth(); 
        var vdo_cont_h = vdo_cont_elem.outerHeight(); 
        var vdo_player_w = vdo_cont_h * 16 / 9;
        if (vdo_player_w > vdo_cont_w) {
            vdo_player_w = vdo_cont_w
        }
        var vdo_player_h = vdo_player_w * 9 /16
        if (vdo_cont_h - vdo_player_h > 40) {
            margin_top = 40;
        }

        vdo_player_elem.css({'width':vdo_player_w +'px', 'margin-top':margin_top +'px'});
    }
    
    summit_window.summit_ads.summit_set_opacity = function() {
        var region_h =   $('#'+region_id).outerHeight(); 
        var scroll_pos = $(summit_doc).scrollTop(); 
        var opa = Math.round(scroll_pos * 100 / region_h) /100;
        opa = 1-opa;
        if (opa < 0) {
            opa = 0;
        }

        if (opa > 1) {
            opa = 1;
        }
        $('.'+region_container_class).css({'opacity':opa}); 
        if (opa < 0.15) {
            jwplayer(jwvdo_cont).pause(true);
        }
    }
    
    summit_window.onresize = function() {
        summit_window.summit_ads.summit_set_margin_top();
    };
        
    summit_window.onscroll = function() {
        summit_window.summit_ads.summit_set_opacity();
    };
    
    //style_page_elem
    $('#'+page_id).css({'position': 'relative', 'background-color': '#fff'});
    $('#'+region_id).css({'display': 'block', 'visibility':'visible'});
   
    
    //add_close_btn
    summit_window.summit_ads.close_elem = summit_doc.createElement('div');
    var close_btn = summit_doc.createElement('div');
    var close_txt = summit_doc.createElement('div');
    summit_window.summit_ads.close_elem.className = 'topsponsor-close';
    close_btn.className = 'topsponsor-close-btn';
    close_txt.className = 'topsponsor-text';
    close_txt.innerHTML = 'TO ARTICLES';
    summit_window.summit_ads.close_elem.appendChild(close_btn);
    summit_window.summit_ads.close_elem.appendChild(close_txt);
    
    summit_window.summit_ads.close_elem.addEventListener('click', function (event) {
        
        if ($('body').hasClass('stop-scrolling')) {
            $('body').removeClass('stop-scrolling');
        }
    
        var h = $('#rjth-region-topsponsor').outerHeight();
        h = h -  summit_window.summit_ads.offset_top;
        if ( $('#ad_summit').length > 0) {
            var offset = 0;

            if ($('#ad_summit').attr('data-offset-mobile')) {
                offset = $('#ad_summit').attr('data-offset-mobile');
            }
            if ($('#ad_summit').attr('data-offset-tablet') && $(summit_window).width() >= 753) {
                offset = $('#ad_summit').attr('data-offset-tablet');
            }
            if ($('#ad_summit').attr('data-offset-pc') && $(summit_window).width() >= 1100) {
                offset = $('#ad_summit').attr('data-offset-pc');
            }

            var offset = parseInt(offset);
            if (!isNaN(offset)) {
                h = h + offset;
            }
        }
        
        var scroll_pos = $(summit_window).scrollTop();
        if (scroll_pos < h) {
            $('html, body').animate({scrollTop:h},500);
        }
        
        jwplayer(jwvdo_cont).pause(true);
    });

    $('.'+region_container_class).append(summit_window.summit_ads.close_elem); //summit_window.summit_ads.region_container_elem.appendChild( summit_window.summit_ads.close_elem);
    
    //set_timer_to_open_summit_region
    setTimeout(function() {
        summit_window.summit_ads.summit_set_margin_top();
        summit_window.scrollTo(0,0);
        if (lock_time > 0) {
            summit_doc.body.classList.add('stop-scrolling');

            setTimeout(function(){
                summit_doc.body.classList.remove('stop-scrolling');
            }, lock_time);
        }
    
        if( !(/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Windows Phone|Opera Mini/i.test(navigator.userAgent)) ) {
            jwplayer(jwvdo_cont).play(true);
        }
    }, open_time);
}    
    
function summit_setup_vdo() {

    //add_play_pause_btn
    summit_window.summit_ads.play_pause_elem = summit_doc.createElement('div');
    summit_window.summit_ads.play_pause_elem.className = 'vdo-play-pause';
    summit_window.summit_ads.play_pause_elem.addEventListener('click', function (event) {
        if ( jwplayer(jwvdo_cont).getState() != 'playing') {
            jwplayer(jwvdo_cont).play(true);
            summit_window.summit_ads.play_pause_elem.classList.add('playing');
        } else {
            jwplayer(jwvdo_cont).pause(true);
            summit_window.summit_ads.play_pause_elem.classList.remove('playing');
        }    
    });

    $('.'+region_container_class).append(summit_window.summit_ads.play_pause_elem); //summit_window.summit_ads.region_container_elem.appendChild( summit_window.summit_ads.play_pause_elem);

    //add_vol_control
    summit_window.summit_ads.vdo_vol = 0; 
    summit_window.summit_ads.vol_ind_elem = summit_doc.createElement('div');
    summit_window.summit_ads.vol_ind_elem.className = 'vdo-vol-ind';
    
    var vol_elem = summit_doc.createElement('div');
    vol_elem.className = 'vdo-vol';
    
    var vol_up_elem = summit_doc.createElement('div');
    vol_up_elem.className = 'vdo-vol-ctrl vdo-vol-up';
    vol_up_elem.addEventListener('click', function (event) {
        summit_window.summit_ads.vdo_vol =  summit_window.summit_ads.vdo_vol + 25;
        if ( summit_window.summit_ads.vdo_vol > 100) {
            summit_window.summit_ads.vdo_vol = 100;
        }
        if (summit_window.summit_ads.vdo_vol > 5) {
            summit_window.summit_ads.vol_ind_elem.style.height =  summit_window.summit_ads.vdo_vol+'%';
        } else {
            summit_window.summit_ads.vol_ind_elem.style.height =  '5%';
        }
            jwplayer(jwvdo_cont).setVolume(summit_window.summit_ads.vdo_vol);
    });
    
    var vol_down_elem = summit_doc.createElement('div');
    vol_down_elem.className = 'vdo-vol-ctrl vdo-vol-down';
    vol_down_elem.addEventListener('click', function (event) {
         summit_window.summit_ads.vdo_vol =  summit_window.summit_ads.vdo_vol - 25;
        if ( summit_window.summit_ads.vdo_vol < 0) {
            summit_window.summit_ads.vdo_vol = 0;
        }
        if (summit_window.summit_ads.vdo_vol > 5) {
            summit_window.summit_ads.vol_ind_elem.style.height =  summit_window.summit_ads.vdo_vol+'%';
        } else {
            summit_window.summit_ads.vol_ind_elem.style.height =  '5%';
        }
            jwplayer(jwvdo_cont).setVolume(summit_window.summit_ads.vdo_vol);
    });
    
    var vol_range_elem = summit_doc.createElement('div');
    vol_range_elem.className = 'vdo-vol-range';
    vol_range_elem.appendChild( summit_window.summit_ads.vol_ind_elem);
        
    vol_elem.appendChild(vol_up_elem);
    vol_elem.appendChild(vol_range_elem);
    vol_elem.appendChild(vol_down_elem);
        
    $('.'+region_container_class).append(vol_elem);
    
    if (summit_window.summit_ads.vdo_vol > 5) {
        summit_window.summit_ads.vol_ind_elem.style.height =  summit_window.summit_ads.vdo_vol+'%';
    } else {
        summit_window.summit_ads.vol_ind_elem.style.height =  '5%';
    }
    
    jwplayer(jwvdo_cont).setVolume(summit_window.summit_ads.vdo_vol);
        
}
</script></div>
		</div>			</div>
		</div>
		<!-- end region topsponsor -->

	<div id="page" class="site">

					<div id="rjth-region-top" class="widget-area rj-container-fluid no-padding">
				<div class="row no-gutter">
					<div id="rj_widget_text-596" class="rj-widget-num-1 rj-widget-first rj-widget-odd  rj-widget rj_widget_text">		<div class="textwidget"><script>
    if (typeof window.$ === 'undefined') {window.$ = window.jQuery;}
    window.rj_newline = new RegExp("\n", "g");
    
    function getCookie(a) {
        var cookies = document.cookie.split(';');
        var cookie;
        for (var i = 0; i < cookies.length; i++) {
            cookie = cookies[i].trim();
            if (cookie.startsWith(a + '=')) {
                return cookie.substring(a.length + 1);
            }
        }
    
        return ''; 
    }
    function validateEmail(email) {
        var v = /\S+@\S+\.\S+/;
        return v.test(email);
    }
</script>
    
<div class="ads-300x250 check-ads-blocker" style="width:1px;height:0;"></div></div>
		</div><div id="rj_widget_text-655" class="rj-widget-num-2 rj-widget-even  rj-widget rj_widget_text">		<div class="textwidget"><script> 
(function ($) {     
window.fuel_arr = [];
window.fuel_arr[0] = {
    'youtube_vdo_id': '', //'vpSqQQSIyOs';
    'ready': false,
    'name': '7@7 FrontPage Channel',
    'title': 'WATCH NOW',
    'description': '7 minutes of local non-stop news, free for all users.',
    'thumbnail': 'https://res.cloudinary.com/review-journal/image/upload/f_auto,q_auto,c_scale,w_400/v1611777282/webdev/renee.jpg',
