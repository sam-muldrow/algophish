  <script>
      // powers the subscription control(s)
      class Subscription {

          constructor(formId) {
              this.formId = formId;
              this.form = document.getElementById(this.formId);
              this.form.addEventListener('submit', (e) => this.onSubmit(e));
          }

          onSubmit(e) {
              e.preventDefault();

              let formData = {};
              formData.name = this.form.elements.namedItem('fname').value.trim();
              formData.email = this.form.elements.namedItem('email').value.trim();
              if (this.form.elements.namedItem('fidArt'))
                  formData.fidArt = this.form.elements.namedItem('fidArt')?.value;
              formData.freferer = document.referrer;
              formData.furl = location.href;

              if (formData.email === '') {
                  return false;
              }

              // deferring all the heavywork to /subscribe page
              localStorage.setItem('WGSubscription', JSON.stringify(formData));
              location.href = "/subscribe.htm";

              return false;
          }
      }
  </script>

