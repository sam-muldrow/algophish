e to browser not repaining screen fast enough */ var redraw = ad.offsetHeight; } else { console.log(constant.ref + ' no ad element found for: "' + adId + '"; aborted'); } }; /** * Make specific ad sticky on scroll; will stick between a min & max vertical threshold * @parm str adWrapper = element that wraps the ad that'll be sticky; this is what will move on y axis * @param num topOffset = optional; default offset from top of viewport; resting state * @param bool refresh = optional; defauilt true - will refresh; else won't * @param mix cb = optional callback; passes optional param of adId of targeted sticky ad */ const convertAdToStickyScroll = function(adWrapper, topOffset, refresh, cb) { /* do some low level checks */ if(typeof adWrapper != 'object') { console.log(constant.ref + 'first argument must be a valid DOM element; aborted!'); return false; } let adSlot = adWrapper.getElementsByClassName('WrapperGPT')[0]; if(!adSlot) { console.log(constant.ref + 'wrapping element doesn\'t contain a valid ad slot'); return false; } refresh = (typeof refresh == 'boolean') ? refresh : true; /* IMPORTANT: we attempt to find the max height at which the sticky can vertically move. * we default to an assumed structure that most pages share; if not, the implementor * should ensure there's enough vertical height on the adWrapper element */ let contentLeftContainerHeight = document.getElementsByClassName('contentLeftColumn')[0]; if(contentLeftContainerHeight) { adWrapper.parentNode.style.height = contentLeftContainerHeight.clientHeight + 'px'; } /* convert to sticky; we attempt to set it for webkit browsers that need the prefix; then default */ adWrapper.style.position = '-webkit-sticky'; if(adWrapper.style.position == '') { adWrapper.style.position = 'sticky'; } adWrapper.style.top = topOffset + 'px'; /* if we're not refreshing stop here, nothing below matters */ if(!refresh) { if(typeof cb == 'function') { cb(adSlot.id); } return true; } if(typeof topOffset == 'number' && topOffset >= 0) { constant.stickyAdTopOffset = topOffset; } const checkStickyPosition = function() { if(constant.stickyAdTimeoutListener) { return false; } constant.stickyAdTimeoutListener = window.setTimeout(function() { try { window.clearTimeout(constant.stickyAdTimeoutListener); }catch(er) {} constant.stickyAdTimeoutListener = null; var currentOffset = JSUtils.viewportOffset(adWrapper, false); if(currentOffset.elementTop <= constant.stickyAdTopOffset) {
                        /* let's turn refreshable on for this ad slot */
                        if(!constant.converted) {
                            convertAdToRefreshable(adSlot.id);
                            constant.stickyAdConverted = true;
                        }
                        /* if our element has gone off the rails (out of viewport), stop refresh */
                        if(currentOffset.elementTop <= 0) {
                            if(!constant.stickyAdOffscreenPause) {
                                disableRefreshableAd(adSlot.id);
                                constant.stickyAdOffscreenPause = true;
                            }
                            constant.stickyAdReEntryRefresh = false;
                        /* if we came back to earth, let's resume refresh */
                        }else if(!constant.stickyAdReEntryRefresh && constant.stickyAdOffscreenPause && currentOffset.elementTop == constant.stickyAdTopOffset) {
                            if(!constant.refreshDisableAll) {
                                enableRefreshableAd(adSlot.id, function(adId) {
                                    refreshSpecificAds(adId);
                                });
                            }
                            constant.stickyAdOffscreenPause = false;
                            constant.stickyAdReEntryRefresh = true;
                        }else {
                            /* if coming off of initial pause, let's trigger a refresh since we 
                            * may have passed the refresh time, which would appear as if it's taking longer on next refresh
                            */
                            if(!constant.refreshDisableAll && adSlot.getAttribute('data-refreshstate') == 'pause') {
                                unregisterTimerRefresh(adSlot.id, function(adId) {
                                    refreshSpecificAds(adId);
                                    triggerAdByTimer(adId);
                                });
                            }
                            adSlot.setAttribute('data-refreshstate', '');
                        }
                    }
                    
                }, constant.stickyAdTimer);
            };
            /* listener that kickstarts entire sticky scroll routine */
            if(!constant.stickyAdListener) {
                constant.stickyAdListener = window.addEventListener('scroll', checkStickyPosition, false);
            }
            if(typeof cb == 'function') {
                cb(adSlot.id);
            }
        };
        /**
         * Convert normal ad slot to refreshable
         * @parm str adId = div ad wrapper ID
         * @param mix cb = optional callback; passes optional param of ad that was converted
        */
        const convertAdToRefreshable = function(adId, cb) {
            let ad = getSlot(adId);
            /* check if this isn't already marked as refreshable; if so, stop here */
            if(ad.refreshable) {
                return true;
            }
            ad.refreshable = true;
            ad.slot.setAttribute('data-setforlazyload', 'false');
            ad.slot.setAttribute('data-refresh', 'true');
            ad.slot.setAttribute('data-refreshstate', '');
            /* do initial refresh before kicking off the timer; else we establish the timer but not refresh */
            if(!constant.refreshDisableAll) {
                refreshSpecificAds(adId, function() {
                    triggerAdByTimer(adId);
                });
            }else {
                triggerAdByTimer(adId);
            }
            if(typeof cb == 'function') {
                cb(ad);
            }
        };
        /**
         * Disable specific refreshable ad slot or All 
         * @parm str adId = div ad wrapper ID OR 'all' to disable all of refreshable ads in active DOM
         * @param mix cb = optional callback; passes optional param of adId that was disabled (but not for all)
        */
        const disableRefreshableAd = function(adId, cb) {
            if(adId === 'all') {
                constant.refreshDisableAll = true;
                if(typeof cb == 'function') {
                    cb();
                }
            }else {
                let ad = getSlot(adId);
                ad.slot.setAttribute('data-refreshstate', 'disable');
                if(typeof cb == 'function') {
                    cb(adId);
                }
            }
        };
        /**
         * Enable specific refreshable ad slot or All 
         * @parm str adId = div ad wrapper ID OR 'all' to enable all of refreshable ads in active DOM
         * @param mix cb = optional callback; passes optional param of adId that was disabled (but not for all)
        */
        const enableRefreshableAd = function(adId, cb) {
            if(adId === 'all') {
                constant.refreshDisableAll = false;
                if(typeof cb == 'function') {
                    cb();
                }
            }else {
                let ad = getSlot(adId);
                ad.slot.setAttribute('data-refreshstate', '');
                if(typeof cb == 'function') {
                    cb(adId);
                }
            }
        };
        /**
         * Unregister (remove) refresh timer for given slot 
         * @parm str adId = div ad wrapper ID OR 'all' to enable all of refreshable ads in active DOM
         * @param mix cb = optional callback; passes optional param of adId
        */
        const unregisterTimerRefresh = function(adId, cb) {
            try {
                /* sometimes we may have an active timer, thus clear it.
                 * we use a try-catch because a timer may not exist and we don't care about catching errors so leave that blank
                */
                window.clearTimeout(constant.refreshTimers[adId]);
            }catch(er) {}
            constant.refreshTimers[adId] = null;
            
            if(typeof cb == 'function') {
                cb(adId);
            }
        };
        /**
         * Trigger specific ad for timer refreshes
         * @param str adId = div ad wrapper ID
         * @param int counter = optional counter
         */
        const triggerAdByTimer = function(adId, counter) {
            if (constant.refreshLimit) {
                if ((typeof counter != 'number') && (typeof counter != 'integer')) {
                    counter = 1;
                }
                /* if we meet/exceed our limit, stop here */
                if (counter > constant.refreshLimit) { /* if this exists, kill it */ if(constant.refreshScrollHandler) { window.removeEventListener('scroll', checkRefreshableScroll); constant.refreshScrollHandler = null; } return false; } } /* create our scroll handler that'll be used in conjuction with refresh timer */ if(!constant.refreshScrollHandler) { constant.refreshScrollHandler = window.addEventListener('scroll', checkRefreshableScroll); } /* always null out any timer for this ad instance, incase of run-away scripts; then recreate timer */ unregisterTimerRefresh(adId); constant.refreshTimers[adId] = window.setTimeout(function() { /* * Important!! if this ad is set for lazyloading, we let that trigger first. * this condition shouldn't occur unless it was explicitly called publically */ let adSlot = getSlot(adId); if (adSlot.lazyload && adSlot.slot.getAttribute('data-loadcomplete') == 'false') { console.log(constant.ref + 'Ad refresh waiting for lazyload -> "' + adId + '"'); /* recursive call to continue timer, but don't increment counter since we're waiting for lazyload */ if (constant.refreshLimit > 0) { triggerAdByTimer(adId, counter); } else { triggerAdByTimer(adId); } } else { /* if we're not marked as 'disable', check if we're idle (not scrolling); if so, pause our refreshable ad */ if(adSlot.slot.getAttribute('data-refreshstate') != 'disable') { if(JSUtils.viewportOffset().top == constant.refreshScrollPosition || constant.refreshScrollPosition == 0) { adSlot.slot.setAttribute('data-refreshstate', 'pause'); }else { adSlot.slot.setAttribute('data-refreshstate', ''); } } /* If we have a special refresh pause state or master disable is true, let's skip this iteration */ if(adSlot.slot.getAttribute('data-refreshstate') == 'pause' || adSlot.slot.getAttribute('data-refreshstate') == 'disable' || constant.refreshDisableAll) { /* recursive call to continue timer */ if(constant.refreshLimit > 0) { triggerAdByTimer(adId, (counter + 1)); }else { triggerAdByTimer(adId); } }else { constant.router(adId, function() { console.log(constant.ref + 'Ad refresh triggered -> "' + adId + '"'); /* recursive call to continue timer */ if(constant.refreshLimit > 0) { triggerAdByTimer(adId, (counter + 1)); }else { triggerAdByTimer(adId); } }); } } }, constant.refreshTime); }; /** * Chek scroll position for refreshable ad trigger * Note: we limit the scroll listener by not firing it on every pxl scroll */ const checkRefreshableScroll = function() { if(!constant.refreshScrollTrigger) { constant.refreshScrollTrigger = window.setTimeout(function() { constant.refreshScrollPosition = JSUtils.viewportOffset().top; constant.refreshScrollTrigger = null; }, 50); } }; /** * Inject new Ad spot at specified DOM location * @param obj sib = [document.body|adjacent] element to append next to * @param str size = ad kw (i.e. '300x250_top') * @param mix opts = json obj of optional params; i.e. {className:'', idName:'', setRefresh:true} */ const injectAd = function(sib, size, opts) { /* make sure our sibling element exists Or it's the body */ if (typeof sib != 'object') { console.log(constant.ref + 'injectAd: the first argument "sib" is not a valid element that exists in DOM; aborting!'); return false; } opts = opts || {}; var gptID = 'div-gpt-ad-' + constant.zone + '-' + size, allowAdRefresh = (typeof opts.setRefresh == 'boolean' && opts.setRefresh != '') ? opts.setRefresh : false; /* If this already exists, let's stop here */ if (document.getElementById(gptID)) { console.log(constant.ref + ' this element "#' + gptID + '" already exists in DOM; aborting!'); return false; } var wrapper = document.createElement('div'); if (typeof opts.className != 'undefined') { wrapper.setAttribute('class', 'adSpot ' + opts.className); } else { wrapper.setAttribute('class', 'adSpot'); } if (typeof opts.idName != 'undefined') { if (!document.getElementById(opts.idName)) { wrapper.setAttribute('id', opts.idName); } } var innerWrapper = document.createElement('div'); innerWrapper.setAttribute('class', 'WrapperGPT'); innerWrapper.setAttribute('id', gptID); innerWrapper.setAttribute('data-slotvar', 'slot' + size); innerWrapper.setAttribute('data-setforlazyload', 'true'); innerWrapper.setAttribute('data-prepped', 'false'); innerWrapper.setAttribute('data-refresh', allowAdRefresh); innerWrapper.setAttribute('data-loadcomplete', 'false'); var _script = document.createElement('script'); _script.setAttribute('type', 'text/javascript'); _script.setAttribute('class', ''); _script.setAttribute('data-dynamic', 'true'); _script.innerHTML = '(function() { googletag.cmd.push(function() { googletag.display("' + gptID + '");googletag.pubads().refresh([gptSlots["slot' + size + '"]]); }); })();'; /* listen for when ad slot is filled */ var observer = new MutationObserver(function(mutation) { if (mutation.length > 0) { innerWrapper.setAttribute('data-loadcomplete', 'true'); /* kill our observer */ observer.disconnect(); } }); observer.observe(innerWrapper, { childList: true }); /* append it all */ innerWrapper.appendChild(_script); wrapper.appendChild(innerWrapper); /* inject new ad slot to our designated spot in the DOM */ if (sib == document.body) { sib.appendChild(wrapper); } else { sib.parentNode.insertBefore(wrapper, sib); } if (document.getElementById(gptID)) { return true; } else { return false; } }; /** * Public Utility Helpers (methods) ----------------------------------- */ /** * Checks if adId exists * @parm str adId = div ad wrapper ID */ const adIdExist = function(adId) { let i; for (i = 0; i < constant.ads.length; i++) {
                if (constant.ads[i]['id'] == adId) {
                    return true;
                }
            }
            return false;
        };
        /**
         * Get specific ad slot (info)
         * @param str adId = div ad wrapper ID
         */
        const getSlot = function(adId) {
            let i;
            /* make sure a slot exists by this id */
            for (i = 0; i < constant.ads.length; i++) {
                if (constant.ads[i]['id'] == adId) {
                    return constant.ads[i];
                }
            }
            return false;
        };
        /**
         * Set adSlot to load complete 
         * @param str adUnit = ad wrapper (placeholder)
         */
        const setAdLoadComplete = function(adUnit) {
            adUnit.setAttribute('data-loadcomplete', 'true');
        };
        /**
         * (Re)create page scroll event for lazyloading 
         * @param bool create = default true; create listener or destroy it
         */
        const createScrollListener = function(create) {
            create = (typeof create == 'boolean') ? create : true;
            if (!create) {
                window.removeEventListener('scroll', CDCGPT.checkAdPosition);
                constant.scrollEvent = null;
            } else if (constant.scrollEvent == null) {
                constant.scrollEvent = window.addEventListener('scroll', CDCGPT.checkAdPosition);
            }
        };

        /**
         * Private Methods -----------------------------------
         */

        /* Fetch and catalog all ads, lazy load, refreshable & others */
        const getAds = function() {
            let i, ad, brk, ads = document.getElementsByClassName('WrapperGPT'), cnt = ads.length;
            for (i = 0; i < cnt; i++) {
                /* ensure we have a proper wrapper and not some rogue element */
                if (!ads[i].getAttribute('data-slotvar')) {
                    continue;
                }
                ad = {
                    slot: ads[i],
                    id: ads[i].id,
                    width: 0,
                    height: 0,
                    kw: '',
                    lazyload: false,
                    refreshable: false
                };
                /* split and get our size & kw */
                brk = ads[i].getAttribute('data-slotvar').substr(4).split('x');
                ad.width = brk[0];
                brk = brk[1].split('_');
                ad.height = brk[0];
                ad.kw = brk[1];
                /* do we have a lazyload directive? */
                if (ads[i].getAttribute('data-setforlazyload') != null && ads[i].getAttribute('data-setforlazyload') != 'false') {
                    ad.lazyload = true;
                    /* keep track how many of these we have */
                    constant.lazyLoadableCnt++;
                }
                /* do we have a refresh directive */
                if (ads[i].getAttribute('data-refresh') === 'true') {
                    ad.refreshable = true;
                }
                constant.ads.push(ad);
            }
            /* Debuggin: Since this is sorta large, let's only show it if we requested it  */
            if (window.location.href.indexOf('showAdCollections=true') != -1) {
                console.log(constant.ref + 'All Ads: ', constant.ads);
            }
        };
        /* trigger ads on initial load - only for...
         * - any ads "not" marked for refresh
         */
        const triggerAdsOnLoad = function() {
            let i, ad, lazyTriggered = false, doScrollEventCall = false, cnt = constant.ads.length;
            for (i = 0; i < cnt; i++) {
                ad = constant.ads[i];
                /* lazyloadable - if found at least 1, trigger our lazyload logic once */
                if (ad.lazyload == true && !lazyTriggered) {
                    if(!doScrollEventCall) {
                        createScrollListener();
                        doScrollEventCall = true;
                    }
                    checkAdPosition();
                    lazyTriggered = true;
                }
                /* lazyloadable - prep slot for lazyloading (DOES NOT trigger ads) */
                if (ad.lazyload == true && lazyTriggered) {
                    refreshSpecificAds(ad.slot.id);

                    /* else regular loading  */
                } else {
                    constant.router(ad.slot.id);
                }
            }
        };

        /* initialize any refreshable ads */
        const initializeRefreshableAds = function() {
            /* refresh mechanism is not enabled  */
            if (constant.refreshTime <= 0 || constant.refreshLimit < 1) {
                return false;
            }
            let i, ad, cnt = constant.ads.length;
            for (i = 0; i < cnt; i++) {
                ad = constant.ads[i];
                if (ad.refreshable) {
                    /*
                     * if we're marked for lazyloading & it's not yet loaded, let's DEFER our ad refresh
                     * and shift it to the lazyload mechanism
                     */
                    if (ad.lazyload && ad.slot.getAttribute('data-loadcomplete') == 'false') {
                        continue;
                    }
                    /* kick off our recursive func call */
                    triggerAdByTimer(ad.slot.id);
                }
            }
        };

        /**
         * Check ad position on scroll for lazyload marked slots
         * note: this can also trigger refreshable ads if they were deferred from 'initializeRefreshableAds()'
         */
        const checkAdPosition = function() {
            let i, adY, isHidden, isPrepped, inView = false, wasTriggered = false, cnt = constant.ads.length, scrollY = (window.pageYOffset || document.documentElement.scrollTop), viewportY = (window.innerHeight || document.documentElement.clientHeight);
            constant.scrollEvent = null;
            /* Let's stop this execution on scroll up */
            if (constant.lastScroll > 1 && constant.lastScroll >= scrollY) { constant.lastScroll = scrollY - 1; return false; } constant.lastScroll = scrollY; for (i = 0; i < cnt; i++) {
                /* skip any ads not marked for lazyloading  */
                if (!constant.ads[i].lazyload) {
                    continue;
                }

                let adUnit = constant.ads[i], pos = adUnit.slot.getBoundingClientRect();
                adY = (pos.top + scrollY);
                /* determine if we're within viewport (less any offset) */
                inView = (scrollY > (adY - viewportY + constant.viewOffset)) ? true : false; /* check if we were properly prepped */ isPrepped = adUnit.slot.getAttribute('data-prepped'); /* some ads are marked as "hidden" from view, let's handle these differently */ isHidden = (adUnit.slot.classList.contains('gptHidden')) ? true : false; /* check if this ad was already triggered (loaded) */ wasTriggered = adUnit.slot.getAttribute('data-loadcomplete'); /* finally trigger ad if possible */ if (inView && !isHidden && isPrepped == 'true' && wasTriggered == 'false') { /* Mark it so it doesn't load again * because scroll events trigger so fast, make sure we do this here and not rely * a delayed plugin to handle it; (the 'router' method can be overloaded by a plugin) */ if (constant.router != triggerAdRouter) { setAdLoadComplete(adUnit.slot); } constant.router(adUnit.id, function() { setAdLoadComplete(adUnit.slot); console.log(constant.ref + 'checkAdPosition: Lazy loaded -> ' + adUnit.id); /* remember, we had deferred these, let's trigger them now if any */ if (adUnit.slot.getAttribute('data-refresh') == 'true') { triggerAdByTimer(adUnit.id); } }); /* every lazyloadable ad triggered, we deduct from cnt */ constant.lazyLoadableCnt--; /* else if not ready (prepped), let's do recursive call to check again */ } else if (inView && !isHidden && isPrepped == 'false' && wasTriggered == 'false') { createScrollListener(false); constant.throttleHandler = window.setTimeout(function() { constant.throttleHandler = null; checkAdPosition(); }, constant.throttle); } } /* reapply our event after it meets throttle */ if (!constant.throttleHandler) { /* Destroy our listener and run a throttled timeout */ createScrollListener(false); /* as long as we have ads still to lazy load, recreate throttled scroll event */ if (constant.lazyLoadableCnt > 0) { constant.throttleHandler = window.setTimeout(function() { constant.throttleHandler = null; createScrollListener(); }, constant.throttle); } else { constant.throttleHandler = null; } } }; /** * Triggers ad refresh/load; apply this to our 'router' constant * NEVER call this directly; use 'constant.router()' */ const triggerAdRouter = function(adId, cb) { if (adIdExist(adId)) { refreshSpecificAds(adId, cb); } }; constant.router = triggerAdRouter; (function() { /* 1. Any Router plugin? */ if ('plugin' in opts) { if (typeof opts.plugin.router == 'function') { let routerObject = opts.plugin.router(); /* if there's a 'router' key, we overload our router function */ if ('router' in routerObject) { constant.router = routerObject.router.bind({ 'constant': constant, 'adIdExist': adIdExist, 'getAds': getAds, 'getSlot': getSlot, 'refreshSpecificAds': refreshSpecificAds, 'injectAd': injectAd }); } /* if there's a CMD wrapper, let's set save it */ if('cmdWrapper' in routerObject) { constant.passThruRoutines.cmdWrapper = routerObject.cmdWrapper; } /* is there any pass-thru routines? If so, add them to our collection */ if ('routines' in routerObject) { if ('beforeAdLoad' in routerObject.routines) { if(routerObject.routines.beforeAdLoad.routine) { constant.passThruRoutines.beforeAdLoad[routerObject.routines.beforeAdLoad.name] = routerObject.routines.beforeAdLoad.routine; } } if ('afterAdLoad' in routerObject.routines) { if(routerObject.routines.afterAdLoad.routine) { constant.passThruRoutines.afterAdLoad[routerObject.routines.afterAdLoad.name] = routerObject.routines.afterAdLoad.routine; } } } } } /* 2. IMPORTANT: Here we build our ad slots */ /* Rewrite our environment based on subdomain - this is ONLY rel for testing against ww1-9 & welcome2 */ var isHost = String(window.location.hostname); if (/ww[1-9]\./.test(isHost) || /welcome2\./.test(isHost)) { constant.isEnviroment = window.location.hostname; } /* Define our Slots */ googletag.cmd.push(function() { gptSlots['slot970x66_top'] = googletag.defineSlot("/2922/carsdirect/CDCXHomepage/970x66_top", [[970, 66], [970, 250]], "div-gpt-ad-CDCXHomepage-970x66_top").addService(googletag.pubads()).setTargeting("kw", "top"); gptSlots['slot426x204_top'] = googletag.defineSlot("/2922/carsdirect/CDCXHomepage/426x204_top", [426, 204], "div-gpt-ad-CDCXHomepage-426x204_top").addService(googletag.pubads()).setTargeting("kw", "top"); gptSlots['slot260x248_top'] = googletag.defineSlot("/2922/carsdirect/CDCXHomepage/260x248_top", [260, 248], "div-gpt-ad-CDCXHomepage-260x248_top").addService(googletag.pubads()).setTargeting("kw", "top"); gptSlots['slot260x248_bottom'] = googletag.defineSlot("/2922/carsdirect/CDCXHomepage/260x248_bottom", [260, 248], "div-gpt-ad-CDCXHomepage-260x248_bottom").addService(googletag.pubads()).setTargeting("kw", "bottom"); gptSlots['slot300x250_top'] = googletag.defineSlot("/2922/carsdirect/CDCXHomepage/300x250_top", [300, 250], "div-gpt-ad-CDCXHomepage-300x250_top").addService(googletag.pubads()).setTargeting("kw", "top"); gptSlots['slot682x250_spotlight'] = googletag.defineSlot("/2922/carsdirect/CDCXHomepage/682x250_spotlight", [682, 250], "div-gpt-ad-CDCXHomepage-682x250_spotlight").addService(googletag.pubads()).setTargeting("kw", "spotlight"); gptSlots['slot1000x90_top'] = googletag.defineSlot("/2922/carsdirect/CDCXHomepage/1000x90_top", [1000, 90], "div-gpt-ad-CDCXHomepage-1000x90_top").addService(googletag.pubads()).setTargeting("kw", "top"); /* Define global targets */ googletag.pubads().setTargeting("zipcode", "03824"); googletag.pubads().setTargeting("environment", constant.isEnviroment); /* Check and inject any custom targeting */ if(typeof opts.targets == 'object') { for(let target in opts.targets) { googletag.pubads().setTargeting(target, opts.targets[target]); } } /* Async */ googletag.pubads().collapseEmptyDivs(false); googletag.pubads().disableInitialLoad(); googletag.pubads().addEventListener('slotOnload', function(e) { /* - Keep this; let's implement this in near future console.log('Ad slot fully rendered: ' + e.lineItemId); */ }); googletag.enableServices(); }); /* 3. create various event listeners * Set up load events to trigger ads */ window.addEventListener('DOMContentLoaded', function() { /* get our collection of ads */ getAds(); /* trigger an ads on load - only lazyloadable and non-lazyloadable */ triggerAdsOnLoad(); /* trigger any refreshable ads */ initializeRefreshableAds(); }, false); })(); return { 'constant': constant, 'refreshSpecificAds': refreshSpecificAds, 'disableRefreshableAd' : disableRefreshableAd, 'enableRefreshableAd' : enableRefreshableAd, 'convertAdToStickyScroll' : convertAdToStickyScroll, 'convertAdToRefreshable' : convertAdToRefreshable, 'triggerAdByTimer': triggerAdByTimer, 'unregisterTimerRefresh' : unregisterTimerRefresh, 'checkAdPosition': checkAdPosition, 'setAdLoadComplete': setAdLoadComplete, 'adIdExist': adIdExist, 'getSlot': getSlot, 'injectAd': injectAd } })({ /* inject additional / custom targets */ targets: { }, plugin: { router: (typeof GPTPluginA9 != 'undefined' && window.location.href.indexOf('enableA9=false') == -1 ? GPTPluginA9.fetchBids : null) } }); </script><script type="text/javascript"> window.GTM = (function(accntID, dLayer) { var gtmAccount = (typeof accntID != 'undefined' && accntID !== '') ? accntID : 'GTM-T8KQG4'; /* CDCX-30415 - unfortunately we need to hard-code this here until we have a better architecture */ let isHostname = "www.carsdirect.com"; /* if("carsdirect.com" != "") { isHostname = ("www" != "" ? "www." : "") + "carsdirect.com"; } */ window.dataLayer = [{ "page" : isHostname }]; buildBoilerplate = function() { /* because this method is executed before page load, we run this snippet on load */ window.addEventListener('load', function() { var noScriptTag = document.createElement('noscript'); noScriptTag.innerHTML = '<iframe src="//www.googletagmanager.com/ns.html?id=' + gtmAccount + '" height="0" width="0" style="display:none;visibility:hidden"></iframe>'; document.body.insertBefore(noScriptTag, document.body.firstChild); }, false); /* this runs immediately */ (function(w,d,s,l,i){ w[l]=w[l]||[];w[l].push({