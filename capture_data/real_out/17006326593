<script>
	$(function(){
		if(ispc){
			$('#float_box').append('<div onclick="gtag(\'event\', \'live chat\', { \'event_category\' : \'Chat_click\',\'event_label\' : \'public2\'})" id="livechat-for-own-test" class="open_chat_window"><div class="img"><img src="/images_2019/livechat/2022/livechat.svg" width="25"></div><span><img src="/images_2019/livechat/2022/xl.png" width="21" height="21"> Can I help you?</span> </div>');
			$('#float_box').append('<div class="float_totop2 hidden"><img src="/images_2019/livechat/2022/top.svg" width="25"></div>');
			$(window).scroll(function(){//totop滚动
				if($(window).scrollTop()>450){
					$('.float_totop2').removeClass('hidden');
				}else{
					$('.float_totop2').addClass('hidden');
				};
			});
			$('.float_totop2').click(function(){
				$('body,html').animate({scrollTop:0},600);
			});
			if(!localStorage.getItem('livechat-for-own')){
				localStorage.setItem('livechat-for-own','true');
				setTimeout(function(){
					$('#livechat-for-own-test').addClass('show');
					setTimeout(function(){
						$('#livechat-for-own-test').removeClass('show');
					},2000);
				},5000);
			}
		}
	})
</script>
