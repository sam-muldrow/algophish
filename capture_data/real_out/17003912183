<script async>
  async function canvasDownload(id,titleImage) {
    html2canvas(document.querySelector("#" + id)).then(canvas => {
        var a = document.createElement('a');
        a.href = canvas.toDataURL("image/jpeg").replace("image/jpeg", "image/octet-stream");
        a.download = titleImage+'.jpg';
        a.click();
    });
  }


  async function canvasShare(id, titleImage) {
    html2canvas(document.querySelector("#" + id)).then(canvas => {
      canvas.toBlob(async (blob) => {
        const files = [new File([blob], titleImage + '.png', { type: blob.type })]
        const shareData = {
          text: 'Share ' + id,
          title: 'Share ' + id + ' ' + titleImage,
          files,
        }
        if (navigator.canShare(shareData)) {
          try {
            await navigator.share(shareData);
          } catch (err) {
            canvasDownload(id, titleImage);
          }
        } else {
          canvasDownload(id, titleImage);
        }
      });
    });
  }


  function showTime(){
    var date = new Date();
    var h = (date.getHours()<10?'0':'') + date.getHours();
    var m = (date.getMinutes()<10?'0':'') + date.getMinutes();
    var s = (date.getSeconds()<10?'0':'') + date.getSeconds();
    var session = "<span>WIB</span>"; 
    
    var time = h + ":" + m + ":" + s + " " + session;
    $('.current-time').html(time); 
    setTimeout( showTime, 1000 );
  }



  function adzanToggle(){
    "use strict";
    $('.timesignal-toggle').click(function(){
    $('.timesignal-toggle').toggle();
    }); 
    
    $('.timesignal-close').click(function(){
     document.getElementById('timesignal').remove();
      return false;
    });
  };





  function adzanSound(){
    var music = document.getElementById("music");
    
    $('.timesignal-toggle[title="Pause"]').click(function(){
    music.pause();
    }); 
    
    $('.timesignal-toggle[title="Play"]').click(function(){
    music.play();
    }); 
  };

      

    



  showTime();
  adzanToggle();
  adzanSound();
</script>
