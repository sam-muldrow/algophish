<script>
 (function() {
   var ua = window.navigator.userAgent.toLowerCase();
   var isIOS = /i(phone|pod|pad)/.test(ua);
   var isAndroid = /android/.test(ua);
   var isPC = !isIOS && !isAndroid;
   new Swiper('.swiper-container', {
     loop: true,
     speed: 300,
     slidesPerView: isPC ? 3.5 : 1,
     spaceBetween: 30,
     centeredSlides : true,
     pagination: {
       el: '.swiper-pagination',
       clickable: true,
     },
     autoplay: {
       delay: 2500
     },
     on: {
       init: function () {
         setTimeout(function(){
           document.querySelector('.swiper-container').classList.add('completed');
         }, 500);
       },
     }
   });
 })();

 (function(options) {
   function scrollLeft(element, to, duration, callback) {
     var start = element.scrollLeft,
         change = to - start,
         currentTime = 0,
         increment = 20;

     var animateScroll = function(){
       currentTime += increment;
       var val = Math.easeInOutQuad(currentTime, start, change, duration);
       element.scrollLeft = val;
       if(currentTime < duration) {
         setTimeout(animateScroll, increment);
       } else {
         callback();
       }
     };
     animateScroll();
   }

   Math.easeInOutQuad = function (t, b, c, d) {
     t /= d/2;
     if (t < 1) return c/2*t*t + b;
     t--;
     return -c/2 * (t*(t-2) - 1) + b;
   };

   var update_wrapper_status = function(wrap, type){
     var ul = wrap.querySelector('.title-banner');

     if (ul.querySelectorAll('li').length <= options[type].list_limit) {
       wrap.classList.add('termination-right');
     } else {
       var style = document.defaultView.getComputedStyle(wrap.querySelector('.title-banner').querySelector('li'), '');
       var box = wrap.querySelector('.title-banner').querySelector('li').clientWidth + (+style['margin-right'].replace('px', ''));
       var w = (ul.querySelectorAll('li').length - (options[type].list_limit + 1)) * box;
       if (wrap.querySelector('.title-banner').scrollLeft > w) {
         wrap.classList.add('termination-right');
       } else {
         wrap.classList.remove('termination-right');
       }
     }
     if (wrap.querySelector('.title-banner').scrollLeft !== 0) {
       wrap.classList.remove('termination-left');
     } else {
       wrap.classList.add('termination-left');
     }
   };

   document.addEventListener('DOMContentLoaded', function(){
     var type = window.innerWidth > 800 ? 'pc' : 'sp';
     window.addEventListener('resize', function() {
       type = window.innerWidth > 800 ? 'pc' : 'sp';
     }, false);

     document.querySelectorAll('.index-title-action-wrapper').forEach(function (wrap) {
       var ul = wrap.querySelector('.title-banner');
       ul.addEventListener('scroll', function(){
         update_wrapper_status(wrap, type);
       },false);

       update_wrapper_status(wrap, type);

       if (ul.querySelectorAll('li').length > options[type].list_limit) {
         wrap.querySelectorAll('.index-title-action-btn').forEach(function (button) {
           button.addEventListener('click', function() {
             var w = ul.querySelector('li').offsetWidth;
             var d = this.classList.contains('direction-left') ? -1 : 1;
             var p = (ul.scrollLeft + d * (w * options[type].list_limit) + w / 3);
             scrollLeft(ul, parseInt(p), 250 , function(){
               update_wrapper_status(wrap, type);
             });
           },false);
         });
       }
     });

   }, false);
 })({
   pc: {
     list_limit: 5
   },
   sp: {
     list_limit: 2
   }
 });
</script>
<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css">
