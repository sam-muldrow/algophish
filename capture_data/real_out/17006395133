<script>
	function getCookie(name) {
		var re = new RegExp(name + '=([^;]+)');
		var value = re.exec(document.cookie);
		return (value != null) ? unescape(value[1]) : null;
	}

	if (getCookie('drtarg') === null) {
		/*
		jQuery.ajax({
			url: window.global.rest_root + 'dr/v1/collect',
			method: 'GET',
			cache: false,
			beforeSend: function (xhr) {
				xhr.setRequestHeader('X-WP-Nonce', window.global.rest_nonce);
			}
		}).done(function (response) {
			dataLayer.push({
				'dr_target': response.drtarg
			});
		});
		*/
	}
	jQuery('#requestDemo').click(function () {
		window.location = '/request-demo';
		return false;
	});
	$('.close-popup').click(function(){
		 $('#video-popup').css('display','none');
	});
</script>


<script>
jQuery(document).ready(function() {
/*

	$(".sign-in").hover(function() {
 
  $('.hover-box-dropdown').addClass('hidden');
  
}, function() {
	$('.hover-box-dropdown').removeClass('hidden');
});

$(".sign-in").on({
    mouseover: function() {
            $(".hover-box-dropdown").toggleClass('hidden');
    },
    mouseout: function() {
            $(".hover-box-dropdown").toggleClass('hidden');
    }
})*/
var cols = $('.dropdown-child.left-section')

// console.log('.dropdown-child', document.getElementsByClassName('dropdown-child')[0].parentElement);

jQuery.each(cols, function(index, col) {
	const colClass = col.className.split(' ')[2];
	col.parentElement.parentElement.parentElement.parentElement.classList.add(colClass);
})
jQuery('.mobile-toggle').on('click', function(event) {
 
 var toggle = jQuery(this).attr('data-toggle');
  
 if (toggle === 'open') {
	  jQuery('nav.navbar').css('overflow-y', 'visible');
      jQuery(this).parent('li').find('div.dropdown-menu div.row div.menu-right').css("display", "none"); 
} else {
	 jQuery('nav.navbar').css('overflow-y', 'auto');
     jQuery(this).parent('li').find('div.dropdown-menu div.row div.menu-right').css("display", "block"); 
 }
});
});
</script>

