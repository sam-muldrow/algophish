            function mm_ss_ga_events(action, label) {
                if ( typeof(ga) != 'undefined' ) {
                    ga('send', 'event', 'social_sharing', action, label);
                }
                dataLayer.push({
                    'event': 'eventTracker',
                    'eventCat': 'Web Component',
                    'eventAct': 'Clicks',
                    'eventLbl': 'left-sidebar | social-share-' + label,
                });
            }
