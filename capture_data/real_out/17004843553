    window.NP_load = function (src, cbs) {
      var script = document.createElement('script'); script.type = 'text/javascript'; script.async = true;
      script.src = src;
      if (typeof cbs === 'function') {
        cbs(script);
      }
      var scripts = document.getElementsByTagName('script')[0] || document.getElementsByTagName("head")[0] || document.getElementsByTagName("body")[0];
      scripts.parentNode.insertBefore(script, scripts);
    }

    window.NP_enableScript = function () {
      const scripts = document.querySelectorAll('script[type="text/wait-for-consent"]');
      for (let i = 0; i < scripts.length; i++) {
        const s = scripts[i];
        const e = s.cloneNode(true);
        e.setAttribute('type', 'text/javascript')
        s.after(e);
        s.remove();
      }
    }

    const socialMediaId = 1; 
    const vendor = {
      [socialMediaId]: {
        enabled: true,
        init() {
          window.NP_EnableSocial = true;
          if (typeof window.enableEmbeds === 'function') {
            window.enableEmbeds();
          }
        }
      }
    }

    window.addEventListener("adnginLoaderReady", function() {
      adconsent.gdpr('getCustomVendorsConsent', Object.keys(vendor), function (consent) {
        Object.keys(vendor).forEach(key => {
          if (consent[key] && vendor[key] && vendor[key].enabled) {
            vendor[key].init();
          }
        });
      });

      __tcfapi('addEventListener', 2, function (tcData, success) {
        if (success && (tcData.eventStatus === 'tcloaded' || tcData.eventStatus === 'useractioncomplete')) {
          window.NP_enableScript();

          if (!tcData.gdprApplies || tcData.purpose.consents[1]) {
            gtag('consent', 'update', {
              'ad_storage': 'granted',
              'analytics_storage': 'granted'
            });
            Object.values(vendor).forEach(v => {
              if (v.enabled) {
                v.init();
              }
            });
          } else {
            gtag('consent', 'update', {
              'ad_storage': 'denied',
              'analytics_storage': 'denied'
            })
          }
        }
      })

      adngin.queue.push(function() {
        window.adQueue.forEach(function (cb) {
          if (typeof cb === 'function') {
            cb();
          }
        })
        window.adQueue = adngin.queue;
      });
    });
