      <section id = "paragraph-id--230330"   class="section entity entity-paragraphs-item paragraph paragraph--type--text-passage paragraph--view-mode--default bg--white padding-none"
    >
      <article  class="content content-container">
              






  
                    
                          






      <div class="">
    <script>//<![CDATA[
  // Add Lytics event to homepage click
  // https://github.com/tableau-mkt/www7/issues/9060
  // https://github.com/tableau-mkt/www/issues/3590
  (function () {
    // jQuery isn't loaded yet, so we'll use vanilla JavaScript
    // Makes sure it doesn't blow up in IE < 9
    if (document.addEventListener) {
      document.addEventListener('DOMContentLoaded', function () {
        var $ = window.jQuery;
        // Listen for clicks on the Analyst links
        $('.datalocation-audience-segment-links a[href^="/tableau-for-analysts"]').click(function onClickAnalyst() {
          // Check to see if window.jstag exists
          if (window.jstag) {
            jstag.send({
              'p13n_segment': 'Analyst'
            });
          }
        });
        // Listen for clicks on the IT links
        $('.datalocation-audience-segment-links a[href^="/tableau-for-it"]').click(function onClickIT() {
          // Check to see if window.jstag exists
          if (window.jstag) {
            jstag.send({
              'p13n_segment': 'IT Decision Maker'
            });
          }
        });
        // Listen for clicks on the Executive links
        $('.datalocation-audience-segment-links a[href^="/tableau-for-executives"]').click(function onClickExecutive() {
          // Check to see if window.jstag exists
          if (window.jstag) {
            jstag.send({
              'p13n_segment': 'Exec'
            });
          }
        });
      });
    }
  })();
  //]]></script>
    </div>
  
            </article>
    </section>
  
        
  </div>
