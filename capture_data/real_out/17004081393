    transition: all 0.2s ease-in-out;
    font-size: 0;
    background-color: rgba(0,0,0,0.9) !important;
    outline: none;
}

.owl-carousel .owl-item.center img {
    width: 100%;
    height: auto;
    max-width: 397px;
}

.owl-carousel .owl-item img {
    width: 100%;
    height: 327px;
    max-width: 100%;
}

.owl-prev:hover, .owl-next:hover {
    opacity: 0.8;
}

.owl-prev {
    right: 0px;
}

.owl-next {
    left: 0px;
    transform: rotate(180deg);
}

button.owl-dot.active {
    width: 18px;
    height: 8px;
}

  .owl-nav.disabled {
      position: static !important;
  }

  @media only screen and (max-width:900px) {
      .owl-item.active.center .item {
          width: 57%;
      }
  }
  @media only screen and (max-width:700px) 
  { .owl-item.active.center .item { width: 66%; } 
  .owl-prev, .owl-next { position: absolute; color: #fff; top: 18.7%; width: 48px !important; height: 185px !important; font-size: 0 !important; background-image: none !important; background-repeat: no-repeat !important; opacity: 0.7; background-position: center !important; transition: all 0.2s ease-in-out; font-size: 0; background-color: rgba(0,0,0,0.9) !important; outline: none; } button.owl-prev:after, button.owl-next:after { content: ''; background-image: url(https://akhbarak.net/assets/arrow.png) !important; position: absolute; top: 0; height: 100%; width: 100%; background-repeat: no-repeat; background-position: center; transform: scale(0.7); transform-origin: center; right: 5px; } .owl-carousel .owl-item.center figure { height: 230px !important; overflow: hidden; } .owl-carousel .owl-item.center img { width: 100%; height: auto !important; margin: 0 auto; } .owl-carousel .owl-item img { width: 100%; height: 230px; max-width: 100%; } .owl-carousel div.owl-item { transform: scale(0.8) translatex(-60px); transform-origin: left; } .owl-item.active.center + div { transform: scale(0.8) translatex(60px); transform-origin: right; } .owl-carousel figure a figcaption p { font-size: 14px; line-height: 25px; } }
</style>

