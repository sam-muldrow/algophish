<script>

    // function to check if cookie exists
    if (typeof getCookies === 'undefined') {
        var getCookies = function (cookieName) {

            var name = cookieName + "=";
            var decodedCookie = encodeURIComponent(document.cookie);
            decodedCookie = decodeURIComponent(decodedCookie);
            var ca = decodedCookie.split(';');

            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return "";
        };
    }

    // first check if dp_regional_pricing cookie set
    var isCookie = getCookies('dp_regional_pricing');

    // if not set it
    if (!isCookie) {
        let initDate = new Date();
        initDate.setTime(initDate.getTime() + (90 * 24 * 60 * 60 * 1000));
        let ckieExpires = "expires=" + initDate.toUTCString();
        let redirectURI = location.protocol + "//" + location.hostname + '/webeng-location/index.php?';
        let request = new XMLHttpRequest();
        request.open('GET', redirectURI + (Math.random() * Date.now()), true);

        request.onload = function () {
            if (request.status >= 200 && request.status < 400) {
                var data = JSON.parse(request.responseText);
                var countryCode = data.country;
                document.cookie = "dp_regional_pricing=" + countryCode + ";" + ckieExpires + ';path=/';
            } else {
                console.log('Notice: - unable to retrieve visitor source country. Defaulting to US site.');
                document.cookie = "dp_regional_pricing=us;" + ckieExpires + ';path=/';
            }
        };
        request.send();
    }


</script>
