<script>var pgFunction = function() {
        const meetingsUrls = {
    'default': 'https://meetings.dialpad.com/signup',
    'www.dialpad.com': 'https://meetings.dialpad.com/signup',
    'www.dialpadbeta.com': 'https://meetings.dialpadbeta.com/signup',
    'dialpad-cms-beta.appspot.com': 'https://meetings.dialpadbeta.com/signup',
    'dialpad-cms-dev.appspot.com': 'https://dialpad-cms-dev.appspot.com/signup',
}

const currentHost = window.location.host
const meetingsUrl = meetingsUrls[currentHost] || meetingsUrls.default

$('.main-nav .cta-item .buttons_wrapper a').attr('href', meetingsUrl);
$('.site-nav .tablet-only-cta-item .buttons_wrapper a').attr('href', meetingsUrl);
    };

        (function defer(method) {
        if (window.jQuery) {
            method();
        } else {
            setTimeout(function() { defer(pgFunction) }, 50);
        }
    })(pgFunction);</script></head>


