        if (isAePlusPage) {
            suffix = '_aePlus';
        }
        if(locale === 'ar_MA')locale = 'ar_SA';
        if(locale === 'iw_IL')locale = 'he_IL';
        if(locale === 'in_ID')locale = 'id_ID';
        var header = document.querySelector('[data-locale="' + locale + suffix + '"]');
        var headerContainer = document.querySelector('.header-container-item')
        var urlParams = new URLSearchParams(window.location.search);
        var isHideHeader = urlParams.get("hideHeader");
        if (isHideHeader === 'true') {
            headerContainer.style.display = 'none';
        }
        if (header) {
            header.style.display = 'block';
            header.removeAttribute('data-temp-header');
        }
        var otherHeader = document.querySelectorAll('[data-locale][data-temp-header]');
        for(var i = 0; i < otherHeader.length;i++) {
            otherHeader[i].parentNode.removeChild(otherHeader[i]);
        }
        window.__locale = locale;
        var name = 'ae-header.css';
        switch(locale) {
            case 'ru_RU':
                name = 'ae-header-ru.css';
                break;
            case 'pt_BR':
                name = 'ae-header-pt.css';
                break;
            case 'es_ES':
                name = 'ae-header-es.css';
                break;
            case 'fr_FR':
                name = 'ae-header-fr.css';
                break;
            case 'id_ID':
            case 'in_ID':
                name = 'ae-header-id.css';
                break;
            case 'he_IL':
            case 'ar_SA':
            case 'iw_IL':
            case 'ar_MA':
                name = 'multilan/ae-header-rtl.css';
                break;
            case 'it_IT':
            case 'de_DE':
            case 'nl_NL':
            case 'tr_TR':
            case 'ja_JP':
            case 'vi_VN':
            case 'ko_KR':
            case 'th_TH':
            case 'pl_PL':
            case 'uk_UA':
                name = 'multilan/ae-header-ltr.css';
                break;
            case 'en_US':
            default:
                name = 'ae-header.css';
        }
        if (name !== 'ae-header.css') {
            var linksrc = '//assets.alicdn.com/g/ae-fe/header-ui/0.0.83/prev/front/' + name;
            var link = document.createElement('link');
            link.type = 'text/css';
            link.rel = 'stylesheet';
            link.href = linksrc;
            header.prepend(link);
        }
        var siteList = {
            en_US: 'glo',
            ru_RU: 'rus',
            es_ES: 'esp',
            fr_FR: 'fra',
            pt_BR: 'bra',
            id_ID: 'glo',
            tr_TR: 'tur',
            de_DE: 'deu',
            it_IT: 'ita',
            th_TH: 'glo',
            he_IL: 'isr',
            ja_JP: 'jpn',
            nl_NL: 'nld',
            vi_VN: 'glo',
            ko_KR: 'kor',
            ar_SA: 'ara',
            pl_PL: 'glo',
            uk_UA: 'glo',
        };
        window.headerConfig = {
          locale: locale,
          site: siteList[locale],
        };

      if(isAePlusPage) {
        var record, spm_ab, spm, aePlusSearchForm, aePlusSearchInput, aePlusSearchInputValue;

        aePlusSearchForm = document.querySelector(".aePlus-searchBar #form-searchbar");
        aePlusSearchInput = document.querySelector(".aePlus-searchBar #search-key");
        aePlusSearchInputValue = "";

        aePlusSearchInput && aePlusSearchInput.addEventListener && aePlusSearchInput.addEventListener("change", function(e) {aePlusSearchInputValue = e.target.value});

        aePlusSearchForm && aePlusSearchForm.addEventListener && aePlusSearchForm.addEventListener("submit", function () {
          record = window.goldlog && window.goldlog.record || function() {};
          spm_ab = window.goldlog && window.goldlog.spm_ab || [];
          spm_ab = spm_ab.join ? spm_ab.join('.') : '';
          spm = spm_ab + '.search';

          record(
            '/ae.pc_click.statweb_ae_click',
            'CLK',
            'ae_button_type=search_button&ae_object_value=keyword=' + aePlusSearchInputValue + '&type=direct' + "&spm=" + spm,
            'POST'
          );
        }, false);
      }
    }());
    // 侧边栏
    (function(){
        var saleHomeContent = {
            en_US: "Sale Home",
            ru_RU: "Главная распродажи",
            pt_BR: "Página Inicial",
            es_ES: "Portada de la promoción",
            fr_FR: "Accueil promotion",
            id_ID: "Beranda Obral",
            tr_TR: "Ev İndirimi",
            th_TH: "การขายในประเทศ",
            it_IT: "Pagina promo",
            de_DE: "Startseite Ausverkauf",
            he_IL: "עמוד הבית של המבצע",
            ja_JP: "販売ホーム",
            ko_KR: "세일 홈",
            nl_NL: "Startpagina uitverkoop",
            vi_VN: "Trang chủ khuyến mãi",
            ar_SA: "الصفحة الرئيسية للتخفيضات",
            pl_PL: "Wyprzedaż Strona główna",
            uk_UA: "Розпродаж товарів для дому",
        }
        var sidebarConfig = window.__sidebarConfig || {};
        // 获取服务器时间
        var now = (new Date()).getTime();
        var serverTime =  Number('1700634323195');
        if( !isNaN(serverTime) ) {
            now = (new Date(serverTime)).getTime();
        }
        // 获取侧边栏上下线时间
        var onlineTime = Number(sidebarConfig.onlineTime);
        var offlineTime = Number(sidebarConfig.offlineTime);
        // 如果不在上线时间内isShowSidebar置为false
        var isShowSidebar = sidebarConfig.isShowSidebar || false;
        if( isNaN(onlineTime) || isNaN(offlineTime)){
            isShowSidebar = false;
        }
        if( now > offlineTime || now < onlineTime) {
            isShowSidebar = false;
        }
        window.AE_SITE_LANGUAGE_CODE = (window.__locale || 'en_US').toLowerCase().replace('_', '-');
        window.globalSiteNormalSidebarConfig = {
            isShowSidebar: isShowSidebar || false,
            isPromotion: sidebarConfig.isPromotion || false,
            isShowSellerCoupon: sidebarConfig.isShowSellerCoupon || false,
            tmallVenueUrl: sidebarConfig.tmallVenueUrl || '',
            tmallTitle: sidebarConfig.tmallTitle || '',
            plazaVenueUrl: sidebarConfig.plazaVenueUrl || '',
            plazaTitle: sidebarConfig.plazaTitle || '',
            mainVenueUrl: sidebarConfig.mainVenueUrl || '',
            promotionTitle: saleHomeContent[window.__locale || 'en_US'],
            promotionBg: sidebarConfig.promotionBg || '',
            promotionLogo: sidebarConfig.promotionLogo || '',
            newUserUrl: sidebarConfig.newUserUrl || '',
            widgetId: sidebarConfig.widgetId || 7533591,
        }
        if (isShowSidebar) {
            var fragment = document.createDocumentFragment();
            var linkSrc = '//assets.alicdn.com/g/gcp/image-assets/0.0.2/css/ae-channel-ui/2.0.0-material-public-bigsale/sidebar.css';
            var link = document.createElement('link');
            link.type = 'text/css';
            link.rel = 'stylesheet';
            link.href = linkSrc;
            var scriptSrc = '//assets.alicdn.com/g/gcp/image-assets/0.0.2/js/ae-channel-ui/2.0.0-material-public-bigsale/promotion-sibebar.js';
            var script = document.createElement('script');
            script.type = 'text/javascript';
            script.crossorigin = 'anonymous';
            script.defer = 'defer';
            script.src = scriptSrc;
            fragment.append(link);
            fragment.append(script);
            header.prepend(fragment);
        }
    }());
</script>





