<ck-pay-pal-messages-init class="custom-element ck-pay-pal-messages-init" data-qa="component ck-pay-pal-messages-init" client-id="AeIs5osBiqCfCs4wfQV1wTylLWV9-Wcn8ENuoUDTzfX7sFA22Kc2jnO7WQ7r09AR7QKVQlDRq-SBNPoz" currency-iso-code="GBP">
