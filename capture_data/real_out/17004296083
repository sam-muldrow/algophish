                    <script data-react-helmet="true" >
            const __llbeanRecaptchaSrc = "https://www.google.com/recaptcha/enterprise.js?render=6Ld17ycoAAAAADnWlKyMyaS5ULg8FOOzzIF23f2P";

            function __llbeanLogInitRecaptchaError() {
              try {
                fetch("/api/udal/client-logger/log/FEA-RECAPTCHA-ENTERPRISE::INIT-ERROR", {
                  method: "POST",
                });
              } catch (e) {
                console.error("Failed to log init recaptcha error", e);
              }
            }

            function __llbeanInjectRecaptchaScript(onErrorCallback) {
              const scriptId = 'recaptcha-enterprise-script-retry';
              const existingScript = document.getElementById(scriptId);
              if (existingScript) {
                existingScript.remove();
              }

              const script = document.createElement('script');
              script.id = scriptId;
              script.onerror = onErrorCallback;
              script.src = __llbeanRecaptchaSrc;
              document.body.appendChild(script);
            }

            function __llbeanInitRecaptchaLogFailure() {
              __llbeanLogInitRecaptchaError()
            }

            function __llbeanInitRecaptchaRetryThird() {
              setTimeout(() => {
                __llbeanInjectRecaptchaScript(__llbeanInitRecaptchaLogFailure);
              }, 10000);
            }

            function __llbeanInitRecaptchaRetrySecond() {
              setTimeout(() => {
                __llbeanInjectRecaptchaScript(__llbeanInitRecaptchaRetryThird);
              }, 5000);
            }

            function __llbeanInitRecaptchaRetryFirst() {
              const ready = fn => document.readyState !== 'loading' ? fn() : document.addEventListener('DOMContentLoaded', fn);
              ready(() => {
                __llbeanInjectRecaptchaScript(__llbeanInitRecaptchaRetrySecond);
              });
            }
          </script><script data-react-helmet="true" id="recaptcha-enterprise-script" src="https://www.google.com/recaptcha/enterprise.js?render=6Ld17ycoAAAAADnWlKyMyaS5ULg8FOOzzIF23f2P" onError="__llbeanInitRecaptchaRetryFirst()" async="true"></script>
                    
