<script>



$(document).ready(function(){
	if($('#widget-8').length)
	{
		var sidebartop = $('#widget-8').closest('div.section').offset().top;
		var sidebarheight = $('#widget-8').closest('div.section').height();
		$(window).scroll(function(){
			if($(window).scrollTop() > sidebartop && sidebarheight < $('#content div.mainContainer').height())
			{
				var top = 5 + Math.min($(document).height() - $(window).scrollTop() - sidebarheight - 200, 0);
				$('#widget-8').closest('div.section').css({position: 'fixed', top: top+'px'});
			}
			else
			{
				$('#widget-8').closest('div.section').css({position: 'static', top: 0});
			}
		});
	}
})
