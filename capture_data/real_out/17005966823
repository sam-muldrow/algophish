  <script type="text/javascript">
    var ua = window.navigator.userAgent.toLowerCase();
    var isIE = (ua.indexOf('msie') >= 0 || ua.indexOf('trident') >= 0);
    var isIE11 = false;
    if (isIE) {
      array = /(msie|rv:?)\s?([\d\.]+)/.exec(ua);
      ieVer = (array) ? array[2] : '';
      if(ieVer == '11.0'){
        isIE11 = true;
      }
    }
  </script>

