<script>
$(function(){

    if(getCookie('testserver')!=null){
      //document.cookie = "testserver=wt1; max-age=0";
    }

    function mmrRegister(email){

      $.post('/list/personal/mailmagazine/post.json',{email:email}
      ).done(function(json){
        if (json.status=='OK'){
          $('#mmrInit').toggleClass('mmr_hidden');
           $('#mmrComplete').toggleClass('mmr_hidden');
        }else{
          //
        }
      }).fail(function(err){
        //
      });
    }

    $(document).on('click', '#mmrRegisterBtn', function(){
      
      var mmr_regexp = /^[A-Za-z0-9]{1}[A-Za-z0-9\_.+-]*@{1}[A-Za-z0-9_.-]{1,}\.[A-Za-z0-9]{1,}$/;
      var mmr_email = $('#mmrEmail').val();
      $('#mmrError').empty();
      if (!mmr_email || mmr_email.length==0) {
        $('#mmrError').text('メールアドレスが未入力です。');
        $('#mmrEmail').addClass('input_error');
      }else if (!mmr_regexp.test(mmr_email)) {
        $('#mmrError').text('メールアドレスが正しくありません。');
        $('#mmrEmail').addClass('input_error');
      }else{
        mmrRegister(mmr_email);
      }

    })
    
    $(document).on('focus', '#mmrEmail', function(){
      if($('#mmrEmail').hasClass('input_error')){
        $('#mmrEmail').toggleClass('input_error');
        $('#mmrError').empty();
      }
    })

})
</script>



