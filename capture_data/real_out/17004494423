    <script language="javasript" type="text/javascript">
        IwAG.$(window).on('pageshow', function () {
            IwAG.$('#search-tab-btn-2').prop('checked', true);
        });

        IwAG.$('#search-tab-btn-1').click(function () {
            IwAG.$('#immo-search').hide();
            IwAG.$('#pricemap').show();
        });
        IwAG.$('#search-tab-btn-2').click(function () {
            IwAG.$('#immo-search').show();
            IwAG.$('#pricemap').hide();
        });

        IwAG.$('input[type=radio][name=search-map]').change(function () {
            var selectedValue = IwAG.$(this).attr('data-name');
            IwAG.$('#etype_esr').val(selectedValue);
        });

        function goToSite() {
            var link = "/immobilienpreise/deutschland";
            var evtLabel = "price_map";

            if (document.getElementById('estate_rating').checked) {
                link = "/anbieten/immobilienbewertung";
                evtLabel = "seller_estimation";
            }

            utag.link({ "event_category": "button-intern", "event_action": "click", "event_label": evtLabel });
            window.location.href = link;
        }
    </script>
