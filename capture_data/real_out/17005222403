<script>
var userChangeLocation = (function(params) {
var geoPlaces = {};

const getGeoPlaces = () => geoPlaces;

const setGeoPlaces = (_geoPlaces) => {
geoPlaces = _geoPlaces;
return true;
}

const fetchLocation = (data, useFormattedAddress) => {
let geoPlaces = {};
data.address_components.forEach(function(value, index, arr) {
if ($.inArray('route', value.types) > -1) {
geoPlaces.route = value.long_name;
} else if ($.inArray('sublocality_level_1', value.types) > -1) {
geoPlaces.subloca = value.long_name;
} else if ($.inArray('locality', value.types) > -1) {
geoPlaces.city = value.long_name;
} else if ($.inArray('postal_town', value.types) > -1) {
geoPlaces.town = value.long_name;
} else if ($.inArray('administrative_area_level_1', value.types) > -1) {
geoPlaces.state = value.long_name;
geoPlaces.stateshort = value.short_name;
} else if ($.inArray('country', value.types) > -1) {
geoPlaces.country = value.long_name;
geoPlaces.countryCode = value.short_name;
}
});

geoPlaces.lat = data.geometry.location.lat();
geoPlaces.lng = data.geometry.location.lng();
geoPlaces.latlng = geoPlaces.lat + ',' + geoPlaces.lng;

if (!geoPlaces.city && geoPlaces.town) {
geoPlaces.city = geoPlaces.town;
}

if (!geoPlaces.city && geoPlaces.subloca) {
geoPlaces.city = geoPlaces.subloca;
}

if (!geoPlaces.city && geoPlaces.state) {
geoPlaces.city = geoPlaces.state;
}

let shortAddress = '';
let shortAddressNames = geoPlaces.city && geoPlaces.subloca && geoPlaces.city != geoPlaces.subloca ? ['subloca', 'city'] : ['city'];
if (geoPlaces.city && geoPlaces.stateshort && geoPlaces.city != geoPlaces.stateshort) {
shortAddressNames.push('stateshort');
} 
shortAddressNames.push('country');

var shortAddressArr = [];
shortAddressNames.forEach(function(name){
for(const key in geoPlaces) {
if(key == name && geoPlaces[key] != '') {
shortAddressArr.push(geoPlaces[key]);
}
}
});

shortAddress = shortAddressArr.join(', ').toString();
geoPlaces.address = useFormattedAddress ? data.formatted_address : shortAddress;

return geoPlaces;
};

const processGeoData = (data) => {
let geoPlaces = data;
if (typeof geoPlaces.lat !== 'string' && typeof geoPlaces.lat !== 'number') geoPlaces.lat = '';
if (typeof geoPlaces.lng !== 'string' && typeof geoPlaces.lat !== 'number') geoPlaces.lng = '';
if (typeof geoPlaces.address !== 'string') geoPlaces.address = '';
if (typeof geoPlaces.country !== 'string') geoPlaces.country = '';
if (typeof geoPlaces.countryCode !== 'string') geoPlaces.countryCode = '';
if (typeof geoPlaces.city !== 'string') geoPlaces.city = '';

return {
lat_F: geoPlaces.lat,
lng_F: geoPlaces.lng,
address_F: geoPlaces.address,
country_F: geoPlaces.country,
country_code_F: geoPlaces.countryCode,
city_F: geoPlaces.city,
};
}

const updateLocationAttrs = (data) => {
let geoPlaces = data;
const latEl = document.querySelector('.search-location-lat');
const lonEl = document.querySelector('.search-location-lon');
const addEl = document.querySelector('.search-location-address');

latEl && latEl.setAttribute('value', geoPlaces.lat !== null && geoPlaces.lat !== undefined ? geoPlaces.lat : '');
lonEl && lonEl.setAttribute('value', geoPlaces.lng !== null && geoPlaces.lng !== undefined ? geoPlaces.lng : '');
addEl && addEl.setAttribute('value', geoPlaces.address !== null && geoPlaces.address !== undefined ? geoPlaces.address : '');

let searchInput = document.querySelector('.pac-target-input');
if (!!searchInput) {
searchInput.value = geoPlaces.address !== null && geoPlaces.address !== undefined ? geoPlaces.address : '';
searchInput.setAttribute('data-location', JSON.stringify(geoPlaces))
}
}

const updateLocation = (geoPlaces) => {
let geoData = {lat_F: '', lng_F: '',  address_F: '', country_F: '', country_code_F: '', city_F: ''};
if (typeof geoPlaces != 'undefined' && geoPlaces != null) {
geoData = processGeoData(geoPlaces);
}
return new Promise((resolve, reject) => {
$.ajax({
type: 'post',
url: '/ajax/ng/geo/change-location',
data: geoData,
success: function(data) {
resolve(data);
},
error: function (error) {
reject(error);
},
});
})
}

const doUpdate = (data, reload = false) => {
if ($('#modalChangeLocation').length > 0) {
$('#modalChangeLocation').modal('hide');
}

let jsonObj = JSON.parse(data);
if (jsonObj.hasOwnProperty('location')) {
$('.js-location').text(jsonObj.location);
}

if(reload) {
location.reload();
}

if ($('#pjax-offers').length) {
updateFindOfferList({lat: jsonObj.lat, lng: jsonObj.lng});
}
}

const updateFindOfferList = (latLng) => {
let module = ws.getModule('findFilters');
let filters = module.getFilters;
let newFilters = {};
newFilters.Xsort_order = 'd';
newFilters.lat = latLng.lat;
newFilters.lng = latLng.lng;
module.appendFilters(newFilters);
module.forceApplyFilters();
if (['md', 'lg', 'xl'].includes(ws.getCurrentViewport())) {
$('.popover').popover('hide');
ws.helpers().hideBackdrop();
}
}

return {
fetchLocation: fetchLocation,
processGeoData: processGeoData,
updateLocation: updateLocation,
doUpdate: doUpdate,
updateFindOfferList: updateFindOfferList,
updateLocationAttrs: updateLocationAttrs,
};
})();



var geoLocation = (function(params) {
let geoPlaces = {};

const getGeoPlaces = () => geoPlaces;

const setGeoPlaces = (_geoPlaces) => {
geoPlaces = _geoPlaces;
return true;
}

const initGeoData = () => {
if(navigator.geolocation) {
navigator.geolocation.getCurrentPosition(successGeoData, failGeoData);
} else {
console.log('Browser does not support geolocation');
}
}

const initGeoDataNearMe = () => {
if(navigator.geolocation) {
navigator.geolocation.getCurrentPosition(successGeoDataNearMe, failGeoDataNearMe);
} else {
console.log('Browser does not support geolocation');
}
}

const successGeoData = (pos) => {
let crd = pos.coords;
let latLng = {lat:crd.latitude, lng:crd.longitude};
hideErrorMessage();
getLocationFromGeo(latLng)
.then((data) => {
setGeoPlaces(data);
userChangeLocation.updateLocationAttrs(data);
})
.catch((error) => {
console.log(error);
})
}

const failGeoData = (error) => {
showErrorMessage();
}

const getLocationFromGeo = (latLng) => {
let geocoder = new google.maps.Geocoder();
return new Promise(function(resolve, reject) {
geocoder.geocode({'latLng': latLng}, function (results, status) {
if (status == google.maps.GeocoderStatus.OK) {
if (results[0]) {
let geoPlaces = userChangeLocation.fetchLocation(results[0], false);
resolve(geoPlaces);
} else {
console.log('No results found');
}
} else {
reject();
}
});
});
}

const successGeoDataNearMe = (pos) => {
var crd = pos.coords;
let latLng = {lat:crd.latitude, lng:crd.longitude};
getLocationFromGeo(latLng)
.then((data) => {
userChangeLocation.updateLocation(data)
.then((data) => {
let jsonObj = jQuery.parseJSON(data);
if (['md', 'lg', 'xl'].includes(ws.getCurrentViewport())) {
userChangeLocation.updateFindOfferList({lat:jsonObj.lat, lng:jsonObj.lng});
}
})
})
}

const failGeoDataNearMe = (error) => {
userChangeLocation.updateLocation()
.then((data) => {
let jsonObj = jQuery.parseJSON(data);  
if (['md', 'lg', 'xl'].includes(ws.getCurrentViewport())) {
userChangeLocation.updateFindOfferList({lat:jsonObj.lat, lng:jsonObj.lng});
}
$('.js-location').text(data.location);
})
}

const showErrorMessage = () => {
let errEl = document.getElementById('google-map__search-box-error');
if (errEl && errEl.style.display === 'none') {
errEl.style.display = 'block';
}
}

const hideErrorMessage = () => {
let errEl = document.getElementById('google-map__search-box-error');
if (errEl && errEl.style.display === 'block') {
errEl.style.display = 'none';
}
}

return {
getLocationFromGeo: getLocationFromGeo,
initGeoData: initGeoData,
initGeoDataNearMe: initGeoDataNearMe,
showErrorMessage: showErrorMessage,
hideErrorMessage: hideErrorMessage,
};
})();


var googleMapSearchBox = (function (options) {
var settings;
var geoPlaces = {};

const getGeoPlaces = () => geoPlaces;

const setGeoPlaces = (_geoPlaces) => {
geoPlaces = _geoPlaces;
return true;
}

const initAll = (options) => {
let autoComplete = getAutoComplete(options);
autoComplete && autoComplete.addListener('place_changed', () => {placeChangedHandler(autoComplete,true)});
};

const initAutoComplete = (options) => {
let autoComplete = getAutoComplete(options);
autoComplete && autoComplete.addListener('place_changed', () => {placeChangedHandler(autoComplete,false)});
};

const placeChangedHandler = (autoComplete, notDefer) => {
let place = autoComplete.getPlace();
if (!place.geometry) {
return false;
}
let geoPlaces = userChangeLocation.fetchLocation(place, true);
setGeoPlaces(geoPlaces);
userChangeLocation.updateLocationAttrs(geoPlaces);
if(notDefer){
userChangeLocation.updateLocation(geoPlaces)
.then((data) => {
userChangeLocation.doUpdate(data, notDefer);
})
.catch((error) => {
console.log(error);
})
}
};

const getAutoComplete = (options) => {
settings = $.extend({
targetSelector: '.google-map__search-box',
}, options);
const input = $(settings.targetSelector).find('input');
if (!input) {
return null;
}
const autoComplete = new google.maps.places.Autocomplete(input.get(0));
return autoComplete;
}

return {
initAll: initAll,
initAutoComplete: initAutoComplete,
};
})();
