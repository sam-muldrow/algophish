var userLocation = (function() {
const revertGeoPlacesAttr = () => {
let searchBox = document.querySelector('.google-map__search-box');
let geoPlacesOrig = JSON.parse(searchBox.getAttribute('data-location-orig'));
userChangeLocation.updateLocationAttrs(geoPlacesOrig); 
}

const updateGeoPlacesAttr = (geoPlaces) => {
let searchBox = document.querySelector('.google-map__search-box');
searchBox.setAttribute('data-location-orig', JSON.stringify(geoPlaces));
}

const moveAutoCompleteList = () => {
let autoCompleteWrapper = document.querySelector('.pac-container-wrapper');
let autoCompleteList = document.querySelector('.pac-container');
autoCompleteWrapper.appendChild(autoCompleteList);
autoCompleteList.style.width = autoCompleteWrapper.width;
}

const bindEvents = () => {
$('body').off('click', '.google-map__search-box-my-location').on('click', '.google-map__search-box-my-location', function () {
geoLocation.initGeoData(true);
});

$('body').off('click', '.close').on('click', '.close', function () {
revertGeoPlacesAttr();
});

$('body').off('click', '.js-cancel-location').on('click', '.js-cancel-location', function () {
revertGeoPlacesAttr();
});

$('body').off('click', '.js-save-location').on('click', '.js-save-location', function (e) {       
e.preventDefault();
e.stopImmediatePropagation();
const searchInput = document.querySelector('.pac-target-input');
const geoPlaces = JSON.parse(searchInput.getAttribute('data-location'));
userChangeLocation.updateLocation(geoPlaces)
.then((data) => {
userChangeLocation.doUpdate(data);
updateGeoPlacesAttr(geoPlaces);
})
.catch((error) => {
//console.log(error);
})
});

$('body').off('click', '.google-map__search-box input').on('click', '.google-map__search-box input', function() {
geoLocation.hideErrorMessage();
moveAutoCompleteList();
});

$('body').off('keydown', '.google-map__search-box input').on('keydown', '.google-map__search-box input', function(event) {
geoLocation.hideErrorMessage();
if(event.keyCode == 13) {
event.preventDefault();
return false;
}
moveAutoCompleteList();
});

$('body').off('hide.bs.modal', '#modalChangeLocation').on('hide.bs.modal', '#modalChangeLocation', function () {
geoLocation.hideErrorMessage();
const autoCompleteList = document.querySelector('.pac-container');
autoCompleteList && autoCompleteList.remove();
});

$('body').off('show.bs.modal', '#modalChangeLocation').on('show.bs.modal', '#modalChangeLocation', function () {
if ('google' in window && typeof google === 'object' && typeof google.maps === 'object') {
googleMapSearchBox.initAutoComplete();
}
});

$('body').off('click', '.js-location').on('click', '.js-location', function(e) {
if ('google' in window && typeof google === 'object' && typeof google.maps === 'object') {
} else {
let apiUrl = 'https://maps.googleapis.com/maps/api/js?key=AIzaSyA-r8QIZeAbsCTUYFYK-1Q0IdMN-Uu_66E&libraries=places&language=en&callback=googleMapSearchBox.initAutoComplete';
