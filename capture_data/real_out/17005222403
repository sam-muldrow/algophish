$('.ws-popover').addClass('mobile-dropdown-height');
$('.ws-popover__content').addClass('mobile-dropdown-height');
$('.filter-box__header').addClass('mobile-dropdown-hide');
$('.filter-box__footer').addClass('mobile-dropdown-hide');
$('.filter-box__body').addClass('mobile-dropdown-max-height');
}
}
dsDropdown.callbacks.dropdownClose = function() {
if (ws.isMobile() == true) {
$('.ws-popover').removeClass('mobile-dropdown-height');
$('.ws-popover__content').removeClass('mobile-dropdown-height');
$('.filter-box__header').removeClass('mobile-dropdown-hide');
$('.filter-box__footer').removeClass('mobile-dropdown-hide');
$('.filter-box__body').removeClass('mobile-dropdown-max-height');
}
