<script>
    var callback = function (response) {
        if (response && response.access && response.access.granted) {
			fetch("/api/piano/access/check", {
                headers: {
                    "content-type": "application/json"
                },
                method: "post",
                credentials: "include",
                body: JSON.stringify(response.access)
            }).then(function (response) {
                if (response.status == 200) {
                    response.json().then(function (data) {
                        if (data && data.status) {
                            document.location.href = document.location.href + (document.location.href.indexOf("?") < 0 ? "?" : "&") + "reloaded&rnd=" + Math.random();
                            //document.location.href = document.location.protocol + "//" + document.location.hostname + document.location.pathname
                        }
                    });
                }
            });
        }
    };
    if (document.location.search.indexOf("reloaded") < 0) {
        tp.push(["init", function () {
            if (userOnPage.userGuid && !userOnPage.isSubscriber)
                tp.api.callApi("/access/check", tp_params, callback);
        }]);
    }
</script>

