    if( window.location.hostname.indexOf('publico.pt') != -1 ) {
  var deferredScripts = window.deferredScripts || [];
  deferredScripts.push(function(){
    $(".box-carousel").hide();
    $.getScript({
      url: "https://static.publicocdn.com/files/site/js/plugins/slick.min.js",
      cache: true
    }).done(function(){
        $('.carousel-content').slick({
        slidesToShow: 1,
        slidesToScroll: 1,
        dots: true,
        arrow: true,
        autoplay: true,
        autoplaySpeed: 6000,
        responsive: [
          {
          breakpoint: 767,
          settings: {
          slidesToShow: 1,
          slidesToScroll: 1,
          autoplay: true,
          autoplaySpeed: 6000,
          dots: true
          }
          }
        ]
      });
       $('.carousel-content').slick('slickUnfilter');
       var userclass='.anonymous';
        if (userOnPage.isSubscriber) {
              userclass='.subscriber';
        } else {
            if (userOnPage.isSubscriber === undefined) {
                userclass='.anonymous'
            } else
            {
              userclass='.register';
            }
        }
       $('.carousel-content').slick('slickFilter', userclass);
       var slidescount=$(".carousel-content").slick("getSlick").slideCount;
       if (slidescount>0){
          $('.carousel-content').slick('slickGoTo', Math.floor(Math.random() * slidescount));
          console.log(Math.floor(Math.random() * slidescount))
          $(".box-carousel").show();
       };
    });
  });
} else {
  $(function(){
