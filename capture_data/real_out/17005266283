	<script type="text/javascript">

		function findValue(li) {
			if (li == null) return alert("No match!");
			$("#keywords").val(li.extra[0]);
		}

		function showSegmentMenu(speed)
		{
			if ($(window).width() >= 1322)
			{
				$(".door_left").show(speed);
				$(".segment").hide(speed);
			}
			else
			{
				$(".segment").show(speed);
				$(".door_left").hide(speed);
			}
		}

		$(document).ready(function() {
			showSegmentMenu(0);

			//$('#bottomad').scrollToFixed( {
			//	bottom: 0,
			//	limit: $('#bottomad').offset().top
			//});

			$(window).scroll(function() {
				$(window).scrollTop() > 10 ? $('#totop').fadeIn(400) : $('#totop').fadeOut(400);
			});

			$('#totop').click(function(event) {
				event.preventDefault();
				$('body').animate({
					scrollTop: 0
				});
				window.scrollTo(0, 0);
			}).hide();

			$('.topstory_nav span').click(function() {
				$('.focus .container > div').removeClass('active');
				$('.topstory_nav span').removeClass('selected');
				$('#' + $(this).attr('class').split(' ', 1)).addClass('active');
				$(this).addClass('selected');
			});

			var timeoutID = window.setTimeout(function() {
				if ($('.topstory_nav span.planning').length) {
					$('.topstory_nav span.channel_one').click();
				}
			}, 18 * 1000);			

			var r = Math.floor(Math.random() * $('.feature .container .top_focus').length);
			$('.feature .container .top_focus:eq(' + r + ')').addClass('active');
			$('.feature_tab span:eq(' + r + ')').addClass('selected');
			$('.feature_tab span').click(function() {
				$('.feature .container > div').removeClass('active');
				$('.feature_tab span').removeClass('selected');
				$('#' + $(this).attr('class')).addClass('active');
				$(this).addClass('selected');
			});
			
			var tabContainers = $('div.side_sale > div');
			tabContainers.hide().filter(':first').show();
			
			$('div.side_sale ul.topnav a').click(function () {
				tabContainers.hide();
				tabContainers.filter(this.hash).show();
				$('div.side_sale ul.topnav a').removeClass('selected');
				$(this).addClass('selected');
				return false;
			}).filter(':first').click();

			$('#keywords').autocomplete('search_autocomplete_ajax.asp', {
				delay: 10,
				width: $('#keywords').innerWidth(),
				minChars: 1,
				matchSubset: false,
				matchContains: false,
				cacheLength: 0, 
				noCache: true,
				onItemSelect: findValue,
				onFindValue: findValue,
				formatItem: function(row) {
					return '<div style="height: 12px;"><div style="float: left;">' + row[0] + '</div></div>';
				},
				autoFill: false,
				mustMatch: false, 
				scroll: false
			});
			
			$('#upgrade').click(function() {
				$(this).hide();
			});

			$(window).resize(function(){
				showSegmentMenu(400);
			});

			//$("#abc").load("usedcar_abc.asp");
		});
	</script>
