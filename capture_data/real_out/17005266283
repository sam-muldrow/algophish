<script type="text/javascript">
$(function() {
	$('#car-make').on('change', function(event) {
		event.preventDefault();

		var make = $(this).val();

		if (make !== '') {
			$("#car-model").empty();
			$('#car-model').append($('<option></option>').val('0').text('車款'));
			$("#car-trim").empty();
			$('#car-trim').append($('<option></option>').val('0').text('車型'));

			$.getJSON("https://news.u-car.com.tw/am/tyre/getmodelyearsbymake?makeid=" + make, function(data) {
				$.each(data, function (i, item)
				{
					$('#car-model').append($('<option></option>').val(item.id).text(item.model + '(' + item.year + ')'));
					//$('#car-model').append('<option value="' + data.id + '">' + data.model + '(' + data.year + ')</option>');
				});    
			});
		}
	});

	$('#car-model').on('change', function(event) {
		event.preventDefault();

		var model = $(this).val();

		if (model !== '') {
			$("#car-trim").empty();
			$('#car-trim').append($('<option></option>').val('0').text('車型'));

			$.getJSON("https://news.u-car.com.tw/am/tyre/gettrims?modelid=" + model, function(data) {
				$.each(data, function (i, item)
				{
					$('#car-trim').append($('<option></option>').val(item.id).text(item.trim));
					//$('#car-model').append('<option value="' + data.id + '">' + data.trim + '</option>');
				});    
			});
		}
	});

	$('#search-by-trim').on('submit', function(event) {
		if (!event.target.checkValidity()) {
			event.preventDefault();
			alert('請選取一個車型！');
		}
	});

	$('#search-by-spec').on('submit', function(event) {
		if (!event.target.checkValidity()) {
			event.preventDefault();
			alert('請填寫完整規格！');
		}
	});

	$('#search-by-brand').on('submit', function(event) {
		if (!event.target.checkValidity()) {
			event.preventDefault();
			alert('請選取一個品牌！');
		}
	});
});
</script>

