<script type="text/javascript">
	$('#make').change(function(){
		var makeid = $.trim($('#make option:selected').val());
		$('#model').empty().append($('<option></option>').val('0').text('選擇車款'));
		$.getJSON("searchbymakemodel_ajax.asp?makeid=" + makeid, function(data) {
			$.each(data, function (i, item)
			{
				$('#model').append($('<option></option>').val(item.modelid).text(item.model));
			});    
		});
	});
	$('#segmentid').change(function(){
		// $('#segmentid').parents('form').submit();
		if($(this).val()!=''){
			location.href=$(this).val();
		}
	});
