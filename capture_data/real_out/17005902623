<script type="text/javascript">
jQuery(document).ready(function($){
	if(document.getElementById('campaign_list_r')){
	$('.selection').each(function(){
		var txt = $(this).html();
		$(this).html(txt.replace(/measurement_csv_cp/g,'measurement_top_cp'));
	});
	}
	else {}

	var arr = [];
	$('#whatsnew_list_shop dl.shop').each(function() {
		arr.push($(this).html());
	});
	$('#whatsnew_list_shop').empty();
	for(i=0; i < 4; i++) {
		$('#whatsnew_list_shop').append('<dl class="shop" style="display: block;">' + arr[i] + '</dl>');
	}

	var arr2 = [];
	$('#whatsnew_list_info dl.info').each(function() {
		arr2.push($(this).html());
	});
	$('#whatsnew_list_info').empty();
	for(i=0; i < 4; i++) {
		$('#whatsnew_list_info').append('<dl class="info" style="display: block;">' + arr2[i] + '</dl>');
	}
});
</script>

