<script>
	/***  Apply with LinkedIn  ***/
	if(jQuery('#event_registration_form').length) {
		onLinkedInLoad();
	}

	/* Linked In */
	function onLinkedInLoad() { /* This one!*/
		IN.Event.on(IN, "auth", onLinkedInAuth);
	}

	function onLinkedInAuth() {
	  IN.API.Profile("me").fields(["firstName","lastName","emailAddress","phone-numbers"])
		.result( function(me) {
		//	console.log(me);
		//	var id = me.values[0].id;
			var firstName = me.values[0].firstName;
			var lastName = me.values[0].lastName;
			var emailAddress = me.values[0].emailAddress;
			var phoneNumber = me.values[0].phoneNumbers.values[0].phoneNumber;

			linktriger(firstName,lastName,emailAddress,phoneNumber);

		});

	}

	function linktriger(firstName,lastName,emailAddress,phoneNumber){
		jQuery('.linkedinprofile').click(function(){
			jQuery('#first_name').val(firstName);
			jQuery('#last_name').val(lastName);
			jQuery('#email').val(emailAddress);
			jQuery('#phone').val(phoneNumber);
		});
	}

</script>

