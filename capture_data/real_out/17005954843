    <script>
  (function () {
    function bypassableLoginWallSuccessfulLoginTracking(window) {
      function getCookieValue(name) {
        var cookiesExploded = document.cookie.split(';');

        for (var i = 0; i < cookiesExploded.length; i++) {
          var cookieParts = cookiesExploded[i].split('=', 2);
          var cookieName = cookieParts[0].replace(/^\s+|\s+$/g, ''); // trim whitespaces
          var cookieValue = cookieParts[1];

          if (cookieName === name) {
            return decodeURIComponent(cookieValue);
          }
        }

        return '';
      }

      function deleteCookie(name) {
        document.cookie = name + '=; expires=Thu, 01 Jan 1970 00:00:01 GMT;';
      }

      var cookieName = 'paywall-redirect-state';
      var cookieValueParts = getCookieValue(cookieName).split(':');

      if (cookieValueParts[0] === 'bypassableLoginWall') {
        deleteCookie(cookieName);

        var ALLOWED_VARIANTS = ['CONTINUE_AS', 'INPUT'];
        var variant = cookieValueParts[1];

        if (ALLOWED_VARIANTS.indexOf(variant) >= 0) {
          window.addPulseExperiment(
            'bypassable-login-wall-success',
            'bypassable-login-wall-' + variant,
          );
        }
      }
    }

    bypassableLoginWallSuccessfulLoginTracking(window);
  })();
</script>
  
