<!-- URL Tracking -->
<script>document.addEventListener('DOMContentLoaded', function() {
    var query = (window.location != window.parent.location) ? document.referrer.split('?')[1] || '' : window.location.search.split('?')[1]|| '';
      var buttons = document.querySelectorAll('a.btn-cta, a.btn, .btn-cta a');

 

      if(query) {
        localStorage.setItem('ab_cref', query);
        setUrlParams();
      } else {
        query = localStorage.getItem('ab_cref');
        setUrlParams();
      }

 

      function setUrlParams() {
        for(var i = 0; i < buttons.length; i++) {
          var current = buttons[i];
          var href = current.href;
          if(href.indexOf('?') > 0) {
            href += '&' + query;
          } else {
            href += '?' + query;
          }
          current.setAttribute('href', href);
        }
      }

 

    });</script>
<!-- End URL Tracking -->

