<div id="channels_name_output"></div>
<div class="channels-scrollbar">
<div id="channels-scrollbar">
<div class="channels-output" id="channels_output">
</div>
</div>
</div>
<p class="text-right">
<a title="Всички канали" href="/sled5/tv/" class="button">Всички канали &gt;</a>
</p>
<script type="text/javascript">
    $(function(){
        
        var channels_scrollbar = $('#channels-scrollbar'),
            selected_channels_name = $('#channels_name_output'),
            selected_channels_container = $('#channels_output'),
            channel_id,
            channel_name,
            channel_position,
            tvsVal = $.cookie('tvprogs'),
            tvs;
            
        if (tvsVal) {
            tvs = tvsVal.split(',');
        
        } else {
            tvs = ['93', '116', '99'];
        }
        
        $('.pick-channel .text').on('click', function(){
            $('.pick-channel').toggleClass('dropdown-open');
        });
        
        $('.pick-channel').on('clickoutside', function(e){
            $('.pick-channel').removeClass('dropdown-open');
        });
        
        function show_channel(channel_id, channel_name){
            var path = window.location.pathname,
                devEnv = path.indexOf('/app_dev.php') === 0,
                pathRoot = devEnv ? '/app_dev.php/' : '/';
            
            $.get(pathRoot + 'ajax/tv/' + channel_id + '/kare/', function(data){
                
                if ($('#channel-' + channel_id).length) {
                    $('#channel-' + channel_id).html(data);
                
                } else {
                    selected_channels_container.append('<div id="channel-'+ channel_id +'">'+ data +'</div>');
                }
                
                channel_position = ($('#channel-' + channel_id).offset().top - selected_channels_container.offset().top + 12).toFixed(0);
                
                selected_channels_name.append('<div id="channel-name-'+ channel_id +'" style="top: '+ channel_position +'px;">'+ channel_name +'</div>');
                
                setTimeout(function(){
                    if ($('.tvNow').length > 0) {
                        channels_scrollbar.mCustomScrollbar('scrollTo', $(document).find('.tvNow:first').position().left);
                    }
                }, 100);
            });
        }
        
        $(document).on('click', 'input[name="channels[]"]', function(e){
        
            var channel_id = this.value,
                channel_name = $(this).data('channel-name');
            
            if( $('input[name="channels[]"]:checked').length > 3 ) {
                
                alert('Можете да изберете до три канала.');
                e.preventDefault();
                
            } else {
                
                if( $(this).is(':checked') ){
                    
                    show_channel(channel_id, channel_name);
                    
                    $('.pick-channel').removeClass('dropdown-open');
                    
                    tvs.push(channel_id);
                    
                } else {
                
                    selected_channels_name.find('#channel-name-' + channel_id).remove();
                    selected_channels_container.find('#channel-' + channel_id).remove().promise().done(function(){
                        
                        $('div', selected_channels_name).each(function(){
                        
                            var channel_name = $(this);
                            
                            channel_position = ($('#' + channel_name.prop('id').replace('-name', '')).offset().top - selected_channels_container.offset().top + 12).toFixed(0);
                            
                            channel_name.css({ 'top' : channel_position + 'px' });
                        });
                    
                    });
                    
                    tvs = $.grep(tvs, function(id, i) {
                        return id != channel_id;
                    });
                }
                
                tvsVal = tvs.join(',');
                    
                if (tvsVal) {
                    $.cookie('tvprogs', tvsVal, { expires: 365, path: '/' });
                }
            }
        });
        
        $('input[name="channels[]"]:checked').each(function(i, e){
            show_channel( $(e).val(), $(e).data('channel-name') );
        });
        
        channels_scrollbar.mCustomScrollbar({
            axis: 'x',
            advanced:{
                autoExpandHorizontalScroll: true
            }
        });
    });
</script>
</div>
</section>
