</div>
</div>
</div>
<div class="video-summary">
<meta>
<h3><a href="/kakvo-stava/2023/11/21/4556245_kakvo_shte_stava_v_zeleno_dvijenie_razgovor_s_toma/">Какво става със &quot;Зелено движение&quot;: Разговор с новите съпредседатели Тома Белев и Даниела Божинова</a></h3>
<meta>
<meta>
<div class="article-tools">
<time datetime="2023-11-21T16:11">16:11, 21 ное 23</time> / <a href="/kakvo-stava/">Какво става</a>
<a href="/kakvo-stava/2023/11/21/4556245_kakvo_shte_stava_v_zeleno_dvijenie_razgovor_s_toma/comments">
</a>
</div>
<span>
<a href="/kakvo-stava/2023/11/21/4556245_kakvo_shte_stava_v_zeleno_dvijenie_razgovor_s_toma/comments" class="i-com"><span>8</span></a>
</span>
</div>
</div>
<script>
    $(function () {
        $(document).on('click', '#video-grid-scrollbar .video-box a', function () {

            var $item = $(this),
                playerCont = $(this).parents('.video-player'), videoType,
                player, playerHtml,
                shareBox = $(this).parents('.video-grid-container-v2').find('.share-buttons'),
                shareBoxHTML = shareBox.html(),
                socials = ['facebook', 'twitter', 'viber', 'google-plus', 'linkedin', 'email'];


            if ($item.is('.in-progress') || typeof($item.data('title')) == 'undefined') return;

            $('#video-grid-scrollbar .video-box a').removeClass('in-progress');
            $item.addClass('in-progress');

            $('#video-grid-scrollbar .video-box').removeClass('active');
            $item.parents('.video-box').addClass('active');

            var videoEmbedCode = $(this).data('embedcode');
            var nativeAdLabel = $(this).data('videolabel');
            var nativeAdAuthor = $(this).data('videoauthor');

            if (videoEmbedCode) {
                playerHtml = atob(videoEmbedCode);
            } else {

                if ($item.data('type')) {
                    switch ($item.data('type')) {
                        case 'flv':
                            videoType = 'video/flv';
                            break;
                        case 'mp4':
                            videoType = 'video/mp4';
                            break;
                    }
                }

                if ($('.general-video-box').find('.video-js')[0] && typeof(videoType) != 'undefined') {
                    player = videojs($('.general-video-box').find('.video-js')[0]);
                    player.dispose();

                    playerHtml = '<video id="video-js" class="video-js vjs-big-play-centered" ' +
                        'controls muted  autoplay preload="auto" width="600" height="300" ' +
                        'poster="' + $item.data('poster') + '"' +
                        'data-mediaid="' + $item.data('id') + '"' +
                        'data-setup=\'{"autoplay": true, "defaultMuted":true}\'>' +
                        '<source src="' + $item.data('video') + '" type="' + videoType + '" />' +
                        '</video>';
                }
            }

            $('.native-ad-label').remove();

                            $(this).parents('.video-grid-container, .video-grid-container-v2')
                    .find('.general-video-box')
                    .html('<div class="video-player"><div class="embed"><div id="ima-videoplayer' + $item.data('id') + '" class="video-js">' + playerHtml + '</div></div><div class="article-tools share-buttons" data-id="' + $item.data('id') + '">' + shareBoxHTML + '</div></div><div class="video-summary"><h2><a href="' + $item.data('link') + '">' + $item.data('title') + '</a></h2><div class="article-tools"><time datetime="">' + $item.data('time') + '</time> / <a href="' + $item.data('link') + '">' + $item.data('section') + '</a></div><a href="' + $item.data('link') + 'comments" class="i-com">' + $item.data('comments') + '</a></div>');
                shareBox = $(this).parents('.video-grid-container-v2').find('.share-buttons')
                
                if (nativeAdLabel) {                     $(this).parents('.video-grid-container, .video-grid-container-v2').find('.video-summary').prepend('<div class="native-ad-label">' + nativeAdLabel + " " + nativeAdAuthor + '</div>')
                }
                
            
            socials.forEach(function(network) {
                shareBox.find('a.' + network).attr('href', $item.data(network + '-link'));
            });

            // Check if video is already liked and interact with Like button
            shareBox.find('a.like').attr('data-id', $item.data('id'));
            shareBox.find('a.like').addClass('index-video');
            var storyid = $item.data('id');
            if (storyid) {
                url = pathRoot + 'ajax/story/checkrecommend/' + storyid;

                $.get(url, {}, function (d) {
                    if (d === 'false') {
                        shareBox.find('a.like').removeClass('active');
                    } else {
                        shareBox.find('a.like').addClass('active');
                    }
                });
            }

            if ($('.general-video-box').find('.video-js')[0]) {

                 if ( $(".video-js > iframe").length > 0 ) {
                      $(".video-js  iframe").attr("loading","lazy");
                      url = $(".video-js  iframe").attr("src");
                    if ( url.indexOf("?") != -1 ){
                            url += '&autoplay=1&mute=1'

                            } else {
                            url += '?autoplay=1&mute=1'
                            }

                        $(".video-js  iframe").attr("src", url);
                 }
                  player = videojs('#video-js');
            }
        });

    });
</script>
</div>
<div class="grid-d-3">
<div class="video-grid-scrollbar">
<div id="video-grid-scrollbar">
<div id="video-grid-scrollbar" class="video-box active">
