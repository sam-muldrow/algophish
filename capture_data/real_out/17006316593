<script type="text/javascript">
        $(document).ready(function () {

           function truncate(text){
                var re = text.match(/^.{0,90}[\S]*/);
                var l = re[0].length;
                var re = re[0].replace(/\s$/,'');

               if(l <= text.length){
                   var lastIndex = re.lastIndexOf(" ");
                   str = re.substring(0, lastIndex);
                   re = str + "...";
                }

                return re;
            }

            var blockText = $('.site-quote-v2 .blockquote blockquote a'),
                parentBlock = $('.site-quote-v2 .blockquote blockquote'),
                newString = '';

            if (blockText[0] && blockText[0]) {

                if (blockText.height() >= parentBlock.height()) {
                    newString = truncate(blockText.html());
                    blockText.attr("title", newString);
                    blockText.html(newString);
                }
            }
        })
    </script>
<div class="site-block" id="editors-pick-and-filter">
<div class="main-content">
