<script>

window.dnevnik = window.dnevnik || {};
window.dnevnik.event = {
	user_id: undefined,
	article_title: undefined,
	article_url: undefined,
	article_id: undefined,
	placement: undefined,
}

window.dnevnik.eventPush = (event, data = {}) => {
	tagManagerData = {};
	switch (event) {
		case 'newsletter_submits':
			tagManagerData = {
				'event': 'newsletter_submits',
				'action': 'newsletter signup',
				'article_title': window.dnevnik.event.article_title,
				'newsletter_number': data.newsletter_number,
				'name': data.newsletter_name,
			};
			break;

		case 'newsletter_form_impression':
			tagManagerData = {
				'event': 'newsletter_impression',
				'action': 'newsletter signup',
				'article_title': window.dnevnik.event.article_title,
				'newsletter_number': data.newsletter_number,
				'name': data.newsletter_name,
			};
			break;

		case 'login_started':
			tagManagerData = {
				'event': 'login_started',
				'action': data.action,
				'placement': window.dnevnik.event.placement,
			};
			break;

		case 'login_success':
			tagManagerData = {
				'event': 'login_success',
				'user_id': data.user_id,
				'action': data.action,
				'placement': window.dnevnik.event.placement,
			};
			break;

		case 'reg_started':
			tagManagerData = {
				'event': 'reg_started',
				'action': data.action,
				'placement': window.dnevnik.event.placement,
			};
			break;

		case 'reg_success':
			tagManagerData = {
				'event': 'reg_success',
				'action': data.action,
				'placement': window.dnevnik.event.placement,
			};
			break;

		case 'social_share':
			tagManagerData = {
				'event': 'social_share',
				'type': data.type,
				'placement': data.placement,
			};
			break;

		case 'custom_clicks':
			tagManagerData = {
				'event': 'custom_clicks',
				'category': data.category,
				'label': data.label,
			};
			break;

		case 'element_view':
			tagManagerData = {
				'event': 'element_view',
				'category': data.category,
				'label': data.label,
			};
			break;

		default:
			console.log('window.dnevnik.eventPush unknown event!');
			return false;
	}

			window.dataLayer.push( tagManagerData );
};


window.dataLayer = window.dataLayer || [];
window.dataLayer.push({
		'logged_in': 'NotLoggedIn',
	'user_type': 'Anonymous',
												});

</script>
