	<script src="/static/2020/static/2019/assets/js/jquery-1.11.0.js" type="text/javascript" charset="utf-8"></script>
	<script src="/static/2020/static/2019/assets/js/amazeui.min.js" type="text/javascript" charset="utf-8"></script>
	<script src="/static/2020/static/2019/assets/js/myslideup.js" type="text/javascript" charset="utf-8"></script>
	<script src="/static/2020/static/2019/assets/js/swiper.js" type="text/javascript" charset="utf-8"></script>
	<script type="text/javascript">
		$(function() {

			if (/(iPhone|iPad|iPod|iOS|Android)/i.test(navigator.userAgent)) { //移动端
				document.getElementsByTagName('html')[0].style.width = window.innerWidth + 'px';
				window.onresize = function() {
					document.getElementsByTagName('html')[0].style.width = window.innerWidth + 'px';
				};
			}
			//新闻上下滚动
			$(".justifyScroll").slideUp();
			//小轮播图
			var littleSwiper = new Swiper('.littleSwiper', {
				slidesPerView: 3,
				spaceBetween: 25,
				slidesPerGroup: 1,
				autoplay: true,
				loop: true,
				loopFillGroupWithBlank: true,
				navigation: {
					nextEl: '.swiper-button-next',
					prevEl: '.swiper-button-prev',
				},
			});
			//大的tab切换
			$('.ownTabContorl').on('mouseenter', '.subOwnTabContorl', function() {
				const _this = $(this);
				const _index = _this.index();
				_this.addClass('active').siblings().removeClass('active');
				let wellShow = $('.owbTabContent').children()[_index]
				$(wellShow).addClass('active').siblings().removeClass('active');
			})
			//友情链接下拉箭头
			$('.firendDropDownBtn').on('mouseenter', function() {
				$(this).parent().css('height', 'auto');
				$(this).removeClass('am-icon-caret-down').addClass("am-icon-caret-up")

			})
			$('.friendLinkTab .am-tab-panel').on('mouseleave', function() {
				$(this).css('height', '104px');
				$(this).find('.firendDropDownBtn').removeClass('am-icon-caret-up').addClass("am-icon-caret-down")
			})

			$('.shwyTabs span').click(function() {
				$('.shwyBox>div').eq($(this).index()).css('display', 'block').siblings().css('display', 'none');
				$(this).addClass('active-swtab').siblings().removeClass('active-swtab');
			})

			$('.bigCarouselContent').flexslider();
			var elevideo = document.getElementsByClassName("videoItem");
			// var elevideo = document.getElementById("video1");
			for (var i = 0; i < elevideo.length; i++) {
				elevideo[i].addEventListener('play', function() {
					console.log("开始播放");
					$('.bigCarouselContent').flexslider('pause');
				});
				elevideo[i].addEventListener('pause', function() {
					console.log("暂停播放");
					$('.bigCarouselContent').flexslider('play');
				});
			}

		})
	</script>
