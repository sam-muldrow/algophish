              <iframe id="wmpRecents" src="//front.wemakeprice.com/recents" style="display:none;"></iframe>
              
              <script>
                // iframe으로 loaded 메시지 전송하여 recents 데이터 세팅
                window.onload = function () {
                  document.getElementById('wmpRecents').contentWindow.postMessage(JSON.stringify({
                    type: 'loadedParent',
                  }), '*');
                }
              
                window.onmessage = function (e) {
                  if (typeof e.data === 'string') {
                    var payload = JSON.parse(e.data);
              
                    if (payload.type === 'fromIframeWeb') {
                      var recentProd = payload.data.recentProd || [];
              
                      window.localStorage.setItem('RECENT_PROD', JSON.stringify(recentProd));
                      typeof window.setRecentList === 'function' && window.setRecentList();
                      // iframe postmessage로 받은 데이터를 다시 iframe으로 전송 불필요로 주석처리
                      // sendStorageInfo();
                    }
                  }
              
                  function sendStorageInfo() {
                    document.getElementById('wmpRecents').contentWindow.postMessage(JSON.stringify({
                      type: 'recentProd',
                      data: {
                        recentProd: JSON.parse(window.localStorage.getItem('RECENT_PROD')),
                      }
                    }), '*');
                  }
                };
              </script>
            
            </div>          </div>
        </div>
