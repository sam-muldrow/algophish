            function isTouchDevice(){return!!(typeof window!=="undefined"&&("ontouchstart"in window||window.DocumentTouch&&typeof document!=="undefined"&&document instanceof window.DocumentTouch))||!!(typeof navigator!=="undefined"&&navigator.maxTouchPoints);}
            function calcPrimaryNavLinkMetrics(alignment){var snailTrailId="snail-trail-container";var gotAlignment=typeof alignment==="string"?alignment:window.platmodMeganavAlignConf;var primaryNavLinkMetrics=[];var snailTrailElement=document.getElementById(snailTrailId);var meganavElement=document.getElementById("platform_modernisation_meganav");var childrenArray=snailTrailElement.children;if(document.documentElement.clientWidth>1024){meganavElement.style.setProperty("--padding","0px");return;}var runningWidth=0;// eslint-disable-next-line no-plusplus
for(var i=0;i<childrenArray.length;i++){var $link=childrenArray[i];var text=$link.textContent;var textWidth=$link.childNodes[0].clientWidth!==undefined?$link.childNodes[0].clientWidth:0;var textStartAt=runningWidth;var textEndAt=runningWidth+textWidth;var noChangeFrom=textStartAt+textWidth*0.4;var noChangeTo=textEndAt-textWidth*0.3;if(noChangeFrom>=noChangeTo){// This can happen with shorter dept names such as "Men" and "Sale", just use the mid-point instead
noChangeFrom=textStartAt+textWidth/2;noChangeTo=textStartAt+textWidth/2;}runningWidth+=textWidth;primaryNavLinkMetrics.push({$link:$link,text:text,textWidth:textWidth,textStartAt:textStartAt,textEndAt:textEndAt,noChangeFrom:noChangeFrom,noChangeTo:noChangeTo});}//  constants import hasn't been used here as this gets printed as vanilla js in the DOM
if(gotAlignment==="center-aligned"){var alignmentProperty="--platmod_meganav_alignment";var screenWidth=document.getElementById(snailTrailId).clientWidth;if(primaryNavLinkMetrics[primaryNavLinkMetrics.length-1].textEndAt>screenWidth){document.getElementById(window.platformModernisationHeader).style.setProperty(alignmentProperty,"left");}else{document.getElementById(window.platformModernisationHeader).style.setProperty(alignmentProperty,"center");return;}}updatePrimaryNavLinkSpacing(primaryNavLinkMetrics,meganavElement);}
            function updatePrimaryNavLinkSpacing(primaryNavLinkMetrics,snailTrailElement){var screenWidth=document.getElementById("snail-trail-container").clientWidth;var newMargin=0;var increaseMarginBy=0;var distributeOver=0;// eslint-disable-next-line no-plusplus
for(var i=0;i<primaryNavLinkMetrics.length;i++){var link=primaryNavLinkMetrics[i];if(link.textEndAt<screenWidth){// This link plus including default margins is completely contained within the view
// Last links should distribute the remaining available space over all links
if(i===primaryNavLinkMetrics.length-1){increaseMarginBy=screenWidth-link.textEndAt;distributeOver=primaryNavLinkMetrics.length*2;newMargin=increaseMarginBy/distributeOver;break;}// Continue and find the first link that extends beyond the screen bounds
// eslint-disable-next-line no-continue
continue;}// This is the first link that extends beyond the screens bounds
if(screenWidth<link.noChangeFrom){// If screen boundry is too near to the start of the link (we need at least a few chars trailing off the scree) then we much use the previous link instead
if(i>0){// increase margins using the previous link such that the previous link is positioned with the last few characters trailing off the screen
var prevLink=primaryNavLinkMetrics[i-1];increaseMarginBy=screenWidth-prevLink.noChangeTo;distributeOver=i*2-1;newMargin=increaseMarginBy/distributeOver;}else{// No previous link to break on, fallback to default margin
newMargin=0;}}else if(screenWidth>link.noChangeTo){// If screen boundry is nearer to the end of the link
increaseMarginBy=screenWidth-link.noChangeTo;// increase margins such that the link is positioned with the last few characters trailing off the screen
distributeOver=i*2+1;newMargin=increaseMarginBy/distributeOver;}else{// Use default margin as we are somewhere in the middle of the link, ie. not close enough to the start or end to think about increasing margins
newMargin=0;}// And we're done, no need to consider other links outside the screen bounds
break;}if(newMargin!==0){newMargin-=0.1;// Apply a slight fudge to ensure that we don't start scrolling unnecessarily
newMargin=parseInt("".concat(newMargin*10),10)/10;// Round value
}// eslint-disable-next-line prefer-template
document.getElementById(window.platformModernisationHeader).style.setProperty("--padding","".concat(newMargin.toFixed(2).toString(),"px"));document.getElementById(window.platformModernisationHeader).style.setProperty("--display-primary-nav","1");}
