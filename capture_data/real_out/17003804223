            if (window.location.pathname === '/' && window.fetch && window.Promise) {
                var foundSearchValue = false;

                function getSearchParams() {
                    var PARAMS_MAPPING = {
                        utmTerm: ["utmTerm", "utm_term", "utm_Term"],
                        utmContent: ["utmContent", "utm_content", "utm_Content"],
                        utmSource: ["utmSource", "utm_source", "utm_Source"],
                        utmMedium: ["utmMedium", "utm_medium", "utm_Medium"],
                        utmCampaign: ["utmCampaign", "utm_campaign", "utm_Campaign"],
                        utmAdGroup: ["utm_adgroup", "utm_ad_group", "utmAdGroup", "utmAdgroup", "utm_ad_Group", "utm_Ad_Group", "utm_AdGroup"]
                    };

                    var queryParams = {};
                    var allParams = Object.keys(PARAMS_MAPPING);

                    for (var k = 0; k < allParams.length; k++) {
                        queryParams[allParams[k]] = null;
                    }
                    try {
                        var currUrl = new URL(window.location.href);
                        var searchParams = currUrl.searchParams;
                        if (!currUrl.search) {
                            return queryParams;
                        }
                        for (var i = 0; i < allParams.length; i++) {
                            var paramKey = allParams[i];
                            var paramAlias = PARAMS_MAPPING[paramKey];
                            for (var j = 0; j < paramAlias.length; j++) {
                                var param = paramAlias[j];
                                var searchParamValue = searchParams.get(param);
                                if (searchParamValue) {
                                    foundSearchValue = true;
                                    queryParams[paramKey] = searchParamValue;
                                    break;
                                }
                            }
                        }
                    }
                    catch (e) {
                        console.log('Error in parsing url for prehomepage');
                    }
                    return queryParams;
                }

                var utmParams = getSearchParams();

                if (foundSearchValue) {
                    fetch("https://logs.naukri.com/uba", {
                        mode: 'no-cors',
                        cache: 'no-cache',
                        method: 'POST',
                        credentials: 'include',
                        headers: {
                            accept: 'application/json',
                            'content-type': 'application/json'
                        },
                        body: JSON.stringify({
                            appId: '103',
                            eventName: 'prehomepage',
                            tenantId: "1",
                            subclientTenantId: "0",
                            pageName: "homepage",
                            deviceType: "WEB",
                            ...utmParams
                        })
                    })
                        .catch(function (e) { console.log('Error in tracking prehomepage', e) })
                }
            }
