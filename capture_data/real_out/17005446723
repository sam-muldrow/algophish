<script>
    $(document).ready(function(){
  $(function() {
      $("tr.info-open").click(function() {
       
        var hide_status = "false";
       
        if($(this).next('tr.info').is(":visible")){
                     hide_status = "true";

        }      
          $(".info").hide();
           $(".watch_text").show();
      
        if(hide_status == "true"){
             $(this).next('tr.info').hide();
                     $(".watch_text",this).show();

        }else{
                           $(this).next('tr.info').show();
               $(".watch_text",this).hide();

        }
        
          
          

          
    //           $('#TableData').toggle();

          
    //   $('tr.visible').removeClass('visible').addClass('info'); .addClass('visible').removeClass('info');          
      });
  });
});
