            const loadUAP = () => {
                const uapscrt = document.createElement('script');
                uapscrt.src = "https://www.carscoops.com/wp-content/themes/carscoops/scripts/uaparser.js?ver=230504-134655";
                // document.head.appendChild(uapscrt);
                document.querySelector('.footer-wrapper').parentElement.insertAdjacentElement('afterend', uapscrt);
                uapscrt.onload = () => {
                    console.log('uap appended');
                    const uap = new UAParser();
                    const client = uap.getResult();
                    console.log(client);
                    if (client.browser.name.toLowerCase().includes('webview')) {
                        const whatPage = document.body.getAttribute('page');
                        document.querySelectorAll('script, link').forEach(a => {
                            if (a.hasAttribute('src')) {
                                let oldSRC = a.src;
                                if (oldSRC !== '' && oldSRC.includes('wp-content')) {
                                    let newSRC = oldSRC.split('?ver=')[0];
                                    a.src = newSRC
                                }
                            }
                            if (a.hasAttribute('href')) {
                                let oldSRC = a.href;
                                if (oldSRC !== '' && oldSRC.includes('wp-content')) {
                                    let newSRC = oldSRC.split('?ver=')[0];
                                    a.href = newSRC
                                }
                            }
                            if (whatPage == 'article') {

                            }
                            localStorage.clear();
                            caches.keys().then(names => {
                                names.forEach(name => caches.delete(name));
                            });
                        })
                    }
                }
            }
            window.addEventListener('load', function () {  
                window.delayedTimeout = setTimeout(function () {
