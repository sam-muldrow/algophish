    let fixedImageSpaces = function (selector, orginImgWith, orginImgHeight) {
        let img = $(selector);
        let newWidth = img.width();
        let newHeight = (newWidth / orginImgWith) * orginImgHeight;

        img.css('height', newHeight).css('width', newWidth);
