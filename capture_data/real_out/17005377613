                <script>
            function getCookie(cname) {
                var name = cname + "=";
                var ca = document.cookie.split(';');
                for(let i = 0; i < ca.length; i++) {
                    var c = ca[i];
                    while (c.charAt(0) == ' ') {
                        c = c.substring(1);
                    }
                    if (c.indexOf(name) == 0) {
                        return c.substring(name.length, c.length);
                    }
                }
                return "";
            }

            function hubspotOnLoad() {
                window._hsp = window._hsp || [];
                window._hsp.push([
                    'addPrivacyConsentListener', (consent) => {
                        const consents = {
                            ad_storage: consent.categories.advertisement ? 'granted' : 'denied',
                            analytics_storage: consent.categories.analytics ? 'granted' : 'denied',
                            functionality_storage: consent.categories.functionality ? 'granted' : 'denied',
                            personalization_storage: 'granted',
                            security_storage: 'granted',
                        };

                        var cookieConsent = getCookie('cookieConsent');
                        var update = true;
                        if (cookieConsent) {
                            // cookieConsent = JSON.parse(cookieConsent);
                            // console.log(cookieConsent);
                            if (cookieConsent === JSON.stringify(consents)) {
                                update = false;
                            }
                        }
                        if (update) {
                            var date = new Date();
                            date.setFullYear(date.getFullYear() + 1);
                            // document.cookie = 'cookieConsent=ad_storage.' + consents.ad_storage + ':analytics_storage.' + consents.analytics_storage + ':functionality_storage.' + consents.functionality_storage + ':personalization_storage.granted:security_storage.granted; expires=' + date.toUTCString() + '; path=/';
                            document.cookie = 'cookieConsent=' + JSON.stringify(consents) + '; expires=' + date.toUTCString() + '; path=/';
                            // Cookies.set('cookieConsent',consents,{expires: 365});
                            window.gtag('consent', 'update', consents);
                            window.dataLayer.push({ event: 'consent_updated' });
                        }
                    },
                ]);
            }
        </script>
        <script id="hs-script-loader" src="//js-eu1.hs-scripts.com/25646044.js" onload="hubspotOnLoad()" async defer ></script>
            </head>
