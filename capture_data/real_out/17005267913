<script>
    $(function () {

        // ie hack
        if (document.documentMode || /Edge/.test(navigator.userAgent)) {
            $('.index-flash li .img a img').each(function(){
                var t = jQuery(this),
                    s = 'url(' + t.attr('src') + ')',
                    p = t.parent(),
                    d = jQuery('<div></div>');

                p.append(d);
                d.css({
                    'height'                : '100%',//t.parent().css('height')
                    'background-size'       : 'cover',
                    'background-repeat'     : 'no-repeat',
                    'background-position'   : '50% 20%',
                    'background-image'      : s
                });
                t.hide();
            });

            $('.index-flash li .img a video').each(function(){
                var t = jQuery(this)
                t.css({
                    'left'      : '50%',
                    'top'       : '50%',
                    'width'     : 'auto',
                    'height'    : 'auto',
                    'min-height': '100%',
                    'min-width' : '100%',
                    'transform' : 'translate(-50%, -50%)',

                });
            });
        }
        // ie hack

        var dom = $('.index-flash');
        var swiper = new swiperFun({
            dom: dom,
            loop: false,
            effect: 'fade'
        });
        dom.find('.prev').click(function () {
            if (swiper.mySwiper.previousIndex === 0) {
                swiper.mySwiper.slideTo(swiper.mySwiper.slides.length - 1)
            }
            return false
        });
        dom.find('.next').click(function () {
            if (swiper.mySwiper.previousIndex === swiper.mySwiper.slides.length - 1) {
                swiper.mySwiper.slideTo(0)
            }
            return false
        });

        var videos = [];
        dom.find('li').each(function () {
            videos.push($(this).find('video')[0] || false)
        });
        videos.forEach(function (item, i) {
            item && item.addEventListener('ended', function () {
                if (i === swiper.mySwiper.slides.length - 1) {
                    swiper.mySwiper.slideTo(0);
                } else {
                    swiper.mySwiper.slideNext();
                }
                //swiper.mySwiper.startAutoplay()
            })
        });
        swiper.change = function (i) {
            change(i)
        };
        change(0);

        function change(i) {
            videos.forEach(function (item) {
                if (item) {
                    item.currentTime = 0;
                    item.pause()
                }
            });
            var video = videos[i];
            if (video) {
                swiper.mySwiper.stopAutoplay();
                //video.playbackRate = 10;//æµè¯ç¨ï¼è§é¢å¤ªé¿äºæ¯è¾é¾ç­
                video.play()
            } else {
                swiper.mySwiper.startAutoplay()
            }
        }




    });

</script>

