      window.fpx_debitAuthCode = "";
		
	  /* 1st - checking browser type */
    function getBrowserType() {
        var userAgent = navigator.userAgent.toLowerCase();
  
        if (userAgent.includes("android")) {
          return "Android";
        } else if (userAgent.includes("iphone") || userAgent.includes("ipad")) {
          return "Ios";
        } else if (userAgent.includes("opr")) {
          return "OPR";
        } else if (userAgent.includes("edg")) {
          return "Edg";
        } else if (userAgent.includes("firefox")) {
          return "Firefox";
        } else if (userAgent.includes("chrome")) {
          return "Chrome";
        } else if (userAgent.includes("macintosh")) {
          return "SafariMac";
        } else if (userAgent.includes("msie") || userAgent.includes("trident")) {
          return "Msie";
        } else {
          return "Unknown";
        }
      }
  
      /* 2nd - get minimum browser version */
      function isBrowserSupported(browserType) {
        var browserSupportedVersion = [
          {
            type: "Edg",
            minVersion: "79.0.0.0"
          },
          {
            type: "Chrome",
            minVersion: "52.0.0.0"
          },
          {
            type: "Msie",
            minVersion: "79.0.0.0"
          },
          {
            type: "SafariMac",
            minVersion: "10.13.4"
          },
          {
            type: "Ios",
            minVersion: "0.0.0"
          },
          {
            type: "Android",
            minVersion: "0.0.0"
          },
          {
            type: "iPad",
            minVersion: "0.0.0"
          },
          {
            type: "Firefox",
            minVersion: "52.0.0"
          },
          {
            type: "OPR",
            minVersion: "39.0.0.0"
          },
          {
            type: "Unknown",
            minVersion: "0.0.0"
          },
        ];
  
        var userVersion = getBrowserVersion(browserType);
  
        if (!userVersion) {
          return false;
        }
  
        var supportedBrowser = browserSupportedVersion.find(function (browser) {
          return browser.type === browserType;
        });
  
        if (!supportedBrowser) {
          return false;
        }
  
        var userVersionParts = userVersion.split(".");
        var minVersionParts = supportedBrowser.minVersion.split(".");
  
        for (var i = 0; i < Math.min(userVersionParts.length, minVersionParts.length); i++) {
          var userPart = parseInt(userVersionParts[i]);
          var minPart = parseInt(minVersionParts[i]);
  
          if (userPart < minPart) {
            return false;
          } else if (userPart > minPart) {
            return true;
          }
        }
  
        return true;
      }
  
      /* 3rd - compare browser version and reroute user to Maybank2u or notice page */
      function getBrowserVersion(browserType) {
        if (browserType === "Ios") {
          var versionMatch = navigator.userAgent.match(/(iPhone OS|CPU OS|iPad; CPU OS) (\d+_\d+(_\d+)?)/);
          return versionMatch ? versionMatch[2].replace(/_/g, ".") : null;
        } else if (browserType === "SafariMac") {
          var versionMatch = navigator.userAgent.match(/Version\/(\d+.\d?)/);
          return versionMatch ? versionMatch[1].replace(/_/g, ".") : null;
        } else if (browserType === "Android") {
          var versionMatch = navigator.userAgent.match(/Android (\d+)/);
          return versionMatch ? versionMatch[1] : null;
        } else {
          var versionMatch = navigator.userAgent.match(new RegExp(browserType + "/([0-9.]+)"));
          return versionMatch ? versionMatch[1] : null;
        }
      }
  
      if (navigator.userAgent.match(/Trident\/(\d.\d+?)/)) {
        window.location.replace(
          "https://www.maybank2u.com.my/maybank2u/html/browser-support/index.html"
        );
      } else {
        var browserType = getBrowserType();
  
        if (isBrowserSupported(browserType)) {
          script.src = "/modules_23111801/main/main.js.gz";
          scriptVendor.src = "/modules_23111801/defaultVendors-main/defaultVendors-main.js.gz";
        } else {
          window.location.replace("https://www.maybank2u.com.my/maybank2u/html/browser-support/index.html");
        }
      }
      
        head.appendChild(scriptVendor);
        head.appendChild(script);
    </script>
