function videoComponentTracking() {
  const video = document.querySelector('.lazy-video');
  const previewImage = document.querySelector('.lazy-video.lazy-video--preview');

  if(video) {
    const control = 'playbutton';
    const videoId = video.dataset.videoid;

    window._1und1datalayer.push({
      event: 'media.info',
      media: {
        type: 'Video',
        source: `YouTube_${videoId}`,
        state: {
          starttype: 'usertriggered',
          currentstate: 'not started'
        }
      }
    });

    const pushToDataLayer = (currentstate) => {
      window._1und1datalayer.push({
        event: 'media.info',
        media: {
          type: 'Video',
          source: `YouTube_${videoId}`,
          state: {
            starttype: 'usertriggered',
            currentstate: currentstate
          },
          interaction: {
            control: control
          }
        }
      });
    };

    const trackEvent = () => {
      window._1und1datalayer.push({
        event: 'genericevent.info',
        genericevent: {
          category: 'video',
          action: 'klick',
          name: control,
          variant: '',
          type: ''
        }
      });

      window._1und1datalayer.push({
        event: 'genericevent.send',
        timestamp: Date.now()
      });
    };

    const handlePlayButtonClick = () => {
      pushToDataLayer('running');
      trackEvent();
    };

    previewImage.addEventListener('click', handlePlayButtonClick);

    // We need to delete the first push, because it is to early from source tealium-append.js
    deleteEventFromDataLayer('pageview.send');

    window._1und1datalayer.push({
      event: 'pageview.send',
      timestamp: new Date()
    });
  }
}
addEventListener('DOMContentLoaded', videoComponentTracking);

function deleteEventFromDataLayer(eventName) {
  const indexToRemove = window._1und1datalayer.findIndex(data => data.event === eventName);
  if (indexToRemove !== -1) {
    window._1und1datalayer.splice(indexToRemove, 1);
  }
}
