    const accountButton = document.querySelector(".js-openMyAccount");
    const menuDiv = document.querySelector(".js-myAccountMenu");

    function toggleAriaExpanded() {
        const isMenuVisible = menuDiv.classList.contains("open");

        if (!isMenuVisible) {
            accountButton.setAttribute("aria-expanded", "true");
        } else {
            accountButton.setAttribute("aria-expanded", "false");
        }
    };
