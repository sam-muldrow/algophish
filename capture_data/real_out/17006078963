<section class="mea-multi-regional mea-multi-regional--national " data-cy="mea-multi-regional" id="mea-multi-regional">
  <h2 class="mea-multi-regional__title-wrapper">
        <span class="mea-multi-regional__title">L’actualité près&#10;de&nbsp;chez&nbsp;vous</span>
    <span class="mea-multi-regional__with-wrapper">
      <span class="mea-multi-regional__title-with">avec</span>
      <span class="text-only-for-screen-reader">france 3 région et la première outremer</span>
          
    
    
  
  <img

          src="/assets/components/shared/mea-multi-regional/img/region-28d74fef.svg"
  
      width="36"
      height="31"
      alt=""
      class="mea-multi-regional__logo"
  
  >

          
    
    
  
  <img

          src="/assets/components/shared/mea-multi-regional/img/outremer-eb09ab62.svg"
  
      width="37"
      height="31"
      alt=""
      class="mea-multi-regional__logo"
  
  >

    </span>
  </h2>
  


<span
  class="skip-block "
  data-to-skip="mea-multi-regional"
  data-position="in">
  


  
<button
  class="fi-button fi-button--primary fi-button--hidden fi-button--no-icons "
            aria-label="Passer la liste l’actualité près&#10;de&nbsp;chez&nbsp;vous"
      >
  
        
