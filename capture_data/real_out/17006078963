  class="fi-button fi-button--primary fi-button--hidden fi-button--no-icons "
            aria-label="Passer la liste lactualit prs&#10;de&nbsp;chez&nbsp;vous"
      >
  
        
    <!-- --><span class="fi-button__text">passer cette liste</span><!-- -->
  
  
