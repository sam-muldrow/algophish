  <section class="rubrique-content " data-cy="rubrique-content">
    <span role="heading" aria-level="3">
      <a href="/vrai-ou-fake/" class="ftvi-click rubrique-content__link-title" data-click-value="nos-rubriques::vrai-ou-fake">
        <p class="rubrique-content__title">
