  $(window).bind("scroll", function () {
    var sTop = $(window).scrollTop();
    var sTop = parseInt(sTop);
    if (sTop >= document.documentElement.clientHeight * 1) {
      if (!$("#btn6").is(":visible")) {
        try {
          $("#btn6").slideDown();
          $("#btn6").show();
        } catch (e) {
          $("#btn6").show();
        }
      }
    } else {
      if ($("#btn6").is(":visible")) {
        try {
          $("#btn6").slideUp();
          $("#btn6").hide();
        } catch (e) {
          $("#btn6").hide();
        }
      }
    }
  });
