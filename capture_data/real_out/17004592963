<script type="text/javascript">	 
	var w=1; // w=é¡µé¢å¯å¨åï¼ç­å¾æµ®å±å¹¿åå¼¹åºçç§æ°
	var c=7; // c-1=æµ®å±å¹¿ååºç°åï¼åè®¡æ¶èªå¨å³é­çç§æ°
	
	var t;
 	t=setTimeout("playFloating()", w*1000); 
	
	function playFloating(){
		justifyFloatingImg();
		document.getElementById("floating").style.display="block";
		autoTimer();
	}
	
	function colseFloating(){
		document.getElementById("floating").style.display="none";
	}
	
	function autoTimer(){
		c=c-1;
		document.getElementById("timer").innerHTML=c;
		if(c>0){
		setTimeout("autoTimer()", 1000);
		}else{
			colseFloating();
			}
		}

	function justifyFloatingImg(){
		var bw;
		if (window.innerWidth){
			bw = window.innerWidth;
		}else if(document.body){ 
			bw = document.body.clientWidth;
		}else{
			bw=1900;
			}
		
		var fi = document.getElementById("floatingImg");
		if(bw>=1900){
		fi.style.marginLeft = "auto";
		}else if(bw>=1000 && bw<1900){
			var fiml=(bw-1900)/2;
			fi.style.marginLeft = fiml+"px";
			}else{
				fi.style.marginLeft = "-220px";
			}
		
		var closeBtn = document.getElementById("closeBox");
		closeBtn.style.right=(bw-1000)/2+"px";
		
	}
</script>
<style type="text/css">
#floating{position: absolute; width: 100%; height: 580px; top: 0px; z-index: 900; display: none; left: 0px; overflow: hidden;}
#floating .imgBox{width: 100%; text-align: center;}
#floating .imgBox img{margin-left: -100px;}
#floating .closeBox{cursor: pointer; position: absolute; top: 530px; width:140px; background: #FFF; background-color:rgba(255,255,255,0.8); padding:5px; border:solid 2px #d3d3d3; border-radius: 4px; text-align:center;}
#floating .closeBox p{font-size: 24px; line-height:34px; color:#000; color:rgba(0,0,0,0.8);}
#floating .closeBox p span#timer{font-size:18px;}</style>
