<script>

function pageToTop(){

    
    window.scrollTo({ top: 0, behavior: 'smooth' });
    }

    const btnFixed = document.querySelector('.btn--rounded-fixed');
    
    window.addEventListener("scroll", function(){


        if(document.querySelector('.featured-box_sticky')) {
            const stickyHeight = document.querySelector('.featured-box_sticky').clientHeight;
            btnFixed.style.bottom = stickyHeight+20+"px";
        }
    
        if(document.querySelector('.landing-header') && document.querySelector('.landing-header').getBoundingClientRect().top <= -90 ){
            btnFixed.classList.remove('d-none');
        } else if (document.querySelector('.header-bk-wem') && document.querySelector('.header-bk-wem').getBoundingClientRect().top <= -90 ) {
            btnFixed.classList.remove('d-none');
        }
        else {
            document.querySelector('.btn--rounded-fixed').classList.add('d-none');
        }
});


</script>


                </section>

            


