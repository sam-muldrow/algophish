var modalCookies = document.getElementById('modal-cookies');
if (modalCookies.classList.contains('opened')){
    bkjs.commons.classesToEl(document.body, 'add', 'no-scroll');} else {bkjs.commons.classesToEl(document.body, 'remove', 'no-scroll');}
