<script>
    function getBrowserType() {
        var userAgentString = navigator.userAgent;

        // Parse the user agent string to extract the browser version
        var browserVersion;
        if (userAgentString.indexOf('Firefox') > -1) {
        browserVersion = userAgentString.match(/Firefox\/(\d+\.\d+)/)[1];
        browserVersion = 'Firefox - ' + browserVersion;
        } else if (userAgentString.indexOf('Chrome') > -1) {
        browserVersion = userAgentString.match(/Chrome\/(\d+\.\d+)/)[1];
        browserVersion = 'Chrome - ' + browserVersion;
        } else if (userAgentString.indexOf('Safari') > -1) {
        browserVersion = userAgentString.match(/Version\/(\d+\.\d+)/)[1];
        browserVersion = 'Safari - ' + browserVersion;
        } else if (userAgentString.indexOf('MSIE') > -1) {
        browserVersion = userAgentString.match(/MSIE\s(\d+\.\d+)/)[1];
        browserVersion = 'MSIE - ' + browserVersion;
        } else {
        browserVersion = 'Unknown';
        }

        return browserVersion;
    }

    /**
    * Report initial dimensions up to GTM layer
    */
    let pageDimensions = {"event":"initialDimensions","customer_id":"","customer_email":"","customer_email_hashed":"","customer_order_count":"","customer_total":"","customer_valid_id":"","closest_store_name":"1106","closest_store_ID":"1106","preferred_store_name":"","preferred_store_ID":"","logged_in_status":"guest","cart_size":"","registered_customer_type":"","plus_customer_type":"guest","cart_value":"","cart_delivery_methods":"","is_mkt_landing_page":"","page_template":"","DWKey":"","isServedWithConstructorSearch":true,"page_type":"Tech","list_type":""};
    if (pageDimensions != null && typeof window.dataLayer != 'undefined') {
        pageDimensions.browserVersion = getBrowserType();
        window.dataLayer.push(pageDimensions);
    }

    //push cart
    let cartDimensions = {"event":"cartContents","contents":{"products":[]}};
    if (cartDimensions != null && typeof window.dataLayer != 'undefined') {
        window.dataLayer.push(cartDimensions);
    }

	
	    let deliveryDestination = {"event":"impressionDeliveryLocation","deliveryDestination":"United States(US)","deliveryDestinationCode":"USA","deliveryCurrency":"US Dollar(USD)"};
	    if (deliveryDestination != null && typeof window.dataLayer != 'undefined') {
	        window.dataLayer.push(deliveryDestination);
	    }
    
</script>


