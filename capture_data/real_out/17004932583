<script>
var isInternational = document.cookie.indexOf("esw.InternationalUser=true") > -1;
window.fetchCookieValue = function(name) {
  var match = document.cookie.match(new RegExp('(^| )' + name + '=([^;]+)'));
  if (match) return match[2];
}
if(isInternational){
        document.querySelector('[data-international-show="false"]').style.display = "none";
        document.querySelector('[data-international-track-order="false"]').style.display = "none";
        document.querySelector('[data-international-smile-show="false"]').style.display = "none";
        document.querySelector('[data-international-show="true"]').style.display = "block";
        document.querySelector('[data-international-track-order="true"]').style.display = "block";
        document.querySelector('[data-international-smile-show="true"]').style.display = "block";    

        var countryPrefix = fetchCookieValue("esw.location");
        if(countryPrefix !== undefined){
document.querySelector('[data-international-show="true"]').href =  "/" + countryPrefix.toLowerCase() +"/create-account";
document.querySelector('[data-international-smile-show="true"]').href =  "/" + countryPrefix.toLowerCase() +"/create-account";
document.querySelector('[data-international-track-order="true"]').href =  "/" + countryPrefix.toLowerCase() +"/track-order";
        }
}
else{
        document.querySelector('[data-international-show="true"]').style.display = "none";
        document.querySelector('[data-international-track-order="true"]').style.display = "none";
        document.querySelector('[data-international-smile-show="true"]').style.display = "none";
        document.querySelector('[data-international-show="false"]').style.display = "block";
        document.querySelector('[data-international-track-order="false"]').style.display = "block";
        document.querySelector('[data-international-smile-show="false"]').style.display = "block";
}
</script>
	</div> <!-- End content-asset -->



    </div>


                        
                    </div>

