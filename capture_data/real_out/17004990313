    // Custom loader for use in module({},...)
    define('emscripten', [], function() {
        function load(name, parentRequire, onload, config) {
            var r = /([^!]*)(!(.*))?/.exec(name);
            var url = r[1];
            module.run([url], function() {
                /* jshint newcap: false */
                NorthstarModule({
                    locateFile: function(path, scriptDirectory) {
                        if (path.endsWith('northstar_api.wasm')) {
                            return "//webasset-akm.imvu.com/asset/b2a2ff3b5d178418/build/northstar/js/northstar_api.wasm";
                        }
                        return scriptDirectory + path;
                    },
                }).then(function (Module) {
                    onload(Module);
                });
            });
        }

        return {
            load: load
        };
    });
</script>
