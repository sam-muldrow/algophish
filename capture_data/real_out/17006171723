 "
          
          style=""
        >
      
        <script>
  function waitForElementToExist(selector, timeout = 5000) {
    return new Promise((resolve, reject) => {
      const startTime = Date.now();
  
      function checkElement() {
        const element = document.querySelector(selector);
        if (element) {
          resolve(element);
        } else if (Date.now() - startTime >= timeout) {
          reject(new Error(`Timeout waiting for element '${selector}' to exist`));
        } else {
          setTimeout(checkElement, 100);
        }
      }
  
      checkElement();
    });
  }
  
  waitForElementToExist('#userwayAccessibilityIcon')
    .then((element) => {
      const accessibilityElements = document.querySelectorAll('[data-handle="accessibility-mode"] a');
      const userwayIcon = document.getElementById("userwayAccessibilityIcon");
      accessibilityElements.forEach(element => {
        element.removeAttribute('href');
        element.addEventListener("click", function(e) {
          userwayIcon.click();
        });
      });
    })
    .catch((error) => {
      console.error('Error:', error);
    });
  </script>
      
        </div>
      



  </div>
</div>

