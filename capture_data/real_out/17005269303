        <script>
            $(document).ready(function () {
                $('input[type=radio][name=uiMode]').change(function (e) {

                    switch (this.value) {
                        case 'mobile':
                            setCookie('webVersion', "mobile");

                            JSAlert.alert("UI模式已切换至[移动版]", null, JSAlert.Icons.Success).then(function () {
                                window.location.reload();
                            });
                            break;
                        default:
                            setCookie('webVersion', "desktop");

                            JSAlert.alert("UI模式已切换至[桌面版]", null, JSAlert.Icons.Success).then(function () {
                                window.location.reload();
                            });
                            break;
                    }
                });
            });

            function setCookie(name, value, expire) {
                if (value && value != "") {
                    localStorage.setItem(name, value);
                    if (expire == null) {
                        var today = new Date();
                        expire = new Date();
                        expire.setTime(today.getTime() + 1000 * 60 * 60 * 24 * 365);
                    }
                    window.document.cookie = name + "=" + escape(value) + ((expire == null) ? "" : ("; expires=" + expire.toGMTString())) + ";path=/";
                } else {
                    localStorage.removeItem(name);
                    window.document.cookie = name + "=;expires=Thu, 01 Jan 1970 00:00:00 UTC;path=/";
                }
            }

            function LogOut() {
                $.post("/api/auth/LoginOut", function (data) {
                    if (data && data.success) {
                        window.location.reload();
                    }
                })
            }

            function search() {
                var keyword = $("#keyword").val();
                if (keyword) {
                    window.location.href = "/searcheasy.html?keyword=" + keyword;
                } else {
                    alert('请输入搜索关键字');
                }
            }
        </script>
