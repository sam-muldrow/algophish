    <script type="text/javascript">

      $(document).ready(function() {
        var elt = document.getElementById('ads_bottom');
        if(elt == null || elt.length == 0) {
          if (typeof jQuery == 'undefined') {
            alert("Warning, an adblocker is activated!!" + "\n" + "Our platform can't work well if an adblocker is activated. Please deactivate it to avoid unexpected behaviors.");
          } else {
            $('#modalContenu_infosModal').remove();

            $('.bloqueur-infos-banner').vitrineBanner({
              type              : 'tooltips',
              shutter           : 'bloqueur-infos-banner',
              container         : 'bloqueur-infos-banner',
              shutterEvent      : 'click',
              tooltip           : true,
              tooltipWidth      : '800',
              tooltipHeight     : '650',
              tooltipAutoPos    : true,
              btnCloseClass     : 'close-panneau',
              autoOpenOnStart   : true,
              hideCloseBtn      : false,
              scrollableContent : '#bloqueur-infos-content'
            });
            $("#bloqueur-infos-content").mCustomScrollbar();
          }
        }

        $('SPAN [id*="pays-"]').click(function () {
          var explode = $(this).attr('id').split('-');
          if (explode[1].length > 0) {
            $("[id^='map-']").removeClass("actif");
            $('#map-'+explode[1]).addClass("actif");
          }
        });
      });
    </script>
