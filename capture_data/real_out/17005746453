    <script type="text/javascript">
  $(document).ready(function() {

    var curDiv = false;
    $('li[id^="jsAdblock_"]').click(function() {
      var elt = $(this);
      var nom = elt.attr('id').split('_')[1];
      var divElt = $('div#jsDivAdblock_' + nom);
      if (false != curDiv) {
        curDiv.addClass('hide');
      };
      divElt.removeClass('hide');
      curDiv = divElt;
    });
  });
</script>


