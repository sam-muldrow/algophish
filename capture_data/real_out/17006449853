   function html_entity_decode( string ) {
     var ret, tarea = document.createElement('textarea');
     tarea.innerHTML = string;
     ret = tarea.value;
     return ret;
   }

   const expandLinks = e => {
     const linksHolders = document.querySelectorAll(e.currentTarget.getAttribute('data-target'));
     linksHolders.forEach(el => {
       el.classList.toggle('links_collapse');
     });
     e.currentTarget.getElementsByClassName('collapse_closed_text')[0].classList.toggle('hidden');
     e.currentTarget.getElementsByClassName('collapse_open_text')[0].classList.toggle('hidden');
   }

   collapseButtons.forEach(el => el.addEventListener('click', e => expandLinks(e)));

