                    galleryPixels[1] = [
                        
                    ];
                

                // Set initial pixels for visible shows
                const screenWidth = window.innerWidth;
                if (screenWidth > 767) {
                    const cardCount = getInitialCardCount();
                    const galleryCards = $('.gallery__card-image').slice(0, cardCount);
                    addDesktopPixels(galleryPixels[0], galleryCards);
                } else {
                    const galleryCards = $('.gallery__card-image').slice(0, 3);
                    addMobilePixels(galleryPixels[0], galleryCards);
                }

                // Set initial pixels when switching to a new tab
                $('.gallery__tab:not(".active-tab")')
                    .on('addPixels', function(event, index) {
                        const pixels = galleryPixels[index];
                        if (pixels.length) {
                            const $newTab = $(`.gallery__tab-content[data-tabindex=${(index + 1)}]`);
                            const galleryCards = $newTab.find('.gallery__card-image');
                            if (screenWidth > 767) {
                                const cardCount = getInitialCardCount();
                                addDesktopPixels(pixels, galleryCards.slice(0, cardCount));
                            } else {
                                addMobilePixels(pixels, galleryCards.slice(0, 3));
                                scrollDistance = 0;
                            }
                        }
                    });

                // Set remaining desktop pixels after expanding the gallery
                $('.gallery__expand-button')
                    .one('addPixels', function() {
                        const $tab = $(this).parents('.gallery__tab-content');
                        const tabIndex = $tab.attr('data-tabindex') - 1;
                        const cardCount = getInitialCardCount();
                        const galleryCards = $tab.find('.gallery__card-image').slice(cardCount);
                        addDesktopPixels(galleryPixels[tabIndex], galleryCards);
                    });

                // Adds pixels when cards scroll into view on mobile
                let scrollTimer = null;
                let scrollDistance = 0;
                $('.gallery__tab-content')
                    .on('scroll', function (event) {
                        const $this = $(this);
                        if (scrollTimer) {
                            clearTimeout(scrollTimer);
                        }
                        scrollTimer = setTimeout(function() {
                            const scrollValue = $this.scrollLeft();
                            if (scrollValue > scrollDistance) {
                                const tabIndex = $this.attr('data-tabindex') - 1;
                                if (galleryPixels[tabIndex].length) {
                                    const galleryCards = $this.find('.gallery__card-image.lazyloaded');
                                    addMobilePixels(galleryPixels[tabIndex], galleryCards);
                                }
                                scrollDistance = scrollValue;
                            }
                        }, 250);
                    });

                function addDesktopPixels(pixels, cards) {
                    for (let i = 0; i < cards.length; i++) {
                        const pixelIndex = getPixelIndex(pixels, cards[i]);
                        if (pixels[pixelIndex]) {
                            generatePixel(pixels[pixelIndex].url);
                            pixels[pixelIndex] = null;
                        }
                    }
                }

                // Adds pixels when at least half of a card scrolled into view
                function addMobilePixels(pixels, cards) {
                    for (let i = 0; i < cards.length; i++) {
                        const pixelIndex = getPixelIndex(pixels, cards[i]);
                        if (pixels[pixelIndex]) {
                            const visibleWidth = screenWidth - cards[i].getBoundingClientRect().left;
                            if (visibleWidth > 107) {
                                generatePixel(pixels[pixelIndex].url);
                                pixels[pixelIndex] = null;
                            }
                        }
                    }
                }

                // Number of cards initially visible on large viewports
                function getInitialCardCount() {
                    if (screenWidth < 992) {
                        return 6;
                    } else if (screenWidth < 1200) {
                        return 8;
                    }
                    return 10;
                }

                // Returns index of show pixel
                function getPixelIndex(pixels, element) {
                    const show = element.getAttribute('show');
                    return pixels.findIndex(pixel => pixel && pixel.name === show);
                }

                // Creates an image making a request to the pixel url
                function generatePixel(pixel) {
                    const image = document.createElement('img');
                    image.setAttribute('style', 'position: absolute; width: 1px; height: 1px;');
                    const timeStamp = new Date().getTime().toString();
                    const srcUrl = pixel.replace(/timestamp|cachebuster/gi, timeStamp);
                    image.src = srcUrl;
                }
            })(jQuery);
        

        // Homepage GA tag manager
        (function($) {
            $('div[data-homepage-area-tagging]').on('click', 'a[href]', function(e) {
                const $this = $(this);
                const targetArea = $(e.delegateTarget).data('homepage-area-tagging');
                const targetUrl = $this.prop('href');
                dataLayer.push({
                    'event': 'homepage-area-tag',
                    'homepage-area': targetArea,
                    'target-url': targetUrl,
                });
            });
        })(jQuery);
    </script>


