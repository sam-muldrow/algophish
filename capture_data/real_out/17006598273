<script type="text/javascript">
//ÂÖ²¥
(function(){
   var single_width=0,dispersion=0;
   var $dom = $('#flashCon li:first').clone();
  $('#flashCon').append($dom);
   var size=$('#flashCon li').length;
   
   
   var linksHtml='';
   for(var i=0; i<size-1;i++){
      linksHtml+='<a href="#" title=""></a>';
   }
   
   $(".flash_links").html(linksHtml);
   $(".flash_links").find("a").eq(0).addClass("on");
   
   var frist = 0;
   var datetime;
  
   $('#flashCon').hover(function(){
      clearInterval(datetime);
   },function(){
      datetime = setInterval(function(){
         showpic(frist)
         frist++;
         
         if(frist==size){
            frist=1;
         }
      },3000);
   }).trigger('mouseleave');
   
   $('.flash_links a').hover(function(){
       frist = $('.flash_links a').index(this);
      showpic(frist);
   })
   
   function showpic(frist){
      var imgheight = $('#flashBox').width();
      
      if(frist+1 == size){
         $('.flash_links a').removeClass('on').eq(0).addClass('on');
         $('#flashBox').stop(true,false).animate({scrollLeft:imgheight*frist},600,function(){
            frist = 0;
               $('#flashBox').stop(true,false).animate({scrollLeft:imgheight*frist},0);
               $('.flash_links a').removeClass('on').eq(frist).addClass('on');
         });
         
      }else{
         $('#flashBox').stop(true,false).animate({scrollLeft:imgheight*frist},600);
         $('.flash_links a').removeClass('on').eq(frist).addClass('on');   
      }
   };  
   
    
   function giveWidth(){
       var browser_width=$(window).innerWidth(); //ä¯ÀÀÆ÷´°¿Ú´óÐ¡
      if(browser_width>=1920){
        single_width=1920;
      }else if(browser_width<1920 && browser_width>1002){
        single_width=browser_width;
        dispersion=1920-browser_width; //²îÁ¿
      }else if( browser_width<1002){
        single_width=1002;
        dispersion=1920-1002;  
      }
      
      $('#flashBox').width(single_width);
      $('#flashBox ul').width(single_width*size);
      
      showpic(frist);
      
      $("#flashBox").find("img").css("margin-left",-dispersion/2);
      $("#flashBox").css("width",single_width).find("#flashCon").css("width",single_width*size).find("li").css("width",single_width);
   }
   giveWidth();
   $(window).resize( function(){giveWidth();});
   
}());

//ÓÎÏ·²¿·Ö£¬Êó±êÒÆ¹ý»¬³ö
$(".col_list dt").hover(function(){
    var _this=$(this);
   var isHasNav=_this.children(".list_nav").length;
   if(isHasNav){
      _this.find(".list_nav").stop().animate({width:"446px"},300);
      if(_this.parents(".col_list").hasClass("list2")){
         _this.find(".kg_ico").stop().animate({left:"596px"},300);
      }
      /*if(_this.parents(".col_list").hasClass("list3")){
         _this.find(".kg_ico").stop().animate({right:"596px"},300);
      }*/
      _this.find(".kg_ico").addClass("kg_ico_on");
      _this.find("ul").show();
   }
},function(){
    var _this=$(this);
   _this.find(".list_nav").stop().animate({width:"0"},300);
    if(_this.parents(".col_list").hasClass("list2")){
         _this.find(".kg_ico").stop().animate({left:"180px"},300);
      }
      /*if(_this.parents(".col_list").hasClass("list3")){
         _this.find(".kg_ico").stop().animate({right:"180px"},300);
      }*/
    _this.find(".kg_ico").removeClass("kg_ico_on");
   _this.find("ul").hide();
})

//Ó¦ÓÃÈí¼þ²¿·Ö£¬µã»÷ÇÐ»»
$(".app_links a").click(function(){
    var _this=$(this);
   var index=$(this).index();
   $("#appCon").find(".con").hide().end().find(".app_con_"+(index+1)).show();
})


//µ¼º½Ð§¹û
$(".nav li").hover(function(){
    var $this=$(this);
    var index=$this.index();
    var tag_width=$this.outerWidth(true);
    var offset=-512+(index*tag_width);
    var left = (index*tag_width);
    $('#line').stop().animate({ left: left+'px' }, 200);


},function(){
    $(this).parents(".head_box").css("background-position","-512px bottom");
    $('#line').stop().animate({ left:'0px' }, 200);
});


