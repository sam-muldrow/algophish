                <div id="p_lt_ctl02_pageplaceholder_p_lt_ctl01_OU_Course_FinderSearchBox_divScript">
                    <script>

                        (function ($, EEP) {
                            function initSearchPG() {
                                new EEP.searchCourseObj({
                                    txtSearchItem: "#txtSearch",
                                    btnSearchItem: ".searchBtn",
                                    studyLevel: ".searchStudylevel",
                                    redirectUrl: "/postgraduate/results"
                                });
                            }
                            function initSearchCourse() {

                                new EEP.searchCourseObj({
                                    txtSearchItem: "#txtSearch",
                                    btnSearchItem: ".searchBtn",
                                    studyLevel: ".searchStudylevel",
                                    redirectUrl: "/courses/results"
                                });
                            }
                            function initSearchIsW() {

                                new EEP.searchCourseObj({
                                    txtSearchItem: "#txtSearch",
                                    btnSearchItem: ".searchBtn",
                                    studyLevel: ".searchStudylevel",
                                    redirectUrl: "/courses/courses/results"
                                });
                            }
                            var currentSite = "CMS.SiteProvider.SiteInfo (cms.site) - Courses";
                            currentSite = currentSite.slice(currentSite.lastIndexOf(' ') + 1);
                            if (currentSite.toLowerCase() == "courses") {
                                initSearchCourse();
                            }
                            else if (currentSite.toLowerCase() == "postgraduate") {
                                initSearchPG();
                            }
                            else if (currentSite.toLowerCase() == "isw") {
                                initSearchIsW();
                            }

                            $('input.searchStudylevel[type="radio"]').on('click change', function (e) {
                                if (currentSite.toLowerCase() == "isw") {
                                    if ($(this).val().toLowerCase() == "undergraduate") { initSearchIsW(); }
                                    else if ($(this).val().toLowerCase() == "postgraduate") { initSearchIsW(); }
                                }
                                else {
                                    if ($(this).val().toLowerCase() == "undergraduate") { initSearchCourse(); }
                                    else if ($(this).val().toLowerCase() == "postgraduate") { initSearchPG(); }
                                }
                            });
                        })($, window.OU.EEP);


                    </script>
                </div>
                <!-- CUSTOM JS END -->
                <!--Hawksearch Header Includes -->
                <div id="p_lt_ctl02_pageplaceholder_p_lt_ctl01_OU_Course_FinderSearchBox_hawk">

                    <link href="https://openuniversity.hawksearch.com/sites/openuniversity/includes/hawksearch.css" type="text/css" rel="stylesheet" />


                </div>

                <script>
                    function charLengthError1(e) {
                        if (e.keyCode == 13) {
                            if ($('.searchTextarea').val().length === 0) {
                                $('#sptxtSearchError').show();
                                e.stopImmediatePropagation();
                                e.preventDefault();
                            }
                        }
                        return true;
                    }
                </script>

                <script>


                    function charLengthError(e) {
                        if ($('.searchTextarea').val().length === 0) {
                            $('#sptxtSearchError').show();
                            e.stopImmediatePropagation();
                            e.preventDefault();
                        }
                        else if ($('.searchTextarea').val().length > 100) {
                            $('#sptxtMaxCountError').show();
                            e.stopImmediatePropagation();
                            e.preventDefault();
                        }
                    }
                </script>

                <script>

                    $('.searchTextarea').keyup(function (e) {
                        if ($('.searchTextarea').val().length > 100) {
                            $('#sptxtMaxCountError').show();
                            $(".searchBtn").addClass("int-button-disabled");
                        }
                        else {
                            $('#sptxtSearchError').hide();
                            $('#sptxtMaxCountError').hide();
                            $(".searchBtn").removeClass("int-button-disabled");
                        }
                    });

                </script>

            </div>


