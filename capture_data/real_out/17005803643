                                            <button id="searchBtn" onclick="charLengthError(event)" class="int-search--submit searchBtn"
                                                    aria-label="Find courses">
                                                <i class="int-icon int-icon-search"></i>
                                                <span class="searchBtnText">Find Courses</span>
