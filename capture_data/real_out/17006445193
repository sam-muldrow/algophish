<script>
	$(function() {
		var skyscrapers = $('.skyscraper').add('.impulse-sky');
	    var menu = $('#menu'),
			pos = menu.offset();

		if(skyscrapers.offset()) {
			var top = skyscrapers.offset().top - 40;

			$(window).scroll(function() {
				var y = $(window).scrollTop();
				if (y > pos.top) {
					skyscrapers.addClass('fixed');
				} else {
					skyscrapers.removeClass('fixed');
				}
			});
		}
	});
</script>

