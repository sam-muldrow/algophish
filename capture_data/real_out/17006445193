	var pushCustomAttributeToOptimizelyX = function(attributeName, attributeValue) {
		var jsonAttribute = {};
		jsonAttribute[attributeName] = attributeValue;

		window['optimizely'] = window['optimizely'] || [];
		window["optimizely"].push({
			"type": "user",
			"attributes": jsonAttribute
		});
	};

	var pushCustomEventToOptimizelyX = function (eventName) {
		window['optimizely'] = window['optimizely'] || [];
		window["optimizely"].push({
			type: "event",
			eventName: eventName
		});
	};

	window['optimizely'] = window['optimizely'] || []; 
