function moreinfo_freedial() {
	var url = ''
		+'http://www.juno.com/start/landing.do?page=www/free/index'
	if (gup('refcd') == 'CJ') {
		url+='&refcd=CJ';
	}
	window.location.href=url
	return false;
}
function moreinfo_freeemail() {
	var url = ''
		+'http://www.juno.com/freeemail'
	if (gup('refcd') == 'CJ') {
		url+='?refcd=CJ';
	} else {
		url+='?refcd=JUWWW1209';
	}
	window.location.href=url
	return false;
}
                   
</script>



