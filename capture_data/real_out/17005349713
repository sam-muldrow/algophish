	function myErrorHandler(errorMessage,url,line) {
	 	var url = 'https://my.juno.com/start/javascript.do?message=' + escape('at  ' + line + ' ' + errorMessage + ' url: ' + url);
	 	if (errImg) errImg.src = url;
	 	return true;
	}
