    <script type="text/javascript">
        function resizeEsquireIframe(iframe) {

            var pagewidth = document.documentElement.clientWidth;
            if (pagewidth <= 432) {
                iframe.height = "470px";
            } else if (pagewidth > 432 && pagewidth < 710) {
                iframe.height = "280px";
            } else if (pagewidth >= 710) {
                iframe.height = "280px";
            }

        }
    </script>


