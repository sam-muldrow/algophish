    </script></div></div></div><div class="flexmodule-clear"></div></div></div></div></div></div><div contenttreeid="reference_1545837558" contenttreestatus="Not published" style="display:none;"></div></div><div class="reference parbase section"><div class="unstructuredpage page basicpage"><div class="par parsys "><div class="reference parbase section"><script>/**
 * Injects Topnav Submenus, which are built via shared content pages, within the TLW Topnav component.
 * Also sets up their show/hide functionality on Topnav menu item hover.
 */
(function($, tlwRollovers) {

    var $topnav = tlwRollovers.$topnav;
    var $topnavSubmenus = $('header .topnav-submenu');
    initTopnavSubmenus();

    /**
     * Initializes all topnav submenus.
     */
    function initTopnavSubmenus() {
        for (var i = 0; i < $topnavSubmenus.length; i++) {
            var $topnavSubmenu = $($topnavSubmenus.get(i)).detach();
            var $topnavMenuBarItem = $topnav.find('.main .menu > li:nth-child(' + (i + 1) + ')');
            updateFlexModuleImgSizes($topnavSubmenu);
            $topnavMenuBarItem.find('.topnav-submenu-slot').replaceWith($topnavSubmenu);
            addHoverClassOnFocus($topnavMenuBarItem);
            removeHoverClassOnBlur($topnavMenuBarItem);
        }
    }

    /**
     * Updates the sizes of all images that are descendants of flex-modules within a given topnav submenu
     * So that they are medium sized squares.
     * @param {jQuery} $topnavSubmenu The topnav submenu jQuery element.
     */
    function updateFlexModuleImgSizes($topnavSubmenu) {
        var $flexModuleImgs = $topnavSubmenu.find('.flexmodule img');
        $flexModuleImgs.each(function (i, img) {
            var $img = $(img);
            // Changed image fixed width & height.
            $img.attr('width', '150');
            $img.attr('height', '150');
            $img.removeClass('img-209 img-209x131 img-60 img-60x60 img-60x88');
            $img.addClass('img-150 img-150x150');

            // Remove all source tags b/c image with and height is being fixed.
            var $picture = $img.parent();
            if ($picture.is('picture')) {
                $picture.find('source').remove();
            }

            // Update all img src/srcset attributes with new fixed size.
            if ($img.attr('data-src')) {
                $img.attr('data-src', $img.attr('data-src').replace(/\.img\.\d+\.\d+\./, '.img.150.150.'));
            }
            if ($img.attr('src')) {
                $img.attr('src', $img.attr('src').replace(/\.img\.\d+\.\d+\./, '.img.150.150.'));
            }
            if ($img.attr('data-srcset')) {
                $img.attr('data-srcset', $img.attr('data-srcset').replace(/\.img\.\d+\.\d+\./, '.img.300.300.'));
            }
            if ($img.attr('srcset')) {
                $img.attr('srcset', $img.attr('srcset').replace(/\.img\.\d+\.\d+\./, '.img.300.300.'));
            }
        });
    }

    /**
     * Adds a .hover class to a given topnav menu bar item when it is focused.
     * The .hover class is used to simulate hovering over the topnav menu bar item while any of it's child elements hold focus.
     * @param {jQuery} $topnavMenuBarItem The topnav menu bar item (li).
     */
    function addHoverClassOnFocus($topnavMenuBarItem) {
        $topnavMenuBarItem.find('> a').on('focus', function() {
            $topnavMenuBarItem.addClass('hover');
        });
    }

    /**
     * Removes a .hover class from a given topnav menu bar item when it and all of its children no longer have focus (are blurred).
     * @param {jQuery} $topnavMenuBarItem The topnav menu bar item (li).
     */
    function removeHoverClassOnBlur($topnavMenuBarItem) {
        $topnavMenuBarItem.find('a').on('blur', function() {
            setTimeout(function() {
                if (!$topnavMenuBarItem.find(document.activeElement).length) {
                    $topnavMenuBarItem.removeClass('hover');
                }
            });
        });
    }

})(jQuery, UBCMS.namespace('tlwRollovers'));
</script><div contenttreeid="reference_1793490947-6" contenttreestatus="Not published" style="display:none;"></div></div><div class="reference parbase section"><script>/**
 * Injects Topnav (Drawer) Submenus, which are built via shared content pages, within the TLW Topnav Drawer component.
 * Also, sets up their contained collapsible children.
 */
 (function($, tlwRollovers) {

    var $topnavDrawer = tlwRollovers.$topnavDrawer;
    var $topnavSubmenus = $('header .topnav-submenu');
    initMobileTopnavSubmenus();

    /**
     * Initializes all mobile topnav submenus.
     */
    function initMobileTopnavSubmenus() {
        for (var i = 0; i < $topnavSubmenus.length; i++) {
            var $topnavDrawerSubmenu = $($topnavSubmenus.get(i)).clone();
            var $topnavDrawerMainItem = $topnavDrawer.find('.main .menu > li:nth-child(' + (i + 1) + ')');
            $topnavDrawerMainItem.find('.topnav-submenu-slot').replaceWith($topnavDrawerSubmenu);
            makeSubmenuColumnsCollapsible($topnavDrawerSubmenu);
            makeHighlightsPanelsCollapsible($topnavDrawerSubmenu);
            makeSubmenuBottomBarCollapsible($topnavDrawerSubmenu);
            renameTopnavIds($topnavDrawerSubmenu);
        }
    }

    /**
     * Makes all simple columns which provide a list of links within a given topnav drawer submenu collapsible.
     * @param {jQuery} $topnavDrawerSubmenu The topnav drawer submenu.
     */
    function makeSubmenuColumnsCollapsible($topnavDrawerSubmenu) {
        var $submenuColumns = $topnavDrawerSubmenu.find('> .parsys_column > .parsys_column:not(:first-child)');
        $submenuColumns.each(function(idx, submenuColumn) {
            const $flexmodules = $(submenuColumn).find('.flexmodule');
            $flexmodules.each(function(idx, flexmodule) {
                var $teaserInner = $(flexmodule).find('> .flexmodule-inner .teaser > .teaser-inner');
                makeTeaserContentCollapsible($teaserInner, true);
            });
        });
    }

    /**
     * Makes a given teaser content element which contains a list of links into a collapsible.
     * @param {jQuery} $teaserInner The teaser inner which will become the collapsible container.
     * @param {boolean} initOpen Whether or not to initialize the collapsible in an open state. Defaults to false.
     */
     function makeTeaserContentCollapsible($teaserInner, initOpen) {
        var $teaserTitle = $teaserInner.find('.teaser-content > .teaser-title');
        var $collapsibleContent = $teaserInner.find('.teaser-end > .teaser-content').clone();
        tlwRollovers.genCollapsible($teaserInner, $teaserTitle, $collapsibleContent, initOpen);
    }

    /**
     * Makes highlights panels, which may be present within a given topnav drawer submenu, into collapsibles.
     * @param {jQuery} $topnavDrawerSubmenu The topnav drawer submenu.
     */
    function makeHighlightsPanelsCollapsible($topnavDrawerSubmenu) {
        var $highlightsPanels = $topnavDrawerSubmenu.find('.topnav-submenu-highlights-panel');
        $highlightsPanels.each(function(idx, highlightsPanel) {
            var $highlightsPanel = $(highlightsPanel);
            var $highlightsPanelTeaserInner = $highlightsPanel.find(
                '> .flexmodule > .flexmodule-inner .teaser > .teaser-inner'
            );
            $highlightsPanelTeaserInner.length
                ? makeTeaserContentCollapsible($highlightsPanelTeaserInner, true)
                : makeTextHighlightsPanelCollapsible($highlightsPanel.find('> .text'));
        });
    }

    /**
     * Makes a text component based highlights panel into a collapsible.
     * @param {jQuery} $highlightsPanelText The highlights panel text element.
     */
    function makeTextHighlightsPanelCollapsible($highlightsPanelText) {
        var $textTitle = $highlightsPanelText.find('> div, > p').first();
        var $textContent = $highlightsPanelText.find('> ul');
        tlwRollovers.genCollapsible($highlightsPanelText, $textTitle, $textContent, true);
    }

    /**
     * Makes a bottom bar within a given topnav drawer submenu into a collapsible.
     * @param {jQuery} $topnavDrawerSubmenu The topnav drawer submenu.
     */
    function makeSubmenuBottomBarCollapsible($topnavDrawerSubmenu) {
        var $submenuBottomBar = $topnavDrawerSubmenu.find('> .topnav-submenu-bottom-bar');
        var $teaserInner = $submenuBottomBar.find('.flexmodule > .flexmodule-inner .teaser > .teaser-inner');
        makeTeaserContentCollapsible($teaserInner, true);
    }

    /**
     * Renames all IDs on elements that are present from copied & modified topnav submenus to be unique.
     * @param {jQuery} $topnavDrawerSubmenu The topnav drawer submenu.
     */
    function renameTopnavIds($topnavDrawerSubmenu) {
        var $submenuIdChildren = $topnavDrawerSubmenu.find('[id^="ubcms-gen"]');
        $submenuIdChildren.each(function(idx, submenuIdChild) {
            var $submenuIdChild = $(submenuIdChild);
            $submenuIdChild.attr('id', $submenuIdChild.attr('id') + '-0');
        });
    }

})(jQuery, UBCMS.namespace('tlwRollovers'));
</script><div contenttreeid="reference-16" contenttreestatus="Not published" style="display:none;"></div></div></div></div><div contenttreeid="reference_1825964370" contenttreestatus="Not published" style="display:none;"></div></div><div class="reference parbase section"><div class="unstructuredpage page basicpage"><div class="par parsys "><div class="list parbase section"></div><script>UBCMS.longTermAlert.init()
</script></div></div><div contenttreeid="reference_299074475" contenttreestatus="Not published" style="display:none;"></div></div></div></div><div contenttreeid="innerheader" contenttreestatus="Not published" style="display:none;"></div></div></header><div id="skip-to-content"></div><div id="columns" class="clearfix"><div id="center-nocols" role="main"><div class="top parsys"><div id="tlw-video" class="appendaround-container"><div data-fixed class="appendaround"><div class="cq-placeholder" data-emptytext="Content Optimization Container - ID for Google Tag Manager"></div></div><div class="reference2 reference parbase section"><div class="hero-video-wrapper"><div id="mobile-play" class="mobile-play icon-play fa fa-play-circle"></div><div class="icon-center"><button class="toggle-playback"> <i class="icon-play icon-pause2"></i> <span class="ada-hidden">Pause background video</span> </button></div></div><link class="hero-video-link" rel="stylesheet" href="/etc/designs/ubmobile/main/clientlibs-static/css/fontawesome.css" type="text/css"><link class="hero-video-link" rel="stylesheet" href="/etc/designs/ubmobile/main/clientlibs-static/css/solid.css" type="text/css"><link class="hero-video-link" rel="stylesheet" href="/content/shared/www/eub/tlw-prototypes/hero-video-playlist-mobile/video.css" type="text/css"><script src="/content/shared/www/eub/tlw-prototypes/hero-video-playlist-mobile/video.js"></script><script>
