</style></div></div><div class="reference parbase section"><div class="unstructuredpage page basicpage"><div class="par parsys "><div class="reference parbase section"><div class="unstructuredpage page basicpage"><div class="par parsys "><div class="reference parbase section"><script>(function($, tlwRollovers) {

    /**
     * Initializes an HTML template (script tag).
     * @param {string} templateSelector The selector for the HTML template script tag.
     * @param {string} slotSelector The selector for the slot that which the initialized template shall replace.
     * @return {jQuery} The initialized template.
     */
    tlwRollovers.initTemplate = function(templateSelector, slotSelector) {
        var elementHTML = $(templateSelector).html();
        var $element = $(document.createElement('div')).html(elementHTML).find('> *');
        $(slotSelector).replaceWith($element);
        return $element;
    }

})(jQuery, UBCMS.namespace('tlwRollovers'));
</script><div contenttreeid="reference_888301681" contenttreestatus="Not published" style="display:none;"></div></div><div class="reference parbase section"><script>(function($, tlwRollovers) {

    var animationRecords = {};

    /**
     * Gets the duration of animations within the custom TLW header.
     * @return {number} The animation duration in milliseconds.
     */
    tlwRollovers.getAnimationDuration = function() {
        var header = $('header .innerheader .tlw-mobile-nav');
        var animationDuration = 200; // Default to 200 in case we cannot get duration from .innerheader CSS style.
        var headerTransition = header.css('transition');
        if (headerTransition) {
            var headerTransitionParts = headerTransition.split(' ');
            if (headerTransitionParts.length > 1) {
                var headerTransitionDuration = headerTransitionParts[1];
                animationDuration = parseFloat(headerTransitionDuration) * 1000;
            }
        }
        return animationDuration;
    }

    /**
     * Accurately tracks a CSS animation until completion. Accounts for any animation direction reversal/interrupts.
     * @param {any} animationConfig A config object which should contain the following properties:
     * - beforeAnimationStart: A callback function that will be invoked just before the animation should start.
     * - startAnimation: A callback function that will be invoked to start the animation.
     * - afterAnimationComplete: A callback function that will be invoked once the animation completes.
     * @return {any} The animation tracking record.
     */
    tlwRollovers.trackAnimation = function(animationConfig) {
        var animationId = animationConfig.id;
        var fullAnimationDuration = tlwRollovers.getAnimationDuration();
        var animationRecord = (animationRecords[animationId] ? animationRecords[animationId] : {});
        animationRecords[animationId] = animationRecord;
        animationRecord.duration = (animationRecord.duration ? animationRecord.duration : fullAnimationDuration);

        // Recalc the (remaining) animation duration if it has reverted directions (been interrupted) before completing.
        if (animationRecord.afterCompleteTimeout) {
            var interruptElapsedTime = (new Date().getTime() - animationRecord.prevAnimationStartTime);
            animationRecord.duration = (fullAnimationDuration - animationRecord.duration) + interruptElapsedTime;
        }

        // Immediately invoke 'beforeAnimationStart' so that any necessary CSS changes may take effect before beginning animation.
        if (animationConfig.beforeAnimationStart) {
            animationConfig.beforeAnimationStart();
        }

        // Wait for any CSS changes (e.g. display changes) in 'beforeAnimationStart' to take effect before invoking 'startAnimation'.
        if (animationConfig.startAnimation) {
            setTimeout(function() {
                animationRecord.prevAnimationStartTime = new Date().getTime();
                animationConfig.startAnimation();
            });
        }

        // Set timeout based off of remaining animation duration so that 'afterComplete' may be invoked just after completion time.
        clearTimeout(animationRecord.afterCompleteTimeout);
        animationRecord.afterCompleteTimeout = setTimeout(function() {
            animationRecords[animationId] = null;
            if (animationConfig.afterAnimationComplete) {
                animationConfig.afterAnimationComplete();
            }
        }, animationRecord.duration);

        return animationRecord;
    }

})(jQuery, UBCMS.namespace('tlwRollovers'));
</script><div contenttreeid="reference" contenttreestatus="Not published" style="display:none;"></div></div></div></div><div contenttreeid="reference_1793490947" contenttreestatus="Not published" style="display:none;"></div></div><div class="reference parbase section"><div class="unstructuredpage page basicpage"><div class="par parsys "><div class="reference parbase section"><style>/*
    (*) large = 24px for regular weight or 18.5px for bold text
    See http://webaim.org/resources/contrastchecker/
*/
.d-none {
  display: none;
}
.collapsible-container > .collapsible-toggle {
  position: relative;
  display: block;
  text-align: left;
  border: none;
  width: 100%;
  cursor: pointer;
  overflow: hidden;
  padding: 0;
  background-color: inherit;
}
.collapsible-container > .collapsible-toggle:after {
  content: "\e91b";
  content: "+";
  overflow-y: hidden;
  float: right;
}
.collapsible-container > .collapsible-toggle[aria-expanded="true"]:after {
  content: "\e91c";
  content: "-";
}
.collapsible-container > .collapsible-toggle > .focus-mask {
  position: absolute;
  display: block;
  box-sizing: border-box;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 0;
}

</style><div contenttreeid="reference_888301679" contenttreestatus="Not published" style="display:none;"></div></div><div class="reference parbase section"><script>/**
 * Constructs a collapsible container via its provided title & content counterparts.
 */
(function($, tlwRollovers) {

    var collapsibleToggleIdCnt = 1; // ID used for auto-generating collapsible toggle IDs.

    /**
     * Generates a collapsible from the shared collapsible template found in 'tlw-rollovers/main-include/shared/collapsible.html'.
     * @param {jQuery} $collapsible A jQuery element that will serve as the collapsible contianer.
     * @param {jQuery} $collapsibleTitle A jQuery element that will serve as the collapsible title. Will be replaced with a toggle button element.
     * Class & ID attributes and text content will be transferred over.
     * @param {jQuery} $collapsibleContent A jQuery element which will serve as the collapsible content.
     * @param {boolean} initOpen Whether or not the collapsible shall be initialized as opened. Defaults to false for closed.
     * @return {jQuery} The generated & fully initialized collapsible.
     */
    tlwRollovers.genCollapsible = function($collapsible, $collapsibleTitle, $collapsibleContent, initOpen) {
        var $collapsibleToggle = genCollapsibleToggle($collapsibleTitle);
        setupCollapsibleContent($collapsibleContent, $collapsibleToggle);
        setupCollapsibleContainer($collapsible, $collapsibleToggle, $collapsibleContent);
        setupCollapsibleToggleFunctionality($collapsible, initOpen);
        return $collapsible;
    }

    /**
     * Generates a collapsible toggle button element from a given collapsible title element.
     * @param {jQuery} $collapsibleTitle The collapsible title jQuery element.
     * @return {jQuery} The generated collapsible toggle jQuery element.
     */
    function genCollapsibleToggle($collapsibleTitle) {
        // Create collapsible toggle button and transfer the given collapsible title class/ID attributes & text content over.
        var $collapsibleToggle = $(
            '<button aria-haspopup="true" aria-expanded="false" class="collapsible-toggle collapsible-title small" type="button">'
        ).addClass($collapsibleTitle.attr('class'))
         .attr('id', $collapsibleTitle.attr('id'))
         .text($collapsibleTitle.text());

        // Auto-generate an ID if one does not exist on given $collapsibleTitle so that association may be made with collapsible content.
        if (!$collapsibleToggle.attr('id')) {
            $collapsibleToggle.attr('id', 'header-collapsible-toggle-' + (collapsibleToggleIdCnt++))
        }

        // Append a focus mask element to prevent highlight on click, but allow highlight on tab focus.
        if (!$collapsibleToggle.find('focus-mask').length) {
            $collapsibleToggle.append($('<span class="focus-mask" tabindex="-1"></span>'))
        }

        return $collapsibleToggle;
    }

    /**
     * Sets up a given collapsible content element.
     * @param {jQuery} $collapsibleContent The collapsible content jQuery element.
     * @param {jQuery} $collapsibleToggle The collapsible toggle jQuery element.
     */
    function setupCollapsibleContent($collapsibleContent, $collapsibleToggle) {
        $collapsibleContent.addClass('collapsible-content');
        $collapsibleContent.attr('role', 'region');
        $collapsibleContent.attr('aria-labelledby', $collapsibleToggle.attr('id'));
    }

    /**
     * Sets up a given collapsible container.
     * @param {jQuery} $collapsible The collapsible container jQuery element.
     * @param {jQuery} $collapsibleToggle The collapsbile toggle jQuery element.
     * @param {jQuery} $collapsibleContent The collapsible content jQuery element.
     */
    function setupCollapsibleContainer($collapsible, $collapsibleToggle, $collapsibleContent) {
        $collapsible.addClass('collapsible-container');
        $collapsible.empty();
        $collapsible.append($collapsibleToggle);
        $collapsible.append($collapsibleContent);
    }

    /**
     * Sets up a collapsible toggle functionality, and initializes starting collapsed state.
     * @param {jQuery} $collapsible The collapsible container (contains collapsible toggle and content).
     * @param {boolean} initOpen An optional flag that determines whether or not to initialize the collapsible as open. Default is false.
     */
    function setupCollapsibleToggleFunctionality($collapsible, initOpen) {
        $collapsible = $($collapsible); // Make sure jQuery element.
        var $collapsibleToggle = $collapsible.find('> .collapsible-toggle');
        var $collapsibleContent = $collapsible.find('> .collapsible-content');

        // Setup collapsible toggle button click handler.
        $collapsibleToggle.click(function() {
            toggleCollapsible($collapsible);
        });

        // Initialize the opened status of the collapsible.
        $collapsibleContent.hide();
        if (initOpen) {
            toggleCollapsible($collapsible);
        }
    }

    /**
     * Toggles a given collapsible.
     * @param {jQuery} $collapsible The collapsible that is to be toggled.
     */
    function toggleCollapsible($collapsible) {
        $collapsible = $($collapsible); // Make sure jQuery element.
        $collapsibleToggle = $collapsible.find('> .collapsible-toggle');
        $collapsibleContent = $collapsible.find('> .collapsible-content');

        $collapsible.toggleClass('expanded');
        var expanded = ($collapsible.hasClass('expanded') ? 'true' : 'false');
        $collapsibleContent.slideToggle('fast');
        $collapsibleToggle.attr('aria-expanded', expanded);
    }

