</script><div contenttreeid="reference_1633253224" contenttreestatus="Not published" style="display:none;"></div></div><div class="reference parbase section"><script>(function($, tlwRollovers) {

    /**
     * Sets up the sticky header functionality.
     */
    tlwRollovers.initStickyHeaderFunctionality = function() {
        var $header = $('header .innerheader');
        var $topnavRibbon = $header.find('.header-ribbon');
        var topnavRibbonHeight = ($topnavRibbon.height() + 10) // (+10) Account for bottom :after part.
        var immediateTransitionThreshold = 6; // Should immediately snap header back into relative position when scroll up to this point.
        var animateTransitionThreshold = (topnavRibbonHeight + 80); // (+80) Give a bit of space before transition.
        var removeStickyTimeout = null;
        var stickyAnimationInterval = null;
        $header.addClass('animated');
        initStickyScrollListener();

        function initStickyScrollListener() {
            $(window).scroll(function() {
                var scrollPos = $(window).scrollTop();
                var hasShowClass = $header.hasClass('show');
                var hasStickyClass = $header.hasClass('sticky');
                if (scrollPos >= animateTransitionThreshold && !hasShowClass) {
                    animatedAddSticky();
                } else if (scrollPos <= immediateTransitionThreshold && hasStickyClass) {
                    immediateRemoveSticky();
                } else if (scrollPos < animateTransitionThreshold && hasShowClass) {
                    animatedRemoveSticky();
                }
            });
        }

        function animatedAddSticky() {
            clearStickyTimers(); // Make sure we clear remove sticky timeout if quickly scrolling back down again to sticky.
            $header.removeClass('animated');
            $header.removeClass('show');
            $header.addClass('sticky');
            setTimeout(function() {
                $header.addClass('animated');
                $header.addClass('show');
            });
        }

        function immediateRemoveSticky() {
            clearStickyTimers();
            $header.removeClass('show');
            setTimeout(function() {
                $header.removeClass('sticky');
            });
        }

        function animatedRemoveSticky() {
            clearStickyTimers();
            $header.removeClass('show');
            removeStickyAfterAnimation();
            // Monitor sticky header position relative to top of document so we can cancel animation if needed.
            stickyAnimationInterval = setInterval(stopAnimationIfDocumentTop, 5);
        }

        function removeStickyAfterAnimation() {
            var animationDuration = tlwRollovers.getAnimationDuration();
            removeStickyTimeout = setTimeout(function() {
                clearStickyTimers();
                $header.removeClass('sticky');
            }, animationDuration);
        }

        function stopAnimationIfDocumentTop() {
            var top = $header.offset().top;
            if (top <= immediateTransitionThreshold) {
                immediateRemoveSticky();
            }
        }

        function clearStickyTimers() {
            clearInterval(stickyAnimationInterval);
            clearTimeout(removeStickyTimeout);
        }
    }

})(jQuery, UBCMS.namespace('tlwRollovers'));
</script><div contenttreeid="reference_1562732734" contenttreestatus="Not published" style="display:none;"></div></div><div class="reference parbase section"><script>/**
 * Constructs the new TLW header, which is used uniformly in both desktop and mobile.
 * Note that this is not responsible for constructing the TLW Topnav or Topnav Drawer components.
 */
(function($, tlwRollovers) {

    tlwRollovers.$oldHeader = $('header .header');
    tlwRollovers.$header = tlwRollovers.initTemplate('#header-tmpl', 'header .header');

    var $header = tlwRollovers.$header;
    determineTitleElement();
    fixJaggedTitleRibbonSafari();
    tlwRollovers.initStickyHeaderFunctionality();

    /**
     * Determines the header title element. If the header is on eub-home, then it transitions to an h1.
     * Otherwise, it remains a div so that it does not conflict with a more specific page h1.
     */
    function determineTitleElement() {
        var $headerTitle = $header.find('.header-title');
        if ($('.eub-home').length > 0) {
            var $headerTitleH1 = $(document.createElement('h1'));
            $headerTitle.children().each(function(idx, headerTitleChild) {
                $headerTitleH1.append(headerTitleChild);
            });
            $headerTitleH1.addClass($headerTitle.attr('class'));
            $headerTitle.replaceWith($headerTitleH1);
        }
    }

    /**
     * Fixes the 'jagged ribbon' bug that only occurs on Safari OSX.
     */
    function fixJaggedTitleRibbonSafari() {
        if (
            navigator.userAgent.indexOf('Safari') != -1 &&
            navigator.userAgent.indexOf('Chrome') == -1 &&
            navigator.userAgent.indexOf('CriOS/') == -1
        ) {
            $header.find('.header-ribbon .ribbon-bottom').addClass('safari-fix-jagged-borders');
        }
    }

})(jQuery, UBCMS.namespace('tlwRollovers'));
</script><div contenttreeid="reference-1" contenttreestatus="Not published" style="display:none;"></div></div></div></div><div contenttreeid="reference_1203489329" contenttreestatus="Not published" style="display:none;"></div></div><div class="reference parbase section"><div class="unstructuredpage page basicpage"><div class="par parsys "><div class="reference parbase section"><style>/*
