</style><div contenttreeid="reference_1023165242-1" contenttreestatus="Not published" style="display:none;"></div></div><div class="reference parbase section"><!-- Topnav drawer toggle button template (to be placed inside .header) --><script type="text/template" id="topnav-drawer-toggle-tmpl">
    <button id="topnav-drawer-toggle" aria-haspopup="true" aria-expanded="false" class="topnav-drawer-toggle">
        <span class="focus-mask" tabindex="-1"></span>
        <span class="ada-hidden">Top Navigation Drawer Toggle</span>
    </button>
</script><!-- Topnav drawer template (to be placed immediately after .header) --><script type="text/template" id="topnav-drawer-tmpl">
    <div class="topnav-drawer-overlay">
        <div class="topnav-drawer-backdrop"></div>
        <div class="topnav-drawer" role="region" aria-labelledby="topnav-drawer-toggle">
            <nav class="topnav-inner" aria-label="site top navigation drawer">
                <!-- Uses appendAround.js script to transfer desktop search form into the following element via data-set attribute. -->
                <div id="mobile-search" class="search bar" data-set="mobile-search"></div>
                <div class="main">
                    <!-- Will be populated with items from original topnav menu. -->
                    <ul class="menu"></ul>
                </div>
                <div class="secondary">
                    <!-- Slots for all of the additional nav menus. Will be populated based off of presence in original header/topnav. -->
                    <div class="audiencenav-slot"></div>
                    <div class="tasknav-slot"></div>
                    <div class="socialnav-slot"></div>
                </div>
            </nav>
        </div>
    </div>
</script><div contenttreeid="reference_1350604688-1" contenttreestatus="Not published" style="display:none;"></div></div><div class="reference parbase section"><script>/**
 * Constructs the new TLW topnav drawer for mobile viewports from the items that are present in the new TLW topnav for desktop.
 */
(function($, tlwRollovers) {

    // Insert custom topnav drawer (replacement for old mobile menu) and initialize its collapsibles.
    var $innerheader = $('header .innerheader');
    var $topnavDrawer = tlwRollovers.initTemplate('#topnav-drawer-tmpl', 'header .topnav-drawer-slot');
    var $topnavDrawerToggle = tlwRollovers.initTemplate('#topnav-drawer-toggle-tmpl', 'header .topnav-drawer-toggle-slot');
    tlwRollovers.$topnavDrawer = $topnavDrawer;
    tlwRollovers.$topnavDrawerToggle = $topnavDrawerToggle;

    initTopnavDrawerMainMenu();
    initTopnavDrawerAudiencenav();
    initTopnavDrawerTasknav();
    initTopnavDrawerSocialnav();
    listenForTopnavDrawerToggle();
    listenForTopnavDrawerScroll();
    closeTopnavDrawerOnBlur();

    /**
     * Initializes the new topnav's menu bar with items from the new topnav.
     */
    function initTopnavDrawerMainMenu() {
        var $topnavMenuBarItems = tlwRollovers.$topnav.find('.main > ul.menu > li');
        var $topnavDrawerMainMenu = $topnavDrawer.find('.main > ul.menu');
        $topnavMenuBarItems.each(function(idx, topnavMenuBarItem) {
            var $topnavMenuBarItem = $(topnavMenuBarItem).clone();
            var $topnavMenuBarItemTitle = $topnavMenuBarItem.find('> a');
            $topnavMenuBarItemTitle.attr('id', $topnavMenuBarItemTitle.attr('id') + '-0');
            var $topnavDrawerSubmenu = $('<div>').append($('<div class="topnav-submenu-slot">'));
            var $topnavDrawerMainMenuItem = $(document.createElement('li')).addClass($topnavMenuBarItem.attr('class'));
            tlwRollovers.genCollapsible($topnavDrawerMainMenuItem, $topnavMenuBarItemTitle, $topnavDrawerSubmenu);
            $topnavDrawerMainMenu.append($topnavDrawerMainMenuItem);
        });
    }

    /**
     * Initializes the new topnav's audiencenav menu with the old topnav's audiencenav menu.
     */
    function initTopnavDrawerAudiencenav() {
        var $audiencenav = tlwRollovers.$oldTopnav.find('.audiencenav').clone();
        var $audiencenavWrapper = $audiencenav.find('.audiencenav-wrapper');
        var $audiencenavLabel = $audiencenavWrapper.find('> .label');
        var $audiencenavContent = $audiencenavWrapper.find('> ul');
        tlwRollovers.genCollapsible($audiencenavWrapper, $audiencenavLabel, $audiencenavContent);
        $topnavDrawer.find('.audiencenav-slot').replaceWith($audiencenav);
    }

    /**
     * Initializes the new topnav drawer's tasknav menu with the old topnav's tasknav menu.
     */
    function initTopnavDrawerTasknav() {
        var $tasknav = tlwRollovers.$oldHeader.find('.tasknav').clone();
        $topnavDrawer.find('.tasknav-slot').replaceWith($tasknav);
    }

    /**
     * Initializes the new topnav drawer's socialnav menu with the old topnav's socialnav menu.
     */
    function initTopnavDrawerSocialnav() {
        var $socialnav = tlwRollovers.$oldHeader.find('.social').clone().toggleClass('social socialnav');
        $topnavDrawer.find('.socialnav-slot').replaceWith($socialnav);
    }

    /**
     * Listens for actions that cause the toggle state of the topnav drawer to change.
     */
    function listenForTopnavDrawerToggle() {
        var $topnavDrawerBackdrop = $('header .topnav-drawer-backdrop');
        $topnavDrawerToggle.click(toggleTopnavDrawer.bind(this, $topnavDrawerToggle));
        $topnavDrawerBackdrop.click(toggleTopnavDrawer.bind(this, $topnavDrawerToggle));
    }

    /**
     * Toggles the topnav drawer open state.
     * @param {jQuery} $topnavDrawerToggle The topnav drawer toggle button.
     */
    function toggleTopnavDrawer($topnavDrawerToggle) {
        var isOpenBeforeToggle = $innerheader.hasClass('topnav-drawer-open');

        tlwRollovers.trackAnimation({
            id: 'topnav-drawer',
            // Add transition class for manipulating display style before animation.
            beforeAnimationStart: function() {
                $innerheader.addClass('topnav-drawer-transitioning');
            },
            // Perform the animation once the display style has fully taken effect.
            startAnimation: function() {
                $('html').toggleClass('disable-scroll');
                $innerheader.toggleClass('topnav-drawer-open');
                $topnavDrawerToggle.attr('aria-expanded', !isOpenBeforeToggle ? 'true' : 'false');
            },
            // Remove the transition class for manipulating display style after animation.
            afterAnimationComplete: function() {
                $innerheader.removeClass('topnav-drawer-transitioning');
            }
        });
    }

    /**
     * Sets up a scroll listen for the topnav drawer.
     * When the content is scrolled down, a border will appear at the bottom of the topnav drawer toggle button.
     */
    function listenForTopnavDrawerScroll() {
        $topnavDrawer.scroll(function() {
            if ($topnavDrawer.scrollTop() === 0 && $topnavDrawerToggle.hasClass('topnav-drawer-scrolled-down')) {
                $topnavDrawerToggle.removeClass('topnav-drawer-scrolled-down');
            } else if ($topnavDrawer.scrollTop() > 0 && !$topnavDrawerToggle.hasClass('topnav-drawer-scrolled-down')) {
                $topnavDrawerToggle.addClass('topnav-drawer-scrolled-down');
            }
        });
    }

    /**
     * Sets up blur listeners that will cause the topnav drawer to close when focus is tabbed out of it.
     */
    function closeTopnavDrawerOnBlur() {
        var $topnavDrawerTargets = $innerheader.find('.header, .topnav-drawer-overlay');
        $topnavDrawerTargets.find('a,input,button').on('blur', function() {
            setTimeout(function() {
                if (!$topnavDrawerTargets.find(document.activeElement).length && $innerheader.hasClass('topnav-drawer-open')) {
                    $topnavDrawerToggle.click();
                }
            });
        });
    }

})(jQuery, UBCMS.namespace('tlwRollovers'));</script><div contenttreeid="reference-3" contenttreestatus="Not published" style="display:none;"></div></div></div></div><div contenttreeid="reference_1984066981" contenttreestatus="Not published" style="display:none;"></div></div><div class="reference parbase section"><div class="unstructuredpage page basicpage"><div class="par parsys "><div class="reference parbase section"><script>(function($, tlwRollovers) {

    // Contains the IDs of all of the Funnelback search-concierge scripts that must be externally loaded before adding concirege (autocomplete) to the page.
    var prereqSearchScriptIds = ['handlebars-script', 'typeahead-script', 'typeahead-fb-script'];
    var loadedSearchScripts = {};
    prereqSearchScriptIds.forEach(function (searchScriptId) {
        loadedSearchScripts[searchScriptId] = false;
    });

    /**
     * Registers an external Funnelback search script as loaded.
     * @param {string} searchScriptId The ID (without hastag) of the search script element.
     */
    tlwRollovers.registerSearchScriptLoad = function(searchScriptId) {
        loadedSearchScripts[searchScriptId] = true;
    }

    /**
     * Invokes a given callback function once all external Funnelback search scripts are async loaded.
     * @param {Function} cbFn The callback function that is to be invoked.
     */
    tlwRollovers.onAllSearchScriptsLoaded = function(cbFn) {
        (areAllPrereqScriptsLoaded())
            ? cbFn()
            : listenForSearchScriptsLoaded(cbFn);
    }

    /**
     * Listens for all external Funnelback search scripts to load, and then invokes a given callback function.
     * @param {Function} cbFn The callback function that is to be invoked.
     */
    function listenForSearchScriptsLoaded(cbFn) {
        prereqSearchScriptIds.forEach(function(searchScriptId) {
            var prereqScript = $('#' + searchScriptId);
            prereqScript.on('load', function() {
                loadedSearchScripts[searchScriptId] = true;
                if (areAllPrereqScriptsLoaded()) {
                    cbFn();
                }
            });
        });
    }

    /**
     * Whether or not all prerequisite Funnelback search (async) scripts have loaded.
     * @return true if they have all loaded, false if not.
     */
    function areAllPrereqScriptsLoaded() {
        var allLoaded = true;
        prereqSearchScriptIds.forEach(function(searchScriptId) {
            allLoaded = (allLoaded && loadedSearchScripts[searchScriptId]);
        });
        return allLoaded;
    }

})(jQuery, UBCMS.namespace('tlwRollovers'));
</script><div contenttreeid="reference_795709110" contenttreestatus="Not published" style="display:none;"></div></div><div class="reference parbase section"><style>/*
