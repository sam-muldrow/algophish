 * Constructs the new TLW topnav from the configurations and items that are present
 * in old Header & Top Navigation components that are present at the top of the containing header config page.
 */
(function($, tlwRollovers) {

    var $oldHeader = tlwRollovers.$oldHeader;
    var $oldTopnav = $('header .topnav').remove();
    var $topnav = tlwRollovers.initTemplate('#topnav-tmpl', 'header .topnav-slot');
    tlwRollovers.$oldTopnav = $oldTopnav;
    tlwRollovers.$topnav = $topnav;

    initTopnavMainMenuBar();
    initTopnavAudiencenav();
    initTopnavTasknav();

    /**
     * Initializes the new topnav's menu bar with items from the old topnav.
     */
    function initTopnavMainMenuBar() {
        var $oldTopnavMenuBarItems = $oldTopnav.find('.main > ul > li');
        var $topnavMenuBar = $topnav.find('.main > ul');
        $oldTopnavMenuBarItems.each(function(idx, topnavMenuBarItem) {
            var $topnavMenuBarItem = $(topnavMenuBarItem).detach();
            var $topnavSubmenuSlot = $(document.createElement('div')).addClass('topnav-submenu-slot');
            $topnavMenuBarItem.append($topnavSubmenuSlot);
            $topnavMenuBar.append($topnavMenuBarItem);
        });
    }

    /**
     * Initializes the new topnav's audiencenav menu with the old topnav's audiencenav menu.
     */
    function initTopnavAudiencenav() {
        var $audiencenav = $oldTopnav.find('.audiencenav').clone();
        $topnav.find('.audiencenav-slot').replaceWith($audiencenav);
    }

    /**
     * Initializes the new topnav's tasknav menu with the old topnav's tasknav menu.
     */
    function initTopnavTasknav() {
        var $tasknav = $oldHeader.find('.tasknav').clone();
        $topnav.find('.tasknav-slot').replaceWith($tasknav);
    }

})(jQuery, UBCMS.namespace('tlwRollovers'));</script><div contenttreeid="reference-2" contenttreestatus="Not published" style="display:none;"></div></div></div></div><div contenttreeid="reference_1984066980" contenttreestatus="Not published" style="display:none;"></div></div><div class="reference parbase section"><div class="unstructuredpage page basicpage"><div class="par parsys "><div class="reference parbase section"><style>/*
