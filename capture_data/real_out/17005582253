.review-carousel-component .review-carousel-container {
	max-width: 73rem !important;
}

.swiper-wrapper > div {
  display: none;
}

.review-card-component{
  cursor: pointer;
}
.review-carousel-component .review-carousel-container .swiper .swiper-pagination .swiper-pagination-bullet{
    margin: 0 0.3rem;}
</style>

    </div>
</div>
