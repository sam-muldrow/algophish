<script>
$(function(){

  $('ul.list-ranking-widget li .right').each(function(){
    //タイトル文字数が多い場合のみ
    if($(this).children('.inner').height()>=80){
      $(this).height($(this).children('.inner').height()).css('opacity','1');
    }
    
  })

  var $carousel = $('.list-carousel').flickity({prevNextButtons: false,pageDots: false,adaptiveHeight: true,draggable:false});
  //showed after init.
  $carousel.css({ opacity: 1 });
  // Flickity instance
  var flkty = $carousel.data('flickity');
  // elements
  var $cellButtonGroup = $('.ranking-page-tab ul');
  var $cellButtons = $cellButtonGroup.find('.tab');

  // update selected cellButtons
  $carousel.on( 'select.flickity', function() {
    $cellButtons.filter('.is-selected')
      .removeClass('is-selected');
    $cellButtons.eq( flkty.selectedIndex )
      .addClass('is-selected');
  });

  // select cell on button click
  $cellButtonGroup.on( 'click', '.tab', function() {
    var index = $(this).index();
    $carousel.flickity( 'select', index );
  });



});
</script>


<script src="https://cdn.jsdelivr.net/clipboard.js/1.5.13/clipboard.min.js"></script>
<script>
$(function(){
  var clipboard = new Clipboard('#copy-btn');
      clipboard.on('success', function(e) {
      //コピー成功時
      $("#copy-btn").addClass('is-copied')
      $("#copy-btn-result").text('ページタイトルとURLがコピーされました。').fadeIn();
      setTimeout(function(){
        $("#copy-btn-result").fadeOut().queue(function() {
          $("#copy-btn-result").text('');
        });
      },3000);
  });
  clipboard.on('error', function(e) {
      //エラー時
      $("#copy-btn").addClass('is-copied');
      $("#copy-btn-result").text('コピー機能に対応していません').fadeIn();
      setTimeout(function(){
        $("#copy-btn-result").fadeOut().queue(function() {
          $("#copy-btn-result").text('');
        });
      },3000);
  });
})
</script>













