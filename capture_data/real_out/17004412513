			<script type="text/javascript">
				var position_indicators = function pos_ind (e) {
					try {
						var top = $('#carousel-scroller .item.active .entry-content > div :last-child').offset().top;
						var left = $('#carousel-scroller .item.active h1.entry-title').offset().left;
						top += $('#carousel-scroller .item.active .entry-content > div :last-child').outerHeight();
						$('#carousel-scroller ol.carousel-indicators').css('top', top).css('left', left).css('margin-left', 0).css('text-align', 'left');
					}
					catch (e) {
						$('#carousel-scroller ol.carousel-indicators').css('top', '').css('left', '');
					}
				}
				$(document).ready( function () {
					position_indicators();
					$( window ).resize( position_indicators );
										onslide({relatedTarget:jQuery('#carousel-scroller div.item').first()});
									});
				var demo;
				window.demo = demo;
				var options = [{
				      ââuseEasing : true, 
				      ââuseGrouping : true, 
				      ââseparator : ',', 
				      ââdecimal : '.', 
				      ââprefix : '' ,
				      ââsuffix : '' 
				      },
				      {
				      ââuseEasing : true, 
				      ââuseGrouping : true, 
				      ââseparator : ',', 
				      ââdecimal : '.', 
				      ââprefix : '' ,
				      ââsuffix : '' 
				      },
				      {
				      ââuseEasing : true, 
				      ââuseGrouping : true, 
				      ââseparator : ',', 
				      ââdecimal : '.', 
				      ââprefix : '' ,
				      ââsuffix : '' 
				      },
				      {
				      ââuseEasing : true, 
				      ââuseGrouping : true, 
				      ââseparator : ',', 
				      ââdecimal : '.', 
				      ââprefix : '' ,
				      ââsuffix : '' 
				      }];
				var onslide = function slide (e){
						$('.carousel-control:hidden').show();
						var elem = $(e.relatedTarget);
						if ( elem.find('.breadcrumb').length > 0 ) { // counters up
							$('#breadcrumb .initial:visible').hide();
							var counters = $('#counters').show().children().hide().addClass('last');
							elem.find('.breadcrumb ul').each( function (i, f) {
								$('#counters div.counter:nth-of-type(' + (2 * i + 1) +')').show().prev().show().removeClass('last').prev().removeClass('last');
								var span = counters.find('#x' + i);
								var text = $(f).children('li:first-of-type').text().split(' ').reverse();
								var num = text.pop();
								if (isNaN( num )) {
									try {
										window.demo[i].stop();
									} catch(err) {/*console.log(i)*/}
									
									counters.find('#c' + i).text($(f).children('li:first-of-type').text() );
								}
								else {
									try {
										options[i]['suffix'] = text.pop();
									} catch (err) {}
									
									window.demo = new countUp('c'+i, 0, num, 0, 1, options[i]);
									window.demo.start();
								}
									
								counters.find('#' + i + 'small').text($(f).children('li:last-of-type').text());
							});
						}
						else { // standard breadcrumb
							$('#breadcrumb .initial').show();
							$('#counters').hide();
						}
						setTimeout(position_indicators, 1000);
					}
				jQuery(function() {
					jQuery('#carousel-scroller').on('slide.bs.carousel', onslide);
				})
			</script>
			
