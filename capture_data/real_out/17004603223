<script>
    var categoryId = '553';
    var productId = '';
    var brandId = '';
    var productPrice = '';
    var IMPRESSION_GTS_TRACKING_API_URL =  'https://tasks.91mobiles.com/nm/api/analytics/desktopEvent';
    var page_id =  '';
    var desktopListPageEvent =  '';
    var isDetailPage =  'home-page';

    var impressionStatus={},date=new Date,origin=null;function checkIfElementInViewPort(e){if(null===e)return!1;var t=e.getBoundingClientRect(),o=e.offsetHeight,i=window.innerHeight;return t.top>=0&&t.top<=i||t.bottom>=0&&t.bottom<=o}function getParameterByName(e,t){e=e.replace(/[\[\]]/g,"\\$&");var o=new RegExp("[?&]"+e+"(=([^&#]*)|&|#|$)").exec(t);return o?o[2]?decodeURIComponent(o[2].replace(/\+/g," ")):"":null}function trackImpressionsAndGts1(e){var t=JSON.stringify(e),o=new XMLHttpRequest;o.open("POST",IMPRESSION_GTS_TRACKING_API_URL,!0),o.setRequestHeader("Content-type","application/json"),o.setRequestHeader("source","web"),o.onreadystatechange=function(){4==o.readyState&&o.status},o.send(t)}function checkImpressionStatus(e,t,o,i){return!!impressionStatus[e+"sept"+t+"sept"+o+"sept"+i]&&impressionStatus[e+"sept"+t+"sept"+o+"sept"+i].status}function setImpressionStatus(e,t,o,i,n){impressionStatus[e+"sept"+t+"sept"+i+"sept"+n]={status:!0,position:o}}function getUserProperties1(){var e=decodeURIComponent((";"+document.cookie).split("; __utma=").pop().split(";").shift());void 0!==e&&""!==e&&(e=(e=e.split("."))[0]+"."+e[1]);var t=decodeURIComponent((";"+document.cookie).split("; PHPSESSID=").pop().split(";").shift());t.indexOf("~")>=0&&(t=t.split("~")[1]);var o=decodeURIComponent((";"+document.cookie).split("; fcmid=").pop().split(";").shift());return void 0===o&&(o=""),{googleId:e,sessionId:t,fcmId:o,userId:document.getElementById("data-uid").textContent?document.getElementById("data-uid").textContent:null}}function getUserPathData1(){if(void 0!==window.localStorage){var e=decodeURIComponent((";"+document.cookie).split("; userPath=").pop().split(";").shift()),t=window.localStorage.getItem("userPath");if(t="string"==typeof t&&""!==t?JSON.parse(t):[],void 0!==e&&""!==e){e=JSON.parse(e);for(var o in e)t.push(e[o]);document.cookie="userPath =;domain=.91mobiles.com; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/;"}return window.localStorage.setItem("userPath",JSON.stringify(t)),t}return JSON.parse(decodeURIComponent((";"+document.cookie).split("; userPath=").pop().split(";").shift()))}function getImpressionsOnScroll1(e,t,o,i,n){var r=0,s=[],a={},p=[];if(""!==i&&""!==n&&""!==t&&""!==o&&"object"==typeof e&&null!==e){if(void 0===(p=e.querySelectorAll(t))||null===p)return;if((p=[].slice.call(p)).forEach(function(e){++r;var t=e.getAttribute("data-store");if("none"!==e.style.display&&!checkImpressionStatus(o,t,i,n)){var a,p=parseInt(e.querySelector("[data-price]").getAttribute("data-price"));null!==origin&&"undefined"!==origin||(origin=getParameterByName("origin",e.querySelector("span[data-href-url]").getAttribute("data-href-url")));var c=getParameterByName("outofstock",e.querySelector("span[data-href-url]").getAttribute("data-href-url"));a={productId:parseInt(productId),catId:parseInt(categoryId),position:r,storeName:t,section:o,storePrice:p,"variant-color":i,"variant-storage":n,outofstock:void 0===c||null===c?null:c},s.push(a),setImpressionStatus(o,t,r,i,n)}}),s.length>0){var c=getUserPathData1(),d="";void 0!==c&&""!==c?d=c[c.length-2]:c=null;var l=decodeURIComponent((";"+document.cookie).split("; landingPageUrl=").pop().split(";").shift());void 0!==l&&""!==l||(l=window.location.pathname+window.location.search);var u=getUserProperties1();a.eventProperties=s,a.userProperties=u,a.eventName="impression",a.source="web",a.timeStamp=parseInt(date.getTime()/1e3),a.landingPageUrl=l,a.lastPageUrl=d,a.pageUrlList=c,a.pageType=origin,trackImpressionsAndGts1(a)}}}function checkAndGetImpression1(e){var t={overview:"overview",pricelist:"store_prices"};"string"==typeof e&&""!==e&&e in t&&(t={sectionToCheck:t[e]});for(var o in t)if(checkIfElementInViewPort(document.getElementById(t[o])))switch(o){case"overview":var i="all",n="all",r=document.getElementById("overview").querySelector('span[data-variant-type="color"]');if("object"==typeof r&&null!==r){for(var s=r.parentNode;"overview"!==s.id;){if("DIV"===s.tagName&&s.className.indexOf("selectBox")>-1){i=