<div id="goikenCommonArea">
	<div class="js-opinion-form">
		<dl>
			<dt>みなさまの声をお聞かせください</dt>
			<dd>
				<p>「こんな企画があったらいいな」「この機能が欲しいな」などなど、ドリームメールをご利用のみなさまのご意見を聞かせてください。<br>
（こちらの投稿にご返事はいたしておりませんが、スタッフがすべて拝見しています。）<br />
◇ご利用に関するお問い合わせは、<a href="/refer/">こちらの専用ページ</a>からお願い致します。</p>
				<form name="goiken_form">
					<textarea rows=6 id="goiken_common_message"></textarea>
					<div><input type="button" onClick="goikening(document.getElementById('goiken_common_message').value);" value="意見を投稿する"></div>
				</form>
			</dd>
		</dl>
		<a href="javascript:void(0);" class="goikenClose">[ とじる ]</a>
	</div>

	<div class="js-opinion-finish" style="display: none;">
		<dl>
			<dt>みなさまの声をお聞かせください</dt>
			<dd>
				<div class="goikenffin">
					<strong>ご意見ありがとうございました</strong>
					<p class="msg">
						いただきましたご意見は、ドリームメールの企画および運営の参考にさせていただきます。なお、個々のご意見への返事はしておりませんので、その旨、ご理解ご了承ください。
					</p>
					<p class="sign">ドリームメール スタッフ一同</p>
					<p class="msg">◇ご質問・お問い合わせは<a href="/refer/">こちらの専用ページから</a></p>
				</div>
			</dd>
		</dl>
		<a href="javascript:void(0);" class="goikenClose">[ とじる ]</a>
	</div>
</div>

<script type="text/javascript">

var isOpinion = false;

/**
 * 意見を登録する
 *
 * @param string str 意見内容
 * @return void
 */
function goikening(str) {
	var suc  = false;
	var comp = false;

	if (isOpinion !== false) {
		alert('ただいまご意見を受付しております。しばらくお待ちください。');
		return ;
	}

	if (!str) {
		alert('メッセージを入力してから[意見を投稿する]ボタンをクリックしてください。');
		return ;
	}

	isOpinion = true;

	$.ajax({
		url     : "/api/opinion/save",
		async   : false,
		data    : {
			"opinion" : str, 
			"path"    : "top/202311/"
		},
		type    : "POST",
		success : function(result) {
			if (result.status == 1) {
				suc = true;
			}
			else {
				alert(result.message);
			}
		},
		complete : function(tmp, message) {
			if (suc) {
				$('.js-opinion-form').hide();
				$('.js-opinion-finish').show();
			}
			isOpinion = false;
		}
	});

}

$(function(){
	/**
	 * 「意見を投稿する」クリック
	 * @return void
	 */
	$(".goiken a").click(function(){
		$("body").append("<div id='TB_overlay'></div>");
		$("#goikenCommonArea").fadeIn("fast");
	})
	/**
	 * 閉じるクリック
	 * @return void
	 */
	$(".goikenClose").click(function(){
		$("#TB_overlay").fadeOut("fast").remove();
		$("#goikenCommonArea").hide();
	})
});
</script><!-- END ご意見箱 -->

<!-- START メダル -->
<!-- END メダル -->

<script src="/js/jquery.cookie.js"></script>
<script>
	var ref = document.referrer;
	var cookieTw = $.cookie('from_tw');
	var cookieYt = $.cookie('from_yt');

	if(ref.match(/t.co/i)){
		$.cookie('from_tw', '1', { expires: 30 });
	} else {
		if(!cookieTw){
			$.cookie('from_tw', '0', { expires: 30 });
		}
	}
//	console.log($.cookie('from_tw'));
	if(ref.match(/youtube/i)){
		$.cookie('from_yt', '1', { expires: 30 });
	} else {
		if(!cookieYt){
			$.cookie('from_yt', '0', { expires: 30 });
		}
	}
</script>


