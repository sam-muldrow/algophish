	<script>

$(function(){

	$('.tenMillion').show();  // ロードが終わってから表示

	// ふわっと読み込み用
	var effect_move = 50,  // どのぐらい要素を動かすか(px)
			effect_time = 600, // エフェクトの時間(ms) 1秒なら1000
			fadeFlag    = 0;

	$(window).on('load scroll resize', function() {

		// ヘッダー固定＆ふわっと用
		var value     = $(this).scrollTop(),
				scrollPos = $(window).height() + value;
				
		// ふわっと読み込み
		$('.fade').each(function(e) {
			var this_pos = $(this).offset().top;
			if(scrollPos < this_pos){
				$(this).css({
					opacity: 0,
					transform: 'translateY('+ effect_move +'px)',
					transition: effect_time + 'ms'
				});
			}
			if ( scrollPos > this_pos ) {
				$(this).delay(100 + e*100).queue(function(){
					$(this).css({
						opacity: 1,
						transform: 'translateY(0)'
					}).dequeue();
				});
				$(this).removeClass('fade');
			}
		});

	});


	$(window).on('scroll load', function() {
		var $campaignWin = $('.campaignWin');
		var value        = $(this).scrollTop();
		
		if(value >= $('#monthly').position().top){
			$campaignWin.addClass('animated bounceInUp').removeClass('bounceInDown');
		} 
		// else if(value < $('#feature').position().top){
		// 	if($campaignWin.css('bottom') == '0'){
		// 		$campaignWin.removeClass('bounceInUp').addClass('bounceInDown');
		// 	}
		// }
	});
	$('.campWinClose').on('click', function(){
		$('.campaignWin').fadeOut('fast');
	});
	$('.campaignCover').on('click', function(){
		$('.campaignWinBody').css({
			'visibility': 'visible',
			'margin-left': '0'
		});
		$(this).fadeOut('fast');
	});

		
});


</script>








