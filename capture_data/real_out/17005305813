<script>
// グローバルのドロップメニュー
function dropMenu(item){
	//console.log('hoge');
//	$(item).each(function(e){
		$(item).hover(function(){
			//$('.dropMenu', this).slideDown(340, 'easeInQuint');
			$('.dropMenu', this).show();
		}, function(){
			//$('.dropMenu', this).slideUp('fast');
			$('.dropMenu', this).hide();
		})
//	})
};
</script>





		


		
