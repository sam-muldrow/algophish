				<div class="et_pb_text_inner">Tú podrías ser la <span id="counter2">67.000</span></div>
			</div><div class="et_pb_module et_pb_code et_pb_code_0">
				
				
				
				
				<div class="et_pb_code_inner"><script type="rocketlazyloadscript">
//Counter Clients - Webempresa
var numberInit = 66589;
let counterUno = document.getElementById('counter1');
let counterDos = document.getElementById('counter2');
startCounter();
function numberWithCommas(x) {
    return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".");
}
function onTimesUp() {
  clearInterval(cancel);
}
function incrementSeconds() {
		var minNume = 1;
    var	maxNume = 3;
 		var randito = Math.floor(Math.random() * (maxNume - minNume + 1) + minNume);
    numberInit += randito;
    numberDos = numberInit + 1;
    counterUno.innerHTML = numberWithCommas(numberInit);
    counterDos.innerHTML = numberWithCommas(numberDos);
    if (numberInit === 66999) {
      onTimesUp();
    }
}
function startCounter() {
		cancel = setInterval(incrementSeconds, 6500);
}
</script></div>
