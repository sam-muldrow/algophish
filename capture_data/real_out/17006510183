<script>
    var cityUrl = "//gz.ihk.cn";
    var SearchType = "sale";
    var searchkey = "";
    function TypeClick(obj) {
        // alert(obj);
        if (document.getElementById("sale") != null) {
            document.getElementById("sale").style.display = "block";
        }
        if (document.getElementById("zu") != null && document.getElementById("zu") != undefined) {
            document.getElementById("zu").style.display = "block";
        }
        if (document.getElementById("newhouse") != null && document.getElementById("newhouse") != undefined) {
            document.getElementById("newhouse").style.display = "block";
        }
        if (obj == "sale") {
            $("#TopSearchBar_KeyWordSearch").attr("placeholder", "请输入区域、商圈或小区名开始找房");
            document.getElementById("spname").innerText = "二手房";
            document.getElementById("sale").style.display = "none";
        }
        else if (obj == "zu") {
            $("#TopSearchBar_KeyWordSearch").attr("placeholder", "请输入区域、商圈或小区名开始找房");
            // "租房";
            document.getElementById("spname").innerText = "租房";
            if (document.getElementById("zu") != null) {
                document.getElementById("zu").style.display = "none";
            }

        }
        else if (obj == "newhouse") {
            $("#TopSearchBar_KeyWordSearch").attr("placeholder", "请输入区域、商圈或楼盘名开始找房");
            document.getElementById("spname").innerText = "新房";
            document.getElementById("newhouse").style.display = "none";
        }

        else {
            if (window.location.href.indexOf("newhouse") > 0) {
                $("#TopSearchBar_KeyWordSearch").attr("placeholder", "请输入区域、商圈或楼盘名开始找房");
                if (document.getElementById("spname") != null) {
                    document.getElementById("spname").innerText = "二手房";
                }
                if (document.getElementById("sale") != null) {
                    document.getElementById("sale").style.display = "none";
                }
            }
            else if (window.location.href.indexOf("blog") > 0) {
                $("#TopSearchBar_KeyWordSearch").attr("placeholder", "请输入姓名、电话号码");

            }
            else if (window.location.href.indexOf("branch") > 0) {
                $("#TopSearchBar_KeyWordSearch").attr("placeholder", "请输入旗舰店名、地址");

            }
            else {
                $("#TopSearchBar_KeyWordSearch").attr("placeholder", "请输入区域、商圈或小区名开始找房");
                if (document.getElementById("spname") != null) {
                    document.getElementById("spname").innerText = "二手房";
                }
                if (document.getElementById("sale") != null) {
                    document.getElementById("sale").style.display = "none";
                }
            }
        }
        SearchType = obj;
        $("#TopSearchBar_SearchType").val(obj);
    }

    function SetSerachaWord(id, key) {
        //$("#" + id).innerText()
        searchkey = key;
        var txtValue = $("#" + id).text();
        $("#TopSearchBar_KeyWordSearch").val(txtValue);

    }

    //$("body").click(function () {
    //     alert(1);
    //    // $(".header_seaboxltboxnew ul").toggle(300);
    //    $("#divseach").hide();
    //});
    function divClick() {
        $("#divseach").hide();
    }
    $("#divseach").click(function (e) {//自己要阻止
        e.stopPropagation();//阻止冒泡到body6
    });

    $("#TopSearchBar_KeyWordSearch").click(function (e) {
        $("#divseach").show();
        //$(".header_seaboxltboxnew ul").toggle(300);
        e.stopPropagation();//阻止冒泡到body
    });


    function bindKeyWordEnter() {
        jQuery("#TopSearchBar_KeyWordSearch").keydown(function (event) {
            if (event.keyCode == 13) {
                TopSearchBar_DoSearch();
                return false;
            }
        });
    }
    function keydown_go_open() {
        if (event.keyCode == 13) {
            if (defaulturl != "" && defaulturl != null) {
                window.open(defaulturl, "_blank");
            }
            else {
                $("#button_go_open").click();
            }

        }
        else {
            defaulturl = null;
        }
    }
</script>
