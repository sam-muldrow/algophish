<script>
    var userAgent = navigator.userAgent; //取得浏览器的userAgent字符串
    var isOpera = userAgent.indexOf("Opera") > -1; //判断是否Opera浏览器
    var isIE = userAgent.indexOf(".NET") > -1 && !isOpera; //判断是否IE浏览器
    var prevent = 0;
    var time1 = new Date();
    function DoSearch() {
        if (isIE && (prevent == 0 || prevent == 1)) {
            prevent++;
            return;
        }

        var txtValue = $("#TopSearchBar_KeyWordSearch").val();
        var type = $("#TopSearchBar_SearchType").val();
        $('#divseach').show();
        if (window.location.href.toLowerCase().indexOf("newhouse") > 0) {
            type = "newhouse";
        }
        var time2 = new Date();
        var date3 = time2.getTime() - time1.getTime()  //时间差的毫秒数
        time1 = new Date();
        if (date3 >= 700)
        {
            index=0//每次做查询后 键盘按上下的index需要回到默认值
            $.ajax({
                type: "GET",
                url: "http://wcf.ihk.cn/GlobalDataWcfService.svc/SearchEngines?" + "searchTypes=" + type + "&searchValue=" + txtValue + "&IsHome=1&city=gz",
                //type: "POST",
                

                datatype: "html",
                success: function (data) {
                    $('#divseach').show();
                    $('#divseach').html(data);
                },
                error: function () {
                    //alert("处理失败!");
                }
            });
        }

    }

</script>

<script>
    function TopSearchBar_DoSearch() {
        var txtValue = $("#TopSearchBar_KeyWordSearch").val();
        searchkey = txtValue;
        var type = $("#TopSearchBar_SearchType").val();
        var url = "//gz.ihk.cn/";
        var searchParm = "";
        var urltype = "esf";
        if (type == "rent") {
            urltype = "zu";
        }
        if (type == "zu") {
            urltype = "zu";
        }
        if (type == "notCommission") {
            // //gz.ihk.cn/myxf/houselist
            urltype = "myxf";
        }
        if (type == "newhouse") {
            // //gz.ihk.cn/newhouse/houselist
            urltype = "newhouse";
        }

          // alert(urltype);
        if (window.location.href.toLowerCase().indexOf("newhouse") > 0) {
            if (document.getElementById("TopSearchBar_KeyWordSearch").value != null && document.getElementById("TopSearchBar_KeyWordSearch").value != "") {
                window.open("/newhouse/houselist/?keyword=" + document.getElementById("TopSearchBar_KeyWordSearch").value, "_self");
            }
            else {
                window.open("/newhouse/houselist/", "_self");
            }

        }
        else if (window.location.href.toLowerCase().indexOf("dictionary") > 0) {
            if (document.getElementById("TopSearchBar_KeyWordSearch").value != null && document.getElementById("TopSearchBar_KeyWordSearch").value != "") {
                window.open("/dictionary/dictionarylist.aspx?keyword=" + document.getElementById("TopSearchBar_KeyWordSearch").value, "_self");
            }
            else {
                window.open("/dictionary/dictionarylist.aspx", "_self");
            }
        }
        else if (window.location.href.toLowerCase().indexOf("blog") > 0) {
            if (document.getElementById("TopSearchBar_KeyWordSearch").value != null && document.getElementById("TopSearchBar_KeyWordSearch").value != "") {
                window.open("/blog/index/?keyword=" + document.getElementById("TopSearchBar_KeyWordSearch").value, "_self");
            }
            else {
                window.open("/blog/index/", "_self");
            }
        }
        else if (window.location.href.toLowerCase().indexOf("branch") > 0) {
            if (document.getElementById("TopSearchBar_KeyWordSearch").value != null && document.getElementById("TopSearchBar_KeyWordSearch").value != "") {
                window.open("/branch/index/?keyword=" + document.getElementById("TopSearchBar_KeyWordSearch").value, "_self");
            }
            else {
                window.open("/branch/index/", "_self");
            }
        }
        else {
            if (txtValue != "") {
                if (type == "newhouse") {
                    //alert(searchkey);
                    window.open("/newhouse/houselist/?keyword=" + searchkey, "_blank");
                }
                else if (type == "dictionary") {
                    window.open("/dictionary/dictionarylist.aspx?keyword=" + searchkey, "_blank");
                }
                else {
                    if (txtValue.indexOf("楼盘") >= 0) {
                        url += "/" + urltype + "/houselist/?keyword=" + searchkey;
                        //  alert(url);
                        window.open(url, "_blank");
                    }
                    if (txtValue.indexOf("区域") >= 0 || txtValue.indexOf("板块") >= 0) {
                        url += urltype + "/houselist/" + searchkey + "/";
                        // alert(url);
                        window.open(url, "_blank");
                    }
                    else {
                        url = "/" + urltype + "/houselist/?keyword=" + txtValue;
                        // alert(url);
                        window.open(url, "_blank");
                    }
                }

            }
            else {
                url = "/" + urltype + "/houselist/";
                // alert(url);
                window.open(url, "_brank");
            }
        }

    }

</script>
<script type="text/javascript">
    $(document).ready(function () {
        if ('gz'=="fs")
        {
            TypeClick('newhouse');
        }
        else
        {
            TypeClick('sale');
        }
        //获取首页视频对象
        var video = document.getElementById("videoIndex");
        if (video != null)
        {
            var height = $(video).height();
            video.onended = function () {
                $(video).hide();
                $("#videoPic").height(height);
                $("#videoPic").show();
            };
        }

    });
    var oDivone = document.getElementById('headeron2')
    function showoDiv1() {
        $("#divseach").show();
        // oDivone.classList.toggle('header_seaboxlton');
        //$(".header_seaboxltboxnew ul").toggle(300);
        if (oDivone != null)
        {
            oDivone.classList.toggle('header_seaboxltonnew');
        }

    }
    function showoDiv2() {
        //alert(1);
        // $("#divseach").show();
        $("#divseach").hide();
        if (oDivone != null)
        {
            oDivone.classList.toggle('header_seaboxltonnew');
        }

        // $(".header_seaboxltboxnew ul").toggle(300);
    }
</script>
<script>
    //上下选择回车跳转
    var index = 0;//li的起始位置
    var li = document.getElementById("divseach").getElementsByTagName("li");
    var defaulturl = "";

    document.onkeydown = function (event) {
        var e = event || window.event || arguments.callee.caller.arguments[0];
        if (e && e.keyCode == 38) { // 按向上
            if (index > 1) {
                index--;
                changgecolorli();
                li[index].style.backgroundColor = "#edebeb";
                defaulturl = li[index].getElementsByTagName("a")[0].href;
                var seachwordobj = li[index].getElementsByTagName("a")[0].getElementsByTagName("strong")[0];

                $("#TopSearchBar_KeyWordSearch").val($(seachwordobj).contents().filter(function (index, content) {
                    return content.nodeType === 3;
                }).text());
                if (index < 5) {
                    $("#divseach").scrollTop(0, 0)
                }

            }

        }
        if (e && e.keyCode == 40) { // 按向下
            if (li != null) {
                if (index < li.length - 1) {
                    index++;
                    changgecolorli();
                    li[index].style.backgroundColor = "#edebeb";
                    defaulturl = li[index].getElementsByTagName("a")[0].href;
                    var seachwordobj = li[index].getElementsByTagName("a")[0].getElementsByTagName("strong")[0];

                    $("#TopSearchBar_KeyWordSearch").val($(seachwordobj).contents().filter(function (index, content) {
                        return content.nodeType === 3;
                    }).text());

                    if (index > 5) {
                        $("#divseach").scrollTop(200, 0)
                    }

                }
            }


        }

    };
    function changgecolorli() {
        for (var i = 1; i < li.length; i++) {
            li[i].style.backgroundColor = "white";
        }
    }
    //鼠标移进去事件
    $('#divseach ul li').mouseover(function () {
        index = $(this).index();
        changgecolorli();
        li[index].style.backgroundColor = "#edebeb";

    });
    //鼠标移开事件
    $('#divseach ul li').mouseout(function () {
        index = $(this).index();
        changgecolorli();

    });
</script>
