    function GetPeter(id, type) {
        $.ajax({
            type: "GET",
            // url: "/newsecond/Home/GetPeter",
            url: "/newsecond/Home/GetPeter",
            data: "id=" + id + "&type=" + type,
            datatype: "html",
            success: function (data) {
                $("#" + id).html(data);
            },
            error: function () {
                alert("程序错误，错误信息(PinPai)：" + errorThrown);
            }
        });

    }
</script>
