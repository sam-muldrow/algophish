    /**
        百度OCPC广告开发API数据回传
        */
    function OCPC() {
        var uList = [{
            logidUrl: window.location.href,
            isConvert: 1,
            convertType: 3,
        }];
        $.ajax({
            type: "POST",
            url: "/newsecond/Property/BaiduOCPC",
            dataType: 'json',
            data: { "uList": uList },
            beforeSend: function (re) {

            },
            error: function (re) {

            },
            success: function (re) {
                console.log(re);
            }
        });

    }
</script>

