                <script type="text/javascript">
    jQuery(".slideBox").slide({ mainCell: ".bd ul", autoPlay: true });

    //20190823 切换房王快报图片
    function changeImg(picUrl,id)
    {  //获取到img
        var picture = $("#kingImg");
        var summary=$(".ihknews-txtbox01")
        //隐藏全部摘要
        $(".ihknews-txtbox01").hide();
        //显示需要显示的摘要
        $("#yaodian_"+id).show();
        //鼠标经过时，修改图片的路径
        picture.attr('src',picUrl)
        //修改图片的链接地址
        $('#hrefImg').attr('href','//gz.ihk.cn/news/'+id+'.html');

    }
    //20190829 切换楼盘测评图片
    function changeLouPanImg(picUrl,titleHeadUrl)
    {  //获取到img
        var picture = $("#LouPanImg");
        //鼠标经过时，修改图片的路径
        picture.attr('src',picUrl)
        //修改图片的链接地址
        $('#hrefLouPan').attr('href',titleHeadUrl);
    }
                </script>
