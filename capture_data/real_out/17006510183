    //2017年8月17日10:40:36添加结束
    function OrderSubmit(o) {
        var houseid = $("#txtHouseId").val();
        var username = '';
        var phone = '';
        var ckbValue = '';
        var groupId = $("#txtGroupId").val();
        var LeaveWord = $("#txt_yiXiang").val();
        if (o == "page") {
            username = $("#pageusername").val();
            phone = $("#pagephone").val();
            ckbValue = $("#page_checkboxInput").val() + $("#page_checkboxInput2").val() + $("#page_checkboxInput3").val();
        } else {
            username = $("#UserName").val();
            phone = $("#UserPhone").val();
            //ckbValue = $("#checkboxInput").val() + $("#checkboxInput2").val() + $("#checkboxInput3").val();
            ckbValue = "看房团";
        }
        if (username == '' || username == '称呼' || phone == '' || phone == '手机') {
            alert('请输入您的称呼或者手机');
        }
        else {
            $.ajax({
                url: "/newsecond/Home/AddYouHui",
                data: { houseid: houseid, phone: phone, username: username, ckbValue: ckbValue, groupId: groupId, LeaveWord: LeaveWord },
                success: function (data) {

                    if (data != null && data != "") {
                        document.getElementById("UserName").value = "";
                        document.getElementById("UserPhone").value = "";
                        document.getElementById("txt_yiXiang").value = "";

                        //$("#number").innerText(data);

                        //alert(data);
                        document.getElementById("number").innerHTML = data;
                        alert("报名成功");
                        //setTimeout("closeHuoQu", 3000);
                    }
                    else {
                        //alert(data);
                        alert("报名失败");
                    }

                }
        });
    }

    }
</script>


    

