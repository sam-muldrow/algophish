		  function getUserCookie(name) {
			if (document.cookie.length > 0) {
			  c_start = document.cookie.indexOf(name + '=');
			  if (c_start != -1) {
				c_start = c_start + name.length + 1;
				c_end = document.cookie.indexOf(';', c_start);
				if (c_end == -1) c_end = document.cookie.length;
				return decodeURI(document.cookie.substring(c_start, c_end));
			  }
			}
			return false;
		  }
		  function setUserCookie(name, value, domain){
			var dominio = (typeof domain != 'undefined') ? domain : window.location.hostname;
			document.cookie = name + '=' + value + '; domain=' + dominio + '; path=/;';
		  }
		  function deleteUserCookie(name, domain){
			var dominio = (typeof domain != 'undefined') ? domain : window.location.hostname;
			document.cookie = name + '=;domain=' + domain + ';path=/;expires=Thu, 01-Jan-70 00:00:01 GMT';
		  }
		  function getAge(dateString) {
			var today = new Date();
			var birthDate = new Date(dateString);
			var age = today.getFullYear() - birthDate.getFullYear();
			var m = today.getMonth() - birthDate.getMonth();
			if (m < 0 || (m === 0 && today.getDate() < birthDate.getDate())) {
			  age--;
			}
			return age;
		  }

		  var cdata = getUserCookie('IDP_data');
		  if(cdata) {
			var data = JSON.parse(atob(cdata));
			if (window.Worker) {
			  var IDPWorker = new Worker('/StaticFiles/RealMadridResponsive/javascripts/idpcheck.js');
			  IDPWorker.postMessage(data);
			  IDPWorker.onmessage = function(msg) {
				if(msg.data != 'error') {
				  // delete cookie
				  deleteUserCookie('IDP_data','realmadrid.com');				  
				  // return
				  if(typeof msg.data.data == 'undefined') throw new Error('Error al recuperar datos de usuario. Vuelva a intentarlo');				  				  
				  var userdata = msg.data.data[0];				  
				  if (typeof data.return != 'undefined' && userdata.fanType != 'employee') {
					var params = '?fanType=' + encodeURI(data.fanType) + '&refreshToken=' + encodeURI(data.refreshToken) + '&socialKey=' + encodeURI((typeof data.socialKey != 'undefined') ? data.socialKey : '') + '&lang=' + encodeURI(userdata.preference.language) + '&redirectUrl=https://www.realmadrid.com/return_newIDP2';
					window.location.href = data.return + params;
				  }else if(typeof data.return == 'undefined'){					  
					  //crea cookie
					  var user_type = (userdata.fanType == 'employee') ? 'usuarioregistradoemployee' : 'usuarioregistrado';
					  var gender = (userdata.gender) ? userdata.gender : '-';
					  var cookie_data = user_type + '@' + encodeURI(userdata.fullName) + '@-@-@' + userdata.tier + '@' + getAge(userdata.birthDateTime) + '@' + gender + '@' + userdata.id + '@' + getAge(userdata.metainformation.createDateTime) + '@' + data.refreshToken + '@' + ((typeof data.socialKey != 'undefined') ? data.socialKey : '') + '@' + data.fanType + '@' + userdata.preference.language;
					  // user cookie
					  setUserCookie('usuario',cookie_data,'realmadrid.com');			  
					  // actualizo cabecera
					  var usrc = decodeURI(cookie_data);
					  if (usrc) {
						var usrg = user_type;
						var quot = usrg.indexOf("\"")
						if (quot != -1) {
						  usrg = usrg.substr(1);
						}
						var usrn = userdata.fullName;
						var usrt = userdata.tier;
						var usr_app = false;
						if (usrt.indexOf('app') != -1) {
						  usr_app = true;
						}
						var regl_ = document.querySelector('.action_link.register');
						if (regl_) regl_.style.display = 'none';
						var usrm_ = document.querySelector('.login_label');
						if (usrm_) usrm_.classList.add('logged');
						var usrn_ = document.querySelectorAll('[data-login-name]');
						if (usrn_) {
						  usrn_.forEach(function (el_) {
							el_.innerHTML = usrn;
						  });
						}
						switch (usrg) {
						  case 'socio':
							usrm_.classList.add('soc');
							break;
						  case 'carnetmadridista':
							usrm_.classList.add('mad');
							break;
						  case 'usuarioregistrado':
							usrm_.classList.add('reg');
							break;
						  default:
							usrm_.classList.add('reg');
						}
						
					  }
				  }
				  //
				  IDPWorker.terminate();
				} else {
				  // console.error('Error en la autorizaciÃ³n');
				  alert('Error al recuperar datos de usuario. Vuelva a intentarlo');
				}
			  }
			}
		  }
