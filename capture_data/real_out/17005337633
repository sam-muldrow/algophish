  </script><script type="text/javascript">

  function async(callback) {
    setTimeout(callback, 0);
  }

  function createDeferElement(theNode) {
    var newNode = document.createElement(theNode.tagName);
    if (newNode.tagName == 'SCRIPT') {
      //newNode.async = theNode.getAttribute('async') != null;
      if (theNode.getAttribute('defer-src') != null) {
        newNode.src = theNode.getAttribute('defer-src');
      } else {
        newNode.innerHTML = theNode.innerHTML;
      }
    } else {
      if (theNode.getAttribute('defer-href') != null) {
        newNode.href = theNode.getAttribute('defer-href');
        newNode.setAttribute('rel', 'stylesheet');
      }
    }
    newNode.type = theNode.getAttribute('type').replace('/defer', '');
    return newNode;
  }

  
  /*Subhome e interiores*/
  (function(w) {
  "use strict";
  var loadCSS = function(obj, before, media) {
    var doc = w.document;
    var ss = doc.createElement("link");
    var ref;
    if (before) {
      ref = before;
    } else {
      var refs = (doc.body || doc.getElementsByTagName("head")[0]).childNodes;
      ref = refs[refs.length - 1];
    }
    var sheets = doc.styleSheets;
    ss.rel = "stylesheet";
    ss.href = obj.getAttribute('defer-href');
    ss.media = "only x";

    function ready(cb) {
      if (doc.body) {
        return cb();
      }
      setTimeout(function() {
        ready(cb);
      });
    }
    ready(function() {
      ref.parentNode.insertBefore(ss, (before ? ref : ref.nextSibling));
    });
    var onloadcssdefined = function(cb) {
      var resolvedHref = ss.href;
      var i = sheets.length;
      while (i--) {
        if (sheets[i].href === resolvedHref) {
          return cb();
        }
      }
      setTimeout(function() {
        onloadcssdefined(cb);
      });
    };

    function loadCB() {
      if (ss.addEventListener) {
        ss.removeEventListener("load", loadCB);
      }
      ss.media = media || "all";
    }
    if (ss.addEventListener) {
      ss.addEventListener("load", loadCB);
    }
    ss.onloadcssdefined = onloadcssdefined;
    onloadcssdefined(loadCB);
    return ss;
  };
  if (typeof exports !== "undefined") {
    exports.loadCSS = loadCSS;
  } else {
    w.loadCSS = loadCSS;
  }
  var loadJS = function(src, cb) {
    "use strict";
    var ref = w.document.getElementsByTagName("script")[0];
    var script = w.document.createElement("script");
    script.src = src;
    script.async = true;
    ref.parentNode.insertBefore(script, ref);
    if (cb && typeof(cb) === "function") {
      script.onload = cb;
    }
    return script;
  };
  // commonjs
  if (typeof module !== "undefined") {
    module.exports = loadJS;
  } else {
    w.loadJS = loadJS;
  }
}(typeof global !== "undefined" ? global : this));

  
    /*Subhome e interiores*/
    function dCSS() {
      var cssobjs = document.querySelectorAll('link[type="text/css/defer"]');
      for (var i = 0; i < cssobjs.length; i++) {
      loadCSS(cssobjs[i]);
      cssobjs[i].parentNode.removeChild(cssobjs[i]);
      }
    }
  

  function dTPJS() {
    var objs = document.querySelectorAll('script[type="text/javascript/defer"]'),
      inlineScript = '';
    for (var i = 0; i < objs.length; i++) {
      if (objs[i].getAttribute('defer-src') != null) {
        var newNode = document.createElement(objs[i].tagName);
        newNode.src = objs[i].getAttribute('defer-src');
        newNode.type = objs[i].getAttribute('type').replace('/defer', '');
        objs[i].parentNode.insertBefore(createDeferElement(objs[i]), objs[i]);
        objs[i].parentNode.removeChild(objs[i]);
      } else {
        inlineScript += objs[i].innerHTML;
      }
    }
    if (inlineScript.length > 0) {
      var newNode = document.createElement('script');
      newNode.innerHTML = inlineScript;
      objs[objs.length - 1].parentNode.insertBefore(newNode, objs[objs.length - 1]);
    }
    return async(dIFR);
  }

  function dIFR() {
    var vidDefer = document.getElementsByTagName('iframe');
    for (var i = 0; i < vidDefer.length; i++) {
      if (vidDefer[i].getAttribute('data-src')) {
        vidDefer[i].setAttribute('src', vidDefer[i].getAttribute('data-src'));
      }
    }
    return;
  }

  function dLJS() {

    loadJS("/StaticFiles/RealMadridResponsive/javascripts/lazysizes.min.js", function() {});
    loadJS("/StaticFiles/RealMadridResponsive/javascripts/jquery.js", function() {
      loadJS("/StaticFiles/RealMadridResponsive/javascripts/lazysizes-allloaded.min.js");
      
        loadJS("/StaticFiles/RealMadridResponsive/javascripts/all.fn_subhomes.js", function() {
      
          loadJS("/StaticFiles/RealMadridResponsive/javascripts/i18n.js", function() {
            loadJS("/StaticFiles/RealMadridResponsive/javascripts/cookie_realmadrid.js");
            loadJS("/StaticFiles/RealMadridResponsive/javascripts/popups.js");
            loadJS("/StaticFiles/RealMadridResponsive/javascripts/search_results.js");
            loadJS("/StaticFiles/RealMadridResponsive/javascripts/calendar.js");
            loadJS("/StaticFiles/RealMadridResponsive/javascripts/gallery_modificado.js");
            loadJS("/StaticFiles/RealMadridResponsive/javascripts/cabecera_login_usuario.js");
			loadJS("/StaticFiles/RealMadridResponsive/javascripts/all.js", function() {
			  RM._execute_callbacks('load');
			  //async(dTPGPT);
			  async(dTPJS);
			});
        });
      });
    });
  }

  function defer() {
    return window.isLoaded = true, dCSS(), dLJS();
  }
  if (window.addEventListener)
    window.addEventListener("load", defer, false);
  else if (window.attachEvent)
    window.attachEvent("onload", defer);
  else window.onload = defer;

</script><!--<link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Oswald:300,400,500,700|Open+Sans:300,400,600"/>--><script defer>

var browserName = (function (agent) { 
  switch (true) {
    case agent.indexOf("edge") > -1: return "MS Edge";
    case agent.indexOf("edg/") > -1: return "Edge ( chromium based)";
    case agent.indexOf("opr") > -1 && !!window.opr: return "Opera";
    case agent.indexOf("chrome") > -1 && !!window.chrome: return "Chrome";
    case agent.indexOf("trident") > -1: return "MS IE";
    case agent.indexOf("firefox") > -1: return "Mozilla Firefox";
    case agent.indexOf("safari") > -1: return "Safari";
    default: return "other";
  }
})(window.navigator.userAgent.toLowerCase());

function onNewUserRegistered(device){
  createInDialog();
  // console.log("Nuevo Registrado > ",device);
}

function onIndigitallInitialized(permissions,device){

  // console.log("onIndigitallInitialized ");

  // admin event
  var not_adm = document.getElementById('notif_link');
  not_adm.addEventListener('click', function(e) {
    e.preventDefault();
    // Verificar si las notificaciones están habilitadas
    if ( permissions.push !== "granted" && !document.getElementById('indiag')) {
      // Creamos tooltip
      not_adm.classList.toggle('notif_enable');
      // Para gestionar las notificaciones debe permitir las notificaciones en su navegador
    } else {
      // Crear dialogo
      if (!document.getElementById('indiag')) {
        createInDialog();
      }
    }

  });
}

function onLocationUpdated(location){}

function onError(error){
  console.log("Error indigitall : ", error);
}

function requestPushPermission(permission){
  // console.log("requestPushPermission");
}

function requestLocationPermission(permission){
}

function onIndigitallLoaded(){

  if ( browserName == "Safari" ){

    indigitall.init({
      appKey: '17042946885b3235b3808411.43552570',
      workerPath: '/worker.min.js',
      onInitialized: onIndigitallInitialized,
      requestPushPermission: requestPushPermission,
      onNewUserRegistered: onNewUserRegistered
    });

    if(indigitall.isSafariPushNotificationSupported() && !localStorage.getItem('indigitall.repository.PUSH_TOKEN')){

      var button = document.createElement('a');
      button.id = 'indigitall-button';
      button.style = 'display:flex; justify-content:center;align-items:center;align-content: center;position: fixed; bottom: 15px; left: 15px; z-index: 99; border-radius: 50%; border: 1px solid #0225A5; width: 50px; height: 50px; color: #fff; background: linear-gradient(to bottom,#0225A5 1%,#023C72 100%); cursor: pointer; box-shadow: 0 0 3px rgba(0,0,0,.25); transition: .35s ease; transition-property: all; font-size: 20px; text-shadow: 0 0 2px rgba(0,0,0,.5);text-decoration: none;';
      button.innerHTML = '<svg style="width:24px;height:24px" viewBox="0 0 24 24"><path fill="#f5f5f5" d="M21,19V20H3V19L5,17V11C5,7.9 7.03,5.17 10,4.29C10,4.19 10,4.1 10,4A2,2 0 0,1 12,2A2,2 0 0,1 14,4C14,4.1 14,4.19 14,4.29C16.97,5.17 19,7.9 19,11V17L21,19M14,21A2,2 0 0,1 12,23A2,2 0 0,1 10,21" /></svg>';

      button.addEventListener('click', function(e){
        e.preventDefault();
        indigitall.launchNotificationPermissionForSafari();
        document.getElementById('indigitall-button').remove();
      });

      document.getElementsByTagName("body")[0].appendChild(button);

      indigitall.deviceGet((device) => {
        // success function
        // console.log(device);
      },() => {
        // error function
      });

    }

  } else {

    indigitall.init({
      appKey: '17042946885b3235b3808411.43552570',
      workerPath: '/worker.min.js',
      requestLocation: true,
      onInitialized: onIndigitallInitialized,
      requestPushPermission: requestPushPermission,
      onNewUserRegistered: onNewUserRegistered,
      requestLocationPermission: requestLocationPermission,
      onLocationUpdated: onLocationUpdated,
      onError: onError 
    });
  }

}
