			function desconexionUsuarioIDP() {
			
				var IDP_URL_Desconexion_IDP = 'https://socios.realmadrid.com/cs/Satellite?language=es&pagename=RealMadridOAS/Comunes/Logica/desconexion'
					
				cookieUsuario = getCookie("usuario").split('@');
				
				if (cookieUsuario == ""){
					cookieUsuario[0] = "no logado";		
				}else{
					
					var fanTypeIDPJS = '';
					if(cookieUsuario[12] != 'null'){
						fanTypeIDPJS = cookieUsuario[12];
					}
								
					if (fanTypeIDPJS != "member"){
						deleteCookie('usuario');
						deleteCookie('registro');
					}else{
						deleteCookie('usuario');
						deleteCookie('registro');
						document.getElementById("reg_desc").href = IDP_URL_Desconexion_IDP;
						location.href=IDP_URL_Desconexion_IDP 
					}					
				}
			}
