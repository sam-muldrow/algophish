			function areaPrivadaUsuarioIDP() {
			
				var IDP_URL_area_socios = 'https://socios.realmadrid.com'
				var IDP_URL_profile = 'https://you.realmadrid.com/#/profile-area/home'
			
				cookieUsuario = getCookie("usuario").split('@');
				
				if (cookieUsuario == ""){
					cookieUsuario[0] = "no logado";		
				}else{
					var refreshTokenIDPJS = '';
					
					if(cookieUsuario[10] != 'null'){
						refreshTokenIDPJS = cookieUsuario[10];
					}
					var socialKeyIDPJS = '';
					if(cookieUsuario[11] != 'null'){
						socialKeyIDPJS = cookieUsuario[11];
					}
					var fanTypeIDPJS = '';
					if(cookieUsuario[12] != 'null'){
						fanTypeIDPJS = cookieUsuario[12];
					}
					var languageIDPJS = cookieUsuario[13];
					if(cookieUsuario[13] != 'null'){
						languageIDPJS = 'es-ES';
					}
								
					var languageSite = 'es'
					var urlResult = 'https://www.realmadrid.com/return_newIDP2'
					
					if (cookieUsuario[0] != "usuarioregistrado" && cookieUsuario[0] != "socio"){
						var urlEscapada = 'https://www.realmadrid.com/return_newIDP'
						var urlRegistro = 'https://signin.realmadrid.com/#/sign-in/user'
						var urlLoginUsuarioIDP = urlRegistro + "?language="+languageSite+"&redirectUrl="+urlEscapada;	
						location.href=urlLoginUsuarioIDP
					}else{					
						var urlPerfilUsuarioIDP  = IDP_URL_profile + '?fanType=' + fanTypeIDPJS + '&socialKey=' + socialKeyIDPJS + '&refreshToken=' + refreshTokenIDPJS + '&language=' + languageIDPJS + '&redirectUrl=' + urlResult
						location.href=urlPerfilUsuarioIDP 
					}					
				}
			}
