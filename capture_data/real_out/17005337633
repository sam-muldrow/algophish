<!-- MODAL GRUPOS SDK INDIGITALL -->
<script defer>

  // idioma de navegacion
  var idioma_ind = "es";

  var id_f = false,
      id_b = false,
      id_v = false,
      id_m = false,
      id_d = false, // ninguna

      id_ca = false, // castellano
      id_in = false, // ingles
      id_fr = false, // frances
      
      litFutbolES = 'fútbol'.replace(new RegExp(/[ùúûü]/g),"u");
      litBaloncestoES = 'baloncesto';
      litVentajasES = 'ventajas';
      litMadridistasES = 'madridistas';
      litNingunaES = 'desactivar';

      litFutbol = 'Fútbol';
      litBaloncesto = 'Baloncesto';
      litVentajas = 'Ventajas';
      litMadridistas = 'Madridistas';
      litNinguna = 'Desactivar';

      litLangES = 'castellano',
      litLangEN = 'inglés',
      litLangFR = 'francés',

      canales = [litFutbolES, litBaloncestoES, litVentajasES, litMadridistasES, litNingunaES];

      canalesLang = [ 'castellano', 'ingles', 'frances' ];

  var removePopup = function() {
    var pop_p = document.getElementById('indiag');
    pop_p.outerHTML = '';
    return true;
  };

  var createInDialog = function() {

    var safari_btn = document.getElementById('indigitall-button');
    if ( safari_btn ) safari_btn.remove();

    // get de suscripciones actuales
    indigitall.topicsList((topics) => {
      // success function
      // console.log(topics);
      if(Array.isArray(topics) && topics.length > 0){
        topics.forEach(function(topic) {
          // console.log(topic.code , topic.subscribed);
          if (topic.code == 'futbol'      && topic.subscribed == true ) id_f = true;
          if (topic.code == 'baloncesto'  && topic.subscribed == true ) id_b = true;
          if (topic.code == 'promociones' && topic.subscribed == true ) id_v = true;
          if (topic.code == 'madridistas' && topic.subscribed == true ) id_m = true;

          if (topic.code == 'desactivar') id_d = true;

          if (topic.code == 'castellano'  && topic.subscribed == true ) id_ca = true;
          if (topic.code == 'ingles'      && topic.subscribed == true ) id_in = true;
          if (topic.code == 'frances'     && topic.subscribed == true ) id_fr = true;
		   
        });
      }

      // si no viene ningun idioma suscrito
      if ( id_ca == false && id_fr == false && id_in == false ){
        // suscribimos al de navegacion o a ingles.
        if ( idioma_ind == "es" ){
          id_ca = true;
        } else if ( idioma_ind == "fr" ){
          id_fr = true;
        } else {
          id_in = true;
        }
      }

      //Mostramos popup para configurar suscripciones
      createInDialogHTML();
    }, () => {
      // error function
    });
  };

  var createInDialogHTML = function() {

    // popup
    var pop = document.createElement('div');
    pop.id = 'indiag';
    // cabecera
    var pop_h = document.createElement('h3');
    pop_h.className = 'notifhead';
    pop_h.innerHTML = "¡No te pierdas ninguna de nuestras notificaciones!";
    // Agrega cabecera a popup
    pop.appendChild(pop_h);
    // form
    var pop_f = document.createElement('form');
    pop_f.className = 'notiform';
    // fieldset
    var pop_s = document.createElement('fieldset');
    // legend
    var pop_l = document.createElement('legend');
    pop_l.className = 'notifleg';
    pop_l.innerHTML = "Selecciona las notificaciones que más te interesan:";
    // Agrega legend a fliedset
    pop_s.appendChild(pop_l);
    // inputs
    canales.forEach(function(canal) {
      // span
      var pop_n = document.createElement('span');
      // label
      var pop_a = document.createElement('label');
      pop_a.htmlFor = 'notif_' + canal.charAt(0).toLowerCase();
      pop_a.className = 'notiflabel';
      // input
      var pop_i = document.createElement('input');
      pop_i.type = 'checkbox';
      pop_i.id = 'notif_' + canal.charAt(0).toLowerCase();
      pop_i.value = canal.normalize('NFD').replace(/[\u0300-\u036f]/g, '').replace(/\s/g, '_').toLowerCase();

      if(canal.substr(0,2).toLowerCase() == 'fu') pop_i.checked = id_f;
      if(canal.charAt(0).toLowerCase()   == 'b' ) pop_i.checked = id_b;
      if(canal.charAt(0).toLowerCase()   == 'v' ) pop_i.checked = id_v;
      if(canal.charAt(0).toLowerCase()   == 'm' ) pop_i.checked = id_m;
	    if(canal.charAt(0).toLowerCase()   == 'd' ) pop_i.checked = id_d;
      // Evento checkboxes
      pop_i.addEventListener('click', function(e){
        var etv = e.target.value;
        var trg = Array.from(e.target.closest('fieldset').querySelectorAll('span .notiflabel .checkbox input[type="checkbox"]'));

        if (etv == 'desactivar') {

          var chk = trg.filter(function (obj) {
            return obj.value != etv;
          });
          chk.forEach(function (el) {
            el.checked = false;
            el.closest('span.checkbox').classList.remove('checked_checkbox');
          });
          e.target.checked = true;
          e.target.closest('span.checkbox').classList.add('checked_checkbox');
          id_f = false, id_b = false, id_v = false, id_m = false, id_d = true;

        } else {

          var nin = trg.filter(function (obj) {
            return obj.value == 'desactivar';
          });

          var chk = trg.filter(function (obj) {
            return obj.checked == true;
          });

          id_f = false, id_b = false, id_v = false, id_m = false;
          chk.forEach(function (el) {
            if (el.id.slice(-1) == 'f') id_f = true;
            if (el.id.slice(-1) == 'b') id_b = true;
            if (el.id.slice(-1) == 'v') id_v = true;
            if (el.id.slice(-1) == 'm') id_m = true;
            if (el.id.slice(-1) == 'd') id_d = true;
          });

          if (nin[0].checked == true && chk.length > 0) {
            id_d = false;
            nin[0].checked = false;
            nin[0].closest('span.checkbox').classList.remove('checked_checkbox');
          } else if (nin[0].checked == false && chk.length == 0) {
            id_d = true;
            nin[0].checked = true;
            nin[0].closest('span.checkbox').classList.add('checked_checkbox');
            document.querySelectorAll('input[name="radio-idioma"]:checked').checked = false;

          }

        }

      }); // Agrega input a label
      pop_a.appendChild(pop_i);
      // Texto canal
      if (canal === litFutbolES){
        var pop_c = document.createTextNode(litFutbol);
      }else if (canal === litBaloncestoES){
        var pop_c = document.createTextNode(litBaloncesto);
      } else if (canal === litVentajasES){
        var pop_c = document.createTextNode(litVentajas);
      } else if (canal === litMadridistasES){
        var pop_c = document.createTextNode(litMadridistas);
      } else if (canal === litNingunaES){
        var pop_c = document.createTextNode(litNinguna);
      }
      // Agrega texto a label
      pop_a.appendChild(pop_c);
      // Agrega label a span
      pop_n.appendChild(pop_a);
      // Agrega span a fieldset
      pop_s.appendChild(pop_n);
    });

    // IDIOMAS
    var pop_idiomas = document.createElement('div');

    var pop_span_idiomas = document.createElement('span');
    pop_span_idiomas.className = 'notifidiomas_lit';
    pop_span_idiomas.innerHTML = 'Elige el idioma en el que quieres recibir las notificaciones' ;
    pop_idiomas.appendChild(pop_span_idiomas);

    pop_idiomas.className = 'notifidiomas';
    canalesLang.forEach(function(canal) {
      // span
      var pop_n = document.createElement('span');
      // label
      var pop_a = document.createElement('label');
      pop_a.htmlFor = 'notif_' + canal.substr(0,2).toLowerCase();
      pop_a.className = 'notiflabel';
      // input
      var pop_i = document.createElement('input');

      pop_i.type = 'radio';
      pop_i.id = 'notif_' + canal.substr(0,2).toLowerCase();
      pop_i.value = canal.normalize('NFD').replace(/[\u0300-\u036f]/g, '').replace(/\s/g, '_').toLowerCase();
      pop_i.name = "radio-idioma";

      if ( id_ca == true ){
        if(canal.substr(0,2).toLowerCase() == 'ca' ) pop_i.checked = id_ca;
      } else if (id_in == true ){
        if(canal.substr(0,2).toLowerCase() == 'in' ) pop_i.checked = id_in;
      } else if (id_fr == true ){
        if(canal.substr(0,2).toLowerCase() == 'fr' ) pop_i.checked = id_fr;
      }

      // Evento checkboxes
      pop_i.addEventListener('click', function(e){
        var etv = e.target.value;
        var trg = Array.from(e.target.closest('fieldset').querySelectorAll('span .notiflabel .radio input[type="radio"]'));

        var chk = trg.filter(function (obj) {
          return obj.checked == true;
        });

        id_ca = false, id_in = false, id_fr = false;
        chk.forEach(function (el) {

          // console.log("El id slice  >", el.id.slice(-2) );

          if (el.id.slice(-2) == 'ca') id_ca = true;
          if (el.id.slice(-2) == 'in') id_in = true;
          if (el.id.slice(-2) == 'fr') id_fr = true;

          // console.log("id_ca", id_ca );
          // console.log("id_in", id_in );
          // console.log("id_fr", id_fr );

        });

      }); // Agrega input a label
      pop_a.appendChild(pop_i);
      // Texto canal
      if (canal === 'castellano'){
        var pop_c = document.createTextNode(litLangES);
      } else if (canal === 'ingles'){
        var pop_c = document.createTextNode(litLangEN);
      } else if (canal === 'frances'){
        var pop_c = document.createTextNode(litLangFR);
      }
      // Agrega texto a label
      pop_a.appendChild(pop_c);
      // Agrega label a span
      pop_n.appendChild(pop_a);
      // Agrega span a fieldset
      pop_idiomas.appendChild(pop_n);
    });
    // IDIOMAS
    // idiomas
    pop_s.appendChild(pop_idiomas);
    // Agrega fieldset a form
    pop_f.appendChild(pop_s);
    // btn
    var pop_b = document.createElement('button');
    pop_b.name = 'button';
    pop_b.className = 'btn notifbut';
    pop_b.innerHTML = "Guardar";
    // Evento btn
    pop_b.addEventListener('click', function(e){
      e.preventDefault();
      subscribe(id_f, id_b, id_v, id_m, id_d, id_ca, id_in, id_fr);
      var pop_p = document.getElementById('indiag');
      pop_p.classList.remove('show');
      // quito popup
      window.setTimeout(removePopup, 550);
    });
    // Agrega botón a form
    pop_f.appendChild(pop_b);
    // Agrega form a popup
    pop.appendChild(pop_f);
    // nota
    var pop_t = document.createElement('p');
    pop_t.className = 'notifnote';
    pop_t.innerHTML = "Si en algún momento deseas cambiar las preferencias de tus notificaciones puedes hacerlo en el pie de la página.";
    // Agrega nota a popup
    pop.appendChild(pop_t);
    // Agrega popup a docuemnto
    document.body.appendChild(pop);
    // Custom checkboxes - Depende de jQuery y Custom-form
    var inputs = $(pop).find("[type=checkbox], [type=radio], select");
    inputs.custom_form();
    // Enseña popup
    pop.classList.toggle('show');
  };

  var subscribe = function(id_f, id_b, id_v, id_m, id_d, id_ca, id_in, id_fr) {
    var _subscribe = [];
    if (id_f) {
      _subscribe.push('futbol');
    }
    if (id_b) {
      _subscribe.push('baloncesto');
    }
    if (id_v) {
      _subscribe.push('promociones');
    }
    if (id_m) {
      _subscribe.push('madridistas');
    }
    if (id_ca && !id_in && !id_fr ) {
      _subscribe.push('castellano');
    }
    if (id_in && !id_ca && !id_fr ) {
      _subscribe.push('ingles');
    }
    if (id_fr && !id_in && !id_ca ) {
      _subscribe.push('frances');				  
    }
    if (id_d && !( id_f || id_b || id_v || id_m )) {
      indigitall.deviceDisable((device) => {
        // success function
      },() => {
        // error function													  
      });
    }

    //Reset de las suscripciones anteriores
    var tagIds = ["futbol", "baloncesto", "promociones", "madridistas","desactivar", "castellano" , "ingles" , "frances" ];
    indigitall.topicsUnsubscribe(tagIds, (topics) => {
      // success function
      if (_subscribe.length > 0) {
        indigitall.topicsSubscribe(_subscribe, (topics) => {
          // success function
           console.log(_subscribe);
        }, () => {
          // error function
        });
      }
    }, () => {
      // error function
    });
  };


</script><script type="text/javascript">
	
	var cookieUsuarioLogado = "";
	if (document.cookie.length>0){
		c_start=document.cookie.indexOf("usuario" + "=");
		if (c_start!=-1){
			c_start=c_start + "usuario".length+1;
			c_end=document.cookie.indexOf(";",c_start);
			if (c_end==-1) c_end=document.cookie.length;
			cookieUsuarioLogado = unescape(document.cookie.substring(c_start,c_end));
	    }
	}
	
	if (cookieUsuarioLogado != ""){
		cookieUsuario = cookieUsuarioLogado.split('@');
	}else{
		cookieUsuario = "";
	}
	
	if (cookieUsuario == ""){
		cookieUsuario[0] = "no logado";		
	}else{
		if (cookieUsuario[0] == "usuarioregistradoemployee"){
			reg_area.style.display="none";
		}
	}
</script></body>
