<script>
    require(['jquery'], function () {
        $(document).ready(function () {
            try {
                // This prevents pages that use iFrameResizer (/home/tv/recommendation) from overriding the iframe code
                disclaimerContentIframe.iFrameResizer.close();
                $('#disclaimerModal .modal-body').html(
                    '<iframe class="disclaimerContent" id="disclaimerContentIframe"></iframe>');
            } catch (e) {
                // Do nothing
            } finally {
                $('a.promo-details-modal-trigger, a.product-details-modal-trigger, a.disclaimer-trigger')
                    .click(function (event) {
                        event.preventDefault();
                        $("#disclaimerModal iframe").attr("src", this.href);
                        $("#disclaimerModal").modal("show");
                    });
                $('a.ptp-modal-trigger')
                    .click(function (event) {
                        event.preventDefault();
                        $("#ptpDisclaimerModal iframe").attr("src", this.href);
                        $("#ptpDisclaimerModal").modal("show");
                        window.dataLayer = window.dataLayer || [];
                        window.dataLayer.push({
                                'event': 'ptp_learn_more'
                            });
                    });
                $('#ptpDisclaimerModal .closeButton').click(function(event) {
                    event.preventDefault();
                    window.dataLayer = window.dataLayer || [];
                    window.dataLayer.push({
                            'event': 'ptp_chat_initiate'
                        });
                });
                $('#ptpDisclaimerModal .close').click(function(event) {
                    event.preventDefault();
                    window.dataLayer = window.dataLayer || [];
                    window.dataLayer.push({
                            'event': 'ptp_close'
                        });
                });
                $('#ptpDisclaimerModal .ecPTP').click(function(event) {
                    window.dataLayer = window.dataLayer || [];
                    window.dataLayer.push({
                            'event': 'ptp_order'
                        });
                });
                $('a.cyber-modal-trigger')
                    .click(function (event) {
                        event.preventDefault();
                        $("#cyberDisclaimerModal iframe").attr("src", this.href);
                        $("#cyberDisclaimerModal").modal("show");
                        window.dataLayer = window.dataLayer || [];
                        window.dataLayer.push({
                                'event': 'cyber_learn_more'
                            });
                    });
                $('#cyberDisclaimerModal .closeButton').click(function(event) {
                    event.preventDefault();
                    window.dataLayer = window.dataLayer || [];
                    window.dataLayer.push({
                            'event': 'cyber_chat_initiate'
                        });
                });
                $('#cyberDisclaimerModal .close').click(function(event) {
                    event.preventDefault();
                    window.dataLayer = window.dataLayer || [];
                    window.dataLayer.push({
                            'event': 'cyber_close'
                        });
                });
                // Firefox won't scroll down to the correct section if src is the same as a previous click
                $('#disclaimerModal .close, #disclaimerModal .closeButton').click(function (event) {
                    $("#disclaimerModal iframe").attr("src", "");
                });
            }
        });
    });

    require(['jquery', 'jquery-ui', 'centurycore', 'cc-shortcuts', 'modals'], function (require) {

        $('#ctam_new-customer-link').click(function (e) {
            e.preventDefault();

            /*toggle display of new customer content in modal block*/
            $('#ctam_current-customer-expanded').hide();
            $('#ec-ctam-modal').hide();
            $('#ec-ctam-acct').hide();
            $('#ec-modal-p').hide();
            $('#divAcct').css("display", "none");
            $('#ctam_existing-customer').css("display", "none");
            $('#ctam_new-customer').toggle();
            $('#nc-ctam-modal').toggle();
            $('#nc-modal-p').toggle();
            $('#ctam_new-customer-expanded').toggle();
        });

        $('#ctam_current-customer-link').click(function (e) {
            e.preventDefault();

            /*toggle display of new customer content in modal block*/
            $('#ctam_new-customer').hide();
            $('#nc-ctam-modal').hide();
            $('#nc-modal-p').hide();
            $('#ec-modal-p').toggle();
            $('#divAcct').css("display", "block");
            $('#ec-ctam-modal').toggle();
            $('#ctam_current-customer-expanded').toggle();
            $('#protectAcct-a').css("display", "block");
            $('#ctam_existing-customer').css("display", "block");
        });
        $('#protectInfo-a').click(function (e) {
            e.preventDefault();

            /*toggle display of link content in modal block*/
            $('#protectContent').css("display", "block");
            $('#contentAcct').css("display", "none");
            $('#divInfo').css("background", "white");
            $('#divAcct').css("background", "none");
            $('#divInfo').addClass("divTab");
            $('#containerContent').addClass("tabContainer");
            $('#infoClose').addClass("showClose");
            $('#acctClose').removeClass("showClose");
        });
        $('#protectAcct-a').click(function (e) {
            e.preventDefault();

            /*toggle display of link content in modal block*/
            $('#protectContent').css("display", "none");
            $('#contentAcct').css("display", "block");
            $('#divAcct').css("background", "white");
            $('#divInfo').css("background", "none");
            $('#divAcct').addClass("divTab");
            $('#protectAcct-a').addClass("protectAcct-display");
            $('#containerContent').addClass("tabContainer");
            $('#acctClose').addClass("showClose");
            $('#infoClose').removeClass("showClose");
        });
        $('#infoClose').click(function (e) {
            e.preventDefault();

            /*toggle display of link content in modal block*/
            $('#protectContent').css("display", "none");
            $('#divInfo').css("background", "none");
            $('#divInfo').removeClass("divTab");
            $('#containerContent').removeClass("tabContainer");
            $('#infoClose').removeClass("showClose");
        });
        $('#acctClose').click(function (e) {
            e.preventDefault();

            /*toggle display of link content in modal block*/
            $('#contentAcct').css("display", "none");
            $('#divAcct').css("background", "none");
            $('#divAcct').removeClass("divTab");
            $('#protectAcct-a').removeClass("protectAcct-display");
            $('#containerContent').removeClass("tabContainer");
            $('#acctClose').removeClass("showClose");
        });
        $('#zam_needed-link').click(function (e) {
            e.preventDefault();

            /*toggle display of link content in modal block*/
            $('#zamcontainerContent').addClass("zam_tabContainer");
            $('#zam_needed').addClass("zamTab");
            $('#zamcontainerContent').show();
            $('#zamcontainerContent').css("margin-bottom", "10px");
            $("#zam-modal .tabs").removeAttr('style').css("margin-top", "0px", "margin-bottom", "-20px",
                "padding-bottom", "0");
            $('#zam_needed-content').addClass("display_content");
        });
        $('#zam_help-a').click(function (e) {
            e.preventDefault();

            /*toggle display of link content in modal block*/
            $('#zamcontainerContent').removeClass("zam_tabContainer");
            $('#zam_needed').removeClass("zamTab");
            $("#zam-modal .tabs").removeAttr('style').css("padding-bottom", "20px");
            $('#zamcontainerContent').hide();
            $('#zam_needed-content').removeClass("display_content");
        });
        $('#zam_zip-code').blur(function () {
            var zipLength = $(this).val();
            zipLength = (zipLength ? zipLength.match(/\d/g).length : 0);

            if (zipLength == 5) {
                $('#zam_overlap-go').click(function () {
                    return true;
                });
            }
        });
        if (/help/.test(window.location.href)) {
            $('#zam_close_link').css("margin-top", "4px");
        }
        if (/home/.test(window.location.href)) {
            $('#zam-CTA').css("margin-top", "0px");
        }
        if (/bundles/.test(window.location.href)) {
            $('#protectAcct-a').css("display", "inline-block");
            $('#acctClose').css("margin-left", "-17px");
            $('#acctClose').css("padding-top", "16px");
        }
        $().ready(function () {
            $('#ec-ctam-modal,#ec-modal-p').hide();
        });
    });
</script>
