var windowWidth = $(window).width();
var windowSm = 768;
if (windowWidth <= windowSm) {
  $slider = $('.slider');
  $slider.slick({	
	    fade:true,
      speed: 1500,
	  arrows: false,
	  dots: true,	
	  dotsClass: 'slide-dots',	
      autoplay: true,
      autoplaySpeed: 4000,
      pauseOnFocus: false,
      pauseOnHover: false
    });	
} else {
  $slider = $('.slider');
  $slider.slick({	
	  arrows: true,
	  dots: true,	
	  dotsClass: 'slide-dots',	
      autoplay: true,
      autoplaySpeed: 4000,
      pauseOnFocus: false,
      pauseOnHover: false,
