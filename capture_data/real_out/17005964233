<script language="Javascript" type="text/javascript" nonce="e327cacca4507cafeacab63c3bc1707945ca527081e821a613ad24d55f2fd33b">
	var selectedPackagetopCard;

	// Allow for flexible discount on either/both annual and 3-year
	var hasAnnualDiscounttopCard = false;
	var hasThreeYearDiscounttopCard = false;

	document.addEventListener('DOMContentLoaded', function () {
		document.getElementById("annual-radio-topCard").addEventListener("click", function (event) {
			selectedPackagetopCard = "hushmail-premium-individual-desktop-annual";
			setPackagePeriodtopCard(true, hasAnnualDiscounttopCard)
		})
		document.getElementById("three-year-radio-topCard").addEventListener("click", function (event) {
			selectedPackagetopCard = "hushmail-premium-individual-desktop-three-year";
			setPackagePeriodtopCard(false, hasThreeYearDiscounttopCard)
		})

		document.getElementById("goToCheckout-topCard").addEventListener("click", function (event) {
			// TODO what is the tag for 3 year and annual? Should we add more tags?
			window.location = "https://secure.hushmail.com/signup/?package=" + selectedPackagetopCard + "&amp;source=website&amp;tag=btn_premium_desktop"
		});

		// Click on annual to set display
		document.getElementById("annual-radio-topCard").click()
	});

	function setPackagePeriodtopCard(isAnnual, isDiscounted) {

		// Handle package period display
		for (ele of document.getElementsByClassName("element_annual-pricing-topCard")) {
			isAnnual ? ele.classList.remove("hide") : ele.classList.add("hide")
		}
		for (ele of document.getElementsByClassName("element_three-year-pricing-topCard")) {
			isAnnual ? ele.classList.add("hide") : ele.classList.remove("hide")
		}

		// Handle discounted display
		var currPriceClassList = document.getElementById("element_current_price-topCard").classList
		isDiscounted ? currPriceClassList.add("discounted") : currPriceClassList.remove("discounted")

		document.getElementById("element_original-price-topCard").style.display = (isDiscounted) ? "block" : "none";
	}
</script>					</div>
