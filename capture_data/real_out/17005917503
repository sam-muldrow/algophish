		<script>
			function showFEBAPage() {
				$('#FEBAPage').fadeIn();
			}
			function closePopups(e) {
				$('#FEBAPage').fadeOut();
			}
			$('body').click(function(e) {
				var value = e.target.classList.contains('transparentBackground');
				if(value) {
					closePopups(e);
				}
			});
		</script>
		<script type="text/javascript">
		<!--
			var b_timer = null; // blink timer
			var b_on = true; // blink state
			var blnkrs = null; // array of spans
			function blink() {
				var tmp = document.getElementsByTagName("span");
				if (tmp) {
					blnkrs = new Array();
					var b_count = 0;
					for (var i = 0; i < tmp.length; ++i) {
						if (tmp[i].className == "blink") {
							blnkrs[b_count] = tmp[i];
							++b_count;
						}
					}
					// time in m.secs between blinks
					// 500 = 1/2 second
					blinkTimer(500);
				}
			}
			function blinkTimer(ival) {
				if (b_timer) {
					window.clearTimeout(b_timer);
					b_timer = null;
				}
				blinkIt();
				b_timer = window.setTimeout('blinkTimer(' + ival + ')', ival);
			}
			function blinkIt() {
				for (var i = 0; i < blnkrs.length; ++i) {
					if (b_on == true) {
						blnkrs[i].style.visibility = "hidden";
					}
					else {
						blnkrs[i].style.visibility = "visible";
					}
				}
				b_on =!b_on;
			}
		//-->
		</script>


<!-- <script type='text/javascript'>
$(function(){

var overlay = $('<div id="overlay"></div>');
overlay.show();
overlay.appendTo(document.body);
$('.popup').show();
//$('#pops').show();

$('.close').click(function(){
$('.popup').hide();
overlay.appendTo(document.body).remove();
return false;
});




$('.x').click(function(){
$('.popup').hide();
overlay.appendTo(document.body).remove();
return false;
});
});

function explode(){

 $('.popup').hide();
jQuery("#overlay").remove();
return false;
}

setTimeout(explode, 5000);


</script> -->



 <script type='text/javascript'>
$(function(){

var overlay = $('<div id="overlay"></div>');
overlay.show();
overlay.appendTo(document.body);
$('.popups').show();
//$('#pops').show();

$('.closes').click(function(){
$('.popups').hide();
overlay.appendTo(document.body).remove();
return false;
});




$('.x').click(function(){
$('.popups').hide();
overlay.appendTo(document.body).remove();
return false;
});
});

function explode(){

 $('.popups').hide();
jQuery("#overlay").remove();
return false;
}

setTimeout(explode, 5000);


</script>



