							$(this).next().slideToggle('normal'); // Toggle dd when the respective dt is clicked
							$(this).toggleClass('active');
						});
					</script>

					<!-- jquery sliding panel for Gujarati -->
					<script type="text/javascript">
						$('.quickLinks #Gujarati4 dt').hover(function () {  $(this).addClass('hover') }, function () {  $(this).removeClass('hover') }).click(function () {

