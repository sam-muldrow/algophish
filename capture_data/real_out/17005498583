<script>
if (document.getElementById("ytplayer")){
	var s = document.getElementById("ytplayer").src;
	if (s.includes("playlist")){
	s = s.substring(s.indexOf('=')+1, 100);
	var emburl = "https://www.youtube.com/embed/";
	emburl = emburl.concat(s);
	document.getElementById("ytplayer").src = emburl;
	}
}
