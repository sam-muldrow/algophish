     x-bind:class="cssClass ? cssClass : 'bg-black/30'"
     x-data="{
         show: false,
         cssClass: false,
     }"
     x-on:show-modal-backdrop.window="show = true; cssClass = $event.detail?.cssClass"
     x-on:hide-modal-backdrop.window="show = false"
     x-show="show"
     x-cloak
></div>
            <div id="global-modals-app"></div>
            
            
            <div class="w-full fixed lg:bottom-2 text-white left-0 right-0 text-sm z-30 lg:flex pt-8"
    x-cloak
    x-data="{
        heading: '',
        message: '',
        timing: 500,
        myTimeout() {
            return setTimeout(() => { this.message = '', this.heading }, this.timing)
        },
        timeoutId: '',
        showMessage(eventDetail) {
            clearTimeout(this.timeoutId)
            this.heading = eventDetail.heading;
            this.message = eventDetail.message;
            this.timing = eventDetail.timing || 2500;
            this.timeoutId = this.myTimeout();
        }
    }"
    x-on:show-toast.window="showMessage(event.detail)"
    x-show="message.length > 0"
            x-bind:class="$store.global.mobileFixedNavPresentComputed ? 'bottom-safe-14' : 'bottom-safe-0'"
    >
    <div class="bg-purple-700 flex lg:inline-flex items-center justify-center lg:rounded space-x-2 p-2 lg:px-4 mx-auto">
        <div class="flex">
            <svg role="presentation" aria-hidden="true"  class="fill-current pointer-events-none w-4 h-4 text-white mr-2 mt-0.5">
    <use href="https://www.mumsnet.com/assets/images/icons.180e99b8b7e4de461c12cc7c0a07af93.svg#check-circle"></use>
</svg>
            <div class="flex flex-col">
                <p class="font-bold" x-text="heading"></p>
                <p x-html="message"></p>
            </div>
        </div>
    </div>
</div>
