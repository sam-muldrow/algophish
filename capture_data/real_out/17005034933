                            <script>

        // market widget menu on mobile

        var windowWidth = $(window).width();

        if (windowWidth <= 950) {

            var widgetMenu = $('#widget-menu');
            var widgetBtn = $("#widget-mobile-btn");

            $(widgetMenu).addClass('hidden');

            $(widgetBtn).click(function () {
                $(this).toggleClass('open');
                $(widgetMenu).toggleClass('hidden');
            });

            $("#widget-menu a").click(function () {
                var selectedTab = $(this).html();
                $(widgetMenu).addClass('hidden');
                $(widgetBtn).removeClass('open');
                $('#selected-tab-mobile').html(selectedTab);
            });
        }
    </script>

                

