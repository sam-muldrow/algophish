            window._io_config["0.2.0"].push({"page_url":"https:\/\/dir.bg","page_url_canonical":"https:\/\/dir.bg","page_title":"Dir.bg - \u0411\u044a\u043b\u0433\u0430\u0440\u0441\u043a\u0438\u044f\u0442 \u0418\u043d\u0442\u0435\u0440\u043d\u0435\u0442 \u043f\u043e\u0440\u0442\u0430\u043b!","page_type":"main","page_language":"bg"});
