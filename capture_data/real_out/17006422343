 var AdDev = (function(){
 'use strict';

 function findAdmUrl(){
 var result='/sitegeneli';
 var temp = new Array();
 var currentPath = window.location.pathname;
 var i=0;
 if(currentPath == "/")
 return "/anasayfa";

if (currentPath.includes("/video"))
return "/webtv";

 for(i=0; i<admList.length; i++){
 var itm = admList[i];
 if(currentPath.includes(itm)){
 temp.push(itm);
 }
 }
 var countTemp=0;
 if(temp.length>0){
 for(i=0; i<temp.length; i++){
 if(temp[i].split('/').length>countTemp){
 result = temp[i];
 countTemp=temp[i].split('/').length;
 }
 }
 }
 return result;
 }

 function generateSlotPath(adminUrl,st, alt){
 var root = '/31110078,22727463451/takvim/';
 var result='';
 var path='';
 try{
 if(szSgList.includes(st)){
 path = '/sitegeneli';
 }else{
 path = alt ? '/viewable':adminUrl;
 }
 var medium = deviceInfo.isMobile ? 'mobile_web' : 'desktop_web';
 result = root + medium + path;
 }
 catch(err){
 AdDev.AdDevLog('AdDev Error', 'Ad error when generateSlotPath for: ' + st);
 }
 return result;
 }

 function calculateThreshold(el) {
 /*Except for article detail pages, almost all pages are infinite pages. In this kind of pages, bound should be set to 0 to prevent viewability issues*/
 try{
 if(
 (bdPathCount==1 && (($(el).attr('ad')).includes('970x250_3') || ($(el).attr('ad')).includes('970x250_4')))
 || (window.location.pathname.includes('galeri') && (($(el).attr('ad')).includes('970x250_3') || ($(el).attr('ad')).includes('970x250_4')))
 ){
 return $(el).attr('threshold'); //return 0;
 }
 else if($(el).attr('threshold')!=null){
 return $(el).attr('threshold');
 }else{
 return defaultViewableThreshold;
 }

 }
 catch(err){
 AdDev.AdDevLog('AdDev Error', 'Ad error when calculateThreshold', err);
 return 0;
 }
 }

 function bdGetPathCount() {
 var pathCount = 0;
 var i=0;
 for (i = 0; i < window.location.pathname.split('/').length; i++) {
 if (window.location.pathname.split('/')[i].length > 0) {
 pathCount++;
 }
 }
 return pathCount;
 }

 function bdGetCookie(o) {
 for (var e = o + "=", t = e.length, n = document.cookie.length, i = 0; n > i;) {
 var d = i + t;
 if (document.cookie.substring(i, d) == e) return bdGetCookieVal(d);
 if (0 == (i = document.cookie.indexOf(" ", i) + 1)) break;
 }
 return null;
 }

 function bdGetCookieVal(o) {
 var e = document.cookie.indexOf(";", o);
 return -1 == e && (e = document.cookie.length), unescape(document.cookie.substring(o, e))
 }

 function bdDeleteCookie(o, e, t) {
 bdGetCookie(o) && (document.cookie = o + "=" + (e ? "; path=" + e : "") + (t ? "; domain=" + t : "") + "; expires=Thu, 01-Jan-70 00:00:01 GMT")
 }

 function bdSetCookie(o, e, t, n, i, d) {
 document.cookie = o + "=" + escape(e) + (t ? "; expires=" + t : "") + (n ? "; path=" + n : "") + (i ? "; domain=" + i : "") + (d ? "; secure" : "")
 }

 function bdHide970x250() {
 googletag.pubads().clear([slot_d_970x250]),$(".slot_control").remove();
 }

 function bdManageBdNtn(liId){
 var date = new Date();
 var expireDate = new Date();
 expireDate.setDate(date.getDate() + 7);
 var bdNtnArr = 1;
 if(AdDev.BdGetCookie('bdNtn')!=null){
 bdNtnArr = AdDev.BdGetCookie('bdNtn').split('-');
 }
 var bdNtnStr = '';

 switch(liId) {
 case 4516013402:
 $('<img/>').attr({src:'https://pubads.g.doubleclick.net/activity;dc_iu=/31110078,22727463451/DFPAudiencePixel;ord=1;dc_seg=838068800?',height:1,width:1}).appendTo('body');
 bdNtnStr = '1';
 bdSetCookie('bdNtn', bdNtnStr, expireDate.toGMTString(), '/');
 break;
 case 5094815940:
 $('<img/>').attr({src:'https://pubads.g.doubleclick.net/activity;dc_iu=/31110078,22727463451/DFPAudiencePixel;ord=1;dc_seg=840096258?',height:1,width:1}).appendTo('body');
 bdNtnStr = '2';
 bdSetCookie('bdNtn', bdNtnStr, expireDate.toGMTString(), '/');
 break;
 case 5095020844:
 $('<img/>').attr({src:'https://pubads.g.doubleclick.net/activity;dc_iu=/31110078,22727463451/DFPAudiencePixel;ord=1;dc_seg=838357660?',height:1,width:1}).appendTo('body');
 bdNtnStr = '3';
 bdSetCookie('bdNtn', bdNtnStr, expireDate.toGMTString(), '/');
 break;
 case 5095027360:
 $('<img/>').attr({src:'https://pubads.g.doubleclick.net/activity;dc_iu=/31110078,22727463451/DFPAudiencePixel;ord=1;dc_seg=838357642?',height:1,width:1}).appendTo('body');
 bdNtnStr = '4';
 bdSetCookie('bdNtn', bdNtnStr, expireDate.toGMTString(), '/');
 break;
 case 5095047004:
 $('<img/>').attr({src:'https://pubads.g.doubleclick.net/activity;dc_iu=/31110078,22727463451/DFPAudiencePixel;ord=1;dc_seg=840096987?',height:1,width:1}).appendTo('body');
 bdNtnStr = '5';
 bdSetCookie('bdNtn', bdNtnStr, expireDate.toGMTString(), '/');
 break;
 case 5095332401:
 $('<img/>').attr({src:'https://pubads.g.doubleclick.net/activity;dc_iu=/31110078,22727463451/DFPAudiencePixel;ord=1;dc_seg=838069487?',height:1,width:1}).appendTo('body');
 bdNtnStr = '6';
