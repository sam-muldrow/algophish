    <script type="text/javascript">

        var gemiusIdForInstagramStory = '';

        function CheckandAppendScript() {
            var scripts = document.getElementsByTagName('script');
            var scriptExist = 'https://i.tmgrup.com.tr/videojs/holascripts/hola_player.js?v=120.7';
            for (var i = scripts.length; i--;) {
                var s = scripts[i].src;
                s= s.substring(0, s.indexOf('?'));
                if (s == scriptExist.substring(0, scriptExist.indexOf('?'))) {
                    return;
                }
            }
            var ele = document.createElement('script');
            ele.setAttribute("type", "text/javascript");
            ele.setAttribute("src", scriptExist);
            $('head').append(ele);
        }

    </script>

