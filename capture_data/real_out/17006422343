    <script async>
        var apiRoot = "https://webapi.tmgrup.com.tr";

        if (window.deviceInfo != undefined && window.deviceInfo.isMobile != undefined && !window.deviceInfo.isMobile) {
            document.addEventListener('DOMContentLoaded', currenyWidgetFunction());
            document.addEventListener('DOMContentLoaded', widgetFunction());
        } else {
