       datanav='<li ><a name="nav1" href="http://book.dangdang.com/" target="_blank">图书</a></li><li ><a name="nav1" href="http://e.dangdang.com/index_page.html" target="_blank">电子书</a></li><li ><a name="nav1" href="http://category.dangdang.com/cid4004344.html" target="_blank">童装童鞋</a></li><li ><a name="nav1" href="http://category.dangdang.com/cid4003844.html" target="_blank">女装</a></li><li ><a name="nav1" href="http://category.dangdang.com/cid4002145.html" target="_blank">食品</a></li><li ><a name="nav1" href="http://category.dangdang.com/cid4002061.html" target="_blank">母婴玩具</a></li>';
    }else{
    datanav='<li ><a name="nav1" href="https://book.dangdang.com" target="_blank">图书</a></li><li ><a name="nav1" href="http://e.dangdang.com/index_page.html" target="_blank">电子书</a></li><li ><a name="nav1" href="http://category.dangdang.com/cid4004344.html" target="_blank">童装童鞋</a></li><li ><a name="nav1" href="http://category.dangdang.com/cid4003844.html" target="_blank">女装</a></li><li ><a name="nav1" href="http://category.dangdang.com/cid4002145.html" target="_blank">食品</a></li><li ><a name="nav1" href="http://category.dangdang.com/cid4002061.html" target="_blank">母婴玩具</a></li>';
    }
    return datanav;
}
