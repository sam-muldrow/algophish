      <script type="text/javascript">
    function waitForAnalyticsInvoked(callback) {
      const analyticsLoaded = (callback) => {
        /* initialized seems to be called on mobile device and invoked on desktop */
        if (window.analytics && (window.analytics.initialized || window.analytics.invoked)) {
          callback();
        } else {
          setTimeout(waitForAnalyticsInvoked(callback), 250);
        }
      }

      analyticsLoaded(callback);
    }
    window.waitForAnalyticsInvoked = waitForAnalyticsInvoked;

    function extractSegmentClickData(target) {
      // allow child elements to pass parent data
      const segmentClickTarget = target.closest('[track-click]')

      if (!segmentClickTarget) { return {} }

      const segmentClickData = $(segmentClickTarget).attr('track-click').split("|").reduce((acc, pair) => {
        const [key, value] = pair.split(":");
        acc[key.trim()] = value ? value.trim() : undefined;
        return acc;
      }, {});
      return segmentClickData;
    }
    window.extractSegmentClickData = extractSegmentClickData;

    function segmentTrackClickHandler(event){
      console.log('segmentTrackClickHandler');
      let eventTargetSkipClick = event.target.tagName == 'SELECT' || event.target.tagName == 'INPUT';
      if ( event.type == 'click' && eventTargetSkipClick){ return; }
      let trackedClickPromise;
      let clickTraits;
      let clickOption;

      if (event.target.tagName == 'SELECT' ){
        let selectedOption = $(event.target).find('option[value="' + event.target.value + '"]');
        if (selectedOption != 'undefined' && selectedOption.length > 0) { 
          clickOption = $(selectedOption[0]).attr('click-option');
          if (clickOption == undefined) { 
            clickOption = selectedOption[0].text;
          }
        } 
      }

      clickTraits = extractSegmentClickData(event.target);    
      if (clickTraits == {}) { return; }

      if (clickOption != undefined) { 
        clickTraits['click option'] = clickOption;
      }

      const { 'event name': eventName, ...segmentClickTraits } = clickTraits;
      let segmentEventName = eventName || "page click";

      if (segmentEventName == 'page clicks') { 
        segmentEventName = 'page click' 
      }
      
      if (typeof(eventProps) != 'undefined' && (eventProps['search context'] === true ||  eventProps['search context'] === 'true')){
        lastSearch = getLastSearchContext();
        if (lastSearch === false){ return;}
        delete(segmentClickTraits["search context"]);

        // unpack all jp_ keys into job post keys
        segmentClickTraits = Object.keys(segmentClickTraits).reduce((acc, key) => {
          const newKey = key.replace(/^jp_/, 'job post ');
          acc[newKey] = segmentClickTraits[key];
          return acc;
        }, {});

        segmentClickTraits = Object.assign(eventProps, lastSearch);
      }

      trackedClickPromise = TrackEvents(segmentEventName, segmentClickTraits, SegmentUserId(), segmentIsLoggedin); 
    }
    window.segmentTrackClickHandler = segmentTrackClickHandler;

    /* This is some code that we need to run after the Segment script finishes loading asynchronously */
    function segmentAnalyticsLoadedCallback(){
      var segmentIsLoggedin = 'false';
      window.segmentIsLoggedin = segmentIsLoggedin;

      function SegmentUserId(){
        let segmentUserId = '';
        if (segmentUserId != '') { 
          return segmentUserId;
        }

        let boldVisitorID = jsUtility.get_Cookie('vsuid');
        if (boldVisitorID != undefined){
          return boldVisitorID;
        } 

        let segmentAnonymousId;
        try { 
          segmentAnonymousId = SegmentAnonymousId();
        } catch (error) {
          console.log(error);
        }

        return segmentAnonymousId;
      }
      window.SegmentUserId = SegmentUserId;

      function SegmentAnonymousId(){
        /* TODO: Should we even be doing this vsuid check?! Probably not right? */
        let visitorId = jsUtility.get_Cookie('vsuid');
        if (visitorId != undefined && visitorId != '') { 
          return visitorId;
        }

        if (typeof(analytics?.user) === 'function') { 
          return analytics.user().anonymousId();
        } else { 
          return localStorage['ajs_anonymous_id']?.replaceAll('"','');
        }
      }
      window.SegmentAnonymousId = SegmentAnonymousId;
      
      function docReady(e){
        "complete"===document.readyState || "interactive"=== document.readyState ? setTimeout(e,1) : document.addEventListener("DOMContentLoaded",e)
      }
      window.docReady = docReady;

        
          docReady(function(){
    waitForAnalyticsInvoked(()=> {
      if (typeof(TrackPageEvents) === 'function') { 
        let pageType = null; 
        TrackPageEvents(pageType, segmentIsLoggedin);
      }
    });
  });


            docReady(function(){
      waitForAnalyticsInvoked(()=> {
      });
    });


        function getLastSearchContext(){
  const lsls = fjLS.getItem("lastSearch");
  if ( lsls?.expired === false ){
    return lsls.content;
  } else {
    false;
  }
};



  docReady(function(){
    waitForAnalyticsInvoked(() => {
      if (typeof(TrackEvents) === 'function') { 

              var eventProps = {
                "experiment name": "FJ-17663",
                "variation name": "Control"
              };
              var trackEventsPromise = TrackEvents("FJ Experiment Viewed", eventProps, SegmentUserId(), segmentIsLoggedin);  

                let body = {
                  test_name: eventProps["experiment name"],
                  user_variant: "A",
                };

                fetch('/info/ab', {
                  method: 'POST',
                  headers: {
                    'Content-Type': 'application/json',
                    'X-CSRF-Token': document.querySelector("meta[name='csrf-token']").getAttribute("content")
                  },
                  body: JSON.stringify(body)
                })
                .catch(error => console.error('/info/ab Error:', error));


      }
    });
  });

        

        
docReady(function(){
    waitForAnalyticsInvoked(()=> {
        if (typeof(TrackEvents) === 'function') {         
          $('#nav-main a.nav-link').click(function(){
            let headerClickText;
            
            if (this.id == 'dropdown-menu-member-link') { 
              headerClickText = 'account settings';
            } else {
              headerClickText = $(this).text().trim();
            }

            let headerClickTraits = {
              'header type': 'header',
              'header': headerClickText,
              'sub header': null,
            };
            TrackEvents('header nav bar clicks', headerClickTraits, SegmentUserId(), segmentIsLoggedin);
          });

          $('#nav-sub a.nav-link').click(function(){
            let subHeaderClickTraits = {
              'header type': 'sub header',
              'header': $('#nav-main li.nav-item.active a.nav-link').text(),
              'sub header': $(this).text()?.trim(),
            }
            TrackEvents('header nav bar clicks', subHeaderClickTraits, SegmentUserId(), segmentIsLoggedin);
          });
        }
    });
});


        
docReady(function(){
  waitForAnalyticsInvoked(()=> {
    if (typeof(TrackEvents) === 'function') { 
      $('[track-click]').off("click input", segmentTrackClickHandler).on("click input", segmentTrackClickHandler);
    }
  });
});

        
// parse an event to see if the target has segment click data
function extractSegmentVisibilityData(target) {
  const segmentVizData = $(target).attr('track-viz').split("|").reduce((acc, pair) => {
    const [key, value] = pair.split(":");
    acc[key.trim()] = value.trim();
    return acc;
  }, {});

  return segmentVizData;
}

docReady(function(){
  waitForAnalyticsInvoked(()=> {
    if (typeof(TrackEvents) === 'function') { 

      var observer = new IntersectionObserver(onIntersection, {
        root: null,   

        threshold: .25 
      })

      function onIntersection(entries, opts){
        entries.forEach(entry => {


          if (entry.isIntersecting) {
            let trackedVizPromise;
            let vizTraits = extractSegmentVisibilityData(entry.target);
            if (vizTraits == {}) { return; }

            vizTraits.action = "viewed";
            const { 'event name': eventName, ...segmentVizTraits } = vizTraits;
            let segmentEventName = eventName || "page element viewed";
            trackedVizPromise = TrackEvents(segmentEventName, segmentVizTraits, SegmentUserId(), segmentIsLoggedin);
            observer.unobserve(entry.target);
          }
        })
      }

        // observer.observe(document.querySelector('[track-viz]'))
        $('[track-viz]').each(function(){
          observer.observe(this);
        })
    }
  });
});


      if (window.segmentOnloadQueue){
        window.segmentOnloadQueue.forEach(function (event) {
          try {
            let segmentPromise = TrackEvents(event.name, event.properties, SegmentUserId(), segmentIsLoggedin);
          } catch (error) {
            console.error('An error occurred while executing TrackEvents:', error);
          }
        });
      }
    };

    // parse an event to see if the target has segment click data
    function extractSegmentClickData(target) {

      // allow child elements to pass parent data
      const segmentClickTarget = target.closest('[track-click]')

      if (!segmentClickTarget) { return {} }

      const segmentClickData = $(segmentClickTarget).attr('track-click').split("|").reduce((acc, pair) => {
        const [key, value] = pair.split(":");
        acc[key.trim()] = value.trim();
        return acc;
      }, {});
      return segmentClickData;
    }

    window.extractSegmentClickData = extractSegmentClickData;
    
