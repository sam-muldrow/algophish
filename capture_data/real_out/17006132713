  <script type="text/javascript">

    var EVENT_LOG_DEST = {
      AMPLITUDE: 'AMPLITUDE',
      GOOGLE: 'GOOGLE'
    }

    function logFJEvent(eventLogDestination, eventName, eventProps) {

        var globalPrivacyControl = navigator.globalPrivacyControl && false;
        if (globalPrivacyControl) { return; }

        switch(eventLogDestination) {
          case EVENT_LOG_DEST.AMPLITUDE:
            logAmplitudeEvent(eventName, eventProps)
            return
          case EVENT_LOG_DEST.GOOGLE:
            logGoogleAnalyticsEvent(eventName, eventProps)
            return
        }
    }

    function logGoogleAnalyticsEvent(eventName, eventProps = {}) {
      
        var globalPrivacyControl = navigator.globalPrivacyControl && false;
        if (globalPrivacyControl) { return; }

        if (typeof gtag !== 'undefined') {
          gtag('event', eventName, eventProps);
        }
        else {
          console.error('gtag is undefined');
        }

      }

   function logAmplitudeEvent(eventName, addProps) {
    }

  </script>
    


