  <script>
    $(document).ready(function() {
      $('a.nav-link-articles,a.nav-link-resources').click(function(e) {
      });

      $('#nav-sub a').click(function() {
        $('#nav-sub a').removeClass('active');
        $(this).addClass('active');
      });

      $('a.nav-link-viewed_jobs').click(function(e) {
        e.preventDefault();
        var url = '/members/jobs-recently';
        if (localStorage['fjrecentjobs']) {
          url += '?ids=' + localStorage['fjrecentjobs'];
        }
        window.location = url;
      });

      if (window.location.href.indexOf('#learning-center-courses-anchor') > 0) {
        $('a.nav-link-linkedin_courses').addClass('active');
      }
      else if (window.location.href.indexOf('#learning-center-webinars-anchor') > 0) {
        $('a.nav-link-webinars').addClass('active');
      }
      else if (window.location.href.indexOf('#learning-center-guides-anchor') > 0) {
        $('a.nav-link-guides').addClass('active');
      }
      else if (window.location.href.indexOf('#learning-center-guides-anchor') > 0) {
        $('a.nav-link-stress').addClass('active');
      }

        $('a.btn-questionnaire').click(function() {
          logFJEvent(EVENT_LOG_DEST.GOOGLE, 'click', { event_category: 'HPQuiz', event_label: 'header-lightbulb-click' });
          var amplitudeEventProps = {
            where: 'hp'
          };
          logFJEvent(EVENT_LOG_DEST.AMPLITUDE, 'clk Quiz Lightbulb', amplitudeEventProps);
        });

      if ($('#search-keywords').length > 0) {
        kw_typeahead = $('#search-keywords').typeahead({
            minLength: 1,
            limit: 30,
            template: function(data) { return '<p>'+data.dropdownValue+'</p>' },
            remote: {
                url: '/info/keywords.js?term=%QUERY',
                filter: function(data) {
                  const result = [];
                  data.forEach((item) => {
                    result.push( { 'value': item.replace(/(<([^>]+)>)/gi, ""), 'dropdownValue': item });
                  });
                  return result;
                }
            },
            cache: false
        });

        $('#search-keywords').bind('typeahead:selected', function(obj, datum, name) { 
          if (datum.value.indexOf('href') > -1 || datum.value.indexOf('<span') > -1) {
            $("#search-keywords").val("");
          } else {
            $('button.search-look').click();
          }
        });

        kw_typeahead.data('ttView').dropdownView.on('suggestionsRendered', function (evtObj) {
          $('#search-keywords-wrapper').find('.tt-suggestion').each(function() {
            if ($(this).find('.heading').length > 0) {
              $(this).removeClass('tt-suggestion');
              if ($(this).find('.heading').length > 0) {
                $(this).addClass('search-keywords-header px-3');
              }
            }
          });
        });

        $('.tt-query').focusin(function() {
        $(this).addClass('highlight');
        });

        $('.tt-query').focusout(function() {
        $(this).removeClass('highlight');
        });

      }

    });
  </script>
