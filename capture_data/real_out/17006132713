 $(document).ready(function(){
   var videoOpen = false;
   $("#vimeo-start").click(function(e) {
     e.preventDefault();
     if (videoOpen == false) { 
       videoOpen = true;

       logFJEvent(EVENT_LOG_DEST.GOOGLE, 'play', {
         'event_category': 'video',
         'event_label': 'hp'
       });

       $("#vimeo-start").text("Hide Video");
       $.ajax({
         url: '/homepage_video?location=homepage',
         complete: function(xhr) {
           $("#homepage-video").html(xhr.responseText);
           $("#homepage-video").slideToggle( "slow" );
           $('html, body').animate({
             scrollTop: $("#homepage-video").offset().top - 300
           }, 1000);
         }
       });
     } else  {
       videoOpen = false;
       $("#vimeo-start").text("Watch a Video");
       $("#homepage-video").slideToggle( "slow" );
       $("#homepage-video").empty();
     }
   });


   $("#homepage-quiz-link").click(function(e) {
