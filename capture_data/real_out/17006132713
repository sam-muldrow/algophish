  <script>

var fjHost = '';
function inputFilter() {
    var e = event || window.event;
    var key = e.keyCode || e.which;
    if ( ((key>=48) && (key<=57)) || (key==8) || (key==37) || (key==39) || (key==46) ) {//allow backspace //and delete
        return true;
    }
    if (e.preventDefault) e.preventDefault(); 
    e.returnValue = false; 
};

$(document).ready(function(){
    var amplitudeEventProps = {
        where: 'hp',
    };




    $('#flexerQuestionnaire').on('hidden.bs.modal', function() {
        $("#questionnaire_tooltip").show(600).delay(5000).hide(600);
    });

    $('#flexerQuestionnaire').on('shown.bs.modal', function() {
        $('<link/>', { rel: 'stylesheet', type: 'text/css', href: '/css/select2.min.css' }).appendTo('head');
        $.getScript('/js/select2.min.js', function() {});
    });

    $('#flexerQuestionnaire').on('shown.bs.modal', function() {
        $("#fq-form").data("fq-page", 0);
        $('#flexerQuestionnaire').addClass('shown');
        $.ajax({
            url: fjHost +'/flexer_questionnaire?popup=1',
            complete: function(xhr) {
            }
        });
    });

    $(document).keypress(function(e) {
        if (e.which == 13) {
            var page_num = $("#fq-form").data("fq-page");
            $("#fq-page-" + page_num).find(".next:enabled").trigger("click"); 
        }
    });

    $(".next").click(function(e) {
        var current_page = $(this).closest("div[id^='fq-page-']").attr('id');
        var page_num = current_page.split('-');
        var next_page = parseInt(page_num[2]) + 1;
        $("#fq-page-" + page_num[2]).slideUp('slow');
        $("#fq-page-" + next_page).slideDown('slow');
        $("#fq-form").data("fq-page", next_page);
        
        $('button.btn-close').attr('track-click', 'event name:personalized search popup|action:clicked|click option:close|popup section number:' + (next_page + 1));

        if (next_page == 1) {
            $('#cats').select2({ maximumSelectionLength: 5, placeholder: 'Select up to five categories.' })
            $('#telecommute').select2();
        } else if (next_page == 2) {
            $("#fq-country").focus();
        }

        if (next_page > 1) {
            var name = $("#fq-name").val();
            $(".fq-name").html(name);
        }

        if (next_page < 3) {
            logFJEvent(EVENT_LOG_DEST.GOOGLE, 'click', { event_category: 'HPQuiz', event_label: 'next - step ' + (next_page + 1) });

            logFJEvent(EVENT_LOG_DEST.AMPLITUDE, 'mv Quiz Step ' + (next_page + 1), amplitudeEventProps);
        }
    });

    $(".prev").click(function(e) {
        var current_page = $(this).closest("div[id^='fq-page-']").attr('id');
        var page_num = current_page.split('-');
        var prev_page = parseInt(page_num[2]) - 1;
        $("#fq-page-" + page_num[2]).slideUp('slow');
        $("#fq-page-" + prev_page).slideDown('slow');
        $("#fq-form").data("fq-page", prev_page);

        $('button.btn-close').attr('track-click', 'event name:personalized search popup|action:clicked|click option:close|popup section number:' + (prev_page + 1) );

        logFJEvent(EVENT_LOG_DEST.GOOGLE, 'next', { event_category: 'HPQuiz', event_label: 'prev page ', value: (prev_page + 1) });
        logFJEvent(EVENT_LOG_DEST.AMPLITUDE, 'mv Quiz Step ' + (prev_page + 1), amplitudeEventProps);
    });

    $("#fq-submit").click(function(e) {

        e.preventDefault();

        var tele_levels = $('#telecommute').val();
        var telecommute_str = '';
        for (i = 0; i < tele_levels.length; i++) {
            telecommute_str += '&tele_level[]=' + tele_levels[i];
        }
        if (tele_levels.length == 1) {
            if (tele_levels[0] == 'Partial Telecommuting') {
                telecommute_str = '&tele_level[]=Partial Telecommuting&tele_level[]=All Telecommuting'
            }
            if (tele_levels[0] == 'Option for Telecommuting') {
                telecommute_str = '&tele_level[]=Option for Telecommuting&tele_level[]=Partial Telecommuting&tele_level[]=All Telecommuting'
            }
        }

        var sched_str = "";
        $("input:checkbox[name^=schedule]:checked").each(function() {
            sched_str = sched_str + "&schedule[]="+$(this).val();
        });

        var cats = $("#cats").val();
        var cat_str = "";
        // MT:  Temp removing per requirements.  Likely will come back.  
        // for (i = 0; i < cats.length; i++) {
        //   cat_str = cat_str + "&category[]="+cats[i];
        // }

        var job_search_situation = [];
        if ( $('div[why_flexjobs].checkbox.active').length > 0 ) {
          $('div[why_flexjobs].checkbox.active').each(function(_idx, elem){
            let strippedInnerText = elem?.innerText?.trim();
            job_search_situation.push(strippedInnerText);
          })
        }

        var search_url = fjHost + "/search?search="+$("#fq-search").val() +
            "&location="+$("input[name='zip']").val() +
            "&country="+$("select[name='country']").val() + sched_str + cat_str + telecommute_str + 
            "&search_type="+encodeURIComponent("personalized search") + 
            "&job_search_situation="+encodeURIComponent(job_search_situation.join(", "));

        $.ajax({
            url: fjHost + '/flexer_questionnaire',
            method: "POST",
            data: $("#fq-form :input").serialize(),
            complete: function(xhr) {
                $("#fq-form").html("<span class=\"spinner-border spinner-border-sm me-3\" role=\"status\" aria-hidden=\"true\"></span> Finding jobs now...");
                logFJEvent(EVENT_LOG_DEST.GOOGLE, 'click', { event_category: 'HPQuiz', event_label: 'completed' });
                logFJEvent(EVENT_LOG_DEST.AMPLITUDE, 'clk Quiz Done', amplitudeEventProps);

                setTimeout(function(){ window.location = search_url; }, 2000);
            }
        });
    });

    /* Step 2 - Begin */

    /* Step 2 - End */


    $("#fq-country").change(function() {
        if ($(this).val() != 'United States') {
            $("#fq-zip").val("");
            $("#fq-zip").attr('disabled', 'disabled');
        }
        else {
            $("#fq-zip").removeAttr('disabled', 'disabled');
        }
        enableSubmit();
    });

    $("#fq-zip").keyup(function() {
        if ($("#fq-country").val() != 'United States' || ($("#fq-country").val() == 'United States' && ($("#fq-zip").val().length == 5) || $("#fq-zip").val().length == 0)) {
            enableSubmit();
        } else {
            disableSubmit();
        }
    });

    function enableSubmit() {
        $("#fq-page-2").find(".next").removeAttr('disabled');
        $("#fq-ready").removeAttr('disabled');
    }

    function disableSubmit() {
        $("#fq-page-2").find(".next").attr('disabled', 'disabled');
        $("#fq-ready").attr('disabled', 'disabled');
    }

    $('#fq-page-0 [data-val]').on('click', function(e) {
        var why = $(this).data("val");
        if (why == "9" || why == 9) {
            $("[data-other-div]").toggleClass("hidden");
        }
        $(this).closest('div.checkbox').toggleClass('active');
        setTimeout(function() {
            if ($("#fq-page-0 input:checkbox:checked").length > 0) {
                $("#show-me-button").removeAttr('disabled');
            } else {
                $("#show-me-button").attr('disabled', 'disabled');
            }
        }, 100);
    });
});

</script>
  <script>
    $(document).ready(function() {
      $('#carouselExampleInterval .carousel-item').each(function(){
        var minPerSlide = 1;
        var next = $(this).next();
        if (!next.length) {
        next = $(this).siblings(':first');
        }
        next.children(':first-child').clone().appendTo($(this));
        
        for (var i=0;i<minPerSlide;i++) {
            next=next.next();
            if (!next.length) {
                next = $(this).siblings(':first');
            }
            
            next.children(':first-child').clone().appendTo($(this));
          }
      });
    });
  </script>



</body>



</html>
