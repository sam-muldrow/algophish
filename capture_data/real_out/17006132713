    <script>
  function addJs(e) {
    var t = document.createElement("script");
    return t.setAttribute("type", "text/javascript"),
            t.setAttribute("src", e),
            document.head.appendChild(t),
            t
  }

  var vstrScript = addJs('https://api-visitor.flexjobs.com/tracking-v8.js');
  vstrScript.onload = function () {
    var productCD = 'FJS';
    var portalCD = 'FXJ';
    var includeUserAgent = false;
    var includeGeolocation = true;
    var isRewriteConfigured = false;
    var cookiePath = '/';
    var isRewriteConfiguredWithPath = false;

    try  {
      TS.Track(productCD, portalCD, includeUserAgent, includeGeolocation, isRewriteConfigured, cookiePath, isRewriteConfiguredWithPath);
    } catch (err) {
      $.post(
        "/info/js_error", 
        {error: err, script: "BOLD visitor script"})
    }
  };
</script>

