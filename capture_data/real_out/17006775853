<script>
      (function () {
        var script = document.createElement('script');
        script.async = true;
        script.setAttribute('type', 'text/javascript');
        script.setAttribute('charset', 'UTF-8');
        const hostUrl = window.location.hostname;
        const port = window.location.port;
        const baseUrl = port ? `${hostUrl}:${port}` : `${hostUrl}`;
        var xhrobject = {};
        var xhr = new XMLHttpRequest();
        xhr.onreadystatechange = function () {
          if (xhr.readyState === 4) {
            if (xhr.status === 200) {
              const xhrprespose = xhr.response;
              const xhrstr = xhrprespose.split(' = ').pop();
              const xhrslice = xhrstr.replace(';', '');
              xhrobject = JSON.parse(xhrslice);
              if (xhrobject.onetrust_banner_script_url) {
                script.src = xhrobject.onetrust_banner_script_url;
                //test & production URL's will load dynamically based on flag set
                console.log('One Trust Banner Script Loaded');
              }
            }
            document.head.appendChild(script);
          } else {
            console.log('One Trust Banner Script Loading...');
          }
        };
        xhr.open('GET', `https://${baseUrl}/AppConfig.php`);
        xhr.send();
      })();
    </script>
