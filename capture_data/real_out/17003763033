    function gtmAddToCart(data){
        if(typeof(data)=='undefined' || typeof(data.name)=='undefined'){
            return true ;
        }
        if (data.cd64 === 'index_feed') {
            data.cd64 = '首页_精选_feed流_' + data.cd49;
        } else {
            data.cd64 = '';
        }
        //神策电商点击
        window.trackSaSdk(data);
        dataLayer.push({
            'event': 'addToCart',
            'ecommerce': {
                'currencyCode': 'CNY',
                'add': {
                    'products': [{
                        'name': data.name,
                        'id':  data.id,
                        'price': data.price,
                        'brand': data.brand,
