			(function(){
			    try {
					var url = 'https://search.hinet.net/islogin';
					$.ajax({
						type: 'GET',
						url: url,
						jsonpCallback: 'islogin',
						contentType: "application/json",
						dataType: 'jsonp',
						success: function(login) {
							if (login) {
			                    $('#memberLogin').hide();
			                    $('#memberLoginBtn').hide();
			                    $('#memberLogout').show();
			                    $('#memberLogoutBtn').show();
			                    $('#btnIcon').addClass("loginAfter");
			                }
						},
						error: function(e) {
							//console.log(e.message);
						}
					});
				} catch (exception) {}
			})();
