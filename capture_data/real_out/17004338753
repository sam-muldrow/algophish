  window.CHTChatOptions.businessId = 'FixedLine'
  window.CHTChatOptions.serviceId = 'HiNet'
  window.CHTChatOptions.fromSystemId = 'HiNet'
  window.CHTChatOptions.bottomOffset = '10'
