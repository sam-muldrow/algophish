<!--main js-->
<script type="text/javascript" src="resources2023/js/main.js"></script>
<script>
	var hinet_privacy  = $.cookie("hinet_privacy");
	if(hinet_privacy==null || hinet_privacy == ""){
		$("#privacy_div").show();
	};
	
	function acceptPrivacy(){
		$.cookie("hinet_privacy", "1", { expires: 365, path: '/' });
		$("#privacy_div").hide();
	}
	
  $(function () {
    // 修改小網滾軸色
    var m_functionColor01 = '#0162A9';
    var m_functionColor02 = '#FF395A';

    $('.m-fcntion-bg-colo-sy1 .imgbx , .m-fcntion-bg-colo-sy1 .swiper-scrollbar-drag').css('background-color', m_functionColor01)
    $('.m-fcntion-bg-colo-sy2 .imgbx , .m-fcntion-bg-colo-sy2 .swiper-scrollbar-drag').css('background-color', m_functionColor02)
    
  });

  function setTab(id){
		$("#header_nav0").hide();
		$("#header_nav1").hide();
		$("#header_nav"+id).show();
	}
  
  function setFrame(id){
  		$("#tab1").removeClass("on");
  		$("#tab2").removeClass("on");
  		$("#tab"+id).addClass("on");
	  	$("#frame1").hide();
	  	$("#frame2").hide();
	  	$("#frame"+id).show();
  }
  
  function searchRate(){
	  var speedQ1 = $("input[name='speedQ1']:checked").val();
	  var speedQ2 = $("input[name='speedQ2']:checked").val();
	  var speedQ3 = $("input[name='speedQ3']:checked").map(function() { return $(this).val(); }).get();
	  //alert("speedQ1:"+speedQ1+",speedQ2:"+speedQ2+",speedQ3:"+speedQ3.length)
	  if(speedQ1==undefined || speedQ2==undefined || speedQ3.length ==0){
		  alert("請填寫完整問題");
		  return false;
	  }
	  $("#rate").html();
	  //當Q2填寫「2小時以內」→查詢結果顯示「HiNet光世代 100M/40M」
	  if(speedQ2 == '1'){
		  $("#popupbox").show();
		  $("#rate").html("HiNet光世代 100M/40M");
		  return false;
	  }
	  //當Q1填寫「1~2個」且Q2「2小時到6小時」且Q3「勾選兩個選項以下(含)」→查詢結果顯示「HiNet光世代 100M/40M」
	  if(speedQ1 == '1' && speedQ2 == '2' && speedQ3.length <=2 ){
		  $("#popupbox").show();
		  $("#rate").html("HiNet光世代 100M/40M");
		  return false;
	  }
	  //當Q1填寫「3~4個」且Q2「2小時到6小時」且Q3「勾選兩個選項以下(含)」→查詢結果顯示「HiNet光世代 100M/40M」
	  if(speedQ1 == '2' && speedQ2 == '2' && speedQ3.length <=2 ){
		  $("#popupbox").show();
		  $("#rate").html("HiNet光世代 100M/40M");
		  return false;
	  }
	  //除了上述邏輯為「HiNet光世代 100M/40M」外，其餘對應速率皆顯示「HiNet光世代 300M/300M(含)以上」
	  $("#popupbox").show();
	  $("#rate").html("HiNet光世代 300M/300M(含)以上");
  }
  
  function changeRate(id){
	  
	  	$("#rate_"+'18a4021bb2e000007b06').hide();
	  	$("#rateTab_"+'18a4021bb2e000007b06').removeClass("on");
	  
	  	$("#rate_"+'18a4021e83d000003b2d').hide();
	  	$("#rateTab_"+'18a4021e83d000003b2d').removeClass("on");
	  
	  	$("#rate_"+'18a4022057b000008aa1').hide();
	  	$("#rateTab_"+'18a4022057b000008aa1').removeClass("on");
	  
	  	$("#rate_"+'18a402230c000000b1a4').hide();
	  	$("#rateTab_"+'18a402230c000000b1a4').removeClass("on");
	  
	  	$("#rate_"+'18a40224e2a00000742e').hide();
	  	$("#rateTab_"+'18a40224e2a00000742e').removeClass("on");
	  
	  	$("#rate_"+'18a40226c04000003f42').hide();
	  	$("#rateTab_"+'18a40226c04000003f42').removeClass("on");
	  
	  	$("#rate_"+'18a4022842400000975d').hide();
	  	$("#rateTab_"+'18a4022842400000975d').removeClass("on");
	  
	  	$("#rate_"+'18a40229dee00000091a').hide();
	  	$("#rateTab_"+'18a40229dee00000091a').removeClass("on");
	  
	  $("#rate_"+id).show();
	  $("#rateTab_"+id).addClass("on");
  }
  
  function locationUrl(){
	  var target_top = $("#searchBtn").offset().top;
	  var $body = (window.opera) ? (document.compatMode == "CSS1Compat" ? $('html') : $('body')) : $('html,body');
	  $body.animate({
    	 scrollTop: target_top
      }, 800);
	  $('#popupbox').hide();
  }
  
</script>
</body>
