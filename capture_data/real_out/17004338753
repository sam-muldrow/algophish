<script>
	var Req = {
		init: function(){
			if('' == location.search) return;
			var req = location.search;
			req = req.substring(1,req.length);
			req = req.split("&");
			for(var i in req){
				var sArr = req[i].split("=");
				if (null !== sArr[0]
					&& null !== sArr[1]
					&& "" !== sArr[0]
					&& "" !== sArr[1] )
					this[sArr[0]] = sArr[1];
			}
	    },
		get: function(key){
			if(undefined === this[key]) return null;
				return this[key];
			}
	};
	Req.init();
</script>
<script>
var cookieDomain = 'hinet.net';
var Config = {
	hisearchUrl: ''
};
function getAA(url,divId) {
    var formData = "";
	$.ajax({
		type: "POST",
        url: url,
        data: formData,
        dataType: 'jsonp',
        jsonp: 'callback',
       // jsonpCallback: 'moneyCallback',
        success: function( data, textStatus, jqXHR) {
        	if(data.TextContent!=undefined)
        	$("#"+divId).html("<a href='"+data.NavigateUrl+"' target='_target'><font color='red'>"+data.TextContent+"</font></a>");
        },
        error: function(jqXHR, textStatus, errorThrown){
//        	 console.log(textStatus);
        }
    });
}
</script>
