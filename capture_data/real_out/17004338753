<script type="text/javascript">
  window.RenderSection = {};
  window.RenderSection.setting = {};
  window.RenderSection.setting.Script = false;
  window.RenderSection.setting.Style = false;
</script>
<script type="text/javascript" src="resources2023/js/jquery.cookie.js"></script>
<script type="text/javascript" src="https://web-eshop.cdn.hinet.net/eshop/js/custom/renderSection.js"></script>
<!--swiper-->
<script src="resources2023/js/swiper-bundle.min.js"></script>
