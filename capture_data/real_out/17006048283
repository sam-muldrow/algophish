  display: inline-block;
  width: 12px;
  height: 12px;
  padding-left: 0.3em;
	padding-right: 0.1em;
  padding-bottom: 0.3em;
  margin-right: 0.2em;
  vertical-align: bottom;
  color: transparent;
  transition: .2s;
	margin-right:10px;
	vertical-align: top;
}

