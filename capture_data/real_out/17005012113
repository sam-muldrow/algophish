<script>
window.onscroll = function() {myFunction()};
 function myFunction()
 {
	var top = 1;
	if (document.body.scrollTop>=top  || document.documentElement.scrollTop >= top)
	{
		document.getElementById("scroll").className = "scrolled";
		document.getElementById("escroll").className = "escrolled";
		document.getElementById("logo").src = "./skins/default/images/logo-small.gif";
		
		
		document.getElementById('date').style.display='none';
		document.getElementById('date').style.visibility='hidden';
		
		document.getElementById('search').style.display='none';
		document.getElementById('search').style.visibility='hidden';
		document.getElementById("t").style.top = "40px";
	
		document.getElementById('livescore').style.display='block';
		document.getElementById('livescore').style.visibility='visible';

		document.getElementById('sendnews').style.display='block';
		document.getElementById('sendnews').style.visibility='visible';

		document.getElementById('reportage').style.display='block';
		document.getElementById('reportage').style.visibility='visible';
	
	}
	else
	{
		document.getElementById("scroll").className = "noscrolled";		
		document.getElementById("escroll").className = "enoscrolled";		
		document.getElementById("logo").src = "./skins/default/images/logo-large.gif";


		document.getElementById('date').style.display='block';
		document.getElementById('date').style.visibility='visible';
		
		document.getElementById('search').style.display='block';
		document.getElementById('search').style.visibility='visible';
		document.getElementById("t").style.top = "108px";

		document.getElementById('livescore').style.display='none';
		document.getElementById('livescore').style.visibility='hidden';

		document.getElementById('sendnews').style.display='none';
		document.getElementById('sendnews').style.visibility='hidden';

		document.getElementById('reportage').style.display='none';
		document.getElementById('reportage').style.visibility='hidden';

	}
 }
myFunction();

</script>


<script type="text/javascript">
	now = new Date();
	var head = document.getElementsByTagName('head')[0];
	var script = document.createElement('script');
	script.async = true;
	script.type = 'text/javascript';
	var script_address = './skins/default/js/common.js?id=8';
	script.src = script_address + '?v=' + now.getFullYear().toString() + '0' + now.getMonth() + '0' + now.getDate() + '0' + now.getHours();
	head.appendChild(script);
</script>



