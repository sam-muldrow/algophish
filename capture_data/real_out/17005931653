<script type="text/javascript">
    $('#search-input').keydown(function (e) {
        var value = $(this).val()
        if (e.keyCode == 13 && value) {
            window.open(
                'https://www.cmbc.com.cn/search/search.do?ty=&dr=true&w=&f=&tid=&q=' +
                    value
            )
        }
    })
		
