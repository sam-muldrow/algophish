    var tempHTML = {}
    $gw_poweb('QryShortTimeProductList', {}, function (res) {
        var list = res.prdList
        if (list.length > 0) {
            rows = chunkArray(
                list.filter(function (_, i) {
                    return i < 3
                }),
                2
            )
            var contentHtml = ''
            rows.forEach(function (row, index) {
                var rowHTML = '<div class="row">'
                var rowInner = ''
                row.forEach(function (item, index) {
                    var incomeInfo = getFinanceProductIncome(item)
                    var incomeArr = incomeInfo.incomeStr.split('#')
                    incomeArr.forEach(function (_, index) {
                        incomeArr[index] +=
                            '<span class="income-percentage">%</span>'
                    })

                    var incomeHTML = incomeArr.join('~')

                    var incomeLong = incomeArr.length > 1

                    var propsHTML = ''

                    var props = [
                        { 3: '1å¤©', 5: 'éç¨éå' }[item.prdType] ||
                            getPrdPeriod(item, true),
                        (parseFloat(item.pfirstAmt) >= 10000
                            ? parseInt(item.pfirstAmt) / 10000 + 'ä¸'
                            : item.pfirstAmt + 'å') + 'èµ·è´­',
                        getInterestDate(item),
                    ]

                    props.forEach(function (value, index) {
                        propsHTML += '<div>' + value + '</div>'
                    })

                    var tagsHTML = ''

                    var tags = getProductTags(item)

                    tags.forEach(function (value, index) {
                        tagsHTML += '<div>' + value.text + '</div>'
                    })

                    rowInner +=
                        ' <div class="col-xs-6">\
                        <div\
                            class="cmbc-site-finance-product-card clearfix hoverable" buy-code="' +
                        item.prdCode +
                        '"\
                        >\
                            <div class="cmbc-site-finance-product-card-left">\
                                <div class="cmbc-site-finance-product-card-code">\
                                    ' +
                        item.prdCode +
