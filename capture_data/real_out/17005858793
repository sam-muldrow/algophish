    lang: "en"
  };

  grecaptchaArr=[];

  var myCallBack = function () {
    var $WhichRecaptcha = '';

    $('.g-recaptcha').each(function (index, obj) {
      var $WhichRecaptcha = ($(this).attr('id'))? $(this).attr('id') : index;

      grecaptchaArr[$WhichRecaptcha] = 
        grecaptcha.render(obj, {
        'sitekey': '6Le1x48aAAAAAC52ITfIdIJwvHgiM4c4JFh5zaJF' //Replace this with your Site key
      });
    });
  };
</script> </body> </html> </th:block>