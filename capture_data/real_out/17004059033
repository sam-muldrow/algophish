	if ((window.location.href.indexOf('dhl.de') !== -1) 
	&& (window.location.href.indexOf('/impressum_dhlpaket.html') === -1)
	&& (window.location.href.indexOf('/datenschutz-gkp.html') === -1)
	&& (window.location.href.indexOf('/datenschutz.html') === -1)) {
        
