    <script type="text/javascript">
(() => {
    const verfolgen = '/de/privatkunden/pakete-empfangen/verfolgen.html';
    const urlParams = new URLSearchParams(window.location.search);
    
    if (urlParams.has('piececode')) {
        const piececode = urlParams.get('piececode');
        let redirectUrl = verfolgen + '?piececode=' + piececode;
        
        if (urlParams.has('cid')) {
            const cid = urlParams.get('cid');
            redirectUrl += '&cid=' + cid;
        }
        
        if (urlParams.has('tid')) {
            const tid = urlParams.get('tid');
            redirectUrl += '&tid=' + tid;
        }
		
		if (urlParams.has('verfuegen_selected_tab')) {
            const verfuegen_selected_tab = urlParams.get('verfuegen_selected_tab');
            redirectUrl += '&verfuegen_selected_tab=' + verfuegen_selected_tab;
        }
        
        window.location.replace(redirectUrl);
    }
})();
</script> 
<script>
    document.addEventListener("DOMContentLoaded", function(event) {
        function checkCookie() {
            const cookieName = "verfolgenHasShipments";
            const cookieValue = "true";
            let cookies = document.cookie.split(";");
            for (var i = 0; i < cookies.length; i++) {
                let cookie = cookies[i].trim();
                if (cookie.indexOf(cookieName + "=") === 0) {
                    let value = cookie.substring(cookieName.length + 1);
                    return value === cookieValue;
                }
            }
            return false;
        }
        if (checkCookie()) {
            const html = '<a href="/de/privatkunden/pakete-empfangen/verfolgen.html?tid=hp-se-sv" title="Zu meinen Sendungen" class="quick-nolp">Zu meinen Sendungen<span class="icon"><object class="quick-nolp-chevron" type="image/svg+xml" data="/.resources/dhl/webresources/assets/icons/sprite-dhl-v1-6.svg#chevronForward-bg--red" tabindex="-1"></object></span></a>';
            let items = document.querySelectorAll('.quickaccess__list .quickaccess-item');
            items.forEach((item) => {
                if (item.getAttribute('data-icon') == 'icon_paketsuche.svg') {
                    if (window.innerWidth < 768) {
                        document.querySelector('.quickaccess').insertAdjacentHTML('beforeend', html);
                    } else {
                        item.querySelector('.quickaccess-item__content').insertAdjacentHTML('beforeend', html);
                    }
                }
            });
        }
    })
</script>
