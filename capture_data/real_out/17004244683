			function setupPreConnect(){
    if (!window.ecosiapreconnected){
        const links = ['//prg.smartadserver.com','//shb.richaudience.com/hb/','//bidder.criteo.com','//ib.adnxs.com','//amazon-adsystem.com','//securepubads.g.doubleclick.net',];
        links.forEach( function(element) {
            var l = document.createElement('link');
            l.rel = 'preconnect';
            l.href = element;
            document.head.appendChild(l);
            console.log(l)

        });
        window.ecosiapreconnected = 1;
    }
}
setupPreConnect();

