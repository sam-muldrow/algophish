function downloadJSAtOnload() {
  // fADebug("<br/>execJSAtOnload");
  var lArr = ["https:\/\/www.malavida.com\/jsu_V38\/js-mv_util-mv_usr-nsmvsite-mv_box-mv_css_async.js"];
