    // import itrace libs
    function loadItraceAsync(itraceUrl, cbName) {
      if (!itraceUrl) {
        return;
      }
      var bodyEl = document.getElementsByTagName('body')[0];
      var script = document.createElement('script');
      script.crossOrigin = 'anonymous';
      script.src = itraceUrl;
      if (cbName) {
        script.onload = window[cbName];
      }
      bodyEl.appendChild(script);
    }
    loadItraceAsync("//g.lazcdn.com/g/woodpeckerx/itrace-next/??itrace-jserror.iife.js,itrace-interface.iife.js,itrace-perf.iife.js,itrace-flow.iife.js,itrace-blank.iife.js,itrace-resource.iife.js,itrace.iife.js", "__itraceBundleReady");
    loadItraceAsync("https://o.alicdn.com/lzdfe/lzd-h5-itrace/index.js", "__itraceExpansionReady");
  } else {
    // mock itrace接口
    // wpk api补偿机制
    var itraceApis = [
      'report',
      'reportError',
      'reportJSError',
      'reportApi',
      'reportApiError',
      'reportBlankPage',
      'reportPerf',
      'reportFlow',
      'setConfig',
    ];
    window.__wpk = {};
    itraceApis.forEach(function (apiName) {
      window.__wpk[apiName] = function () {};
    });
    itraceApis.forEach(function (apiName) {
      Object.defineProperty(window.__wpk, apiName, {
        get: function get() {
          return function () {
            if (!window['__iTraceLogQueue']) {
              window['__iTraceLogQueue'] = [];
            }
            if (apiName === 'reportError') {
              apiName = 'reportJSError';
            }
            if (apiName === 'reportApiError') {
              apiName = 'reportApi';
            }
            var item = [apiName].concat(Array.prototype.slice.call(arguments));
            window['__iTraceLogQueue'].push(item);
          };
        },
      });
    });
  
    // 拓展包自定义插件补偿
    !(function () {
      var expansionApi = [
        'iLoggerInit',
        'iLoggerMtopPluginInit',
        'iLoggerReportApi',
        'iLoggerSelfDefineFSP',
        'iLoggerIntegrate',
      ];
      window.iLogger = {};
      expansionApi.forEach(function (apiName) {
        window.iLogger[apiName] = function () {
          if (window.iLogger.isReady && typeof window.iLogger[apiName] === 'function') {
            // itraceExpansionReady后, window.iLogger会被重新赋值
            window.iLogger[apiName].apply(null, arguments);
          } else {
            if (!window['__iTraceExpansionQueue']) {
              window['__iTraceExpansionQueue'] = [];
            }
            var item = [apiName].concat(Array.prototype.slice.call(arguments));
            window['__iTraceExpansionQueue'].push(item);
          }
        };
      });
    })();
  }
