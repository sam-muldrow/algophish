window.onload = function() {
var str1 = document.URL;
var str2 = "checkout/cart";
if(str1.indexOf(str2) != -1){

$(document).ready(function() {



$('.popup').css( "display", "block" );




$('.close').click(function(){
$('.popup').hide();
});

});

}


var str4 = document.URL;
var str5 = "catalog";
//var str6 = "mini-business-card";
var myPopup = [];
myPopup['mini-business-cards']= 100;
myPopup['classic-business-cards']= 100;
myPopup['premium-business-cards']= 125;
myPopup['premium-vertical-business-card']= 125;
myPopup['rounded-corner-business-cards']= 175;
myPopup['square-business-cards']= 175;
myPopup['folded-business-cards']= 250;
myPopup['letterhead']= '';
myPopup['envelopes']= '';

var str6='';
var str7 = str4.split("/").pop().split('?');
str6=str7[0];
if((str4.indexOf(str5) != -1 ) && ((str4.indexOf('business-card')!= -1) || (str4.indexOf('letterhead')!= -1) || (str4.indexOf('envelopes')!= -1) || ((str4.indexOf('free-business-cards-1')= -1))){

$('.popup1').hide();
$(document).ready(function() {
$('#product-name-popup1').css('text-transform', 'capitalize');
$('#product-name-popup1').html(str6.replace(/-/g ,' '));



if((str4.indexOf('envelopes')!= -1)){
$('#product-offer-popup1').text('Flat 30% Off');
$('#product-price-popup1').html('100 Envelopes at Rs 1050 '+myPopup[str6]);
$('#product-price-popup1').css('font-size',19);
$('#product-offer-code-popup1').text('PRINT30');

}
else if ((str4.indexOf('letterhead')!= -1)){
$('#product-offer-popup1').text('To Get Free Shipping');
$('#product-price-popup1').html('Buy now '+myPopup[str6]);
$('#product-price-popup1').css('font-size',19);
$('#product-offer-code-popup1').text('SHIP');
}
else
{
$('#product-price-popup1').html('Buy now at Rs '+myPopup[str6]);
}

$('.popup1').show();

$('.popup1').css( "right", 515);




$('.close1').click(function(){
$('.popup1').hide();
});

});

}
else
$('.popup1').hide();

}
</script>

