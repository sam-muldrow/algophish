% auto;-webkit-background-position:center bottom;background-position:center bottom;}}@media (min-width: 980px){.us-eoy-thrasher-1yhjgzf-Component{width:calc(100% + 20px);-webkit-transform:none;-moz-transform:none;-ms-transform:none;transform:none;background-image:url("https://i.guim.co.uk/img/media/584e714b126b30b5af523360dc69cca2c1291f5e/1_0_2238_1344/2238.jpg?width=990&quality=90&s=0924facca7f5b1fb8fd9b4935f759aac");-webkit-background-position:center center;background-position:center center;}}</style><section class="us-eoy-thrasher-1yhjgzf-Component"></section></div></div></div></section></div><div><script>var tickerIdSelector = "US_EOY_2023_THRASHER_TICKER_01"; // make sure this matches the prop set in ThrasherTemplate.tsx

async function fetchTickerData() {
  var tickerEl = document.querySelector(`#${tickerIdSelector}`);
  if (!tickerEl) {
    return;
  }
  var tickerBarEl = tickerEl.querySelector(".ticker-bar");
  var tickerValuesContainerEl = tickerEl.querySelector("[data-goal_amount]");
  var tickerTotalEl = tickerEl.querySelector(".ticker-total-value");
  var tickerGoalEl = tickerEl.querySelector(".ticker-goal-value");
  var tickerDataUrl = tickerEl.dataset.ticker_data_url;
  var currencyCode = tickerEl.dataset.ticker_currency_code;
  if (
    !tickerDataUrl ||
    !tickerBarEl ||
    !tickerTotalEl ||
    !tickerGoalEl ||
    !currencyCode ||
    !tickerValuesContainerEl
  ) {
    return;
  }
  var data = await fetch(tickerDataUrl);
  var json = await data.json();
  if (typeof json.goal === "number" && typeof json.total === "number") {
    var goal = json.goal;
    var total = json.total;
    tickerBarEl.style.width = `${(total / goal) * 100}%`;
    tickerTotalEl.innerText = tickerValueCurrencyFormat(total, currencyCode);
    tickerGoalEl.innerText = tickerValueCurrencyFormat(goal, currencyCode);
    tickerValuesContainerEl.setAttribute("data-goal_amount", goal);
  }
}

function tickerValueCurrencyFormat(value, currencyCode) {
  return value.toLocaleString("en-US", {
    style: "currency",
    currency: currencyCode,
    minimumFractionDigits: 0,
  });
}

fetchTickerData().catch(console.error);
</script></div></div></section></div><span class="dcr-90inr0"><div class="ad-slot-container dcr-1sc2u5y"><div id="dfp-ad--merchandising-high" data-link-name="ad slot merchandising-high" data-name="merchandising-high" data-refresh="false" aria-hidden="true" class="js-ad-slot ad-slot ad-slot--merchandising-high dcr-zkf5jw"></div></div></span></div><section id="in-focus" data-link-name="container-4 | in-focus" data-component="in-focus" data-container-name="fixed/small/slow-III" class="dcr-1sz9f2"><div class="dcr-1cmtw8x"></div><div class="dcr-54985"><div class="dcr-15t9kli"><div class="dcr-1atljbz"><h2 class="dcr-eubdc">In focus</h2></div></div></div><div class="dcr-owfrse"><button data-link-name="Hide" data-show-hide-button="in-focus" aria-controls="in-focus" aria-expanded="true" class="dcr-18feyk8">Hide</button></div><div id="container-in-focus" class="dcr-1lex7d6"><ul class="dcr-b42ulr"><li class="dcr-1qmyfxi"><div data-format-theme="0" data-format-design="0" data-format-display="0" class="dcr-u5xuo1"><div class="dcr-le7ii1"><div class="dcr-12ilguo"><a href="/world/2023/nov/19/russia-ukraine-war-at-a-glance-what-we-know-on-day-634" data-link-name="news | group-0 | card-@1" aria-label="Russia-Ukraine war: what we know on day 634" class="dcr-lv2v9o"></a><div class="dcr-1a53cuj"><div class="dcr-1shzr3u"><picture data-size="medium" class="dcr-yosj9c"><source srcSet="https://i.guim.co.uk/img/media/319991f5497e5cd5560b08929712d52a61778368/0_364_5472_3283/master/5472.jpg?width=460&amp;dpr=2&amp;s=none" media="(min-width: 980px) and (-webkit-min-device-pixel-ratio: 1.25), (min-width: 980px) and (min-resolution: 120dpi)"/><source srcSet="https://i.guim.co.uk/img/media/319991f5497e5cd5560b08929712d52a61778368/0_364_5472_3283/master/5472.jpg?width=460&amp;dpr=1&amp;s=none" media="(min-width: 980px)"/><source srcSet="https://i.guim.co.uk/img/media/319991f5497e5cd5560b08929712d52a61778368/0_364_5472_3283/master/5472.jpg?width=330&amp;dpr=2&amp;s=none" media="(min-width: 740px) a