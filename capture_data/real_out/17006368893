window.homeEarlyJavascriptErrors = [];
window.onerror = function (errorMsg, url, lineNumber, column, errorObj) {
if (window.homeNewRelicReporter && typeof window.homeNewRelicReporter.addPageAction === 'function') {
window.homeNewRelicReporter.addPageAction('WindowError', {
"isEarlyError" : false,
"errorMessage" : errorMsg,
"errorStack" : errorObj ? errorObj.stack : 'Unknown'
});
} else {
window.homeEarlyJavascriptErrors.push({
message : errorMsg,
errorObject: errorObj
});
}
}
</script>
