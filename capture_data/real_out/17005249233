	window.addEventListener('message',function(e) {
		var key = e.message ? 'message' : 'data';
		var data = e[key];
		if (data == "closeIframeMiddle") {
			var catfishWrapper = document.getElementById("piano_catfish_wrapper_middle");	
		
			if (catfishWrapper)
				catfishWrapper.style.display = 'none';
		}
	},false);
</script>
