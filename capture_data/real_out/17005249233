<!-- Piano Catfish close -->
<script>
	window.addEventListener('message',function(e) {
		var key = e.message ? 'message' : 'data';
		var data = e[key];
		if (data == "closeIframe") {
			var catfishWrapper = document.getElementById("piano_catfish_wrapper");	
		
			if (catfishWrapper)
				catfishWrapper.style.display = 'none';
		}
	},false);
</script>
<!-- Piano Catfish close -->

