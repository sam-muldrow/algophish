                        var promoView = { promotions: [] };
                        var itemsList = [];
                        var category = '';
                        var formattedPrice = '';
                        var price = '';
                        banners.forEach(function(banner, i) {
                            if(i < 4){
                                category = juegosonce.Commons.productType(banner.codigoBannerSet);
                                promoView.promotions.push({
                                    'name': 'HP_Botes y premios',
                                    'creative': banner.tipo,
                                    'position': (i+1)
                                });
                                //Nueva Medición GTM GA4
                                price = banner.precio != null ? banner.precio : juegosonce.Commons.productPrice(banner.codigoBannerSet);
                                formattedPrice= juegosonce.Commons.formatPrices(""+price);
                                itemsList.push({
                                    'item_name': juegosonce.Commons.getGA4FormatProductName(banner.desProducto),
                                    'item_id': banner.codigoBannerSet,
                                    'price': formattedPrice,
                                    'item_category': category,
                                    'item_list_name': 'lista_HP_botes_y_premios',// Nombre del listado
                                    'index': (i+1) //Posición en el listado
                                });
                                
                            }
                        });
                        setTimeout(function(){
                            dataLayer.push({   
                                'event': 'eec',
                                   'eventCategory': 'Promocion_interna',
                                   'eventAction': 'HP_Botes y premios',
                                    'nonInteraction': true,
                                   'eventLabel': friendlyName,
                                'ecommerce' : {
                                    'promoView': promoView
                                }
                            });
                            //Nueva Medición GTM GA4
                            dataLayer.push({
                                'event': 'view_item_list',
                                 'item_list_id': 'botes_y_premios',
                                 'item_list_name': 'lista_HP_botes_y_premios',
                                 'user_id': '',
                                 'fecha_registro': '',
                                 'saldo' : '',
                                 'ecommerce' : {
                                     'items': itemsList
                                 }
                             });
                        }, 2000);

                        $('.botesypremios a').on('click', function(e) {
                            var $parent = $(this).closest('li');
                            var i = $('li.botesypremios').index($parent);
                            var banner = banners[i];
                            var promoClick = { promotions: [] };
                            var category = juegosonce.Commons.productType(banner.codigoBannerSet);
                            promoClick.promotions.push({
                                'name': 'HP_Botes y premios',
                                'creative': banner.tipo,
                                'position': (i + 1)
                            });
                            dataLayer.push({
                                'event': 'eec',
                                   'eventCategory': 'Promocion_interna',
                                   'eventAction': 'HP_Botes y premios',
                                   'eventLabel': friendlyName,
                                'ecommerce' : {
                                    'promoClick': promoClick
                                }
                            });
                            //Nueva Medición GTM GA4
                            var itemsList = [];
                            price = banner.precio != null ? banner.precio : juegosonce.Commons.productPrice(banner.codigoBannerSet);
                            formattedPrice= juegosonce.Commons.formatPrices(""+price);

                            itemsList.push({
                                'item_name': juegosonce.Commons.getGA4FormatProductName(banner.desProducto),
                                'item_id': banner.codigoBannerSet,
                                'price': formattedPrice,
                                'item_category': category,
                                'item_list_name': 'lista_HP_botes_y_premios',
                                'index': (i+1) 
                            });
                            dataLayer.push({
                                 'event': 'select_item',
                                 'user_id': '',
                                 'fecha_registro': '',
                                 'ecommerce' : {
                                     'items': itemsList
                                 }
                             });
                            //Se añade información de la lista en la que está el item en SessionStorage, para recuperarla en la página de info del propio item
                            juegosonce.Commons.setInSession("gtm_ga4_item_list", 'lista_HP_botes_y_premios');
                            juegosonce.Commons.setInSession("gtm_ga4_item_index", (i+1));
                            /**********************/
                            
                        });
                    });
            </script>
