        <script type="text/javascript">
            $(document).ready(function(){    
                var friendlyName = location.pathname.replace(juegosonce.contextPath, '').replace(/^\/?([^/?#:~]*)\/?.*/, '$1');
            	var banner = 'rasca-super-millones';
                var promoView = { promotions: [] };
                    promoView.promotions.push({
                       'name': 'HP_Modulo_Premiosrascas',
                       'creative': 'HP_Modulo_Premiosrascas',
                       'position':  3
                    });
                    setTimeout(function(){
                       dataLayer.push({
                           'event': 'eec',
                              'eventCategory': 'Promocion_interna',
                              'eventAction': 'HP_Modulo_Premiosrascas',
                              'nonInteraction': true,
                              'eventLabel': friendlyName,
                           'ecommerce' : {
                                'promoView': promoView
                            }
                        });
                    }, 2000);
                $('.enlacepremiorasca').on('click', function(e) {
                    var $parent = $(this).parent();
                    var promoClick = { promotions: [] };
                        promoClick.promotions.push({
                            'name': 'HP_Modulo_Premiosrascas',
                            'creative': banner,
                            'position': 3
                        });
                        dataLayer.push({
                            'event': 'eec',
                            'eventCategory': 'Promocion_interna',
                                  'eventAction': 'HP_Modulo_Premiosrascas',
                                  'eventLabel': friendlyName,
                            'ecommerce' : {
                                'promoClick': promoClick
                            }
                        });
                });
            });
        </script>
                
