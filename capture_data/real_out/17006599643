                        var tipoBanner = 'cuatrocolumnas';					
						var promoView = { promotions: [] };
                        var itemsList = [];
                        var category = '';    
                        var formattedPrice;						
						banners.forEach(function(banner, i) {	
							 var tipoBannerActual = banner.tipoBanners[tipoBanner];
							 var tipoBannerGif = banner.movil;
	                         var dateIni = new Date(banner.fechaIniComer);
	                         var dateFin = new Date(banner.fechaFinComer);
                         	 category = juegosonce.Commons.productType(banner.codigoBannerSet);
	                           if (banner.fechaIniComer && banner.fechaFinComer && tipoBannerActual && (dateIni < new Date() < dateFin)) {
									promoView.promotions.push({
										'name': 'HP_Modulo_Rascas',
										'creative': banner.codigoBannerSet,
										'position':  (i+1)
									});
	                                //Nueva Medición GTM GA4
	                                formattedPrice=juegosonce.Commons.formatPrices(banner.precio);
	                                itemsList.push({
	                             	   'item_name': juegosonce.Commons.getGA4FormatProductName(banner.desProducto),
	                             	   'item_id': banner.codigoBannerSet,
	                             	   'price': formattedPrice,
	                             	   'item_category': category,
	                             	   'item_list_name': 'lista_HP_modulo_rascas',// Nombre del listado
	                             	   'index': (i+1) //Posición en el listado
	                                });
									
	                           }
							
						});

						setTimeout(function(){
							dataLayer.push({
								'event': 'eec',
		   	                    'eventCategory': 'Promocion_interna',
		   	                    'eventAction': 'HP_Modulo_Rascas',
		   	                 	'nonInteraction': true,
		   	                    'eventLabel': friendlyName,
								'ecommerce' : {
									'promoView': promoView
								}
							});
	                        //Nueva Medición GTM GA4
	                        dataLayer.push({
	                            'event': 'view_item_list',
	                             'item_list_id': 'modulo_rascas',
	                             'item_list_name': 'lista_HP_modulo_rascas',    
	                             'user_id': '',
	                             'fecha_registro': '',   
	                             'saldo' : '',                                            
	                             'ecommerce' : {
	                                 'items': itemsList
	                             }
	                         });     							
						}, 2000);

						$('a.botonrascahome, a.informacionrascahome').on('click', function(e) {		
							var $parent = $(this).closest('li');
							var siblings = $parent.parent().children();
	                        var i = siblings.index($parent);
	                        var banner = banners[i];
							var promoClick = { promotions: [] };
                            var category = juegosonce.Commons.productType(banner.codigoBannerSet);
							promoClick.promotions.push({
								'name': 'HP_Modulo_Rascas',
								'creative': banner.codigoBannerSet,
								'position': (i+1)
							});
							dataLayer.push({
								'event': 'eec',
		   	                    'eventCategory': 'Promocion_interna',
		   	                    'eventAction': 'HP_Modulo_Rrascas',
		   	                    'eventLabel': friendlyName,
								'ecommerce' : {
									'promoClick': promoClick
								}
							});
							//Nueva Medición GTM GA4
							var itemsList = [];
							var formattedPrice=juegosonce.Commons.formatPrices(banner.precio);
                            itemsList.push({
                         	   'item_name': juegosonce.Commons.getGA4FormatProductName(banner.desProducto),
                         	   'item_id': banner.codigoBannerSet,
                         	   'price': formattedPrice,
                         	   'item_category': category,
                         	   'item_list_name': 'lista_HP_modulo_rascas',// Nombre del listado
                         	   'index': (i+1) //Posición en el listado
                            });											
                            dataLayer.push({
                                 'event': 'select_item',
                                 'user_id': '',
                                 'fecha_registro': '',   
                                 'ecommerce' : {
                                     'items': itemsList
                                 }
                             });		
                            //Se añade información de la lista en la que está el item en SessionStorage, para recuperarla en la página de info del propio item
                            juegosonce.Commons.setInSession("gtm_ga4_item_list", 'lista_HP_modulo_rascas');	
                            juegosonce.Commons.setInSession("gtm_ga4_item_index", (i+1));	    					
							/**********************/	                                            
							
						});
		            });
