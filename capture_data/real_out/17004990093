<script type="text/javascript">
	(function($){
		$(document).ready(function(){
		
			var $tabs = $(".sliderpic");
			$tabs.tabs().tabs("rotate", 7000, true);
			
			$(".ui-tabs-panel",$tabs).each(function(i){
				var totalSize = $(".ui-tabs-panel",$tabs).size() - 1;
				//console.log(totalSize);
				
				i != totalSize ? next = i + 2 :  next = 1;
				$(".next-tab",this).attr("rel",next);

				i != 0 ? prev = i : prev = totalSize + 1;
				$('.prev-tab',this).attr("rel", prev);
					
			});
				
			$('.next-tab, .prev-tab',$tabs).click(function() { 
				$tabs.tabs('select', $(this).attr("rel"));
				return false;
			});	   
		});
	})(jQuery);
</script>
	</div>

</div><!-- /.block -->
