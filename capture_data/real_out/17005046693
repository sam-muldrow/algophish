								for ( var j = 0; j < unique.length; j++ ) {
									if ( unique[ j ] == array[ i ] )
										continue arrayLoop;
								}
								unique.push( array[ i ] );
							}
							return unique.sort();
						};
						for ( var i = 0; i < useThese.length && i < 8; i++ ) {
							container.append( template( useThese[ i ] ) );
							ids.push( useThese[ i ].id );
							mmgWidget.user.addImpression( useThese[ i ].id );
						}
						ids = ArrayUnique( ids );
						ga( 'send', {
							hitType: 'event',
							eventCategory: "widget1",
							eventAction: 'impression',
							eventLabel: ids.join(),
							nonInteraction: true
						} );
						container.find('a').click( function() {
							mmgWidget.user.addClick( $(this).attr( 'data-postid' ) );
							ga( 'send', {
								hitType: 'event',
								eventCategory: $(this).attr( 'data-widgetid' ),
								eventAction: 'click',
								eventLabel: $(this).attr( 'data-postid' ),
								transport: 'beacon'
							} );
						});
					} );

						}
					} );
				})(jQuery);
			} );
		</script>
