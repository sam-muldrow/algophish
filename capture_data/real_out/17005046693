						if ( e.detail.slideshowObject.currentSlideIndex === 6 && ! trackingSet ) {

					var template = function( data ) {
						var html = '<div class="item column column-block">';
						html += '<a href="$$LINK$$?itm_source=bossip_content_widget&itm_medium=slideshow_widget&itm_campaign=content_widget" data-postid="$$ID$$" data-widgetid="widget1">';
