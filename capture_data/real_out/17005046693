						for ( var i = 0; i < count; i++ ) {

							if ( mmgWidget.user.hasClicked( posts[ i ].id ) > 0 )
								continue;

							useThese.push( posts[i] );
						}

						var top_row = useThese.splice( 0, 4).shuffle();
						var twoOrThree = Math.round( Math.random() + 3 );
						var bottom_row = useThese.splice( 0, twoOrThree ).shuffle();

						useThese = top_row.concat( bottom_row.concat( useThese ) );
