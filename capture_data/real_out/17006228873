	.mobilead {
  		display:block;
  		}

  	.desktopad
  		{
  		display:none !important;
  		}
	}

	    @media only screen and (max-width: 2600px)	and (min-width: 610PX)
    {
	.mobilead {
  	     display:none !important;
  		}

  	.desktopad
  		{
  		display:block;
