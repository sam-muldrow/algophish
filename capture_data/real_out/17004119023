</script>


<script>
$(document).ready(function(){
$('#loginPost').on('submit',function(e){
  e.preventDefault();

  var username = $("#username").val();
	var password = $("#password").val();
	var response = grecaptcha.getResponse(0);

	if(username.length == "") {

    Swal.fire({
      icon: 'warning',
      title: 'Oops...',
      text: 'Isi Email Sebagai Username Anda !'
    });

  } else if(password.length == "") {

    Swal.fire({
      icon: 'warning',
      title: 'Oops...',
      text: 'Isi Kata Sandi Anda Dengan Benar !'
    });

  }else if(!response) {

		Swal.fire({
      icon: 'warning',
      title: 'Oops...',
      text: 'Captcha Belum Benar !'
    });

	}else {

    $.ajax({

      url: "https://www.tempo.co/loginuserteco",
      type: "POST",
      data: {
          "username": username,
          "password": password,
					"g-recaptcha-response": response
      },

      success:function(response){

				console.log(response);

        if (response.return == true) {

          Swal.fire({
						icon: 'success',
            // title: 'Login Berhasil!',
						title: 'Loading...',
            // text: 'Loading...',
            text: response.msg,
            timer: 3000,
            showCancelButton: false,
            showConfirmButton: false
          })
          .then (function() {
            // window.location.href = "https://www.tempo.co";
            window.location.reload(1);
          });

        } else {

          Swal.fire({
						icon: 'warning',
            // title: 'Login Gagal!',
						title: response.msg,
            text: 'silahkan coba lagi!'
          })
					.then (function() {
            // window.location.href = "https://www.tempo.co";
            window.location.reload(1);
          });

        }

        console.log(response);

      },

      error:function(response){

          Swal.fire({
						icon: 'error',
            title: 'Opps!',
            text: 'server error!'
          });

          console.log(response);

      }

    });

  }

});

});

</script>


<script>
$(document).ready(function(){
$('#regisPost').on('submit',function(e){
  e.preventDefault();

  var nama = $("#nama").val();
	var email = $("#email").val();
	var atmail = email.indexOf("@");
	var dotmail = email.lastIndexOf(".");
	var password = $("#passuser").val();
	var repassword = $("#repassuser").val();
	var response = grecaptcha.getResponse(1);

	if(nama.length == "") {

    Swal.fire({
      icon: 'warning',
      title: 'Oops...',
      text: 'Nama Wajib Diisi !'
    });

  } else if(email.length == "") {

    Swal.fire({
      icon: 'warning',
      title: 'Oops...',
      text: 'Email Wajib Diisi !'
    });

  }else if(atmail<1 || dotmail<atmail+2 || dotmail+2>=email.length) {

    Swal.fire({
      icon: 'warning',
      title: 'Oops...',
      text: 'Isi Email Dengan Benar !'
    });

  }else if(password.length == "") {

    Swal.fire({
      icon: 'warning',
      title: 'Oops...',
      text: 'Kata Sandi Wajib Diisi !'
    });

  }else if(repassword.length == "") {

    Swal.fire({
      icon: 'warning',
      title: 'Oops...',
      text: 'Ulangi Kata Sandi Anda !'
    });

  }else if(password != repassword) {

    Swal.fire({
      icon: 'warning',
      title: 'Oops...',
      text: 'Kata Sandi Tidak Sama Dengan Kata Sandi Ulang !'
    });

  }else if(!response) {

		Swal.fire({
      icon: 'warning',
      title: 'Oops...',
      text: 'Captcha Belum Benar !'
    });

	}else {

    $.ajax({

      url: "https://www.tempo.co/registeruserteco",
      type: "POST",
      data: {
          "nama": nama,
					"email" : email,
					"password" : password,
          "repassword": repassword,
					"g-recaptcha": response
      },

			success:function(response){
				console.log(response);
        if (response.return == true) {

          Swal.fire({
            icon: 'success',
            title: 'Loading ...',
            // title: response.msg,
						timer: 3000,
            text: response.msg,
            // text: 'Loading ...',
						showCancelButton: false,
						showConfirmButton: false
          }).then (function() {
            // window.location.href = "https://www.tempo.co";
            window.location.reload(1);
          });

          // $("#nama").val('');
          // $("#email").val('');
          // $("#password").val('');

        } else {

          Swal.fire({
            icon: 'error',
            title: response.msg,
            text: 'silahkan coba lagi!'
          });

        }

        console.log(response);

      },

      error:function(response){
          Swal.fire({
            type: 'error',
            title: 'Opps!',
            text: 'server error!'
          });
      }



    });

  }

});

});

</script>










<script>
  $(document).ready(function() {
  $('.adv-cls').click(function () {
    $('.fix-bottom').hide();
    $('.footer').removeClass('w-fix-bottom');
  });
  $('.slider-sub-menu').slick({
    slidesToShow:7,
    slidesToScroll:7,
    arrows: true,
    autoplay: false,
    centerMode: false,
    variableWidth: true,
    slidesToScroll:3,
    infinite: false,
    adaptiveHeight: false,
    });
  });
</script>

