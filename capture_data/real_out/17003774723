  (function() {
      var p;
          if (!window.Promise || !window.CustomEvent || !window.fetch || !Element.prototype.closest) { p = '/etc.clientlibs/hp-com/clientlibs/clientlib-public/resources/js/polyfills/ie11.5c5f1f9a.js'; }
          else if (!window.URLSearchParams || !window.IntersectionObserver || !window.customElements || !window.ResizeObserver) { p = '/etc.clientlibs/hp-com/clientlibs/clientlib-public/resources/js/polyfills/edge-safari.5c5f1f9a.js'; }
