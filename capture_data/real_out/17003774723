<script>
    (function (customInfoBannerV2) {
        if (!customInfoBannerV2) return ;
        const c = function (v) {
            return '.c-custom-info-banner-v2' + v + ' ';
        };
        const container = customInfoBannerV2.querySelector(c('__container'));
        if (!container || !container.offsetParent) return ;
        if (window.matchMedia('(min-width: 1280px)').matches) {
            // setContentOffset() function
            let contentOffset = 0;
            let extraTitle = customInfoBannerV2.classList.contains('title-on-side');
            let title = customInfoBannerV2.querySelector(c('__header ') + c('__header--title-wrapper'));
            let subtitle = customInfoBannerV2.querySelector(c('__header ') + c('__header--subtitle-wrapper'));
            if (extraTitle && title) contentOffset += title.offsetHeight;
            if (subtitle) contentOffset += subtitle.offsetHeight;
            container.style.marginTop = -1 * contentOffset + 'px';
        }
    })(document.querySelector('hp-custom-info-banner-v2.idfd95e973cf93bda7ec8a0acdcf5ca8c9287ca13dd5ffc720e65412a44e37e2ff'));
</script>
</div>


