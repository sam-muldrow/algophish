            function closeDiv() {
                $(".sbox_2").css("display", "none");
                $("#overlay").css("display", "none");
            }
            //Õ¹Ê¾appÏÂÔØ¶þÎ¬Âë
            function showImage(){
                $(".jjwxc_download").css('display','block');
            }
            function hideImg(){
                $(".jjwxc_download").css('display','none');
            }
        </script>
