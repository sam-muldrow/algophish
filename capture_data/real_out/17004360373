        <script type="text/javascript">
            function onblock(id) {
                if ($.browser.msie&&(($.browser.version=="7.0")||($.browser.version=="6.0"))) {
                    leftPx = document.scrollLeft+$("#"+id).offset().left+0+"px";
                } else {
                    leftPx = $("#"+id).offset().left+150+"px";
                }
//                leftPx = $("#"+id).offset().left+0+"px";
                topPx = $("#"+id).offset().top+20+"px";
                $("#"+id+'_span').css("position", "absolute");
                $("#"+id+'_span').css("left", leftPx);
                $("#"+id+'_span').css("top", topPx);
                $("#"+id+'_span').show();
            }
            function ondisplay(id) {
                $("#"+id+'_span').hide();
            }
            $(function() {
                if (typeof (showTime)=='function') {
                    showTime();
                }
                $('.tab').hover(
                        function() {
                            var id = $(this).attr('rel');
                            var cid;
                            if (id>4) {
                                cid = id-4;
                            } else {
                                cid = id;
                            }
                            $('.contant'+cid).hide();
                            $('#url'+id).show();
                        },
                        function() {

                        }
                )
            })
        </script>
        <script language="javascript">
            function addfavor(url, title) {
                if (confirm("ÍøÕ¾Ãû³Æ£º"+title+"\nÍøÖ·£º"+url+"\nÈ·¶¨Ìí¼ÓÊÕ²Ø?")) {
                    var ua = navigator.userAgent.toLowerCase();
                    if (ua.indexOf("msie 8")>-1) {
                        external.AddToFavoritesBar(url, title, title);//IE8
                    } else {
                        try {
                            window.external.addFavorite(url, title);
                        } catch (e) {
                            try {
                                window.sidebar.addPanel(title, url, "");//firefox
                            } catch (e) {
                                alert("¼ÓÈëÊÕ²ØÊ§°Ü£¬ÇëÊ¹ÓÃCtrl+D½øÐÐÌí¼Ó");
                            }
                        }
                    }
                }
                return false;
            }
        </script>
        <script>
            /**
             * »ñÈ¡cookie
             * @param string cName cookei ¼üÃû
             */
            function getcookie(cName) {
                var search = cName+"=";
                var returnvalue = 0;
                if (document.cookie.length>0) {
                    offset = document.cookie.indexOf(search);
                    if (offset!=-1) {
                        offset += search.length;
                        end = document.cookie.indexOf(";", offset);
                        if (end==-1) {
                            end = document.cookie.length;
                        }
                        returnvalue = unescape(document.cookie.substring(offset, end));
                    }
                }
                return returnvalue;
            }
