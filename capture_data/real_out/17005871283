        <!-- BEGIN Redirect old post id anchors -->
        <script>
	var anchor = window.location.hash.match(/^#(post|p|msg|entry)(\d+)$/);
	if (anchor && anchor.length === 3) {
		var redirectAnchor = 'post-' + anchor[2];
		// if we have an anchor on the page, simply jump to the anchor
		if (document.getElementById(redirectAnchor) != null) {
			window.location.hash = redirectAnchor;
		} else {
			window.location = "/posts/" + anchor[2];
		}
	}
</script>
        <!-- END Redirect old post id anchors -->

        
            <script>
		window.addEventListener("message", (event) => {
			if (event.origin !== location.origin) {
				return;
			}

			if (event.data === "googleOneTapError") {
				XF.alert(XF.phrase("oops_we_ran_into_some_problems"));
			}
		}, false);

		window.onload = function() {
			adjustIframeVerticalPosition();
		}

		$(window).onPassive('scroll', adjustIframeVerticalPosition);

		function adjustIframeVerticalPosition() {
			if (window.innerWidth > 650) {
				let iframeElement = document.getElementById("google-one-tap-iframe");
				let header = document.getElementById("cali-header");
				let headerBannerHeight = document.getElementById("header-banner").offsetHeight;
				let scrollY = window.scrollY;
				if (scrollY < (headerBannerHeight - header.offsetHeight)) {
					iframeElement.style.position = "absolute";
					iframeElement.style.top = (header.offsetHeight + header.offsetTop) + "px";
				}
				else {
					iframeElement.style.position = "fixed";
					iframeElement.style.top = header.offsetHeight + "px";
				}

			}
		}
	</script>
        
        
