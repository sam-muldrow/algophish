    <script>
        function vsReadCookie(name) {
            let nameEQ = name + "=";
            let ca = document.cookie.split(";");
            let cValue = null;

            ca.forEach((entry) => {
            while (entry.charAt(0) === " ") {
                entry = entry.substring(1, entry.length);

                if (entry.indexOf(nameEQ) === 0) {
                cValue = entry.substring(nameEQ.length, entry.length);
                return;
                }
            }
            });
            return cValue;
        }
