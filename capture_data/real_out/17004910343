<script type="text/javascript">
window.redactiePraatQuery = "";

(function() {
  var now = new Date()
  var cookieSet = false;
  var timer = setTimeout(function() {
    console.warn("[revive-ads] no betting event received");

    if (window.gtag) {
      gtag("event", "missing_betting_event");
    }
  }, 10000);

  window.addEventListener("message", function(event) {
    var cookieSetBefore = cookieSet;

    if (event.data && event.data.event) {
      switch (event.data.event) {
        case "allowBet":
          console.log("[revive-ads] received allowBet event");
          setBettingCookie(true);

          if (!cookieSetBefore && window.gtag) {
            gtag("event", "betting_event", {
              label: "allow",
              took: ((new Date() - now) / 1000).toFixed(0)
            });
          }
          break;
        case "noBet":
          console.log("[revive-ads] received noBet event");

          if (!cookieSetBefore && window.gtag) {
            gtag("event", "betting_event", {
              label: "deny",
              took: ((new Date() - now) / 1000).toFixed(0)
            });
          }
          break;
      }
    }
  });

  function setBettingCookie() {
    if (timer) {
      clearTimeout(timer);
    }

    var img = document.createElement("img");
    img.src = "/btads/1.gif";

    document.body.appendChild(img);
    cookieSet = true;
  }
}())
</script>


  

