    <script>
        (function() {
            var intellimizeLoaded = false;
            var intellimizeLoadTimer;
            // This utility method will load the Intellimize snippet
            function loadIntellimize() {
                // Prevent multiple loads
                clearTimeout(intellimizeLoadTimer);
                if (intellimizeLoaded) return;
                intellimizeLoaded = true;
                // Load the snippet
                var intellimizeSnippet = document.createElement('script');
                intellimizeSnippet.type = 'text/javascript';
                intellimizeSnippet.async = true;
                intellimizeSnippet.src = 'https://cdn.intellimize.co/snippet/117871812.js';
                intellimizeSnippet.onerror = function() {
                    document.documentElement.className = document.documentElement.className.replace(RegExp(' ?anti-flicker'), '');
                }
                var s = document.getElementsByTagName('script')[0];
                s.parentNode.insertBefore(intellimizeSnippet, s);
            }

            // Check if the 6sense data already has been loaded for this visitor
            if (localStorage.getItem('_6senseCompanyDetails') === null) { // Data is not available.
                // 6sense data will be loaded by the 6sense script below and then the
                // `enableCompanyDetails` callback will be triggered when completed.
                // Add fallback here to try to load Intellimize after 3000ms if 6sense fails to load
                intellimizeLoadTimer = setTimeout(function() { loadIntellimize(); }, 3000);
            } else { // Data is available.
                // Load Intellimize right away
                loadIntellimize();
            }

            window._6si = window._6si || [];
            window._6si.push(['enableEventTracking', true]);
            window._6si.push(['setToken', '40ef6f675f9808f27d0ceae0f13492bc']);
            window._6si.push(['setEpsilonKey', '0aeb13286c250682fc1f48d9793fe71f7c685490']);
            window._6si.push(['setEndpoint', 'b.6sc.co']);
            window._6si.push(['enableCompanyDetails', true, loadIntellimize]);

            var gd = document.createElement('script');
            gd.type = 'text/javascript';
            gd.async = true;
            gd.src = '//j.6sc.co/6si.min.js';
            var s = document.getElementsByTagName('script')[0];
            s.parentNode.insertBefore(gd, s);
        })();
    </script>

