	<script type="text/javascript">
		var hijos = document.querySelectorAll("ol.carousel-indicators > li");
		
		for (var i = 0; i < hijos.length; i++) {
			var unHijo = hijos[i];
			unHijo.addEventListener("keyup", function(event){
				var hijo = event.target;
				if (event.key=="Enter"||event.key==" ") {
				   event.preventDefault();
				   hijo.click();
				}
			});
			unHijo.addEventListener("click", function(event){
				var hijo = event.target;
				if (event.which == 1) {
					hijo.blur();
				}
			});
		}
	
		var indicators = document.getElementsByClassName("carousel-control");
		for (var i = 0; i < indicators.length; i++) {
			var element = indicators[i];
			element.addEventListener("click", function(event){
				var elemento = event.target;
				if (event.which == 1) {
					elemento.blur();
				}
			});
		}
		
		var imgEnlaces = document.querySelectorAll("ul.carousel-inner > li.carousel-item > a");
		for (var i = 0; i < imgEnlaces.length; i++) {
			var imgEnlace = imgEnlaces[i];
			imgEnlace.addEventListener("click", function(event){
				var elemento = event.target;
				if (event.which == 1) {
					elemento.parentElement.blur();
				}
			});
		}
		
		var playPause = document.querySelectorAll("div.carousel-play-pause > span");
		for (var i = 0; i < playPause.length; i++) {
			var pP = playPause[i];
			pP.addEventListener("keyup", function(event){
				var pP = event.target;
				if (event.key=="Enter"||event.key==" ") {
				   event.preventDefault();
				   pP.click();
				}
			});
		}
		$(document).ready(function () {
			$('#carruselBootstrap-174496').carousel({
				interval: 5000
			});
			
				$('#playButton-174496').click(function () {
					$('#carruselBootstrap-174496').carousel('cycle');
					$('#playButton-174496').hide();
					$('#pauseButton-174496').show();
					$('#pauseButton-174496').focus();
				});
				$('#pauseButton-174496').click(function () {
					$('#carruselBootstrap-174496').carousel('pause');
					$('#pauseButton-174496').hide();
					$('#playButton-174496').show();
					$('#playButton-174496').focus();
				});
			
				$('#carruselBootstrap-174496').carousel('cycle');
				$('#playButton-174496').hide();
		});

		$(window).on("load",function() {
			$('body #wrapper .listaCarrusel .carousel-item').removeClass("d-block");
			$('body #wrapper .listaCarrusel .carousel-item').removeClass("invisible");

			/* Para las listas que no sean COVER vamos a fijar un alto fijo del carrusel en funcion del tamaño de la primera imagen */
				/* Nos guardamos el valor del alto y ancho de la primera imagen del carrusel */
				var widthPrimeraImagen= $("#carruselBootstrap-174496 .carousel-inner .carousel-item img").prop('naturalWidth');
				var heightPrimeraImagen= $("#carruselBootstrap-174496 .carousel-inner .carousel-item img").prop('naturalHeight');
				$("#carruselBootstrap-174496 .carousel-inner").attr("data-width", widthPrimeraImagen);
				$("#carruselBootstrap-174496 .carousel-inner").attr("data-height", heightPrimeraImagen);
				
				
				/* Llamamos a una función para que calcule el alto que debe tener el carrusel tomando como referencia las proporciones de la primera imagen */
				calculaAltoCarrusel174496();
				
				/* Cada vez que se produzca un cambio en el tamanyo de la ventana llamamos a la misma funcion */
				$(window).resize(function() {
					calculaAltoCarrusel174496();
				});
				
				/* Funcion que calcula el alto que debe tener el carrusel en funcion de las proporciones de la primera imagen y el ancho que se tiene en estos momentos */
				function calculaAltoCarrusel174496() {
					var widthActual= $("#carruselBootstrap-174496 .carousel-inner").width();
					var widthOriginal = $("#carruselBootstrap-174496 .carousel-inner").attr("data-width");
					var heightOriginal = $("#carruselBootstrap-174496 .carousel-inner").attr("data-height");
					//console.log(174496 + " - widthActual=" + widthActual);
					//console.log(174496 + " - widthOriginal=" + widthOriginal);
					//console.log(174496 + " - heightOriginal=" + heightOriginal);
					
					var heightActual = widthActual*heightOriginal/widthOriginal;
					//console.log(174496 + " - heightActual=" + heightActual);
					
					var cssAplicarCarrsuel = 'height: '+heightActual+'px !important; line-height: '+heightActual+'px; text-align: center; ' ;
					// var cssAplicarImage = 'width: auto;';
					var cssAplicarImage = 'object-fit: contain; height: 100% !important;' ;
					var cssAplicarCaption = 'line-height: initial;';
					//console.log(174496 + " - cssAplicarImage=" + cssAplicarImage);
					
					$("#carruselBootstrap-174496 .carousel-inner .carousel-item").css('cssText', cssAplicarCarrsuel);
					$("#carruselBootstrap-174496 .carousel-inner .carousel-item img").css('cssText', cssAplicarImage );
					$("#carruselBootstrap-174496 .carousel-inner .carousel-item .carousel-caption").css('cssText', cssAplicarCaption );

					/* Indicamos a cada imagen que como mucho van a poder tener como ancho el ancho original de la imagen */
					/*
					$('#carruselBootstrap-174496 .carousel-inner .carousel-item img').each( function(e) {
						var natWidth = $(this).get(0).naturalWidth;
						var natHeight = $(this).get(0).naturalHeight;
						var widthActual= $("#carruselBootstrap-174496 .carousel-inner").width();
						var widthProporcional= natWidth* heightActual / natHeight;
						var maxWidth = Math.min(natWidth,widthActual,widthProporcional);
						//console.log(174496 + " - natWidth=" + natWidth);
						//console.log(174496 + " - widthActual=" + widthActual);
						//console.log(174496 + " - widthProporcional=" + widthProporcional);
						$(this).css('max-width',maxWidth+'px');
					});				
					*/
				}
			
		});
	</script>

