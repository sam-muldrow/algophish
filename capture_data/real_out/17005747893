    var urlParams = new URLSearchParams(window.location.search);
    if(urlParams.has("codigo")){
        _es_gva_es_siac_portlet_SiacTaxonomiasPortlet_callServeResource(urlParams.get("codigo"));
    }

    $( "#nav-taxonomias" ).on("click", ".nav-elemento",function() {_es_gva_es_siac_portlet_SiacTaxonomiasPortlet_callServeResource($(this).attr("codigo")); });

    $( "#camino-migas-taxonomias" ).on("click", ".nav-elemento",function() {_es_gva_es_siac_portlet_SiacTaxonomiasPortlet_callServeResource($(this).attr("codigo")); });

    $( "#portlet_com_liferay_site_navigation_menu_web_portlet_SiteNavigationMenuPortlet_INSTANCE_navigation" ).on("click", ".nav-elemento",function() {_es_gva_es_siac_portlet_SiacTaxonomiasPortlet_callServeResource($(this).attr("codigo")); });

    var hoverTimeout = setTimeout(0);
    $(document).on({
        mouseenter: function () {
            clearTimeout(hoverTimeout);
            $(".mostrar-enlace-hover").text(window.location.href.split("?")[0] + "?codigo=" + $(this).attr("codigo"));
            $(".mostrar-enlace-hover").fadeIn(200);
        },
        mouseleave: function () {
            hoverTimeout = setTimeout(function() {
                $(".mostrar-enlace-hover").fadeOut(400);
            }, 600);
        }
    }, ".nav-elemento");

    function _es_gva_es_siac_portlet_SiacTaxonomiasPortlet_callServeResource(codigo){
        var url = "https://www.gva.es/ca/inicio/presentacion?p_p_id=es_gva_es_siac_portlet_SiacTaxonomiasPortlet&p_p_lifecycle=2&p_p_state=normal&p_p_mode=view&p_p_cacheability=cacheLevelPage" + "&codigo=" + codigo;
        $.ajax({
            url:             url,
            type:           "post",
            contentType:    "application/json",
            async:          true,
            beforeSend:     function(){         $(".loading-area").css("display", "flex");},
            success:        function(data){     processData(data, codigo); $(".loading-area").css("display", "none");},
            error:          function(){         console.log('Ha ocurrido un error mientras se procesaba la peticiÃ³n');}
        });
    }
    function processData(data, codigo){
        (window.location.href.split("?")[0]).slice(-1) == "/"? window.history.pushState("", "", (window.location.href.split("?")[0]).slice(0, -1) + "?codigo=" + codigo ) : window.history.pushState("", "", "?codigo=" + codigo );
        var jsonObj = jQuery.parseJSON(data.replace(/(\r\n|\n|\r)/gm, ""));
        for(i = 0; i < jsonObj.length; i++){
            var res   = jsonObj[i].resultado;
            var th    = res[0].taxonomiaHijas;
            var pr    = res[0].procedimientos;
            var tp    = res[0].taxonomiaPadres;
            var an    = res[0].arbolNavegacion;
        }
        pintaCaminoMigas(tp); pintaNavegacion(th, tp); pintaProcedimientos(pr); montaUrlCopy(); pintaArbolNavegacion(an, tp); cambiaTituloPortlet(tp); ocultaVisorContenidoWeb(tp);
    }

    if("false" == "true"){
        try{
            document.querySelector("#portlet_com_liferay_site_navigation_menu_web_portlet_SiteNavigationMenuPortlet_INSTANCE_navigation .portlet-body > div > ul").innerHTML = '<li><a href="/web/ciutadania?codigo=10492248">Busque treball</a></li><li><a href="/web/ciutadania?codigo=10492251">Vull informació sobre família</a></li><li><a href="/web/ciutadania?codigo=10492257">Necessite anar al metge</a></li><li><a href="/web/ciutadania?codigo=10492261">Vull accedir a una vivenda</a></li><li><a href="/web/ciutadania?codigo=10492265">Vull estudiar</a></li><li><a href="/web/ciutadania?codigo=104922418">Necessite informació tributària</a></li>';
        }catch(e){
            console.log("Para que el arbol de navegacion de taxonomias funcione es necesario tener el modulo de navegacion: " + e);
        }
    }
    function ocultaVisorContenidoWeb(tp){
        document.querySelector(".taxonomias-pagina-principal") ? Object.keys(tp).length > 2 ? document.querySelector(".taxonomias-pagina-principal").style.display = "none" : document.querySelector(".taxonomias-pagina-principal").style.display = "block"  : "";
    }
    function pintaArbolNavegacion(an,tp){
        if("false" == "true"){
            var tp = Object.values(tp).reverse().slice(2);
            var an = Object.values(an).reverse().slice(1);
            var elementosArbol= "";
            function pintaArbolRecursivo(an, tp, i){
                var eArbol = "";
                i < an.length && i < tp.length ? Object.values(an[i]).map(element => element.codigo !== tp[i].codigo ? eArbol += '<li><span class="nav-elemento" codigo="' + element.codigo + '">' + element.descripcion + '</span></li>' : eArbol += '<li><span style="font-weight:bold;" class="nav-elemento" codigo="' + element.codigo + '">' + element.descripcion + '</span><ul>' + pintaArbolRecursivo(an, tp, i+1) + '</ul></li>') : Object.values(an[i]).map(element => eArbol += '<li><span class="nav-elemento" codigo="' + element.codigo + '">'+element.descripcion+'</span></li>')
                return eArbol;
            }
            try{
                document.querySelector("#portlet_com_liferay_site_navigation_menu_web_portlet_SiteNavigationMenuPortlet_INSTANCE_navigation .portlet-body > div > ul").innerHTML = Object.values(an[0]).map(element =>
                    an.length > 1 ? 0 < tp.length && element.codigo !== tp[0].codigo ? elementosArbol = '<li><span class="nav-elemento" codigo="' + element.codigo + '">' + element.descripcion + '</span></li>' : elementosArbol = '<li><span style="font-weight:bold;" class="nav-elemento" codigo="' + element.codigo + '"> ' + element.descripcion + '</span><ul>' + pintaArbolRecursivo(an, tp, 1) + '</ul></li>' : elementosArbol = '<li><span class="nav-elemento" codigo="' + element.codigo + '">'+element.descripcion+'</span></li>'
                ).join('');
            }catch(e){
                console.log("Para que el arbol de navegacion de taxonomias funcione es necesario tener el modulo de navegacion: " + e);
            }
        }
    }
    function pintaCaminoMigas(tp){
        document.querySelector(".breadcrumb").innerHTML = Object.keys(tp).length !== 0 ? Object.values(tp).reverse().map((element,index) => {
            if (index==0){
                var title   = document.querySelector(".breadcrumb .breadcrumb-item:first-child a").getAttribute("title");
                var href    = document.querySelector(".breadcrumb .breadcrumb-item:first-child a").getAttribute("href");
                return '<li class="breadcrumb-item"><a class="breadcrumb-link" href="' + href + '" title="'+ title +'"><span class="breadcrumb-text-truncate">' + title +'</span></a></li>'
            }else{
                return '<li class="breadcrumb-item" codigo="' + element.codigo + '"><a class="breadcrumb-link" href="https://www.gva.es/ca/inicio/presentacion?codigo=' + element.codigo + '" title="' + element.descripcion + '"><span class="breadcrumb-text-truncate">' + element.descripcion + '</span></a></li>'
            }
        }).join('') : "" ;
        "false" == "true" ? document.querySelector("#camino-migas-taxonomias").innerHTML = Object.keys(tp).length !== 0 ? Object.values(tp).reverse().map((element,index) => index > 0 ? '<li class="breadcrumb-item nav-elemento" codigo="' + element.codigo + '" title="' + element.descripcion + '">' + element.descripcion + '</li>' : '' ).join('') : "" :"";
    }
    function pintaProcedimientos(pr){
        if("false" == "true"){
            document.getElementById("listado-procedimientos").innerHTML = Object.keys(pr).length !== 0 ? Object.values(pr).map(element => {
                switch (element.subtipo){
                    case "ENLACE_INFORMATIVO":  return '<li><a href="'+element.enlace+'" target="_blank">'+element.descripcion.replace(/\n|\r/g, '')+'</a></li>';                                                   break;
                    case "PROCEDIMIENTO":       return '<li><a href="https://www.gva.es/inicio/procedimientos?id_proc=?id_proc='+element.codigo+'">'+element.descripcion.replace(/\n|\r/g, '')+'</a></li>';  break;
                    default:                    return '<li><a href="https://www.gva.es/inicio/procedimientos?id_proc=?id_proc='+element.codigo+'">'+element.descripcion.replace(/\n|\r/g, '')+'</a></li>';  break;
                }
            }).join('') : '';
        }
    }
    function pintaNavegacion(th, tp){
        var first = Object.values(tp)[0];
        document.getElementById("nav-taxonomias").innerHTML = Object.keys(th).length !== 0 ? Object.values(th).map(element => '<li><span style="'+ (first.codigo==element.codigo ? 'font-weight:bold;': '' )+ '" class="nav-elemento" codigo="' + element.codigo + '">' + element.descripcion + '</span></li>' ).join('') : '' ;
    }
    function montaUrlCopy(){
        if("false" == "true"){
            document.getElementById('url-copy').innerText = window.location.href;
        }
    }
    function cambiaTituloPortlet(tp){
        document.querySelector("#portlet_es_gva_es_siac_portlet_SiacTaxonomiasPortlet h2.portlet-title-text").innerHTML = Object.values(tp)[0].descripcion;
    }

	
