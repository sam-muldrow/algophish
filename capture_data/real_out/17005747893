<section class="portlet" id="portlet_com_liferay_site_navigation_language_web_portlet_SiteNavigationLanguagePortlet_INSTANCE_idiomas">


	<div class="portlet-content">
	

		
			<div class=" portlet-content-container">
				


	<div class="portlet-body">



	
		
			
				
					



















































	

				

				
					
						


	

		



































































	

	<div class="idiomas-texto">
    
<span class="idiomas-texto-corto Valencià" lang="ca-ES" > Val</span>
<a href="/ca/c/portal/update_language?p_l_id=18433&amp;redirect=%2Fca%2Finicio%2Fpresentacion&amp;languageId=es_ES" class="idiomas-texto-corto Castellà" lang="es-ES" > Cas</a>
<a href="/ca/c/portal/update_language?p_l_id=18433&amp;redirect=%2Fca%2Finicio%2Fpresentacion&amp;languageId=en_US" class="idiomas-texto-corto english" lang="en-US" > eng</a></div>
<div class="idioma-menu">
    <div id="cabecera-idiomas"><span class="info-idioma"><i aria-hidden="true" class="fa-solid fa-earth-europe"></i></span><button aria-expanded="false" class="idioma-actual"></button></div>
    <div id="menu-opciones" class="opciones">
        <ul class="level-1">
        </ul>
    </div>
</div>

<script>

    /* Este codigo pinta el html del menu desplegable de idiomas en version movil.
        Los valores necesarios se recogen del html pintado del portlet de idiomas texto corto.*/
    //Se obtiene el idioma actual.
    var idiomaActual = $(".idiomas-texto span.idiomas-texto-corto")[0].classList[1];
    //Se obtiene el numero total de idiomas que estan activos.
    var numElementosTotal = $(".idiomas-texto > .idiomas-texto-corto").length;
    //Se declara array con todos los idiomas menos el actual.
    var arrayIdiomas = [];
    //Se declara array con los enlaces correspondientes a los idiomas del array anterior.
    var arrayEnlaces = [];
    //Se recorren todos los elementos "a" que son hijos de la clase "idiomasTexto".
    //Se añade a los arrays creados el texto (el idioma) y el valor
    //del atributo href.
    $(".idiomas-texto-corto").each(function(index){
        const [idioma] = $(this);
        const language = idioma.classList[1];
        arrayIdiomas.push(language);
        arrayEnlaces.push($(this).attr("href"));
    });

    for(i=0; i<=numElementosTotal; i++){
        let idioma = arrayIdiomas[i];
        if(idioma === idiomaActual){
            //Se pinta el idioma actual.
            $(".idioma-actual").append(idiomaActual + '<i aria-hidden="true" class="fa fa-caret-down"></i><i aria-hidden="true" class="fa fa-caret-up hide"></i>');
        }else if (idioma){
            //Se pinta los idiomas plegados.
            $(".opciones .level-1").append('<li class="nav-item"><a class="idioma-plegado'+i+'"> </a></li>');
            $(".idioma-plegado"+i).text(arrayIdiomas[i]);
            $(".idioma-plegado"+i).attr("href",arrayEnlaces[i]);
        }
    }
</script>


	
	
					
				
			
		
	
	


	</div>

			</div>
		
	</div>
</section>
	

		
		







