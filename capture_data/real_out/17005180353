        function topMao(target) {
            $('html, body').animate({ scrollTop: $(target).offset().top - 60 }, 300);//130ä¸ºéç¹å°è·é¡¶é¨çè·ç¦»ï¼500ä¸ºæ§è¡æ¶é´
            return false;
        }
        function topMaoSet(target) {
            $('html, body').animate({ scrollTop: 0 }, 300);//130ä¸ºéç¹å°è·é¡¶é¨çè·ç¦»ï¼500ä¸ºæ§è¡æ¶é´
            return false;
        }


        function sub(str, n) {
            var r = /[^\x00-\xff]/g;
            if (str.replace(r, "**").length <= n) { return str; }
            var m = Math.floor(n / 2);
            for (var i = m; i < str.length; i++) {
                if (str.substr(0, i).replace(r, "**").length >= n) {
                    return str.substr(0, i) + "...";
                }
            }
            return str;
        }

