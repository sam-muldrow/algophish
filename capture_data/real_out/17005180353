                            //            $('#living_con').attr('href', 'http://online.gongkong.com/ShowProseminfo/' + live.Id + '.html')
                            //        }
                            //    }
                            //}
                        }
                    })
                    $('.close-v').click(function () {
                        $('#living_con').remove();
                        $(this).remove();
                    })
                </script>



