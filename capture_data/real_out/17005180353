                <script type="text/javascript">
                    function htmlReg(msg) {
                        var msg = msg.replace(/<[^>]+>|&[^>]+;/g, ''); //å»é¤HTML Tag
                        msg = msg.replace(/[|]*\n/, '') //å»é¤è¡å°¾ç©ºæ ¼
                        msg = msg.replace(/&npsp;/ig, ''); //å»ænpsp
                        return msg;
                    }
                    $.ajax({
                        type: 'get',
                        url: 'http://online.gongkong.com/api2021/GetTodayProseminarList',
                        success: function (res) {
                            var liveres = res.data.Live;
                            //å½å¤©æä¼è®®
                            liveres.map(live => {
                                console.log('live', live)
                                if (live.OnlineType == "1" || live.OnlineType == "6") {
                                    var livedec = htmlReg(live.Description)
                                    var vsrcarr = live.OnlineProsemUser;  //åå®¾ä¿¡æ¯
                                    var videosrc = vsrcarr[0].Url; //è§é¢å°å
                                    $('#live-video').attr('src', videosrc);
                                    var start_time = new Date(live.StartDate); //å¼å§æ¶é´
                                    var end_time = new Date(live.EndTime);  //ç»ææ¶é´
                                    var strat_tamp = parseInt(new Date(start_time).getTime() / 1000);  //è§é¢å¼å§æ¶é´
                                    var end_tamp = parseInt(new Date(end_time).getTime() / 1000); //ä¼è®®ç»ææ¶é´
                                    var now_tamp = parseInt(new Date().getTime() / 1000);  //å½åæ¶é´
                                    var playTime_tamp = now_tamp - strat_tamp; //å·²ç»æ­æ¾çæ¶é´(ç§)
                                    var checkend_time = end_tamp - now_tamp;  //è¿å©å¤å°ç§æ­å®(ç§)
                                    if (playTime_tamp > 0 && checkend_time > 0) {
                                        console.log('å¨æ¶é´å')
                                        $('#living_con').show();
                                        $('.close-v').show();
                                        document.getElementById("live-video").currentTime = playTime_tamp;
                                        $('.live-title').text(live.Title)
                                        $('.live-time').text('ç´æ­æ¶é´ï¼' + live.StartFormatDate);
                                        $('.dec-con').html(livedec)
                                        $('#living_con').attr('href', 'http://online.gongkong.com/ShowProseminfo/' + live.Id + '.html')
                                    }
                                }
                                return;
                            })


                            //å½å¤©æä¼è®®
                            //if (liveres[0] != undefined) {
                            //    var live = liveres[0];
                            //    var livedec = htmlReg(live.Description)
                            //    if (live.OnlineType == "1" || live.OnlineType == "6") {
                            //        var vsrcarr = live.OnlineProsemUser;  //åå®¾ä¿¡æ¯
