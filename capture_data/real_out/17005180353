            <div class="news-view news-view-select">
                <!--7ç±»æ°é»èµè®¯-->
                <script type="text/javascript">
    GK.fingerprint(function (guid) {
        $.post('//s.gongkong.com/webapi/GetUserEsData',
           {
               guid: guid,
               ty: 'news',
               top: 10
           }, function (data) {
               var newshtml = "";
               var rootList = data.root.list.slice(0, 12);
               for (var i = 0; i < rootList.length;i++)
               {
                   if (i == 5) {
                       break;
                   }
                   newshtml += "<a href=\"" + rootList[i].Source.Url + "\" target=\"_blank\"><div class=\"recommend-box\">";
                   if (rootList[i].Source.ImgUrl != "") {
                       newshtml += "<div style=\"display:flex;flex-direction:column;justify-content:space-between;height:80px;margin-right:10px;max-width:460px;\">";
                   } else {
                       newshtml += "<div style=\"display:flex;flex-direction:column;justify-content:space-between;height:80px;margin-right:10px\">";
                   }

                   newshtml += "<div class=\"recommend-title\">" + rootList[i].Source.Title + "</div>";
                   
                   newshtml += "<div class=\"recommend-text\">";
                   if (rootList[i].Source.ImgUrl != "") {
                       //newshtml += sub(rootList[i].Source.Content, 100);
                       newshtml += rootList[i].Source.Content.length > 100 ? rootList[i].Source.Content.substring(0, 100) : rootList[i].Source.Content;
                   } else {
                       //newshtml += sub(rootList[i].Source.Content, 160);
                       newshtml += rootList[i].Source.Content.length > 160 ? rootList[i].Source.Content.substring(0, 160) : rootList[i].Source.Content;
                   }

                   newshtml+="</div>";
                   newshtml += "<div class=\"recommend-tagbox\">";
                   if (rootList[i].Source.KeyWord!="")
                   {
                       var commaSplite = rootList[i].Source.KeyWord.split(',');
                       if(commaSplite.length>0)
                       {
                           for(var j=0;j<commaSplite.length;j++)
                           {
                               newshtml += "<div class=\"recommend-tag\"><a href=\"/tags/" + commaSplite[j] + "\" target=\"_blank\">" + commaSplite[j] + "</a></div>";
                           }
                       } else {
                           var spaceSplite = rootList[i].Source.KeyWord.split(' ');
                           if(spaceSplite.length>0)
                           {
                               for (var j = 0; j < spaceSplite.length; j++) {
                                   newshtml += "<div class=\"recommend-tag\"><a href=\"/tags/" + spaceSplite[j] + "\" target=\"_blank\">" + spaceSplite[j] + "</a></div>";
                               }
                           }
                       }
                   }
                   
                   newshtml+="</div>";
                   newshtml += "</div>";
                   if (rootList[i].Source.ImgUrl != "")
                   {
                       newshtml += "<div class=\"recommend-img\">";
                       newshtml += "<img src=\"" + rootList[i].Source.ImgUrl + "\" />";
                       newshtml += "</div>";
                   }

                   newshtml+="</div></a>";

                   console.log(rootList[i].Source.Url)
               }
               $("#divNewsCenterIntelligentRecommendation").html(newshtml);
           });
    });
</script>
