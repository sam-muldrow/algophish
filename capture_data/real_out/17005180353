            stopOnLastSlide: false,
            disableOnInteraction: false,
            grabCursor: true,
            parallax: true,
        },
    })

    annualMeetingSwiper.el.onmouseover = function () { //é¼ æ æ¾ä¸æåè½®æ­
        annualMeetingSwiper.autoplay.stop();
    }
    annualMeetingSwiper.el.onmouseleave = function () {
        annualMeetingSwiper.autoplay.start();
    }

</script>
