<script type="text/javascript">
    function bar1190_show_r() {
        setTimeout(function(){
            document.getElementById('Bar1190_big_r').style.display = '';
            document.getElementById('Bar1190_small_r').style.display = 'none';
        },100)
    }
    function bar1190_hidden_r() {
        setTimeout(function(){
            document.getElementById('Bar1190_big_r').style.display = 'none';
            document.getElementById('Bar1190_small_r').style.display = '';
        },100)
    }

    function bar1190_show() {
        setTimeout(function(){
            document.getElementById('Bar1190_big').style.display = '';
            document.getElementById('Bar1190_small').style.display = 'none';
        },100)
    }
    function bar1190_hidden() {
        setTimeout(function(){
            document.getElementById('Bar1190_big').style.display = 'none';
            document.getElementById('Bar1190_small').style.display = '';
        },100)



    }


    $(window).resize(function(){
        var windoWidth = $('body').width();
        //   console.log(windoWidh);
        if (windoWidth < 1360) {
            bar1190_hidden();
            bar1190_hidden_r();
        } else {
            bar1190_show();
            bar1190_show_r();
        }
    })
    var windoWidth = $('body').width();

    //   console.log(windoWidh);
    if (windoWidth < 1360) {
        bar1190_hidden();
        bar1190_hidden_r();
    } else {
        bar1190_show();
        bar1190_show_r();
    }


</script>

