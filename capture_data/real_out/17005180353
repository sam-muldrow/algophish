        <script type="text/javascript">
            var popvideo = document.getElementById('popvideo')
            $('.popsee').click(function () {
                var vurl = $(this).find('video').attr('src');
                $('.vpop video').attr('src', vurl);
                $('.vpop').show()
                $('.end-item').removeClass('act')
                popvideo.play()
            })
            $('.gb-popv').click(function () {
                $('.vpop').hide()
                popvideo.pause()
            })
            popvideo.addEventListener('ended', function () {
                // æ§è¡æä½
                $('.end-item').addClass('act')
            });

            $('.endshow').click(function () {
                var vsrc = $(this).attr('data-src');
                $('.vpop video').attr('src', vsrc);
                $('.end-item').removeClass('act')
                popvideo.play()
            })
        </script>
