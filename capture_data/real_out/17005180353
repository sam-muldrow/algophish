            <div class="news-view technology-center ey-margin-top-10">
                <!--ææ¯ä¸­å¿-->
                <script type="text/javascript">
    GK.fingerprint(function (guid) {
        console.log("guid:"+guid)
        $.post('//s.gongkong.com/webapi/GetUserEsData',
           {
               guid: guid,
               ty: 'technicalpaper',
               top: 10
           }, function (data) {
               var newshtml = "";
               var rootList = data.root.list.slice(0, 12);
               for (var i = 0; i < rootList.length; i++) {
                   if (i == 3)
                   {
                       break;
                   }
                   newshtml += "<a href=\"" + rootList[i].Source.Url + "\" target=\"_blank\"><div class=\"recommend-box\" style=\"padding:20px 10px 20px 10px;\">";
                   if (rootList[i].Source.ImgUrl != "") {
                       newshtml += "     <div style=\"display:flex;flex-direction:column;justify-content:space-between;height:80px;margin-right:10px;max-width:480px;\">";
                   } else {
                       newshtml += "     <div style=\"display:flex;flex-direction:column;justify-content:space-between;height:80px;margin-right:10px;\">";
                   }

                   newshtml += "         <div class=\"recommend-title\">" + rootList[i].Source.Title + "</div>";
                   newshtml += "         <div class=\"recommend-text\">";
                   if (rootList[i].Source.ImgUrl != "") {
                       newshtml += sub(rootList[i].Source.Content, 160);
                   } else {
                       newshtml += sub(rootList[i].Source.Content, 200);
                   }
                   newshtml+="         </div>";
                   newshtml+="         <div class=\"recommend-tagbox\">";
                   if (rootList[i].Source.KeyWord != "") {
                       var commaSplite = rootList[i].Source.KeyWord.split(',');
                       if (commaSplite.length > 0) {
                           for (var j = 0; j < commaSplite.length; j++) {
                               newshtml += "<div class=\"recommend-tag technology\"><a href=\"/tags/" + commaSplite[j] + "\" target=\"_blank\">" + commaSplite[j] + "</a></div>";
                           }
                       } else {
                           var spaceSplite = rootList[i].Source.KeyWord.split(' ');
                           if (spaceSplite.length > 0) {
                               for (var j = 0; j < spaceSplite.length; j++) {
                                   newshtml += "<div class=\"recommend-tag technology\"><a href=\"/tags/" + spaceSplite[j] + "\" target=\"_blank\">" + spaceSplite[j] + "</a></div>";
                               }
                           }
                       }
                   }
                   newshtml+="         </div>";
                   newshtml += "     </div>";
                   if (rootList[i].Source.ImgUrl != "") {
                       newshtml += "     <div class=\"recommend-img\">";
                       newshtml += "         <img src=\"" + rootList[i].Source.ImgUrl + "\" />";
                       newshtml += "     </div>";
                   }
                   newshtml+=" </div></a>";
               }
               $("#divTechnicalPaperCenterIntelligentRecommendation").html(newshtml);
           });
    });
</script>

