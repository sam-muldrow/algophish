<script>
    $(document).ready(function () {
        $('nav li.menuItem').hover(
            function () {
                $('.submenu').stop().fadeOut(50);
                $(this).children('.submenu').fadeIn(350);
            },
            function () {
                $(this).children('.submenu').fadeOut(50);
            }
        );

        $(".menu-button").on("click", function () {
            if ((!$('.mobile-menu--all').hasClass('in')) && (!$('.black-background').hasClass('active-menu'))) {
                $('#main').addClass('active-menu');
                $('body').css("position", "fixed");
                $('.dropdown.open').removeClass('open');

            } else {
                $('#main').removeClass('active-menu');
                $('body').css('position', 'static');
            }
        });

        $('.close-menu').on('click', function (e) {
            $('.collapse').collapse('hide');
            $('#main').removeClass('active-menu');
            e.preventDefault();
        });

        $('#main').on('click', function (e) {
            $('.collapse').collapse('hide');
            $('#main').removeClass('active-menu');
        });
        $('ul.dropdown-menu [data-toggle=dropdown]').on('click', function (event) {
            event.preventDefault();
            event.stopPropagation();
            $(this).closest('li.dropdown').toggleClass('open');
        });


        $('.top-level').on('click', function (e) {

            if ((($(window).width() > 767) || ($(window).width() < 1023)) && ($(this).next('.submenu').length > 0)) {
                // e.preventDefault();
            }
        });
        $('.has-second-level>a').on('click', function (e) {
            if ((($(window).width() > 767) || ($(window).width() < 1023))) {
                // e.preventDefault();
            }
        });
        $(document).on('click', 'span.dropdown-toggle', function() {
            console.log('klik');
            $(this).closest('li.dropdown').toggleClass('open');
        });
    });
</script></header>
<section class="quote-mobile visible-xs">
<div class="container">
<div class="row">
