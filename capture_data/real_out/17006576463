<script type="text/javascript">
    //<![CDATA[
    $(function () {
        showNewsBox();

        setInterval(function() {
            checkLive()
        }, 180000);

        $(document).on('click', 'section.recommended .tableWrapper .button', function () {
            $("section.recommended .tableWrapper .button").removeClass('active');
            $(this).addClass('active');
            $("section.recommended .sliderBox").removeClass('active');
            $($(this).data('slider')).addClass('active');
        });

        $(document).on('click', 'section.news .bookmark .header', function () {
            $("section.news .bookmark.active").removeClass('active');
            $(this).parent('.bookmark').addClass('active');
        });

        $("#newsSection").waypoint(function(direction) {
                if(direction == 'down') {
                    $(".carousel").carousel();
                }
            },
            {
                offset: 150
            }
        );

        $("#newsSection").waypoint(function(direction) {
                if(direction == 'down') {
                    $(".carousel").carousel();
                }
            },
            {
                offset: 150
            }
        );
    });

    function showNewsBox(id) {
        if (!id) {
            $("section.news .bookmark:nth-of-type(1)").addClass('active')
        } else {

        }
    }

    function isTouch() {
        return !!('ontouchstart' in window);
    }

    function checkLive() {
        $.ajax({
            url: "/cajax/newsstream/get-for-homepage",
            global: false,
            type: "GET",
            dataType: "html",
            cache: false,
            success: function (html) {
                $('.marquee').html(html);
                $('.marquee').marquee({
                    duration: 20000,
                    gap: 20,
                    delayBeforeStart: 0,
                    direction: 'left',
                    duplicated: 'true',
                    pauseOnHover: true
                });
            }
        });
    }
    //]]>
</script> </div>
