<script type="text/javascript">
    //<![CDATA[
    var scrolled=0;
    var wHeight = window.innerHeight;

    
    $(document).ready(function() {
        $('#topCarousel .carousel-inner').hammer().on('swipeleft', function(){
            $(this).parent().carousel('next');
        });
        $('#topCarousel .carousel-inner').hammer().on('swiperight', function(){
            $(this).parent().carousel('prev');
        });

        $('.ttp').qtip({ // Grab all elements with a non-blank data-tooltip attr.
            content: {
                attr: 'title' // Tell qTip2 to look inside this attr for its content,
            },
            position: {
                my: 'top right',  // Position my top left...
                at: 'center left', // at the bottom right of...
//                target: $('body') // my target
            },
            show: 'click',
            hide: 'unfocus',
            style: {
                classes: 'qtip-dark qtip-rounded'
            }
        });

        $(".downArrow").on("click" ,function(){
            if($(this).hasClass('r180')) {
                scrolled = 0;
                $("html, body").animate({
                     scrollTop:  scrolled
                 });
                $(this).removeClass('r180');
            }
            else {
                var dHeight = $(document).height();
                scrolled=scrolled+wHeight-25;
                
                $("html, body").animate({
                     scrollTop:  scrolled
                 });

                var sc2 = scrolled+wHeight+50;

                if(sc2 > dHeight) {
                    $(this).addClass('r180');
                }
            }
        });
    });
    //]]>
</script>
