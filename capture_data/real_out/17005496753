<script type="text/javascript">
            /*JS DESKTOP  HP CUSTOM MOLTO_SALUTE*/

            _waitOnceFor('ready', function() {

                /* tutte le testatine articolo salute moltosalute */

                var h3 = document.querySelectorAll(".testatabox h3.moltosalute");

                var i;

                for (i = 0; i < h3.length; i++) {

                    var boxH3 = h3[i].getBoundingClientRect();

                    h3[i].innerHTML = svgRepropose(boxH3.width, boxH3.height, 140, 40, logoSalute);

                };

                /* tutte le testatine articolo salute moltosalute */

                var testatina_article = document.querySelectorAll("article.salute.moltosalute .testatina");

                var i;

                for (i = 0; i < testatina_article.length; i++) {

                    var box_article = testatina_article[i].getBoundingClientRect();

                    testatina_article[i].innerHTML = svgRepropose(box_article.width, box_article.height, 90, 13,
                        logoSalute);

