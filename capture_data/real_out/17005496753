                    // ask for prebid
                    pbjs.que.push(function() {
                        pbjs.addAdUnits(adUnits);    

                        // aggiungo i targeting come per la dfp (ma qui tramite keywords!)
                        try {
                            ((i,k) => {
                                for (i=0; i < adUnits.length; i++)
                                    for (k=0; k < adUnits[i].bids.length; k++)
                                        if (adUnits[i].bids[k].bidder == 'appnexus')
                                            adUnits[i].bids[k].params.keywords = {category: "Homepage"};
                            })();
                        } catch(e) { CED.warn(e); }

