                new Promise((resolve, reject) => {
                    _waitOnceFor('cookielaw_5_accepted', () => {
                        // load script
                        !function(a9,a,p,s,t,A,g){if(a[a9])return;function q(c,r){a[a9]._Q.push([c,r])}a[a9]={init:function(){q("i",arguments)},fetchBids:function(){q("f",arguments)},setDisplayBids:function(){},targetingKeys:function(){return[]},_Q:[]};A=p.createElement(s);A.async=!0;A.src=t;g=p.getElementsByTagName(s)[0];g.parentNode.insertBefore(A,g)}("apstag",window,document,"script","//c.amazon-adsystem.com/aax2/apstag.js");

                        apstag.init({
                            pubID: '3695',
                            adServer: 'googletag'
                        });

                        apstag.fetchBids({
                                slots:  [{"slotID":"adv_Skin","slotName":"/38681514,22676308858/Mattino/Homepage/Skin","sizes":[[1,1],[728,90],[980,90],[980,250],[970,250]]},{"slotID":"adv_BoxTop","slotName":"/38681514,22676308858/Mattino/Homepage/BoxTop","sizes":["fluid",[300,250],[300,600]]},{"slotID":"adv_BoxBottom","slotName":"/38681514,22676308858/Mattino/Homepage/BoxBottom","sizes":["fluid",[300,250],[300,251]]},{"slotID":"adv_LdbMastheadPush","slotName":"/38681514,22676308858/Mattino/Homepage/LdbMastheadPush","sizes":[[728,90],[980,90],[980,250],[970,250]]},{"slotID":"adv_PromoBox","slotName":"/38681514,22676308858/Mattino/Homepage/PromoBox","sizes":["fluid",[300,100],[300,250],[300,252]]},{"slotID":"adv_MiniMasthead","slotName":"/38681514,22676308858/Mattino/Homepage/MiniMasthead","sizes":["fluid",[600,180],[600,90],[468,60],[300,250],[300,249]]},{"slotID":"adv_BoxPromoAdv","slotName":"/38681514,22676308858/Mattino/Homepage/BoxPromoAdv","sizes":["fluid",[300,101],[300,250],[300,254]]},{"slotID":"adv_Native1","slotName":"/38681514,22676308858/Mattino/Homepage/Native1","sizes":["fluid",[2,2],[600,181],[300,250],[301,250]]},{"slotID":"adv_Native2","slotName":"/38681514,22676308858/Mattino/Homepage/Native2","sizes":["fluid",[2,2],[2,4],[300,250],[302,250]]},{"slotID":"adv_Native3","slotName":"/38681514,22676308858/Mattino/Homepage/Native3","sizes":["fluid",[2,2],[2,5],[300,250],[303,250]]},{"slotID":"adv_Native4","slotName":"/38681514,22676308858/Mattino/Homepage/Native4","sizes":["fluid",[2,2],[2,6],[300,250],[304,250]]},{"slotID":"adv_BoxPromoAdvCeds","slotName":"/56686444/Mattino/Homepage/BoxPromoAdvCeds","sizes":[300,250]}]                            },
