                for (i = 0; i < testatina_article.length; i++) {
                    var box_article = testatina_article[i].getBoundingClientRect();
                    testatina_article[i].innerHTML = svgRepropose(box_article.width, box_article.height, 90, 13,
                        logoFinanzaPerTutti);
                }


            });
        </script>
