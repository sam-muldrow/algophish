            (() => {
                function s(i, m, o, n) {
                    n = typeof i == 'object';
                    if (n && i.length) {
                        for (m=0; m < i.length; m++) {
                            i[m] = s(i[m]);
                        }
                    } else if (n) {
                        for (m in i) {
                            if (m == '_consent') {
                                for (o in i[m]) {
                                    if (doesPurposeHaveConsent(o)) {
                                        return i[m][o];
                                    }
                                }
                                if (i[m]['0']) return i[m]['0'];
                            } else {
                                i[m] = s(i[m]);
                            }
                        }
                    }
                    return i;
                }
                adUnits = s(adUnits);
            })();
            
var prebidConfig = { enableSendAllBids: false, targetingControls: { alwaysIncludeDeals: true }, bidderTimeout: 2500, bidderSequence: 'fixed', consentManagement: { gdpr: { cmpApi: 'iab', timeout: 800, defaultGdprScope: true } }, currency: { adServerCurrency: 'USD' }, priceGranularity: { buckets: [{ precision: 2, max: 2, increment: 0.01 }, { max: 4, increment: 0.02 }, { max: 8, increment: 0.05 }] }, userSync: { filterSettings: { iframe: { bidders: '*', filter: 'include' } }, syncsPerBidder: 0, userIds: [{ name: 'identityLink', params: { pid: '13951', notUse3P: false }, storage: { type: 'html5', name: 'idl_env', expires: 15, refreshInSeconds: 1800 } }], syncDelay: 3000 }, timeoutBuffer: 300, useBidCache: true, 'rubicon': { singleRequest: true, netRevenue: true }, 'criteo': { fastBidVersion: 'latest' } };

