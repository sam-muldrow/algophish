                // PromiseAll RESOLVED ! ************************************************************
                // yee all promises finished!
                CED.debug("Prebid Manager - ALL RESOLVED");
                _waitOnceFor('cookielaw_5_accepted', () => {
                    CED.debug("Prebid Manager - Consent OK");
                    googletag.cmd.push(function () {
                        pbjs.que.push(function() {
                            pbjs.setTargetingForGPTAsync();
                        });
                        googletag.pubads().refresh();
