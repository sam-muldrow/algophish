        <script>
          // localisation global function
          window.translateTexts = function(root = document) {
            let elements = root.querySelectorAll("[translation-key]");
            elements.forEach((element)=> {
              const key = element.getAttribute("translation-key");
              const translation = window.serverData && window.serverData.appStrings && window.serverData.appStrings[key] || "";
              element.innerText = translation;
            });
          };
        </script>
        <script>
            window.serverData = {};

            document.addEventListener("DOMContentLoaded", () => {
              window.translateTexts();
            });

