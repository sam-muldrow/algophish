		<script src="//pages.lumentech.com/js/forms2/js/forms2.min.js"></script>
		<form id="mktoForm_5724"></form>
		<script>MktoForms2.loadForm("//pages.lumentech.com", "926-HYV-740", 5724, function (form) {
				MktoForms2.whenReady(function (form) {
					let formEl = form.getFormElem()[0]
					// declare required inputs as a variable
					const reqInputs = formEl.querySelectorAll('.mktoField.mktoRequired');
					const billingInput = formEl.querySelector('#Comments__c');
					const commentInput = formEl.querySelector('#Lead_Comments__c');

					
					// loop thru each input to find the label 
					reqInputs.forEach((labels, i) => {
						// assign event listeners for focus and blur to change the color of the label font
						labels.addEventListener('focus', (event) => event.target.previousElementSibling.previousElementSibling.style.color = '#0075c9')

						labels.addEventListener('blur', (event) => event.target.previousElementSibling.previousElementSibling.style.color = '')
					})

					// billingInput.forEach((labels, i) => {
						// assign event listeners for focus and blur to change the color of the label font
						if(billingInput){
							billingInput.addEventListener('focus', (event) => event.target.previousElementSibling.previousElementSibling.style.color = '#0075c9')
							billingInput.addEventListener('blur', (event) => event.target.previousElementSibling.previousElementSibling.style.color = '')						
						}
						if(commentInput){
							commentInput.addEventListener('focus', (event) => event.target.previousElementSibling.previousElementSibling.style.color = '#0075c9')
							commentInput.addEventListener('blur', (event) => event.target.previousElementSibling.previousElementSibling.style.color = '')
						}						
					// })

					var formId = form.getId()

					if (formId === 5724) {
						form.onSuccess(function (currentForm) {
							form.getFormElem().hide();
							document.getElementById('confirm-5724').style.display = 'block';
                                                        window.scrollBy(0, -500);
							return false;
						});
					}
				});
			});

		</script>

