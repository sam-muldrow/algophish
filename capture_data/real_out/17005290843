    <script>
        var windowLoc = window.location.href;
        if (windowLoc.indexOf("/support/account/detect-device.html")>-1) {
            getDeviceType();
        }

        function getDeviceType() {
            var userAgent = navigator.userAgent;
            if (/iPad|iPhone|iPod/.test(userAgent) && !window.MSStream) {
                window.location.href = "https://apps.apple.com/us/app/quantum-fiber-360-wifi/id1573846896";
            } else if (/Android/i.test(userAgent)) {
                window.location.href = "https://play.google.com/store/apps/details?id=com.plumewifi.plume.quantum";
            }
            else{
                window.location.href ="/support/account/app.html";
            }
        }
    </script>

	
    
    

    

    


