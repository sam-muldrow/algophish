  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
<script>
	

$(document).ready(function(){
	
	const QueryString = window.location.search;
	const urlParams = new URLSearchParams(QueryString); 
	var heroSlide = urlParams.get("hero-slide");
	
	if (heroSlide == 2){
		setTimeout(function(){
			$('.cmp-carousel__action--next').trigger('click');
		}, 100);
	}
	
	if (heroSlide == 3){	
		setTimeout(function(){
			$('.cmp-carousel__action--next').trigger('click');
		}, 100);
		setTimeout(function(){
			$('.cmp-carousel__action--next').trigger('click');
		}, 200);
	}
	
	if (heroSlide == 4){
		setTimeout(function(){
			$('.cmp-carousel__action--next').trigger('click');
		}, 100);
		setTimeout(function(){
			$('.cmp-carousel__action--next').trigger('click');
		}, 200);
		setTimeout(function(){
			$('.cmp-carousel__action--next').trigger('click');
		}, 300);
	}
	
	if (heroSlide == 5){
		setTimeout(function(){
			$('.cmp-carousel__action--next').trigger('click');
		}, 100);
		setTimeout(function(){
			$('.cmp-carousel__action--next').trigger('click');
		}, 200);
		setTimeout(function(){
			$('.cmp-carousel__action--next').trigger('click');
		}, 300);
		setTimeout(function(){
			$('.cmp-carousel__action--next').trigger('click');
		}, 400);
	}
	
	$("a[href*='#chat']").click(function(){
		launchChat();
	});

	$(".cmp-experiencefragment--enterprise-navigation").find(".hero-image").remove(); 
	
	$(".hero-image img").removeAttr("data-lightbox");
	
	$(".cmp-subnav__dropdown-left li").click(function(){
		$(".cmp-subnav__dropdown-left li").removeClass("subNavSelected");
		$(this).addClass("subNavSelected");
	});
	
	var position = $(window).scrollTop(); 

	// should start at 0
	$(window).scroll(function() {
		var scroll = $(window).scrollTop();
		if(scroll > position && position > 300) {
			$(".persistent-wrapper").addClass("pcta-sticky-top");
		} else {
			$(".persistent-wrapper").removeClass("pcta-sticky-top");
		}
		
		
		if(scroll > position && position > 100) {
			$('.cmp-carousel__action--pause').trigger('click');
		}
		
		if(scroll > position && position < 101) {
			$('.cmp-carousel__action--play').trigger('click');
		}

		position = scroll;	
	});
});
</script>

