<script>

    var playerId = "alephLive";
    var bkpId = "alephLiveBkp";
    //var mainUrl = "https://streamb.m.ro/Aleph/Alephnew2.stream/playlist.m3u8";
    var mainUrl = "https://streamw.m.ro/Aleph/ngrp:Alephnewsmain.stream_all/playlist.m3u8";
    var bkpUrl = "https://stream-aleph.m.ro/Aleph/ngrp:Alephnewsbackup.stream_all/playlist.m3u8";
    var videoUrl = mainUrl;

    // initialize Cxense DMP Events
    window.cX = window.cX || {}; cX.callQueue = cX.callQueue || [];
    cX.callQueue.push(['setSiteId', '1135102062960807735']);
    // var prefix = 'brt';
    var prefix = 'v15';	
    cX.callQueue.push(['setEventAttributes',
        { origin: prefix + '-video',
            //persistedQueryId: 'fcadb0c22bec63a3e94af5a65644fc6b869880e0'
            persistedQueryId: '932631ac111d926e3fa6d5c7b533913faa7543e2'
        }]);

    var _cx_videoId = "AlephNews-Live";
    var _cx_curProgress = -1;
    var _cx_curBlock = 25;

    var start = -1;
    var end = new Date();
    var timespend = 0;
    var counterPerSec = 0;
    var	durata = "nedeterminata";  
    var tip_play = "autoplay";

    jwplayer(playerId).setup({
        file: videoUrl,
        aspectratio: "16:9",
        width: "100%",
        key: "QbHNZfGUNFEIh7N7SPJDUmufw+zVCK0xMON1ao8VYW6+mQay",
        autostart: true,
        floating: true,
        mute: true,
        cast: {},
        repeat:'always',
        // ga: {}
    });

     jwplayer(playerId).on('ready', function(){
        var qoe = this.qoe();
    //  logs.log('The player setup in:', JSON.stringify(qoe.setupTime),'ms');
     });

    jwplayer(playerId).on('play', function(){
    	sendPlayerEventsVideo();
        start = new Date();
        cX.callQueue.push(['sendEvent', 'play', {vid: _cx_videoId, durata: durata, tip_play: tip_play }]);	
        console.log("s-a trimis eventul - play - " + _cx_videoId);	
        console.log("prefix " +prefix );
    });
    jwplayer(playerId).on('error', function(){
    	sendPlayerEventsVideo();
        var oldPlayer = document.getElementById(playerId);
        oldPlayer.style.display = 'none';
        var newPlayer = document.getElementById(bkpId);
        newPlayer.style.display = "block";
		console.log("bkp error");
        jwplayer(bkpId).setup({
            file: bkpUrl,
            aspectratio: "16:9",
            width: "100%",
            key: "QbHNZfGUNFEIh7N7SPJDUmufw+zVCK0xMON1ao8VYW6+mQay",
            autostart: true,
            floating: true,
            mute: true,
            cast: {},
            repeat:'always',
            // ga: {}
        });
        jwplayer(bkpId).on('play', function(){
            start = new Date();
            cX.callQueue.push(['sendEvent', 'play', {vid: _cx_videoId, durata: durata, tip_play: tip_play }]);	
        	console.log("s-a trimis eventul - play - " + _cx_videoId);
        	console.log("prefix " +prefix );
        });
        jwplayer(bkpId).on('firstFrame', function(){
            //cX.callQueue.push(['sendEvent', 'percentile', { percent: '0', vid: _cx_videoId }]);
            cX.callQueue.push(['sendEvent', 'percentile', { percentage: '0', vid: _cx_videoId, durata: durata, tip_play: tip_play }]);
            console.log("s-a trimis eventul - firstFrame - " + _cx_videoId + "+" +durata + "+" +tip_play);
            console.log("prefix " +prefix );
        });
        jwplayer(bkpId).on('pause', function(){
            //cX.callQueue.push(['sendEvent', 'pause', { vid: _cx_videoId }]);
             cX.callQueue.push(['sendEvent', 'pause', { vid: _cx_videoId, durata: durata, tip_play: tip_play }]);
             console.log("s-a trimis eventul - pause - " + _cx_videoId + "+" +durata + "+" +tip_play);
             console.log("prefix " +prefix );
        });
        jwplayer(bkpId).on('time', function(e) {
            end = new Date();
            if (counterPerSec % 4 === 0) {
                if (_cx_curProgress == -1) { // first progress - send start event
                    //cX.callQueue.push(['sendEvent', 'play', {vid: _cx_videoId}]);
                    cX.callQueue.push(['sendEvent', 'play', {vid: _cx_videoId, durata: durata, tip_play: tip_play }]);
                    console.log("s-a trimis eventul - play din ontime - " + _cx_videoId + "+" +durata + "+" +tip_play);
                    _cx_curProgress = 0;
                } else {
                    var dimStatus = 'large';
                    var mutedStatus = jwplayer(bkpId).getMute() ? 'muted' : 'unmuted';
                    var customEvent =  dimStatus + '_' + mutedStatus;
                    timespend = (start > 0) ? Math.round((end - start) / 1000) : 0;
                    if (timespend > 0 && (timespend % 60 === 2)) {
                        console.log("trimite event:  still_here - backup");
                        cX.callQueue.push(['sendEvent', 'still_here', {vid: _cx_videoId}]);
                        console.log("event: " + customEvent);
                        cX.callQueue.push(['sendEvent', customEvent, {vid: _cx_videoId}]);
                        console.log("prefix " +prefix );
                    }
                }
            }
            counterPerSec = counterPerSec + 1;
        });
    });
    console.log("bkp nonerror");
    jwplayer(playerId).on('firstFrame', function(){
    	sendPlayerEventsVideo();
       // cX.callQueue.push(['sendEvent', 'percentile', { percent: '0', vid: _cx_videoId }]);
       cX.callQueue.push(['sendEvent', 'percentile', { percentage: '0', vid: _cx_videoId, durata: durata, tip_play: tip_play }]);
       console.log("s-a trimis eventul - firstFrame - " + _cx_videoId + "+" +durata + "+" +tip_play);
       console.log("prefix " +prefix );
    });
    jwplayer(playerId).on('pause', function(){
    	sendPlayerEventsVideo();
        //cX.callQueue.push(['sendEvent', 'pause', { vid: _cx_videoId }]);
        cX.callQueue.push(['sendEvent', 'pause', { vid: _cx_videoId, durata: durata, tip_play: tip_play }]);
        console.log("s-a trimis eventul - pause - " + _cx_videoId + "+" +durata + "+" +tip_play);
        console.log("prefix " +prefix );
    });
    jwplayer(playerId).on('time', function(e){         // report progress evry 25 percent
        end = new Date();
        // fac %4 pentru ca el imi da la sfert de secunda
        if (counterPerSec % 4 === 0) {
            //console.log(Math.round((end - start) / 1000));
            if (_cx_curProgress == -1) { // first progress - send start event
                //cX.callQueue.push(['sendEvent', 'play', {vid: _cx_videoId}]);
                sendPlayerEventsVideo();
                cX.callQueue.push(['sendEvent', 'play', {vid: _cx_videoId, durata: durata, tip_play: tip_play }]);
                console.log("s-a trimis eventul - play din ontime - " + _cx_videoId + "+" +durata + "+" +tip_play);
                _cx_curProgress = 0;
                //  console.log("start video");
            } else {
				sendPlayerEventsAdblock();
                // var viewportWidth = Math.max(window.parent.document.documentElement.clientWidth || 0, window.parent.innerWidth || 0);
                // var videoWidth = Math.max(document.documentElement.clientWidth || 0, window.innerWidth || 0);
                // var dimStatus = (videoWidth > 0.7 * viewportWidth) ? 'large' : 'small';
                var dimStatus = 'large';
                var mutedStatus = jwplayer(playerId).getMute() ? 'muted' : 'unmuted';
                var customEvent =  dimStatus + '_' + mutedStatus;

                timespend = (start > 0) ? Math.round((end - start) / 1000) : 0;

                // console.log("start: " + start);
                // console.log("end: " + end);
                // console.log("merge banda " + timespend);
                // trimite la secunda 2 si dupa din minut in minut

                if (timespend > 0 && (timespend % 60 === 2)) {
                    console.log("trimite event: still_here");
                    cX.callQueue.push(['sendEvent', 'still_here', {vid: _cx_videoId}]);
                    // console.log("timespend: " + timespend);
                    console.log("event: " + customEvent);
                    cX.callQueue.push(['sendEvent', customEvent, {vid: _cx_videoId}]);
                    console.log("prefix " +prefix );
                }
            }
        }
        counterPerSec = counterPerSec + 1;
    });
    
    function sendPlayerEventsVideo(){
		var prefix = 'v15';	
		cX.callQueue.push(['setEventAttributes',
			{ origin: prefix + '-video',
				persistedQueryId: '932631ac111d926e3fa6d5c7b533913faa7543e2'
			}]);
		}
	function sendPlayerEventsAdblock(){
		var prefix = 'brt';	
		cX.callQueue.push(['setEventAttributes',
			{ origin: prefix + '-adblock',
				persistedQueryId: 'fcadb0c22bec63a3e94af5a65644fc6b869880e0'
			}]);
		}
</script>






