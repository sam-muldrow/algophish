<script type="text/javascript">
var myTimer = 0;
$("#woord").on('keydown', function() {
var wl2=document.getElementById('woord').value.length;
if (wl2<3) return;
else if (wl2>8) return;
else {
	var e = document.getElementById("src");
	var src_val = e.options[e.selectedIndex].text;
	var f = document.getElementById("des");
	var des_val = f.options[f.selectedIndex].text;
if (myTimer) {clearTimeout(myTimer);}
myTimer = setTimeout(function() {
$.post("https://www.mijnwoordenboek.nl/autoComplete/rpc.php", {woord: ""+document.vform.woord.value+"",src: ""+src_val+"",des: ""+des_val+""}, function(data){
$('#suggestions').show();$('#autoSuggestionsList').html(data);
});}, 1000);}});

$(document).ready(function() {
    $('#swap').click(function() {
        var talen = [];
        $('.mySel').each(function(i) {
            talen[i] = $(this).val();
        });
        talen.reverse();
        $('.mySel').each(function(i) {
            $(this).val(talen[i]);
        });
    });
    var brontaal = getCookie("src");
    var doeltaal = getCookie("des");
	$('#src').val(brontaal);
	$('#des').val(doeltaal);
});
</script>
</body>
</html>
