						}
									
						else{				
							adSlot = googletag.defineSlot('/40807296/El_Colombiano_Desktop/'+ sectionName, tamanioSlot, AdsSlotsNamesArray.namesArrays[i]).addService(googletag.pubads());
							if(!isNaN(validateTargeting(AdsSlotsNamesArray.namesArrays[i]))){
								adSlot.setTargeting("pos", validateTargeting(AdsSlotsNamesArray.namesArrays[i]));
								if ( No_Apto == '["No_Apto"]'){
									adSlot.setTargeting("Cont", "No_Apto");
								}
					
