								adslotlatizquierdo=adSlot.setTargeting("Form", "Lateral_Izquierdo");
								if ( No_Apto == '["No_Apto"]'){
									adslotlatizquierdo=adSlot.setTargeting("Cont", "No_Apto");
								}
								
							}else if(validateTargeting(AdsSlotsNamesArray.namesArrays[i]) == "lateralIzquierdoRegistrado"){
							    
								adslotlatizquierdo=adSlot.setTargeting("Form", "Lateral_IzquierdoRegistrado");
								if ( No_Apto == '["No_Apto"]'){
									adslotlatizquierdo=adSlot.setTargeting("Cont", "No_Apto");
								}
					
							}else if(validateTargeting(AdsSlotsNamesArray.namesArrays[i]) == "lateralDerecho"){
								adslotlatderecho=adSlot.setTargeting("Form", "Lateral_Derecho");
								if ( No_Apto == '["No_Apto"]'){
									adslotlatderecho=adSlot.setTargeting("Cont", "No_Apto");
