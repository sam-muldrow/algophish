</style> <script>/*<![CDATA[*//*<![CDATA[*/
 // FunciÃ³n para obtener el nombre del mes
 function obtenerNombreMes(mes) {
 const nombresMeses = [
 "Enero", "Febrero", "Marzo", "Abril", "Mayo", "Junio",
 "Julio", "Agosto", "Septiembre", "Octubre", "Noviembre", "Diciembre"
 ];
 return nombresMeses[mes];
 }

 // FunciÃ³n para obtener el dÃ­a de la semana en formato texto
 function obtenerDiaSemana() {
 const diasSemana = ["Domingo", "Lunes", "Martes", "MiÃ©rcoles", "Jueves", "Viernes", "SÃ¡bado"];
 const fechaActual = new Date();
 const diaSemana = fechaActual.getDay();
 return diasSemana[diaSemana];
 }

 // FunciÃ³n para obtener la fecha actual en el formato deseado
 function obtenerFechaFormateada() {
 const fechaActual = new Date();
 const diaSemana = obtenerDiaSemana();
 const dia = fechaActual.getDate();
 const mes = obtenerNombreMes(fechaActual.getMonth());
 const aÃ±o = fechaActual.getFullYear();

 return `<span class="bold">${diaSemana}</span>, ${dia} de ${mes} de ${aÃ±o}`;
 }

 function pintarfechaactual(){
 const fechaFormateada = obtenerFechaFormateada();
 const fechaDiv = document.getElementById("fechacabecera");
 fechaDiv.innerHTML = fechaFormateada;
 }
