								if ( No_Apto == '["No_Apto"]'){
									adslotlatderecho=adSlot.setTargeting("Cont", "No_Apto");
								}

							}else if(validateTargeting(AdsSlotsNamesArray.namesArrays[i]) == "flotante"){
								adSlot.setTargeting("Form", "Barra_Flotante");
								
				
							}else if(validateTargeting(AdsSlotsNamesArray.namesArrays[i]) == "flotante2"){
								adSlot.setTargeting("Form", "Barra_Flotante");
								
				
							}else if(validateTargeting(AdsSlotsNamesArray.namesArrays[i]) == "desplegable"){
								
								adSlot.setTargeting("Form", "Desplegable");
								adSlot.setTargeting("Pos", "1");
					
							}else if(validateTargeting(AdsSlotsNamesArray.namesArrays[i]) == "zocalo"){
								adSlot.setTargeting("Form", "Desplegable");
								
							}
						}	
					}
				}
			
			
			googletag.pubads().setTargeting("CxSegments",CX_SEGMENTS);
			if(enreachresp){googletag.pubads().setTargeting('ecid', enreachresp.split('|'))};
			if(window.ecsegmArr){
				for(var i=0;i<window.ecsegmArr.length;i++){
				googletag.pubads().setTargeting(window.ecsegmArr[i].key, window.ecsegmArr[i].value);
				}
			}
			googletag.pubads().disableInitialLoad();
			googletag.pubads().enableSingleRequest();
			googletag.pubads().collapseEmptyDivs();
			googletag.pubads().enableLazyLoad({
											  fetchMarginPercent: 125,  
											  renderMarginPercent: 50, 
											  mobileScaling: 2.0
											});
			(function(w) {
				try {
					var name, col, persona = JSON.parse(window.localStorage.getItem('nvgpersona43296'));
					for (col in persona) {
						name = 'nvg_' + col;
						name = name.substring(0, 10);
						if (typeof(googletag) == "object")
							googletag.pubads().setTargeting(name, persona[col]);
						if (typeof(GA_googleAddAttr) == "function")
							GA_googleAddAttr(name, persona[col]);
					}
				} catch (e) {console.log('Error Naveg :'+ e);}
			})(window);

			googletag.enableServices();

		});

	}
	
  
  
  
function validateTargeting(item){
	try{
		data = item.split("_");

		if(data.length>1){	
			var lastElement = data[data.length - 1];
			if (!isNaN(lastElement)){
				//El string se puede convertir a nÃºmero
				return lastElement;
			}else if(lastElement == "lateralIzquierdo"){
				
				return "lateralIzquierdo";
			}else if (lastElement == "lateralDerecho"){
				return "lateralDerecho";
				
			}else if(lastElement == "desplegable"){
				return "desplegable";
				
			}else if (lastElement == "flotante"){
				return "flotante";
				
			}else if (lastElement == "flotante2"){
				return "flotante2";
			
			}else if (lastElement == "zocalo"){
				return "zocalo";
			
			}else if (lastElement == "intelecto"){
				return "intelecto";
				
			}
		}
		return "NA";
	}catch(err){
		return "";
	}
}


function get_size(elem){
	try{
		data = elem.split("x");
		if(data.length>1){
			a = parseInt(data[0]);
			b = parseInt(data[1]);
			return [a,b];
		}else if(elem == "COMSCORE-SLOT" || elem == "SUTARGET_ARTICULO" || elem == "SUTARGET_HORIZONTAL" &&  elem == "SUTARGET_PORTADA"){
			return "NA";
		}
	    else if (elem == "fuera_pagina" ){
			return "fuera_pagina";
		}
		else if (elem == "300POR100-250_Dinamico1" ){
			return "Dinamico1";
		}
		else if (elem == "300POR250-600_Dinamico2" ){
			return "Dinamico2";
		}
		else if (elem == "300POR250-600_Dinamico3" ){
			return "Dinamico3";
		}
		else if (elem == "300POR250-600_Dinamico4" ){
			return "Dinamico4";
		}
		else if (elem == "300POR250-600_Dinamico5" ){
			return "Dinamico5";
		}
		else if (elem == "300POR250-600_Dinamico6" ){
			return "Dinamico6";
		}
		else if (elem == "300POR250-600_Dinamico7" ){
			return "Dinamico7";
		}
		else if (elem == "300POR250-600_Dinamico8" ){
			return "Dinamico8";
		}
		else if (elem == "300POR250-600_Dinamico9" ){
			return "Dinamico9";
		}
		else if (elem == "300POR250-600_Dinamico10" ){
			return "Dinamico10";
		}
		else if (elem == "300POR250-600_Dinamico11" ){
			return "Dinamico11";
		}
		else if (elem == "300POR250-600_Dinamico12" ){
			return "Dinamico12";
		}
		else if (elem == "300POR250-600_Dinamico13" ){
			return "Dinamico13";
		}
		else if (elem == "300POR250-600_Dinamico14" ){
			return "Dinamico14";
		}
	    else if (elem == "190POR220_Carrusel1" ){
			return "Carrusel1";
		}
	    else if (elem == "190POR220_Carrusel2" ){
			return "Carrusel2";
		}
		else if (elem == "Notus1-1" ){
			return "Notus1-1";
		}
        else if (elem == "articulo1-1" ){
			return "articulo1-1";
		}
		else if (elem == "m_barra_flotante" ){
			return "movil_barra_flotante";
		}
		else if (elem == "m_dinamico_1" ){
			return "movil_dinamico1";
		}
		else if (elem == "m_dinamico_2" ){
			return "movil_dinamico2";
		}

		else if (elem == "m_dinamico_3" ){
			return "movil_dinamico3";
		}
		
		else if (elem == "m_dinamico" ){
			return "movil_dinamico";
		}
		else if (elem == "m_fuera_pagina" ){
			return "movil_fuerapagina";
		}
		else if (elem == "m_nativa_1" ){
			return "movil_nativa1";
		}
		else if (elem == "m_nativa_2" ){
			return "movil_nativa2";
		}
		else{
			return "outOfPage";
		}	
		
	}catch(err){return "NA";}
}

   
 
