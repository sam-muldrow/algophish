<script type="text/javascript">
  $(function() {
    // 页面访问统计
    setTimeout(function() {
      Locals.get('userInfo,uuid', function(data) {
        var userInfo = data.userInfo;
        aliStatFns.uuid = data.uuid;
        aliStatFns.ukey = userInfo ? userInfo._ukey : 0;
        aliStatFns.vip = userInfo ? userInfo._wsvip : 0;
        aliStatFns.pageView('首页', '首页', '华数TV全网影视-超新超全超热门电影电视剧综艺动漫直播在线观看');
      })
    }, 1000)
    
    // 页面加载时长
    pageLoadTime = new Date().getTime() - pageBeginTime;
    
    //搜索&播放器下载按钮 点击统计
    $('#search_prompt, #player_download_btn').on('click', function() {
      aliStatFns.click( $(this), '首页', '华数TV全网影视-超新超全超热门电影电视剧综艺动漫直播在线观看' );
    })
    
    // 点击统计
    $('body').on('click', '.ali_click_stat', function() {
      // 发送点击统计请求
      aliStatFns.click( $(this), '首页', '华数TV全网影视-超新超全超热门电影电视剧综艺动漫直播在线观看' );
    })
    
    // 记录登陆成功后的跳转地址
    if(location.href.indexOf(_memberUrl) === -1) {
      Locals.set('_log_reg_back', location.href)
    }
  })
</script>
