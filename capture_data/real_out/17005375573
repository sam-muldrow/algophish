<script>

	
	 <!--广告关闭事件-->
	function off_ad(){
		$('.ad_bag_a').hide();
		$('.focus_inner').show();
		$('.sider_control').show();			
		$('.fall_down').hide();	
		$('.herd_ad').show();
	}
	
	//广告加载次数累加
	
	if($.cookie('ad_show')){
		var ad_show	= $.cookie('ad_show');
		if(ad_show<=4){
			var date = new Date();
			date.setTime(date.getTime()+60*60*24*1000);
			$.cookie('ad_show',++ad_show,{expires:date});					
		}		
	}else{
		var date = new Date();
		date.setTime(date.getTime()+60*60*24*1000);
		$.cookie('ad_show', '1', {expires:date});	
	}
	//初始化广告
	//检测次数
	//后台上线状态
	$(function(){
		var is_show = '' //上下线依据
		var ad_show	= $.cookie('ad_show');
		if(ad_show<5 && is_show){
			sum_show_ad();
		}else{
			$('.herd_ad').show();
			$('.ad_bag_a').hide();
			$('.focus_inner').show();
			$('.sider_control').show();			
		}
		
	});
	//广告展现事件
	function sum_show_ad(){		
		$('.ad_bag_a').show();
		$('.focus_inner').hide();
		var oDiv = document.getElementById('box');
		var span = document.getElementById('span');
		var num = span.innerHTML;
		var timer = null;;
		setTimeout( function(){
			oDiv.style.display = 'block';
			timer = setInterval(function(){
				num--;	
				span.innerHTML = num;
				if(num == 0){	
				$('.ad_bag_a').hide();
				$('.focus_inner').show();
				$('.sider_control').show();			
				$('.fall_down').hide();
				$('.herd_ad').show();
				clearInterval(timer);
			    }
			},1000);
		},500 );
		
	}
	

</script>
  

