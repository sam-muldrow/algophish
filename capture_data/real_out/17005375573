						advChannel = channels[i];
						Locals.set('advChannel', advChannel);
						break;
					}
				}
			} else {
				advChannel = data;
            }
            ac_content_targeting = $.inArray(advChannel, channels) == -1 ? "" : "付费引流渠道:" + advChannel;
        })
    })
