    // 推荐位播放控制
    $('body').on('mouseenter', '.hover-video-play', function() { 
      $(this).find('video')[0].play(); 
      if(!$(this).hasClass('collected')) {
        isRecCollect($(this))
      }
    })
    $('body').on('mouseleave', '.hover-video-play', function() { 
      $(this).find('video')[0].pause(); 
    })
    $('body').on('click', '.rec-collect-btn', function() { 
      var $parent = $(this).parents('.suspend-hover')
      if(!$parent.hasClass('collected')) {
        recCollect($parent);
      }
    })
