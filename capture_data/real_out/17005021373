					xfbml      : true,
					version    : 'v2.9'
				});

				FB.AppEvents.logPageView();   

			};

			(function(d, s, id){
				var js, fjs = d.getElementsByTagName(s)[0];
				if (d.getElementById(id)) {return;}
				js = d.createElement(s); js.id = id;
				js.src = "https://connect.facebook.net/en_US/sdk.js";
				fjs.parentNode.insertBefore(js, fjs);
				}(document, 'script', 'facebook-jssdk')
			);

			(function($){
				
				$(document).ready(function(){
					
					// if(typeof mixpanel != 'undefined'){
					// 	$('.modal-login-form form input.user_login').focus(function(){
					// 		mixpanel.track("Login Email focus", {"Form ID": $('.modal-login-form form').attr('class')});
					// 	})
					// 	$('.modal-login-form form input.user_pass').focus(function(){
					// 		mixpanel.track("Login Password focus", {"Form ID": $('.modal-login-form form').attr('class')});
					// 	})
					// }
					
					$('.fb-login').click(function(e){
						e.preventDefault();
						
						$('.ej-login-container-wrapper .disabled-overlay').show();
						var MPEventTitle = 'User Login';
						var MPProperties = {};
						MPProperties.method = "Facebook";
						var newsletter = $(this).parent().find('form input[name="newsletter"]:checked').length ? 1 : 0;
						
						var formWrapper = $(this).parent();
						formWrapper.find('#login_error').remove();
						formWrapper.addClass('loading');
						
						var redirect = $(this).parents('.modal-login-form').find('input[name="redirect_to"]').val();
						if(redirect == "comments" || redirect == "self" || !redirect || redirect == ""){
							redirect = window.location.href;
							if(redirect == "comments"){
								window.location.reload();
							}
						}
						
						FB.login(function(response) {
							var redirecting = false;
							MPProperties.FacebookResponse = response.status;
							if (response.status === 'connected') {
								FB.api('/me', { fields: 'email' },
									function(meResponse) {
										if(typeof meResponse.error != 'undefined'){
											MPProperties.userLookup = "Undefined Error";
											$('.ej-login-container-wrapper .disabled-overlay').hide();
											formWrapper.prepend('<div id="login_error">We are having trouble connecting to Facebook. There is nothing you can do. Facebook is having problems. We apologize on their behalf. In the meantime, feel free to connect using your email address below.</div>')
											formWrapper.removeClass('loading');
										} else if(typeof meResponse.email != 'undefined'){
											var user_source = $('input[name="user_source"]', formWrapper).val();
																							if($('form', formWrapper).attr('class')){
													user_source = $('form', formWrapper).attr('class');
												}
																							
											var distinct_id = "";
											if(typeof mixpanel != 'undefined'){
												if (typeof mixpanel.get_distinct_id === "function") { 
													distinct_id = mixpanel.get_distinct_id();
												}
											}
											
											var data = {
												action: 'create_user_from_fb',
												email: meResponse.email,
												mp_distinct_id: distinct_id,
												newsletter: newsletter,
												user_source: user_source,
												method: 'Facebook',
												fbid: meResponse.id
											}
											$.ajax({
												method: 'POST',
												url: 'https://www.elephantjournal.com/wp-admin/admin-ajax.php',
												data: data,
												timeout: 13000,
												success: function(ajaxResponse){
													if(ajaxResponse != "fail"){
														redirecting = true;
														if(ajaxResponse != "newuser"){
															redirect = window.location.href;
															if(typeof mixpanel != 'undefined'){
																mixpanel.track(MPEventTitle, MPProperties);
															}
														}
														window.location.href = redirect;
													} else {
														MPProperties.userLookup = "New User Creation Failed";
														$('.ej-login-container-wrapper .disabled-overlay').hide();
													}
												},
												error: function(e){
													if(typeof mixpanel != 'undefined'){
														mixpanel.track('Login Timeout', {"Login Type": "Facebook"});
													}
													formWrapper.prepend('<div id="login_error">Oops, something got messed up on our end. Please reload the page and give it another shot, or try <a style="text-decoration: underline;" href="/ele-login">logging in here</a>.</div>')
													formWrapper.removeClass('loading');
												}
											})
										} else {
											MPProperties.userLookup = "Could not authenticate";
											$('.ej-login-container-wrapper .disabled-overlay').hide();
											formWrapper.prepend('<div id="login_error">Could not authenticate you via Facebook. Please try again, or use your email address instead to create an account.</div>')
											formWrapper.removeClass('loading');
										}
	  								}
								);
							} else {
								formWrapper.prepend('<div id="login_error">Could not authenticate you via Facebook. Please try again, or use your email address instead to create an account.</div>')
								formWrapper.removeClass('loading');
							}
						}, {
							scope: 'public_profile,email'
						});
					})
				})
			})(jQuery);
		</script>

