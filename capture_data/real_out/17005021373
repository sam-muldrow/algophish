			(function($){
				$(document).ready(function(){
					
										
					var user_id = '0';
					
					$('.article-winning-author a.author-id').click(function(e){
						e.preventDefault();
						var t = $(this);
				
						var author_id = $(this).attr('data-author-id');
						var unfollow = 0;
						if($(this).hasClass('active')){
							unfollow = 1;
							$(this).html('Follow')
						} else {
							$(this).html('Following')
						}
						$(this).toggleClass('active');
				
						if(typeof mixpanel != 'undefined'){
							mixpanel.track("Follow Author", {"Unfollow": unfollow, "Author Followed Name": $(this).attr('data-author-name'), "Author Followed": author_id});
						}
				
						var p = $(this).parents('.author');
							
						var data = {
							action: 'add_follower',
							author: author_id,
							user: user_id,
							unfollow: unfollow,
							completed_preferences: '0'
						}
						$.ajax({
							method: 'POST',
							url: admindata.ajaxurl,
							data: data,
							timeout: 10000,
							success: function(response){

							},
							error: function(e){
								console.log(e);
							}
						})
					})
				})
			})(jQuery);
