<script type="text/an-js" class="an-js">
  if(typeof window['player-eS1pNIsAieE'] != 'undefined'){
    try {
      window['player-eS1pNIsAieE'].destroy();
    }catch(e){};
    window['player-eS1pNIsAieE'] = undefined;
  }
  if ($.utilities.video){
    $.utilities.video.YTinit('eS1pNIsAieE', ($.anConstants.deviceHeight + 200), false, '#playerBG');
  }else{
    $.anConstants.YTVDOqueue = $.anConstants.YTVDOqueue || [];
    $.anConstants.YTVDOqueue.push({id: 'eS1pNIsAieE', h: ($.anConstants.deviceHeight + 200), w: '', e: '#playerBG'});
  }
    $('#nlogo').removeClass('o0');
    $('#textAnim').addClass('tp51');
    setTimeout(function(){
      $('#nlogo').addClass('t48');
      $('#bgLogo').addClass('o2');
      setTimeout(function(){
        $('#selGen').removeClass('o0');
      },1200);
    },400);
      var elements = document.getElementsByClassName('txt-rotate');
      elements[0].innerHTML = '<span class="wrap">fashion</span>';
      var TxtRotate = function(el, toRotate, period) {
        this.toRotate = toRotate;
        this.el = el;
        this.loopNum = 0;
        this.period = parseInt(period, 10) || 2000;
        this.txt = '';
        this.tick();
        this.isDeleting = false;
      }
      TxtRotate.prototype.tick = function() {
        var i = this.loopNum % this.toRotate.length;
        var fullTxt = this.toRotate[i];
        if (this.isDeleting) {
          this.txt = fullTxt.substring(0, this.txt.length - 1);
        } else {
          this.txt = fullTxt.substring(0, this.txt.length + 1);
        }
        this.el.innerHTML = '<span class="wrap">'+this.txt+'</span>';
        var that = this;
        var delta = 300 - Math.random() * 100;
        if (this.isDeleting) { delta /= 2; }
        if (!this.isDeleting && this.txt === fullTxt) {
          delta = this.period;
          this.isDeleting = true;
        } else if (this.isDeleting && this.txt === '') {
          this.isDeleting = false;
          this.loopNum++;
          delta = 400;
        }
        setTimeout(function() {
          that.tick();
        }, delta);
      }
      for (var i=0; i<elements.length; i++) {
        var toRotate = elements[i].getAttribute('data-rotate');
        var period = elements[i].getAttribute('data-period');
        if (toRotate) {
          new TxtRotate(elements[i], JSON.parse(toRotate), period);
        }
      }
</script>
	
