    const showModal = 0;
    $(document).ready(function () {
        // $('#info-modal').css('display','none');
        if (!localStorage.getItem('visited')) {
            $('#info-modal').css('display','block');

            $("#btnInfo").on('click',  function(){
                $("#info-modal").css('display','none');
                localStorage.setItem('visited', 'true');
            });
            
        }
    });
    
</script>
