      <script>
     function showBigFlag() {
        const hiddenDiv = document.getElementById('hidden-div');
        if (hiddenDiv) {
            hiddenDiv.classList.remove('d-none');
        }
        const hiddenMobileDiv = document.getElementById('hidden-big-flag-mobile');
        if (hiddenMobileDiv) {
            hiddenMobileDiv.classList.remove('d-none');
        }
    }
    function showSmallFlag() {
      const hiddenDiv = document.getElementById('flag-display')
      if (hiddenDiv) {
        hiddenDiv.classList.remove("flag-display")
      }
    }
</script>
