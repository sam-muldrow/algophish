    <script>
        // Have used role to handle advancedsearch and search form commonly.
        // Replace apostrophe if present in the location name. it makes no
        // difference to the search e.g., King%27s+Lynn and Kings+Lynn both
        // give out the same results.
        $(function() {
            $('form[role=search]').on('submit', function() {
                var geo = $(this).find("input[name=geo]");
                geo.val( geo.val().replace("'",'') );
            });
        });
    </script>
