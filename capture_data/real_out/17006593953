            function jsLinkHandler(el) {
                var params, url;
                params = el.dataset.js_url;
                params = JSON.parse(params);
                url = jsLinkBuildUrl(params);
                el.setAttribute('href', url);
                return true;
            };
            function jsLinkBuildUrl(params) {
                var url = "";
                var queryString;
                if (typeof params[0] !== "undefined") {
                    url = url + "/" + params[0].join("/");
                }
                if (typeof params[1] !== "undefined") {
                    queryString = this.jsLinkBuildQueryString(params[1]);
                    if (queryString.length > 0) {
                        url = url + "?" + queryString;
                    }
                }
                return url;
            };
            function jsLinkBuildQueryString(obj, prefix) {
                if (typeof obj !== "object") {
                    return "";
                }
                var str = [];
                var qsKey = "";
                var qsValue = "";
                for (var prop in obj) {
                    if (obj.hasOwnProperty(prop)) {
                        qsKey = prefix ? prefix + "[" + prop + "]" : prop;
                        qsValue = obj[prop];
                        if (qsValue !== null && typeof qsValue === "object") {
                            str.push(this.jsLinkBuildQueryString(qsValue, qsKey));
                        } else {
                            str.push(
                                window.encodeURIComponent(qsKey) + "=" + window.encodeURIComponent(qsValue)
                            );
                        }
                    }
                }
                return str.join("&");
            };
            
(function() {
    try {
        function buildUrl (params) {
            var url = '';
            if (typeof params[0] != 'undefined') {
                var path = params[0];
                url = '/' + path.join('/');
                if (typeof params[1] != 'undefined') {
                    var queryParams = params[1];
                    var query = [];
                    for (var i in queryParams) {
                        query.push(encodeURIComponent(i) + '=' + encodeURIComponent(queryParams[i]));
                    }
                    if (query.length > 0) {
                        url += '?' + query.join('&');
                    }
                }
            }
            return url;
        }
        var preferredLanguages = navigator.languages || [navigator.language || navigator.userLanguage];
        var userTimezone;
        if (typeof Intl != "undefined") {userTimezone = Intl.DateTimeFormat().resolvedOptions().timeZone}
        var params = ['pl='+encodeURIComponent(preferredLanguages),'tz='+encodeURIComponent(userTimezone)];
        if ('') {params.unshift('');}
        var request = new(window.XMLHttpRequest || ActiveXObject)('MSXML2.XMLHTTP.3.0');
        var visitorUrl = buildUrl([["ua","usertrack","index"],[]]);
        request.open('POST', visitorUrl, true);
        request.setRequestHeader('X-Requested-With', 'XMLHttpRequest');
        request.setRequestHeader('Content-type', 'application/x-www-form-urlencoded');
        request.send(params.join('&'));
    } catch (e) {}
})();
