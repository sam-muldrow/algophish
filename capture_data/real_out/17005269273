<script type="text/javascript">
  $(function() {
    $('.cookie-consent-container').show();

    $('.cookie-consent-btn').on('click', function() {
      $('#accept-cookies-btn-icon').removeClass('fa-check').addClass('fa-spinner').addClass('fa-spin');
      $.ajax({
        url: "/ajax/ajax_consent.php",
        type: "POST",
        cache: false,
        dataType: "json",
        data: {
          action: "accept_all_cookies"
        },
        complete: function(jqXHR, textStatus) {
          $('.cookie-consent-container').hide();
          $('.cookie-consent-container').remove();
        }
      });
    });
  });

</script> <body>

