  <script>
  (function(){
	  // chargement du CSS des flux
	  rktDefer.scriptLoaded('a-load', {
		type: 'css',
		path: 'https://fr.shopping.rakuten.com/assets/canopy-build/styles/styles.css',
	  });
	
	// flux rebuy
	   if (window.screen.width > 992) {
		 rktDefer.scriptLoaded('merchCampaign', 'lazyloadTriggerRebuyFlux', {
		   targetId: 'rebuy-flux',
		   campaignId: '1146562126',
		   campaignOP: '[PMC]-[H]-[Homepage]-[occasion]-[vente]',
		   startIndex: 0,
		   size: 30,
		   template: 'https://fr.shopping.rakuten.com/assets/canopy-build/templates/rebuy-flux.jst',
		   carousel: {
			 infinite: true,
			 slidesToShow: 4,
			 slidesToScroll: 2,
			 variableWidth: true,
			 dots: false
		   }
		 });
	   }
		if (window.screen.width <= 992) {
		  rktDefer.scriptLoaded('merchCampaign', 'lazyloadTriggerRebuyFlux', {
			targetId: 'rebuy-flux',
			campaignId: '1146562126',
			campaignOP: '[PMC]-[H]-[Homepage]-[occasion]-[vente]',
			startIndex: 0,
			size: 30,
			template: 'https://fr.shopping.rakuten.com/assets/canopy-build/templates/rebuy-flux.jst',
		  });
  
		}
		let isInitSellFlux = false;
		function initSellFlux(){
		  if(!isInitSellFlux){
			isInitSellFlux = true;
			setTimeout(function() {
			  window.RKTFLUXPROMORENDER("sell-flux", {
				"user" : { isMember: isMember, priceClubRank: priceClubRank },
				"campaignId": 667951715,
				"size": 35,
				"slider": true,
				"tracking": "[PMC]-[H]-[Homepage]-[occasion]-[achat]"
			  })
			},1000)
		  }
		}
  
		const tabsc2c = document.querySelectorAll('.bloc-c2c-tab')
		const fluxc2c = document.querySelectorAll('.bloc-c2c--products-flux')
		tabsc2c.forEach(tab => {
		  tab.addEventListener('click', () => {
			  const tabId = tab.dataset.id
			  tabsc2c.forEach(tab => {
				  if (tab.classList.contains('bloc-c2c--img-selected')) tab.classList.remove('bloc-c2c--img-selected')
				  if (!tab.classList.contains('bloc-c2c--img-notselected')) tab.classList.add('bloc-c2c--img-notselected')
				  if (tabId === tab.dataset.id) {
					tab.classList.add('bloc-c2c--img-selected')
					tab.classList.remove('bloc-c2c--img-notselected')
				} 
			  })
			  fluxc2c.forEach(flux => {
				const fluxId = flux.dataset.id
				if (fluxId === tabId && !flux.classList.contains('bloc-c2c-selected')){
				  flux.classList.add('bloc-c2c-selected')
				  initSellFlux()
				} 
				if (fluxId === tabId && flux.classList.contains('bloc-c2c-selected')) return 
				else { 
				  flux.classList.remove('bloc-c2c-selected')
				}
			  })
		  })
		})
  })()
  
	</script>
