/* override for rc-tooltip */

._h-rc-tooltip._h-rc-tooltip-zoom-enter,
._h-rc-tooltip._h-rc-tooltip-zoom-leave {
    display: block;
}

._h-rc-tooltip-zoom-enter,
._h-rc-tooltip-zoom-appear {
    opacity: 0;
    -webkit-animation-duration: 0.3s;
            animation-duration: 0.3s;
    -webkit-animation-fill-mode: both;
            animation-fill-mode: both;
    -webkit-animation-timing-function: cubic-bezier(0.18, 0.89, 0.32, 1.28);
            animation-timing-function: cubic-bezier(0.18, 0.89, 0.32, 1.28);
    -webkit-animation-play-state: paused;
            animation-play-state: paused;
}

._h-rc-tooltip-zoom-leave {
    -webkit-animation-duration: 0.3s;
            animation-duration: 0.3s;
    -webkit-animation-fill-mode: both;
            animation-fill-mode: both;
    -webkit-animation-timing-function: cubic-bezier(0.6, -0.3, 0.74, 0.05);
            animation-timing-function: cubic-bezier(0.6, -0.3, 0.74, 0.05);
    -webkit-animation-play-state: paused;
            animation-play-state: paused;
}

._h-rc-tooltip-zoom-enter._h-rc-tooltip-zoom-enter-active,
._h-rc-tooltip-zoom-appear._h-rc-tooltip-zoom-appear-active {
    -webkit-animation-name: _h-rcToolTipZoomIn;
            animation-name: _h-rcToolTipZoomIn;
    -webkit-animation-play-state: running;
            animation-play-state: running;
}

._h-rc-tooltip-zoom-leave._h-rc-tooltip-zoom-leave-active {
    -webkit-animation-name: _h-rcToolTipZoomOut;
            animation-name: _h-rcToolTipZoomOut;
    -webkit-animation-play-state: running;
            animation-play-state: running;
}

@-webkit-keyframes _h-rcToolTipZoomIn {
    0% {
        opacity: 0;
        -webkit-transform-origin: 50% 50%;
                transform-origin: 50% 50%;
        -webkit-transform: scale(0, 0);
                transform: scale(0, 0);
    }
    100% {
        opacity: 1;
        -webkit-transform-origin: 50% 50%;
                transform-origin: 50% 50%;
        -webkit-transform: scale(1, 1);
                transform: scale(1, 1);
    }
}

@keyframes _h-rcToolTipZoomIn {
    0% {
        opacity: 0;
        -webkit-transform-origin: 50% 50%;
                transform-origin: 50% 50%;
        -webkit-transform: scale(0, 0);
                transform: scale(0, 0);
    }
    100% {
        opacity: 1;
        -webkit-transform-origin: 50% 50%;
                transform-origin: 50% 50%;
        -webkit-transform: scale(1, 1);
                transform: scale(1, 1);
    }
}

@-webkit-keyframes _h-rcToolTipZoomOut {
    0% {
        opacity: 1;
        -webkit-transform-origin: 50% 50%;
                transform-origin: 50% 50%;
        -webkit-transform: scale(1, 1);
                transform: scale(1, 1);
    }
    100% {
        opacity: 0;
        -webkit-transform-origin: 50% 50%;
                transform-origin: 50% 50%;
        -webkit-transform: scale(0, 0);
                transform: scale(0, 0);
    }
}

@keyframes _h-rcToolTipZoomOut {
    0% {
        opacity: 1;
        -webkit-transform-origin: 50% 50%;
                transform-origin: 50% 50%;
        -webkit-transform: scale(1, 1);
                transform: scale(1, 1);
    }
    100% {
        opacity: 0;
        -webkit-transform-origin: 50% 50%;
                transform-origin: 50% 50%;
        -webkit-transform: scale(0, 0);
                transform: scale(0, 0);
    }
}

