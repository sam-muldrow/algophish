      <section class="hp--megaban relative" id="megaban_container">
        <div id="slider-img-skeleton" class="hp--megaban--content absolute">
            <img data-nolazy class="hp--megaban--img relative db-ns dn" src="https://fr.shopping.rakuten.com/visuels/0_merch/megaban/desk-skeleton.png" alt="megaban homepage rakuten">
            <img data-nolazy class="hp--megaban--img relative db dn-ns" src="https://fr.shopping.rakuten.com/visuels/0_merch/megaban/mob-skeleton.png" alt="megaban homepage rakuten">
