/* Headings modifiers

 To display text as headings.

 .heading_1     - heading 1
 .heading_2     - heading 2
 .heading_3     - heading 3
 .heading_4     - heading 4
 .heading_5     - heading 5
 .heading_6     - heading 6

Markup:
<p class="{{modifier_class}}">P tags with .{{modifier_class}} class</p>
*/

h1,
._h-heading_4 {
    font-weight: bold;
}

h1 {
    line-height: 2.6rem;
    font-size: 2rem;
}

._h-heading_4 {
    line-height: 1.8rem;
    font-size: 1.4rem;
}

/* additionnal commons markup */

._h-text {
    color: #4D4D4D;
    color: var(--text);
}

/* should be use if flex must be hide in a conditionnal way, because of organisations of css. */

/* flex helper */

/* flex helper */

._h-helper_flex-occupy {
    -webkit-box-flex: 1;
    -webkit-flex-grow: 1;
            flex-grow: 1;
}

/* When showing element depending from a breakpoint, that mean by default it's hidden */

._h-helper_show-BelowDesktop {
    display: none;
}

._h-helper_hide-BelowPhone {
    display: inherit;
}

._h-helper_hide-BelowTablet {
    display: inherit;
}

/*
    Rappels :
    --phone: 320px;
    --landscape: 576px;
    --tablet: 768px;
    --desktop: 992px;
    --wide: 1200px;
    --max: 1492px;
*/

@media screen and (max-width: 75em) {
    ._h-helper_hide-BelowWide {
        display: none ;
    }
}

@media screen and (max-width: 93.25em) {
    ._h-helper_hide-BelowMax {
        display: none ;
    }
}

@media screen and (max-width: 62em) {
    ._h-helper_show-BelowDesktop {
        display: inherit;
    }

    ._h-helper_hide-BelowDesktop {
        display: none !important;
    }
}

@media screen and (max-width: 36em) {
    ._h-helper_hide-BelowLandscape {
        display: none !important;
    }
}

@media screen and (max-width: 20em) {
    ._h-helper_hide-BelowPhone {
        display: none !important;
    }

    header {
        padding-top: 1.2rem;
    }
}

@media screen and (min-width: 48em) {
    ._h-helper_hide-AboveTablet {
        display: none !important;
    }
}

/* use this to hide strictly after breakpoint value */

@media screen and (max-width: 48em) {
    ._h-helper_hide-BelowTablet {
        display: none !important;
    }
}

