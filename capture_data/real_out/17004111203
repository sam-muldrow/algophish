/* Variables */

:root {
      --spacing-none: 0;
      --spacing-extra-small: .25rem;
      --spacing-small: .5rem;
      --spacing-medium: 1rem;
      --spacing-large: 2rem;
      --spacing-extra-large: 4rem;
    }

/*
       SPACING
       Docs: http://tachyons.io/docs/layout/spacing/

       An eight step powers of two scale ranging from 0 to 16rem.

       Base:
         p = padding
         m = margin

       Modifiers:
         a = all
         h = horizontal
         v = vertical
         t = top
         r = right
         b = bottom
         l = left

         0 = none
         1 = 1st step in spacing scale
         2 = 2nd step in spacing scale
         3 = 3rd step in spacing scale
         4 = 4th step in spacing scale
         5 = 5th step in spacing scale
         6 = 6th step in spacing scale
         7 = 7th step in spacing scale

       Media Query Extensions:
         -ns = not-small
         -m  = medium
         -l  = large

    */

._h-pa0 { padding: 0; padding: var(--spacing-none); }

._h-pa3 { padding: 1rem; padding: var(--spacing-medium); }

._h-pa4 { padding: 2rem; padding: var(--spacing-large); }

._h-pl0 { padding-left: 0; padding-left: var(--spacing-none); }

._h-pl1 { padding-left: .25rem; padding-left: var(--spacing-extra-small); }

._h-pl2 { padding-left: .5rem; padding-left: var(--spacing-small); }

._h-pl3 { padding-left: 1rem; padding-left: var(--spacing-medium); }

._h-pl4 { padding-left: 2rem; padding-left: var(--spacing-large); }

._h-pl5 { padding-left: 4rem; padding-left: var(--spacing-extra-large); }

._h-pr3 { padding-right: 1rem; padding-right: var(--spacing-medium); }

._h-pr4 { padding-right: 2rem; padding-right: var(--spacing-large); }

._h-pb1 { padding-bottom: .25rem; padding-bottom: var(--spacing-extra-small); }

._h-pb2 { padding-bottom: .5rem; padding-bottom: var(--spacing-small); }

._h-pb3 { padding-bottom: 1rem; padding-bottom: var(--spacing-medium); }

._h-pb4 { padding-bottom: 2rem; padding-bottom: var(--spacing-large); }

._h-pt1 { padding-top: .25rem; padding-top: var(--spacing-extra-small); }

._h-pt2 { padding-top: .5rem; padding-top: var(--spacing-small); }

._h-pt3 { padding-top: 1rem; padding-top: var(--spacing-medium); }

._h-pt4 { padding-top: 2rem; padding-top: var(--spacing-large); }

._h-pv3 {
      padding-top: 1rem;
      padding-top: var(--spacing-medium);
      padding-bottom: 1rem;
      padding-bottom: var(--spacing-medium);
    }

._h-ph4 {
      padding-left: 2rem;
      padding-left: var(--spacing-large);
      padding-right: 2rem;
      padding-right: var(--spacing-large);
    }

._h-ma0  {  margin: 0;  margin: var(--spacing-none); }

._h-ma4  {  margin: 2rem;  margin: var(--spacing-large); }

._h-ml1 {  margin-left: .25rem;  margin-left: var(--spacing-extra-small); }

._h-ml2  {  margin-left: .5rem;  margin-left: var(--spacing-small); }

._h-ml4  {  margin-left: 2rem;  margin-left: var(--spacing-large); }

._h-mr2  {  margin-right: .5rem;  margin-right: var(--spacing-small); }

._h-mr3  {  margin-right: 1rem;  margin-right: var(--spacing-medium); }

._h-mb2  {  margin-bottom: .5rem;  margin-bottom: var(--spacing-small); }

._h-mb3  {  margin-bottom: 1rem;  margin-bottom: var(--spacing-medium); }

._h-mb4  {  margin-bottom: 2rem;  margin-bottom: var(--spacing-large); }

._h-mt2  {  margin-top: .5rem;  margin-top: var(--spacing-small); }

._h-mt3  {  margin-top: 1rem;  margin-top: var(--spacing-medium); }

._h-mv0   {
      margin-top: 0;
      margin-top: var(--spacing-none);
      margin-bottom: 0;
      margin-bottom: var(--spacing-none);
    }

._h-mv2   {
      margin-top: .5rem;
      margin-top: var(--spacing-small);
      margin-bottom: .5rem;
      margin-bottom: var(--spacing-small);
    }

._h-mv4   {
      margin-top: 2rem;
      margin-top: var(--spacing-large);
      margin-bottom: 2rem;
      margin-bottom: var(--spacing-large);
    }

._h-mh4   {
      margin-left: 2rem;
      margin-left: var(--spacing-large);
      margin-right: 2rem;
      margin-right: var(--spacing-large);
    }

/*
   NEGATIVE MARGINS

   Base:
     n = negative

   Modifiers:
     a = all
     t = top
     r = right
     b = bottom
     l = left

     1 = 1st step in spacing scale
     2 = 2nd step in spacing scale
     3 = 3rd step in spacing scale
     4 = 4th step in spacing scale
     5 = 5th step in spacing scale
     6 = 6th step in spacing scale
     7 = 7th step in spacing scale

   Media Query Extensions:
     -ns = not-small
     -m  = medium
     -l  = large

*/

