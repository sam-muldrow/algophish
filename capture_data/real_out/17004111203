	.hp-reco--suggestions .hp-reco {
		margin: auto;
	}
	.hp-reco .slick-slider {
		max-height: 370px;
	}
	@media all and (max-width: 992px) {
		.hp-reco--suggestions {
		border-top-right-radius: 0;
		border-bottom-right-radius: 0;
	}	
	}
</style>

