    ======================================
     Extra z-index
    ======================================
    */

._h-z-10 { z-index: 10; }

/*
    ======================================
     screen reader only
    ======================================
    */

._h-sr-only {
    position: absolute;
    width: 1px;
    height: 1px;
    padding: 0;
    margin: -1px;
    overflow: hidden;
    clip: rect(0, 0, 0, 0);
    border: 0;
}

img {
    max-width: 100%;
}

