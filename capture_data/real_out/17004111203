}

.rc-tooltip .rc-tooltip-arrow {
  position: relative;
  width: 18px;
  height: 10px;
  overflow: hidden;
  top: 2px;
  margin-left: 1px;
  border: 0;
}

.rc-tooltip-placement-bottomRight .rc-tooltip-arrow {
  top: -8px !important;
}

.rc-tooltip-placement-bottomRight .rc-tooltip-arrow {
  left: auto;
  float: right;
  right: 20%;
}

/* dark arrow */

.rc-tooltip .rc-tooltip-arrow::before {
  position: absolute;
  content: '';
  width: 0;
  height: 0;
  left: -9px;
  top: 0;
  border-left: 18px solid transparent;
  border-right: 18px solid transparent;
  border-bottom: 18px solid #999;
  border-bottom: 18px solid var(--greyTooltip);
}

/* White arrow */

.rc-tooltip .rc-tooltip-arrow::after {
  position: absolute;
  content: '';
  width: 0;
  height: 0;
  top: 3px;
  left: 2px;
  border-left: 7px solid transparent;
  border-right: 7px solid transparent;
  border-bottom: 7px solid white;
}

.rc-tooltip-inner {
  padding: 0;
  border: 2px solid #999;
  border: 2px solid var(--greyTooltip);

  /* box-shadow: 0 0 90px rgba(150, 150, 150, 0.3); */
  box-shadow: 10px 10px 50px 0 rgba(0, 0, 0, 0.05);
}

