        function setElementAttributesWithinJwPlayer() {
            try {
                if (document.readyState !== 'complete') {
                    return;
                }

                var URL = window.location.href;
                var allIcons = document.getElementsByClassName('jw-svg-icon');

                // #region ALL jw-svg-icon ELEMENTS - APPLY ALT ATTRIBUTE DESCRIPTIVE TO EACH ICON

                for (var iIndex = 0; iIndex < allIcons.length; iIndex++) {

                    var svgElementClassNames = allIcons[iIndex].className.baseVal;

                    if (svgElementClassNames === undefined || svgElementClassNames === null) {
                        continue;
                    }

                    if (svgElementClassNames.includes('jw-svg-icon-close')) {
                        allIcons[iIndex].setAttribute('alt', 'Close');
                    }
                    if (svgElementClassNames.includes('jw-svg-icon-rewind')) {
                        allIcons[iIndex].setAttribute('alt', 'Rewind');
                    }
                    if (svgElementClassNames.includes('jw-svg-icon-buffer')) {
                        allIcons[iIndex].setAttribute('alt', 'Buffer');
                    }
                    if (svgElementClassNames.includes('jw-svg-icon-replay')) {
                        allIcons[iIndex].setAttribute('alt', 'Replay');
                    }
                    if (svgElementClassNames.includes('jw-svg-icon-play')) {
                        allIcons[iIndex].setAttribute('alt', 'Play');
                    }
                    if (svgElementClassNames.includes('jw-svg-icon-pause')) {
                        allIcons[iIndex].setAttribute('alt', 'Pause');
                    }
                    if (svgElementClassNames.includes('jw-svg-icon-next')) {
                        allIcons[iIndex].setAttribute('alt', 'Next');
                    }
                    if (svgElementClassNames.includes('jw-svg-icon-arrow-left')) {
                        allIcons[iIndex].setAttribute('alt', 'Arrow Left');
                    }
                    if (svgElementClassNames.includes('jw-svg-icon-arrow-right')) {
                        allIcons[iIndex].setAttribute('alt', 'Arrow Right');
                    }
                    if (svgElementClassNames.includes('jw-svg-icon-quality-100')) {
                        allIcons[iIndex].setAttribute('alt', 'Quality 100');
                    }
                    if (svgElementClassNames.includes('jw-svg-icon-sharing')) {
                        allIcons[iIndex].setAttribute('alt', 'Share');
                    }
                    if (svgElementClassNames.includes('jw-svg-icon-facebook')) {
                        allIcons[iIndex].setAttribute('alt', 'Facebook');
                    }
                    if (svgElementClassNames.includes('jw-svg-icon-twitter')) {
                        allIcons[iIndex].setAttribute('alt', 'Twitter');
                    }
                    if (svgElementClassNames.includes('jw-svg-icon-tumblr')) {
                        allIcons[iIndex].setAttribute('alt', 'Tumbler');
                    }
                    if (svgElementClassNames.includes('jw-svg-icon-link')) {
                        allIcons[iIndex].setAttribute('alt', 'Link');
                    }
                    if (svgElementClassNames.includes('jw-svg-icon-embed')) {
                        allIcons[iIndex].setAttribute('alt', 'embed');
                    }
                    if (svgElementClassNames.includes('jw-svg-icon-stop')) {
                        allIcons[iIndex].setAttribute('alt', 'Stop');
                    }
                    if (svgElementClassNames.includes('jw-svg-icon-volume-0')) {
                        allIcons[iIndex].setAttribute('alt', 'Volume 0');
                    }
                    if (svgElementClassNames.includes('jw-svg-icon-volume-50')) {
                        allIcons[iIndex].setAttribute('alt', 'Volume 50');
                    }
                    if (svgElementClassNames.includes('jw-svg-icon-volume-100')) {
                        allIcons[iIndex].setAttribute('alt', 'Volume 100');
                    }
                    if (svgElementClassNames.includes('jw-svg-icon-cc-on')) {
                        allIcons[iIndex].setAttribute('alt', 'Closed Captions On');
                    }
                    if (svgElementClassNames.includes('jw-svg-icon-cc-off')) {
                        allIcons[iIndex].setAttribute('alt', 'Closed Captions Off');
                    }
                    if (svgElementClassNames.includes('jw-svg-icon-settings')) {
                        allIcons[iIndex].setAttribute('alt', 'Settings');
                    }
                    if (svgElementClassNames.includes('jw-svg-icon-fullscreen-off')) {
                        allIcons[iIndex].setAttribute('alt', 'Fullscreen Off');
                    }
                    if (svgElementClassNames.includes('jw-svg-icon-fullscreen-on')) {
                        allIcons[iIndex].setAttribute('alt', 'Fullscreen On');
                    }

                    //console.log('' + svgElementClassNames);
                }
                // #endregion

                if (URL.includes('shophq') == false) {
                    //console.log('JW Player SVG Elements Updated: ' + allIcons.length);
                }
            }
            catch
            {
                //console.log('Unable to update JW player svg elements');
            }

            try {
                // #region jw-volume-update - APPLY CONTRAST UPDATE
                var volumeContrastElement = document.getElementsByClassName('jw-volume-update')[0];
                volumeContrastElement.style.color = "#757575";

                if (URL.includes('shophq') == false) {
                    //console.log('JW Player Contrast Updated.');
                }
                // #endregion
            }
            catch
            {
                //console.log('Unable to update JW player contrast elements');
            }

        }

        document.addEventListener("readystatechange", setElementAttributesWithinJwPlayer);

    </script>

