    <script type="text/javascript">
   var productUrlStart =
        "/Product/topLine/offerCode";
    var onAirUrl =
            "/onair/OnToday#content";

        $(document).ready(function () {
            var animationEnd = 'webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend';
            //Get last 12 OnAir items and current OnAir
            var owl;
            if ($("#last-items-aired-carousel").length !== 0) {
                owl = $('#last-items-aired-carousel');
                owl.owlCarousel({
                    items: 3,
                margin: 11,
                dots: false,
                responsive: {
                        0: {
                            items: 2
                        },
                    600: {
                            items: 3
                    }
                    }
                });
            }

            //alert('1');
        $.ajax({
                type: "GET",
            async: false,
            url: "/HomePageOnAirItems"
        }).done(function (data) {
                if (data !== null && data !== undefined && data.length > 0) {
                    var pageNum = data.length - 1;
                    var dataprodposition=0;
                    var onAirfound = false;

                $.each(data,
                    function (index, offer) {
                        addRecentlyAirItem(offer, pageNum, index);
                        if (offer.IsOnAirNow) {
                            //alert(offer.TopLine + ":" + offer.RetailPrice.DisplayPrice);
                            $('#hdnretailprice').val(offer.RetailPrice.DisplayPrice);
                            onAirfound=true;
                            updateCurrentOnAirItem(offer);
                            updateCurrentOnAirItemOverlay(offer);
                            //alert($('#hdnretailprice').val());
                        } else {
                            var offerLink = getOfferLink(pageNum, offer.ShowName, offer.OfferCode, offer.TopLine);
                            if(onAirfound)
                            {
                                dataprodposition=(data.length -index);
                            }
                            else
                            {
                                dataprodposition=((data.length-1) - index);
                            }

                            addItemToLastTwelve(offerLink, offer.OfferCode,offer.TopLine, offer.ImageUrl,index,dataprodposition);
                            pageNum -= 1;
                        }
                    });
                    $("#on-air-container").show();
                    $("#homeItemsOnAir").show();
                    if (owl !== null && owl !== undefined) {
                        owl.trigger('refresh.owl.carousel');
                    }

                }

            }).fail(function(xhr, textStatus, errorThrown) {
                if (console)
                    console.log(errorThrown);
            });

        $('#last-item-carousel-next').click(function() {
                if (owl !== null && owl !== undefined) {
                    owl.trigger('next.owl.carousel');
                }
            });

        $('#last-item-carousel-prev').click(function() {
                if (owl !== null && owl !== undefined) {
                    owl.trigger('prev.owl.carousel');
                }
            });


        //Get homepage overlay
        $.ajax({
                async: false,
            url: "/HomePageOverlay",
            cache: false
        })
            .done(function(data) {
                if (data != '') {
                    // Setup Home Page Overlay dialog
                    $("#homepage-overlay-body").html(data);
                    $("#homepage-overlay-dialog").modal("show");
                }
            })
            .fail(function(xhr, textStatus, errorThrown) {
                if (console)
                    console.log(errorThrown);
            });


                    var watchUsLive = $.connection.onAirHub;
                    $.extend(watchUsLive.client,            {
                                HomepageOnAirProductUpdate: function(offer)
                                    {
                                    onCurrentOnAirChanged(offer);
                                    }
                                });
                    var configOptions = {};
                            if (shopHq.pageUtils.isIPad())
                                {
                                    configOptions = { transport: 'longPolling' };
                                }
                    $.connection.hub.start(configOptions);




            function onCurrentOnAirChanged(onAirItemData) {
                //alert('onair');
                var currentOnAirItem = $('#on-air-product-container');
                if (currentOnAirItem !== null &&
                    currentOnAirItem !== undefined &&
                    parseInt(currentOnAirItem.attr("data-offer-id")) !== onAirItemData.OfferId) {
                    //Add On Air item to last 12
                    var offerLink = currentOnAirItem.find(".on-air-product-img-link").attr("href");
                    var offerCode = currentOnAirItem.find(".on-air-product-img-link img").attr("alt");
                    var imgUrl = currentOnAirItem.find(".on-air-product-img-link img").attr("src");
                    addItemToLastTwelve(offerLink, offerCode,onAirItemData.TopLine, imgUrl, 0,1);

                    if ($("#on-air-products-container .on-air-product .on-air-now").length >= 1) {
                        $("#on-air-products-container .on-air-product .on-air-now").remove();
                    }


                    updateLinks($("#last-items-aired-carousel .item a"));

                    updateLinks($("#on-air-products-container .quickview-hovereffect a"));
                    updateLinks($("#on-air-products-container .on-air-product-description a"));
                    //Update current on air item
                    updateCurrentOnAirItem(onAirItemData);
                    addRecentlyAirItem(onAirItemData, 0, "OnAir");
                    updateCurrentOnAirItemOverlay(onAirItemData);
                    //Remove last item from list
                    if (owl !== null && owl !== undefined) {
                        owl.trigger('remove.owl.carousel', 12);
                        owl.trigger('refresh.owl.carousel');
                    }
                }

            }


            function updateCurrentOnAirItem(offer) {
                //alert('2');
                if (!offer.OfferFound) {
                    return;
                }
                $("#on-air-product-container .line-class-overlay").attr("class", "line-class-overlay");

                var offerLink = getOfferLink(0, offer.ShowName, offer.OfferCode, offer.TopLine);
                $("#onAirShow").html(offer.ShowName);
                $("#onAirShow").attr("href", onAirUrl);
                $("#on-air-product-container").attr("data-offer-id", offer.OfferId);
                $("#on-air-product-container .on-air-product-img-link").attr("href", offerLink);
                $("#on-air-product-learn-more-link").attr("href", offerLink);

                $("#currentOnAirProductContainer").attr("data-offercode", offer.OfferCode);
                $("#currentOnAirProductContainer").attr("data-topline", offer.TopLine);

                $("#on-air-product-container .on-air-product-img-link img").attr("alt", "Offer "+offer.OfferCode);
                $("#on-air-product-container .on-air-product-img-link img").attr("src", offer.ImageUrl);
                $("#on-air-product-quick-view").attr("data-offercode", offer.OfferCode);
                $("#on-air-product-quick-view").attr("aria-label", "Offer "+offer.OfferCode);
                $("#on-air-product-quick-view").attr("id", "QuickView_"+offer.OfferCode);
                $("#on-air-product-container .on-air-product-description a").html(offer.TopLine);
                $("#on-air-product-container .on-air-product-description a").attr("href", offerLink);

                if ($("#on-air-product-container .on-air-product-details .on-air-product-price strong").length > 0) {
                    $("#on-air-product-container .on-air-product-details .on-air-product-price strong:eq(0)").html(offer.ListedPrice.Label);
                    $("#on-air-product-container .on-air-product-details .on-air-product-price strong:eq(1)").html(offer.ListedPrice.DisplayPrice);
                }

                $("#on-air-product-container .on-air-product-img-link").attr("href", offerLink);
                $("#on-air-product-container .line-class-overlay").addClass(offer.ImageOverlay);
            }

            function updateCurrentOnAirItemOverlay(offer) {
                //alert('2');
                if (!offer.OfferFound) {
                    return;
                }
                $("#on-air-product-overlay .line-class-overlay").attr("class", "line-class-overlay");

                $("#on-air-product-overlay .offer-availability").removeClass("offer-availability-amlost-gone").removeClass("offer-availability-sold-out");

                var offerLink = getOfferLink(0, offer.ShowName, offer.OfferCode, offer.TopLine);

                $("#on-air-product-overlay .on-air-product-img img").attr("alt", "Offer "+offer.OfferCode);
                $("#on-air-product-overlay .on-air-product-img img").attr("src", offer.ImageUrl);

                $("#on-air-product-overlay .on-air-product-description a").html(offer.TopLine);
                $("#on-air-product-overlay .on-air-product-description a").attr("href", offerLink);

                $("#on-air-product-overlay .on-air-product-img-link").attr("href", offerLink);
                $("#on-air-product-overlay").attr("data-offercode", offer.OfferCode);
                $("#on-air-product-overlay").attr("data-topline", offer.TopLine);
                if (offer.ListedPrice
                    .Low !==
                    offer.RetailPrice.Low &&
                    offer.ListedPrice.High !== offer.RetailPrice.High) {
                    $("#on-air-product-overlay .on-air-product-retail-price").removeClass("hidden");
                    $("#on-air-product-overlay .on-air-product-retail-price span:eq(0)").html(offer.RetailPrice.Label);
                    $("#on-air-product-overlay .on-air-product-retail-price span:eq(1)").html(offer.RetailPrice.DisplayPrice);
                } else {
                    $("#on-air-product-overlay .on-air-product-retail-price").addClass("hidden");
                }
                $("#on-air-product-overlay .on-air-product-list-price strong:eq(0)").html(offer.ListedPrice.Label);
                $("#on-air-product-overlay .on-air-product-list-price strong:eq(1)").html(offer.ListedPrice.DisplayPrice);
                if (offer.PaymentTerm > 1) {
                    $("#on-air-product-overlay .on-air-product-vp-price span:eq(0)").html(offer.PaymentTerm);
                    $("#on-air-product-overlay .on-air-product-vp-price span:eq(1)").html(offer.ValuePayPrice.DisplayPrice);
                } else {
                    $("#on-air-product-overlay .on-air-product-vp-price").hide();
                }

                $("#on-air-product-overlay .on-air-product-shipping-price span").html(offer.ShippingPrice.DisplayPrice);

                $("#on-air-buy-now-btn").attr("data-offercode", offer.OfferCode);
                $("#on-air-mobile-buy-now-btn").attr("data-offercode", offer.OfferCode);

                $("#on-air-product-overlay .line-class-overlay").addClass(offer.ImageOverlay);

                $("#on-air-product-overlay .offer-availability").addClass(offer.AvailabilityStatusCssClass);

                $("#on-air-product-overlay").addClass("animated pulse").one(animationEnd,
                             function() {
                                 $(this).removeClass('animated pulse');
                             });

            }
            function slugify(str) {
                //alert('3');
            if (str !== undefined && str !== null) {
                str = str.replace(/^\s+|\s+$/g, ''); // trim
                str = str.toLowerCase();

                // remove accents, swap √± for n, etc
                var from = "√£√†√°√§√¢·∫Ω√®√©√´√™√¨√≠√Ø√Æ√µ√≤√≥√∂√¥√π√∫√º√ª√±√ß¬∑/_,:;";
                var to = "aaaaaeeeeeiiiiooooouuuunc------";
                for (var i = 0, l = from.length; i < l; i++) {
                    str = str.replace(new RegExp(from.charAt(i), 'g'), to.charAt(i));
                }

                str = str.replace(/[^a-z0-9 -]/g, '') // remove invalid chars
                    .replace(/\s+/g, '-') // collapse whitespace and replace by -
                    .replace(/-+/g, '-'); // collapse dashes

            }
            return str;
        }
            function addItemToLastTwelve(offerLink, offerCode, TopLine, imageUrl, index, dataproductposition) {
                //alert('4');
                /* SO-179 : Add product tracking (OnToday - LAST 12 ITEMS AIRED) */
                var html = '<div class="item"><div id="qv12-heffect-'+offerCode+'" class="quickview-hovereffect prodTrack" data-offercode="'+offerCode+'" data-topline="'+TopLine.replace(/"/g,"&quot;")+'" data-productposition="'+dataproductposition+'" data-productList="RecentlyAired"><a href="' +
                    offerLink +/*SO-131 : Homepage: Add aria-label attribute to Quickview hover*/
                    '" ><img  class="img-responsive" alt="Offer ' +
                    offerCode +
                    '" src="' +
                    imageUrl +
                   /*SO-132 :Homepage: Quickview buttons need unique ID attributes*/
                    '" loading="lazy"></a><div class="quickview-overlay" id="quickview__'+offerCode+'"><a class="addQuickViewLink" tabindex="-1" data-toggle="modal" data-offercode="' +
                    offerCode +
                    '" aria-label="Offer '+offerCode+'" href="#shophq">QUICKVIEW</a></div></div></div>';
                if (owl !== null && owl !== undefined) {
                    owl.trigger('add.owl.carousel', [html, index]);

                }
            }

            function addRecentlyAirItem(offer, pageNum, index) {
                //alert('5');
                var onAirHtml = '';
                var priceHtml = '';
                var offerLink = getOfferLink(pageNum, offer.ShowName, offer.OfferCode, offer.TopLine);
                if (offer.IsOnAirNow) {
                    onAirHtml =
                        '<div class="on-air-now text-center"><em class="fa fa-video-camera pr-5" aria-hidden="true"></em>On Air</div>';
                }
                //alert('1234');
                if (offer.ListedPrice
                    .Low !==
                    offer.RetailPrice.Low &&
                    offer.ListedPrice.High !== offer.RetailPrice.High) {
                    priceHtml = '<strong>' + offer.ListedPrice.DisplayPrice + '</strong>&nbsp;&nbsp;&nbsp;&nbsp;<del>' + offer.RetailPrice.DisplayPrice + '</del>';
                } else {
                    priceHtml = '<strong>' + offer.ListedPrice.DisplayPrice + '</strong>';
                }
                var dataProductposition=pageNum+1;
                var html =
                    '<div class="col-xs-6 col-sm-3 my-20 on-air-product prodTrack" data-offercode="'+offer.OfferCode+'" data-topline="'+offer.TopLine+'" data-productposition="'+dataProductposition+'" data-productList="RecentlyAired">' +
                        onAirHtml +
                        '<a href="' +
                        offerLink +/* SO-179 : Add product tracking (OnToday - LAST 12 ITEMS AIRED) SO-131 : Homepage: Add aria-label attribute to Quickview hover*/
                        '"><div id="qv-heffect-'+offer.OfferCode+'-'+ index +'" class="quickview-hovereffect"><div class="line-class-overlay ' +
                        offer.ImageOverlay +
                        '"></div><img  class="img-responsive" alt="Offer ' +
                        offer.OfferCode +
                        '" src="' +
                        offer.ImageUrl +/*SO-132 :Homepage: Quickview buttons need unique ID attributes*/
                    '"></a><div class="quickview-overlay" id="quickview_' + offer.OfferCode + '-' + index +'"><a class="addQuickViewLink" tabindex="-1" data-toggle="modal" data-offercode="' +
                        offer.OfferCode +
                        '" aria-label="Offer '+offer.OfferCode+'" href="#shophq">QUICKVIEW</a></div></div>' +
                        '<div class="offer-availability ' +
                        offer.AvailabilityStatusCssClass +
                        ' my-15"><div></div></div>' +
                        '<div class="on-air-product-description my-10 px-5 ellipsis-2-line "><a href="' +
                        offerLink +
                        '">' +
                        offer.TopLine +
                        '</a></div>' +
                        '<div class="on-air-product-price my-5 px-5">' + priceHtml + '</div>' +
                        '<div class="on-air-product-vp-price my-5 px-5 evine-green">' +
                        ((offer.PaymentTerm > 1)
                            ? '<span>' +
                            offer.PaymentTerm +
                            '</span>&nbsp;ValuePay of&nbsp;<span>' +
                            offer.ValuePayPrice.DisplayPrice +
                            '</span>'
                            : '') +
                        '</div>' +
                        '<div class="my-5"><button type="button" class="btn btn-primary on-air-product-buy-now" data-offercode="' +
                        offer.OfferCode +
                        '">Buy Now</button></div>' +
                        '</div>';
                $("#on-air-products-container").prepend(html);
                if ($("#on-air-products-container .on-air-product").length > 12) {
                    $("#on-air-products-container .on-air-product:first").addClass("animated pulse").one(animationEnd,
                                function() {
                                    $(this).removeClass('animated pulse');
                                });
                    $("#on-air-products-container .on-air-product:eq(12)").remove();
                }

            }

            function getOfferLink(pageNum, showName, offerCode, topLine) {
                //alert('6');
            var baseUrl = productUrlStart.replace("offerCode", offerCode).replace("topLine", slugify(topLine));
                var offerLink = "";
                var query = "";
                if (pageNum !== null && pageNum !== undefined && pageNum <= 0) {
                    query = 'OnAirNowPg-' + showName + offerCode;
                    offerLink = baseUrl + '?icid=' + query + '&cm_re=' + query;
                } else {
                    query = 'OnAirLastPg' + pageNum + '-' + showName + '-' + offerCode;
                    offerLink = baseUrl + '?icid=' + query + '&cm_re=' + query;
                }
                return offerLink;
            }

            function updateLinks(links) {
                //alert('7');
                //Now increment all the indicies - each item needs to move over by one (and the last one drops off)
                $.each(links,
                    function(index, link) {
                        if (link.href.indexOf("OnAirNowPg") > -1) {
                            var pattern = /(.*OnAirNowPg)(.*OnAirNowPg)(.*)/;
                            link.href = link.href.replace(pattern,
                                function(match, beginning, middle, end) {
                                    return beginning.replace("OnAirNowPg", "OnAirLastPg1") +
                                        middle.replace("OnAirNowPg", "OnAirLastPg1") +
                                        end;
                                });
                        } else {
                            var pattern = /(.*OnAirLastPg)([0-9]+)(.*OnAirLastPg)([0-9]+)(.*)/;
                            link.href = link.href.replace(pattern,
                                function(match, beginning, pageNumber, middle, pageNumber2, end) {
                                    var pageNum = parseInt(pageNumber) + 1;
                                    return beginning + pageNum + middle + pageNum + end;
                                });
                        }
                    });

            }

            $(document).on("click",
                ".on-air-product-buy-now",
                function(e) {
                    //Prevent submission of post data before ajax success
                    e.preventDefault();
                    var offercode = $(this).attr("data-offercode");
                    shopHq.quickview.viewModel.load(offercode);
                });



            $(document).on("click",
                "#on-air-close-overlay",
                function(e) {
                    //Prevent submission of post data before ajax success
                    e.preventDefault();
                    $('#on-air-product-overlay').addClass('animated fadeOutLeft').one(animationEnd,
                        function() {
                            $('#on-air-product-overlay').hide();
                            $('#on-air-product-overlay').removeClass('animated fadeOutLeft');
                            $('#on-air-expand-overlay').show();
                            $('#on-air-expand-overlay').addClass('animated fadeInLeft').one(animationEnd,
                                function() {
                                    $('#on-air-expand-overlay').removeClass('animated fadeInLeft');
                                });
                        });
                });
            $(document).on("click",
                "#on-air-expand-overlay",
                function(e) {
                    //Prevent submission of post data before ajax success
                    e.preventDefault();
                    $('#on-air-expand-overlay').addClass('animated fadeOutLeft').one(animationEnd,
                        function() {
                            $('#on-air-expand-overlay').hide();
                            $('#on-air-expand-overlay').removeClass('animated fadeOutLeft');
                            $('#on-air-product-overlay').show();
                            $('#on-air-product-overlay').addClass('animated fadeInLeft').one(animationEnd,
                                function() {
                                    $('#on-air-product-overlay').removeClass('animated fadeInLeft');
                                });
                        });

                });

            if ($("#evine-video-player").length > 0 && $("#evine-video-player").is(':visible')) {

                var autoStart = true;
                var eventData = {
                    VideoAutoplay: autoStart,
                    VideoType: "LIVE STREAM",
                    VideoPlayer: "jwplayer"
                }

                var playerConfiguration =
                {
                    playerName: 'evine-video-player',
                    aspectratio: "16:9",
                    width: "100%",
                    autoStart: autoStart,
                    //flashStreamOpenCaption now holds clean hls feed!
                    hlsStream: 'https://aos01-evine.secure.footprint.net/evine/cleanfeed/master.m3u8',
                    isLiveStream: true,
                    eventData: eventData
            };

            //use videoJS with flash fallback to handle IE11 on win7
            if (shopHq.pageUtils.isIE11 && (Hls === undefined || !Hls.isSupported())) {
                $("#evine-video-player").html('<video id="evinehtml5player" autoplay controls playsinline style="width: 100%" class="video-js vjs-default-skin vjs-16-9"></video>');
                playerConfiguration.playerName = 'evinehtml5player';
                shopHq.video.watchLiveInVideoJS(playerConfiguration);
            }
            else if (shopHq.video.useHTML5Player()) {
                $("#evine-video-player").html('<video id="evinehtml5player" autoplay controls playsinline style="width: 100%"></video>');
                playerConfiguration.playerName = 'evinehtml5player';
                shopHq.video.watchLiveInHTML5Player(playerConfiguration);
            } else {
                shopHq.video.watchLive(playerConfiguration);
            }

        }



    });
        
        function getKeyCode(key) {
            //return the key code
            return (key == null) ? event.keyCode : key.keyCode;
        }

        /* for usabiity capture keyboard tabbing */
        $(document).on('keyup', function (eventObj) {
            //tab keycode = 9

            if (getKeyCode(eventObj) == '9') {
                var $el = $(document.activeElement);
                $('.item').find("*").removeClass("keyfocus");
                $('.on-air-product-img-container').find("*").removeClass("keyfocus");
                //remove any current active elements
                if ($el.parent().hasClass('quickview-hovereffect')) {
                    $el.parent().addClass("keyfocus");
                }
            }
        });
    </script>




