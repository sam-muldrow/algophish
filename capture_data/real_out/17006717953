<script>
    var _asynccallcomplete = 0;
    function LoadPromotionView() {
        $.ajax
            ({
                url: '/PromotionView',//"/Home/PromotionView",
                contentType: "application/html; charset=utf-8",
                type: "GET",
                async:true,
                cache: !0,
                datatype: "html",
                success: function (t) {
                    $("#dvPromotionView").html(t)
                    callback()
                },
                error: function () {
                    $("#dvPromotionView").html("PromotionView Not Found")
                    callback()
                }
            })
    }
    function LoadQuickView() {
        $.ajax
            ({
                url: '/QuickView',//"/Home/QuickView",
                contentType: "application/html; charset=utf-8",
                type: "GET",
                cache: !0,
                datatype: "html",
                success: function (t) {
                    $("#dvQuickView").html(t)
                    callback()
                },
                error: function () {
                    $("#dvQuickView").html("QuickView Not Found")
                    callback()
                }
            })
    }

    function callback() {
        //_asynccallcomplete += 1;
        //if (_asynccallcomplete >= 2) {
        //    AfterAsyncFinalLoad();
        //}
    }

    $(document).ready(function () {
        LoadPromotionView();
        LoadQuickView();
    });
</script>


