<script type="text/javascript">


    $("#welcome-header").on('click', function () {
        shopHq.pageUtils.ShowSubMenu();
    });

    $(".myaccountsubmenu").on('click', function () {
        ShowMainMenu();
    });

    function ShowMainMenu() {
        $(".myaccountsubheader").hide();
        $(".myaccountheader").show();
        $(".menuheader").show();
        $(".menuaccountheader").hide();
        $(".myaccountheader").css("transform", "translate(0px, 0px)");
        $(".myaccountsubheader").css("transform", "translate(-100%, 0px)");
        $(".myaccount-leftnav").show();
    }
</script>

            
