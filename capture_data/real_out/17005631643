<script type="text/javascript">
                           if(typeof(EventSource) !== "undefined") {
                               var foldWidgetFirstHeadlineTitleEvent = new EventSource("foldWidgetHeadlineNewsInfo.php");
                               foldWidgetFirstHeadlineTitleEvent.onmessage = function (event) {
                                   eventOrigin = event.origin.replace(/\/+$/,'');
                                   eventBaseUrl = baseUrl.replace(/\/+$/,'');
                                   
                                   if(event.data.length > 0 && eventOrigin == eventBaseUrl){
                                       data = JSON.parse(event.data);
                                       
                                       $.each(data, function(i, val) {
                                           if(val.title.length > 0){
                                               if(typeof $("#headline" + val.id + "Title") !== "undefined"){
                                                   $("#headline" + val.id + "Title a").attr("title",clearHtmlSpecialChars(val.title));
                                                   $("#headline" + val.id + "Title a").text(clearHtmlSpecialChars(val.title));
                                               }
                                               
                                               if(i == 0 && typeof $("#headline" + val.id + "MobileTitle") !== "undefined"){
                                                   $("#headline" + val.id + "MobileTitle a").attr("title",clearHtmlSpecialChars(data[i].title));
                                                   $("#headline" + val.id + "MobileTitle a").text(clearHtmlSpecialChars(data[i].title));
                                               }
                                           }
                                           if(val.description.length > 0){
                                               if(typeof $("#headline" + val.id + "Description") !== "undefined"){
                                                   $("#headline" + val.id + "Description a").attr("title",clearHtmlSpecialChars(data[i].description));
                                                   $("#headline" + val.id + "Description a").text(clearHtmlSpecialChars(data[i].description));
                                               }
                                               
                                               if(i == 0 && typeof $("#headline" + val.id + "MobileDescription") !== "undefined"){
                                                   $("#headline" + val.id + "MobileDescription a").attr("title",clearHtmlSpecialChars(data[i].description));
                                                   $("#headline" + val.id + "MobileDescription a").text(clearHtmlSpecialChars(data[i].description));
                                               }
                                           }
                                           if(i == 0 && val.live_icon.length > 0){
                                               if(val.live_icon == 0){
                                                   if($("#headline" + val.id + "LiveIcon").length == 1){
                                                       $("#headline" + val.id + "LiveIcon").remove();
                                                       if($(".card-contents").html().length == 0){
                                                           $(".card-contents").remove();
                                                       }
                                                   }
                                                   
                                                   if($("#headline" + val.id + "MobileLiveIcon") == 1){
                                                       $("#headline" + val.id + "MobileLiveIcon").remove();
                                                       if($(".mobile-card-contents").html().length == 0){
                                                           $(".mobile-card-contents").remove();
                                                       }
                                                   }
                                               } else {
                                                   if($("#headline" + val.id + "LiveIcon").length == 0){
                                                       if($(".card-contents").length == 0){
                                                           $(".fold-widget-container .fold-widget-wrapper .special-hero-related-card-new .text-frame").append('<div class="card-contents"></div>');
                                                       }
                                                       $(".card-contents").prepend('<a href="https://www.trthaber.com/canli-yayin-izle.html" title="Canlı Yayın" target="_blank" id="headline' + val.id + 'LiveIcon"><div class="card-content"><svg width="20" height="20" class="blink-icon"><circle cx="10" cy="10" r="10" fill="#cf1d39" /></svg><div class="card-content-title"><span style="color: #c31719; font-family: TRT-Bold, sans-serif; margin-right: 5px;">CANLI YAYIN</span></div></div></a>');
                                                   }
                                                   
                                                   if($("#headline" + val.id + "MobileLiveIcon").length == 0){
                                                       if($(".mobile-card-contents").length == 0){
                                                           $(".fold-widget-container .fold-widget-wrapper .special-hero-related-card-new .mobile-text-frame").append('<div class="mobile-card-contents"></div>');
                                                       }
                                                       $(".mobile-card-contents").prepend('<a href="https://www.trthaber.com/canli-yayin-izle.html" title="Canlı Yayın" class="_self" id="headline' + val.id + 'MobileLiveIcon"><div class="mobile-card-content"><svg width="20" height="20" class="blink-icon"><circle cx="10" cy="10" r="10" fill="#cf1d39" /></svg><div class="mobile-card-content-title"><span style="color: #c31719; font-family: TRT-Bold, sans-serif; margin-right: 5px;">CANLI YAYIN</span></div></div></a>');
                                                   }
                                               }
                                           }
                                       });
                                   }
                               };
                           }
