<script type="text/javascript">
    $(document).ready(function () {
        if (getStatsCookie("TRT_HABER_" + statsDeviceType + "_HOMEPAGE").length && getStatsCookie("TRT_HABER_" + statsDeviceType + "_HOMEPAGE") == 1) {
            var isUnique = 0;
        } else {
            var isUnique = 1;
        }
                                
        $.ajax({
            type: "GET",
            url: statsIpUrl,
            success: function (data, textStatus) {
                ipData = $.parseJSON(data);
                ipAddress = ipData.ip;
                
                $.ajax({
                    type: "POST",
                    url: "https://istatistik.trthaber.com/service/api/stats/homepage",
                    data: JSON.stringify({
                        "ipAddress": ipAddress,
                        "site": "TRT_HABER",
                        "title": "Anasayfa",
                        "url": "https%3A%2F%2Fwww.trthaber.com",
                        "source": statsDeviceType,
                        "unique": isUnique
                    }),
                    success: function (data, textStatus) {
                        setStatsCookie("TRT_HABER_" + statsDeviceType + "_HOMEPAGE", 1);
                    }
                });
            }
        });
    });
