<script type="text/javascript">
                                $(document).ready(function() {
                                    if(getCookie("selectedLocalNewsCity")){
                                        var localNewsCity = getCookie("selectedLocalNewsCity");
                                    } else {
                                        var localNewsCity = "ankara";
                                    }
                        
                                    $.ajax({
                                        type: "GET",
                                        url: "/ajax/getLocalNewsWidgetData/" + localNewsCity + "/",
                                        success: function (data, textStatus) {
                                            if(data.length){
                                                $("#localNewsWidgetContainer").html(data);
                                                
                                                if(isMobile.any === true){
                                                    var localNewsUrlTarget = "_self";
                                                } else {
                                                    var localNewsUrlTarget = "_blank";
                                                }
                                            
                                                $("#localNewsWidgetContainer .site-url").attr("target",localNewsUrlTarget);
                                            }
                                        }
                                    });
                                });
                                $(document).on("click","#localNewsWidgetSelectedCity",function(){
                                    $("#localNewsWidgetCityList").toggle();
                                });
                                $(document).on("click","#localNewsWidgetCityList li",function(){
                                    setCookie("selectedLocalNewsCity",$(this).attr("data-val"),1);
                                    var url = "/yerelhaberler/" + $(this).attr("data-val") + "/";
                                    if(isMobile.any === true){
                                        window.open(url,"_self");
                                    } else {
                                        window.open(url,"_blank");
                                    }
                                });
                            </script>
