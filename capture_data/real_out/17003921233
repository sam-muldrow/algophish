                            <script type="text/javascript">
							function countdown(elId, date) {

								var today = new Date();
								var timeLeft = (date.getTime() - today.getTime()) / 1000;

								formatTime(timeLeft);

								var interval = setInterval(function () {
									if (timeLeft < 1) {
										clearInterval(interval);
										element(elId).classes.add('timeover');
									}
									timeLeft -= 1;
									formatTime(timeLeft);
								}, 1000
								)

								function declineMe(n, p1, p4, p2) {
									n = Math.abs(n);
									return ((n == 1) ? p1 : (n >= 2 && n <= 4) ? p4 : p2);
								};

								function formatTime(time) {

									var daysLeft = Math.floor(time / (60 * 60 * 24));
									var hoursLeftM = time % (60 * 60 * 24);
									var hoursLeft = Math.floor(hoursLeftM / (60 * 60));
									var minutesLeftM = hoursLeftM % (60 * 60);
									var minutesLeft = Math.floor(minutesLeftM / (60));

									var labelDny = declineMe(daysLeft, "den", "dny", "dní");
									var labelHodiny = declineMe(hoursLeft, "hodina", "hodiny", "hodin");
									var labelMinuty = declineMe(minutesLeft, "minuta", "minuty", "minut");

									document.getElementById(elId + '-clock').innerHTML = '<span data-label="' + labelDny + '">' + zeroFill(daysLeft, 2) + '</span>'
										+ '<span data-label="' + labelHodiny + '">' + zeroFill(hoursLeft, 2) + '</span>'
										+ '<span data-label="' + labelMinuty + '">' + zeroFill(minutesLeft, 2) + '</span>';
								}

								function zeroFill(number, width) {
									width -= number.toString().length;
									if (width > 0) {
										return new Array(width + (/\./.test(number) ? 2 : 1)).join('0') + number;
									}
									return number + '';
								}

							}

						</script>

						<link rel="stylesheet" href="//1gr.cz/b/css/maga-hrazlato.css">
						<a id="hrazlato" href="https://www.idnes.cz/ekonomika/domaci/megahra-zlate-mince-soutez-idnes-ceska-mincovna.A231005_132251_ekonomika_hig">
							<div id="hrazlato-countdown">
								<h4>ZBÝVÁ:</h4>
								<div class="countdown-clock" id="hrazlato-countdown-clock">
									<span data-label="dny">55</span>
									<span data-label="hodiny">55</span>
									<span data-label="minuty">55</span>

								</div>
							</div>
						</a>
						<script>
							var rok = 2023;
							var mesic = 11;
							var den = 20;
							var hodina = 10;
							var minuta = 0;

							var date = new Date(rok, (mesic - 1), den, hodina, minuta, 0, 0);
							countdown('hrazlato-countdown', date)
						</script>
                            
							
							
