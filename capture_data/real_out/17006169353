   function showAds(showid, i){
       var para = document.createElement("script");
       var divbx = document.createElement("div");
       divbx.id=showid;
       var a = "googletag.cmd.push(function() { googletag.display('"+divbx.id+"'); });";
       var t = document.createTextNode(a);   
       para.appendChild(t);
       divbx.appendChild(para); 
       var widthWindow=$(window).width();  
       if(widthWindow<=1024){
           document.getElementById("mob-target-"+i).appendChild(divbx); }  
       else{
           document.getElementById("web-target-"+i).appendChild(divbx);
       }
   }
   // device
