</div><script>

var searchKey='';
function cls(){
apContent=false;
$('#age').val('');
$('#height').val('');
$('#weight').val('');
$("input:radio[name='radio']")[0].checked=true;
$("input:radio[name='radio']")[1].checked=false;
}

function isNumberKey(evt) {
    var charCode = (evt.which) ? evt.which : event.keyCode;
    if (charCode != 46 && charCode > 31 && (charCode < 48 || charCode > 57)) {
        return false;
    } else {
        return true;
    }      
}

function calculate(){
	var ageVal = $('#age').val();
	var heightVal = $('#height').val();
	var weightVal = $('#weight').val();
	var flag = false;
	if(ageVal>1 && ageVal <101)
	{
		$('#errorId').html('');
		 flag = true;
	}else{
		$('#errorId').html('Please enter the age between 2 - 100 (yr)');	
		return false;
	}
	
	if(weightVal >9 && weightVal <251)
	{
		$('#errorId').html('');
		flag = true;
	}else{
		$('#errorId').html('Please enter the weight between 10 - 250 (kg)');
		return false;
	}
	
	if(heightVal >9 && heightVal <201)
	{
		$('#errorId').html('');
		flag = true;
	}else{
		$('#errorId').html('Please enter the height between 10 - 200 (cm)');	
		return false;
	}	
	if(flag == true){
		$('#errorId').html('');
		var heightm=$('#height').val()/100;
		var score=$('#weight').val()/(heightm*heightm);
		var percentScore=(score*2)+'%';
		score=Math.round(score);		
		if(score<=18.5){
			searchKey='Underweight';
		}else if(score >18.5 && score <=24.9){
			searchKey='Normal';
		}else if(score >24.9 && score <= 33){
			searchKey='Overweight';
		}else{
			searchKey='Obesity';
		}		
		$('#graph').html(score);
		$('#graph').css({left:percentScore});
		$('#detailResult').html('BMI = '+score+' Kg/m2  ('+searchKey+')');
		apContent=false;
		loadmore();
		return true;
	}else{
		return false;
	}
}

</script>
