    function submitEnterSearchFunc(searchInput, locale) {
        var q = document.getElementById(searchInput).value;
        //alert(q);
        if (q == "") {
            alert("Please enter search criteria");
            return false;
        }

        q = replaceWithHyphen(q);
        var searchText = q;
        //alert(locale);
        if (locale == 'hindi') {
            var searchText = q + '-hindi';
            var url = '/search_' + searchText + '_Articles.html';
            //var url = healthPrefix + 'search_' +searchText+ '_Articles.html';
        } else {
            var url = '/search_' + q + '_Articles.html';
        }

        //alert(url);
        //searchField.action = url;
        window.location.href = url;
        //searchForm.submit();
    }

    function replaceWithHyphen(str) {
        str = str.replace(/[^a-zA-Z\u0900-\u097F- 0-9]+/g, ' ');
        str = str.trim();
        return str.replace(/\s+/g, '-');
    }

</script><script>

$(function() {
    var didScroll;
    var lastScrollTop = 0;
    var delta = 10;
    var navbarHeight = $('.header_sticky').outerHeight();
    $(window).scroll(function(event){
        didScroll = true;
    });
    setInterval(function() {
        if (didScroll) {
            hasScrolled();
            didScroll = false;
        }}, 0);
    function hasScrolled() {
        var st = $(this).scrollTop();
        if(Math.abs(lastScrollTop - st) <= delta)
            return;
        if (st > lastScrollTop && st > navbarHeight){
            $('.header_sticky').removeClass('header_sticky').addClass('header_sticky_hide');
        } else {
            if(st + $(window).height() < $(document).height()) {
                $('.header_sticky_hide').removeClass('header_sticky_hide').addClass('header_sticky');
            }
        }    
        lastScrollTop = st;}
    });
    
    $(document).ready(function(){
        $(".ham").click(function(){
            $(".full_n").show();
            $("body").addClass('overflow');
        });
        $(".close_navbar").click(function(){
            $(".full_n").hide();
            $("body").removeClass('overflow');
        });
    });
    $(document).ready(function(){
        $(".searchPopup").click(function(){
            $(".search_popup").show();
            $("body").addClass('overflow');
        });
        $(".close_search").click(function(){
            $(".search_popup").hide();
            $(".full_n").hide();
            $("body").removeClass('overflow');
        });
       $('.dropdown').click(function (e) {
           e.preventDefault();
           e.stopPropagation();
           $(this).toggleClass("active");
           $(this).siblings().removeClass("active");
       });
       $('.dropdown-menu').click(function (e) {
           e.stopPropagation();
       });
       $('body').click(function () {
           $('.dropdown').removeClass("active");
       });
    });
    
