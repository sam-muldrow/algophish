            $('#hz-video li:first').addClass('activeVideoTab');  
                $('#video-list li').click(function() {
                    $('#video-list li').removeClass("active");
                    $(this).addClass("active");
                    $('#hz-video li').removeClass('activeVideoTab');
                    var indexer = $(this).index();
                    $('#hz-video li:eq(' + indexer + ')').addClass('activeVideoTab');      

                $(".hz-video-frame iframe").each(function() { 
                    var src= $(this).attr('src');
                    $(this).attr('src',src);   
                });
            });
