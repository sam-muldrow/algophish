</div><script type="text/javascript">
    
        // on scroll
        var gdprscroll = false;
        $(window).scroll(function() {
            if ($(window).scrollTop() >= 1 && gdprscroll == false) {
                gdprscroll = true;

                setTimeout( function() 
                {
                    //do something special
                    // If the 'hide cookie is not set we show the message
                    if (!readCookie('hide')) 
                    {
                        $('#gdprbx').show();
                    }
                    // Add the event that closes the popup and sets the cookie that tells us to
                    // not show it again until one day has passed.
                    $('.close').click(function() 
                    {
                        $('#gdprbx').hide();
                        createCookie('hide', true, 60);
                        return false;
                    });
                }, 5000);

            }
        });
        // ---
        // And some generic cookie logic
        // ---
        function createCookie(name,value,days) {
          if (days) {
            var date = new Date();
            date.setTime(date.getTime()+(days*24*60*60*1000));
            var expires = "; expires="+date.toGMTString();
          }
          else var expires = "";
          document.cookie = name+"="+value+expires+"; path=/";
        }
        
        function readCookie(name) {
          var nameEQ = name + "=";
          var ca = document.cookie.split(';');
          for(var i=0;i < ca.length;i++) {
            var c = ca[i];
            while (c.charAt(0)==' ') c = c.substring(1,c.length);
            if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
          }
          return null;
        } 
       
    </script><script>
        
            var scrollNot = 0;
            function isInViewport(el) {
                const rect = el.getBoundingClientRect();
                return (
                    rect.top >= 0 &&
                    rect.left >= 0 &&
                    rect.bottom <= (window.innerHeight || document.documentElement.clientHeight) &&
                    rect.right <= (window.innerWidth || document.documentElement.clientWidth)
                );
            }
        
            function imageOnScroll(showTag) {
                var imgEl = document.getElementsByTagName(showTag);
                for (var m = 0; m < imgEl.length; m++) {
                    if (imgEl[m].getAttribute('data-src')) {
                        if (isInViewport(imgEl[m]) == true) {
                            imgEl[m].setAttribute('src', imgEl[m].getAttribute('data-src'));
                            imgEl[m].removeAttribute('data-src');
                        }
                    }
                }
            }
            imageOnScroll('img');
            document.addEventListener('scroll', function() {
                imageOnScroll('img');
                imageOnScroll('iframe');
                imageOnScroll('script');
            });
        
        //image attribute
        document.addEventListener("DOMContentLoaded", function () {
            var imgE2 = document.getElementsByTagName('img');
            for (var n = 0; n < imgE2.length; n++) {
                imgE2[n].setAttribute('width', imgE2[n].offsetWidth);
                imgE2[n].setAttribute('height', '100%');
            }
        });
        $(document).ready(function () {
            //Alt attribute
            $('img').each(function () {
                var attr = $(this).attr('alt');
                if (typeof attr == typeof undefined || attr == false) {
                    var $img = $(this);
                    var filename = $img.attr('src')
                    $img.attr('alt', 'OnlyMyHealth');
                }
            });
            //target attribute
            $('a').each(function () {
                if ($(this).attr('target') == '_blank') {
                    $(this).attr('rel', 'noopener noreferrer');
                }
            })
        });
        
    </script>
