        if (document.cookie.match(new RegExp("NBTOKEN=([^;]+)")) != null) {
           match = document.cookie.match(new RegExp("NBTOKEN=([^;]+)"));
        } else if (document.cookie.match(new RegExp("MBTOKEN=([^;]+)")) != null) {
           match = document.cookie.match(new RegExp("MBTOKEN=([^;]+)"));
        } else if (document.cookie.match(new RegExp("WEBTOKEN=([^;]+)")) != null) {
           match = document.cookie.match(new RegExp("WEBTOKEN=([^;]+)"));
        }
        if (match) {
            crn = match[1];
            dataLayer.visitorCrn = crn;
