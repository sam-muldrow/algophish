        var dataLayer = {};
        dataLayer.page = {};
        var digitalData = {};
        digitalData.page = [];
        digitalData.page.pageinfo = [];
        var pagename = window.location.pathname;
        pagename = pagename.replace("en.html", "website.html");
        pagename = pagename.substring(1, pagename.length - 5);
        pagename = pagename.replace("en/", "website/");
        pagename = pagename.replace("content/kotakcl/", "");
        digitalData.page.pageinfo.pageName = pagename;
        digitalData.page.category = [];
        var category = window.location.pathname.replace("/en", "").split("/")[1];
        if (category) {
            if (category.indexOf(".html") > -1) {
                category = category.substring(0, category.length - 5);
            }
        } else {
            category = "Home";
        }
        digitalData.page.category.primaryCategory = category;
        digitalData.website = [];
        var language;
        if (window.location.pathname.indexOf("/en") === 0 || window.location.pathname.indexOf("content/kotakcl/en") === 0) {
            language = "English";
        } else if (window.location.pathname.indexOf("/hi") === 0 || window.location.pathname.indexOf("content/kotakcl/hi") === 0) {
            language = "Hindi";
        } else {
            language = "Other";
        }
        digitalData.website.language = language;
        digitalData.web = [];
        digitalData.web.identifier = "website";
        dataLayer.page.title = pagename;
        if (window._satellite && _satellite.getVisitorId() && _satellite.getVisitorId().getMarketingCloudVisitorID()) {
            dataLayer.visitorId = _satellite.getVisitorId().getMarketingCloudVisitorID();
        }

