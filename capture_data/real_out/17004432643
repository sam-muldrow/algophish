<!-- BOXU KONIEC --><script>
document.addEventListener("DOMContentLoaded", function() {
  var utmlnknum=-1;
var utmlnkcmp="";
var utmlnkdomain = "https://hashtag.zoznam.sk";
var utmplatform = "desktop";
var utmsource = "zoznam";
var utm2articlesoffset = 2;
var utmlinks = $('.article-list li a[href*="hashtag.zoznam.sk"], #feed-most_read li a.article-top[href*="hashtag.zoznam.sk"], #feed-most_read li a.article-title[href*="hashtag.zoznam.sk"]');

utmlinks.each(function(index,el) { 
if (index < utmlinks.length - utm2articlesoffset) if (utmlinks.eq(index + utm2articlesoffset).attr('href').indexOf(utmlnkdomain)>-1 && $(this).attr('href').indexOf(utmlnkdomain)>-1 && utmlnknum==-1)	utmlnknum=0;
if (utmlnknum>=0)
{
	if(utmlnkcmp.indexOf($(this).attr('href'))<0) {utmlnknum=utmlnknum+1}; 
	utmlnkcmp = $(this).attr('href'); 
	if($(this).attr('href').indexOf(utmlnkdomain)>=0) { $(this).attr('href',$(this).attr('href')+'?utm_source='+utmsource+'&utm_medium=box_'+utmplatform+'&utm_campaign=box'+utmsource+'_'+utmplatform+'_position'+utmlnknum); }
}

});

var utmlnknum=-1;
var utmlnkcmp="";
var utmlnkdomain = "https://plnielanu.zoznam.sk";
var utmplatform = "desktop";
var utmsource = "zoznam";
var utm2articlesoffset = 1;
var utmlinks = $('.article-list li a[href*="plnielanu.zoznam.sk"],a.article-big[href*="plnielanu.zoznam.sk"], #feed-most_read li a.article-top[href*="plnielanu.zoznam.sk"], #feed-most_read li a.article-title[href*="plnielanu.zoznam.sk"]');

utmlinks.each(function(index,el) { 
if (index < utmlinks.length - utm2articlesoffset) if (utmlinks.eq(index + utm2articlesoffset).attr('href').indexOf(utmlnkdomain)>-1 && $(this).attr('href').indexOf(utmlnkdomain)>-1 && utmlnknum==-1 && ($(this).attr('href').length-2)>utmlnkdomain.length)	utmlnknum=0;
if (utmlnknum>=0)
{
	if(utmlnkcmp.indexOf($(this).attr('href'))<0) {utmlnknum=utmlnknum+1}; 
	utmlnkcmp = $(this).attr('href'); 
	if($(this).attr('href').indexOf(utmlnkdomain)>=0) { $(this).attr('href',$(this).attr('href')+'?utm_source='+utmsource+'&utm_medium=box_'+utmplatform+'&utm_campaign=box'+utmsource+'_'+utmplatform+'_position'+utmlnknum); }
}

});
});
