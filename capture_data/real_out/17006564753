                break;
            case 'pilot.quickteller.com':
                oneTrustScr.src = 'https://cdn.cookielaw.org/scripttemplates/otSDKStub.js';
                oneTrustScr.setAttribute("data-domain-script", "ea521a5a-7c3f-4e15-ad2e-f50025d670e6");
                oneTrustScr.innerHTML = null;
                document.body.appendChild(oneTrustScr); 
                oneTrustFunc;  
                break;
            default:
                oneTrustScr = '';
                oneTrustFunc = '';
                break;
             }
        </script>
        <!-- OneTrust Cookies Consent Notice end for quickteller.com -->
        

