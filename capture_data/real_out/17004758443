    numslides = 5;

    numRows = 0;
    maxRows = 4;

    var rowheights = [];

    function getheight(images, width)
    {
        width -= images.length * 8;

        var aspectRatio = 0;

        for(var i = 0; i < images.length; ++i)
        {
            aspectRatio += $(images[i]).data('width') / $(images[i]).data('height');
        }

        // Return back the calculated height of all of the images next to one another.
        return width / aspectRatio;
    }

    function setheight(images, height)
    {
        rowheights.push(height);

        var widthContainer = parseInt(document.getElementById('imageflowcontainerdiv').offsetWidth) - 0;
        var widthRow = 0;
        var display = 'inline-block';

        for(var i = 0; i < images.length; ++i)
        {
            widthRow += $(images[i]).data('width')/$(images[i]).data('height')*height;
        }

        if(widthRow < 0.85 * widthContainer || numRows >= maxRows)
        {
            display = 'none';
        }

        if(images.length == 1)
        {
            var targetHeight = widthContainer * $(images[0]).data('height')/$(images[0]).data('width');

            if(targetHeight <= height * 1.2)
            {
                $(images[0]).css({'display':display, 'width': widthContainer, 'height': targetHeight});
                numRows++;
            }
            else
            {
                $(images[0]).css({'display': 'none'});
            }

            return;
        }

        for(var i = 0; i < images.length; ++i)
        {
            $(images[i]).css({'display': display, 'width': parseInt(height * $(images[i]).data('width') / $(images[i]).data('height')), 'height': height});
        }

        numRows++;

    }

    function resize(images, width)
    {
        setheight(images, getheight(images, width));
    }

    function run(max_height)
    {
        var widthcontainer = parseInt(document.getElementById('imageflowcontainerdiv').offsetWidth) - 0;
        numRows = 0;

        // Get all of the images inside of #imagecontainerdiv.
        var images = $('#imageflowcontainerdiv > .flowdiv');

        w: while(images.length > 0)
        {
            for(var i = 1; i < images.length + 1; i++)
            {
                // Continue adding images to the "slice" array, and after you add each image, check the new minimum height of that slice of images
                // by calling getheight.

                var slice = images.slice(0, i);

                var h = getheight(slice, widthcontainer);

                if(h < max_height)
                {
                    setheight(slice, h);

                    // Continue with the remaining images from index "i" on up.
                    images = images.slice(i);

                    continue w;
                }
            }

            setheight(slice, Math.min(max_height, h));

            break;
        }

    }
    
    function showCollectionGroup(collectionGroupId, action, tag, page, productType)
    {
        var localvalue = requestcount;
        var targetDivId = 'collectionsContainerDiv';
        
        if(typeof productType === undefined)
        {
            productType = '';
        }

        var availableTabs = document.getElementsByClassName('collectionGroupTab');

        for (i = 0; i < availableTabs.length; i++)
        {
            if(availableTabs[i].id == 'collectionGroupTab-' + collectionGroupId)
            {
                availableTabs[i].className = 'collectionGroupTab collectionGroupTabSelected';
            }
            else
            {
                availableTabs[i].className = 'collectionGroupTab';
            }
        }        

        //var productType = '';
        
        var postString = 'action=' + action + '&collectionGroupId=' + collectionGroupId + '&tag=' + encodeURIComponent(tag) + "&page=" + page + "&productType=" + productType;

        //alert(postString);
        //return;
        
        document.getElementById(targetDivId).innerHTML = "<img src='/assets/images/Searching.gif' style='display: inline-block; padding-top: 25px; padding-bottom: 25px;' alt='Search in Progress' title='Search in Progress'>";
        document.getElementById(targetDivId).style.display = 'inline-block';

        if(action === 'showPage')
        {
            document.getElementById('collectionGroupTabContainerDiv').scrollIntoView(true);
        }

        httpobject[localvalue] = newHttpReq();
        httpobject[localvalue].onreadystatechange = function(){

            try
            {
                if(httpobject[localvalue].readyState == 4)
                {
                }

                if(httpobject[localvalue].status == 200)
                {
                }
            }
            catch (e)
            {
                return;
            }

            if(httpobject[localvalue].readyState == 4 && httpobject[localvalue].status == 200)
            {
                document.getElementById(targetDivId).innerHTML = httpobject[localvalue].responseText;
            }


        };

        var url = '/queries/querySearchEngineCollectionsHomepage.php';

        httpobject[localvalue].open('post',url,true);
        httpobject[localvalue].setRequestHeader("content-type", "application/x-www-form-urlencoded");
        httpobject[localvalue].send(postString);

        requestcount += 1;
    }
    
	
