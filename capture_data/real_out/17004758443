    //http://www.w3schools.com/jquery/jquery_syntax.asp
    // Resize images inside of the image flow div.
    $(document).ready(function() {

        if(document.getElementById('imageflowcontainerdiv'))
        {
            window.addEventListener('resize', function () { run(250); });
            run(250);
        }

    });
    
    
    // This function is not currently used.
    function highlightCollections()
    {
        var targetElement = document.getElementById('headerNavigationDiscoverLink');
        var computedStyles = window.getComputedStyle(targetElement);
        var targetElementWidth =  parseInt(computedStyles.getPropertyValue('width'));
        var targetElementHeight =  parseInt(computedStyles.getPropertyValue('height'));
        var targetElementX = parseInt(window.scrollX + targetElement.getBoundingClientRect().left);
        var targetElementY = parseInt(window.scrollY + targetElement.getBoundingClientRect().top);
        
        var widthClient = parseInt(document.documentElement.clientWidth);
        var heightClient = parseInt(document.documentElement.clientHeight);        
        
        // Foreground Div
        var newDiv = document.createElement('div');
        var cssStyle = 'position: fixed; box-sizing: border-box; z-index: 5001; left: ' + targetElementX + 'px; top: ' + targetElementY + 'px; width: 10px; height: 10px; overflow: auto; padding: 0px; border: 1px solid #FF0000; background-color: #FFFFFF; text-align: center;';
        newDiv.id = 'globalForegroundDiv';
        newDiv.setAttribute('style',cssStyle);
        newDiv.style.cssText = cssStyle;
        document.body.appendChild(newDiv);            
    }

