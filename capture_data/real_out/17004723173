<script type="text/javascript">
    _waitOnceFor('ready', function() {
      const socialclubDivs = document.querySelectorAll(".socialclub");

      const socialNetworks = {
        "facebook.com": {
          class: "facebook",
          icon: "ri-facebook-fill",
          followText: "Segui su Facebook",
          url: "https://www.facebook.com/Messaggero.it/"
        },
        "tiktok.com": {
          class: "tiktok",
          icon: "ri-tiktok-fill",
          followText: "Segui su TikTok",
          url: "https://www.tiktok.com/@ilmessaggero.it"
        },
        "t.me": {
          class: "telegram",
          icon: "ri-telegram-fill",
          followText: "Segui su Telegram",
          url: "https://web.telegram.org/k/#@messaggeroit"
        },
        "instagram.com": {
          class: "instagram",
          icon: "ri-instagram-fill",
          followText: "Segui su Instagram",
          url: "https://www.instagram.com/ilmessaggero.it/"
        },
        "twitter.com": {
          class: "twitter",
          icon: "ri-twitter-fill",
          followText: "Segui su Twitter / X",
          url: "https://twitter.com/ilmessaggeroit"
        }
      };

      socialclubDivs.forEach((socialclubDiv) => {
        const articleElements = socialclubDiv.querySelectorAll("article");

        articleElements.forEach((articleElement) => {
          const links = articleElement.querySelectorAll('a');
          links.forEach((link) => {
            link.setAttribute('target', '_blank');
          });

          const aTag = articleElement.querySelector("a");
          //const figureTag = articleElement.querySelector("figure");
          const figureTag = articleElement.querySelector(".testatina");
          if (aTag) {

            // const domain = new URL(aTag.href).hostname;
            const domain = new URL(aTag.href).hostname.replace(/^www\./i, '');


            if (domain in socialNetworks) {
              const socialNetwork = socialNetworks[domain];
              articleElement.classList.add(socialNetwork.class);

              if (figureTag) {
                const divTag = document.createElement("div");
                divTag.classList.add("social_icon", `${socialNetwork.class}`);

                const spanTag = document.createElement("span");
                spanTag.classList.add(socialNetwork.icon);

                divTag.appendChild(spanTag);
                figureTag.insertAdjacentElement("beforeend", divTag);
              }

              
              const itemContent = articleElement.querySelector('h2');
              if (itemContent) {
                const followButton = document.createElement("div");
                followButton.className = 'follow';
                followButton.classList.add(socialNetwork.class);
                followButton.innerHTML = `<a href="${socialNetwork.url}" target="_blank"><span class="${socialNetwork.icon}"></span> ${socialNetwork.followText}</a>`;

                if (socialclubDiv.classList.contains('carousel')) {
                  articleElement.appendChild(followButton)
                } else {
                  itemContent.insertAdjacentElement('afterend', followButton)
                }
              }
            }

          }
        });
      });


    });
  </script> <script type="text/javascript">
