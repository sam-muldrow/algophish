            // ADV without consent
            var prebidAdUnitStatus = {};
            // fetch and render winning bids
            function renderPrebidAds() {
                var winners = pbjs.getHighestCpmBids();
                CED.debug("Prebid Manager - NO CONSENT - winners found", winners.length, winners);
                
                if (!winners || !winners.length) {
                    return;
                }
                
                winners.forEach((bid, index) => {
                    prebidSetAdUnitBidReady(bid['adUnitCode']);
                });
                CED.debug("Prebid Manager - NO CONSENT - winners found", Object.keys(prebidAdUnitStatus));

                // populate prebidAdUnitStatus with already ready advDiv
                try {
                    var banners = document.getElementsByClassName('adv_banner');
                    for (var i=0; banners && i < banners.length; i++) {
                        var adv_id = banners[i].id;
                        prebidSetAdUnitDivReady(adv_id);
                    }
                } catch(e) {
                    CED.error("Prebid Manager - NO CONSENT - no adv_banner class elements found in DOM or other related error", e);
                }
            }
            // set "auction" finished flag for current adUnit. We need to wait the auction before render!
            function prebidSetAdUnitBidReady(adUnitCode) {
                if (!prebidAdUnitStatus[adUnitCode]) prebidAdUnitStatus[adUnitCode] = {prebidReady: false, divReady: false};
                prebidAdUnitStatus[adUnitCode].prebidReady = true;
                
                // aggiungo l'adUnit all'array di advs
                (window.c_advs = window.c_advs || []).push(adUnitCode.replace('adv_', ''));

                // try to display the creative now
                prebidDisplayAdUnit(adUnitCode);
            }
            // set "div" ready in DOM flag for current adUnit. We need a DIV as wrap for the auction before render!
            function prebidSetAdUnitDivReady(adUnitCode) {
                if (!prebidAdUnitStatus[adUnitCode]) prebidAdUnitStatus[adUnitCode] = {prebidReady: false, divReady: false};
                prebidAdUnitStatus[adUnitCode].divReady = true;

                // try to display the creative now
                prebidDisplayAdUnit(adUnitCode);
            }
            // checks that both flags are set, if so render the adUnit!
            function prebidDisplayAdUnit(adUnitCode) {
                // we need both
                if (prebidAdUnitStatus[adUnitCode].prebidReady && prebidAdUnitStatus[adUnitCode].divReady) {
                    showPrebidAd(adUnitCode);
                }
            }
            // renders the adUnit in page!
            function showPrebidAd(adUnitCode) {
                var winners = pbjs.getHighestCpmBids(),
                    bidToShow = false;

                winners.forEach((bid, index) => {
                    if (bid['adUnitCode'] == adUnitCode) {
                        bidToShow = bid;
                    }
                });

                var slotId = adUnitCode.replace('adv_', '');

                if (!bidToShow) {
                    CED.error("Prebid manager - NO CONSENT - trying to show adUnit", adUnitCode, " but no matching adUnit found in winner list", winners);
                    _trigger("adv_render_ended_" + slotId, [null, "prebid_no_consent", true, [bidToShow.width, bidToShow.height]]);
                    return;
                }

                CED.debug("Prebid Manager - NO CONSENT - showing adUnit", bidToShow);

                // create iframe
                var id_iframe = 'anon_' + adUnitCode;
                var adID = bidToShow['adId'];
                var iframe = document.createElement('iframe');
                iframe.id = id_iframe;
                iframe.setAttribute('scrolling', 'no');
                iframe.setAttribute('style', 'border:0');
                iframe.setAttribute('marginwidth', '0');
                iframe.setAttribute('marginheight', '0');
                iframe.setAttribute('height', '0');
                iframe.setAttribute('width', '0');

                // insert into adUnit Div
                try {
                    document.getElementById(adUnitCode).appendChild(iframe);
                } catch(e) {
                    CED.error("Prebid manager - NO CONSENT - error inserting iframe into adUnit div", adUnitCode, e);
                    _trigger("adv_render_ended_" + slotId, [document.getElementById(adUnitCode), "prebid_no_consent", true, [bidToShow.width, bidToShow.height]]);
                    return;
                }

                var iframe_dom = iframe.contentWindow.document;
                if (bidToShow.mediaType == "native") {
                    if (typeof CustomPrebidNativeRender == "function") {
                        CED.debug("Prebid Manager - NO CONSENT - rendering adUnit through native render function", adUnitCode);
                        // e' una funzione che possiamo definirci anche noi stessi dove vogliamo
                        CustomPrebidNativeRender(bidToShow);
                    } else {
                        CED.warn("Prebid Manager - NO CONSENT - adUnit native render function `CustomPrebidNativeRender` not found", adUnitCode);
                    }
                } else {
                    CED.debug("Prebid Manager - NO CONSENT - rendering adUnit", adUnitCode);
                    pbjs.renderAd(iframe_dom, adID);
                }

                _trigger("adv_render_ended_" + slotId, [document.getElementById(adUnitCode), "prebid_no_consent", false, [bidToShow.width, bidToShow.height]]);
            }
            // fine ADV NO CONSENT
        
        
