<script type="text/javascript">
    var logoMakerfaire =  '<img width="placeholderW" height="placeholderH" id="logomakerfaire" src="https://utils.cedsdigital.it/img/customizations/logo_makerfaire.svg" />';

    function svgReproposeMakerfaire(width, height, maxWidth, maxHeight, svg, valRatio) {
        if (valRatio !== undefined) {
            var ratio = valRatio;
        } else {
            var ratio = (800 / 195.1);
        }
        var newHeight = Math.round(maxWidth / ratio);
        var newWidth = Math.round(maxWidth);
        svg = svg.replace('placeholderW', newWidth + 'px');
        svg = svg.replace('placeholderH', newHeight + 'px');
        return svg;
    }
</script>
