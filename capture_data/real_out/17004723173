                function s(i, m, o, n) {
                    n = typeof i == 'object';
                    if (n && i.length) {
                        for (m=0; m < i.length; m++) {
                            i[m] = s(i[m]);
                        }
                    } else if (n) {
                        for (m in i) {
                            if (m == '_consent') {
                                for (o in i[m]) {
                                    if (doesPurposeHaveConsent(o)) {
                                        return i[m][o];
                                    }
                                }
                                if (i[m]['0']) return i[m]['0'];
                            } else {
                                i[m] = s(i[m]);
                            }
                        }
                    }
                    return i;
                }
                adUnits = s(adUnits);
            })();
            
var prebidConfig = { enableSendAllBids: false, targetingControls: { alwaysIncludeDeals: true }, bidderTimeout: 2500, consentManagement: { gdpr: { cmpApi: 'iab', timeout: 800, defaultGdprScope: true } }, currency: { adServerCurrency: 'USD' }, priceGranularity: { buckets: [{ precision: 2, max: 2, increment: 0.01 }, { max: 4, increment: 0.02 }, { max: 8, increment: 0.05 }] }, userSync: { filterSettings: { iframe: { bidders: '*', filter: 'include' } }, syncsPerBidder: 0, userIds: [{ name: 'identityLink', params: { pid: '13951', notUse3P: false }, storage: { type: 'html5', name: 'idl_env', expires: 15, refreshInSeconds: 1800 } }], syncDelay: 3000 }, timeoutBuffer: 300, useBidCache: true, 'rubicon': { singleRequest: true, netRevenue: true }, 'criteo': { fastBidVersion: 'latest' } };

            Promise.all([
                // PBJS *************************************************************************
                new Promise((resolve, reject) => {
                    loadScript("https://statics.cedscdn.it/utils/js/prebid7.54.5.js");
                    
                    // ask for prebid
                    pbjs.que.push(function() {
                        pbjs.addAdUnits(adUnits);    

                        // aggiungo i targeting come per la dfp (ma qui tramite keywords!)
                        try {
                            ((i,k) => {
                                for (i=0; i < adUnits.length; i++)
                                    for (k=0; k < adUnits[i].bids.length; k++)
                                        if (adUnits[i].bids[k].bidder == 'appnexus')
                                            adUnits[i].bids[k].params.keywords = {category: "Homepage"};
                            })();
                        } catch(e) { CED.warn(e); }

                        pbjs.setConfig(prebidConfig);
                        
                        // Bidder Specific Supplay Chain Object 
                        pbjs.setBidderConfig({
                            "bidders": ['adasta'],   
                            "config": {
                              "schain": {
                                "validation": "relaxed",
                                "config": {
                                  "ver":"1.0",
                                  "complete": 1,
                                  "nodes": [
                                    {
                                      "asi":"adasta.it",
                                      "sid":"145",
                                      "hp":1
                                    }
                                  ]
                                }
                              }
                            }
                        });
                                                
                        pbjs.requestBids({
                            bidsBackHandler: () => {
                                // Prebid bids returned
                                CED.debug("Prebid Manager - pbjs finished!");
                                resolve();

                                
                                // gestione adv non consensate, lo metto qui perche' devo per forza aspettare
                                // che finisca il recupero dei bidder, il timeout non va bene!
                                                                _waitOnceFor('cookielaw_5_denied', () => {
                                    CED.debug("Prebid Manager - adv cookie denied, starting adv no consent");

                                    // render prebid ads without calling the AdServer
                                    renderPrebidAds();
                                });
                                
                            }
                        });

