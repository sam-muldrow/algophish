        function handleLoad() {
            if (impostazioni_testata.dfp.load_method == 'instant') {
                // le adv partono subito (sempre async, ma subito)
                advLoading();
            } else if (impostazioni_testata.dfp.load_method == 'load') {
                // solo al load
                _waitOnceFor('load', advLoading);
            } else {
                // DEFAULT: le adv partono alla prima interazione o dopo il timeout inattività
                _waitOnceFor('first-interaction', advLoading);
            }
        }
        
        if (impostazioni_testata.load_on_cookielaw_accepted) {
            _waitOnceFor('cookielaw.accepted', handleLoad);
        } else {
            handleLoad();
        }
    })();

    
