<!--DESKTOP CUSTOM  FINANZA PER TUTTI -->
<style>article.economia.finanza_per_tutti .testatina-articolo img{width:100px}</style>
<script type="text/javascript">
            /*JS DESKTOP  HP CUSTOM FINANZA PER TUTTI*/

            _waitOnceFor('ready', function() {

                /* tutte le testatine articolo salute  FINANZA PER TUTTI */
                var h3 = document.querySelectorAll(".testatabox h3.economia.finanza_per_tutti");
                var i;
                for (i = 0; i < h3.length; i++) {
