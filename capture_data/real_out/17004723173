<script>
// test su percentuale di traffico
var test_cmp_id = parseInt(localStorage.getItem('test_cmp_id'), 10);
if (typeof test_cmp_id != 'number' || isNaN(test_cmp_id) || test_cmp_id < 0 || test_cmp_id >= 100) {
    test_cmp_id = Math.floor(Math.random() * 100);
    localStorage.setItem('test_cmp_id', test_cmp_id);
}

// calcolo l'altezza base dipendente dall'adv_sticky
function getAdvStickyHeightIfAny() {
	return ((adv) => {return adv ? adv.clientHeight : 0})(document.querySelector('div.adv_sticky'));
}

_waitFor('cookielaw.accepted', function() {
    __tcfapi('getTCData', 2, function(tcdata,success) {
        let consensi_adv_completi = tcdata.purpose.consents[2] && tcdata.purpose.consents[3] && tcdata.purpose.consents[4] && tcdata.purpose.consents[7];
        if (consensi_adv_completi) {
            _trigger('tcf.full_adv_consent.accepted');
        } else {
            _trigger('tcf.full_adv_consent.denied');
        }
        console.log("retrieving TCDATA:", consensi_adv_completi)
    });
});

function canShowMe() {
	try {
		var isPay = document.querySelector('meta[name="pay"][content="1"]');
		var isDismissed = localStorage.getItem('test_cmp_dismiss');
		return !isPay && !isDismissed;
	} catch (e) {
		console.error(e);
	}
	return true;
}

if (test_cmp_id < 100) {
    _waitFor([['tcf.full_adv_consent.accepted', 'cookielaw_5_accepted'], ['user.subscribed']], function() {
        document.getElementById('richiesta_accettazione_abbonamento').classList.remove('visible');
    });
    
    _waitOnceFor([['tcf.full_adv_consent.denied', 'user.not-subscribed'],['cookielaw_5_denied', 'user.not-subscribed']], function() {
	    	if (!canShowMe()) {
			console.debug("Modèle français not opened because item is pay");
			return;
		}
		var h = getAdvStickyHeightIfAny();
		var raa = document.getElementById('richiesta_accettazione_abbonamento');
		if (h > 0) raa.style.bottom = h + 'px';
		raa.classList.add('visible');
    });

} //else console.log("TEST CMP: "+test_cmp_id);

function acceptAllCookies() {
    var pulsante = document.querySelector('#richiesta_accettazione_abbonamento .pulsanti a.accetta');
    pulsante.classList.add('loading');

    const styleEl = document.createElement('style');
    document.body.appendChild(styleEl);
    const styleSheet = styleEl.sheet;
    styleSheet.insertRule('body #iubenda-iframe.iubenda-iframe-visible {display: none!important;}', 0);

    try {
	// nascondo sulla fiducia (e fra pocorefresho tutto)
        //document.getElementById('richiesta_accettazione_abbonamento').classList.remove('visible');
        window.setTimeout(function() {
            document.querySelector('.iubenda-cs-preferences-link').click();
            window.setTimeout(function() {
                document.querySelector('button.iub-btn.iub-btn-consent.iub-btn-accept.purposes-btn.purposes-btn-accept').click();
                window.setTimeout(function() {
    	             document.querySelector('button#iubFooterBtn').click();
                     document.querySelector('#richiesta_accettazione_abbonamento .pulsanti a.accetta').classList.remove('loading');
		     document.location.reload();
    	        }, 750);
            }, 750);
        }, 0);
    } catch (e) {
    	//styleSheet.deleteRule(0);
        styleSheet.insertRule('body #iubenda-iframe.iubenda-iframe-visible {display: flex!important;}');
	alert('Si è verificato un errore, si prega di accettare i cookie manualmente');
        document.querySelector('.iubenda-cs-preferences-link').click();
    }
    return;
}

// sposto dal bottom il banner se ho una adv o se ho la strip del paywall
_waitFor('adv_render_ended_AnchorAd', (_, slot, type, isEmpty, size) => {
	// CED.log(slot, type, isEmpty, size)
	if (isEmpty) {
		document.getElementById('richiesta_accettazione_abbonamento').style.bottom = getAdvStickyHeightIfAny() + 'px';
		return;
	}

	// recupero l'altezza, default a 50
	var h = typeof size == "object" && size.length > 1 ? size[1] : 50;
	document.getElementById('richiesta_accettazione_abbonamento').style.bottom = h + "px";
});

</script>
