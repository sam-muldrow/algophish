<script type="text/javascript">
var readyFg = false;
var loadFg = false;
var sHost = window.location.host;

function fn_sdk_lp(s,l,c){if(c.length>l){return s;}s+="";c+="";while(s.length<l)s=c+s;s=s.length>=l?s.substring(0,l):s;return s;}

function fn_sdk_gT(){
	var db=new Date();
	return db.getFullYear().toString()+"-"+fn_sdk_lp(db.getMonth()+1,2,0)+"-"+fn_sdk_lp(db.getDate().toString(),2,0)
		+"T"+fn_sdk_lp(db.getHours().toString(),2,0)+":"+fn_sdk_lp(db.getMinutes().toString(),2,0)+":"+fn_sdk_lp(db.getSeconds().toString(),2,0)+"."+fn_sdk_lp(db.getMilliseconds().toString(),3,0)+"Z";
}

function fn_sdk_gR(){
	var r = '';
    try {
        r = window.top.document.referrer;
    } catch (e) {
        if (window.parent) {
            try {
                r = window.parent.document.referrer;
            } catch (e2) {
                r = '';
            }
        }
    }
    if (r === '') {
        r = document.referrer;
    }
    return r;
}

function fn_sdk_gEc(t){
	return window.encodeURIComponent(t);
}

var execSdk = function() {
	var _ait = _ait || [];
	_ait.push({'ait.startTime': (new Date().getTime()), 'event': 'ait.Start'});
	var d=document, g=d.createElement('script'), s=d.getElementsByTagName('script')[0];
	g.type='text/javascript'; g.async=true; g.defer=true; g.src='/js/container_intro.js'; s.parentNode.insertBefore(g,s);
};

function fn_sdk_ajax() {
	if (navigator.appName == "Microsoft Internet Explorer") { //IE 10 or Lower
		readyFg = true;
	}

	if (sHost.indexOf("www.kbstar.com") > -1) {
		loadFg = true;
	}

	$.ajax({
		url: "/quics?page=intro",
		type: "POST",
		xhrFields : { withCredentials : true },
		data: {"_LOG_TYPE":"WM", "lgTmClnt": fn_sdk_gT(), "pageURL":window.location.href, "referrer":fn_sdk_gEc(fn_sdk_gR())},
		success: function() {
			if (!readyFg && loadFg) {
				var _ait = window["_ait"] || [];

				(function(_ait){
					var d = document;
					var g = d.createElement('script');
					var s = d.getElementsByTagName('script')[0];

					try{
						var sdk_date = new Date();
						var sdk_yyyy = sdk_date.getFullYear();
						var sdk_mm =''+ (sdk_date.getMonth()+1);
						sdk_mm = sdk_mm.length==1?'0'+sdk_mm:sdk_mm;
						var sdk_dd = ''+sdk_date.getDate();
						sdk_dd = sdk_dd.length==1?'0'+sdk_dd:sdk_dd;
						var sdk_hour = ''+sdk_date.getHours();
						sdk_hour = sdk_hour.length==1?'0'+sdk_hour:sdk_hour;
						var sdk_min = ''+sdk_date.getMinutes();
						sdk_min = sdk_min.length==1?'0'+sdk_min:sdk_min;

						_ait.push( { 'ait.startTime': new Date().getTime(), 'event': 'ait.Start' });
						g.type='text/javascript';
						g.async=true;
						g.defer=true;
						g.charset='utf-8';
						g.src='/js/container_intro.js?' +sdk_yyyy+sdk_mm+sdk_dd+sdk_hour+sdk_min;
						g.onerror = function(e){
							g.src = '';
						};

						s.parentNode.insertBefore(g,s);
					}catch(e){
						(window.$ || function(){})(function(){
							$("body").append(g);
						});
					}
				})(_ait);
			}

			$(document).ready(function(){
				if (readyFg && loadFg) execSdk();
			});
		},
		error: function() {
		}
	});
}
</script>
