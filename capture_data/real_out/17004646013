        <div class="layer-stop">
            <input id="chkday" name="chkday" type="checkbox" class="inp-check">
            <label for="chkday" class="lab-cek">일주일 그만보기</label>
        </div>
        <div class="layer-close"><span class="hidden">닫기</span></div>
    </div>



<script type="text/javascript">
function setCookie(name, value, expiredays) {
    var todayDate = new Date();
    todayDate.setDate(todayDate.getDate() + expiredays);
    document.cookie = name + "=" + escape(value) + "; path=/; expires=" + todayDate.toGMTString() + ";"
}

$(document).ready(function () {
    //slide randem banner
    var random = Math.floor(Math.random() * $('.bn-promotion-support.slide .support-con').length);
    $('.bn-promotion-support.slide .support-con').hide().eq(random).show();
    //slide banner
    var $supportLayer = $('.bn-promotion-support.slide');
    var $supportLayerClose = $supportLayer.find('.layer-close');
    var prevScrollTop = $(window).scrollTop(),
        nowScrollTop = $(window).scrollTop();

    $(window).on('scroll touchmove', function () {
        nowScrollTop = $(window).scrollTop();
        if ($supportLayer.hasClass('ready')) {
            if ($('html, body').scrollTop() > 1700) {
                $supportLayer.addClass('open').removeClass('ready');
            }
        } else {
            if (nowScrollTop < prevScrollTop) {
                if ($supportLayer.hasClass('open')) {
                    if ($('html, body').scrollTop() <= 1800) {
                        $supportLayer.addClass('ready').removeClass('open');
                    }
                }
            }
            //alert('올림');
            prevScrollTop = nowScrollTop;
        }
    });

    $('body').on('click', '.bn-promotion-support .layer-close', function () {
        $supportLayer.removeClass('ready open').addClass('close');
        if ($("#chkday").is(':checked')) {
            setCookie("supportPop", "done", 7);

        }
    });

    var cookiedata = document.cookie;
    if (cookiedata.indexOf("supportPop=done") < 0) {
        $supportLayer.addClass('ready');
    } else {
        $supportLayer.removeClass('ready');
    }
});
</script>
