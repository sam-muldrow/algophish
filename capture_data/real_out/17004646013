<script type="text/javascript">
    jQuery(document).ready(function(){
            if($("#ad5").height > 5){
                $("#ad5before").hide();
            }else{
                $("#ad5after").hide();
            }
            onScrollDirection();

            onScrollAdjustEndLine("#main-top03-scroll-start", "#main-top", "#main-top03-scroll-in");
            onScrollAdjustEndLine("#main-top01-scroll-start", "#main-top", "#main-top01-scroll-in");
            onScrollAdjustEndLine("#main-top02-scroll-start", "#main-top", "#main-top02-scroll-in");

            onScrollAdjustEndLine("#main1-top01-scroll-start", "#main-middle", "#main1-top01-scroll-in");
            onScrollAdjustEndLine("#main1-top02-scroll-start", "#main-middle", "#main1-top02-scroll-in");
            onScrollAdjustEndLine("#main1-top03-scroll-start", "#main-middle", "#main1-top03-scroll-in");
            slideHaniLayerAuto('#hani_main_bn-promotion', "hani_home_promotion_scroll_id");
            slideHaniLayerAuto('#hani_main_notice_box', "hani_home_notiece_scroll_id");
            slideHaniLayerAuto('#hani_main_bn-president', "hani_home_president_scroll_id");
            selectLoginButtonLayermain();
        }
    );

    function selectLoginButtonLayermain() {
        //회원개편 member.hani.co.kr 변경 쿠키값 변경
        var     is_login        = (document.cookie.indexOf("HARMONYINFO")) > -1;
        var     html            = "";
        if ( is_login ) {
            if(document.location.href.includes('wwwtest.hani.co.kr')){
                jQuery(".login-box .join").attr("href", "https://membertest.hani.co.kr/logout.php?url="+encodeURIComponent(document.location.href)).attr("title", "로그아웃").html("로그아웃");
                jQuery(".login-box .mypage").attr("href", "https://membertest.hani.co.kr/help/support/sponsor_info.hani").attr("title", "마이페이지").html("마이페이지");
            }else{
                jQuery(".login-box .join").attr("href", "https://member.hani.co.kr/logout.php?url="+encodeURIComponent(document.location.href)).attr("title", "로그아웃").html("로그아웃");
                jQuery(".login-box .mypage").attr("href", "https://member.hani.co.kr/help/support/sponsor_info.hani").attr("title", "마이페이지").html("마이페이지");
            }
        }else{
            if(document.location.href.includes('wwwtest.hani.co.kr')){
                jQuery(".login-box .join").attr('href','https://membertest.hani.co.kr/join/join.php').html("회원가입");
                jQuery(".login-box .mypage").attr("href", "https://membertest.hani.co.kr/login.php?url="+encodeURIComponent(document.location.href)).attr("title", "로그인").html("로그인");
            }else{
                jQuery(".login-box .join").attr('href','https://member.hani.co.kr/join/join.php').html("회원가입");
                jQuery(".login-box .mypage").attr("href", "https://member.hani.co.kr/login.php?url="+encodeURIComponent(document.location.href)).attr("title", "로그인").html("로그인");
            }

        }
    };


</script>
