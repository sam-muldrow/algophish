<script>
    $('body').on('click', '.hamburger', function () {
        $(this).toggleClass('active');
        var $this = $(this).siblings('.over-list');
        if ($this.css("display") == "none") {
            $this.css("display", "block");
            settingGA('전체메뉴', '열기');
        } else {
            $this.css("display", "none");
            settingGA('전체메뉴', '닫기');
        }
    });



</script>

