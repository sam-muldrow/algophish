<script type="text/javascript">
	if(fromMobile == false && IsMobile!='') {
		var url = parent.document.referrer;
		var fb_Checkurl = /m\.facebook\.com|facebook\.com|www\.facebook\.com/;
		var fb_Checkurl_m = /m\.facebook\.com|lm\.facebook\.com/;
		var tw_Checkurl = /mobile\.twitter\.com|twitter\.com|t\.co|bit\.ly|www\.twitter\.com/;
		var nv_Checkurl = /m\.naver\.com|naver\.com|www\.naver\.com/;
		var du_Checkurl = /m\.daum\.net|daum\.net|www\.daum\.net/;
		var gg_Checkurl = /m\.google\.com|google\.com|google\.co\.kr|www\.google\.com/;
		var kakao_Checkurl = /_fr=kt/;
		var ad_hani_Checkurl = /ad\.hani\.co\.kr/;
		var param = "";
        if(fb_Checkurl.test(url)) {
            param = "?_fr=fb";
            document.location="https://m.hani.co.kr"+param;
        }else{
            if(tw_Checkurl.test(url)) {
            	param = "?_fr=tw";
            } else if(nv_Checkurl.test(url)) {
            	param = "?_fr=nv";
            } else if(du_Checkurl.test(url)) {
            	param = "?_fr=du";
            } else if(gg_Checkurl.test(url)) {
            	param = "?_fr=gg";
            }  else if(kakao_Checkurl.test(url)) {
            	param = "?_fr=kt";
            } else if(ad_hani_Checkurl.test(url)) {
            	param = "?_fr=ad_hani";
            }
            window.location.href="https://m.hani.co.kr"+param;
        }
	}
</script>
