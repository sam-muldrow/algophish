<script type="text/javascript">
    // 페이지 맞춤측정기준 세팅
    var depth0 = 'PC';
    var depth1 = '한겨레';
    var depth2 = '전체';
            depth2 = '메인';
        var depth3 = '';
    var depth4 = '';
    var page_title = `${depth0}>${depth1}>${depth2 != '' ? depth2:''}${depth3 != '' ? '>'+depth3:''}${depth4 != '' ? '>'+depth4:''}`;

    // 고객 맞춤측정기준 세팅

    var up_cid = getCookie('_ga');
    var up_visit_channel = depth0;
    var up_loginyn = '';
    var up_logintype = '';
    var up_uid = '';


    if(HARMONYINFODATA.data){
        up_loginyn = 'Y';
        up_logintype = getLoginMedia(HARMONYINFODATA);
        up_uid = 'def930ff754ff3d3bf4dca67dca8cd5a05506970';
    }else{
        up_loginyn = 'N';
        up_logintype = '';
        up_uid = '';
    }

    var page_info = {
        title : page_title,
        ep_page_host: window.location.hostname,
        ep_page_0depth: depth0,
        ep_page_1depth: depth1,
        ep_page_2depth: depth2,
        ep_page_3depth: depth3,
        ep_page_4depth: depth4,
        ep_page_url:window.location.href,
        ep_page_day:'월',
        up_cid: up_cid,
        up_visit_channel: up_visit_channel,
        up_loginyn: up_loginyn,
        up_logintype: up_logintype,
        up_uid: up_uid

        // ep_page_scrolldepth: 0,
    }

    // console.log(decodeURIComponent(HARMONYINFODATA.data).split(':')[0]);
    // $(window).scroll(function(){
    //     clearTimeout($.data(this, 'scrollTimer'));
    //     var scrolldepth = $(document).scrollTop();
    //     $.data(this, 'scrollTimer', setTimeout(function(){
    //         page_info.ep_page_scrolldepth = scrolldepth;
    //         sendGAEvent(page_info)
    //     }, 3000))
    // })

    // 메인페이지 변수 세팅 후 기본 전송 --article 이 아닐때 전송해야함
    if('main' != 'article'){
        sendGAPage(page_info);
    }
    // article 페이지에서 따로 전송 예정

    var click_event = {
        event_name : 'click_event',
        ep_event_page : '',
        ep_event_area : '',
        ep_event_label : '',
    };
    var settingGA = (ep_event_area, ep_event_label, ep_event_page=$('body').data('ga4-page')) => {
        click_event.ep_event_page = ep_event_page;
        click_event.ep_event_area = ep_event_area;
        click_event.ep_event_label = ep_event_label;
        sendGAEvent(click_event);
    };


</script>
