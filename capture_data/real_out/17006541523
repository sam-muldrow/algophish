        window.updateAllCarousels = () => {
            setTimeout(() => {
                if (window.carouselInstanses && window.carouselInstanses.length != 0) {
                    Array.from(window.carouselInstanses, carousel => {
                        if (carousel) {
                            carousel.update();
                        }
                    })
                }
            }, 250);
        }
    </script>

