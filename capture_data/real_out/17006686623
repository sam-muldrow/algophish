<script>
    var timer = 1701169200;
    if(timer) {
        let countdownDate = new Date(timer * 1000).getTime();

        let countdownFunction = setInterval(function() {

        let now = new Date().getTime();
        let distance = countdownDate - now;

        let days = Math.floor(distance / (1000 * 60 * 60 * 24));
        let hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
        let minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
        let seconds = Math.floor((distance % (1000 * 60)) / 1000);
        
        if (distance < 0) {
            clearInterval(countdownFunction);
            let countdownElement = document.querySelector(".countdown-container-7337899");
            let triangle = document.querySelector('.triangle-topright-7337899');

            triangle.style.display = "block";

            let articleClass = document.querySelector('.content-article-7337899');
            articleClass.classList.add("bg-pluss");
            countdownElement.parentNode.removeChild(countdownElement); // Remove the 'countdown' element
            timer = 0;
        }
        }, 1000);
    }
    </script>
