<script>
    var timer = 1700686800;
    if(timer) {
        let countdownDate = new Date(timer * 1000).getTime();

        let countdownFunction = setInterval(function() {

        let now = new Date().getTime();
        let distance = countdownDate - now;

        let days = Math.floor(distance / (1000 * 60 * 60 * 24));
        let hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
        let minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
        let seconds = Math.floor((distance % (1000 * 60)) / 1000);
        
            document.querySelector(".countdown-container-7348654").style.display = 'flex';
            document.getElementById("cd-d-7348654").innerHTML = days + "d";
            document.getElementById("cd-hr-7348654").innerHTML = hours + "h";
            document.getElementById("cd-min-7348654").innerHTML = minutes + "m";
            document.getElementById("cd-sec-7348654").innerHTML = seconds + "s";
        if (distance < 0) {
            clearInterval(countdownFunction);
            let countdownElement = document.querySelector(".countdown-container-7348654");
            let triangle = document.querySelector('.triangle-topright-7348654');

            triangle.style.display = "block";

            let articleClass = document.querySelector('.content-article-7348654');
            articleClass.classList.add("bg-pluss");
            countdownElement.parentNode.removeChild(countdownElement); // Remove the 'countdown' element
            timer = 0;
        }
        }, 1000);
    }
    </script>
