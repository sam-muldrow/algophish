<script type="text/javascript">
  var languageList = document.getElementsByClassName('language');
  var languageToggle = document.getElementById('language_switcher_toggle');
  var pathname = window.location.pathname;
  
  languageToggle.onclick = openPopover;

  // iterate through defined languages, where on each page refresh or reroute a new URL will be dynamically populated
  // this will allow users to translate the content to any of the listed languages
  for (let language of languageList) {
    var newPath = pathname.match(/^(\/)(?:\w{2})?(?=\/|$)/)
      ? pathname.replace(/^(\/)(?:\w{2})?(?=\/|$)/, '$1' + language.hreflang)
      : pathname + `?locale=${language.hreflang}`;
    language.setAttribute('href', newPath);
  }

  // assign active class to selected language
  var selectedLanguage = document.querySelector(`a[hreflang="${currentLocale}"]`)
  selectedLanguage.classList.add("active");  
</script>

