    if (deviceId !== '' || sessionId !== '') {
      var xhr = new XMLHttpRequest();
      var params = 'device_id=' + deviceId + '&session_id=' + sessionId;
      xhr.open('POST', '/home/set_amplitude_properties', true);
      xhr.setRequestHeader('Content-type', 'application/x-www-form-urlencoded');
      xhr.setRequestHeader('X-Requested-With', 'XMLHttpRequest');
      xhr.send(params);
    }
  });





  var params = {
    'locale': 'en'
  };

  // Each method can set additional event properties
  var customEventProperties = {};

  for (var property in customEventProperties) {
    params[property] = customEventProperties[property];
  };

  var isPublicPreviewPage = false;

  // if passed as a locale variable
    try {
      var isEmbeddedUrl = window.location.pathname.indexOf('maps/public_map_shell') >= 0 ||
        window.location.pathname.indexOf('public_maps/map_shell') >= 0;

      if (isEmbeddedUrl &&
        window.parent !== window &&
        window.parent.location.origin === window.location.origin) {

        isPublicPreviewPage = true;
      }
    } catch (e) {
      // In "real" embedded maps cases checking for window.parent.location will
      // throw an exception
      isPublicPreviewPage = false;
    }

  if (!isPublicPreviewPage) {
    amplitude.getInstance('mindmeister').logEvent('home_index', params);
  }




</script>

