    window.addEventListener('load', (event) => {
        if (document.querySelectorAll(".secondSliderFrame--right-top").length > 0) {
            if (window.innerWidth < 768) {

            }
        }

        if (window.innerWidth < 768) {
            if (document.querySelectorAll(".js-ntv-main-slider-second-items").length > 0) {
                var partialSliderItemHeight;

                document.querySelectorAll(".js-ntv-main-slider-second-items")[0].childNodes[0].childNodes[0].childNodes.forEach((e) => {
                    if (e.classList.contains("slick-current")) {
                        partialSliderItemHeight = e.offsetWidth;
                    }
                })
                document.querySelectorAll(".js-ntv-main-slider-second-items")[0].style.height = partialSliderItemHeight + "px";
            }
        }
    });
