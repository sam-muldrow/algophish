			var width = $(window).width();
			var margin = (width-960)/2 - 160;
			//alert('margin : '+margin);
			$('#deco1').css({'margin-left':  margin+'px'});
			$('#deco2').css({'margin-right': margin+'px'});
			if (margin < -160) {
				$('#deco2').css({'display': 'none'});
			}else{
				$('#deco2').css({'display': 'block'});
			}
		}
		adjustMargins();
		window.onresize = function(event) {
			adjustMargins();
		};
	</script>
</div>

