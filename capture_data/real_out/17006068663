<script type="text/javascript">
	__tcfapi('addEventListener', 2, function (tcData, success) {
		window.gdpr = 0;
		window.gdprString = null;
		if (tcData.tcString !== '') {
			if (tcData.vendor.consents[577] && tcData.vendor.consents[468]) window.gdpr = 1;
			if (window.gdpr) window.gdprString = tcData.tcString;
			onLoadTapad();
		}
	});

	function onLoadTapad(){
		var category = '';
		var subCategory = '';
		if (parseInt(subCategory) || subCategory === '') {
			subCategory = null;
		}
		if (category === '') {
			category = 'home';
			subCategory = null;
		}
		if (category === 'tag' || category === 'pages') {
			subCategory = null;
		}
		if (category && category.indexOf('=') !== -1) {
			var tempCategory = category.slpit('=');
			category = tempCategory[0];
			subCategory = null;
		}
		if (subCategory && subCategory.indexOf('=') !== -1) {
			var tempCategory = subCategory.slpit('=');
			subCategory = tempCategory[0];
		}
		var metadata = {
			gdpr: window.gdpr,
			gdpr_consent: window.gdprString,
			ccpa: null,
			bp_id: "starllc"
		}
		var config = {
			name: "P&G",
			pixelUrl: "https://pandg.tapad.com/tag"
		}
		var tagger = Tapad.init(metadata, config);
		var data = {
			category: category,
			subcategory: subCategory
		}
		tagger.sync(data);
