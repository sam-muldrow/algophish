  function loadBanner(){
    if(!$('.banner-section video').length) return

    if(document.body.clientWidth > 992) {
      $('.banner-section video').each(function(index,bv){
        bv.src = bv.getAttribute('data-src')
        bv.poster = bv.getAttribute('data-poster')
      })
