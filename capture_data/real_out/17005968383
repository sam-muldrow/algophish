        // $(document).ready(function($) {
        jQuery("img").unveil();
        // });
        $("a").on("click", function() {
          jQuery("img").unveil();
        });
        $(".jobike .scrollable").scrollable({
          circular : true
        });
        if (jQuery(".goods_history_list").length > 0) {
          jQuery.ajax({
            type : "GET",
            url : "/wbs/click-item-json.html?offset=0&limit=5&charset=utf8",
            dataType : "json",
            success : function(data) {
              if (!data || !data.clickItemList || data.clickItemList.length == 0) {
                return;
              }
              var ul = jQuery(".goods_history_list");
              for (var i = 0; i < data.clickItemList.length; i++) {
                var item = data.clickItemList[i];
                ul.append('<li><a href="https://www.webike.net/sd/' + item.syouhinSysCode + '/" target="_blank"><div class="photo"><img alt="' + item.syouhinMaker + ' ' + item.syouhinName
                    + '" src="https://img.webike-cdn.net/sys_images/bg.png" data-src="https://img.webike-cdn.net/catalogue/' + item.syouhinThumbnailUrl + '" loading="lazy"></div></a></li>');
              }
              jQuery(".goods_history_list img").unveil(1000);
            }
          });
        }
      }, 3000);
      $(document).ready(function() {
        $(".specialshop_guide .scrollable").scrollable({
          circular: true
        });
      });
      // 新規登録
      jQuery('#doSignup').click(function() {
        var regist_url = "https://www.webike.net/wbs/member-regist.html" + "?referer=" + encodeURIComponent(window.location.href);
        location.href = regist_url;
      });
      // ログイン
      jQuery('#doLogin').click(function() {
        var login_url = "https://www.webike.net/wbs/login.html" + "?nextpage=" + encodeURIComponent(window.location.href);
        location.href = login_url;
      });
    </script>
    <script>
      // ショップNAVI コンボボックス初期化
      var AJAX_FILTER_URL='https://www.webike.net/moto-api/shop-navi/model-filter/';
      var MOTO_IMG = 'https://img.webike-cdn.net';
      deferscript('https://img.webike-cdn.net/js/moto/pcv2/pages/shop-navi.js');
      deferscript('https://img.webike-cdn.net/js/moto/pcv2/pages/shop-navi-top.js','shop-navi-top',100, function() {
        var form = $('#bike_model_tab form');
        var formData = getFilterFormObject(form);
        formData.canonical   = false;
        formData.device      = 'pc';
        formData.format_html = 1;
        formData.show_area_filter = 0;
        $('#form-loading').removeClass('hidden');
        $.post(AJAX_FILTER_URL, formData).done(function(response) {
          form.find('.select-filter').html(response.html);
        })
        .always(function() {
            $('#form-loading').addClass('hidden');
        });
      });
      defer(function() {
        $('body').on('click', function (e) {
          if (!$('.shopnav_mybike_inner').is(e.target) && $('.shopnav_mybike_inner').has(e.target).length === 0) {
            $('#mybike_toggle').prop('checked', false);
          }
        });
      });
    </script></div>
