        //-----------banner--------------------------------------------
        //-----------banner_campain----------
        var banner_slider = $('#banner_carousel_1');
        var banner_inner = banner_slider.find('.banner_carousel_inner:first');
        var banner_pages = banner_inner.find('.banner_list');
        var banner_prev = banner_slider.siblings('.list_prev');
        var banner_next = banner_slider.siblings('.list_next');
        var banner_width = banner_slider.width();
        var banner_active = 1;
        banner_inner.width(banner_width * banner_pages.length);
        function gotoPage(page) {
          var current_page = banner_pages.eq(banner_active - 1);
          if (page == 'next') {
            banner_active = banner_active + 1;
          } else if (page == 'prev') {
            banner_active = banner_active - 1;
          }
          if (banner_active < 1) {
            banner_pages.filter(':nth-child(' + (banner_pages.length) + ')').prependTo(banner_inner);
            var temp = 2;
            banner_inner.css('margin-left', (-((temp - 1) * banner_width)));
            banner_active = 1;
          } else if (banner_active > banner_pages.length) {
            banner_pages.filter(':nth-child(1)').appendTo(banner_inner);
            var temp = banner_pages.length - 1;
            banner_inner.css('margin-left', (-((temp - 1) * banner_width)));
            banner_active = banner_pages.length;
          }
          banner_inner.animate({
            marginLeft : (-((banner_active - 1) * banner_width)) + 'px'
          }, 'slow');
        }
        banner_prev.click(function() {
          gotoPage('prev');
        });
        banner_next.click(function() {
          gotoPage('next');
        });
        //-----------banner_other----------
        //category カテゴリー選択
        var category_list = $('#portal .section_category .button_list li');
        var category_box = $('#portal .section_category .big_box .small_box');
        if (category_list) {
          category_list.first().addClass("selected");
        }
        category_list.click(function() {
          var idx = category_list.index(this), self = $(this);
          category_list.removeClass("selected");
          self.addClass("selected");
          category_box.addClass("close").eq(idx).removeClass("close");
        });
        //motoranking 排気量選択
        var moto_list = $('#portal .section_moto_ranking .button_list li');
        var moto_box = $('#portal .section_moto_ranking .big_box .small_box');
        moto_list.click(function() {
          var idx = moto_list.index(this), self = $(this);
          moto_list.removeClass("selected");
          self.addClass("selected");
          moto_box.addClass("close").eq(idx).removeClass("close");
          moto_box.find("img").each(function() {
            jQuery(this).unveil();
          });
        });
        //motoshop お知らせ
        var motoshop_list = $('#portal .section_moto_shop_info .button_list li');
        var motoshop_box = $('#portal .section_moto_shop_info .big_box .small_box');
        motoshop_list.click(function() {
          var idx = motoshop_list.index(this), self = $(this);
          motoshop_list.removeClass("selected");
          self.addClass("selected");
          motoshop_box.addClass("close").eq(idx).removeClass("close");
          motoshop_box.find("img").each(function() {
           jQuery(this).unveil();
          });
