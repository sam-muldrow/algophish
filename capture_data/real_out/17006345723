        <script>
			var pmwidth=$(window).width();
            if(pmwidth<1160) {
				$('body').css('width','1280px');
            }
            var investor_url = 'https://irm.cnstock.com/company/index/';
            $("#j_img_switch").Superslide( { mainCell:".bd ul",autoPlay:true,delayTime:0,interTime:3000} );
			$("#j_jdlb_switch").Superslide( { mainCell:".bd ul",autoPlay:true,delayTime:0,interTime:3000} );
            if ($("#j_wslide_ad").length > 0) {
                $("#j_wslide_ad").Superslide( { mainCell:".bd ul",autoPlay:true,delayTime:0,interTime:3000} );
            }
            $(function(){
                sideWidget(['back','share','top']);
            });
            $(document).ready(function($) {
                var bkzsflash = function(){
                    var _this = this;
                    var ele = $("#bkzs");
                    this.GetZsData = function(callback){
                        $.ajax({ 
                            url: 'https://apihq.cnstock.com/apihq/latest.do',
                            type: 'GET',
                            dataType: 'jsonp',
                            data: {'code':'sh000001,sz399001,sz399006,sh000688'},
                            success: function(msg){
                                callback(msg);
                            }
                        });
                    };
                    this.ShowZs = function(){
                        this.GetZsData(function(data){
                            var bkarr = ['szzs','szcz','cybz','kc50'];                            
                            $.each(bkarr,  function(i, v) {
                                var zd = (data[i][5] - data[i][1]).toFixed(2);
                                var color = zd>0?"red":"green";
                                var zdf = ((zd / data[i][1]) * 100).toFixed(2) + "%";
                                var zsdiv = ele.children('#'+bkarr[i]);
                                zsdiv.attr('class', color);
                                //zsdiv.find('.zd').html(zd);
                                zsdiv.find('.index').html(data[i][5].toFixed(2));
                                zsdiv.find('.zdf').html(zdf);                                
                            });
                        });
                    };
                    this.Init = function(){
                        ShowZs();
                    }();
                    setInterval(function(){ 
                        _this.ShowZs();
                    }, 2000000);
                }();

                var wtf = new waterFallCnstock(); 
                wtf.opt = {
                    father: $('#j_waterfall'),
                    moreBtn: $('#j_waterfall').find('#j_more_btn'),
                    waterList: $('#j_waterfall #j_waterfall_list'),
                    ajaxUrl:"https://app.cnstock.com/api/waterfall",
                    isAutoLoad: true,
                    colum: "sd",
                    isStartLoad: false,
					num: 10,
					AutoLoadPageLimited:4,
                    domCallBack:function(e){
                        var html = '';
                        html += '<li class="newslist" id="'+e.id+'">';
                        if(e.i.length > 0) {
                            html += '<div class="img"><a href="'+e.link+'" title="'+e.title+'" target="_blank"><img src="'+e.i+'" alt="'+e.title+'"></a></div>';
                        };                                    
                        html += '<h2><a href="'+e.link+'" title="'+e.title+'" target="_blank">'+e.title+'</a></h2>';
						if (e.desc.length > 0) {      
							html += '<p class="des">'+e.desc+'</p>';
						}
						html += '<p class="info">';
                        if (e.channel.length > 0) {
                            html += '<a href="'+e.channelurl+'" target="_blank" class="key b">'+e.channel+'</a>';
                        }
                        for (j = 0; j < e.keyword.length; j++) {
                            html += '<a href="https://search.cnstock.com/tag?k='+encodeURI(e.keyword[j])+'" target="_blank" class="key">'+e.keyword[j]+'</a>';
                        }
						
						if(e.time.substr(5,5)=='11-22'){
							html += '<span class="time">'+e.time.substr(11,5)+'</span></p></li>';
						}else if(e.time.substr(0,4)=='2023'){
							html += '<span class="time">'+e.time.substr(5,11)+'</span></p></li>';	
						}else{
							html += '<span class="time">'+e.time.substr(0,11)+'</span></p></li>';
						}
                        
                        return html;
                    },
                }             
                wtf.setInit();
                var systockprice = new AF_Stock_Price();
                systockprice.HasFatherInit();
            });
			        </script>
