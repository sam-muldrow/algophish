  <script type="text/javascript">
    if (self == top) { document.documentElement.style.display = 'block'; document.documentElement.style.visibility = 'visible'; } else {
      top.location = self.location;
    }
  </script>
  <script type="text/javascript">
    function showPage() {
      if (self == top) {
        document.getElementById('errorpage').style.display = 'none';
        document.getElementById('realpage').style.visibility = 'visible';
      } else {
        top.location = self.location;
      }
    }
  </script>
  <script type="text/javascript">
    $(document).ready(function () {
      var url = location.href;
      var value1 = url.search('iframe');
      var value2 = url.search('script');
      var value3 = url.search('<');
      var value4 = url.search('>');
      var value5 = url.search('%');
      var value6 = url.search(';');
      if ((value1 != -1) || (value2 != -1) || (value3 != -1) || (value4 != -1) || (value5 != -1) || (value6 != -1)) {
        top.location.href = "https://" + location.host + location.pathname;
      }
      else {
      }

    });
  </script>
  <script type="text/javascript">
    function lms() {
      if (top.location.protocol == "http:") {
        var a = location.host;
        top.location.href = "https://" + a + location.pathname + location.search;

      }
    }
  </script>
 
	<script>
	
	/* 2 new Popup Scrip code */
	
	$(document).ready(function(){
		$('.newclose').on('click', function(){
			
			if($(".newoverlay").is(":visible")){
               
			   $('.newoverlay').hide();
			   
			   $('.newoverlay2').show();
			   
            } 
		});
		
		$('.newclose2').on('click', function(){
			$('.newoverlay2').fadeOut();
		});
	})
	
	
$(document).on('click', '#pop2img', function(e){ 
		e.preventDefault(); 
		var url = "/corporate-and-institutional-banking/policies/key-policies-and-commitments/reservebank-ios.htm?eOfferCode=INHPOLRBIOS"; 
		window.open(url, '_blank');
		$('.newoverlay2').fadeOut();
	});
	
	$(document).on('click', '#pop1img', function(e){ 
		e.preventDefault(); 
		var url = "/corporate-and-institutional-banking/policies/key-policies-and-commitments/fraud-awareness.htm?eOfferCode=INHPOLNWFAC"; 
		window.open(url, '_blank');
		$('.newoverlay2').fadeOut();
	});
	
	</script>
	



