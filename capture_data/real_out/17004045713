    $(document).ready(function () {
      var flag = 0;
      var element1 = document.getElementsByClassName("slides")[0].children[0]
      var observer1 = new MutationObserver(function (mutations) {
        mutations.forEach(function (mutation) {
          if (mutation.attributeName == "class" && (mutation.target.getAttribute("class").indexOf("flex-active-slide") != -1) && buttonclick !== 1 && bannertab !== 1 && stop == "yes") {
            var Pos = "1";
            var ban1 = "Know more" + "|" + "Home page:Banner" + Pos + "|" + "28022023" + "|" + "INHPS128022023";
            flag = 1;
            console.log("HomeBanner1Impression");
            var HomeBanner1Impression = new CustomEvent('HomeBanner1Impression');
            document.querySelector('body').dispatchEvent(HomeBanner1Impression);
            flagcheck();
          }
        });
      });
      observer1.observe(element1, {
        attributes: true
      });
      var element2 = document.getElementsByClassName("slides")[0].children[1]
      var observer2 = new MutationObserver(function (mutations) {
        mutations.forEach(function (mutation) {
          if (mutation.attributeName == "class" && mutation.target.getAttribute("class").indexOf("flex-active-slide") != -1 && buttonclick !== 1 && bannertab !== 1 && stop == "yes") {
            var Pos = "2";
            var ban2 = "Convert Now" + "|" + "Home page:Banner" + Pos + "|" + "26092023" + "|" + "INHPS226092023";
            flag = 2;
            console.log("HomeBanner2Impression");
            var HomeBanner2Impression = new CustomEvent('HomeBanner2Impression');
            document.querySelector('body').dispatchEvent(HomeBanner2Impression);
            flagcheck();
          }
        });
      });
      observer2.observe(element2, {
        attributes: true
      });
      var element3 = document.getElementsByClassName("slides")[0].children[2]
      var observer3 = new MutationObserver(function (mutations) {
        mutations.forEach(function (mutation) {
          if (mutation.attributeName == "class" && mutation.target.getAttribute("class").indexOf("flex-active-slide") != -1 && buttonclick !== 1 && bannertab !== 1 && stop == "yes") {
            var Pos = "3";
            var ban3 = "Know More" + "|" + "Home page:Banner" + Pos + "|" + "15112023" + "|" + "INHPS315112023";
            flag = 3;
            console.log("HomeBanner3Impression");
            var HomeBanner3Impression = new CustomEvent('HomeBanner3Impression');
            document.querySelector('body').dispatchEvent(HomeBanner3Impression);
            flagcheck();

          }
        });
      });
      observer3.observe(element3, {
        attributes: true
      });

      var element4 = document.getElementsByClassName("slides")[0].children[3]
      var observer4 = new MutationObserver(function (mutations) {
        mutations.forEach(function (mutation) {
          if (mutation.attributeName == "class" && mutation.target.getAttribute("class").indexOf("flex-active-slide") != -1 && buttonclick !== 1 && bannertab !== 1 && stop == "yes") {
            var Pos = "4";
            var ban4 = "Book Now" + "|" + "Home page:Banner" + Pos + "|" + "26082023" + "|" + "INHPS426082023";
            flag = 4;
            console.log("HomeBanner4Impression");
            var HomeBanner4Impression = new CustomEvent('HomeBanner4Impression');
            document.querySelector('body').dispatchEvent(HomeBanner4Impression);
            flagcheck();
          }
        });
      });
      observer4.observe(element4, {
        attributes: true
      });

      function flagcheck() {
        if (flag == 4 && stop != "no" && buttonclick !== 1 && bannertab !== 1) {
          stop = "no";
          console.log("HomeBannerFirstCycleComplete");
          var HomeBannerFirstCycleComplete = new CustomEvent('HomeBannerFirstCycleComplete');
          document.querySelector('body').dispatchEvent(HomeBannerFirstCycleComplete);
          return true;
        }
      }
    });
