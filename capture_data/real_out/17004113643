<section class="paragraph paragraph--type--cu-tabbed-content-tab paragraph--view-mode--default" id="tab-6677" data-paragraph-id="6677">
  <h2 class="tab-label js-hide" data-paragraph-id="6677">Community</h2>
  
      <div class="field field--name-field-cu-tab-content field--type-entity-reference-revisions field--label-hidden field--items">
