	<script>
	var jobsbox_config = {
		'code': 	'196_C95_450_1_8735',
		'track':	'f6868',
		'target':	'#a-jobs',
		'fcid':		'8735',
		'area':		'/area=Array/',
	
		'tracker':	{
			'click':	'https://a.centrum.cz/cent/adclick/FCID={fcid}/site=ihned{area}size=measurement/relocate=https://mer.stdout.cz/?url={url}&s=admon&v=1&e=click&m={code}{track}&j={nocache}',
			'view':		'<img src="https://mer.stdout.cz/l.gif?s=admon&v=1&e=view&m={code}{track}&j={nocache}">'
		}
	};
	function displayJobs() {
		if(ecoLogin == undefined) {
			setTimeout('displayJobs', 250);
			return;
		}

		ecoLogin.updateUserInfoCallback.push(function() {
			var target = $(jobsbox_config.target);

			if(!ecoLogin.isJobsVisible()) {
				target.parent().remove();
				if ($('.under-article-with-jobs').length) {
					$('.under-article-with-jobs .col1').addClass('no-resize');
					$('.under-article-with-jobs').removeClass('under-article-with-jobs');
				}
				return;
			}

			var reg = /{([^}]{1,})}/gi;

			$('#jobs-cz', target)
				.before('<!-- spir.czMonS['+jobsbox_config.code+'] -->')
				.after('<!-- spir.czMonE -->')
			;

			var replacement = {
				'code': jobsbox_config.code,
				'track': jobsbox_config.track,
				'fcid':	jobsbox_config.fcid,
				'area': jobsbox_config.area,
				'nocache': Math.ceil(Math.random() * 1000000)
			};

			if(jobsbox_config.tracker.view) target.append(jobsbox_config.tracker.view.fillTpl(reg, replacement));
			if(jobsbox_config.tracker.click) {
				$('a', target).each(function() {
					replacement.url = encodeURIComponent($(this).attr('href'));
					$(this).attr('href', jobsbox_config.tracker.click.fillTpl(reg, replacement));
				});
			}

			if ( Math.random() > 0.75 ) {
				$('.jobs').addClass('j-kurzy');
			} else {
				$('.jobs').addClass('j-prace');
			}
			target.parent().addClass('a-visible jobs-visible');

			if (typeof onScrollResponse === 'object') { onScrollResponse.reinit(); }
		});
	}
	displayJobs();
	
	</script>


