<script>
    var acc = document.getElementsByClassName("tozluMenuAccordion");
    var i;

    for (i = 0; i < acc.length; i++) {
        acc[i].addEventListener("click", function () {
            this.classList.toggle("tozluMenuAccordionActive");
            var panel = this.nextElementSibling;
            if (panel.style.maxHeight) {
                panel.style.maxHeight = null;
            } else {
                panel.style.maxHeight = panel.scrollHeight + "px";
            }
        });
    }



</script>
<script>
var red;
function colorChange() {


    if(red === 0){

red=255;

    }else{red=0}
    
    var menuItems = document.querySelectorAll(".menuRenkRed2");
    for (var i = 0; i < menuItems.length; i++) {
      menuItems[i].style.color = "rgb(" + red + ", 0, 0)";
    }
  }
  setInterval(colorChange, 500);
</script>
