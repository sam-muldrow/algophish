<script>
    var windowLocation = window.location.href;
    if (windowLocation.includes('panel.')) {
        document.head.querySelector('link[rel="preload"][as="style"]').setAttribute('rel', 'stylesheet')
        var styleLink = document.body.querySelectorAll('link');
        styleLink.forEach(function (e, i) {
            if (e.getAttribute('href').includes('style.css')) {
                e.remove();
            }
        })
    }
</script>
