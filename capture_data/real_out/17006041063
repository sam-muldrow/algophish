<script>
    function dNoneJS(x) {
        if (x.matches) {
            $('.d-none-js').closest("li").show();
        } else {
            $('.d-none-js').closest("li").hide();
        }
    }
    var x = window.matchMedia("(max-width: 700px)");
    dNoneJS(x);
    x.addListener(dNoneJS);

</script>
