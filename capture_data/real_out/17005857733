<script>

  var colorAbbreviateMap = {
    "black": "blk",
    "navy": "nav",
    "purple": "pur",
    "charcoal": "cha",
    "red": "red",
    "green": "gre",
    "heather-gray": "hgr",
    "royal": "roy",
    "white": "whi",
    "turquoise": "tur",
    "powder-blue": "blu",
    "cream": "crm",
    "dark-chocolate": "dch",
  }
    
  $(".odad-image__container.left").slick({
    prevArrow: $('.odad-left.slider__prev'),
    nextArrow: $('.odad-left.slider__next'),
  });

  $(".odad-image__container.right").slick({
    prevArrow: $('.odad-right.slider__prev'),
    nextArrow: $('.odad-right.slider__next'),
  });
  
  $(".odad-image__container.rightmost").slick({
    prevArrow: $('.odad-rightmost.slider__prev'),
    nextArrow: $('.odad-rightmost.slider__next'),
  });


  $(document).ready( function() {

    // Do ODAD heat bar calculation
    const leftBarTotal = Math.abs($(".selection-bar-left").data("prog_bar_total"));
    const rightBarTotal = Math.abs($(".selection-bar-right").data("prog_bar_total"));
    const rightmostBarTotal = 0/*Math.abs($(".selection-bar-rightmost").data("prog_bar_total"))*/;


    const barTotal = leftBarTotal + rightBarTotal + rightmostBarTotal;

    /*const leftBarPercentage = Math.round(Math.abs(((barTotal - leftBarTotal) / barTotal) * 100));
    const rightBarPercentage = Math.round(Math.abs(((barTotal - rightBarTotal ) / barTotal) * 100));
    const rightmostBarPercentage = Math.round(Math.abs(((barTotal - rightmostBarTotal ) / barTotal) * 100));*/
    
    const leftBarPercentage = Math.round(Math.abs(((100 * leftBarTotal) / barTotal)));
    const rightBarPercentage = Math.round(Math.abs(((100 * rightBarTotal ) / barTotal) ));
    const rightmostBarPercentage = Math.round(Math.abs(((100 * rightmostBarTotal ) / barTotal)));

    console.log("barTotal: "+barTotal);
    console.log("leftBarPercentage: "+leftBarPercentage);
    console.log("rightBarPercentage: "+rightBarPercentage);
    console.log("rightmostBarPercentage: "+rightmostBarPercentage);

    // Apply to bar label
    $(".odad-main-container .percentage-label.left").html(leftBarPercentage+"%");
    $(".odad-main-container .percentage-label.right").html(rightBarPercentage+"%");
     $(".odad-main-container .percentage-label.rightmost").html(rightmostBarPercentage+"%");

    // Apply to bar width
    $(".odad-main-container .odad-versus-left").css("width", leftBarPercentage+"%");
    $(".odad-main-container .odad-versus-right").css("width", rightBarPercentage+"%");
    $(".odad-main-container .odad-versus-rightmost").css("width", rightmostBarPercentage+"%");

    (function() {
      function addZero(num) {
        if(parseInt(num) >= 10) {
          return num;
        }
        else {
          return "0" + num;
        }
      }

      var timerHr = $("#timer__hr");
      var timerMin = $("#timer__min");
      var timerSec = $("#timer__sec");
      
      var timerHr2 = $("#timer__hr2");
      var timerMin2 = $("#timer__min2");
      var timerSec2 = $("#timer__sec2");

      function updateCountDown() {
        var newYork = new Date().toLocaleString("en-US", {
          timeZone: "America/New_York"
        });
        var nowNow = new Date(newYork.toString());
      
        timerHr.html(addZero(23 - nowNow.getHours()) + "<p class='time__txt'>Hrs</p>");
        timerMin.html(addZero(59 - nowNow.getMinutes()) + "<p class='time__txt'>Mins</p>");
        timerSec.html(addZero(59 - nowNow.getSeconds()) + "<p class='time__txt'>Secs</p>");
        
        timerHr2.html(addZero(23 - nowNow.getHours()) + "<p class='time__txt'>Hrs</p>");
        timerMin2.html(addZero(59 - nowNow.getMinutes()) + "<p class='time__txt'>Mins</p>");
        timerSec2.html(addZero(59 - nowNow.getSeconds()) + "<p class='time__txt'>Secs</p>");
      }

      setInterval(updateCountDown, 1000);
    })(); 

    $(".js-odad-icon__wrapper").on("click", function() {
      var $this_odad = $(this);
      var swatchGroup = $this_odad.data("swatch-colors");
      var swatchGroupSelector = $("#" + swatchGroup);
      var side = $this_odad.data("side");
  
      $(".js-swatch-color__wrapper.js-odad-" + side).each(function() {
        var $this = $(this);
        if($this.is(":visible") && !$this.is(swatchGroupSelector)) {
          $this.hide();
        }
      });
      
      swatchGroupSelector.css({ "display": "flex" });
      swatchGroupSelector.children(".js-odad-color-swatch[data-side='" + side + "']")[0].click();

      // Price and buy now
      var productLink = $this_odad.data("link");
      var productPrice = $this_odad.data("price");
      $(".js-odad-buy-now__" + side).attr("href", productLink);
      $(".js-odad-buy-now-price__" + side).text(productPrice);

      // Remove title and artist
      $(".odad-title__" + side).addClass("display-none");

      $(".js-odad-icon__wrapper").removeClass("active");
      $(this).addClass("active");

    });

    // Odad image link
    $(".js-odad-link").on("click", function(e) {

       e.preventDefault();
      console.log('odad click...');
      
      var link = $(this).data("link");const localeName = "";
      console.log('localeName');
      console.log(localeName)
      console.log(link)

      let redirctURL = link;
      if (localeName && !link.includes(`/${localeName}/`)) {
        redirctURL = '/'+localeName + link;
      }

      console.log(redirctURL);
      window.location.href = redirctURL;
    });

    $(".js-odad-color-swatch").on("click", function(event) {
      event.preventDefault();
      var $this = $(this);
      var productId = $this.data("prod-id");
      var swatchColor = $this.data("color");
      var imgSelector = "#prod-type-" +  productId + "-img-color-" + colorAbbreviateMap[swatchColor];
      var slideIndex = $(imgSelector).data('slick-index');
      var side = $this.data("side");

      $(".odad-image__container."+ side).slick('slickGoTo', slideIndex, true); // "don't animate" = true
      
      // Remove title and artist
      $(".odad-title__" + side).addClass("display-none");
    });
  
    // Odad image link
    // $(".js-odad-link").on("click", function() {
    //   var link = $(this).data("link");
    //   window.location.href = link;
    // });

//    $(".timer__wrapper").tooltip();
//    $(".odad-versus__img").tooltip();


    // Check whether products are available
    $(".odad-selection-category__container .odad-selection-button").each(function(){
        var thisCategoryHandles = $(this).data("category-handles").split('|');
        var $thisParent = $(this).parents(".odad-selection-bar__container").first();
        
      
        // check for available icons
        var isIconsAvailable = false;
        for(let i = 0; i < thisCategoryHandles.length; i++) {
            if($thisParent.find(".odad-icon__wrapper[data-odad-type='"+thisCategoryHandles[i]+"']").length > 0) {
                isIconsAvailable = true;
            } 
        }
        
        if(!isIconsAvailable) {
            $(this).hide();
        }
    });

    $(".odad-selection-category__container .odad-selection-button").click(function(){
      
      

      var thisCategoryHandles = $(this).data("category-handles").split('|');
      var $thisParent = $(this).parents(".odad-selection-bar__container").first();

      // console.log($thisParent);
      

      $thisParent.find(".odad-selection-category__container").hide();
      $thisParent.find(".odad-selection-icon__container").css("display", "flex");
      
      $thisParent.find(".odad-icon__wrapper").hide();

      for(let i = 0; i < thisCategoryHandles.length; i++) {
        
        $thisParent.find(".odad-icon__wrapper[data-odad-type='"+thisCategoryHandles[i]+"']").show();  
        if($(".odad-icon__wrapper[data-odad-type='"+thisCategoryHandles[i]+"']").length)
        {
          if(thisCategoryHandles[i] == 'tee' || thisCategoryHandles[i] =='towel' || thisCategoryHandles[i] =='notebook' || thisCategoryHandles[i] =='pet-tank')
          {
          $thisParent.find(".odad-icon__wrapper[data-odad-type='"+thisCategoryHandles[i]+"']").click();
          }
        }
      }
      
      $(".odad-selection-button").removeClass("active");
      $(this).addClass("active");

    });

    $(".odad-selection-icon__container .back-to-category").click(function(){
      $thisParent = $(this).parents(".odad-selection-bar__container").first();

      // console.log($thisParent);

      $thisParent.find(".odad-selection-category__container").show();
      $thisParent.find(".odad-selection-icon__container").hide();
    });

  });
</script>


