<script type="text/javascript">



function videoChange() {
    document.querySelectorAll(".billBoardwithVO2video").forEach((res,index)=>{
        var getBG = res.querySelector(".billBoardBackgroundImage").style.background;
        if(getBG !== '') {
            res.querySelector(".billBoardBackgroundImage").setAttribute("data-bg",getBG);
        }
        res.querySelector(".billBoardBackgroundImage").style.background = "";
        if(res.querySelector(".billBoardBackgroundImage video")) {
            res.classList.add("hasVideo");
        } else {
            res.classList.add("noVideo");
            res.querySelector(".billBoardBackgroundImage").style.background = res.querySelector(".billBoardBackgroundImage").getAttribute("data-bg");
        }
        if($(window).width() >= 1036 && $(window).width() <= 1100) {
            if(res.querySelector(".billBoardBackgroundImage video")) {
                res.querySelector(".billBoardBackgroundImage video").style.height = res.querySelector(".billBoardBackgroundImage").offsetHeight + 'px';
            }   
        } else if($(window).width() > 1100) {
            if(res.querySelector(".billBoardBackgroundImage video")) {
                res.querySelector(".billBoardBackgroundImage video").style.height = null;
            }
        } else {
            if(res.querySelector(".billBoardBackgroundImage video")) {
                res.querySelector(".billBoardBackgroundImage video").style.height = null;
            } else {
                res.querySelector(".billBoardBackgroundImage").style.background = res.querySelector(".billBoardBackgroundImage").getAttribute("data-bg");
            }
        }
    })
}

function setWidthTab() {
var elements = ".billboardXXL .text-left-image-right, .billboardXXL .text-right-image-left";
document.querySelectorAll(elements).forEach(res => {
    if(res) {
        if(screen.width >= 836 && screen.width <= 999) {
            res.style.width = screen.width+'px';
        } else {
            res.style.width = null
        }
    }
})
}



var myInterval = setInterval(videoChange, 10);
$(window).on('resize', (res) => {
    videoChange();
    setWidthTab();
});
$( document ).ready(function() {
    setTimeout(()=>{
        clearInterval(myInterval);
        setWidthTab();
    },5000);
});

