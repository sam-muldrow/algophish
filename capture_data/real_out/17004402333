<script type="text/javascript">
    var cache = {};

    function tmpl(str, data) {
        var fn = !/\W/.test(str) ? cache[str] = cache[str] || tmpl(document.getElementById(str).innerHTML) : new Function("obj", "var p=[],print=function(){p.push.apply(p,arguments);};" + "with(obj){p.push('" + str.replace(/[\r\t\n]/g, " ").split("<%").join("\t").replace(/((^|%>)[^\t]*)'/g, "$1\r").replace(/\t=(.*?)%>/g, "',$1,'").split("\t").join("');").split("%>").join("p.push('").split("\r").join("\\'") + "');}return p.join('');");
        return data ? fn(data) : fn
    };
</script>
<script type="text/javascript">
    var tpl = '<div class="video-item">' +
            '<div class="video-image-container">' +
            '<a class="video-image-warp" target="_blank" href="<%=videoUrl%>">' +
            '<img src="<%=videoImage%>" />' +
            '</a>' +
            '</div>' +
            '<div>' +
            '<h3>' +
            '<a href="<%=videoUrl%>" target="_blank"><%=videoName%></a>' +
            '</h3>' +
            '<div class="vd-info">' +
            '<div class="info-name">' +
            '<img src="/images/author.png" class="vd-info-icon" />' +
            '<span><%=authorName%></span>' +
            '</div>' +
            '<div class="play-count">' +
            '<img src="/images/play.png" class="vd-info-icon" />' +
            '<span><%=videoCount%></span>' +
            '</div>' +
            '</div>' +
            '</div>' +
            '</div>';



    $.ajax({
        type: "get",
        url: "https://tvshow.ku6.com/Index/listRooms",
        data: {},
        dataType: "jsonp",
        jsonp: "cb",
        success: function(data) {
            console.log(data);
            var jcfragment = $('<div>');
            $(data).each(function(i,el){

                var itemData = data[i];
                var jcItemDom = tmpl(tpl, {
                    videoUrl: 'https://tvshow.ku6.com/' + itemData.roomId,
                    videoImage: itemData.img,
                    videoName: itemData.title,
                    authorName: itemData.author,
                    videoCount: itemData.views
                });
                jcfragment.append(jcItemDom);

            });
            $('#live-container').append(jcfragment);
            if ($('#live-container div .video-item').length > 0){
                $('#live-container').show();
            }
        },

        error: function() {
            console.log("error");
        }
    });


    var page = 0;
    var pageSize = 40;

    function loadData() {
        $.ajax({
            type: "get",
            url: "/video/feed",
            data: {
                "pageNo": page,
                "pageSize": pageSize
            },
            dataType: "json",
            // dataType: "jsonp",
            // jsonp: "callback",
            success: function(result) {
                if (result.retCd === 0) {
                    var listData = result.data;
                    if (listData && listData.length) {
                        var fragment = $('<div>');
                        for (var i = 0; i < listData.length; i++) {
                            var itemData = listData[i];
                            var itemDom = tmpl(tpl, {
                                videoUrl: itemData.detailUrl,
                                videoImage: itemData.picPath,
                                videoName: itemData.title,
                                authorName: itemData.publisher,
                                videoCount: itemData.playCnt
                            });
                            fragment.append(itemDom);
                        }
                        $('#video-container').append(fragment);
                    }
                }
            },
            error: function() {
                console.log('error');
            }
        });
    }

    loadData();

    $('.load-more').click(function() {
        page++;
        loadData();
        // TODO 5次加载更多动作完成后，“加载更多”按钮隐藏显示  https://lpms.51y5.net/tasks/109221
        if(page >= 5) {
             $('.load-more').hide();
        }
    });
<!-- 广告位：酷6 pc -->
</script>
