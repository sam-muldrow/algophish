function windowWidth() {
  return window.innerWidth ? window.innerWidth : document.documentElement.clientWidth;
}
function smallScreen() {
  return (windowWidth() < 540) || !!navigator.userAgent.match(/Android|iPod|iPhone/);
}
if(smallScreen()) { 
  return;    
