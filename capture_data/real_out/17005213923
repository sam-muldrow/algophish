  const customConfigObject = {
    'buckets': [{
      'max': 20,
      'increment': 0.01
    }, {
      'max': 30,
      'increment': 0.05
    }, {
      'max': 50,
      'increment': 0.1
    }]
  };

  var requestManager = {
    sendAdServerRequestWasTriggered: false,
    apsHasCalledBack: false,
    pbjsHasCalledBack: false
  };

  function sendAdServerRequest() {

    if (requestManager.sendAdServerRequestWasTriggered) {
      return;
    }
    requestManager.sendAdServerRequestWasTriggered = true;

    googletag.cmd.push(function() {
      googletag.pubads().refresh();
    });
  }

  function combinedBiddersCallBack() {
    if (requestManager.apsHasCalledBack && requestManager.pbjsHasCalledBack) {
      sendAdServerRequest();
    }
  }
  var googletag = googletag || {};
  googletag.cmd = googletag.cmd || [];
  googletag.cmd.push(function() {
    googletag.pubads().disableInitialLoad();
  });

  var pbjs = pbjs || {};
  pbjs.que = pbjs.que || [];

  pbjs.que.push(function() {
    pbjs.addAdUnits(adUnits);
    pbjs.setConfig({
      priceGranularity: customConfigObject,
      timeoutBuffer: 400,
      enableSendAllBids: false,
      publisherDomain: 'https://hip2save.com',
      consentManagement: {
        cmpApi: 'iab',
        timeout: 1800,
        allowAuctionWithoutConsent: true
      },
      userSync: {
        userIds: [{
          name: "id5Id",
          params: {
            partner: 465
          },
          storage: {
            type: "cookie",
            name: "id5id.1st",
            expires: 90,
            refreshInSeconds: 8*3600
          }
        }],
      },
      schain: {
        validation: "relaxed",
        config: {
          ver: "1.0",
          complete: 1,
          nodes: [
            {
              asi: "gourmetads.com",
              sid: "8575",
              hp: 1
            }
          ]
        }
      }
    });
    pbjs.bidderSettings = {
      gourmetads: {
        bidCpmAdjustment: function(bidCpm) {
          return bidCpm * 0.50;
        }
      },
    };
    pbjs.requestBids({
      bidsBackHandler: pbjsBidsCallBackHandler,
      timeout: PREBID_TIMEOUT
    });
    pbjs.enableAnalytics({
      provider: "sovrn",
      options: {
        affiliateId: 260859
      }
    });
  });

  function pbjsBidsCallBackHandler() {

    if (pbjs.pbjsBidsCallBackHandlerWasTriggered) {
      return;
    }

    pbjs.pbjsBidsCallBackHandlerWasTriggered = true;

    googletag.cmd.push(function() {
      pbjs.setTargetingForGPTAsync();
      requestManager.pbjsHasCalledBack = true;
      combinedBiddersCallBack();
    });
  }

  apstag.init({
    pubID: 'df18b975-8e47-400d-bc6f-0c5dbb93388c',
    adServer: 'googletag'
  });
  apstag.fetchBids(
    apsBids,
    function(bids) {
      googletag.cmd.push(
        function() {
          apstag.setDisplayBids();
          requestManager.apsHasCalledBack = true;
          combinedBiddersCallBack();
        }
      );
    }
  );

  // Failsafe to call gpt
  setTimeout( function() {
    sendAdServerRequest();
  }, FAILSAFE_TIMEOUT );
</script>

<script>
  (function () {
    var gads = document.createElement('script');
    gads.async = true;
    gads.type = 'text/javascript';
    var useSSL = 'https:' == document.location.protocol;
    gads.src = (useSSL ? 'https:' : 'http:') +
            '//securepubads.g.doubleclick.net/tag/js/gpt.js';
    var node = document.getElementsByTagName('script')[0];
    node.parentNode.insertBefore(gads, node);
  })();
</script>

