<script>
window.reportedJavascriptErrors = [];
homeNewRelicReporter = {
appEnvironment: '',
addPageAction : function (actionName, data) {
if (typeof actionName === 'string' && window.newrelic !== undefined && typeof window.newrelic.addPageAction === 'function') {
data = data || {};
if (window.HOME) {
data.isWebview = HOME.isWebview;
data.homeProduct = HOME.currentProduct;
data.homeType = HOME.homeType;
data.channel = HOME.channel;
if (window.HOME.upaTrackerData) {
data.userTrackerId = HOME.upaTrackerData.userid;
data.pageviewId = HOME.upaTrackerData.pageview_id;
}
}
newrelic.addPageAction(actionName, data);
}
},
noticeError : function (message, err) {
if (window.newrelic !== 'undefined' && typeof window.newrelic.noticeError === 'function') {
var environmentTag = window.homeNewRelicReporter.appEnvironment;
var errorContext = {};
if (window.HOME) {
errorContext.isWebview = HOME.isWebview;
errorContext.homeProduct = HOME.currentProduct;
errorContext.homeType = HOME.homeType;
errorContext.channel = HOME.channel;
if (window.HOME.upaTrackerData) {
errorContext.userTrackerId = HOME.upaTrackerData.userid;
errorContext.pageviewId = HOME.upaTrackerData.pageview_id;
}
}
window.reportedJavascriptErrors.push(err);
newrelic.noticeError(new Error(environmentTag + '[HomeReporter] ' + message + ' | ' + err.toString()), errorContext);
}
}
};
</script>
<script>
if (window.homeEarlyJavascriptErrors && window.homeEarlyJavascriptErrors.length > 0) {
for (var earlyErrorIndex in window.homeEarlyJavascriptErrors) {
var message = window.homeEarlyJavascriptErrors[earlyErrorIndex].message;
var errorObj = window.homeEarlyJavascriptErrors[earlyErrorIndex].errorObject;
window.homeNewRelicReporter.addPageAction('WindowError', {
"isEarlyError" : true,
"errorMessage" : message,
"errorStack" : errorObj ? errorObj.stack : 'Unknown'
});
}
}
</script>
