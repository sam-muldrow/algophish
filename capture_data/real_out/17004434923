                    Object.assign(logSWGSubscriptionCheckoutStarted.detail, __tnt.subscription.offers.swgData);
                    window.dispatchEvent(logSWGSubscriptionCheckoutStarted);
                }
            });
        });

        __tnt.subscription.swg = subscriptions;

