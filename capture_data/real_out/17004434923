<script>/*<![CDATA[*/ 
        const marketingPromo = document.querySelector("#tncms-block-2827104 .tsMarketing.inBlockMessage");
        const targetAnonymous = "include";
        const targetRegistered = "exclude";
        const targetSubscribers = "";
        const ctaButton2827104 = document.querySelector("#cta-2827104");
        let userAnonymous = true;
        let userRegistered = false;
        let userSubscriber = false;
        if(TNCMS.User.isLoggedIn())
        {
            userAnonymous = false;
            if(__tnt.user.services==undefined)
            {
                userRegistered=true;
            } else{
                if (__tnt.user.services!="0")
                {
                    userSubscriber = true;
                } else {
                    userRegistered = true;
                }
            }
        } else {
            userRegistered = false;
            userSubscriber = false;
        }
        //ok we have user state, what do the targeting rules say?
        let okToShow = false;
        if(targetAnonymous=='include' && userAnonymous)
        {
            okToShow = true;
        }else if(targetAnonymous=='exclude' && userAnonymous)
        {
            okToShow = false;
        }
        if(targetRegistered=='include' && userRegistered)
        {
            okToShow = true;
        } else if(targetRegistered=='exclude' && userRegistered)
        {
            okToShow = false;
        }
        if(targetSubscribers=='include' && userSubscriber)
        {
            okToShow = true;
        }else if(targetSubscribers=='exclude' && userSubscriber)
        {
            okToShow = false;
        }
        if(okToShow==true)
        {
            try{
                marketingPromo.classList.remove("hidden");
            }catch(err){}
        }
         /*]]>*/</script>
