function show_txt_area()
{
	$('#coup_code_text_area').css('display','inline-block');
	$('#coup_code').css('display','none');
	$('#coup_code_text_area').select();
	document.execCommand('copy');
	$('#coup_code_text_area').css('display','none');
	$('#coup_code').css('display','inline-block');
	window.history.pushState({urlPath:base_url+'hot'},"",base_url+'hot');
   	$('#cp_code').css('background-color', '#2ecc71');
   	$('#cp_code').html('COPIED');
   	$('#coup_code').css('border-color', '#2ecc71');
}

