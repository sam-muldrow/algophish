          $("#need-help-pop-clk").click(function(){
         
            $("#need-help-pop").show();
             $("#new-signup-pop").hide();
            $("#new-login-pop").hide();
          });
         
           $("#need-help-pop").click(function(e)
          {
          $("#need-help-pop").hide();
          e.stopPropagation();
          });
         
           $(".common-modal-pop").click(function(e)
           {
            $(".common-modal-pop").hide();
            e.stopPropagation();
           });
           $(".modal-content-pre").click(function(e)
           {
            e.stopPropagation();
           });
           
           
           $('body').click(function(e) {
    e.stopPropagation();
    $("#sujestion").hide();
});

         });
         
         
