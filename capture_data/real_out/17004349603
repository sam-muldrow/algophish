                  function getTheApp()
                  {
                  	$.post("/get-the-app", $("#get-the-app").serialize(), function(data)
                  	{
                  		
                  			if(data==200)
                  			{
                  				$("#get-the-app-msg").html("Download link has been sent to your mobile");
                  				$("#get-the-app-msg").fadeIn(300).fadeOut(600);
                  			}
