   function custom_search_click(ele)
   {
   
   	window.location=base_url+"search?searchword="+$(ele).text();
   }		
   
   function red_search(item)
   {
   	window.location=base_url+"search?searchword="+item;
   }
   
   $(document).ready(function(){
   				
   	$("input#keyword,input#keyword1").typeahead({	
   		source: function (query, process) 
   		{
   			if(query.length>2)
   			{
   			$.ajax({
   					type: "POST",
   					dataType : "json",
   					data: "option=suggestion&searchword="+query,
   					url: base_url+"search",
   					success: function(data) 
   					{ 
   
   					  process(data);
   					  $('.typeahead.dropdown-menu').children('li.active').removeClass('active').before('<li class="active"><a class="dropdown-item" href="javascript:void(0);" onclick="custom_search_click(this)" role="option">'+query+'</a></li>');
   					}
   				});
   			}			
   		},
   		updater: function(item)
   		{
   			
   			if(typeof item != 'undefined')
   				red_search(item);
   			else
   			{
   				var s_term = '';
   				if(typeof $("input#keyword").val() != 'undefined')
   					s_term = s_term + $("input#keyword").val();
   				if(typeof $("input#keyword1").val() != 'undefined')
   					s_term = s_term + $("input#keyword1").val();
   				window.location=base_url+"search?searchword="+s_term;
   			}
       	}
   	});
   });
</script> <script>
