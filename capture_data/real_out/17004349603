   function red_search1(item)
   {
   	window.location=base_url+"searchnew?searchword="+item;
   }
   
   $(document).ready(function(){
   				
   	$("input#keyword2,input#keyword1").typeahead({	
   		source: function (query, process) 
   		{
   			if(query.length>2)
   			{
   			$.ajax({
   					type: "POST",
   					dataType : "html",
   					data: "option=suggestion&searchword="+query,
   					url: base_url+"searchoption",
   					success: function(data) 
   					{ 
						$("#sujestion").html(data);
