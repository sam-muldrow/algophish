function subscribe_for_mailer(sts)
{
	$('.dealsuberror').css('top','-18px').css('color','#E24A64').hide(0);
	event.preventDefault();
	
		var foot_email = $('#footerinput').val();
		if(foot_email.trim() != '')
		{
			var x = foot_email;
			var atpos = x.indexOf("@");
		    var dotpos = x.lastIndexOf(".");
		    if (atpos<1 || dotpos<atpos+2 || dotpos+2>=x.length)
		    {
		        $('.dealsuberror').html('Please enter valid email id').fadeIn(0).fadeOut(3000);
		        $('#footerinput').val('');
		        return false;
		    }
		    else
		    {
		    	$.post(base_url + 'user/subscribe-mailer', 
						{
							'user_email':foot_email
						}, 
						function(data)
						{
							
							if(data.trim() == '2')
							{
								$('.dealsuberror').html('Please enter valid email id').fadeIn(0).fadeOut(3000);
						        $('#footerinput').val('');
						        return false;
							}
							else if(data.trim() == 'already')
							{
								$('.footerform').hide(0);
								$('.dealsuberror').html('You have already subscribed for our newsletter').css('top','24px').css('color','#6FB945').show(0);
								return false;
							}
							
							else if(data.trim() == 'added')
							{
								$('.footerform').hide(0);
								$('.dealsuberror').html('You have been subscribed for our newsletter').css('top','24px').css('color','#6FB945').show(0);
								return false;
							}
							else
							{
								$('.dealsuberror').html('Something went wrong please try again later').fadeIn(0).fadeOut(3000);
								setTimeout(function(){
									location.reload();
								},3500);
								return false;
							}
						}
					);
		    }
		}
		else
		{
			$('.dealsuberror').html('Please enter valid email id').fadeIn(0).fadeOut(3000);
			return false;
		}
	// }
}
</script> </body> </html> 