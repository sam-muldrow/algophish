<script type="text/javascript">
    function WhiteBarNotifyLoad() {
        $.ajax({
            type: "post",
            url: "/Home/BWhiteBarNotifyPartial",
            dataType: "html",
            data: {},
            success: function (data) {
                if (data != "") {
                    if (data.indexOf("no_notice") != -1) {
                        $(".dropdown_notice").addClass("no_notice");
                    }
                    $(".dropdown_notice").html(data);
                    //分頁
                    $(".scroll").scroll(function () {
                        var _this = $(this);
                        // 可見高度
                        var viewH = _this.height();
                        // 內容高度
                        var contentH = _this.get(0).scrollHeight;
                        // 滾動高度
                        var scrollTop = _this.scrollTop();
                        if (contentH - viewH - scrollTop <= 30) {
                            if ($("[id^=recordbarMore]").length > 0) {
                                var fid = $("[id^=recordbarMore]").attr("id").split("_");
                                $("#recordbarMore_" + fid[1]).remove();
                                $.ajax({
                                    type: "post",
                                    url: "/Home/BWhiteBarNotifyPartial",
                                    dataType: "html",
                                    data: { pages: fid[1] },
                                    success: function (dataNext) {
                                        $(".scrollBar").append(dataNext);
                                    }
                                });
                            }
                        }
                    });
                }
            }
        });
    }
</script>


        

