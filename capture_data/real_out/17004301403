    <!-- bootstrap.js -->
    
    <!-- swiper.js -->
    <script type="text/javascript" src="/assets/js/swiper-bundle.min.js"></script>
    <script type="text/javascript" src="/assets/js/main.min.js?v=20230928"></script>
    <script src="https://www.1111.com.tw/includesU/clickCount.js" type="text/javascript"></script>
    <script async src="https://images.1111.com.tw/aidma/adImpression.js" type="text/javascript"></script>
    
    <script type="text/javascript">

            (() => detectMobileBanner('False'))()

        function detectMobileBanner(isMobile)
        {
            const _isMobile = isMobile.toLowerCase() === 'true'
            const _appDownloadHome = localStorage.getItem('AppDownloadHome')
            const downloadApp = document.getElementById('downloadApp')

            if (_isMobile && _appDownloadHome !== '1') downloadApp.style.display = ''
            if (_appDownloadHome == '1')
            {
                var searchbar = document.querySelector('.search-bar');
                if (searchbar) { searchbar.classList.remove('downloadShow') }
            }
        }

        // 關閉ios滾動橡皮筋效果
        //document.body.addEventListener('touchmove', (e) => {
        //    if (e._isScroller) return;
        //    e.preventDefault();
        //}, { passive: false });
        //document.querySelector('.handleTouch').addEventListener('touchmove', (e) => {
        //    e._isScroller = true;
        //}, { passive: false });
        function clearSearchInput() {
            var inputElement = document.querySelector(".search-input");
            var closeIcon = document.querySelector("#closeIcon");
                inputElement.value = "";
                closeIcon.style.display = "none";
            }

        document.addEventListener('DOMContentLoaded', () => {
            function autoSlideHeight() {
                const screenWidth = window.innerWidth;
                const slideEventNews = document.querySelector(".swiperEventNews");
                if (screenWidth < 992) {
                    slideEventNews.style.height = ((slideEventNews.offsetWidth / 3 * 2) + 20) + 'px';
                } else {
                    slideEventNews.removeAttribute('style');
                }
            };

            autoSlideHeight();

            window.addEventListener('resize', () => {
                autoSlideHeight();
            });

            // 身份類別拖動 START
            // 檢測寬度
            function checkScreenWidth() {
                const screenWidth = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
                if (screenWidth <= 1400) {
                    const clickable = document.querySelector('.nav-wrapper-in');
                    clickable.classList.add('clickable');
                } else {
                    const clickable = document.querySelector('.nav-wrapper-in');
                    clickable.classList.remove('clickable');
                }
            }
            function checkChickable() {
                const clickable = document.querySelectorAll('.clickable');
                let isDragging = false;
                let startPosition = 0;
                let scrollLeft = 0;
                clickable.forEach((clickable) => {
                    // 開始拖動事件
                    const startDragging = (e) => {
                        isDragging = true;
                        startPosition = e.clientX || e.touches[0].clientX;
                        scrollLeft = clickable.scrollLeft;
                        clickable.style.cursor = 'grabbing';
                        clickable.classList.add('touching');
                    };

                    // 結束拖動事件
                    const endDragging = () => {
                        isDragging = false;
                        clickable.style.cursor = 'grab';
                        clickable.classList.remove('touching');
                    };

                    // 滑鼠監聽
                    clickable.addEventListener('mousedown', (e) => {
                        startDragging(e);
                    });
                    clickable.addEventListener('mousemove', (e) => {
                        if (!isDragging) return;
                        const delta = (e.clientX || e.touches[0].clientX) - startPosition;
                        clickable.scrollLeft = scrollLeft - delta;
                    });
                    clickable.addEventListener('mouseup', endDragging);
                    clickable.addEventListener('mouseleave', endDragging);

                    // 滑動監聽
                    clickable.addEventListener('touchstart', (e) => {
                        startDragging(e.touches[0]);
                    });
                    clickable.addEventListener('touchmove', (e) => {
                        if (!isDragging) return;
                        const delta = e.touches[0].clientX - startPosition;
                        clickable.scrollLeft = scrollLeft - delta;
                    });
                    clickable.addEventListener('touchend', endDragging);
                    clickable.addEventListener('touchcancel', endDragging);
                });
            }
            const navWrapperIn = document.querySelector('.nav-wrapper-in');
            window.addEventListener('load', (e) => {
                checkScreenWidth();
                if (navWrapperIn.classList.contains('clickable')) {
                    checkChickable();
                }
            });
            window.addEventListener('resize', (e) => {
                checkScreenWidth();
                if (navWrapperIn.classList.contains('clickable')) {
                    checkChickable();
                }
            });
            // 身份類別拖動 END

            // swiper 公用
            const commonSwiper = (name,title)=>
            {
                const storageName = 'adCount'

                const getCounter = () => {

                    const content = localStorage.getItem(storageName)
                    const result = !content ? init : JSON.parse(content)

                    return result
                }

                const setCounter = (result) => {
                    localStorage.setItem(storageName, JSON.stringify(result))
                }

                // 累加 曝光紀錄
                // send to API
                const addCount = (countName, ad_id) => {
                    if (!ad_id) return undefined
                    const adCounter = getCounter()
                    const counter = adCounter[countName]
                    const isExist = counter.find(c => c === ad_id)

                    if (!isExist) {

                        counter.push(ad_id)

                        setCounter(adCounter)

                        return ad_id
                    }

                    return undefined
                }

                const onSildeChange = async (swiper, name, index) => {
                    const adIdName = 'adid'
                    // 更新圖片顯示次數
                    const sildePages = {
                        eventNews: 2,
                        hqCompanies: 12,
                        mainEvents: 2
                    }

                    const getImageAdIds = (images) =>
                    {
                        return images.map((image) => {
                            const _a = image.querySelector('a')
                            const id = _a.dataset[adIdName]
                            return id
                        })
                    }

                    const totalAdIds = getImageAdIds(swiper.slides)

                    //一頁有幾個
                    const pages = sildePages[name]

                    let activeIndex = !!index ? index : swiper.activeIndex

                    if (name === 'hqCompanies')
                    {
                        activeIndex = activeIndex % 4 === 0 && activeIndex !== 0 ? ((activeIndex / 4) * pages) : 0
                    } else
                    {
                        activeIndex = activeIndex != 0 ? (activeIndex* pages) : 0
                    }

                    const imageCount = activeIndex + pages

                    const images = swiper.slides.slice(activeIndex, imageCount)

                    const ids = getImageAdIds(images)

                    const result = ids.map(id => addCount(name, id)).filter(function (id) { return !!id })

                    if (result.length === 0) return

                    await $.post('/adbanner/adImpression.asp', { 'adids': result.join(',') })
                        .then(() => {
                            console.log(`%c ${title}-曝光紀錄-共(${result.length})筆,傳送請求都完成了!`, 'color:greenyellow;paddingleft:15px;')
                        })
                        .catch(function(error){
                            console.error(`${title}-曝光紀錄請求失敗了`, error)
                        })
                }

                const action = (swiper)=>
                {
                    if(!swiper)
                    {
                        return {
                            init: (swiper) => onSildeChange(swiper, name, 0),
                            slideChange: async (swiper) => onSildeChange(swiper, name),
                        }
                    }
                    return onSildeChange(swiper, name)
                }
                return action
            }

            const commonPagination = {el: '.swiper-footer .swiper-pagination'}

            const commonNavigation = {
                nextEl: '.swiper-footer .swiper-button-next',
                prevEl: '.swiper-footer .swiper-button-prev'}

            //  活動快訊
            const swiperEventNews = new Swiper('.swiper.swiperEventNews', {
                slidesPerView: 1,
                /*slidesPerGroup: 2,*/
                spaceBetween: 20,
                grid: {
                    fill: 'column',
                    rows: 2,
                },
                pagination: {
                    el: '.swiper-footer .swiper-pagination',
                },
                navigation: {
                    nextEl: '.swiper-footer .swiper-button-next',
                    prevEl: '.swiper-footer .swiper-button-prev',
                },
                // autoplay: {
                //     delay: 8000,
                // },
                on: {
                    init: async function() {

                        commonSwiper('eventNews', '活動快訊')(this)

                        setTimeout(function() {
                            function startAutoPlay() {
                                autoPlayInterval2 = setInterval(function() {
                                swiperEventNews.slideNext(); // 切换到下一個 slide
                            }, 8000);
                        }
                        function stopAutoPlay() {
                            clearInterval(autoPlayInterval2);
                        }
                        swiperEventNews.el.addEventListener('mouseenter', function() {
                            stopAutoPlay();
                        });
                        swiperEventNews.el.addEventListener('mouseleave', function() {
                            startAutoPlay();
                        });
                        swiperEventNews.el.addEventListener('touchstart', function() {
                            stopAutoPlay();
                        });
                        swiperEventNews.el.addEventListener('touchend', function() {
                            startAutoPlay();
                        });
                        // 初始啟動自動播放
                        startAutoPlay();
                        // resize
                        window.addEventListener('resize', function() {
                            stopAutoPlay();
                            startAutoPlay();
                        });
                        }, 500);
                    },
                    slideChange: async (swiper) => commonSwiper('eventNews', '活動快訊')(swiper) /*曝光統計*/
                },
                rewind: true,
                disableOnInteraction: false,
            })

            // swiper 優質企業
            var swiperHqCompany = new Swiper('.swiper.swiperHqCompany', {
                slidesPerView: 3,
                slidesPerGroup: 3,
                spaceBetween: 6,
                grid: {
                    fill: 'row',
                    rows: 1,
                },
                // autoplay: {
                //     delay: 8000,
                // },
                on: {
                    init: function () {

                        commonSwiper('hqCompanies', '優質企業')(this)

                        setTimeout(function () {
                            function startAutoPlay2() {
                                autoPlayInterval = setInterval(function () {
                                    swiperHqCompany.slideNext(); // 切换到下一個 slide
                                }, 8000);
                            }
                            function stopAutoPlay2() {
                                clearInterval(autoPlayInterval);
                            }
                            swiperHqCompany.el.addEventListener('mouseenter', function () {
                                stopAutoPlay2();
                            });
                            swiperHqCompany.el.addEventListener('mouseleave', function () {
                                startAutoPlay2();
                            });
                            swiperHqCompany.el.addEventListener('touchstart', function () {
                                stopAutoPlay2();
                            });
                            swiperHqCompany.el.addEventListener('touchend', function () {
                                startAutoPlay2();
                            });
                            // 初始啟動自動播放
                            startAutoPlay2();
                            // resize
                            window.addEventListener('resize', function () {
                                stopAutoPlay2();
                                startAutoPlay2();
                            });
                        }, 500);
                    },
                    slideChange: async (swiper) => commonSwiper('hqCompanies', '優質企業')(swiper)/*曝光統計*/
                },
                breakpoints: {
                    576: {
                        slidesPerView: 4,
                        slidesPerGroup: 4,
                        spaceBetween: 18,
                        grid: {
                            fill: 'column',
                            rows: 1,
                        },
                    },
                    992: {
                        slidesPerView: 4,
                        slidesPerGroup: 12,
                        spaceBetween: 18,
                        grid: {
                            fill: 'column',
                            rows: 3,
                        },
                    },
                },
                pagination: {
                    el: '.swiper-footer .swiper-pagination',
                },
                navigation: {
                    nextEl: '.swiper-footer .swiper-button-next',
                    prevEl: '.swiper-footer .swiper-button-prev',
                },
                rewind: true,
                disableOnInteraction: false,
            });

            // 精選類別 收藏按鈕
            //const bookmarkJobCard = document.querySelectorAll('.rmJob-left .card-bookmark');
            //bookmarkJobCard.forEach(bookmarkJobCard => {
            //    bookmarkJobCard.addEventListener('click', () => {
            //        const thisClick = event.target.closest('.card-bookmark');
            //        if (thisClick.classList.contains('active')) {
            //            thisClick.classList.remove('active');
            //        } else {
            //            thisClick.classList.add('active');
            //        }
            //    });
            //});

            // swiper Card精選類別
            var swiperJobMore = new Swiper('.swiper.swiperJobMore', {
                slidesPerView: 1,
                slidesPerGroup: 1,
                spaceBetween: 10,
                grid: {
                    fill: 'column',
                    rows: 4,
                },
                autoplay: {
                    delay: 10000,
                },
                breakpoints: {
                    768: {
                        slidesPerView: 2,
                        slidesPerGroup: 4,
                        spaceBetween: 16,
                        grid: {
                            fill: 'column',
                            rows: 4,
                        },
                    },
                    992: {
                        slidesPerView: 2,
                        slidesPerGroup: 8,
                        spaceBetween: 16,
                        grid: {
                            fill: 'column',
                            rows: 4,
                        },
                    },
                },
