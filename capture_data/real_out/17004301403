        $(function () {
            $.login.init();

            $('#c0Cht').click(function (e) {
                e.preventDefault();
                open1111Menu('c0', 'city_', '10', 'categoryInputText jbdark arrowInputText', '', 'chkhint(1); ', '');
                $(".tcode-btn,.tcode__btn").click(function (e) {
                    e.preventDefault();
                    var strCity = $("#c0").val();
                    if (strCity == "") {
                        $(".label_numC0").html(0);
                        $(".label_numC0").addClass("d-none");
                    } else {
                        $(".label_numC0").html(strCity.split(",").length);
                        $(".label_numC0").removeClass("d-none");
                    }
                });
            });
            $('#d0Cht').click(function (e) {
                e.preventDefault();
                open1111Menu('d0', 'duty_NM', '10', 'categoryInputText jbdark arrowInputText', '', 'chkhint(3); ', '', '120218');
                $(".tcode-btn,.tcode__btn").click(function (e) {
                    e.preventDefault();
                    var strDuty = $("#d0").val();
                    if (strDuty == "") {
                        $(".label_numD0").html(0);
                        $(".label_numD0").addClass("d-none");
                    } else {
                        $(".label_numD0").html(strDuty.split(",").length);
                        $(".label_numD0").removeClass("d-none");
                    }
                });
            });
            var HtmlUtil = {
                HtmlEncode: function (html) {
                    var tempDiv = document.createElement("div");
                    if (tempDiv.textContent != undefined) {
                        tempDiv.textContent = html;
                    } else {
                        tempDiv.innerText = html;
                    }
                    var output = tempDiv.innerHTML;
                    tempDiv = null;
                    return output;
                }
            };
            $('#searchjobBtn').click(function () {
                var pam = "";
                if ($('#ks').val() != "") {
                    if (pam != "") {
                        pam = pam + '&';
                    }
                    pam = pam + 'ks=' + HtmlUtil.HtmlEncode(encodeURI($('#ks').val())).replace(/,/g, '%2C');
                }
                if ($('#d0').val() != "" && $('#d0').val() != "0") {
                    if (pam != "") {
                        pam = pam + '&';
                    }
                    pam = pam + 'd0=' + $('#d0').val();
                }
                if ($('#c0').val() != "" && $('#c0').val() != "0") {
                    if (pam != "") {
                        pam = pam + '&';
                    }
                    pam = pam + 'c0=' + $('#c0').val();
                }
                if (pam != "") {
                    top.location = '/search/job?' + pam;
                } else {
                    top.location = '/search/job';
                }
            });

            $('#ks').click(function () {
                var $KS = HtmlUtil.HtmlEncode($('#ks').val());
                if ($KS == "") {
                    var _html = '';
                    var _logs = undefined;
                    try { _logs = JSON.parse(localStorage.getItem("AdvSearchLog")); } catch (err) { }
                    if (_logs == undefined) {
                        _logs = [];
                    }

                    for (var i = 0; i < _logs.length; i++) {
                        var _log = _logs[i];
                        if (i < 5) {
                            if (_log['c'].length > 50) {
                                _log['c'] = _log['c'].substring(1, 50) + "...";
                            }
                            _html += '<li><a href = "{2}" target = "_self"><p>{0}－{1}</p></a><button type="button" onclick="searchLogDel(this)"><img src="assets/images/icon/Icon-X.svg" alt="delete" width="24" height="24" decoding="async" loading="lazy"/></button></li>'.format(_log["searchType"], _log['c'], _log['q']);
                        }
                    }
                    if (_html.length > 0) {
                        $(".dropdown-header-searchlog").html('<li class="dropdown-header"><p>最近搜尋</p></li>' + _html);
                        cleanSearch();
                        openHistory();
                        if (!$(".search-dropdown").hasClass('show')) {
                            $(".search-dropdown").addClass('show');
                        }
                    }
                }
            });

            $("#ks").blur(function () {
                $(".search-dropdown").removeClass('show');
            });

            $(".search-dropdown").mouseover(function () {
                $('#ks').unbind('blur');
            }).mouseout(function () {
                $('#ks').on('blur', function () {
                    $('.search-dropdown').removeClass('show');
                });
            });
            
            var searchDropdown = document.querySelector('.search-dropdown');
            var ksInput = document.getElementById('ks');
            document.addEventListener('click', function (event) {
                if (!searchDropdown.contains(event.target) && event.target !== ksInput) {
                    searchDropdown.classList.remove('show');
                }
            });
            document.querySelector('.handleTouch').addEventListener("scroll", () => {
                const screenWidth = window.innerWidth;
                if (screenWidth > 480) {
                    $(".search-dropdown").removeClass('show');
                }
            });

            $("#ks").keyup(function (event) {
                if (event.keyCode === 13) {
                    $("#searchjobBtn").click();
                }
                if ($('#ks').val() == '') {
                    $(".search-dropdown").removeClass('show');
                }
            }).autocomplete({
                minLength: 1,
                matchSubset: false,
                matchContains: true,
                source: function (request, response) {
                    $(".search-dropdown").removeClass('show');
                    var strKS = HtmlUtil.HtmlEncode($('#ks').val());
                    if (strKS != null && strKS != '') {
                        $.ajax({
                            url: '/Home/AutoCompleteEmpAjax',
                            type: 'Post',
                            data: { keyword: HtmlUtil.HtmlEncode(request.term) },
                        }).done(function (result) {
                            var strlog = "";
                            $.map(result.result, function (item) {
                                strlog += "<li>" +
                                    "     <a href=\"javascript:;\" onclick=\"selectOption('" + item + "')\" target=\"_self\">" +
                                    "         <img src=\"assets/images/icon/icon-search.svg\" alt=\"search\" width=\"16\" height=\"16\" decoding=\"async\" loading=\"lazy\" />" +
                                    "         <p>" + item + "</p>" +
                                    "     </a>" +
                                    " </li>";
                            });
                            if (strlog.length > 0) {
                                $(".search-autocomplete").html(strlog);
                                cleanSearch();
                                openAutocomplete();
                                if (!$(".search-dropdown").hasClass('show')) {
                                    $(".search-dropdown").addClass('show');
                                }
                            }
                        }).fail(function (err) {
                            // console.log(err);
                        });
                    } else { $(".search-dropdown").removeClass('show'); }
                }
            });

            const searchBar = document.querySelector('#searchDropdown');
            const searchParent = searchBar.closest('.search-bar');
            const searchAuto = searchParent.querySelector('.search-autocomplete');
            const searchHist = searchParent.querySelector('.search-history');
            const searchKywo = searchParent.querySelector('.search-keyword');
            const inputElement = document.querySelector(".search-input");
            const closeIcon = document.querySelector("#closeIcon");
            function cleanSearch() {
                if (searchAuto.classList.contains('d-none')) {
                    searchAuto.classList.remove('d-none');
                }
                if (searchAuto.classList.contains('d-block')) {
                    searchAuto.classList.remove('d-block');
                }
                if (searchHist.classList.contains('d-none')) {
                    searchHist.classList.remove('d-none');
                }
                if (searchHist.classList.contains('d-block')) {
                    searchHist.classList.remove('d-block');
                }
                if (searchKywo.classList.contains('d-none')) {
                    searchKywo.classList.remove('d-none');
                }
                if (searchKywo.classList.contains('d-flex')) {
                    searchKywo.classList.remove('d-flex');
                }
            };
            function openAutocomplete() {
                searchAuto.classList.add('d-block');
                searchHist.classList.add('d-none');
                searchKywo.classList.add('d-none');
            };
            function openHistory() {
                searchAuto.classList.add('d-none');
                searchHist.classList.add('d-block');
                searchKywo.classList.add('d-flex');
            };
            function openDropdown(){
                if((searchBar.querySelector('input').value)==''){
                    cleanSearch();
                    openHistory();
                    closeIcon.style.display = "none";
                } else {
                    cleanSearch();
                    openAutocomplete();
                    closeIcon.style.display = "block";
                }
            }
            searchBar.querySelector('input').addEventListener('click', () => {
                openDropdown();
            });
            searchBar.querySelector('input').addEventListener('input', () => {
                openDropdown();
            });
             
            $('.lnkCollect').click(function() {
                var self = $(this);
                var eno = $(this).attr('eno');
                
                $.login.instance.updateLoginResponse({
                    loginSuccess: function () {
                        $.fancybox.close();
                        addOrDelJobCollect(self);
                        window.open("/", "_self");
                    }
                });

                
                $.login.instance.getLoginStatus({
                    isLogin: function () {
                        addOrDelJobCollect(self, eno);
                    }
                });
            });
        });
        function searchLogDel(sender) {
            var _result = [];
            var _logs = undefined;
            var _q = $(sender).prev("a").attr("href");
            if (_q.length > 0) {
                try { _logs = JSON.parse(localStorage.getItem("AdvSearchLog")); } catch (err) { }
                if (_logs == undefined)
                    _logs = [];
                for (var i = 0; i < _logs.length; i++) {
                    var _log = _logs[i];
                    if (_q != _log["q"])
                        _result.push(_log);
                }
                try { localStorage.setItem("AdvSearchLog", JSON.stringify(_result)); } catch (err) { }
                $("#ks").click();
            }
        }
         
        function addOrDelJobCollect(self, eno) {
            self.prop("disabled", true);
            var isHaveCollect = self.hasClass('active');
            if (isHaveCollect) {
                // 已收藏，取消收藏
                $.ajax({
                    url: '/Job/DelJobCollectAjax',
                    type: "post",
                    data: { employeeNo: eno },
                    dataType: "JSON",
                    success: function (data) {
                        if (data.errMsg === '') {
                            self.removeClass("active");
                        }
                        self.prop("disabled", false);
                    }
                });
            } else {
                // 未收藏，新增收藏
                $.ajax({
                    url: '/Job/AddJobCollectAjax',
                    type: "post",
                    data: { employeeNo: eno },
                    dataType: "JSON",
                    success: function (data) {
                        if (data.errMsg === '') {
                            self.addClass("active");
                        }
                        self.prop("disabled", false);
                    }
                });
            }
        }
        function setHrefKK(lNo, tNo, eNo) {
            $.get('https://www.1111.com.tw/LoginService/talentServiceOut.ashx?apFun=kk_track&tNo=' + tNo + '&eNo=' + eNo);
            setHref(lNo);
        }

        function selectOption(option) {
            document.getElementById('ks').value = option;
            $(".search-dropdown").removeClass('show');
        }
    </script>

    <script src="/Content2/js/plugin/bootstrap.min.js" type="text/javascript" charset="utf-8"></script>
    <script src="/chat_plugin/js/talk.js?v=202310180"></script>
    <script>
        const mobileSidebarBtn = document.querySelector("a.mobileSidebarBtn");
        const sidebarBtn = document.querySelector("a.nav_more.sidebar_btn");
        const sidebarSlide = document.querySelector(".sidebar-slide");
        const sidebarClose = document.querySelector('.sidebar-close');
        mobileSidebarBtn.addEventListener("click", function (e) {
            e.preventDefault();
            if (sidebarSlide.className.indexOf('show') > -1) {
                sidebarClose.click();
            } else {
                sidebarBtn.click();
            }
        });
    </script>
    
