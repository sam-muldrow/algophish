    function trackLogs($url,$referrer,$Page,$eList) {
        var _fnReplace = function (s) { return s.replace(/&/g, '@!1!@').replace(/#/g, '@!2!@').replace(/\?/g, '@!3!@'); };
        var $talentsLogID = localStorage.getItem('talentsLogID') + '';

		if ($talentsLogID == '' || $talentsLogID == 'null' || $talentsLogID.length < 20) { 
            $talentsLogID = '20231120054221326185';
            localStorage.setItem('talentsLogID', $talentsLogID); 
        }        
        
        var $CanonicalURL = _fnReplace('');
        var $SEOCount = '';
        var sp = document.createElement('script'); sp.type = 'text/javascript'; sp.async = true;
		    sp.src = 'https://www.1111.com.tw/LoginService/talentServiceOut.ashx?apFun=sp_track&talentsLogID=' + encodeURIComponent($talentsLogID)
            + '&talentNo=' + encodeURIComponent('')
            + '&host=' + encodeURIComponent(location.host)
            + '&pathname=' + encodeURIComponent(location.pathname)
            + '&href=' + encodeURIComponent(_fnReplace($url))
            + '&referrer=' + encodeURIComponent(_fnReplace($referrer))
            + '&CanonicalURL=' + encodeURIComponent($CanonicalURL)
            + '&SEOCount=' + encodeURIComponent($SEOCount)
            + '&Page=' + encodeURIComponent($Page)
            + '&eList=' + encodeURIComponent($eList);
		var sn = document.getElementsByTagName('script')[0]; sn.parentNode.insertBefore(sp, sn);        
    }

    try {
