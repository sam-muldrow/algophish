<script type="text/javascript">
(function(){
  var times = 20;
  var currentTime = 1;
  var interval = 1000;
  var changed = false;

  function changeQrcode() {
    try{
       var qrDom = document.getElementsByClassName('expand-qrc')[0];
      if(qrDom && !changed ) {
        qrDom.style.backgroundImage = "url(https://gw.alicdn.com/imgextra/i4/O1CN01nnWuYI1fzJW1BI47n_!!6000000004077-2-tps-500-500.png_128x128)"
        changed = true;
      }
      (changed || currentTime > times) && clearInterval(intervalID);
    }catch(e){}
    ++currentTime;
  }
  var intervalID = setInterval(changeQrcode, 1000)
})();
</script>

