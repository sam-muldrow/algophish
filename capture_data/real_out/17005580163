        function getJsonObj(jsonStr) {
            var jsonObj = {};
            if (jsonStr) {
                try {
                    jsonObj = JSON.parse(jsonStr);
                } catch (e) {
    
                    jsonObj = {}
                }
    
            } else {
                jsonObj = {}
            }
            return jsonObj;
        }
