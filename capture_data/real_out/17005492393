document.cookie = disableStr + '=true; expires=Thu, 31 Dec 2099 23:59:59 UTC; domain=.'
+ location.hostname.replace(/^(([^.]+.)(.*?))$/, '$3')
+ '; path=/';
window[disableStr] = true;
