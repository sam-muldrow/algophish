const addScriptTag = (src, id) => {
const scriptTag = document.createElement('script');	
scriptTag.async = true;
scriptTag.src = src;
if(id !== "") {
scriptTag.id = id;
}	
const ls = document.getElementsByTagName('script')[0];
ls.parentNode.insertBefore(scriptTag, ls);
}
checkCustVendConsent = function (consentedVendors, success) {
var arrConsentedCustVendNames = [];
for (var i = 0; i < consentedVendors.length; i++) {
arrConsentedCustVendNames.push(consentedVendors[i].name);
}
if (arrConsentedCustVendNames.indexOf("Echobox") > -1) {
addScriptTag('//applets.ebxcdn.com/ebx.js', 'ebx');	
}
if (arrConsentedCustVendNames.indexOf("Google Analytics") > -1) {
// js code or function call goes here
// js variables are not yet available in (memcached) pagehead_tocache.inc.
// solution is extending the first dataLayer array field with additional properties 
// ('push()'' as documented doesn't work here, because at initial pageview only first array field becommes processed):
if (typeof dataLayer === 'object') {
dataLayerExt = {
'iViewportWidth': jQuery(window).width(),
'blnUserLoggedIn': bIsLoggedin
}
dataLayer[0] = Object.assign(dataLayer[0], dataLayerExt);
}
