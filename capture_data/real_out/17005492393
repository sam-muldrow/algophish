});
}	
});
});
},
init: function () {
ga4Module.gtag('js', new Date());
ga4Module.gtag('config', ga4Module.gaCode);	
},
gtag: function () {
window.dataLayer = window.dataLayer || [];
dataLayer.push(arguments);
},
push: function (params) {
window.dataLayer = window.dataLayer || [];
dataLayer.push(params);
},
sendEvent: function (name, params) {
ga4Module.gtag('event', name, params);	
},
setSendPageView: function (sendPageView) {
ga4Module.gtag('config', ga4Module.gaCode, {
'send_page_view': sendPageView
});
},
loadTagManager: function (gId) {
return new Promise((resolve, reject) => {
try {
let elemScript = document.createElement("script");
elemScript.setAttribute("src", "https://www.googletagmanager.com/gtag/js?id=" + gId);
elemScript.setAttribute("async", "async");
elemScript.addEventListener("load", () => {
resolve("Tagmanager loaded");
});
elemScript.addEventListener("error", () => {
reject("Failed to load TagManager");
});
document.body.appendChild(elemScript);
}
catch(error) {
reject(error);
}
});	
}
}
ga4Module.initCmp();	
</script>	
