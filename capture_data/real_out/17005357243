iv></div></div><div id="floating-button-box" class="floating-button"><div id="floating-button-notification" class="notification"><div class="content">Have questions? Get help from a DJI expert.</div></div><button id="floating-button" data-link="https://dji-glb.livechat-support.com/yc-media/pages/web-client.jsp?channelKey=naowentry_Others&amp;keyCode=default&amp;language=en&amp;site=brandsite&amp;from=landing_page" style="background-image:url(&quot;https://www1.djicdn.com/cms/uploads/e637a7bcf96efd34a40ec4430876cbda.jpg&quot;)" class="button"></button></div><script>(function(){
  const notificationDom = document.querySelector('#floating-button-notification')
  const buttonDom = document.querySelector('#floating-button');
  const floatButtonDom = document.querySelector('#floating-button-box');
  const isMobile = $('body').hasClass('dji-mobile');
  const link = buttonDom && buttonDom.dataset.link;
  //- 处理初始的倒计时
  function setTimer() {
    setTimeout(() => {
      // 倒计时 0.3s 显示通知
      handleNotificationLoad(true);
    }, 300);
    setTimeout(() => {
      // 倒计时 5s 隐藏通知
      handleNotificationLoad(false);
    }, 5000);
  };
  //- 处理 文案出现
  function handleNotificationLoad(flag) {
    if (flag) {
      notificationDom.classList.add('active')
    } else {
      notificationDom.classList.remove('active')
    }
  }
  //- 鼠标hover 出现文案
  function handleMouseEnter() {
    if(!isMobile) {
      buttonDom.addEventListener('mouseenter', function() {
        handleNotificationLoad(true);
        window.ga && window.ga('send', 'event', 'float_button', 'hover', 'service_hover');
        buttonDom.classList.remove('active');
      })
      buttonDom.addEventListener('mouseleave', function() {
        handleNotificationLoad(false);
      })
    }
  }
  //- 处理按钮点击事件
  function handleBtnClick() {
    buttonDom.addEventListener('click', function() {
      handleNotificationLoad(false);
      window.ga && window.ga('send', 'event', 'float_button', 'click', 'service_click');
      if (isMobile) {
        const top = (window.screen.availHeight - 30 - 667) / 2;
        const left = (window.screen.availWidth - 10 - 375) / 2;
        window.open(
          link,
          '',
          `width=375,height=667,top=${top},left=${left},scrollbars=yes,resizable=yes`,
        );
      } else {
        const top = (window.screen.availHeight - 30 - 630) / 2;
        const left = (window.screen.availWidth - 10 - 768) / 2;
        window.open(
          link,
          '',
          `width=768,height=630,top=${top},left=${left},scrollbars=yes,resizable=yes`,
        );
      }
      buttonDom.classList.add('active');
    })
  }
  //- 处理让悬浮按钮出现
  function handleShowFloatingButton() {
    floatButtonDom.style.display = 'block';
  }
  //- didMount
  window.addEventListener('DOMContentLoaded', function() {
    if (!window.ABTest) {
      window.ABTest = {};
    }
    window.ABTest.showProductFloating = () => {
      handleShowFloatingButton();
      setTimer();
      handleMouseEnter();
      handleBtnClick();
    };
  });
}())</script><div class="footer-about"><div class="footer-nav"><a href="https://www.dji.com" data-ga-category="pc-footer" data-ga-action="click" data-ga-label="other-logo" class="nav-brand ga-data"><img data-layzr="https://www1.djicdn.com/dps/176201ad61d0753e17d860c4ca117932.svg" alt="DJI Logo" class="logo gray"><img data-layzr="https://www1.djicdn.com/dps/a4cb06c299208ffb34ecdcf8b2a5d94a.svg" alt="DJI Logo" class="logo white"></a><ul class="nav-right"><li class="nav-item"><a href="https://www.dji.com/company?site=brandsite&amp;from=footer" target="_blank" data-ga-category="pc-footer" data-ga-action="click" data-ga-label="other-about-Who We Are" aria-label="other-about-Who We Are" class="ga-data">Who We Are</a></li><li class="nav-item"><a href="https://www.dji.com/contact?site=brandsite&amp;from=footer" target="_blank" data-ga-category="pc-footer" data-ga-action="click" data-ga-label="other-about-Contact Us" aria-label="other-about-Contact Us" class="ga-data">Contact Us</a></li><li class="nav-item"><a href="https://we.dji.com/index_en.html?site=brandsite&amp;from=footer" target="_blank" data-ga-category="pc-footer" data-ga-action="click" data-ga-label="other-about-Careers" aria-label="other-about-Careers" class="ga-data">Careers</a></li><li class="nav-item"><a href="https://prm.dji.com/?site=brandsite&amp;from=footer" target="_blank" data-ga-category="pc-footer" data-ga-action="click" data-ga-label="other-about-Dealer Portal" aria-label="other-about-Dealer Portal" class="ga-data">Dealer Portal</a></li><li class="nav-item"><a href="https://www.robomaster.com?site=brandsite&amp;from=footer" target="_blank" data-ga-category="pc-footer" data-ga-action="click" data-ga-label="other-about-RoboMaster" aria-label="other-about-RoboMaster" class="ga-data">RoboMaster</a></li><li class="nav-item"><a href="https://auto.dji.com/?site=brandsite&amp;from=footer" target="_blank" data-ga-category="pc-footer" data-ga-action="click" data-ga-label="other-about-DJI Automotive" aria-label=