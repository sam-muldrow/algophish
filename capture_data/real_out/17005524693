<script type="text/javascript">
	window.handleClick = null
	window.page_id = 'aipai_official_index_1'
		$(function() {

			window.showTwelve = function () {
				$('body').find('#TwelveDialog').show()
			}

			window.closeTwelveDialog = function () {
				$('body').find('#TwelveDialog').hide()
			}
			window.handleClick = function(type) {
				var targetUrl = ''
				if (type === 'clip') {
					targetUrl = PageHeader.downPopupMode.clip !== 0 ? ('https://jianji.aipai.com/down.php?type=down&mode=' + PageHeader.downPopupMode.clip) : 'https://jianji.aipai.com'
					window.open('https://download.aipai.com/aipai_cliper_files/aipai_official/aipai_cliper.exe')
					window.location.href = targetUrl
				} else if (type === 'record') {
					targetUrl = PageHeader.downPopupMode.record !== 0 ? ('https://luping.aipai.com/down.php?type=down&mode=' + PageHeader.downPopupMode.record) : 'https://luping.aipai.com'
					window.open('https://download.aipai.com/aipaire_files/aipai_official/aipai_recorder.exe')
					window.location.href = targetUrl
				} else if (type === 'aipai') {
					window.open('https://download.aipai.com/aipai_files/aipai.exe')
					if (PageHeader.downPopupMode.aipai !== 0) {
						window.location.href = 'https://www.aipai.com/lastest_aipai/down.php?type=down'
					}
				} else if (type === 'geshi') {
					window.open('https://download.aipai.com/aipai_files/aipai_factory.exe')
					window.location.href = 'https://geshi.aipai.com'
				}
			}

			$('.ap_part_ad').on('click', '.list_logo', function() {
        var type = 'aipai'
        if ($(this).parents('.product_list').hasClass('clip')) {
					PageHeader.apWebEvent('aipai_pc_official_index_click_8')
          type = 'clip'
        } else if ($(this).parents('.product_list').hasClass('record')) {
					PageHeader.apWebEvent('aipai_pc_official_index_click_9')
          type = 'record'
        } else if ($(this).parents('.product_list').hasClass('aipai')) {
					PageHeader.apWebEvent('aipai_pc_official_index_click_7')
          type = 'aipai'
        } else if ($(this).parents('.product_list').hasClass('geshi')) {
					PageHeader.apWebEvent('aipai_pc_official_index_click_13')
          type = 'geshi'
        }
				var btn = $(this).data('btn')
				PageHeader.uploadLandPage({
					eid: 'click_button',
					btn: btn || 'button_other'
				})
        handleClick(type)
      })
      $('.ap_part_ad').on('click', '.list_pro_name', function() {
        var type = 'aipai'
        if ($(this).parents('.product_list').hasClass('clip')) {
					PageHeader.apWebEvent('aipai_pc_official_index_click_11')
          type = 'clip'
        } else if ($(this).parents('.product_list').hasClass('record')) {
					PageHeader.apWebEvent('aipai_pc_official_index_click_12')
          type = 'record'
        } else if ($(this).parents('.product_list').hasClass('aipai')) {
					PageHeader.apWebEvent('aipai_pc_official_index_click_10')
          type = 'aipai'
        } else if ($(this).parents('.product_list').hasClass('geshi')) {
					PageHeader.apWebEvent('aipai_pc_official_index_click_14')
          type = 'geshi'
        }
				var btn = $(this).data('btn')
				PageHeader.uploadLandPage({
					eid: 'click_button',
					btn: btn || 'button_other'
				})
        handleClick(type)
      })
      $('.ap_part_ad').on('click', '.list_pro_download', function() {
        var type = 'aipai'
        if ($(this).parents('.product_list').hasClass('clip')) {
					PageHeader.apWebEvent('aipai_pc_official_index_click_5')
          type = 'clip'
        } else if ($(this).parents('.product_list').hasClass('record')) {
					PageHeader.apWebEvent('aipai_pc_official_index_click_6')
          type = 'record'
        } else if ($(this).parents('.product_list').hasClass('aipai')) {
					PageHeader.apWebEvent('aipai_pc_official_index_click_4')
          type = 'aipai'
        } else if ($(this).parents('.product_list').hasClass('geshi')) {
					PageHeader.apWebEvent('aipai_pc_official_index_click_15')
          type = 'geshi'
        }
				var btn = $(this).data('btn')
				PageHeader.uploadLandPage({
					eid: 'click_button',
					btn: btn || 'button_other'
				})
        handleClick(type)
      })
