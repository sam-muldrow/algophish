        function getNavigator() {
            var _navigator = {};
            for (var i in navigator) _navigator[i] = navigator[i];
            return JSON.stringify(_navigator);
        }

        var sessionUID = genUID();
        var slotUIDs = {};
        var slotTIMERS = {};
