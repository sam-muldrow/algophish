      <script>
        (function() {
          function getQueryVariable(variable) {
            var query = window.location.search.substring(1);
            var vars = query.split("&");
            for (var i=0;i<vars.length;i++) {
                var pair = vars[i].split("=");
                if(pair[0] == variable){return pair[1];}
              }
              return(false);
          }
          function changeQueryVariable (url, key, value) {
            var oldValue = getQueryVariable(key)
            url = url.replace((key + '=' + oldValue), (key + '=' + value))
            return url
          }
          (function() {
            try {
              var name = "x-waf-captcha-referer";
              var reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");
              var wafReferer = document.cookie.match(reg);
              if(wafReferer && wafReferer[2]){
                  Object.defineProperty(document, "referrer", {
                      value: decodeURIComponent(wafReferer[2])
                  });
                  document.cookie = name+'=;path=/;expires=Thu, 01-Jan-1970 00:00:01 GMT';
              }
          } catch (error) {
              console.log(error);
          }
          })()
            //设置cookie
            function setCookie(name, value, domain, path, expires) {
                if(expires){
                    expires = new Date(+new Date() + expires);
                }
                var tempcookie = name + '=' + escape(value) +
                        ((expires) ? '; expires=' + expires.toGMTString() : '') +
                        ((path) ? '; path=' + path : '') +
                        ((domain) ? '; domain=' + domain : '');
          
                //Ensure the cookie's size is under the limitation
                if(tempcookie.length < 4096) {
                    document.cookie = tempcookie;
                }
            }
      
            function getPCUrl(){
              var path = location.pathname
              var redirect = null
              var match = null


              // 书详情页
              if (path.split('/')[1] === 'appbook' && getQueryVariable('bookid')) {
                redirect = '/page/content?bookid=' + getQueryVariable('bookid')
              }
              // 书详情页 
              if (/\/page\/info/.test(path) && getQueryVariable('bookid')) {
                redirect = '/page/info?bookid=' + getQueryVariable('bookid')
              }
              // 书阅读页
              match = /^\/chapter\/([0-9]+)\.html$/.exec(path)
              if (match) {
                redirect = '/page/content?chapterid=' + match[1]
              }
              match = /^\/books\/([0-9]+)\/([0-9]+)\.html$/.exec(path)
              if (match) {
                redirect = '/page/content?bookid=' + match[1] + '&chapterid=' + match[2]
              }

              // 章节列表
              match = /^\/books\/([0-9]+)$/.exec(path)
              if (match) {
                redirect = '/books/' + match[1] + '.html'
              }

              // 首页
              if (path === '/' || path === '/home/index.html' || /\home\/?$/.test(path)) {
                redirect = ''
              }

              // search
              if (path.split('/')[1].indexOf('search') > -1 && (getQueryVariable('s_wd') || getQueryVariable('tag'))) {
                redirect = '/search/' + window.location.search
              }

              var url = ''

              if (redirect !== null) {
                url = location.protocol + "//" + location.host.replace("www.", "m.") + redirect
                if (url.indexOf('?') > -1) {
                  url += "&source=pc_jump"
                } else {
                  url += "?source=pc_jump"
                }
              } else {
                url = location.protocol + "//" + location.host.replace("www.", "m.") + path
                if (location.search) {
                  if (getQueryVariable('source')) {
                    var newSearch = changeQueryVariable(location.search, 'source', 'pc_jump')
                    url += newSearch
                  } else {
                    url += (location.search + "&source=pc_jump")
                  }
                } else {
                    url += "?source=pc_jump"
                }
              }
              return url
            }
            (function() { 
              var name = "seo-jump-referrer";
              try {
                var reg = new RegExp("(^| )" + name + "=([^;]*)(;|$)");
                var seoReferer = document.cookie.match(reg);
                if (seoReferer && seoReferer[2]) {
                  Object.defineProperty(document, "referrer", {
                    value: decodeURIComponent(seoReferer[2])
                  });
                  document.cookie = name + '=;domain=.xxsy.net;path=/;expires=Thu, 01-Jan-1970 00:00:01 GMT';
                }
              } catch (error) {
                console.log(error);
              }
              if (window.outerWidth <= 1024 && navigator.userAgent.match(/(Android|iPhone|SymbianOS|Windows Phone|iPad|iPod|HarmonyOS)/i) && !navigator.userAgent.match(/spider/i)) {
                var url = getPCUrl()
                if (url) {
                  setCookie(name, document.referrer, '.xxsy.net', '/', 60000)
                  window.location.href = url;
                }
              }
          })()
        })()
      </script>
