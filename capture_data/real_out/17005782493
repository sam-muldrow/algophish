<script>


var addTag = function(e) {
  	e.preventDefault();
  	var editor = $(".redactor_BbCodeWysiwygEditor")[0].contentWindow.document.body;
  	$(".placeholder").remove()
  	if (editor.innerHTML == "<p><br></p>") {
  		editor.innerHTML = $(this).attr("data-content");
  		return;
  	} else if (editor.innerHTML.substr(editor.innerHTML.length - 6) != "&nbsp;") {
  		editor.innerHTML += " ";
  	}
  	
	editor.innerHTML += $(this).attr("data-content");
};
$(".TagUser").click(addTag).show();

