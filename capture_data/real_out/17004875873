                                                                        $(document).ready(function(){
                                                                        var swiper = new Swiper(".swiper-web", {
                                                                        //    effect: 'fade',
                                                                        fadeEffect:  { crossFade: true },
                                                                                slidesPerView: 3,
                                                                                spaceBetween: 20,
                                                                                loop: true,
                                                                                autoplay: {
                                                                                delay: 2500,
                                                                                }
                                                                        });
                                                                        var swiper = new Swiper(".swiper-mobile", {
                                                                        //    effect: 'fade',
                                                                        loop: true,
                                                                                autoplay: {
                                                                                delay: 4000,
                                                                                },
                                                                                navigation: {
                                                                                nextEl: '.swiper-button-next',
                                                                                        prevEl: '.swiper-button-prev',
                                                                                },
                                                                        });
                                                                        var mySwiper = document.querySelector('.swiper-web').swiper;
                                                                        $(".swiper-web").mouseenter(function() {
                                                                        mySwiper.autoplay.stop();
                                                                        console.log('slider stopped');
                                                                        });
                                                                        $(".swiper-web").mouseleave(function() {
                                                                        mySwiper.autoplay.start();
                                                                        console.log('slider started again');
                                                                        });
                                                                        });</script>
