<script>
$(document).ready(function() { 
	var floatPosition = parseInt($(".side-banner").css('top'));
	
	$(window).scroll(function() {
		if($(document).height() - $(window).height() - $(window).scrollTop() > 168){
			// 현재 스크롤 위치를 가져온다.
			var scrollTop = $(window).scrollTop();
			//var newPosition = scrollTop + floatPosition + "px";
			var newPosition = "50px";
			if(scrollTop > 250){
				var newPosition = (scrollTop - 250) + "px";
			}

			//alert(newPosition+"=="+scrollTop+"=="+floatPosition);
	 
			$(".side-banner").css('top', newPosition); 
		} 
	}).scroll();
});

function go_calculator(){

	$.ajax({
		type: 'POST',
		url: 'https://www.iporter.com/ko/api/clicklog-insert',
		cache: false,
		dataType: 'json',
		data: { 'id' : '7'},
		success: function(message) {			
		}
	});

	var popWin = Utils.popup('/ko/popup/calculator','calculator',635,620);
	form.submit();
	popWin.focus();

}

function go_rewardranking(){

	$.ajax({
		type: 'POST',
		url: 'https://www.iporter.com/ko/api/clicklog-insert',
		cache: false,
		dataType: 'json',
		data: { 'id' : '14'},
		success: function(message) {			
		}
	});

	var popWin = Utils.popup('/ko/popup/reward-ranking','reward-ranking',360,630);
	form.submit();
	popWin.focus();

}

function go_clicklog(url,id){

	alert("\n아이포터 자동신청서는 PC 에서 어떠한 정보도 수집하지 않으며\n윈도우 보안 정책상 '안전하지 않은 파일'로 나오는데\n전혀 문제 없음을 알려드립니다 ");

	$.ajax({
		type: 'POST',
		url: 'https://www.iporter.com/ko/api/clicklog-insert',
		cache: false,
		dataType: 'json',
		data: { 'id' : id},
		success: function(message) {			
		}
	});

	location.href=url;
}

