<script>
var event_topBanner = Utils.cookie.get('event_topBanner');

if(event_topBanner=='n'){
	$("#topbanner").hide();
}

function topBannerClose(){
	document.cookie = "event_topBanner=n";
	//Utils.cookie.set('event_topBanner', 'n', 0, '/');
	$("#topbanner").hide();
}

function go_blackfriday2016(){
	location.href="/ko/event/view?event_id=505";
}

function go_header_clicklog(url,id){

	$.ajax({
		type: 'POST',
		url: 'https://www.iporter.com/ko/api/clicklog-insert',
		cache: false,
		dataType: 'json',
		data: { 'id' : id},
		success: function(message) {			
		}
	});


	if(id==15) location.href=url;
	else window.open('about:blank').location.href=url
}

var NUMBER_OF_SLIDES = "5";
var slide_number = 1;

$(".h-banner").css("display","none");
$("#h-banner"+slide_number).show();

$(".h_arrow").click(function(){
	var val = $(this).attr("val");
	var math = parseInt(0);
	if(val == "left") math = parseInt(-1);
	else math = parseInt(1);

	slide_number = Number(slide_number) + math;

	if(slide_number > NUMBER_OF_SLIDES && val != "left") slide_number = 1;
	else if(slide_number == 0) slide_number = NUMBER_OF_SLIDES;
	
	$(".h-banner").hide();
	$("#h-banner"+slide_number).fadeIn("1000");
});

</script>

