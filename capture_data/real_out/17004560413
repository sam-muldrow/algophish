	//	=========== 메인배너 액션
		
	var NUMBER_OF_SLIDES = "16";
	var slide_width = document.getElementById('slideshow').scrollWidth / NUMBER_OF_SLIDES; 
	var slide_number = 1;
	var linkurl = "";
	var bannerVal = "";
	var clickVal = "";
	var interval;

	interval = setInterval(function(){
		doStuff(); //Call Interval Body
	},4000);

	function doStuff(){	
		$(".slide-selecter").css('color','#B5B5B5');

		if (slide_number < NUMBER_OF_SLIDES){
			slide_number++;	
			if(slide_number > 1) $(".banner").hide();
			$("#banner"+slide_number).fadeIn("1000");
		}else{
			if(slide_number > 1) $(".banner").hide();
			$("#banner1").fadeIn("1000");
			slide_number = 1;
		}

		$("#selecter" + slide_number).css("color", "#FA7E76");
	}

	/*
		=========== !메인배너 액션
	*/	

	$("#temp_area").click(function(){
		linkurl = $("#banner"+slide_number).attr("linkurl");
		bannerVal = $("#banner"+slide_number).attr("val");

		if(bannerVal == 756){
			clickVal = 47;
			banner_click(clickVal);
		}

		if(bannerVal == 781){
			clickVal = 50;
			banner_click(clickVal);
		}

		/*
		if(bannerVal == 402){
			if(bannerVal == 402) clickVal = 20;
			banner_click(clickVal);
		} else if(bannerVal == 417){
			if(bannerVal == 417) clickVal = 20;
			banner_click(clickVal);
		} else if(bannerVal == 418){
			if(bannerVal == 418) clickVal = 21;
			banner_click(clickVal);
		} else if(bannerVal == 419){
			if(bannerVal == 419) clickVal = 22;
			banner_click(clickVal);
		}
		*/
		window.open(linkurl, "_blank");		
	});

	$(".arrow").click(function(){
		var val = $(this).attr("val");
		var math = parseInt(0);
		if(val == "left") math = parseInt(-1);
		else math = parseInt(1);

		slide_number = Number(slide_number) + math;

		if(slide_number > NUMBER_OF_SLIDES && val != "left") slide_number = 1;
		else if(slide_number == 0) slide_number = NUMBER_OF_SLIDES;

		$(".slide-selecter").css('color','#B5B5B5');
		$("#selecter" + slide_number).css("color", "#FA7E76");
		
		slide_select(slide_number);
	});

	$(".slide-selecter").hover(function(){
		clearInterval(interval);
		$(".slide-selecter").css('color','#B5B5B5');
		slide_number = $(this).attr('val');
		$(this).css("color", "#FA7E76");

		slide_select(slide_number);
	}, function(){
		//$(this).css('color','#B5B5B5');

		interval = setInterval(function(){
			doStuff(); //Call Interval Body
		},4000); 
	});

	

    // 저장된 쿠키값을 가져와서 ID 칸에 넣어준다. 없으면 공백으로 들어감.

    var userInputId = getCookie("userInputId");

    $("input[name='loginname']").val(userInputId);  

    if($("input[name='loginname']").val() != ""){ // 그 전에 ID를 저장해서 처음 페이지 로딩 시, 입력 칸에 저장된 ID가 표시된 상태라면,
        $("#c-save-id").attr("checked", true); // ID 저장하기를 체크 상태로 두기.
    }

    $("#c-save-id").change(function(){ // 체크박스에 변화가 있다면,
        if($("#c-save-id").is(":checked")){ // ID 저장하기 체크했을 때,
            var userInputId = $("input[name='loginname']").val();
            setCookie("userInputId", userInputId, 7); // 7일 동안 쿠키 보관
        }else{ // ID 저장하기 체크 해제 시,
            deleteCookie("userInputId");
        }
    });

    // ID 저장하기를 체크한 상태에서 ID를 입력하는 경우, 이럴 때도 쿠키 저장.

    $("input[name='loginname']").keyup(function(){ // ID 입력 칸에 ID를 입력할 때,
        if($("#c-save-id").is(":checked")){ // ID 저장하기를 체크한 상태라면,
            var userInputId = $("input[name='loginname']").val();
            setCookie("userInputId", userInputId, 7); // 7일 동안 쿠키 보관
        }
    });
});

function slide_select(num){
	$(".banner").hide();	
	$("#banner"+num).fadeIn("1000");
}

