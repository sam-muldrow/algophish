   $(document).ready(function () {
      var onAlertBarUpdate = function () {
         $("#container").removeClass("alert-bar-visible");
      };

      var widgetProps = {
         displayCustomerServiceMessage: false,
         customerServiceMessageString: "&quot;&quot;",
         errors: [],
         warnings: [],
         updateWidgetState: onAlertBarUpdate
      };

      if (widgetProps.errors.length === 0 && widgetProps.warnings.length === 0 && !widgetProps.displayCustomerServiceMessage) {
         return;
      }
      var alertBarWidgetContainer = $(".alert-bar-widget-container")[0];
      AlertBarWidget.init(alertBarWidgetContainer, widgetProps);
   });
</script>
