<script language="javascript">
    $(document).ready(function(){
        setTimeout(function(){
         $(".index_ad_popup").each(function(){
                var width = $(this).find("img").attr("width");
                width = parseInt(width) - 8;
                var height =  $(this).find("img").attr("height");
                height = parseInt(height) - 8;
                var span = '<span class="ad_more" style="margin-top:'+ height +'px;margin-left:'+ width +'px;"> </span>';
                $(this).prepend(span);
        });
        },1);
        $("#zoomer").mouseleave(function(){
            $("#zoomer").css({
                "display":"none"
            });
            $("#zoomer").html('');
        });

        var $that = $('#hmScrollLinks .scrollin div');
        var w = $that.find('p').width();
        var htm = $that.html();
        var s = w/0.06;

        $that.append(htm);

        function move(){
            s = (w + parseInt($that.css("left")))/0.06;
            $that.animate({'left':-w},s,'linear',function(){
                $that.css('left',0);
                move();
            });
        }

        move();

        $('#hmScrollLinks .scrollin').hover(function(){
            $that.stop(true);
        },function(){
            move();
        });

        //Ðü¸¡ËÑË÷¿ò
        $(window).scroll(function(){
            var adtop=$('.hm .entry').offset().top;
            var wtop=document.documentElement.scrollTop+document.body.scrollTop;
            if(wtop>adtop){
                $('.header.fix').fadeIn(400);
            }else{
                $('.header.fix').fadeOut(400);
            }
        });
        //·µ»Ø¶¥²¿
        // var stop='';
        // $(window).scroll(function(){
        //     $('#goTop').hide();
        //     stop=$(this).scrollTop();
        //     if(stop>0){
        //         $('#goTop').css('display','block');
        //     }else{
        //         $('#goTop').hide();
        //     }
        // });
        $('.extraWrap').css('top',$(window).height()-290);
        $('.goApp').addClass('on');
        setTimeout(()=>{
            $('.goApp').removeClass('on');
        },2000)
        $(window).on('scroll',function(){
            var scrollTop=$(window).scrollTop();
            if(scrollTop > 100){
                $('#goTop').css('display','block');
                //$('.goApp').removeClass('on');
            }else{
                $('#goTop').css('display','none');
                //$('.goApp').addClass('on');
            }
        })

        $('.showLogin').click(function(){
            window.location.href = (supporthttps==1 ? 'https://' : 'http://') + 'login.51job.com/login.php?lang=c&from_domain=i&source=&url='+encodeURIComponent(window.cfg.domain.www+"/default.php");
        });

        $('.hideLogin').click(function(){
            $('.content .ufm').animate({'bottom':-248},300)
        });
    })
</script>
