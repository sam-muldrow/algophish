<script  type="text/javascript">
    function advMenuLogo(logoPath) {
        let el = document.querySelector("img#logo__image");
        if (el) {
            el.src = logoPath;
        }

        let el_sticky = document.querySelector("img#logo__image-sticky");
        if (el_sticky) {
            el_sticky.src = logoPath;
        }
    }

    function advMenuBackground(backgroundPath, backgroundPathSticky) {
        let el = document.querySelector("div.header-7days__branding");
        if (el) {
            el.style.backgroundImage = 'url("' + backgroundPath + '")';
        }

        let el_sticky = document.querySelector("div.header-7days__infopanel-sticky");
        if (el_sticky) {
            el_sticky.style.backgroundImage = 'url("' + backgroundPathSticky + '")';
        }
    }

    function advMenuURL(logoUrl) {
        let el = document.querySelector("a#logo__url");
        if (el) {
            el.setAttribute("href", logoUrl);
        }

    	let el_sticky = document.querySelector("a#logo__url-sticky");
        if (el_sticky) {
            el_sticky.setAttribute("href", logoUrl);
        }

    }
</script>

