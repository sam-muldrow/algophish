<div id="adfox_149492749261972582"></div>
<script>
const firstElementChild = parent=>{
    var node = parent && parent.firstElementChild;
    if (!node && parent) {
        node = parent.firstChild;
        while (node && node.nodeType !== 1) {
            node = node.nextSibling;
        }
    }
    return node;
}
</script>
