	    <!-- Hotlive V2 Script -->
    <script language="JavaScript" type="text/javascript">
	  var timeInterval = 14000;
    var hotlive_page = 0;
    var autoChange = null;
    var live_timer_in;
    var live_timer_out;
    var live_mouse_active = false;
    var live_autoScroll_active = false;

    $$(function(e){
        autoChange = setInterval(autoScroll, timeInterval);

        $$('body').on('mouseenter', '#hot-live-main-box', function () {
            live_mouse_active = true;
        })

        $$('body').on('mouseleave', '#hot-live-main-box', function () {
            live_mouse_active = false;
        })

        $$('body').on('mouseenter', '#hot-live-top-main-box', function () {
        	  hotlive_trackEvent('Homepage hotlive','mouseover P1','');
            HotLiveBoxShow();
        })

        $$('body').on('mouseleave', '#hot-live-top-main-box', function () {
            HotLiveBoxHide();
        })

        $$('body').on('click', '#hot-live-scroll-left', function(){
        	  hotlive_trackEvent('Homepage hotlive','previous','current page is '+(hotlive_page+2));
			      prevPage();
        });

        $$('body').on('click', '#hot-live-scroll-right',function(){
        	  hotlive_trackEvent('Homepage hotlive','next','current page is '+(hotlive_page+2));
            nextPage();
        });

        $$('body').on('click', '#hot-live-top-hint', function(){
            hotlive_trackEvent('Homepage hotlive','more','');

            var _top = $$('#hot-live-box-container').offset().top - 68;
            $$('html,body').stop().animate({scrollTop:_top,scrollLeft:0},200);
        });
    });

    function autoScroll() {
        if (!live_autoScroll_active || live_mouse_active) return;
        nextPage();
    }

    function switchPage(hotlive_page){
        $$('#hot-live-box').animate({scrollLeft: 900 * hotlive_page}, 200);
    }

	  function prevPage(){
        if(hotlive_page > 0){
            hotlive_page -= 1;
            switchPage(hotlive_page);
        }else{
        	hotlive_page = getShowPage() - 1;
        	switchPage(hotlive_page);
        }
	  }

    function nextPage(){
        if(hotlive_page < getShowPage() - 1){
            hotlive_page += 1;
            switchPage(hotlive_page);
        }else if(hotlive_page === getShowPage() - 1){
            hotlive_page = 0;
            switchPage(hotlive_page);
        }
    }

    function HotLiveBoxHide(){
        if(live_timer_in){
          clearTimeout(live_timer_in);
        }
        live_timer_out = setTimeout(function(){
          HotLiveBoxHideFunction();
        }, 200);
    }

    function HotLiveBoxShow(){
        if(live_timer_out){
          clearTimeout(live_timer_out);
        }
        live_timer_in = setTimeout(function(){
          HotLiveBoxShowFunction();
        }, 200);
    }

    function HotLiveBoxHideFunction() {
		    $$('#hot-live-top-main-box').animate({width: '253px'}, 200, function(){
            $$('#hot-live-top-hint').css('display', 'none');
        });
    }

    function HotLiveBoxShowFunction() {
        $$('#hot-live-top-hint').css('display', 'block');
        $$('#hot-live-top-main-box').animate({width: '466px'}, 200);
    }

    function hotlive_trackEvent(category,action,label){
        // if(typeof dataLayer != "undefined"){
        //     dataLayer.push({
        //         'event': 'GAEvent',
        //         'eventCategory': category,
        //         'eventAction': action,
        //         'eventLabel': label
        //     });
        // }
    }

    function getShowPage(){
        const page = $$("input[name='showPageTmp']").val()
        if(!!page){
			      return parseInt(page)
        }
        return 20    }
    </script>
    
    <div class="hotlive">
        <div class="hotlive__title mb-1"></div>
    </div>

