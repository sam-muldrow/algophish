    googletag.pubads().collapseEmptyDivs();
	
    googletag.pubads().setTargeting('dfid', ['0']);
    googletag.pubads().setTargeting('dgid', ['0']);
    googletag.pubads().setTargeting('dtid', ['0']);
    googletag.pubads().setTargeting('daid', ['']);
    googletag.pubads().setTargeting('page', ['home']);
    googletag.pubads().setTargeting('domain', ['www.discuss.com.hk']);
	googletag.pubads().setPublisherProvidedId(ppidHash);
    
	googletag.pubads().disableInitialLoad();
    googletag.enableServices();
});
</script>


