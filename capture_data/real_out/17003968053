<script>
    $$('#hot-live-box-container').appear();
    $$('#hot-live-box .live-box-page').appear();
    $$('#hot-live-top-box .live-box-page').appear();

    $$(document.body).on('appear', '#hot-live-box-container', function(e) {
        live_autoScroll_active = true;
    });

    $$(document.body).on('disappear', '#hot-live-box-container', function(e) {
        live_autoScroll_active = false;
    });

    var live_top3_tids_reported = false;
    var live_page1_tids_reported = false;

    $$(document.body).on('appear', '#hot-live-top-box .live-box-page', function(e) {
        if (live_top3_tids_reported) return;
        live_top3_tids_reported = true;

        var hot_tids = [];

        $$('#hot-live-top-box #live-box-page-0 .live-box-block[data-tid]').each(function (index) {
            var tid 	= $$(this).attr('data-tid');
            var index = hot_tids.indexOf(tid);

            if(index == -1){
                hot_tids.push(tid);
            }
        });

        var icon = document.createElement('img');
        icon.src = 'https://tr.discuss.com.hk/icon.gif?type=g&tr_so=w&tr_act=jtvw&tr_des=viewed_' + hot_tids.join(',') + '|jtp_1';
        document.body.appendChild(icon);
    });

    $$(document.body).on('appear', '#hot-live-box #live-box-page-0', function(e) {
        if (live_page1_tids_reported) return;
        live_page1_tids_reported = true;

        var hot_tids = [];

        $$('#hot-live-box #live-box-page-0 .live-box-block[data-tid]').each(function (index) {
            var tid 	= $$(this).attr('data-tid');
            var index = hot_tids.indexOf(tid);

            if(index == -1){
                hot_tids.push(tid);
            }
        });

        var icon = document.createElement('img');
        icon.src = 'https://tr.discuss.com.hk/icon.gif?type=g&tr_so=w&tr_act=jtvw&tr_des=viewed_' + hot_tids.join(',') + '|jtp_2';
        document.body.appendChild(icon);
    });
</script>
