    $(document).ready(function() {
        // open the popup
        $('a.show_microsoft_popup').click(function(e) {
            var msPopup = $('#microsoft_popup');
            var msPopupShadow = $('#microsoft_popup_shadow');

            // where we're putting it!
                var left = ($(window).width() / 2) - (msPopup.width() / 2);
                var top = $(window).scrollTop() + 10;

                msPopup.css({
                    top: top,
                    left: left,
                    position: "absolute"
                });
                msPopupShadow.css('height', $(document).height());
                msPopupShadow.fadeIn('2000', function() {
                    msPopup.fadeIn('2000', function() {
                        //$('html, body').animate({ scrollTop: msPopup.offset().top }, 'fast');
                        msPopupShadow.click(function() {
                            msPopup.fadeOut('2000', function() {
                                msPopupShadow.fadeOut('2000');
                            });
                        });
                    });
                });
                return false;
            });
            // close the popup
            $('a.close_microsoft_popup').click(function() {
                var msPopup = $('#microsoft_popup');
                var msPopupShadow = $('#microsoft_popup_shadow');

                msPopup.fadeOut('2000', function() {
                    msPopupShadow.fadeOut('2000');
                    $("body").css("overflow", "auto");
                });
                return false;
            });
        });

    
