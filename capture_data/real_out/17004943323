function addToCartClick(productId, price, pricingConfig, productVersion)
{
    var bundle,version;
    if (pricingConfig) {
        bundle = pricingConfig;
    } else {
        bundle = '';
    }
    
    if (productVersion) {
        version = productVersion;
    } else {
        version = '';
    }
    dataLayer.push({
        'event': 'addToCart', 
        'ecommerce': { 
            'add': {
                'products': [{
                    'id': productId, 
                    'price': price, 
                    'quantity': 1, 
                    'license':'licenseType',
                    'bundle': bundle,
                    'version': version
                }]
            }
        }
    });
}
function productClick(el, productId, productName, price, position, pageName) {
  dataLayer.push({
    'event': 'productClick',
    'ecommerce': {
    //'currencyCode': 'EUR', //optional; add only IF USD is not currency used in price attribute below
      'click': {
        'actionField': {'list': pageName}, // Optional list Property
        'products': [{
          'id': productId,
          'name': productName,
         //'brand': 'productBrand', //optional
         //'category': 'productCategory', //optional
         //'version': versions_pricing[productId].v, 
         'price': price, //use USD if possible, if any other currency, please make sure currencyCode attribute above matches
         //'bundle': versions_pricing[productId].pc,
         'position' : position, //optional
         'creative' : 'tile', //optional
         'list' : pageName,
         }]
       }
     } //,
//     'eventCallback': function() {     //this handles navigation
//       document.location = $(el).attr('href');  //after the click
//     }
  });
}
