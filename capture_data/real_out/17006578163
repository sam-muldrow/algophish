    $(function(){
        $(document).ready(function(){
            $('a[href^="#go-top"]').click(function(){
                $('html, body').animate({scrollTop:0}, 'fast');
                return false;
            });
            $('a[href^="#go-bottom"]').click(function(){
                var scbottom = document.body.scrollHeight;
                $('html, body').animate({scrollTop:scbottom}, 'fast');
                return false;
            });

            // フリーワード検索カテゴリ選択
            var tabs_cat = $.cookie("tabs_cat");
            if(!_is_sp){
                if(tabs_cat)
                {
                    changeImage(tabs_cat);
                }
            }
            else if(_is_sp)
            {
                if(tabs_cat){
                    $('#tabs_cat').val(tabs_cat);
                }
            }

        });
    });


    // フリーワード検索ボタンを変更
    function changeImage(id)
    {
        document.getElementById("search-btn-bike").src = "/bb_img/default/v1/search-btn-bike.png";
        document.getElementById("search-btn-kiji").src = "/bb_img/default/v1/search-btn-kiji.png";
        document.getElementById(id).src = "/bb_img/default/v1/" + id + "-on.png";

        setFreewordCategory(id);
    }

    function setFreewordCategory(id)
    {

        if(id == "search-btn-bike")
        {
            document.form01.category.value = "catalog";
            $.cookie("tabs_cat", "search-btn-bike");
        }
        else if(id == "search-btn-item")
        {
            document.form01.category.value = "parts";
            $.cookie("tabs_cat", "search-btn-item");
        }
        else if(id == "search-btn-kiji")
        {
            document.form01.category.value = "vb";
            $.cookie("tabs_cat", "search-btn-kiji");
        }

    }

</script>

