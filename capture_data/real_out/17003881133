			})
			function oneTapScript(){
				console.log('oneTapScript Function Called')
				if(getCookie("g_token")===null)
				{
					login();
				}
				else
				{
					if(getCookie("isSignedIn")===null)
					{
						checkUserExists(getCookie("g_token"));
					}
				}
				if(getCookie("g_username") && getCookie("isSignedIn")){
					//logged in ui show
					let g_username = getCookie("g_username");
					let isSignedIn = getCookie("isSignedIn");


				let gToken = getCookie('g_token');
				let user = parseJwt(gToken)

				if(user && user?.email){
					clevertap.onUserLogin.push({
						"Site": {
							"Name": g_username,            // String
							"Identity": user?.email,              // String or number
							"Email": user?.email,         // Email address of the user
							//"Phone": "",           // Phone (with the country code)
							//"Gender": "M",                     // Can be either M or F
							//"DOB": new Date(),                 // Date of Birth. Date object
							// optional fields. controls whether the user will be sent email, push etc.
							"MSG-email": true,                // Disable email notifications
							"MSG-push": true,                  // Enable push notifications
							"MSG-sms": true,                   // Enable sms notifications
							"MSG-whatsapp": true,              // Enable WhatsApp notifications
							"NLOptin_ForeignPolicy": false,
							"NLOptin_Sports": false,
						}
					})
				}

					// jQuery('#login-user-name').html('Hi,'+g_username);
					document.querySelector('#login-user-name').innerHTML = `Hi,${g_username}`;
					// jQuery('#search-fp').css('width','80%');	
					document.querySelector('#search-fp').style.width = "80%";				
					// jQuery('#after-sign-in').css('display','inline-flex');
					document.querySelector('#after-sign-in').style.display = "inline-flex";
				}else{
					// jQuery('#search-fp').css('width','100%');
					document.querySelector('#search-fp').style.width = "100%";
					// jQuery('#after-sign-in').css('display','none');
					document.querySelector('#after-sign-in').style.display = "none";
				}
			}
			
			// jQuery('#logout-one-tap').click(function(){
			// 	logout();
			// 	jQuery('#after-sign-in').css('display','none');
			// 	$('#drop-sign-out-box').hide();
			// })

			document.querySelector('#logout-one-tap').addEventListener("click",function () {
				logout(), 
				document.querySelector('#after-sign-in').style.display = "none";
				document.querySelector("#box").style.display = "none";
			})
			
			// jQuery('#after-sign-in').click(function(){
			// 	if($('.feather').hasClass('active')){
			// 		$('#drop-sign-out-box').hide();
			// 		$('.feather').removeClass('active');
			// 	}else{
			// 		$('#drop-sign-out-box').show();
			// 		$('.feather').addClass('active');
			// 	}
			// })

			document.querySelector('#after-sign-in').addEventListener("click",function () {
				document.querySelector(".feather").classList.contains("active") ? (document.querySelector("#drop-sign-out-box").style.display = "none",document.querySelector(".feather").classList.remove("active")) : (document.querySelector("#drop-sign-out-box").style.display = "block",document.querySelector(".feather").classList.add("active"));
			})
			
		</script>
		<script>
			(function (i, s, o, g, r, a, m) {
		   i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
			   (i[r].q = i[r].q || []).push(arguments)
		   }, i[r].l = 1 * new Date(); a = s.createElement(o), m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
		})(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');
		ga('create', 'UA-22956444-1', { name: 'home' });
		
		
		
		function elementInViewport(el) {
		   const rect = el.getBoundingClientRect();        
		   const elemTop = rect.top;
		   const elemBottom = rect.bottom;
		   const isVisible = (elemTop >= 0) && (elemBottom <= window.outerHeight);
		   return isVisible;
		}
		
		const gaScrollDisplay = {}
		
		function onScrollIdCheckAndGaCall(idValue,{eventCategory,eventAction,nonInteraction=1},rhsCheck=false){
		   if(idValue && elementInViewport(idValue)){
			   if(!(eventCategory in gaScrollDisplay)){
				   ga('home.send', {
					   'hitType': 'event',
					   'eventCategory': eventCategory,
					   'eventAction': eventAction,
					   'nonInteraction': nonInteraction
				   });
				   gaScrollDisplay[eventCategory] = true
			   }
		   }
		}
		function onClickCheckAndGaCall(eventCategory, ea, nonInteraction = 0) {
		   ga('home.send', {
			   'hitType': 'event',
			   'eventCategory': eventCategory,
			   'eventAction': ea,
			   'nonInteraction': nonInteraction
		   });
		}  
		</script>



<script defer>
	function setCookie(e, o, t) {
		var n = new Date();
		n.setTime(n.getTime() + t * 3600 * 1000);
		var i = "expires=" + n.toGMTString();
		document.cookie = e + "=" + o + ";domain=.firstpost.com;path=/;";
	}
	function getCookie(e) {
		for (var o = e + "=", t = document.cookie.split(";"), n = 0; n < t.length; n++) {
			for (var i = t[n]; " " == i.charAt(0); ) i = i.substring(1, i.length);
			if (0 == i.indexOf(o)) { console.log("Match found\n",i,"\n",o); return i.substring(o.length, i.length); } 
		}
		return null;
	}

	function parseJwt(token) {
        try {
            return JSON.parse(atob(token.split('.')[1]));
        } catch (e) {
            return null;
        }
    }

	function youtubeNudgeClose(){
		const you_subcrbe_btn = document.getElementById("subcrbe_btn");
		if(you_subcrbe_btn){
			onClickCheckAndGaCall('YouTube_Subscription_Nudge','close')
			you_subcrbe_btn.style.display = "none"
		}
	}
	
	document.addEventListener("DOMContentLoaded", function (e) {
		const you_subcrbe_btn = document.getElementById("subcrbe_btn");
		if(you_subcrbe_btn){
			you_subcrbe_btn.innerHTML = `
		<style>.subcrbe_btn {z-index: 50;position: fixed;bottom: 20px;left: 0;right: 0;max-width: 547px;margin: 0 auto;display: flex;background-color: #000; padding: 10px 10px;border-radius: 10px; justify-content: center; align-items: center;box-shadow: 0px 3px 6px #00000029;}.clseBlock{display: flex;}.subcrbe_bx{background-color: #000;}.clseBtn {margin-left: auto;display: inline-block;width: 20px;height: 20px;background-color: #000;border-radius: 4px;margin-left: 10px;text-align: center;line-height: 23px;cursor: pointer; padding: 2px;}.clseBtn img {height: auto;}.subcrbe_bx{display: flex;align-items: center;justify-content: center;}.subTxt {color: #fff;font: 700 16px/18px 'Arial', sans-serif;}.subBtnRed {width: 181px;background-color: #ff0000;padding: 1px 10px 2px;color: #fff;text-transform: uppercase;font: 600 14px/24px 'arial';display: flex;align-items: center;margin-left: 10px;border-radius: 4px;}.subBtnRed img {margin-right: 10px;}.subcrbe_bx img {max-width: 30px;}</style>
		<div class="subcrbe_btn">
			<div  class="subcrbe_bx">
				<p class="subTxt">Stay updated with latest news videos</p>
				<a href="https://www.youtube.com/channel/UCz8QaiQxApLq8sLNcszYyJw?sub_confirmation=1" class="subBtnRed" onclick="return onClickCheckAndGaCall('YouTube_Subscription_Nudge','Click')" ><img src="https://www.firstpost.com/static/images/youtube.png" width="25" >Subscribe Now</a>
			</div>
			<div class="clseBlock">
				<div class="clseBtn" onclick="return youtubeNudgeClose()">
					<img src="https://www.firstpost.com/static/images/close.png" width="20" height="20" >
				</div>
			</div>
        </div>`
		if(!(getCookie('youtube_subcribe') && (getCookie('youtube_subcribe') == new Date().toLocaleDateString()))){
			you_subcrbe_btn.style.display = "block";
			onClickCheckAndGaCall('YouTube_Subscription_Nudge','View')
			setCookie('youtube_subcribe',new Date().toLocaleDateString(),24);
			setCookie('load_oneTap',true,0.5)
		}
		if(getCookie('load_oneTap')){
			
				const adContainer = document.getElementById('FSTPST_ENG/FSTPST_ENG_HOME/FSTPST_ENG_HOME_HOME/FSTPST_ENG_HP_Shosh_OOP');
				if (adContainer) {
					adContainer.parentNode.removeChild(adContainer);
				}
				
		}
		}
	})
</script>
