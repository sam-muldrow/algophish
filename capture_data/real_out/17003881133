<script>
        (function () {
            var PLAYER_LIBRARY = 'https://www.firstpost.com/static/v2-9/pubstack-player.web.api.min.js';
            var PUBSTACK_VIDEO_CONFIG = {
                env: 'production',
                access_token: 'eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJzdWIiOiI2MjhjZGM0ZDFmZGJhYjVjOTI4MTY4Y2YiLCJzY29wZSI6IklOX0hPVVNFIiwic2Vzc2lvbl9pZCI6IjM5YjMzZTljLTBjOGYtNDE4Ni04OTZkLThhNGMxNGNkYjA0ZSIsImZpcnN0bmFtZSI6IlByZWV0YW0iLCJsYXN0bmFtZSI6IkNoYWtyYWJvcnR5IiwiZW1haWxfaWQiOiJQcmVldGFtLkNAbncxOC5jb20iLCJpYXQiOjE2ODMwMjYxODQsImV4cCI6MTcxNDU2MjE4NH0.Z0AZsCdvmiy491VjzXlTjuHIfeDT3FHBj5f34AoA6mQ',
                website: {
                id: '5ee745560f4b0950cbcf4319',
                c3: '17',
                title: 'Firstpost'
                },
                base_href: 'https://www.firstpost.com/static/v2-9/',
                version: '4.2',
                publisher_id: '6683813'
            };
            
            var loadPubstackPlayer = function () {
                if (window.pubstackJSLoaded) {
                return;
                }
            
                window.PUBSTACK_VIDEO_CONFIG = PUBSTACK_VIDEO_CONFIG;
            
                var head = document.getElementsByTagName('HEAD')[0];
                var script = document.createElement('script');
                script.src = PLAYER_LIBRARY + '?v=' + PUBSTACK_VIDEO_CONFIG.version;
                script.onerror = function (error) {
                console.log(error);
                };
                head.appendChild(script);
            };
            
            loadPubstackPlayer();
            })();
    </script>

    <script>
        function refreshVantagePage(){
            window.location.reload();
        } 
    </script>
    <script>
        function isIntoView(elem)
        {
            var documentViewTop = $(window).scrollTop();
            var documentViewBottom = documentViewTop + $(window).height();
            var elementTop = $(elem).offset().top;
            var elementBottom = elementTop + $(elem).height();
            return ((elementBottom <= documentViewBottom) && (elementTop >= documentViewTop));
        }
        let gaPodcastCount = 0
        $(window).scroll(function(){
           if (isIntoView($('#podbean-podcast')) && (!gaPodcastCount)){
                ga('home.send', {
                    'hitType': 'event',
                    'eventCategory': 'PodcastWidget',
                    'eventAction': 'View',
                    'nonInteraction': 1
                });
                gaPodcastCount++
            }
        })
    </script>
    
    <script>
        const homeMainStorySliderId = document.getElementById('mainstoryslider')
        const homeWebStoriesSliderId = document.getElementById('webstories-slider')
        const homeTrendingWidget = document.getElementById('homeTrending')
        const homeVantageVideos = document.getElementById('vantage-videos')
        const homePhotoCarousel = document.getElementById('photos-carousel')
        const mostReadRhs = document.getElementById('mostReadSecondArticle')
    
        window.addEventListener('scroll', () => {
    
            onScrollIdCheckAndGaCall(homeMainStorySliderId, {
                eventCategory: 'BigStory_Home',
                eventAction: 'View',
                nonInteraction: 1
            })
            onScrollIdCheckAndGaCall(homeWebStoriesSliderId, {
                eventCategory: 'WebStory_Home',
                eventAction: 'View',
                nonInteraction: 1
            })
            onScrollIdCheckAndGaCall(homeTrendingWidget, {
                eventCategory: 'Trending_Home',
                eventAction: 'View',
                nonInteraction: 1
            })
            onScrollIdCheckAndGaCall(homeVantageVideos, {
                eventCategory: 'VantageVOD_Home',
