<script type="text/javascript">
  $('.read-more').on('click',function(){
    $('.tag-top-txt').css({'height':'auto','overflow':'visible'});
    $(this).hide();
    $('.read-less').show();
  })
  $('.read-less').on('click',function(){
    $('.tag-top-txt').css({'height':'250px','overflow':'hidden'});
    $(this).hide();
    $('.read-more').show();
  })
</script>

