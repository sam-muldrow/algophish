<script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script>
  let menu_open = false;
  let sub_menu_open = false;
  let menu_selector = document.getElementById("menu");
  let hamburger = document.querySelector(".hamburger");
  let list_open_nav = [];

  const stickyHeader = document.querySelector(".sticky-header");
  const mobileOverlay = document.createElement("div");
  mobileOverlay.classList.add("mobile-overlay");
  mobileOverlay.onclick = clickMenu;

  document.addEventListener("keydown", function (event) {
    if (event.key == "Escape" && menu_open) {
      menu_open = false;
      clickMenu();
    }
  });

  function handleToggleMenu() {
    if (event.key == "Enter") {
      event.preventDefault();
      clickMenu();
    } 
  }

  function handleToggleSub(clicked) {
    if (event.key == "Enter") {
      event.preventDefault();
      open_sub(clicked)
    }
  }

  /* IS ON DESKTOP */
  function clickMenu() {
    if (document.body.classList.contains("sticky-header-show-menu")) {
      menu_selector.classList.remove("animation-out");
      document.body.classList.remove("sticky-header-show-menu");
      document.getElementById("menu-text").innerHTML = "Meny";
      document.getElementById("menu-text").style.color = "inherit";
      menu_selector.classList.remove("active");
      menu_selector.getElementsByClassName("panel")[0].style.maxHeight = "0vh";
      hamburger.setAttribute("aria-expanded", "false");
      menu_selector.setAttribute("aria-hidden", "true");
      mobileOverlay.remove();

      menu_open = false;
    } else {
      document.body.classList.add("sticky-header-show-menu");
      document.getElementById("menu-text").innerHTML = "Lukk meny";
      document.getElementById("menu-text").style.color = "#ec0000";
      menu_selector.classList.add("active");
      menu_selector.getElementsByClassName("panel")[0].style.maxHeight = "94vh";
      hamburger.setAttribute("aria-expanded", "true");
      menu_selector.setAttribute("aria-hidden", "false");
      stickyHeader.insertBefore(mobileOverlay, stickyHeader.firstChild);

      menu_open = true;
    }

    sub_menu_open = false;
  }

  function open_sub(clicked) {
    if (!list_open_nav.includes(clicked)) {
      list_open_nav.push(clicked);

      let this_sub = document.getElementById(clicked);
      this_sub.children[0].style.transform = "rotate(-180deg)";
      this_sub.parentNode.classList.add("openSub");
      this_sub.parentNode.parentNode.lastElementChild.classList.add("openSub");

      sub_menu_open = true;
    } else {
      list_open_nav = list_open_nav.filter(function (e) {
        return e !== clicked;
      });

      let this_sub = document.getElementById(clicked);
      this_sub.children[0].style.transform = "rotate(0deg)";
      this_sub.parentNode.classList.remove("openSub");
      this_sub.parentNode.parentNode.lastElementChild.classList.remove("openSub");
      sub_menu_open = false;
    }
  }

  /* IS NOT ON DESKTOP */

</script>
