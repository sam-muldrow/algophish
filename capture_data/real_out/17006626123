<script async>

var breadcrumb_open = false;
var breadcrumbNav = document.getElementById("breadcrumb-nav");
var bcButton = document.getElementById("bcButton");

document.addEventListener("keydown", function (event) {
  if (event.key == "Escape" && breadcrumb_open) {
    breadcrumb_open = false;
    rotateArrow();
  }
});
function handleToggleBc() {
  if (event.key == "Enter") {
    event.preventDefault()
    rotateArrow();
  }
}

function rotateArrow() {
  var x = document.getElementById("breadcrumb-nav");
  var z = document.getElementById("arrow-down-breadcrumb");
  if (x.className === "breadcrumb") {
    x.className += " responsive";
    z.style.transform = "rotate(-180deg)";
    breadcrumbNav.setAttribute("aria-hidden", "false");

    breadcrumb_open = true;
  } else {
    x.className = "breadcrumb";
    z.style.transform = "rotate(0deg)";
    breadcrumbNav.setAttribute("aria-hidden", "true");

    breadcrumb_open = false;
  }
}

/* IS NOT ON DESKTOP */

</script>
