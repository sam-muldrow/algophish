  <script>
    $(document).ready(function () {
      //左侧悬浮图片
      // 回到顶部
      $('.to-top').click(function () {
        $('body,html').animate({
          scrollTop: 0
        })
      })

      var isLogin = function () {
        var loginstatus = '';
        $.ajaxSetup({
          async: false
        });
        $.get('/isLogin', '', function (data) {
          if (data != 1) {
            loginstatus = false;
          } else {
            loginstatus = true;
          }
        });
        return loginstatus;
      }
    })
  </script>
