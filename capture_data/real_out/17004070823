    <style>
.passt-stoerer__text-wrapper { cursor: pointer; }
</style>

<script>
let Stoerer = document.getElementsByClassName('passt-stoerer');
for (let i = 0; i < Stoerer.length; i++) {
    let Link = Stoerer[i].getElementsByClassName('info-icon');

    if (Link[0] != 'undefined' && Link[0] != '') {
    Stoerer[i].onclick = function() {
window.location = Link[0];
}
}
}

</script>

