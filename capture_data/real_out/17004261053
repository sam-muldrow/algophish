    $(function () {
        $.ajax({
            type: 'GET',
            url: '/openid/js/openid-en.js?v=dee84722',
            success: function () {
                openid.img_path = '/openid/images/';
                openid.init('openid_identifier');
                $('#openid_form,#openid_container,#openid_prefs').show();
            },
            dataType: "script",
            cache: true
        });
    });
</script>
<script type="text/javascript">
// Monkey patching of openid.getBoxHTML to allow customizing icons
    var tabfrom = 5;
    if (typeof openid !== 'undefined')
    {
        openid.getBoxHTML = function (box_id, provider, box_size, index) {
            var faClass = 'fa-' + provider['name'].toLowerCase();
            var jsHandler = 'javascript:openid.signin(\'' + box_id + '\');';
            if (provider["name"] === 'Google') {
                jsHandler = '#';
                faClass += '-plus';
            }
            return '<a tabindex="' + (++tabfrom) + '" class="header-alt-login-button ' + provider["name"] + '-signin-button" title="' + this.image_title.replace('{provider}', provider["name"]) + '"  href="' + jsHandler + '"><i class="fa ' + faClass + ' "><' + '/i><' + '/a>';
        };
    }
</script>
