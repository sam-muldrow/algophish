        var collection = [];
    // variables used in homepage.js
    var upload_form_max_upload_size = 314572800;
    var upload_form_drag_url = 'https://fs13u.sendspace.com/dragupload?SPEED_LIMIT=0&MAX_FILE_SIZE=314572800&UPLOAD_IDENTIFIER=1204375572.1700426105.40DEC9F6.27.0&DESTINATION_DIR=36';
