tyle>  <script>
		
	$(function(){
		var wp_url = 'https://en.wikipedia.org/w/api.php?action=query&format=json&callback=wiki_update&list=search&srwhat=text&srinfo=suggestion&srprop=&srredirects=1&srsearch=';
	    var yt_url = 'https://suggestqueries.google.com/complete/search?hl=en&client=youtube&hjson=f&jsonp=yt_update&ds=yt&cp=3&q=';
	    var bi_url = 'https://api.search.live.net/json.aspx?Appid=F550969A2EBF1211A345960C88E67163180795FE&sources=RelatedSearch&web.count=5&JsonType=callback&JSONCallback=bing_update&query=';
	    var yh_url = 'https://query.yahooapis.com/v1/public/yql?q=select%20*%20from%20search.suggest%20where%20query=""&limit=5&format=json&callback=yahoo_update';
	    var gg_url = 'https://google.com/complete/search?jsonp=gg_update&q=';

	    function wp_parse(data) {
	        var r = [];
	        if(!data.query || !data.query.search) return r;
	        $.each(data.query.search, function(key, row) {
	            r.push({
	                data: row.title.toLowerCase(),
	                value: row.title
	            });
	        });
	        return r;
	    }

	    function bing_parse(data) {
	        var r = [];
	        var t = data.SearchResponse.RelatedSearch;
	        if(!t) return r;
	        $.each(t.Results, function(key, row) {
	            r.push({
	                data: row.Title.toLowerCase(),
	                value: row.Title
	            });
	        });
	        return r;
	    }

	    function yt_parse(data) {
	        var r = [];
	        if(!data[1]) return r;
	        $.each(data[1], function(key, row) {
	            r.push({
	                data: row[0],
	                value: row[0]
	            });
	        });
	        return r;
	    }

	    function yahoo_parse(data) {
	        var r = [];
	        if(!data.results) return r;
	        $.each(data.results, function(key, row) {
	            r.push({
	                data: row[0].toLowerCase(),
	                value: row[0]
	            });
	        });
	        return r;
	    }

	    function lastWord(q){
	    	if(!q) return '';
	    	var l = q.split(/[ \t_-]/g);
	    	return l;
	    	// if( l.length >= 2){
	    	// 	return l[l.length-2] + l[l.length-1];
	    	// }
	    	// return l[l.length-1];
	    }

        var qid=100;

		$('.top_query').autocomplete({
			deferRequestBy: 150,
			lookup: function (query, done) {
		        // Do ajax call or lookup locally, when done,
		        // call the callback and pass your results:
		        var result = {
		        	suggestions: []
		            // suggestions: [
		            //     { "value": "United Arab Emirates", "data": "AE" },
		            //     { "value": "United Kingdom",       "data": "UK" },
		            //     { "value": "United States",        "data": "US" }
		            // ]
		        };

		      var c = 0;
					window.yt_update = function(data) {
					parsed = yt_parse(data);
					//result.suggestions.push('yt');
					result.suggestions.push.apply(result.suggestions, parsed);
					done(result);
			    };
					window.gg_update = function(data) {
					parsed = yt_parse(data);
					//result.suggestions.push('gg');
					result.suggestions.push.apply(result.suggestions, parsed);
					done(result);
			    };
			    window.wiki_update = function(data) {
					parsed = wp_parse(data);
					//result.suggestions.push('wi');
					result.suggestions.push.apply(result.suggestions, parsed);
					done(result);
			    };
			    window.bing_update = function(data) {
					parsed = bing_parse(data);
					//result.suggestions.push('bi'+parsed[0]);
					result.suggestions.push.apply(result.suggestions, parsed);
					done(result);
			    };
			    window.yahoo_update = function(data) {
					parsed = yahoo_parse(data);
					//result.suggestions.push('yh'+parsed[0]);
					result.suggestions.push.apply(result.suggestions, parsed);
					done(result);
			    };
			    $.ajax({
					dataType: 'jsonp',
					jsonp: 'cb',
					cache: true,
					jsonpCallback: 'wiki_update',
					url: wp_url + lastWord(query)
			    });
			    $.ajax({
					dataType: 'jsonp',
					cache: true,
					url: yt_url + lastWord(query)
			    });
			    $.ajax({
					dataType: 'jsonp',
					cache: true,
					url: gg_url + lastWord(query)
			    });
			    $.ajax({
					dataType: 'jsonp',
					cache: true,
					url: bi_url + lastWord(query)
			    });
			    $.ajax({
					dataType: 'jsonp',
					cache: true,
					url: yh_url.replace('query=""', 'query="'+query+'"')
			    });
		        //done(result);
		    },
		    onSelect: function (suggestion) {
		    	//return suggestion.value + ', ' + suggestion.data);
				$('.top_query').focus();
				//return suggestion.data+' ';
		    },
		    function (suggestion, currentValue) {
		    	return (currentValue ? currentValue + ' ': '') + suggestion;
		    }
		})
	});
	</script> <script type="text/javascript">
	
		function loadColumns(query, photo_query, pn, div_name, section, num, photo_num) {	
				$("html, body").animate({ scrollTop: 0 }, "slow");
				var container_html = $('#'+div_name);						
				container_html.html('<p style="margin: 40px;">loading <img src="//ecdn4.wn.com/or/assets/assets/wn/img/712.GIF" alt="" width="28" height="28"></p>');
            $.ajax({
                url: 'https://upge.wn.com/api/upge/worldnews/columns_videos02',
                data: {
                    'query': query,
                    'photo_query': photo_query,
                    'pn': pn,										
                    'section': section,
										'container': div_name,
                    'num': num,
                    'photo_num': photo_num
                },
                cache: false,
                dataType: 'jsonp',
								jsonpCallback: 'cbLoadSection',
                success: function(text) {		
	
                    container_html.html(text);	
                },
                error: function( req, status, err ) {
                    console.log( 'something went wrong ', status, err );                 
               } 
            });
    }    
		   
		
		function reloadNews(query, pn, tabname, section) {	
			$("html, body").animate({ scrollTop: 0 }, "slow");
			var container_html = $('#'+tabname);		
      container_html.html('<p style="margin: 40px;">loading <img src="//ecdn4.wn.com/or/assets/assets/wn/img/712.GIF" alt="" width="28" height="28"></p>');
            $.ajax({
                url: 'https://upge.wn.com/api/upge/worldnews/section_videos',
                data: {
                    'query': query,
                    'pn': pn,                    
                    'section': section,
                    'tabname': tabname
                },
                cache: false,
                dataType: 'jsonp',
                success: function(text) {																
                    container_html.html(text);
												
                },
                error: function( req, status, err ) {
                 console.log( 'something went wrong ', status, err );                 
               } 
            });
    }
    
						
    function findVideo(query){		
    		$.ajax({					
    	         url: 'https://upge.wn.com/api/upge/worldnews/video_id',                       
    	         cache: true,
    	         data: { 'query': query },
    	         dataType: 'jsonp',
    					 jsonpCallback: 'cbLoadVideoId',
    	         success: function(text) {
    	                    if (text.length > 0) {
    	                        video_id = text[0].id;									
    													
    													if (playerOn == false) {																	
                        					 triggerSmallPlayer(video_id);			
                        			} else {		
                        					 reloadSmallPlayer(video_id);	
    													}	 			 															      
    	                    }
    	         }
    		});							
    }	
        
		function popUp(url) {			
			window.open(url, 'ShareWindow', 'height=450, width=550, top=' + ($(window).height() / 2 - 275) + ', left=' + ($(window).width() / 2 - 225) + ', toolbar=0, location=0, menubar=0, directories=0, scrollbars=0');
			e.preventDefault();
		}
		



	function mouseOverMe(elem){
		$('.video-player').show();
		if (elem.attr('id')){
			var video_id = elem.attr('id');			
			var data = elem.parent().find('.video-data').html();
			$('.player-video-data').html(data);
			$('.video-player').html('<iframe src="https://www.youtube.com/embed/'+ video_id +'?enablejsapi=1&version=3&wmode=transparent&autoplay=1" frameborder="0" allowfullscreen="allowfullscreen" allow="accelerometer; clipboard-write; encrypted-media; gyroscope; picture-in-picture" style="width: 100%; height: 100%;"></iframe>');						
		}else {	
			var query = elem.find('.keywords').html();	
			if (!query.length) {
	            var query = elem.find('.title_keywords').html();	
	        }  						
			$.ajax({
							context: elem,
	                        url: 'https://wn.com/api/upge/cheetah-search-adv/video',                       
	                        cache: true,
	                        data: {
	                            'query': query
	         },
	                        dataType: 'jsonp',
	                        success: function(text) {
	                        	if (text.length > 0) {
	                            video_id = text[0].id;	                            
	                            $('.video-player').html('<iframe id="+ video_id +" src="https://www.youtube.com/embed/'+ video_id +'?enablejsapi=1&version=3&wmode=transparent&autoplay=1" frameborder="0" allowfullscreen="allowfullscreen" style="width: 100%; height: 100%;"></iframe>'); 
	                                            
	                         	}
	                       }
	                   });  										
				
			}		
	}	
    </script> <script type="text/javascript">		
    window.currentTab = '';
    if(window.console && console.log && $.cookies.get('debug')){
        clog = console.log.bind(console);
    }else{
        function clog(info){}
    }
    function updateUrl(tabUrl){
        clog("Update URL:", tabUrl);
        if(tabUrl == 'home') tabUrl = '';				
        document.location.hash = '#/'+tabUrl;
    }
    function loadTab(tabName){
        clog("Load tab:", tabName);
        var $activeTab = $('.nav a[data-url="' + tabName+'"]');
        if(tabName == 'local'){
            loadLocalNews(window.pageEdition, 10, 1, 'local');
        }else if(tabName == 'edition'){
            loadLocalNews(window.pageEdition, 10, 1, 'local');
        }else if($activeTab.attr('href')=='#content'){
            var query = $activeTab.data("hidden");						
						var section = $activeTab.text();		
            reloadNews(query, 1, 'content', section);							
        }else if(tabName == 'search'){
            setTimeout(function(){
                $('.top_query').focus();
            }, 200);
        }
    }					

    function setPageEdition(edition){
        window.pageEdition = edition;
        $.cookies.set('wnedition', window.pageEdition);
        updateSelectLanguage();
    }
		
    function activateTab(tabName, fromClick){
        clog("Activate tab:", tabName, 'fromClick=', fromClick);
        if(tabName.match('^(local|edition)/')){ // decompose
            setPageEdition(tabName);
            tabName = 'local';
        }	
        var tabUrl = (tabName == 'local') ? window.pageEdition : tabName;

        if(tabUrl == window.currentTab) return;
        window.currentTab = tabUrl;

        var $activeTab = $('.nav a[data-url="' + tabName+'"]');	
        //$activeTab.tab('show');
				$('.nav a[data-url="' + tabName+'"]').tab('show');
        if(!fromClick){
            $activeTab.click(); // call onclick="" handler.						
        }
        updateUrl(tabUrl);
        loadTab(tabName);
        $("html, body").animate({ scrollTop: 0 }, "slow");
				$('.nav a[data-url="' + tabName+'"]').next('.submenu').slideDown();	
				$('.submenu a[data-url="' + tabName+'"]').parent().parent().slideDown();	
    };

    function setDetectedLanguage(){
        setPageEdition('local/'+$('#select-language').val().toLowerCase());
    }

    window.useHash = (document.location.hostname != 'wn.com')

    function getUrlTab(){
        var path = document.location.pathname.replace(/^\//,'');
        var hash = document.location.hash.replace(/^#\/?/,'').replace(/\/$/,'');
        var tabName = useHash ? hash: (path || hash);
        return tabName.toLowerCase();
    }

    $(function(){		
		
        window.pageEdition = $.cookies.get('wnedition');
				
        activateTab(getUrlTab());
				
        if(useHash){
	        $(window).on('hashchange', function(){
	            var tabName = getUrlTab();
	            clog("Hash updated:", tabName);
	            activateTab(tabName);
      				if(!tabName){
      					activateTab('home');
      				}
	        });
        }		
			  var menu_expanded = false;


        $('.nav .parent a').each(function(){
              var $this = $(this);   
							var offsetLeft = $this.parent().offset().left;	
							var offsetRight = $this.parent().offset().right;		
							var winWidth   = $(window).width();	
      		  	var marginLeft  = winWidth - offsetLeft;	
							if (marginLeft > 700){									
									var offsetLeftHalf = offsetLeft / 2;		
									$this.parent().find('.submenu').children().first().css('padding-left', offsetLeftHalf+'px');	
									$this.parent().find('.submenu').css('text-align', 'left');									
							} else {
									var marginLeftHalf = marginLeft / 2;							
									$this.parent().find('.submenu').children().last().css('padding-right', marginLeftHalf+'px');			
									$this.parent().find('.submenu').css('text-align', 'right');
							}   				
        })				
      	$('.nav .parent').hover(function() {								
							$('.bottom-menu').stop().animate({ backgroundColor: "#337ab7" }, "slow");
							if (!$(this).hasClass("active")){					 				
									$('.nav .active').find('.submenu').stop(true, true).slideUp(200);
							}
      				$(this).find('.submenu').stop(true, true).slideDown(200);						
      	}, function() {
						if (!$(this).hasClass("active")){
      					$(this).find('.submenu').stop(true, true).slideUp(200);								
						}				
						
						$('.nav .active').find('.submenu').stop(true, true).slideDown(200);
						$('.bottom-menu').stop().animate({ backgroundColor: "#f9f9f9" }, "300");
				
      	});				

				$('.nav a[data-url]').click(function (e) {		
						$('.submenu li').removeClass('active');
						if ($(this).parent().parent().is(':visible') &&  $(this).parent().parent().hasClass('submenu'))  {															
								
            }else if ( $(this).parent().hasClass('active') ){								
								
						}else{								
							$('.dropdown-menu').slideUp();
						}	
						activateTab($(this).attr('data-url'), true);
						$('.bottom-menu').animate({ backgroundColor: "#337ab7" }, "slow");
        });					


        $('.local_news').click(function (e) {
            e.preventDefault();
            chooseEdition();
            activateTab('local');           
            menu_expanded = false;
       });
        $('#chat-search').submit(function(event) {
                var search_string = $("#chat_query").val();
                search_string = search_string.toLowerCase().replace(/[\.\/\: _!]+/g, '_');
								window.open('https://wn.com/' + search_string + '/chat', '_blank');
                return false; // do not submit the form
        });

       	$('#weather-map').submit(function(event) {
            var search_string = $("#city_map_name").val();
            search_string = search_string.toLowerCase().replace(/[\.\/\: _!]+/g, '_');
						window.open('https://upge.wn.com/?t=cheetah-weather/map.txt&query=' + search_string, '_blank');
            return false; // do not submit the form
         });

         $('#radio-search').submit(function(event) {
                    $('.radio-search-results').show().html("$('Loading...')");
                    var query = $('.radio_query').val();
                    $.ajax({
                            data: {'query': query },
                            url: '/api/upge/cheetah-photo-search/radio_wn',
                            cache: true,
                            success: function(text) {
                                $('.radio-search-results').html($(text));
                                $('.radio-user-results').hide();
                            },
                       });
            return false; // do not submit the form
        });


     $('#main-search').submit(function(event) {
            var search_type = 'news';
                return true;
     });
		 $('#main-search-m').submit(function(event) {
            var search_type = 'news';
                return true;
     });

    	$('.popup').click(function(e) {
        	e.preventDefault();
			    	window.open($(this).attr('href'), 'ShareWindow', 'height=450, width=550, top=' + ($(window).height() / 2 - 275) + ', left=' + ($(window).width() / 2 - 225) + ', toolbar=0, location=0, menubar=0, directories=0, scrollbars=0');
	        return false;
    	});
		$('.audio-hide ').click(function (e) {
				$('#audio-player').animate({bottom:'-240px'},1000);
				$('#audio').attr('src', '');		
				
				$('.icon').removeClass('active');
    });

