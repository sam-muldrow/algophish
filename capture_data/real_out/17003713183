<script>
window.ajaxLoginUrl = "https://www1.pconline.com.cn/2023/cms/home/js/pconline.login.js";
window.ajaxLogin = function () {
var url = "";
/*Í·²¿Ö¸¶¨ajaxLoginUrl£¬±ÜÃâÍ·²¿ÄÚÇ¶£¬µ×²¿ÍâÁ´Ôì³É¸üÐÂ²»Í¬²½µÄÎÊÌâ£¨½â³ýÒÀÀµ£©*/
if (document.getElementById('ajaxLogon') && "ajaxLoginUrl" in window) {
url = ajaxLoginUrl;
loadJs(url);
}
function loadJs(url) {
var js = document.createElement("script");
js.charset = "GBK";
js.src = url;
document.getElementsByTagName("head")[0].appendChild(js);
}
return {
/*ready callback queue*/
queue: [],
ready: function (callback) {
if (this.queue) {
this.queue.push(callback);
} else {
callback.call(this);
}
/*Ã»ÒýÓÃÍ·²¿£¬Ôò°´ÐèÒýÓÃlogin js*/
url && (loadJs(url), url = "");
}
}
}();
</script>
