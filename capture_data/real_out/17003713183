<script>
function ksSubmit(e){
var searchName = 'OPPO Find N3';
var tgLink = '';
var ksTxt = document.getElementById('JksQ');
if((ksTxt.value==searchName||ksTxt.value=='')&&tgLink!=''){
window.open(tgLink);
return false;
}else{
ksTxt.value = ksTxt.value||ksTxt.getAttribute('placeholder');
return true;
}
}
</script>
