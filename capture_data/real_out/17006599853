    <section id="section--24c433cb-2b78-4593-88a1-e018b2a7590b" class="article-listing featured-interests" aria-label="Feature Interest Listing">
        <div class="container-fluid">
            <div class="row no-gutters">
                        <div class="col-sm-12 col-md">
                            <a href="/academics/schools-colleges/engineering/academics/undergraduate/resource-energy" class="article-listing-link" target="_self">
                                <div class="uta-lazy-bg" data-src="https://cdn.web.uta.edu/-/media/project/website/engineering/general-images/ree/woman-working-with-windmill-models-behind-her.ashx?revision=05de238d-7f9a-40c0-aac8-8c88ccb46723" aria-hidden="true"></div>
