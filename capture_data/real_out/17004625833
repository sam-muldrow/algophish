    $(document).ready(function () {
        if ('True' == "True" || 'False' == "True") {
            document.getElementById('priceEarnings').click();
        }
        else {
            document.getElementById('otherTabs').click();
        }

        RatiosSummaryWidget();

        $("#priceEarnings_14").click(function () {
            var mdi = 14;
            var pe = $('#priceEarnings_14').val();
            if (mdi == 14 && pe == 14) {
                LoadTabs(this, '/ar/financialratios/financialpriceearningsummarypartial/1/priceperbook/quarter/14')
            }
        })
    });
