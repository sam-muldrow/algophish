<script>
    var a = [];
a.push([77,'سابك','سابك']);a.push([74,'نماء للكيماويات','نماء-للكيماويات']);a.push([72,'التصنيع','التصنيع']);
    $(document).ready(function () {
        $('#slideNext').click(function () {

            var index = parseInt($('#slideNext').attr('relf'));
            var maxIndex = parseInt($('#next_prev_section').attr('maxIndex'));

            if (index == maxIndex) {
                // disable next link
                $('#slideNext').addClass('disabled');
            }
            else {

                OnNextPrevClick(index);

                $('#slidePrev').attr('relf', index - 1);// enable prev link
                $('#slidePrev').removeClass('disabled');
                $('#slideNext').attr('relf', index + 1);

                if (index == (maxIndex - 1)) {
                    // disable next link
                    $('#slideNext').addClass('disabled');
                }
            }

            return false;
        });
        $('#slidePrev').click(function () {

            var index = parseInt($('#slidePrev').attr('relf'));

            if (index < 0) {
                // disable prev link
                $('#slidePrev').addClass('disabled');
            }
            else {

                OnNextPrevClick(index);

                $('#slidePrev').attr('relf', index - 1);
                $('#slideNext').attr('relf', index + 1);// enable next link
                $('#slideNext').removeClass('disabled');

                if (index == 0) {
                    // disable prev link
                    $('#slidePrev').addClass('disabled');
                }
            }

            return false;
        });
    });

    function OnNextPrevClick(index) {
        $('#lblComp').fadeOut(500, function () {
            $('#lblComp').html(a[index][1]);
            var moreUrl = $('#ProdCapMoreURL').attr('relf');
            moreUrl = moreUrl.replace('-1', a[index][0]);
            moreUrl = moreUrl.replace('-2', a[index][2]);
            $('#ProdCapMoreURL').attr('href', moreUrl);
            getTableForPetroChemProducts(index);
        });
        $('#lblComp').fadeIn(500);
    }
    function getTableForPetroChemProducts(index) {
        var compID = a[index][0];
        var ajUrl = '/ar/petrochemicalproductsbycompanyonhomepage/282/-1';
        ajUrl = ajUrl.replace(-1, compID);

        $.ajax({
            type: 'GET',
            url:  ajUrl,
            //url: '/en/petrochemicalproductsbycompanyonhomepage/' + $('#hdnSectionID').val() + '/' + compID,
            cache: false,
            async: false,
            beforeSend: function () {
                //$("#customPDFLoading").show();
            }
        })
        .done(function (data) {
            $("#dvProdCapProds").html(data);
        })
        .fail(function (jqXHR, textStatus, errorThrown) {
            //$("#customPDFLoading").hide();
            if (jqXHR.readyState != 0) {
                alert('error occurred');
            }
        });
    }
</script>
                </div>

