<script type="text/javascript">
    $(document).ready(function (){
        $(".table-compare-freeChecking .bankName td.bank4, .table-compare-freeChecking .bankName td.bank7").addClass("chart-icircle");      
        if (getWinW() <= 767) {
            $(".chart-icircle .discover-tooltip").on("mouseover click hover focus tap", function (e) {
                    if (e.target.tagName && ['button', 'span'].indexOf(e.target.tagName.toLowerCase()) > -1) {
                        $(".bankName .bank0, .bankName .bank1").removeClass("sticky-col");
                        $(".bankName td .discover-tooltip").parents('.bankComapreWrapper').css('overflow', 'visible');
                    }  
                }
            );  
            $(".chart-icircle .discover-tooltip a.closebtn").on("click hover focus tap", function (e) {  
                    $(".bankName .bank0, .bankName .bank1").addClass("sticky-col");
                    $(".chart-icircle .discover-tooltip").find(".cashback").hide(); 
                    $(".bankName td .discover-tooltip").parents('.bankComapreWrapper').css('overflow', 'auto');  
                }
            );  
            $(".chart-icircle .discover-tooltip").on('mouseleave', function (e) {  
                e.preventDefault();  
                $(".bankName .bank0, .bankName .bank1").removeClass("sticky-col");  
                $(".bankName td .discover-tooltip").parents('.bankComapreWrapper').css('overflow', 'visible');  
                e.stopImmediatePropagation();  
            });  
            $(".chart-icircle .discover-tooltip .cashback").on('mouseleave', function (e) {  
                e.preventDefault();  
                $(".bankName .bank0, .bankName .bank1").addClass("sticky-col");  
                $(this).find(".cashback").hide();  
                $(".bankName td .discover-tooltip").parents('.bankComapreWrapper').css('overflow', 'auto');  
                e.stopImmediatePropagation();  
            });  
        }
    });
</script>
