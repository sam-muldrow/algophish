                $(this).find("video").attr("preload","auto");
                $(this).find("video").attr("src", $videosrc + "?autoplay=1");
});
$(".video-container .modal").on("hide.bs.modal",function(){
                $(this).find("video").attr("preload","none");
                $(this).find("video").attr("src", $videosrc);
                $(this).find("video").trigger("pause");
});
}
});
