<!-- Start Reskin Tablet Nav Fixes-->
<script>
if($('.rebrand-page').length >0){
      $(window).on('resize',function(){
            if($(window).width() > 1024){
            reskinDeskScroll();
            $(window).on('scroll',reskinDeskScroll);
            }else if($(window).width() <=1024){
                  reskinTabScroll();
                  $(window).on('scroll',reskinTabScroll);
            }
      });
      if($(window).width() <= 1024){
            $("body").on('click',function(e){
                  $(".rebrand-page .header-wrapper .secondary-header-wrapper").removeClass("active");
                  if($(window).width() < 768){
                        $('div#overlayBG').css({"display":"none", "opacity":"0.4"});
                  }
            });
      }
}
function reskinDeskScroll(){
      var heightP = 167;          
      var scrollAmountP= $(window).scrollTop();
      if(scrollAmountP>heightP){
            $('.rebrand-page .header-wrapper').addClass('affix');
      }else{
            $('.rebrand-page .header-wrapper').removeClass('change-to-mobile');
            $('.rebrand-page .header-wrapper').removeClass('affix');
            $('.rebrand-page .navbar-toggle').hide();
            $('.rebrand-page .secondary-header-wrapper').removeClass('down');
      }    
}
function reskinTabScroll(){
      $('.rebrand-page .header-wrapper').addClass('change-to-mobile');
      $('.rebrand-page .header-wrapper').addClass('affix');
      $('.rebrand-page .navbar-toggle').show();
      $('.rebrand-page .secondary-header-wrapper').addClass('down');
      if($(window).width() <= 767){
            $('.rebrand-page .header-wrapper').removeClass('change-to-mobile');
            $('.rebrand-page .header-wrapper').removeClass('affix');
      }
}
</script>
