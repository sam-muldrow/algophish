$(document).ready(function(){
      if($('.cta-quiz').length >0 ){
            $('.cta-quiz .next').on('click', function(e) {
                  var flagVal= false;
                  var disclaimerContent1= '<div class="disc-content"><p>This simulation assumes that you participate in a workplace retirement plan. If you do not, then your contribution up to the contribution limit is probably fully tax deductible.</p></div>';
                  var disclaimerContent2= '<div class="disc-content"><p>This simulation assumes that you or your spouse participate in a workplace retirement plan. If neither of you do, then your contribution up to the contribution limit is probably fully deductible.</p></div>';
