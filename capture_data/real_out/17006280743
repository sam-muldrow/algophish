<script>
    function ub_principal_search(keyword){
        let keyPress = event.keyCode;
        if (keyPress === 13) {
             event.preventDefault();
             let params = new URLSearchParams(window.location.href)
             const ubsearch = 'https://web.ub.edu/cercador';
             params = new URLSearchParams(ubsearch+"?keyword="+keyword.value);
             let decoded = decodeURIComponent(params);
             window.location.href = decoded;
        }
    }

    const launchPropuestaSearch = (propuesta) => {
        let params = new URLSearchParams(window.location.href)
        const ubsearch = 'https://web.ub.edu/cercador';
        params = new URLSearchParams(ubsearch+"?keyword=" + propuesta);
        let decoded = decodeURIComponent(params);
        window.location.href = decoded;
    }

    Liferay.on('allPortletsReady', function() {
        document.querySelector('.btnSearch').addEventListener("click", () => {
            let propuestasHtml = '<div class="propuestas">';
            propuestasHtml += '<span class="propuestas-title">També et pot interessar...</span>';
            propuestasHtml += '<a class="propuesta-link" href="javascript:void(0);" onclick="launchPropuestaSearch(\'Doctorat\')">Doctorat</a>';
            propuestasHtml += '<a class="propuesta-link" href="javascript:void(0);" onclick="launchPropuestaSearch(\'Facultats\')">Facultats</a>';
            propuestasHtml += '<a class="propuesta-link" href="javascript:void(0);" onclick="launchPropuestaSearch(\'Transparència\')">Transparència</a>';
            propuestasHtml += '</div>';

            document.getElementById('theme-search-propuestas').innerHTML = propuestasHtml;

        });
    });

</script></body>

</html>
		
	

