    <div id="searchLinks" class="d-none d-xl-block">
            <h2>Recursos per a:</h2>
            <ul>
        
                    <li><a href="https://web.ub.edu/recursos#investiga">Investigadors</a></li>
        
                    <li><a href="https://web.ub.edu/recursos#futurs">Futurs estudiants</a></li>
        
                    <li><a href="https://web.ub.edu/recursos#formacio">Personal investigador en formació</a></li>
        
                    <li><a href="https://web.ub.edu/recursos#internacional">Internacional</a></li>
        
                    <li><a href="https://web.ub.edu/recursos#empreses">Empreses</a></li>
        
                    <li><a href="https://web.ub.edu/recursos#alumni">Alumni</a></li>
        
                    <li><a href="https://web.ub.edu/recursos#comunicacio">Mitjans de comunicació</a></li>
            </ul>
    </div>
</div>
<script>
    var propuestas = ['Arxiu històric','Beques i ajuts','Cultura','Divulgació','Doctorat','Facultats','Graus','Igualtat','Màsters i postgraus','Participa','Producció científica','Responsabilitat social','Rànquings','Serveis científics','Transparència',];
    var length = propuestas.length;
    var i = 0;
    var x = 0;
    var speed = 100;
    var texto = "";

    var placeHolder = {
    	cxyw__modify: function (){
    			if (i < length){
    				document.getElementById("cxyw__searchField").placeholder = "";
    				let timerId = window.setTimeout(typeWriter.cxyw__write, speed);
    				i++;
    			} else {
    				i = 1;
    				document.getElementById("cxyw__searchField").placeholder = "";
    				timerId = window.setTimeout(typeWriter.cxyw__write, speed);
    			}
    	}
	}

    var typeWriter = {
    	cxyw__write: function () {
    		if (x < propuestas[i-1].length) {
    			document.getElementById("cxyw__searchField").placeholder += propuestas[i-1].charAt(x);
    			x++;
    			timerId = setTimeout(typeWriter.cxyw__write, speed);
    		} else {
    			x = 0;
    		}
    	}
	}

    let intervalId = setInterval(placeHolder.cxyw__modify,2000);

    document.getElementById("cxyw__searchField").onblur = function() {
		texto = document.getElementById("cxyw__searchField").value;
		if(texto == ""){
			intervalId = setInterval(placeHolder.cxyw__modify,2000);
		} else {
			document.getElementById("cxyw__searchField").placeholder = "";
		}
	};

    document.getElementById("cxyw__searchField").onfocus = function() {
		clearTimeout(timerId);
		clearInterval(intervalId);
		x = 0;
		texto = document.getElementById("cxyw__searchField").value;
		if(texto == ""){
			document.getElementById("cxyw__searchField").placeholder = "Cerca per text";
		} else {
			document.getElementById("cxyw__searchField").placeholder = "";
		}
	};
	
    document.addEventListener('visibilitychange', function() {
		if(document.hidden) {
			clearTimeout(timerId);
			clearInterval(intervalId);
			x = 0;
			texto = document.getElementById("cxyw__searchField").value;
			if(texto == "") {
				document.getElementById("cxyw__searchField").placeholder = "Cerca per text";
			} else {
				document.getElementById("cxyw__searchField").placeholder = "";
			}
		} else {
			texto = document.getElementById("cxyw__searchField").value;
			if(texto == "") {
				intervalId = setInterval(placeHolder.cxyw__modify,2000);
			} else {
				document.getElementById("cxyw__searchField").placeholder = "";
			}
		}
	});
</script>
		</div>

		

	




							
						</div>
					
				
			
		
	




	

	

	

	

	

	




	
	

					
				
			
		
	
	



	</div>

