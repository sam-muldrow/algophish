                screenLG = getComputedStyle(document.documentElement).getPropertyValue("--breakpoint-lg").slice(0, -2),//992
                screenMD = getComputedStyle(document.documentElement).getPropertyValue("--breakpoint-md").slice(0, -2);//768
                screenSM = getComputedStyle(document.documentElement).getPropertyValue("--breakpoint-sm").slice(0, -2);//576
            Liferay.on('allPortletsReady', function(e) {
                new Glider(document.querySelector('.news.ymal_ .glider'), {
                    slidesToShow: 1,//El número de diapositivas que se mostrarán en el contenedor. Si este valor se establece en auto, se calculará automáticamente en función del número de elementos que caben en la ventana del contenedor
