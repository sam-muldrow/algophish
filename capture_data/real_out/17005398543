<script type="text/javascript">
    
   $.extend($.ui.autocomplete.prototype.options, {
        open: function(event, ui) {
            $(this).autocomplete("widget").css({
                "width": ($(this).outerWidth() + "px"),'overflow':'hidden'
            });
        }
    });
     $( function() {
        function log( message ) {
//          $( "<div>" ).text( message ).prependTo( "#log" );
//          $( "#log" ).scrollTop( 0 );
            console.log(message);
        }
        $( "#q, #search_term" ).autocomplete({
//            source: SUGGESTKEYWORDURL+"?query=an",
          source: function( request, response ) {
//              var ajaxUrl = "https://jqueryui.com/resources/demos/autocomplete/search.php";
//            var ajaxUrl = SUGGESTKEYWORDURL;
//            console.log(ajaxUrl);
            $.ajax( {
              url: window.ajaxurl,
              dataType: "json",
              data: {
                act:'getSearchKeyword',
                query: request.term
              },
              success: function(data) {
//                   console.log("success");
//                  console.log(data);
                response( data );
              },
              error : function (data){
                  console.log("error");
                  console.log(data);
              }
//              ,
//              complete : function(data){
////                  console.log("completed");
////                  console.log(data);
//              }
            } );
          },
          minLength: 2,
          select: function( event, ui ) {
            log( "Selected: " + ui.item.value );
          },
          classes: {
             "ui-autocomplete": "overlay-autocomplete",
          }
        } );
      } );
    
</script>

<script type="text/javascript">
    
    $(document).on("click", ".search_overlay_trigger", function(){
        _gaq.push(['_trackEvent', 'jsClickSearchJob', 'click_', 'f7ff850b337b0b4468f47b3a36eaae1d_-_64.222.201.246']);
    });
    
    $(document).on("click", ".submit_search_queries", function(){
        _gaq.push(['_trackEvent', 'jsSearchJob', 'click_', 'f7ff850b337b0b4468f47b3a36eaae1d_-_64.222.201.246']);
    });
    
    $(document).on("click", ".jobSearchButton", function(){
        _gaq.push(['_trackEvent', 'jsSearchJob', 'click_', 'f7ff850b337b0b4468f47b3a36eaae1d_-_64.222.201.246']);
    });

    function resize_course_cards() {
        var maxHeight = 0;
        jQuery(".course_thumb p").each(function(i,el) {
          var height = jQuery(el).outerHeight();
          if(height > maxHeight) {
            maxHeight = height;
          }
        });
        // console.log("maxHeight : " + maxHeight);
        jQuery(".course_thumb p").height(maxHeight);
    }

    function showOverlay() {
        $("#recruiter_search_overlay").fadeIn();
        resize_course_cards();
    }

    jQuery(document).ready(function () {

        jQuery(".mobverhdicon i.fa-search").off('click');
        jQuery(".mobverhdicon i.fa-search").on('click', showOverlay);

        jQuery("a.search_overlay_trigger").on('click', function(e) {
            e.preventDefault();
            showOverlay();
            resize_course_cards();

            var squareWidth = jQuery(".employee_thumb").width();
            console.log("width",squareWidth);
            
            var maxHeight = 190 + squareWidth;
            jQuery(".hero_image").css('max-height', maxHeight);
        });
        jQuery("#recruiter_search_overlay").on('click', ".select-dropdown", function(){
            if(jQuery(this).hasClass("inactive")) {
                jQuery("#recruiter_search_overlay .select-dropdown").addClass("inactive");
                jQuery(this).removeClass("inactive");
            } else {
                jQuery(this).addClass("inactive");
            }          
        });
        
        jQuery(".close_overlay").on('click', function() {
          jQuery("#recruiter_search_overlay").fadeOut();
          jQuery("body").removeClass('fixbody');
        });

        jQuery("#search_experience").on('click','li', function() {
          var selected_value = jQuery(this).attr('value');
          console.log("selected : " + selected_value);
          jQuery("#recruiter_search_overlay .expSelectBox").find("option[value=" + selected_value +"]").prop('selected','selected');
          jQuery("#search_experience span.dropdown_label").html(jQuery("#recruiter_search_overlay .expSelectBox").find("option[value=" + selected_value +"]").text());
        });
        
        jQuery("#search_location").on("change", "li.dropdown_list input[type=checkbox]", function() {
            var selectedCount = jQuery("#search_location").find("input[type=checkbox]:checked").length;
            console.log(selectedCount);

            if(selectedCount == 0) {
                jQuery("#search_location").find(".dropdown_label").text("Location");
            } else {
                jQuery("#search_location").find(".dropdown_label").text(selectedCount + " selected");
            }
        });

        jQuery("#search_range").on('click','li', function() {
          var selected_value = jQuery(this).attr('value');
          console.log("selected : " + selected_value);
          jQuery("#recruiter_search_overlay .rangeSelectBox").find("option[value=" + selected_value +"]").prop('selected','selected');
          jQuery("#search_range span.dropdown_label").html(jQuery("#recruiter_search_overlay .rangeSelectBox").find("option[value=" + selected_value +"]").text());
        });

        jQuery("#recruiter_search_overlay .submit_search_queries").on('click', function() {

            var searchQuery = $("#search_term").val();
            searchQuery = searchQuery.replace(/\//g, "SLASH");
            searchQuery = searchQuery.trim();
            searchQuery = searchQuery.replace(/\s+/g, '+');

            var currsearchurl = $("#currsearchurl").val();

            
            if ($('#overlay-exp').val() != '') {
                var experienceValue = $('#overlay-exp').val();
            } else {
                var experienceValue = 0;
            }
            
            var rangeQuery = "";
            var searchRange = $('#overlay-range').val();
            if (searchRange != '' && searchRange > 0) {
              rangeQuery = "?range="+searchRange;
            }

            var locationValue = '0';
            var locationText = 'any';
            var countCheck = $('#search_location input[type=checkbox]:checked').length;

            if (countCheck > 0) {
              locationValue = '';
              locationText = '';
              $('#search_location input[type=checkbox]:checked').each(function () {
                locationValue += $(this).val() + "_";
                countCheck += $(this).attr('loctext') + "_";
              });

              locationValue = locationValue.trim('-');
              locationValue = locationValue.slice(0, -1);
            }
            else {
               locationValue = 0;
            }
            searchQuery = encodeURIComponent(encodeURIComponent(searchQuery));

            window.location.href = currsearchurl+searchQuery+'-'+locationValue+'-'+ experienceValue+'-0-1.html'+rangeQuery;;
            return false;
        });
    });

    jQuery(window).resize( function() {
        resize_course_cards();
    });

    function resizedw(){
      if(jQuery(window).height() < jQuery("#recruiter_search_overlay .overlay_wrapper").outerHeight()) {
        console.log("1");
        jQuery("#recruiter_search_overlay .overlay_container").addClass('scrollContent');
      } else {
        console.log("2");
        jQuery("#recruiter_search_overlay .overlay_container").removeClass('scrollContent');
      }
    }

    var doit;
    window.onresize = function(){
      clearTimeout(doit);
      doit = setTimeout(resizedw, 100);
    };

        
</script>
<script type="text/javascript">
    var delhiNCRCities = [36,41,40,37,38];
