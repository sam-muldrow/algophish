    .app-linkage__option--go {
        background-color: var(--primary-color);
        color: var(--secondary-color);
        font-weight: bold;
    }
</style>

<div id="mobile-app-linkage-banner">
    <div class="app-linkage__container">
        <h1 class="app-linkage__heading">Experience better on iiimjobs app</h1>
        <p class="app-linkage__subheading">Apply on the go and get instant notification on any recruiter activity.</p>
        <div class="app-linkage__options-container">
            <button id="web-stay-button" class="app-linkage__option app-linkage__option--stay">Not Now</button>
            <button id="app-link-button" class="app-linkage__option app-linkage__option--go">Switch to App</button>
        </div>
    </div>
</div>

<script>
    function gaEvent(event, data) {
        if (typeof _gaq !== 'undefined' && _gaq && _gaq.push) {
            const dataStr = Object.entries(data).map(([k, v]) => `${k}=${v}`).join('__');
            _gaq.push(['_trackEvent', 'CallOuts', event, dataStr]);
        }
    }
    function homepageLinkageModal() {
        const appLinkButton = document.getElementById('app-link-button');
        const webStayButton = document.getElementById('web-stay-button');
        const modal = document.getElementById('mobile-app-linkage-banner');
        if (window.innerWidth <= 800) {
            gaEvent('ViewMobileAppNudge', { Type: 'LoggedOut_Banner', UserId: 'NA' });
            modal.style.display = 'block';
        }
        if (appLinkButton) {
            appLinkButton.addEventListener('click', (ev) => {
                ev.preventDefault();
                gaEvent('DownloadApp', { Type: 'LoggedOut_Banner', UserId: 'NA' });
                if (/iPad|iPhone|iPod/.test(navigator.userAgent)) {
                    window.location = 'https://apps.apple.com/in/app/iimjobs/id538258273'
                } else {
                    window.location = 'https://play.google.com/store/apps/details?id=com.org.iimjobs'
                }
                modal.style.display = null;
            });
        }
        if (webStayButton) {
            webStayButton.addEventListener('click', (ev) => {
                ev.preventDefault();
                gaEvent('IgnoreApp', { Type: 'LoggedOut_Banner', UserId: 'NA' });
                modal.style.display = null;
            })
        }
    }
    homepageLinkageModal();
</script>
</div>
