<script type="text/javascript">
        //<![CDATA[
        var _gaq = _gaq || [];
        _gaq.push(['_setAccount', 'UA-2175565-2']);
        _gaq.push(['_setDomainName', '.iimjobs.com']);
        _gaq.push(['_setAllowLinker', true]);
        _gaq.push(['_setAllowHash', false]);
        _gaq.push(['_trackPageview']);

        (function() {
            var ga = document.createElement('script');
            ga.type = 'text/javascript';
            ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') +
                '.google-analytics.com/ga.js';
            var s = document.getElementsByTagName('script')[0];
            s.parentNode.insertBefore(ga, s);
        })();
        //]]>
    </script>
     <!-- ga4 code start -->
     <script async src="https://www.googletagmanager.com/gtag/js?id=UA-2175565-2"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());
        gtag('config', 'UA-2175565-2');
    </script>
    <script type="text/javascript">
        if (true || (typeof _gaq === 'undefined' || typeof _gaq !== 'object')) {

            var old_gaq = _gaq;
            var newGAQ = {};
            newGAQ.push = function (eventDataArr) {
                // as per ga3
                old_gaq.push(eventDataArr);


                // if (array[0] === '_trackPageview') {
                //     gtag('event', '_trackPageview', {
                //         'non_interaction': true
                //     })
                // }
                // else if (array[0] === '_trackEvent') {
                //     gtag('event', '_trackEvent', {
                //         'event_label': array[3],
                //         'event_category': array[1],
                //         'event_action': array[2],
                //         'non_interaction': true
                //     })
                // }

                // as per ga4 
                var ga4action = eventDataArr[2] + "New";
                var ga4category = eventDataArr[1];
                var ga4label = eventDataArr[3];
                var ga4params = [];
                if(ga4label){
                    ga4params = ga4label.split("__");
                }
                var ga4customEvents = {
                    // 'event_label': ga4label,
                    'event_category': ga4category,
                    'event_action': ga4action,
                    'non_interaction': true
                };
                var ga4i = 1;

                while (ga4params.length && ga4i < 50) {
                    ga4i++;
                    if (ga4params.length == 1) {
                        ga4customEvents["Others"] = ga4params.shift();
                    } else {
                        var ga4firstElement = ga4params.shift();
                        var ga4prefixSplit = ga4firstElement.split("_")
                        if (ga4prefixSplit.length != 1) {
                            ga4customEvents["EventType"] = ga4prefixSplit[0];
                        }
                        ga4customEvents[ga4firstElement] = ga4params.shift(); 
                    }
                }

                gtag('event', ga4action, ga4customEvents);
            }

            _gaq = newGAQ;
        }
    </script>
    <!-- ga4 code end -->
</body>

