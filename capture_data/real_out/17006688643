<div data-component="Video" data-settings="{&#34;autoplay&#34;:true,&#34;autoplayOnMouseOver&#34;:true,&#34;backgroundColor&#34;:&#34;transparent&#34;,&#34;blurredBackgroundClasses&#34;:[&#34;filter&#34;,&#34;blur-s&#34;,&#34;scale-120&#34;],&#34;jwplayerMediaId&#34;:&#34;J7R59CFB&#34;,&#34;lazyload&#34;:true,&#34;loop&#34;:true,&#34;useJwplayerDeliveryAPI&#34;:false,&#34;useJwplayerVideoThumbnail&#34;:true}" class="relative overflow-hidden bg-transparent lazytrigger rounded">
