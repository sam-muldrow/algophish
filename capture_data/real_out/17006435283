<script type="text/javascript">
$(function(){
	var keyword;
	var jpnthost      = location.hostname;
	var $searchBox    = $('.searchArea2 .text_layout').children('input[type=text]');
	var $searchButton = $('.searchArea2 .btn_layout').children('a');
	$searchButton.attr('onclick','').on('click',function(){
		keyword = $searchBox.val();
		  csvq();
		   	set3code = keyword;
	if( set3code !=null){
		 set3code= set3code.replace(/[`-y-O-X]/g, function(s) {
    return String.fromCharCode(s.charCodeAt(0) - 65248);
   });
		 set3code= set3code.replace(/@/g," ");
		 set3code= set3code.replace(/|/g,"-");
		 set3code= set3code.replace(/[/g,"-");

