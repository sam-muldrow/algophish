<script type="text/javascript">
$(window).load(function(){
for(var i=0; i<$('.RecommendProducts').length; i++){
	if($('.RecommendProducts .ProductName').eq(i).text().indexOf('AY-N22TD')>-1){
		$('#Deals'+i).empty();
	}
}
$('.ShoppingTop .ShoppingBlock .Deals').css('display','inline-block');
});
</script>
