<!--jquery.dateshow.js-->
<script type="text/javascript">
(function( $ ){
$.fn.dateShow = function(options) {
var date_now = new Date();
var settings = $.extend({
date_start : date_now,
date_end : date_now,
fadein : false,
fade_speed : 1000
}, options);
if($(this).get(0) === undefined ){
return false;
}
return this.each(function() {
var $this = $(this);
var d_start = new Date(settings.date_start);
var d_end = new Date(settings.date_end);
if(d_start <= date_now  && date_now <= d_end){
if( settings.fadein === true ){
$this.fadeIn(settings.fade_speed);
}else{
$this.show();
}}
});
};
})( jQuery );
</script>
