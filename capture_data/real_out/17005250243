<script async>
    window.addEventListener('DOMContentLoaded', function() {
        const buttons = document.querySelectorAll('.common-header--outer .btn');
        buttons.forEach((btn) => {

            btn.addEventListener("click", () => {
                const isMobile = navigator.userAgent.toLowerCase().match(/mobile/i);
                const linkText = btn.getAttribute('linkText');
                if (linkText) {
                    if (isMobile) {
                        iwa('trackEvent', 'nawigacja', 'pasek_gorny', 'klik', 'hamburger', 'kat_' + linkText)
                    } else {
                        iwa('trackEvent', 'nawigacja', 'pasek_gorny', 'klik', 'pasek_gorny', 'kat_' + linkText)
                    }
                }
            });
        });
    });
</script>
