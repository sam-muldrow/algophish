<script type="text/javascript" id="asyncjs">$script('https://cdn.staticneo.com/neoassets/assets/neo-swish.1504224225.js','asyncjs-neo-swish');$script('https://cdn.staticneo.com/neoassets/assets/frontpage.1698958011.js','asyncjs-frontpage');</script>
<script type="text/javascript">
$(function() {
	if ($('.codesnippet').length>0) {
		var ls=document.createElement("link");
		ls.rel = "stylesheet";
		ls.type = "text/css";
		ls.href = 'https://cdn.staticneo.com/neoassets/js/highlightjs/docco.css';//new default light theme Jan 31 2018 w/ very faint grey bg
		
		document.getElementsByTagName("head")[0].appendChild(ls);

		$script('https://cdnjs.cloudflare.com/ajax/libs/highlight.js/9.12.0/highlight.min.js', 'highlightjs');
		$script.ready('highlightjs', function() {
			hljs.tabReplace = '    ';
			hljs.initHighlightingOnLoad();
		});
	}
});
</script>


<script type="text/javascript">
	var lazyload_check_timeout = null, page_last_scrolled_ts = null, in_view_load_timeout = null;
	const lazyloadAdUnits = [];
	var distance = Math.floor($(window).height());
	var adsLoaded = [];
	var refresh_ads_timeout = [],  refresh_ads_counter = [], refresh_ad_at_sec = 120;
	const refresh_ads_elmId = ['div-gpt-ad-300x250-right-btf'];
	var refresh_paused_by_inactivity, ads_refreshed_counter = [];
	refresh_ads_elmId.forEach((elm) => {
		ads_refreshed_counter[elm] = 0;
	});
	var lazyload_screen_fold = 3;

	function locate_lazyload_ads() {
		let lazyload_ads = [];
		for (let i = 0; i < lazyloadAdUnits.length; i++) {
			let val = lazyloadAdUnits[i];
			if (typeof val !== 'object') break;

			let $adUnit = $(val.el).last();
			if ($adUnit.length) {
				let ad_offset = $adUnit.offset(), ad_height = $adUnit.height();
				ad_offset.bottom = ad_offset.top + ad_height;
				lazyload_ads[val.name] = ad_offset;
				lazyloadAdUnits[i].offset = ad_offset;
			} else {
				lazyloadAdUnits[i].offset = { };
			}
		}
	}

	function render_lazyload_ads(on_page_load) {
		let lazy_adUnits = [], lazy_apstagSlots = [], slots = [];
		for (let i = 0; i < lazyloadAdUnits.length; i++) {
			let val = lazyloadAdUnits[i];
			if (typeof val !== 'object') { break; }
			let slotID = val.apstagSlot.slotID;
			if ($.inArray(val.name, adsLoaded) > -1 && !($.inArray(slotID, refresh_ads_elmId)!=-1 && refresh_ads_counter[slotID] >= refresh_ad_at_sec)) {
				continue;
			}
			let scrollTop = $(window).scrollTop(), scrollBottom = scrollTop + $(window).height();

			let ad_offset = val.offset;

			if (refresh_ads_counter[slotID] >= refresh_ad_at_sec) {
				console.debug("%c ***** Refreshing "+slotID, 'background: #558C03; color: #fff');

				let adVal = find_ad_details(slotID);
				lazy_apstagSlots.push(adVal[0]);
				lazy_adUnits.push(adVal[1]);
				slots.push(adVal[2]);

				refresh_ads_counter[slotID] = 0;
				ads_refreshed_counter[slotID]++;
			} else if (scrollBottom > (ad_offset.top - distance*lazyload_screen_fold) || (lazy_adUnits.length && scrollBottom > (ad_offset.top - distance*2))) {
				console.debug('%c ***** queued lazyloaded ad "'+val.name+'" ('+ad_offset.top+' / '+lazyload_screen_fold+' fold) for display ***** at '+scrollBottom, 'background: #ff0000; color: #fff');
				adsLoaded.push(val.name);
				lazy_apstagSlots.push(val.apstagSlot);
				lazy_adUnits.push(val.adUnit);
				slots.push(lazyloadSlots[val.name]);
			}
		}
		if (lazy_adUnits.length) {
			pbjsneo.que.push(function () {
				console.debug('%c ***** calling lazyloaded ads for display *****', 'background: #ff0000; color: #fff');
				fetchHeaderBids(lazy_apstagSlots, lazy_adUnits, slots);
			});
		}
		lazyload_check_timeout = null;
	}

	request_lazyload_bidding = function(on_page_load) {
		if (lazyload_check_timeout != null) {
			return;
		}
		let delay = (headerBiddingStarted ? 1000 : 1);
		lazyload_check_timeout = setTimeout(function() {
			render_lazyload_ads(on_page_load);
		}, delay);
	}

	$(window).data('scrollTop', $(window).scrollTop()).on('scroll', function (e) {
		page_last_scrolled_ts = Date.now();
		if (typeof lazyloadAdUnits[0].offset == 'undefined') {
			locate_lazyload_ads();
		}
		let scrollTop = $(window).scrollTop(), prev_scrollTop = $(window).data('scrollTop');
		if (in_view_load_timeout != null || (Math.abs(scrollTop - prev_scrollTop) < 100)) return;
		request_lazyload_bidding();
		if (refresh_ads_elmId.length && refresh_paused_by_inactivity) {
			let ads_in_view = find_refresh_ads_in_view();
			for (let i = 0; i < ads_in_view.length; i++) {
				let slotId = ads_in_view[i];
				console.debug("***** " + slotId + " - Resume timeout from scroll");
				increment_lazyload_refresh(slotId);
			}
			refresh_paused_by_inactivity = null;
		}
		$(window).data('scrollTop', scrollTop);
	}).on("keyup", function (e) {
		if (e.which == 33 || e.which == 34 || e.which == 35 || e.which == 36) { // page up/down, Home/End
			request_lazyload_bidding();
		}
	});
	if ($(window).scrollTop() > 0) {
		if (typeof lazyloadAdUnits[0].offset == 'undefined') {
			locate_lazyload_ads();
		}
		for (let i = 0; i < lazyloadAdUnits.length; i++) {
			let val = lazyloadAdUnits[i];
			if (typeof val !== 'object') break;
			let $adEl = $(val.el).last();
			if ($adEl.length && ($(window).scrollTop() + $(window).height()) > (val.offset.top - distance)) {
				in_view_load_timeout = setTimeout(function () {
					googletag.cmd.push(function () {
						request_lazyload_bidding(1);
					});
					in_view_load_timeout = null;
				}, 2000);
				break;
			}
		}
	}

	find_refresh_ads_in_view = function() {
		let ads_in_view = [];
		for (let i = 0; i < refresh_ads_elmId.length; i++) {
			let slotId = refresh_ads_elmId[i];

			let scrollTop = $(window).scrollTop(), scrollBottom = scrollTop + $(window).height();

			let $adUnit = $('#' + slotId).last();
			if ($adUnit.length) {
				let ad_offset = $adUnit.offset(), ad_height = $adUnit.height();
				ad_offset.bottom = ad_offset.top + Math.floor(ad_height / 2);

				if (scrollBottom > ad_offset.bottom) {
					ads_in_view.push(slotId);
				}
			}
		}
		return ads_in_view;
	}
	find_ad_details = function(slotId) {
		let apstagSlot, adUnit, adSlot;
		for (let i = 0; i < apstagSlots.length; i++) {
			if (apstagSlots[i].slotID == slotId) {
				apstagSlot = apstagSlots[i];
				for (let j = 0; j < adUnits.length; j++) {
					if (adUnits[j].code == apstagSlot.slotName) {
						adUnit = adUnits[j];
						break;
					}
				}
				for (let k = 0; k < neoseekerSlots.length; k++) {
					if (neoseekerSlots[k].getSlotElementId() == slotId) {
						adSlot = neoseekerSlots[k];
						break;
					}
				}
				break;
			}
		}
		if ((!apstagSlot || !adUnit || !adSlot) && lazyloadAdUnits.length) {
			for (let i = 0; i < lazyloadAdUnits.length; i++) {
				if (lazyloadAdUnits[i].apstagSlot.slotID == slotId) {
					apstagSlot = lazyloadAdUnits[i].apstagSlot;
					adUnit = lazyloadAdUnits[i].adUnit;
					adSlot = lazyloadSlots[lazyloadAdUnits[i].name];
					break;
				}
			}
		}
		return [apstagSlot, adUnit, adSlot];
	}

	increment_lazyload_refresh = function(slotId, force_run) {
		if (ads_refreshed_counter[slotId] >= 4) {
			console.debug("***** Slot "+slotId+" has refreshed 4x - skipped");
			pause_refresh(slotId);
			return;
		}
		let last_scrolled = Date.now() - page_last_scrolled_ts;
		if (!force_run && Math.floor(last_scrolled / 1000) > (60 * 2)) { // 2 minutes
			console.debug("***** Page has been inactive for > 2 minutes - skipped");
			refresh_paused_by_inactivity = 1;
			pause_refresh(slotId);
			return;
		}
		refresh_ads_counter[slotId]++;
		if (refresh_ads_counter[slotId] == 5 || refresh_ads_counter[slotId] % 10 == 0) {
			console.debug(slotId, " Counter at " + refresh_ads_counter[slotId]);
		}
		pause_refresh(slotId);
		if (refresh_ads_counter[slotId] >= refresh_ad_at_sec) {
			// console.debug("***** Refreshing ", slotId);
			//
			// let adVal = find_ad_details(slotId);
			// fetchHeaderBids([ adVal[0] ], [ adVal[1] ], [ adVal[2] ]);
			//
			// refresh_ads_counter[slotId] = 0;
			// ads_refreshed_counter[slotId]++;
		} else {
			refresh_ads_timeout[slotId] = setTimeout(function() {
				increment_lazyload_refresh(slotId);
			}, 1000);
		}
	}
	pause_refresh = function(slotId) {
		clearTimeout(refresh_ads_timeout[slotId]);
		refresh_ads_timeout[slotId] = 0;
	}

	googletag.cmd.push(function(){
		googletag.pubads().addEventListener("slotVisibilityChanged", (event) => {
			const slot = event.slot;
			const slotId = slot.getSlotElementId();

			if ($.inArray(slotId, refresh_ads_elmId) > -1) {
				console.debug(slotId, " - Visible area: "+event.inViewPercentage+'%');
				if (event.inViewPercentage > 50) {
					if (typeof refresh_ads_counter[slotId] == 'undefined') {
						refresh_ads_counter[slotId] = 1;
					}
					if (!refresh_ads_timeout[slotId]) {
						refresh_ads_timeout[slotId] = setTimeout(function () {
							increment_lazyload_refresh(slotId);
						}, 1000);
					}
				} else {
					console.debug("***** "+slotId+" - Paused timeout");
					pause_refresh(slotId);
				}
			}
		});
		googletag.pubads().addEventListener("slotRenderEnded", (event) => {
			const slot = event.slot;
			const slotId = slot.getSlotElementId();
			if (event.isEmpty && slotId.indexOf('-gpt') > -1 && slotId.indexOf('-oop') == -1) {
				console.debug("%c ***** "+slotId+" IS EMPTY *****", 'background: orange; color: #fff');
				setTimeout(function() {
					let adVal = find_ad_details(slotId);
					fetchHeaderBids([ adVal[0] ], [ adVal[1] ], [ adVal[2] ]);
				}, 10000);
			}
		});
	});
	$(window).blur(function() {
		for (var key in refresh_ads_timeout) {
			if (refresh_ads_timeout.hasOwnProperty(key)) {
				console.debug("***** " + key + " - Paused timeout from blur");
				pause_refresh(key);
			}
		}
	});
	$(window).focus(function() {
		let ads_in_view = find_refresh_ads_in_view();
		for (let i = 0; i < ads_in_view.length; i++) {
			let slotId = ads_in_view[i];
			console.debug("***** " + slotId + " - Resume timeout from focus");
			increment_lazyload_refresh(slotId, 1);
		}
	});
</script>





							<div id="debugfooter" class="desktop-fullwidth"><div id="debugfootercontent" style="width: 980px;" class="konafilter"><div id="public_timer">(0.2551/mc/www1)</div></div></div></div></body></html>