	<script type="text/javascript">
		$(function() {
			var $main_div = $(".column-main", "#maincolumn");
			var $right_div = $(".column-right", "#maincolumn");
			var $right_content = $("#right_content", ".column-right");
			if ($main_div.length && $right_div.length && $right_content.length) {
				if ($main_div.height() > $right_div.height()) {
					$right_div.css('min-height', $main_div.height());
					$right_content.scrollToFixed($right_div, $main_div);
				}
			}
		});
