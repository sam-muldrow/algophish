<script>
{
window.addEventListener("message", receiveMessage, false);
var b3;
var b3i;
var body_margin;
var video_started = false;
function iscrResize() {
	if (typeof b3i !== 'undefined') {
		let w = 1090;
		let h = 613;
		if (isMobile()) {
			w = b3.offsetWidth;
			h = w / 1.777;
		}
		b3i.style.width = w + 'px';
		b3i.style.height = h + 'px';
		b3i.querySelector('iframe').style.height = h + 'px';
	}
}
$(window).on('resize', function() {
	iscrResize();
});
function receiveMessage(event)
{
	if (event.data == 'video ended'){
		document.getElementById('adfox_top').classList.remove("iscr_fixed");
		document.body.style.marginTop = body_margin;
	}
	if (event.data == 'video playing' && !video_started){
		b3 = document.querySelector('.banner_3');
		b3.classList.add("iscr");
		b3i = document.querySelector('.banner_3 > div > div > div');
		iscrResize();
		body_margin = document.body.style.marginTop;
		document.body.style.marginTop = b3.offsetHeight + 'px';
		document.getElementById('adfox_top').classList.add("iscr_fixed");
		video_started = true;
	}
}
}
