<script>
    $("#pollForm").submit(function( event ) {
        var $form = $( this ),
        term = $form.find( "input[name='vote_field']:checked" ).val(),
        url = $form.attr( "action" );
        event.preventDefault();
        $.post( url, { answer: term } ).done(function( data ) {
            $form.parent().empty().append( data );
        });
        return false;
    });
</script>

