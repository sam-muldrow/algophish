<script type="text/javascript">
    $('.dropdown-menu a, .dropdown-menu').click(function(e) {
        e.stopPropagation();
    });
</script>

<script type="text/javascript">
    var userid;
    var username;
    var userguid;
    
        username = null, userguid = null;
        userid = null;
    

    get_notifications(userid).then(function(response) {
        display_notification_count(response);
        $(".hitexpand").one('click', function() {
            THREADLESS.HandlebarsLoader.init({staticUrl:  'https://cdn-media.threadless.com/static_11/'})
            THREADLESS.HandlebarsLoader.getHandlebars().then(function() {
                var notifications_page = 0;
                display_notifications(response);
                attach_load_more_notifications(userid, "https://cdn-media.threadless.com/");
            });
        });

    })
</script>
<script type="text/javascript">
    // Make MEDIA_URL globally available
    window.MEDIA_URL = "https://cdn-media.threadless.com/";
</script>



