<nav class="nls-nav" data-mobile-nav="slide"><header class="_is-inactive group headerPrimary" data-header data-mobile-nav="slide"><a class="nls-bars icon" title="Menu" href="#"  data-mobile-nav="trigger"><i class="fa fa-lg fa-bars" aria-hidden="true"></i><span class="visuallyhidden">Toggle Navigation</span></a><h1><a href="/" title="Shop Threadless Art &amp; Artists"><img src="https://cdn-images.threadless.com/threadless-media/images/logo.png?v=3&amp;d=eyJvcHMiOiBbWyJyZXNpemUiLCBbMjgwXSwge31dXSwgImZvcmNlIjogZmFsc2UsICJvbmx5X21ldGEiOiBmYWxzZX0=" alt="Threadless logo"><span class="visuallyhidden">Black Friday - All Tees $12.50</span></a></h1><div class="search"><form class="site-search" id="search_form" role="search" action="/search"><fieldset><legend class="siteSearch-label visuallyhidden">Search all Threadless products</legend><label for="search" class="siteSearch-label visuallyhidden">Search the Artist Marketplace</label><input aria-label="Search Threadless" type="text" name="q" id="search" class="siteSearch-dropdown search-dropdown" placeholder="Search the Artist Marketplace" autocomplete="off" /><button type="submit"><i class="fa fa-search fa-spin-blue" aria-hidden="true"></i><span class="visuallyhidden">Search Threadless</span></button></fieldset></form></div><div class="activityMod-notifications activity-stream"><a class="icon hitexpand" href="#"><i class="fa fa-lg fa-fw fa-globe" aria-hidden="true"></i><span class="visuallyhidden">View Notifications</span></a><div class="notifications-list"><script type="text/x-handlebars-template" id="notification-entries">
        
{{#each data}}
    <li class="group new_{{this.new}}"><figure><a href="{{this.link_url}}" 
            onClick="ga('send', 'event', 'Site Notifications', 'Header Dropdown', '{{this.notification_type}}');"><img class="user-image lazyload" data-src="{{this.source_image_url}}?&amp;w=75&amp;h=75" alt="Threadless" /><figcaption>
                    {{this.content}}
                </figcaption>
                {{#if this.destination_image_url}}
                <div class="design-image-wrapper">
                    {{giraffe_src this.destination_image_url}}
                </div>
                {{/if}}
            </a></figure></li>
{{/each}}

        </script><script type="text/javascript">

    THREADLESS.HandlebarsLoader.registerHelper('if_eq', function(a, b, block) {
        a = JSON.stringify(a);
        return ( a == b ) ? block.fn(this) : block.inverse(this);
    });

    // The giraffe_src helper function resizes the notification image, otherwise a
    // 1200 x 1200 AS (new product launch) image loads in the feed. Everything after
    // the ? (query string in the image url) is removed so that way a new query
    // string can be added with giraffe image operations that resize and center the
    // image in the notification feed
    THREADLESS.HandlebarsLoader.registerHelper('giraffe_src', function(value) {
        var img_src = JSON.stringify(value).replace(/\"/g, "").replace('https://cdn-images.threadless.com/','').replace('https://images-backend.threadless.com', ''),
        img_src = img_src.replace(/\?.*/,'');
        // Gif images are not loading with the giraffe v3 image processing.
        // Check if image is a gif and if it is, change the image width and height.
        var gif = /\.gif$/i.test(value);
        if (gif) {
            return new Handlebars.SafeString(
                '<img class="design-image lazyload" data-src="'+value+'?&amp;w=75&amp;h=75' + '"alt=""/>'
            )
        }
        else {
            var giraffe_src = "https://cdn-images.threadless.com/img_src?v=3&amp;d=eyJvcHMiOiBbWyJyZXNpemUiLCBbNzUsIDc1XSwge31dLCBbImNhbnZhc19jZW50ZXJlZCIsIFs3NSwgNzVdLCB7ImJhY2tncm91bmQiOiAiIzAwMDAifV1dLCAiZm9yY2UiOiBmYWxzZSwgIm9ubHlfbWV0YSI6IGZhbHNlfQ==".replace(
                'img_src', img_src
            );
            return new Handlebars.SafeString(
                '<img class="design-image lazyload" data-src="'
                + giraffe_src
                +'" alt=""/>'
            );
        }

    });

    THREADLESS.HandlebarsLoader.registerHelper('others', function(audience) {
        if(audience.length == 2) {
            return "and 1 other friend has scored";
        } else if(audience.length > 2) {
            return "and "+audience.length+" other friends have scored";
        } else {
            return "has scored";
        }
    });

</script><span class="lm showmore"></span></div><!-- EO .activity-center --></div><!-- EO Activity Stream --><a class="icon nls-cart" href="/cart/"><i class="fa fa-lg fa-fw fa-shopping-cart" aria-hidden="true"></i><span class="visuallyhidden">Show Shopping Cart</span><span class="nls-bug visuallyhidden"></span></a><a class="nls-shop-cta" data-php="NOT_SHOP_OWNER" href="/artist-shops/signup/art?thref=topnav" title="Open a free online POD (print-on-demand) store">
