<script>
    _$_( function() {
        function removeLoader() {
            document.querySelectorAll(".js-ads-loader").forEach(function(el){
                el.remove();
            });
        }
        setTimeout(removeLoader, 15000);
    });
</script>

<style>#toTop {display: none !important;}</style>
<script>
    _$_( function() {
        let secureSearch = document.querySelector(".secure-search");
        const findPhone = "Look up any phone number to see its owner and identify who's calling or texting you.",
              findAddr = "Find anyone who has ever resided at a specific place, including phone numbers and organizations.",
              findEmail= "Look up any email to view its owner and other contact information.",
              findBuss = "Look up comprehensive data on any business including background check, locations, phones, employees, contact information and who runs the business.",
              findPers = "Search a friend, relative, yourself or anyone you may know";

        if (document.location.hash) {
            Rdf.removeClass(".b-index-search_tab a", "active");
            Rdf.removeClass(".b-index-search-wrap form", "active");
            switch (document.location.hash) {
                case "#findPhoneH":
                    Rdf.addClass("#findPhone", "active");
                    Rdf.addClass("#IdfindPhoneForm", "active");
                    Rdf.focus("#phone1");
                    secureSearch.innerHTML = findPhone;
                    break;
                case "#findAddrH":
                    Rdf.addClass("#findAddr", "active");
                    Rdf.addClass("#IdfindAddrForm", "active");
                    Rdf.focus("#street");
                    secureSearch.innerHTML = findAddr;
                    break;
                case "#findEmailH":
                    Rdf.addClass("#findEmail", "active");
                    Rdf.addClass("#IdfindEmailForm", "active");
                    Rdf.focus("#email");
                    secureSearch.innerHTML = findEmail;
                    break;
                case "#findBussH":
                    Rdf.addClass("#findBuss", "active");
                    Rdf.addClass("#IdfindBussForm", "active");
                    if (document.getElementById("bname").value == "") {
                        Rdf.focus("#bname");
                    }
                    secureSearch.innerHTML = findBuss;
                    break;
                case "#findPersH":
                default:
                    Rdf.addClass("#findPers", "active");
                    Rdf.addClass("#IdfindPersForm", "active");
                    Rdf.focus("#topsearch");
                    secureSearch.innerHTML = findPers;
                    break;
            }
        } else {
            if (document.getElementById("topsearch").value == "") {
                Rdf.focus("#topsearch");
            }
        }


        Rdf.onClick(".b-index-search_tab a", function (event) {
            let e = event.target;
            if (!e.classList.contains("b-index-search_tab_more")) {
                Rdf.removeClass(".b-index-search_tab a", "active");
                Rdf.removeClass(".b-index-search-wrap form", "active");
                Rdf.addClass([e], "active");
                Rdf.addClass("#Id" + e.getAttribute("id") + "Form", "active");
                let el = document.querySelector("#Id" + e.getAttribute("id") + "Form input");
                if (el) {
                    Rdf.focus([el]);
                }
                switch (e.getAttribute("id")) {
                    case "findPhone":
                        gtag('event', 'Search_Click', {'category':'Main','option':'Phone'});
                        secureSearch.innerHTML = findPhone;
                        break;
                    case "findAddr":
                        gtag('event', 'Search_Click', {'category':'Main','option':'Address'});
                        secureSearch.innerHTML = findAddr;
                        break;
                    case "findEmail":
                        gtag('event', 'Search_Click', {'category':'Main','option':'Email'});
                        secureSearch.innerHTML = findEmail;
                        break;
                    case "findBuss":
                        gtag('event', 'Search_Click', {'category':'Main','option':'Buisiness'});
                        secureSearch.innerHTML = findBuss;
                        break;
                    case "findPers":
                        gtag('event', 'Search_Click', {'category':'Main','option':'Name'});
                        secureSearch.innerHTML = findPers;
                        break;
                    default:
                        secureSearch.innerHTML = findPers;
                        break;
                }
            }
        });

        let el = document.querySelector('#topsearch'),
            el_search = document.querySelector('.index-wrapper');
        // Rdf.focus_flash(el);
            Rdf.focus('#topsearch');
        Rdf.onClick(".js-bc-now", function(){
            console.log('111');
            let tergetSearch = Rdf.offset(el_search);
            // Rdf.focus_flash(el);
            Rdf.focus('#topsearch');
            //     Rdf.focus(el);
            if (window.innerWidth < 768) {
                Rdf.scrollTo(window, tergetSearch.top - 50);
            } else {
                Rdf.scrollTo(window, tergetSearch.top);
            }
        });

        // accordion
        let faq = document.querySelector(".faq-item");
        faq.classList.add("active");
        for (let sibling of faq.parentNode.children) {
            if (sibling !== faq) {
                for (let sp of sibling.querySelectorAll(".faq-body")) {
                    Rdf.hide([sp]);
                }
            }
        }

        Rdf.onClick(".faq-item .faq-subtitle", function (event) {
            let e = event.target;
            let faq = e.parentNode;
            for (let sibling of faq.parentNode.querySelectorAll(".faq-item")) {
                if (sibling == faq) {
                    Rdf.addClass([sibling], "active");
                    Rdf.show(sibling.querySelectorAll(".faq-body"));
                } else {
                    Rdf.removeClass([sibling], "active");
                    Rdf.hide(sibling.querySelectorAll(".faq-body"));
                }
            }
        });

        document.querySelectorAll(".js-more-itms").forEach(function(el){
            Rdf.onClick(el.querySelectorAll("#moreItems"), function (event) {
                Rdf.removeClass(el.querySelectorAll(".item-link"), "hide_element");
                el.querySelectorAll("#moreItems").forEach(function(el){ el.remove() })
            });
        });
    });
</script>



</body>
