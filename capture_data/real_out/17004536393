<script>
    function changeTheme(theme) {
        let themeColor = localStorage.getItem("theme")
        console.log(themeColor)
        if (themeColor == "dark" || theme === "light") {
            lightMode()
            localStorage.setItem("theme", "light");
            const pianoWindow = document.querySelector('.piano-container iframe');
            if (pianoWindow) {   pianoWindow.contentWindow.postMessage({ darkMode: { isOn: false } }, '*'); }
        } else {
            darkMode()
            localStorage.setItem("theme", "dark");
            const pianoWindow = document.querySelector('.piano-container iframe');
            if (pianoWindow) { pianoWindow.contentWindow.postMessage({ darkMode: { isOn: true } }, '*');    }
        }
        activeTickThemeFunction(theme, themeColor)
    }
    let theme = localStorage.getItem("theme");
    function activeTickThemeFunction(theme, themeColor) {
        if (themeColor == "dark" || theme === "light") {
            document.getElementById(`mpp-menu-settings-theme-light`).innerHTML = `<svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" viewBox="0 0 10 10" fill="none"><path d="M1 5.8L4.2 8.36L9 1.64" stroke="#4990E0" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" /></svg>`
            document.getElementById(`mpp-menu-settings-theme-dark`).innerHTML = ""
        } else {
            document.getElementById(`mpp-menu-settings-theme-dark`).innerHTML = `<svg xmlns="http://www.w3.org/2000/svg" width="10" height="10" viewBox="0 0 10 10" fill="none"><path d="M1 5.8L4.2 8.36L9 1.64" stroke="#4990E0" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" /></svg>`
            document.getElementById(`mpp-menu-settings-theme-light`).innerHTML = ""
        }
    }
    const a = window.matchMedia("(prefers-color-scheme: dark)");
    var theme_status = false;
    console.log(theme, a)
    if (a.matches) {
        if (theme == "dark") {
            darkMode();
            theme_status = true;
            activeTickThemeFunction('dark')
        } else if (theme == "light") {
            lightMode();
            theme_status = false;
            activeTickThemeFunction('light')
        } else if (theme == null) {
            darkMode();
            theme_status = true;
            activeTickThemeFunction('dark')
        }
    } else {
        if (theme == "dark") {
            darkMode();
            theme_status = true;
            activeTickThemeFunction('dark')
        } else if (theme == "light") {
            lightMode();
            theme_status = false;
            activeTickThemeFunction('light')

        } else if (theme == null) {
            lightMode();
            theme_status = false;
            activeTickThemeFunction('light')
        }
    }
    function darkMode() {
        $("body").addClass("dark-mode");
        $("body").css("background-color", "#121212");
        // $('.mpp-mobile-logo').attr('src','https://sv1.mathrubhumi.com/stat/prototype/assets/logo-m-w.svg');
        // $('.mpp-header-logo-desktop').attr('src','https://sv1.mathrubhumi.com/stat/prototype/assets/images/logo-desk-svg.png');
        $(".mpp-header-trending-container").css(
                "background",
                "none"
        );
        $(".mpp-element-card").css("background-color", "var(--section-white)");
        $("h1").css("color", "var(--section-heading)");
        $("h2").css("color", "var(--section-heading)");
        $("h3").css("color", "var(--section-small)");
        $("h4").css("color", "var(--section-heading)");
        $("h5").css("color", "var(--section-heading)");
        $("h6").css("color", "var(--section-heading)");
        $(".mpp-header-network-list-group li a").css(
                "color",
                "var(--section-tag)"
        );
        $("button").css("color", "var(--section-heading)");
        $("a.btn").css("color", "var(--section-heading)");
        $(".mpp-footer-container").css(
                "background-color",
                "var(--section-white)"
        );
        $(".offcanvas").css("background-color", "var(--mobile-bg)");
        $(".mpp-header-section-top").css(
                "background-color",
                "var(--section-white)"
        );
        $(".mpp-main-element-ahort-breaking-heading h1").css("color","var(--section-label-background)")
        $(".mpp-story-column-small h3").css("color", "var(--section-small)")
    }
    function lightMode() {
        $("body").removeClass("dark-mode");
        // $('.mpp-mobile-logo').attr('src','https://sv1.mathrubhumi.com/stat/prototype/assets/logo-m-b.svg');
        // $('.mpp-header-logo-desktop').attr('src','https://sv1.mathrubhumi.com/stat/prototype/assets/images/logo-desk.svg')
        $("body").css("background-color", "white");
        $(".mpp-header-trending-container").css(
                "background-color",
                "var(--mobile-bg)"
        );
        $(".mpp-element-card").css("background-color", "var(--section-white)");
        $(".mpp-footer-container").css("background-color", "var(--mobile-bg)");
        $(".offcanvas").css("background-color", "var(--section-white)");
        $(".mpp-header-section-top").css(
                "background-color",
                "var(--mobile-bg)"
        );
        $(".mpp-main-element-ahort-breaking-heading h1").css("color","var(--section-label-background)");
        $(".mpp-story-column-small h3").css("color", "var(--section-small)");
    }
</script>
