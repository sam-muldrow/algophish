                          for (i = 0; i < videoClasses.length; i++) {
                              var dataVideo
                              //console.log(document.querySelectorAll('[data-video-id="videoKZXs8ja3177"]').offsetTop);
                              // if(videoClasses[i].getAttribute("data-video-id") == "videoKZXs8ja31771"){
