<script>
    $(document).ready(function (){
        $(window).scroll(function(){
            if ($(this).scrollTop() > 300) {
                $('.backtotop1').fadeIn();
            } else {
                $('.backtotop1').fadeOut();
            }
        });
        $('.backtotop1').click(function(){
            $("html, body").animate({ scrollTop: 0 }, 600);
        });
    });
</script>
