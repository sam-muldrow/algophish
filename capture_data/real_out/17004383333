	<script>
	jQuery(document).ready(function () {
		jQuery(document).on('click', '.downlpop', function(e) {

			e.preventDefault();
	    	// Blink Form
		    jQuery('html,body').animate({ scrollTop: jQuery(".steps_content").offset().top}, 0);
		    jQuery(".steps_content").addClass("intro");
			setTimeout(function () {
				jQuery('.steps_content').removeClass('intro');
			}, 1500);

			return false;

    	});
    });
	</script>

