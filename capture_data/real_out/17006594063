<script>
jQuery("#globalFooter .draftBox form").submit(function(){
		var form = this;
		function getToken(xhr) {
		  var json;
		  try { 
		  	json = xhr.getResponseHeader('X-Json'); 
		  }catch(e) {}
		  if (json) {
		    var data = eval('(' + json + ')'); // or JSON.parse or whatever you like
		    return data['token'];
		  }
		  return null;
		}

		jQuery.ajax({
			url: "/ajax/token.jsp",
			type: "GET",
			async: "false",
			beforeSend : function( xhr ){
				xhr.setRequestHeader("X-Requested-With", "jquery");
			},
			complete: function(xhr){
				var token = getToken(xhr);
				try{
				jQuery('<input>').attr({'type':'hidden','name': 'request_token','value': token}).appendTo(form);
				form.submit();
				}catch(e){alert(e);}
			},
			error: function(httpObj){
			},
			}
		);
	return false;
});
</script>




