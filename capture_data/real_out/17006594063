var _gaq = _gaq || [];

function track(trackID, customUrl, ns) {

	var pref = "";
	if(ns && ns.length > 0){
		pref = ns + ".";
	}
	_gaq.push([pref+'_setAccount', trackID]);
	if(status != "")
		_gaq.push([pref+'_trackPageview', "/" + status + ".html?page="+document.location.pathname+document.location.search]);
	else if(customUrl.length > 0)
		_gaq.push([pref+'_trackPageview', customUrl]);
	else
		_gaq.push([pref+'_trackPageview', "/"]);
}

