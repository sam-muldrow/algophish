<script type="text/javascript">
function sectionLogin(){
	$("login").hide();
	new Effect.Appear($("globalLogin"));
	new Ajax.Request(
	"/ajax/common/cookieUser.jsp", 
	{
		method: "get",
		asynchronous:false,
		parameters: "a="+Math.round(Math.random() * 100000000),
		onComplete:function(httpObj, json){
			if(json["success"] == "true"){
				var form = $("header-login");
				form.username.value = httpObj.responseText.replace(/^\s+|\s+$/g, '');
				form.permanentLogin.checked = json["permanentLogin"] == "true";
			}
		},
		onFailure:function(httpObj){
		}
	}
);
	
}
</script>
