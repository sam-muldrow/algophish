<div class="c-ad-container -T3N_D_Sidebar-3"><div id="T3N_D_Sidebar-3-label" class="c-ad-label">Anzeige</div><div id="T3N_D_Sidebar-3" class="c-ad -T3N_D_Sidebar-3"><div class="c-ad-placeholder">Anzeige</div></div></div><div class="c-card c-events-sidebar u-gap-bottom"><div class="c-card__header c-events-sidebar__header tg-event-box" style="background-image: url(https://assets.t3n.de/t3n-de/neos/2f3edaef5ccfcbb51d9b8c67e723edc50f5638c4/events_teaser-image.png?variant=custom&amp;width=600&amp;height=210&amp;fit=smart)"><h3 class="c-events-sidebar__headline">Events 2023</h3></div><div class="c-card__content c-events-sidebar__content"><div class="js-loading-info">Lade Events...</div><ul class="o-list js-list c-events-sidebar__list"></ul><a href="events/" class="c-button u-size-full">Alle Events ansehen</a></div><template style="display: none;"><li class="o-list__item c-events-sidebar__item"><strong data-value="title"></strong><br /><span data-value="date"></span>, <span data-value="location"></span></li></template><script>            const wrapper = document.querySelector(".c-events-sidebar");            const listElement = wrapper.querySelector(".js-list");            const itemTemplate = wrapper.querySelector("template");            const loadingElement = wrapper.querySelector(".js-loading-info");                        const formatDate = function(date) {                return ("0" + date.getDate()).slice(-2) + "."                    + ("0" + (date.getMonth()+1)).slice(-2) + "."                    + date.getFullYear();            };                        const removeElementFromDom = function() {                wrapper.remove();            };            const createListItem = function(element) {                const newElement = itemTemplate.cloneNode(true).content;                const titleElement = newElement.querySelector("[data-value=title]");                titleElement.innerHTML = element.title;                const dateElement = newElement.querySelector("[data-value=date]");                const startsAt = new Date(element.startsAt);                let endsAt = null;                let dateString = formatDate(startsAt);                                if (element.endsAt) {                    endsAt = new Date(element.endsAt);                    dateString = dateString + " - " + formatDate(endsAt);                }                dateElement.innerHTML = dateString;                const locationElement = newElement.querySelector("[data-value=location]");                locationElement.innerHTML = element.location;                listElement.appendChild(newElement);            };            const fetchEvents = function() {                fetch("https://strapi.t3n.de/graphql", {                    method: "POST",                    headers: {                        "Content-Type": "application/json"                    },                    body: JSON.stringify({                        query: `
        query EventRecommendations($filters: EventRecommendationFiltersInput) {
            eventRecommendations(filters: $filters, pagination: {limit: 5}, sort: "startsAt:asc") {
                data {
                    attributes {
                        title
                        startsAt
                        endsAt
                        location
                    }
                }
            }
        }
    `,                        variables: {                            filters: {                                startsAt: {                                    gte: new Date().toISOString()                                }                            }                        }                    })                }).then(function(response) {                    if (response.ok) {                        return response.json();                    }                    throw new Error("Something went wrong");                }).then(function(data) {                    if (!data ||                        !data.data ||                        !data.data.eventRecommendations ||                        !data.data.eventRecommendations.data ||                        !data.data.eventRecommendations.data.length ||                        itemTemplate === null                    ) {                        removeElementFromDom();                        return;                    }                    loadingElement && loadingElement.remove();                    const eventRecommendations = data.data.eventRecommendations.data.sort(function(a, b) {                        return a.attributes.startsAt > b.attributes.startsAt ? 1 : -1;                    });                    eventRecommendations.forEach(function(element){                        createListItem(element.attributes);                    });                }).catch(function(e) {                    removeElementFromDom();                });            };                    fetchEvents();        </script></div></aside></div></div></div></div><span id="podcast" class="anchor"></span><div class="c-section -grey"><div class="c-section__body -grey"><div class="c-section__content neos-contentcollection">


