<script type="text/javascript">

        $('.slider8-for').slick({ slidesToShow: 1, slidesToScroll: 1, rtl: !0, dots: !1, asNavFor: '.slider8-nav', prevArrow: '#slider8-prev', nextArrow: '#slider8-next', autoplaySpeed: 6000, appendArrows: '.slider8-for' }); $('.slider8-nav').slick({ infinite: !0, autoplaySpeed: 6000, slidesToShow: 7, slidesToScroll: 1, rtl: !0, arrows: !1, dots: !1, autoplay: !0, asNavFor: '.slider8-for', focusOnSelect: !0, responsive: [{ breakpoint: 1199, settings: { slidesToShow: 5, slidesToScroll: 1 } }, { breakpoint: 991, settings: { slidesToShow: 4, slidesToScroll: 1 } }, { breakpoint: 767, settings: { slidesToShow: 3, slidesToScroll: 1 } }, { breakpoint: 575, settings: { slidesToShow: 4, slidesToScroll: 1 } }] });


        $('#topslider').slick({ slidesToShow: 4, slidesToScroll: 1, rtl: !0, dots: !0, autoplaySpeed: 6000, arrows: !1 });

        $(function () {
            $('[data-toggle="tooltip"]').tooltip();
            $("#homelink").addClass("active");
        });

        $('.ResultsVot').click(function (e) {

            e.preventDefault();

            var PollId = $(this).attr("data-PollId");

            $.ajax({
                type: "GET",
                url: '/Polls/GetResults',
                data: { id: PollId },
                contentType: "html/text",
                dataType: "html",
                cache: false,
                success: function (msg) {

                    if (msg.trim() !== "") {
                        $('.ResultPolling').remove();

                        $('.PollingHome').hide();
                        $('.PollingHomeH2').hide();
                        $("#colrecentall").append(msg);
                        $(".BtnVoteRecent").click(function (e) {
                            if ($(this).hasClass("unclickable")) {
                                e.preventDefault();
                            } else {

                                $(this).addClass("unclickable");
                                //Your code continues here
                                //Remember to remove the unclickable class when you want it to run again.


                                e.preventDefault();


                                $('.PollingHome').show();
                                $('.PollingHomeH2').show();
                                $('.ResultPolling').remove();
                                $('.ResultPollingH2').hide();
                            }
                            return false;
                        });

                    }

                }
            });

            return false;
        });


        $('.HomeBlocksList').click(function (e) {

            $('.spinner').show();
            e.preventDefault();
            $(this).removeClass("bg-white");
            $('.HomeBlocksGrid').addClass("bg-white");


            $.ajax({
                type: "GET",
                url: '/Home/GetLatestStoriesforHomePage',

                data: { maximum: 41, lastId: 0 },

                contentType: "html/text",
                dataType: "html",
                cache: false,
                success: function (msg) {

                    if (msg.trim() !== "") {

                        $(".HomeContainerBlocks").html(msg);
                        $('.spinner').hide();


                    }

                }
            });

            return false;
        });


        $('.HomeBlocksGrid').click(function (e) {
            $('.spinner').show();
            e.preventDefault();
            $(this).removeClass("bg-white");
            $('.HomeBlocksList').addClass("bg-white");


            $.ajax({
                type: "GET",
                url: '/Home/GetSectionsAndStoriesforHomePage',



                contentType: "html/text",
                dataType: "html",
                cache: false,
                success: function (msg) {

                    if (msg.trim() !== "") {

                        $(".HomeContainerBlocks").html(msg);
                        $('.spinner').hide();


                    }

                }
            });

            return false;
        });


        function MoreFunction() {


            var lastId = $(".FunctionParm:last").attr("LastId");

            $.ajax({
                type: "GET",
                url: '/Home/aJAXLatestStoriesforHomePage',
                async: false,
                data: { maximum: 20, lastId: lastId },

                contentType: "html/text",
                dataType: "html",
                cache: false,
                success: function (msg) {

                    if (msg.trim() !== "") {


                        $("#ContainerList").append(msg);


                    }

                }
            });
        }

    </script>
