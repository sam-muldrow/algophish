var getNidStatus = function(){
	  if( getCookie("nid_consent") === "ACCEPTED" ){
	    return "accepted";
	  }
	  if( getCookie("nid_supr") ){
	    return "later";
	  }
	  if( getCookie("nid_consent") === "DECLINED" && !getCookie("cookie.nid_supr") ){
	    return "declined";
	  }
	  return "na";
};

