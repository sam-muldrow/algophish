  <script>
    (function() {
      function formatUserId() {
        if (!(window.DN.userInfo && window.DN.userInfo.userId)) return undefined;
        const userId = window.DN.userInfo.userId;
        return userId.replace("splus.", "splus/").replace("credentials.", "");
      }
      function formatUserIdV4() {
        if (!(window.DN.userInfo && window.DN.userInfo.userId)) return undefined;
        const userId = window.DN.userInfo.userId;
        return userId.replace("splus.", "").replace("credentials.", "");
      }

      const userId = formatUserId();
      const userIdV4 = formatUserIdV4();

      window.DN = window.DN || {};
      window.DN.gtmData = {"event":"Pageview","environmentInfo":{"brandName":"DNSE","brandLine":"DN","platformName":"bang","environment":"production","isFailover":false},"userInfo":{"loggedIn":false,"isStudent":false,"fromApp":false},"pageInfo":{"displayEvent":"Page load","pageId":"dn.section.nyheter","page":"/","pageType":"StartPage","pageTitle":"Dagens Nyheter","mainSectionURISegment":"/","mainSectionLabel":"Nyheter","httpStatusCode":200}};
      window.DN.gtmData.userInfo.loggedIn = !!window.DN.hasSession;
      window.DN.gtmData.userInfo.hasSubscription = window.DN.userInfo && window.DN.userInfo.accountLevel !== 'Gratis' || false;
      window.DN.gtmData.userInfo.userId = userId;
      window.DN.gtmData.userInfo.isStudent = window.DN.hasEduToken ? window.DN.hasEduToken : false;
      if (window.DN.gtmData.pageInfo) window.DN.gtmData.pageInfo.browserHasAdblocker = window.DN.abt === false ? "Yes" : (window.DN.abt === true ? "No" : "Unknown");

      if (window.DN.af) {
        window.DN.gtmData.userInfo.hasAdFreeSubscription = true;
      } else {
        window.DN.gtmData.userInfo.hasAdFreeSubscription = window.DN.gtmData.userInfo.loggedIn ? false : undefined;
      }
      window.DN.gtmData.userInfo.fromApp = window.DN.fromApp || false;

      const personalized = JSON.parse(window.localStorage && window.localStorage.getItem("personalized") || "{}");
      const isLoggedIn = !!window.DN.hasSession;
      window.DN.gtmDataV4 = {"event":"page view","environment":{"isProduction":true,"platform":{"name":"bang","version":"f363b7e"},"dataSource":"web","isFailover":false},"org":{"brand":"Dagens Nyheter","brandLine":"Dagens Nyheter"},"content":{"section":["Nyheter"],"id":{"value":"dn.section.nyheter","system":"nav"},"title":"Dagens Nyheter – senaste nyheterna från Sverige och världen","type":"start","trigger":"page load","pageNotFound":false}};
      if (window.DN.fromApp) {
        window.DN.gtmDataV4.environment.dataSource = "app";
        const searchParams = new URL(window.location.href).searchParams;
        const appName = searchParams.get("app_name") || null;
        const appVersion = searchParams.get("app_version") || null;
        window.DN.gtmDataV4.app = {
          id: "se.dn",
          name: appName,
          version: appVersion
        };
      }
      window.DN.gtmDataV4.content.location = document.location.href;
      // if there is no paywall, the user has access
      window.DN.gtmDataV4.content.userHasAccess = document.querySelector(".esi-paywall, .paywall-fallback") === null,
      window.DN.gtmDataV4.user = {
        isLoggedIn,
        id: userIdV4 ? {
          value: userIdV4,
          system: "credentials"
        } : undefined,
        isSubscriber: isLoggedIn ? window.DN?.userInfo?.accountLevel !== "Gratis" || false : undefined,
        subscriptionCodes: window.DN?.userInfo?.accountType ? [window.DN?.userInfo?.accountType] : [],
        segment: personalized?.userSegments,
        hasAdFreeSubscription: isLoggedIn ? window.DN?.userInfo?.hasAnnonsfri || false : undefined,
      };
      window.DN.gtmDataV4.browser = {
        adblocker: window.DN.abt === true ? "not found" : (window.DN.abt === false ? "active" : "searching"),
        cdnCookie: getCookie("weight")
      };

      window.dataLayer = window.dataLayer || [];

      function handleFullpageAdConsent() {
        var enableFullpageAd = window.Didomi.getUserConsentStatus("select_basic_ads", "c:bonniernews");
        if (enableFullpageAd && document.cookie.indexOf("canShowFullpageAd") < 0) {
          const date = new Date();
          date.setTime(date.getTime()+(30*24*60*60*1000)); //30 days
          document.cookie = "canShowFullpageAd=true; expires=" + date.toGMTString() + "; path=/";
        } else if (!enableFullpageAd && document.cookie.indexOf("canShowFullpageAd") >= 0) {
          document.cookie = "canShowFullpageAd=true;path=/;expires=Thu, 01 Jan 1970 00:00:00 GMT"; //delete cookie
        }
      }

      function didomiEventDispatcher() {
        document.dispatchEvent(new CustomEvent("cmp:onConsent", {}));
      }

      function handleNoticeChange() {
        handleFullpageAdConsent();
        var isStartPage = true;
        if (window.DN && window.DN.hasClickedFooterCmpLink || !isStartPage) {
          window.location.reload();
        }
      }

      window.didomiConfig = {
        cookies: { storageSources: { cookies: true, localStorage: false } },
        preferences: { content: { viewAllPartners: "Inställningar per partner" } },
        notice: { enable: true },
        user: { externalConsent: { enabled: true } }
      };
      window.didomiOnReady = window.didomiOnReady || [];
      window.didomiOnReady.push(function (Didomi) {
        if (window.DN.hasEduToken) {
          Didomi.setUserDisagreeToAll();
          window.didomiConfig.notice.enable = false;
          return;
        }

        window.dataLayer.push(window.DN.gtmData, window.DN.gtmDataV4, {"gtm.start": new Date().getTime(), event: "gtm.js"});

        handleFullpageAdConsent();

        window.didomiEventListeners = window.didomiEventListeners || [];
        window.didomiEventListeners.push(
          {event: 'preferences.clicksavechoices',listener: didomiEventDispatcher},
          {event: 'notice.clickagree',listener: didomiEventDispatcher}
        );

        if (navigator.cookieEnabled) {
          window.didomiEventListeners.push(
            {event: 'preferences.clicksavechoices',listener: handleNoticeChange},
            {event: 'notice.clickagree',listener: handleNoticeChange});
        }

        Array.from(document.getElementsByClassName("pumba")).forEach(function(testGroupEl) {
          var test = JSON.parse(decodeURIComponent(testGroupEl.getAttribute("data-test-group")));
          window.dataLayer.push({ event: "AB-test", eventInfo: {experimentName: test.name, experimentVariation: test.group} });
        });


        window.dataLayer.push({
          event: "Google Analytics Event",
          eventInfo: {
            eventCategory: "Other",
            eventAction: "WeightCookie",
            eventLabel: getCookie("weight")
          }
        });

        window.setTimeout(function() {
          window.dataLayer.push({
            event: "Critical Content Loaded" /* Allow GTM to load more non-critical scripts such as Orvesto */
          });
        }, 500);
      });

      function getCookie(key) {
        try {
          const cookies = document.cookie.split(";").reduce(function(acc, part) {
            const [key, val] = part.split("=");
            acc[key.trim()] = val === undefined ? true : val;
            return acc;
          }, {});
          return cookies[key];
        } catch (error) {
          // ignore
        }
        return null;
      }
    })();
  </script>
</body>
</html>
