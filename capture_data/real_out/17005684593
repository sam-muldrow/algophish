function closeLK247BalloonAds()
{ 
	  $("#divBalloonAds").fadeOut("slow");
	  $("#toTop").css("z-index","1000000"); 
}

function minLK247BalloonAds()
{ 
	  document.getElementById("divBalloonAds").style.clip = "rect(190px, 260px, 230px, 0px)";
}

