          var loadMoreBtn = document.querySelector('#widget-1ef6e52a-b90d-5b60-a2f0-c5b9c8377e93 .js-btn-load-more');
          if (!loadMoreBtn) {
            window.TOOFAB.dispatch(window.TOOFAB.actions.renderInfiniteTetris());
            return;
          }

          loadMoreBtn.addEventListener('click', function(event) {
            event.preventDefault();
            if (clicked) {
              event.stopPropagation();
              return;
            }

            clicked = true;
            loadMoreBtn.innerText = 'Loading...';
            loadMoreBtn.classList.remove('js-btn-load-more');
            document.getElementById('1ef6e52a-b90d-5b60-a2f0-c5b9c8377e93-load-more-tetris').classList.add('js-html-import-lazy');
            window.TOOFAB.utils
              .htmlImport('/_/tetris-widget/news/main.html?page=2&cursor=eyJvcmRlcl9kYXRlX2JlZm9yZSI6MTcwMDM1MzM5Nn0', '1ef6e52a-b90d-5b60-a2f0-c5b9c8377e93-load-more-tetris', true)
              .then(function() { loadMoreBtn.remove(); });
          });
        });
      </script>
    
          </section><!-- tetris -->
    
  
