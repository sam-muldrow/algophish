					location.href = url;
				}
			}
		});

    });

    $(".recommend_tag").click(function(e){
		e.preventDefault();

		var tag = $.trim($("#tag").val());

		is_recommend = $(this).hasClass('recommend_tag')
		if (is_recommend)
		{
			tag = $(this).text().replace('#', '');
		}

		$.ajax({
			data: { 'tag': tag },
			url: "/member/tag/add",
			dataType: 'json',
			success: function (res) {
				if (res.result)
				{
					location.href="/#tag";
                    location.reload();
				}
			}
		});
