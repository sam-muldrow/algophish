<script>
//상품찜하기
function do_zzim(obj)
{
	
	location.href='/mypage/login';
	return;
	

    var goods_id=$(obj).data('id');

	if ($(obj).hasClass('off')) {
		$(obj).addClass('on').removeClass('off');

        url =  "/zzim/add/"+goods_id+'/';
        ga('send', 'event', {
            eventCategory: 'Zzim',
            eventAction: 'add',
            eventLabel: goods_id
        });
        if($('#zzim_count').length > 0) $('#zzim_count').text(parseInt($('#zzim_count').text())+1);
	} else {
		$(obj).addClass('off').removeClass('on');

        url =  "/zzim/del/"+goods_id+'/';
        ga('send', 'event', {
            eventCategory: 'Zzim',
            eventAction: 'remove',
            eventLabel: goods_id
        });
        if($('#zzim_count').length > 0) $('#zzim_count').text(parseInt($('#zzim_count').text())-1);
	}

    $.ajax({
		data: {},
		url: url,
		dataType: 'json',
		success: function (res) {
			if (!res.result)
			{
				alert('찜하기에 실패하였습니다.');
			}

		}

	});
}

//쇼핑몰 찜하기
function DoShopZzim(obj)
{
	//미로그인시 로그인페이지로 이동
	
		location.href='/mypage/login';
		return;
	

	_this = $(obj);
	var shop_id = _this.attr('data-id');
	var url = "/zzim/shop/add/"+shop_id;

	//찜해제
	if (_this.hasClass('on'))
	{
		var url = "/zzim/shop/del/"+shop_id;
	}

	$.ajax({
		data: {},
		url: url,
		dataType: 'json',
		success: function (res){
			if (res.result)
			{
				if (_this.hasClass('on'))
				{
					_this.removeClass('on');
				}
				else
				{
					_this.addClass('on');
				}
			}
		}
	});
}

//쇼핑몰찜
$(".good.shop a").on('click', function(e) {
	e.preventDefault();
    DoShopZzim(this);
});

//문의하기
$("#btn_send_qna").click(function(e){
	e.preventDefault();

	var email = $("#qna_email").val();
	var content = $("#qna_content").val();
	//var term_agree("#term_agree");

	if ($.trim(email) == "")
	{
		alert("이메일을 입력하세요.");
		$("#qna_email").focus();
		return false;
	}
	if (!ck_email(email))
	{
		alert("이메일을 정확하게 입력하세요.");
		$("#qna_email").focus();
		return false;
	}
	if($.trim(content) == "")
	{
		alert("내용을 입력하세요.");
		$("#qna_content").focus();
		return false;
	}
	if (!$("#term_agree").is(":checked"))
	{
		alert("개인정보 수집에 동의해 주세요.");
		return false;
	}

	$.ajax({
		data: {'email':email, 'content':content},
		url: '/qna/send',
		dataType: 'json',
		success: function (res) {
			if (res.result) {
				alert(res.msg);
				$("#layer-inquiry .btn-close").click();
				window.location.reload();
			}

		}
	});


});

// 체크박스
$('.chk-lst input').iCheck({
	checkboxClass: 'icheckbox_square-orange',
	increaseArea: '0%'
});
</script>



