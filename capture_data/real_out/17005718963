<script>

    // this is or the bak to top button
    $(function() {
        $('#back-to-top').fadeOut();
    
        $('#back-to-top').click(function() {
        	$('body,html').animate({scrollTop:0},800);
        });	
    });

    // This is for the sticky header
    $window = $(window);
    
    if (!z.device.isMobile) {
        $window.scroll(function() {
        $scroll_position = $window.scrollTop();
        if ($scroll_position > 300) { // if body is scrolled down by 300 pixels
            $('.your-header').addClass('sticky');
        
            // to get rid of jerk
            header_height = $('.your-header').innerHeight();
            $('body').css('padding-top' , header_height);
        } else {
            $('body').css('padding-top' , '0');
            $('.your-header').removeClass('sticky');
        }
        }); 
    } else {
        //$(".header").hide();
        //$(".your-header").addClass("sticky");       
    }
</script>
	

<script>

var real = {

    construct: function()
    {
        $( document ).ready(function() {
            	
                real.buildPageRefresh();
            	
            real.buildSlPress();
        });    
    },
    
    setContentRefresh: function(everyms)
    {
        $( document ).ready(function() {
            setTimeout("location.reload(true);", everyms);
        });        
    },
    
    buildPageRefresh: function() 
    {
        var Url=window.location.href;
        var UrlPath=window.location.pathname;
        if (UrlPath=='/' || UrlPath=="/real.gr.v1/" )
        {
            this.setContentRefresh(120000);
            //console.log("refresh " + 120000)
        } else if ( (Url.includes("/arthro/") || Url.includes("subid=2") ) && !$(".audiojs")[0] && !$("iframe[src*=you]")[0] && !$("iframe[src*=glom]")[0] && !$("iframe[src*=daily]")[0]  && !$("iframe[src*=grnet]")[0]  && !$("iframe[src*]")[0]    ) {
            this.setContentRefresh(120000*2); 
            //console.log("refresh other " + 120000*2);
        }
    },
    
    buildSlPress: function()
    {        
        if (document.getElementById("slpressContent")!=null) {   		

            $.getJSON( "https://www.real.gr/slpress/data.rpx", function( data ) {

				var html="";

            	$.each(data.posts, function (key, post) {

	                var title=post.title;
	                var url=post.url;
	                var author=post.author.name;
	                  
	                var item='<h3><a href="%URL%" target="_blank">%TITLE%</a></h3><h4><a href="%URL%" target="_blank">%AUTHOR%</a></h4>%HR%';
	                
	                item=item.replace("%URL%", url);
	                item=item.replace("%URL%", url);
	                item=item.replace("%TITLE%", title);
	                item=item.replace("%AUTHOR%", author);

	                if (key<data.posts.length-1) {
	                	item=item.replace("%HR%", "<hr />");	                		
	                } else {
	                	item=item.replace("%HR%", "");
	                }

	                html+=item;
              	})

	            document.getElementById("slpressContent").innerHTML=html;
            });
        }
    }
    
}

real.construct();


</script><style>

