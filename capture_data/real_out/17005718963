    var zentech = zentech || {};
    var z=zentech;
    
    zentech.cmd = zentech.cmd || [];

    zentech.device = {
        class:null ,
        isMobile:null ,
        
        construct:function() {
            if (navigator.userAgent.match(/EdgeHTML/i) ||
                navigator.userAgent.match(/ZuneWP/i) || 
                navigator.userAgent.match(/HTC_/i) || 
                navigator.userAgent.match(/iPhone/i) || 
                navigator.userAgent.match(/iPod/i) || 
              ( navigator.userAgent.match(/Android/i) && navigator.userAgent.match(/Mobile/i) )) 
            {
                this.class=4  // mobile
                this.isMobile=true;
            } else {
                this.class=0  // generic
                this.isMobile=false;
            }
        }
    }
    zentech.boot = {
        construct:function() {
            zentech.device.construct();
            zentech.cmd.push(function() { 
                zentech.social.setBySelector(".shareBox"); zentech.images.lazy.setBySelector(".lazy");
zentech.ads.construct();
zentech.ads.dfp.construct();
            })
        }
    }

    // boot
    zentech.boot.construct();
    
    // direct javacrict
    
</script>
