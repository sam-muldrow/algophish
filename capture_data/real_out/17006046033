    <script type="text/javascript">

        function RedirectToMobile(url) {
            window.parent.location = url;
        }

        function disableBackground() {
            var hei = $(document).height();
            var divw = $("<div></div>").attr("id", "divBackgroundDisable").attr("class", "backgroundwait").css({ height: hei });
            $("body").prepend(divw);
        }

        function disableBackground2() {
            var hei = $(document).height();
            var divw = $("<div></div>").attr("id", "divBackgroundDisable").attr("class", "backgroundwait").css({ height: hei });
            $("body").prepend(divw);

            setTimeout(function () {
                divw.css("opacity", ".8").css("filter", "alpha(opacity=8)");
            }, 3000);
        }

        function enableBackground() {
            $("#divBackgroundDisable").remove();

            // lock scroll position, but retain settings for later
            var scrollPosition = [
                self.pageXOffset || document.documentElement.scrollLeft || document.body.scrollLeft,
                self.pageYOffset || document.documentElement.scrollTop || document.body.scrollTop
            ];

            // un-lock scroll position
            var html = $('html');
            var scrollPosition = html.data('scroll-position');
            html.css('overflow', html.data('previous-overflow'));
        }

    </script>
