<script type="text/javascript">
       function OptanonWrapper() {
            let onetrustAcceptBtnHandler = document.querySelector("#onetrust-accept-btn-handler") 
            let onetrustCloseBtn = document.querySelector("#onetrust-close-btn-container button") 
            if (onetrustAcceptBtnHandler) {
                BN.pushDataLayer(onetrustAcceptBtnHandler, "click", { 'event': 'one_trust_interaction', 'userAction': 'Accept' })
            }
            if (onetrustCloseBtn) {
                BN.pushDataLayer(onetrustCloseBtn, "click", { 'event': 'one_trust_interaction', 'userAction': 'Close'})
            }
        }
