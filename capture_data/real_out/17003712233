<div id="gw-layout" class="a-section a-spacing-none aok-relative"><!--RowSeparator::1::C--><div id="gw-card-layout" data-grid-breakpoint="ws" data-flow-dir="h" class="a-section a-spacing-none gw-card-layout"><script>
  (function setMeshBreakPoint(w, d) {
    var g = d.getElementById('gw-card-layout'),
        WS = 1239,
        wdh =  w.innerWidth || d.documentElement.clientWidth || d.body.clientWidth,
        bp = wdh <= WS ? "sm" : "ws";
    g && g.setAttribute('data-grid-breakpoint', bp);
  }(window, document));
</script>
