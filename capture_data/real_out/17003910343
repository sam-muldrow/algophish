<script type="text/javascript">
    var activeMenu = 0;

    $("#bottom-menu" + activeMenu + " .bottom-menu-rows:nth-child(0)").addClass("selected");
    $("#bottom-menu" + activeMenu + " .bottom-menu-rows2:nth-child(0)").addClass("selected");
    $("#menu-temp a.menu-rows").each(function (index) {
        if (index == 2 || index == 3|| index == 8) {
            return;
        }
        this.index = index;
        var time;
        $(this).mouseover(function () {
            //jQuery('#dvAlisverisSwfCont').show();
            var index = this.index;
            var self = this;
            time = window.setTimeout(
                function () {
                    if (index == activeMenu) {
                        return;
                    }
                    $(".menu-center a").removeClass("selected");
                    $(self).addClass("selected");
                    $("#bottom-menu" + activeMenu).removeClass("bottom-menu-on");
                    $("#bottom-menu" + (index)).addClass("bottom-menu-on");
                    activeMenu = index;
                }, 300);
        });
        $(this).mouseout(function () {
            window.clearTimeout(time);
        });
    });
</script>

        
