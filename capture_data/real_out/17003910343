        if (getCookie("intdate") == null) {
            setCookie("intdate", Date.now() - 150000 , expireTime);
        }

        if (Date.now() - getCookie("intdate") > 120 * 1000) {
            AdmostClient.getBanner(51257);
            setCookie("intdate", Date.now(), expireTime);
