        }

        animateNews(0);

        $("#news-slide-container").mouseenter(function () {
            $("#home-news").stop(true, false);
        }).mouseleave(function () {
            var myLeft = $("#home-news").position().left;
            animateNews(-myLeft);
        });
        */
    });
</script>

                

