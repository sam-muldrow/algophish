<script>
    $(".standing-menubar a").click(function () {
        var league = $(this).attr("league");
        $.ajax({
            url: "//arsiv.mackolik.com/Home/StandingHandler.aspx?league=" + league,
            success: function (response) {
                $("#dvStandingControl").html(response);
            }
        });
    });
    
    function showBanner() {
        
        //14799
    }
    
    var data = [[2,"Şampiyonlar Ligi Eleme",1,2,"0046C5"],[9,"Avrupa Ligi Eleme",3,1,"4F8DFF"],[60,"Konferans Ligi Elemeler",4,1,"00CCFF"],[6,"Alt Lig",17,4,"F03B3B"]];
    if ($("#tblLightStanding")[0]) {
        for (var i = 0; i < data.length; i++) {
            for (var j = 0; j < data[i][3]; j++) {
                if ($("#tblLightStanding tr[class^=alt]")[j + data[i][2] - 1]) {
                    var cell = $("#tblLightStanding tr[class^=alt]")[j + data[i][2] - 1].cells[0];
                    cell.title = data[i][1];
                    cell.style.backgroundColor = "#" + data[i][4];
                    cell.style.fontWeight = "bold";
                    cell.style.color = "#ffffff";
                }
            }
        }
    }
</script>

