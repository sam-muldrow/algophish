        nav_theme: 'red',
        show_captions: true,
        caption_text_color: 'white',
        overlay_text_color: '#FEEFFE',
        pause_on_hover: false
    });
    
    var activeNewsPage = 0;

    var boolSlideNews = true;
    /*
    window.setInterval(
        function () {
            if (!boolSlideNews) {
                return;
            }
            if ($("#home-news").position().left <= -1910) {
                $("#home-news").css("left", "0px");
            }
            $("#home-news").animate({ left: "-=10px" }, 100);
        }, 100
    );
    */
    var gotoNews = function (newPage) {
        if (newPage < 0) {
            newPage = 2;
        }
        if (newPage == 3) {
            newPage = 0;
        }
        var constWidth = 636;
        $(".img-bar-next-temp .img-bar-next-active").removeClass("img-bar-next-active").addClass("img-bar-next");
        $($(".img-bar-next-temp a")[newPage]).removeClass("img-bar-next").addClass("img-bar-next-active");
        activeNewsPage = newPage;
        $("#home-news").animate({ left: -1 * constWidth * newPage });
    };
</script>
