    <iframe id="featured-video" class="content-media__object"
        src="/VideoPlayer/VideoPlayer.aspx?id=ac24zzvao4bb16lwh85vxi5sp&autoplay=true&showads=true&page=canlisonuc" style="border:none;width:100%;height:100%;" scrolling="no" frameborder="0" allowfullscreen="" webkitallowfullscreen=""></iframe>
