    section: 'AnsaHomepage_BRW' // *DYNAMIC METADATA*: section of site **REQUIRED**	  
	

    }; 
    
  // Event 'staticstart' Call
  nSdkInstance.ggPM("staticstart", nielsenMetadata);
</script>
