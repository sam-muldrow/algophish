                            window.megogo.widgets.widget_1 = new window['wrapper_v1']('#widget_1', 'wrapper_v1');
                        }
                    
                

                if($.cachedScript){
                   var staticScripts = [];

                    
                        staticScripts.push("//s1.vcdn.biz/static/166437351/app.js");
                    

