		<script>
		if ($('#articlesCarousel11').length) {
			//Owl carousel #homeCarousel
			$('#articlesCarousel11').owlCarousel({
				items: 1,
				loop: true,
				center: false,
				dots: true,
				nav: false,
				margin: 0,
				autoplay: true,
				autoplayHoverPause: true,
				autoplayTimeout: 7000,
				dotsContainer: '#articlesCarousel11Dots',
				rtl: true,
				
			});
			// Go to the next item #fullWidthSection
			$('#articlesCarousel11Prev').on('click', function (e) {
				// prevent default anchor click behavior
				e.preventDefault();
				$('#articlesCarousel11').trigger('prev.owl.carousel');
			});

			// Go to the previous item #fullWidthSection
			$('#articlesCarouselNext11').on('click', function (e) {
				// prevent default anchor click behavior
				e.preventDefault();
				// Parameters has to be in square bracket '[]'
				$('#articlesCarousel11').trigger('next.owl.carousel', [300]);
			});
		}
		</script>
