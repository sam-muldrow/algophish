    <!-- Start Webvitals -->
<script type="text/javascript">
    (function () {
        var script = document.createElement('script');
        script.src = '/assets/js/web/webvitals.js';
        script.onload = function () {
            webVitals.onCLS(sendC24WebVitals);
            webVitals.onFID(sendC24WebVitals);
            webVitals.onLCP(sendC24WebVitals);
            webVitals.onFCP(sendC24WebVitals);
        };
        document.head.appendChild(script);
    })();
    function sendC24WebVitals(data) {
        var request = new XMLHttpRequest();
        if (!request) {
            return;
        }
        request.open('POST', '/ajax/misc/webvitals/');
        request.setRequestHeader("Content-Type", "application/x-www-form-urlencoded");
        var params = "name=" + data.name + "&delta=" + data.delta + "&value=" + data.value + "&rating=" + data.rating;
        if (data.name === 'LCP' && data.attribution) {
            params = params + "&element=" + data.attribution.element + "&renderDelay=" + data.attribution.elementRenderDelay;
        }
        request.send(params);
    }
</script>
<!-- End Webvitals -->
  



