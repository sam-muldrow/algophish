    $(function(){
        function c24ifrsnpau2() {
            $(".c24-iframe-wrapper-c24ifrsnpau2").append('<iframe id="c24ifrsnpau2" class="c24-scrolling-iframe c24-scrolling-v1" frameborder="0" scrolling="no"></iframe>');
            $(".c24-iframe-wrapper-c24ifrsnpau2 iframe").attr('data-src','https://urlaub.check24.de/vergleiche/39-1-6-0/a/b/Urlaub+T%C3%BCrkei?widget=1&deviceoutput=desktop');
            $(".c24-iframe-wrapper-c24ifrsnpau2 iframe").on("load", function(){
                $(".c24-scrolling-wrapper-c24ifrsnpau2 .c24-scrolling-loading").hide();
                $(".c24-iframe-wrapper-c24ifrsnpau2").css("visibility","visible");
            });
        };
        c24ifrsnpau2();
    });
    
