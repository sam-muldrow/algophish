    $(function(){
        function c24ifrsnpau1() {
            $(".c24-iframe-wrapper-c24ifrsnpau1").append('<iframe id="c24ifrsnpau1" class="c24-scrolling-iframe c24-scrolling-v1" frameborder="0" scrolling="no"></iframe>');
            $(".c24-iframe-wrapper-c24ifrsnpau1 iframe").attr('data-src','https://urlaub.check24.de/vergleiche/2-6-6-0/Balearen/Spanien/Mallorca+?widget=1&deviceoutput=desktop');
            $(".c24-iframe-wrapper-c24ifrsnpau1 iframe").on("load", function(){
                $(".c24-scrolling-wrapper-c24ifrsnpau1 .c24-scrolling-loading").hide();
                $(".c24-iframe-wrapper-c24ifrsnpau1").css("visibility","visible");
            });
        };
        c24ifrsnpau1();
    });
    
