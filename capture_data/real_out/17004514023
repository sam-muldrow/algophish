function finrkrValidate() {
    Check24.BPM.set("credit_amount", $("#c24-rkr-credit-amount").val());
    var kp = parseInt($("#c24-rkr-credit-amount").val().replace(/\./g, ""), 10);
    return true;
}
$(document).ready(function() {
    Check24.BPM.get("credit_amount", function(val) {
        if(typeof val !== undefined && val !== "") {
            $("#c24-rkr-credit-amount").val(val.toString().replace(/\B(?=(\d{3})+(?!\d))/g, "."));
        }
    });
    Check24.BPM.get("credit_usage", function(data){
        if(typeof data !== undefined && data !== "") {
            $("#c24-rkr-credit-usage").val(data);
        }
    });
    Check24.BPM.get("credit_period", function(data){
