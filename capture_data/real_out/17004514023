///////////////////////////////////////////////////////////////

    document.addEventListener("DOMContentLoaded", function() {
        initSelects();
        updateSelection();
        initTypeChip();
    });

     function initTypeChip() {
        document.querySelectorAll(".c24-aut-type-chip").forEach((chip) => {
            chip.addEventListener("click", function (e) {
                selectChip(e.target.closest(".c24-aut-type-chip"));
            });
        });
    }

    function initSelects() {
        document.querySelectorAll(".c24-aut-select .c24-yellow-dropdown-hidden-input").forEach((select) => {
            select.addEventListener("change", function (e) {
                updateSelection();
            });
        });
    }

    function selectChip(chip) {
        if (!chip) { return; }

        var newTypeValue = chip.dataset.value
        var typeInput = document.querySelector(".c24-aut-slidercontainer input[name=\"season\"]");
        if (!typeInput || !newTypeValue) { return; }

        typeInput.value = newTypeValue;

        updateSelection();
    }

    function updateSelection() {
        var seasonInput = document.querySelector(".c24-teaser-slider-shoaut input[name=\"season\"]");
        var vehicleTypeInput = document.querySelector(".c24-teaser-slider-shoaut input[name=\"vehicleType\"]");
        var widthSelect = document.querySelector(".c24-aut-select .c24-yellow-dropdown-hidden-input[name=\"width\"]");
        var heightSelect = document.querySelector(".c24-aut-select .c24-yellow-dropdown-hidden-input[name=\"height\"]");

        requestNewConfiguration(
            seasonInput.value,
            widthSelect.value,
            heightSelect.value,
            vehicleTypeInput.value
        );
    }

    var carpartsFirstCall = true;
    function requestNewConfiguration(season, width, height, vehicleType) {
        var request = new XMLHttpRequest();

        var params = new URLSearchParams({
            season: season ?? "",
            width: width ?? "",
            height: height ?? "",
            vehicleType: vehicleType ?? ""
        });
        var url = "https://reifen.check24.de/ajax/tyreselector?" + params.toString();

        request.onload = function() {
            try {
                handleTireConfigurationLoad(request);
                if (carpartsFirstCall) {
                    updateBPMfields();
                    carpartsFirstCall = false;
                }
            } catch(e) {}
        };
        request.open("GET", url);
        request.send();
    }

    function handleTireConfigurationLoad(request) {
        if (request.status !== 200) { return; }

        var data = JSON.parse(request.response);

        var widthSelect = document.querySelector(".c24-teaser-slider-shoaut ul[name=\"width\"]");
        var heightSelect = document.querySelector(".c24-teaser-slider-shoaut ul[name=\"height\"]");
        var inchSelect = document.querySelector(".c24-teaser-slider-shoaut ul[name=\"inch\"]");

        if (widthSelect && data.widths) {
            updateSelectOption(widthSelect, data.widths);
        }
        if (heightSelect && data.heights) {
            updateSelectOption(heightSelect, data.heights);
        }
        if (inchSelect && data.diameters) {
            updateSelectOption(inchSelect, data.diameters);
        }
    }

    function updateSelectOption(selectEle, optionList) {

        var previousValue = selectEle.dataset.value;

        var allOption = document.createElement("li");
        allOption.classList.add("c24-option");
        allOption.classList.add("selected");

        allOption.dataset.value = "";
        allOption.innerText = "Alle";

        selectEle.innerHTML = "";
        selectEle.appendChild(allOption);

        optionList.forEach(function (optionValue) {
            var newOption = document.createElement("li");

            newOption.classList.add("c24-option");

            newOption.dataset.value = optionValue;

            newOption.innerText = optionValue;

            selectEle.appendChild(newOption);
        });

        if (optionList.includes(previousValue)) {
            selectEle.dataset.value = previousValue;
        }
    }

    function updateBPMfields() {
        Check24.BPM.get("_all_", function(data) {
            if (typeof data.carparts_season !== "undefined" && data.carparts_season !== "") {
                document.querySelector(".c24-teaser-slider-shoaut .c24-select-tabs").classList.add("bpm");
                Check24.removeClass(".c24-aut-type-chip", "active");
                Check24.addClass(".carpartsSeason-" + data.carparts_season, "active");
            } else {
                return;
            }

            var selectW = document.querySelector(".c24-teaser-slider-shoaut ul[name=\"width\"]");
            var selectH = document.querySelector(".c24-teaser-slider-shoaut ul[name=\"height\"]");
            var selectI = document.querySelector(".c24-teaser-slider-shoaut ul[name=\"inch\"]");

            if (typeof data.carparts_width !== "undefined") {
                selectW.value = data.carparts_width;
                if (!checkSelectOptions(selectW, data.carparts_width)) {
                     createBPMOption(selectW, data.carparts_width);
                }
                showBPMOption(selectW, data.carparts_width);

            } else {
                selectW.dataset.value = "";
            }
            if (typeof data.carparts_height !== "undefined") {
                selectH.value = data.carparts_height;
                if (!checkSelectOptions(selectH, data.carparts_height)) {
                    createBPMOption(selectH, data.carparts_height);
                }
                showBPMOption(selectH, data.carparts_height);
            } else {
                selectH.dataset.value = "";
            }
            if (typeof data.carparts_inch !== "undefined") {
                selectI.value = data.carparts_inch;
                if (!checkSelectOptions(selectI, data.carparts_inch)) {
                    createBPMOption(selectI, data.carparts_inch);
                }
                showBPMOption(selectI, data.carparts_inch);
            } else {
                selectI.dataset.value = "";
            }

        }, "carparts");

    }

    function checkSelectOptions(ele, val) {
        const options = ele.querySelectorAll(".c24-option");
        options.forEach(option => {
            if (option.dataset.value === val) {
                return true;
            } else {
                return false;
            }
        })
    }

   function showBPMOption(ele, val) {
        const selectedListItem = ele.querySelector(".c24-option.selected");
        selectedListItem.classList.remove("selected");
        const listItems = ele.querySelectorAll(".c24-option");
        listItems.forEach(item => {
            if (item.dataset.value === val) {
                item.classList.add("selected");
            }
        })
    }

     function createBPMOption(ele, val) {
        const selectedListItem = ele.querySelector(".c24-option.selected");
        selectedListItem.classList.remove("selected");
        var option = document.createElement("option");
        option.value = val;
        option.innerText = val;
        option.classList.add("c24-option");
        option.classList.add("selected");
        ele.appendChild(option);
    }

///////////////////////////////////////////////////////////////

$(function(){
    var loginOpener = document.querySelectorAll(".c24-login-ele");
    loginOpener.forEach(function(ele){
        ele.addEventListener("click", function() {
            Check24.uliloginlayer.open();
        });
    });
});

