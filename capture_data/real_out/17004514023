$(function(){
    C24.webask.trigger = $(".c24-input-ask-search");
	Check24.potential.init();
    C24.webask.init();
    C24.webask.acSubmit = function() {
        $(".c24-submit-ask").focus();
    };
    $(".c24-input-ask-search").on("keyup focusin", function(e) {
        $(".c24-webask-ac-error").hide();
        if (e.keyCode == 13 && $(this).val() != "" && $(this).val() != "") {
            C24.webask.submit($(this));
        }
        if (e.keyCode != "13" && e.keyCode != "9") {
            $(".c24-webask-ac-msg").hide();
        }
        if (e.keyCode != "13" && e.keyCode != "38" && e.keyCode != "40") {
            C24.webask.controls.posActEle = 0;
            $(this).closest("form").removeAttr("action");
            if (e.keyCode != 39 && e.keyCode != 37) {
                C24.webask.getCategories($(this).val());
            }
        }
    });
});

