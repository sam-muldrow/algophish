    Check24.infoFrame.init("https://versicherungen.check24.de/datenschutz-agb/versicherungen/erstinformation/",1140,"c24-ei-trigger","c24-info-frame-wrapper-ei");

    $(document).ready(function() {
        // Open
        $(".c24-ei-trigger").click(function() {
            openInfo();
        });

        // Hide
        $(".c24-info-frame-close").click(function() {
            closeInfo();
        });
        $(".c24-blocking-layer").click(function() {
            closeInfo();
        });
    });

    function openInfo() {
        $(".c24-info-frame-wrapper-ei").show();
    }

    function closeInfo() {
        $(".c24-info-frame-wrapper-ei").hide();
        $("#c24-page-and-ads").css("height", "auto");
    }

