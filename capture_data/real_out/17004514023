$(function() {
	Check24.show([".c24-customer-trigger", ".c24-customer-trigger .c24-header-hover-layer"]);
    Check24.removeClass(".c24-customer-trigger .c24-customer-layer", "c24-header-hover-layer");
    Check24.addClass(".c24-customer-trigger .c24-customer-layer", "c24-customer-trigger-layer");
    Check24.removeClass(".c24-customer-trigger .c24-customer-trigger-layer", "c24-customer-layer");
    var triggerlayer = document.querySelector(".c24-customer-trigger .c24-customer-trigger-layer");
    if (triggerlayer !== null) {
        triggerlayer.style.visibility = "visible";
        setTimeout(function() {
            Check24.hide([".c24-customer-trigger", ".c24-customer-trigger .c24-header-hover-layer"]);
        }, 10000);
    }
});

