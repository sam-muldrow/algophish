$(function(){
    Check24.cookieBanner.init();
    window.addEventListener("pageshow", function (event) {
        var historyTraversal = event.persisted || (typeof window.performance != "undefined" && window.performance.navigation.type === 2);
        if (historyTraversal) {
            var c24consent = Check24.cookieBanner.c24consent.getCookieWithName("c24consent");
            if (c24consent && c24consent !== "") {
                Check24.removeClass("body", "c24-cc-visible");
            }
        }
    });
});

///////////////////////////////////////////////////////////////

$(function() {
    Check24.uliloginlayer.init({
        "deviceoutput": "desktop",
        "api_product": "check24_lp",
        "register_type": "secure"
    });
    Check24.uliloginlayer.closeAfter = function(data) {
        if (data) {
            if (data.STATE && data.STATE !== "") {
                if (data.STATE === "loggedin" || data.SSO_SESSION !== "") {
                    document.location.reload();
                }
            }
        }
    };
});

///////////////////////////////////////////////////////////////

$(function() {
    Check24.uliloginlayer.beforeOpen = function() {
        var chatJson = window.localStorage.getItem("c24-chat-assistant");
        if (chatJson) {
            var chatAssistant = JSON.parse(chatJson);
            if (chatAssistant.active === true && chatAssistant.uliSession) {
                var i = 0;
                var l = chatAssistant.uliSession.length;
                for (i = 0; i < l; i++) {
                    var chatSession = chatAssistant.uliSession[i];
                    if (chatSession.location.href === window.location.href) {
                        Check24.uliloginlayer.config.params.social_login_base_url = "https://kundenbereich." + Check24.getDomain() + "/";
                        Check24.uliloginlayer.config.params.social_login_closing_url = "https://" + Check24.getHost() + "/assets/html/sso/v3/closingULIIframeChatty.html";
                    }
                }
            }
        }
    };
});

//]]>
</script>
