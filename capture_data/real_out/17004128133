<script>
    $(document).ready(function () {

        const togglePassword = $('#togglePassword');
        togglePassword.on('click', function () {
            const password = $('input[name="password_login"]');
            const type = password.attr('type') === 'password' ? 'text' : 'password';
            password.attr('type', type);
            $(this).toggleClass('bi-eye');
        });

        sliderFormObj = $('#sliderText');

        $('#room_name').on('keypress', function (e) {
            if (e.which == 13){
                e.preventDefault();
                $('#sendRoom').trigger('click');
            }
        });

        $(".m-bottom-text").delegate('.iknowroom', 'click', function () {

            var action = sliderFormObj.attr('type');
            $('.sliderForm').find('input').val('');
            if (action == "guest") {
                sliderFormObj.find('.sliderForm').addClass('right');
                sliderFormObj.find('.sliderForm').attr('iknowroom', true);
                sliderFormObj.find('em').html('.web.tv');
                sliderFormObj.attr('type', 'iknowroom');
                sliderFormObj.find('.m-bottom-text').html("Video konferans mı yapmak istiyorsun? <a href=\"javascript:void(0)\" class=\"iknowroom\">Odanı oluştur</a>  ");
                sliderFormObj.find('h3').html("Kanal adını yaz, görüşmeye başla");
                sliderFormObj.find('input').attr("placeholder", " ");
            } else {
                sliderFormObj.find('.sliderForm').removeClass('right');
                sliderFormObj.find('.sliderForm').attr('iknowroom', false);
                sliderFormObj.find('em').html('web.tv/oda/');
                sliderFormObj.attr('type', 'guest');
                sliderFormObj.find('.m-bottom-text').html("Kanalı biliyor musun? Gitmek için <a href=\"javascript:void(0)\" class=\"iknowroom\">tıklayınız.</a>  ");
                sliderFormObj.find('h3').html("40 dakika ücretsiz toplantı imkanı için odanı yaz ");
                sliderFormObj.find('input').attr("placeholder", "Odanızın adını yazın");
            }
        });

        $('.sliderForm').find('input').on('keyup', function (e) {
            var action = $('.sliderForm').attr('iknowroom');

            var charMap = {
                Ç: 'C',
                Ö: 'O',
                Ş: 'S',
                İ: 'I',
                I: 'i',
                Ü: 'U',
                Ğ: 'G',
                ç: 'c',
                ö: 'o',
                ş: 's',
                ı: 'i',
                ü: 'u',
                ğ: 'g'
            };

            var str = $(this).val();

            str_array = str.split('');

            for (var i = 0, len = str_array.length; i < len; i++) {
                str_array[i] = charMap[str_array[i]] || str_array[i];
            }

            str = str_array.join('');

            var clearStr = str.replace(/[çöşüğı]/gi, "");

            $(this).val(clearStr);

        });

        $('#sendRoom').click(function () {

            var roomName = $('.sliderForm').find('input').val();
            var action = sliderFormObj.attr('type');

            if (roomName.length >= 3) {

                var BASE_URL = 'https://web.tv/';
                var ajaxUrl = BASE_URL + '/api/webrtc/rooms/create';
                var buttonName = "Odaya Katıl";
                var data = {room: roomName};

                if (action != "guest") {

                    ajaxUrl = BASE_URL + '/api/webrtc/conference/getchannelmode';
                    buttonName = "Kanala Katıl";
                    data = {channel: roomName};
                }

                $.ajax({
                    url: ajaxUrl,
                    //url : 'http://hakan.dev.artankito.com/c/create/a',
                    type: 'post',
                    data: data,
                    dateType: "json",
                    success: function (data) {
                        if (action == "guest") {
                            var redirectUrl = BASE_URL + '/oda/' + data.obj.room;
                            if(data.result == "fail") {
                                if(data.key.indexOf('ROOM EXISTS') >= 0) {
                                    BootstrapDialog.show({
                                        type: 'type-' + data.modal.type,
                                        title: "Uyarı ",
                                        message: data.modal.msg,
                                        buttons: [{
                                            label: buttonName,
                                            action: function (dialog) {
                                                window.location.href = redirectUrl;
                                            }
                                        }, {
                                            label: "İptal",
                                            action: function (dialog) {
                                                dialog.close();
                                            }
                                        }]
                                    });
                                } else {
                                    BootstrapDialog.show({
                                        type: data.modal.type,
                                        title: data.modal.title,
                                        message: data.modal.msg,
                                        buttons: [{
                                            label: "İptal",
                                            action: function (dialog) {
                                                dialog.close();
                                            }
                                        }]
                                    });
                                }
                            } else {
                                window.location.href = redirectUrl;
                            }
                        } else {
                            if(data.result == 'success') {
                                if (data.key.indexOf('ON') >= 0) {
                                    var redirectUrl = 'https://' + roomName + '.web.tv/video-konferans';
                                    BootstrapDialog.show({
                                        type: 'type-' + data.modal.type,
                                        title: "Uyarı ",
                                        message: data.modal.msg,
                                        buttons: [
                                            {
                                                label: buttonName,
                                                action: function (dialog) {
                                                    window.location.href = redirectUrl;
                                                }
                                            },
                                            {
                                                label: "İptal",
                                                action: function (dialog) {
                                                    dialog.close();
                                                }
                                            }
                                        ]
                                    });
                                } else {
                                    BootstrapDialog.show({
                                        type: 'type-' + data.modal.type,
                                        title: "Uyarı ",
                                        message: data.modal.msg,
                                        buttons: [
                                            {
                                                label: "İptal",
                                                action: function (dialog) {
                                                    dialog.close();
                                                }
                                            }
                                        ]
                                    });
                                }
                            } else {
                                BootstrapDialog.show({
                                    type: 'type-' + data.modal.type,
                                    title: data.modal.title,
                                    message: data.modal.msg,
                                    buttons: [{
                                        label: "Kapat",
                                        action: function (dialog) {
                                            dialog.close();
                                        }
                                    }]
                                });
                            }
                        }
                    }
                });

            }

        });

        $('#goLink').click(function () {
            let link = $('#room-url').val();
            if(link.length > 0)
                window.location.href = link;
        });
        $('#try_conference').click(function (e) {
            $('#conference_open').val("ok");
        });

        $('#loginBtn').click(function () {
            $('#conference_open').val("");
        });


        $("#register-form").validationEngine(
            'attach',
            {
                promptPosition: "centerLeft",
                scroll: false,
                "ajaxUserCall": {
                    // remote json service location
                    "url": MAIN_URL + 'registration/checkemailajaxhome/'
                },
                "ajaxUserCallPhp": {
                    // remote json service location
                    "url": MAIN_URL + 'registration/checkloginnameajaxhome/'
                }
            });

        /*
