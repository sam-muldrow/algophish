	<div class="fl-module-content fl-node-content">
		<div
  class="c-map c-map--static"
  x-data="{
  tip: &#039;&#039;,
  tip_x: 0,
  tip_y: 0,
  setTip( label, x, y ) {
    this.tip = label;
    this.tip_x = x;
    this.tip_y = y;
  },
  tipOn( site ) {
    if ( &#039;core&#039; !== site.type ) return;
    this.setTip( site.name, site.x, site.y );
  },
  tipOff() {
    this.setTip( &#039;&#039;, 0, 0 );
  }
}"
  data-network-state="successful"
  data-network-filter="all"
  x-bind:data-network-filter="$store.network_data.type"
>
