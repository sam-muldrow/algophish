      var lazyComponentObserver = new IntersectionObserver(function (
        entries,
        observer
      ) {
        entries.forEach(function (entry) {
          if (entry.isIntersecting) {
            var script = lazyComponents.find(function (node) {
              return node.id === entry.target.id;
            }).script;
            eval(script);
            lazyComponentObserver.unobserve(entry.target);
          }
