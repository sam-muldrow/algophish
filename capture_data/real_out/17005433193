    let reggieVersion;
    let jsPath;
    let cssPath;

    // If you need a dev mode because you're working in localhost
    // Update this idea as you need to
    const devMode = true;

    const fetchReggieVersion = async () => {
        // Derive the current host
        const currentLocation = window.location;
        let host = currentLocation.host.split(".").splice(-2).join(".")


        if (devMode) {
            host = "domain.com";
        }

        // Unfortunately, this bit of legacy magic is required by reggie-client
        window.V = window.V || {};
        window.V.grammar = {};
        window.V.grammar.reggie_api_base = host;


        // The current production version of reggie can be derived here
        // At some point we will move this to an S3 bucket file or possibly AEM config
        // Note: propertyID=47 = domain.com.  Bluehost is 52.
        const reggieVersionURL = "https://registration." + host + "/cmsAttributes?propertyID=47&name=reggie_release_version";

        console.info("Fetch reggie version from" + reggieVersionURL);
        try {
            await fetch(reggieVersionURL)
                .then((res) => res.json())
                .then((data) => {
                    reggieVersion = data?.reggie_release_version;
                });

            console.info("Loading Reggie Bundle:" + reggieVersion);
            if (reggieVersion) {
                jsPath = "https://static.registration." + host + "/" + reggieVersion + "/static/reggie/js/reg3-bundle.js";
                cssPath = "https://static.registration." + host + "/" + reggieVersion + "/static/reggie/css/miniCart.css";

                // Developer note, if trying to use the MiniCart for a brand for the first time and
                // it doesn't load, the Minicart may not have been enabled officially as a component
                // for the brand in reggie. This requires adding to brandComponentList from a reggie dev
                const script = document.createElement('script');
                const body = document.querySelector('body');

                script.setAttribute('src', jsPath);
                script.defer = true;
                script.classList.add('reggie-bundle-script');
                body.append(script);

                const styleTagWithPreload = document.createElement('link');
                styleTagWithPreload.setAttribute('rel', 'preload');
                styleTagWithPreload.setAttribute('as', 'style');
                styleTagWithPreload.setAttribute('onload', "this.onload=null;this.rel='stylesheet'");
                styleTagWithPreload.setAttribute('type', 'text/css');
                styleTagWithPreload.setAttribute('href', cssPath);
                body.append(styleTagWithPreload);
            }
        } catch (e) {
            console.log("Something went wrong retrieving reggie version:" + e + "from" + reggieVersionURL);
        }
    }

