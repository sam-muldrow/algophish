R9.globals.formtoken = seoR9.formToken;
}
if (seoR9.globalPlacementEnabled) {
if (seoR9.placement) {
R9.globals.placement = seoR9.placement;
}
if (seoR9.affiliate) {
R9.globals.affiliate = seoR9.affiliate;
}
}
}
