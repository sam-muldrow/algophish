		#img_focus_btn a.current{
			border:1px solid #e9981b;color:#fff;background-color:#bb0008;
		}
	</style><script type="text/javascript">
		$("#img_focus_btn a").click(function(){
			var idx=$(this).index();
			if(!$(this).hasClass("current")){
				img_focus_show(idx);
			}
		});

		var interval_img_focus=null;
		function img_focus_show(idx){
			var $img_link=$("#img_focus a:eq("+idx+")");
			var $img_btn=$("#img_focus_btn a:eq("+idx+")");

			$("#img_focus a:visible").hide();
			$("#img_focus_btn a.current").removeClass("current");

			$img_link.show();
			$img_btn.addClass("current");

			$("#img_focus_title a").attr("href",$img_link.attr("href"));
			$("#img_focus_title a").text($img_link.attr("title"));

			var img_num=$("#img_focus a").length;
			if(idx>=0 && idx<img_num){
				clearInterval(interval_img_focus);
				interval_img_focus=setTimeout(function(){
					img_focus_show((idx+1)%$("#img_focus_btn a").length);
				},3000);
			}
		}
		img_focus_show(0);
