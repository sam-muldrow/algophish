                                if ('2023-11-21 00:14:33' != '') {
                                    var server = new Date('2023-11-21 00:14:33'.replace(/-/g, '/')),
                                        now = lc = new Date(),
                                        diff = server.getTime() - now.getTime();
                                    setInterval(function () {
                                        lc = new Date();
                                        lc.setTime(lc.getTime() + diff);
                                        var d = zeroPad([lc.getFullYear(), lc.getMonth() + 1, lc.getDate()], 2),
