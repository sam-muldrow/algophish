const ajaxHeaders = new Headers();
ajaxHeaders.append('X-Requested-With', 'XMLHttpRequest');
ajaxHeaders.append('hash', 'toto');

window.lcm.fetch = function(url, options = {}) {
    options.headers = {...options.headers, ...ajaxHeaders};
    return fetch(url, options).then(response => {
        if(response.status >= 400)  { throw new Error('error '+ response.status); }
        return response;
    });
};

