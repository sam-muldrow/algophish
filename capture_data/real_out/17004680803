                    historical: '<svg class="icon icon-Historical " aria-hidden="true"><use xlink:href="/images/int/svg/icons-v1.1.15.svg#historical" /></svg>',
                    starFull: '<svg class="icon icon-StarFull " aria-hidden="true"><use xlink:href="/images/int/svg/icons-v1.1.15.svg#star-full" /></svg>',
                    chevronLeft: '<svg class="icon icon-ChevronLeft " aria-hidden="true"><use xlink:href="/images/int/svg/icons-v1.1.15.svg#angle-left" /></svg>',
                    chevronRight: '<svg class="icon icon-ChevronRight " aria-hidden="true"><use xlink:href="/images/int/svg/icons-v1.1.15.svg#angle-right" /></svg>'
                })
                .setPopinParams({
                    delete: {
                        title: "Confirmation",
                        message: "Votre favori a bien été supprimé",
                        close: true,
                        icon: 'none',
                        buttons: [{
                            label: "Fermer"
                        }]
                    },
