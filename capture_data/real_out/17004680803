        evt.listen('sliderLoaded', () => {
            window.lcm.slider
                .setIconLeft('<svg class="icon icon-ChevronLeft " aria-hidden="true"><use xlink:href="/images/int/svg/icons-v1.1.15.svg#angle-left" /></svg>')
                .setIconRight('<svg class="icon icon-ChevronRight " aria-hidden="true"><use xlink:href="/images/int/svg/icons-v1.1.15.svg#angle-right" /></svg>');
        });
        evt.listen('forecast_map_timetableLoaded', () => {
            window.lcm.forecastMapTimeTable.load();
        });

        const breakpointChangeSliderActu = (e) => {
            if (['xl', 'l', 'm'].indexOf(e.detail.size) !== -1) {
                loadScriptBuild('slider_actu');
                document.removeEventListener('breakpointChange', breakpointChangeSliderActu);
            }

        };


        evt.listen('DOMContentLoaded', () => {
            loadScriptBuild('slider');
            loadScriptBuild('video');
            loadScriptBuild('forecast_map_timetable');


                        if (['xl', 'l', 'm'].indexOf(window.lcm.breakpoint.getCurrent()) !== -1) {
                loadScriptBuild('slider_actu');
            } else {
                document.addEventListener('breakpointChange', breakpointChangeSliderActu, {passive: true});
            }
            



        });

