<script>
    evt.listen('breakpoint', () => {
        if(["xxs","xs","s","m","l","xl"].indexOf(window.lcm.breakpoint.getCurrent())!== -1) {
            const f = document.createDocumentFragment();
            const a = document.createElement('aside');
            f.append(a);
            const d = document.createElement('div');
            d.className = 'ad sponso OUTBRAIN smartbox home';
            d.dataset.src = window.location.href;
                        //  d.dataset.src = 'https://www.lachainemeteo.com/meteo-france/ville-33/previsions-meteo-paris-aujourdhui';
            d.dataset.widgetId = 'SF_1';
            a.append(d);
            document.getElementById('ob_in-feed').append(f);
            if(/outbrain/.test(new URLSearchParams(window.location.search).get('extra'))) {
                d.textContent = 'outbrain in-feed [SF_1]';
            }


            window.dataLayer.push({'event': 'outbrain', 'widget':'SF_1', 'url' : location.href});
        }
    });

</script>
