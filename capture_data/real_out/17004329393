<script>
    window.googletag = window.googletag || {
        cmd: []
    }, window.ta = window.ta || {
        gam: {
            ready: !1,
            tags: {}
        }
    };
    var mbw = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
    getDataLayerValue = function (e, t, a) {
        for (var o = 0, n = window.dataLayer.length; o < n; o++) {
            var g;
            if (void 0 !== dataLayer[o][e] && (g = t ? cleanKeyValue_GAM(dataLayer[o][e]) : dataLayer[o][e], a)) return g
        }
        return g
    }, getURLSegments = function (e) {
        return e.split("/").filter(function (e) {
            return null != e && "" != e && null != e
        })
    }, getCMSEnvironment = function () {
        var dev_env_check = new RegExp(/^.+\.localhos\.tips/).test(window.location.hostname)
        return dev_env_check ? "dev" : "prod"
    }, cleanKeyValue_GAM = function (e) {
        if (e)
            if (Array.isArray(e))
                for (i = 0; i < e.length; i++) e[i] = e[i].replace(/["'=!+#*~;^()<>\[\],&]/g, "").replace(/\s+/g, " ").replace(/^\s+|\s+$/g, "");
            else "object" == typeof e && null !== e ? Object.keys(e).forEach(function (t) {
                e[t] = e[t].replace(/["'=!+#*~;^()<>\[\],&]/g, "").replace(/\s+/g, " ").replace(/^\s+|\s+$/g, "")
            }) : e = e.replace(/["'=!+#*~;^()<>\[\],&]/g, "").replace(/\s+/g, " ").replace(/^\s+|\s+$/g, "");
        return e
    }, dumpGAMTargeting = function () {
        if (window.googletag && googletag.pubadsReady) {
            var e = window.googletag.pubads().getTargetingKeys();
            for (k = 0; k < e.length; k++) console.log(e[k], googletag.pubads().getTargeting(e[k]))
        }
    }, refreshDFPTags = function () {
        if (window.googletag && googletag.pubadsReady)
            for (var e in window.ta.gam.tags) window.ta.gam.tags.hasOwnProperty(e) && null != window.ta.gam.tags[e] && void 0 !== window.ta.gam.tags[e].slot && window.ta.gam.tags[e].refreshable && (window.ta.gam.tags[e].refreshed = !0, window.ta.gam.tags[e].refresh_counter++, googletag.pubads().refresh([window.ta.gam.tags[e].slot], {
                changeCorrelator: !1
            }))
    }, hide__div = function (e) {
        if (e) {
            if (window.googletag && googletag.pubadsReady) {
                var t = e.replace(/-outer/, "").replace(/-/g, "");
                void 0 !== window.ta.gam.tags[t].slot && (googletag.destroySlots([window.ta.gam.tags[t].slot]), window.ta.gam.tags[t] = null)
            }
            document.getElementById && (document.getElementById(e).style.visibility = "hidden", document.getElementById(e).innerHTML = "")
        }
    };
</script>

<script>
    googletag.cmd.push(function () {
        /****************************
		/* Targeting Attributes
		****************************/

		// Clean values as needed for ingestion
		var _env = getCMSEnvironment();
		var _clean_path = cleanKeyValue_GAM(getURLSegments(window.location.pathname));
		var _clean_category = cleanKeyValue_GAM([]);
		var _clean_kw = cleanKeyValue_GAM(["wonderhowto.com","WonderHowTo"]);

		// Set Content Attributes
		googletag.pubads().setTargeting('env', _env);
		googletag.pubads().setTargeting('url', window.location.hostname.concat(window.location.pathname));
		googletag.pubads().setTargeting('hostname', window.location.hostname);
		googletag.pubads().setTargeting('category', _clean_category);
		googletag.pubads().setTargeting('kw', _clean_kw);
		googletag.pubads().setTargeting('path', _clean_path);
		googletag.pubads().setTargeting('lazyload', 'true');

		googletag.pubads().enableLazyLoad({
		    fetchMarginPercent: 300, // when to fetch the ad?
			renderMarginPercent: 150, // when to render the ad
			mobileScaling: 2.0 // config for mobile
		});

		// Important - Enable single-request mode, product requirement
		googletag.pubads().enableSingleRequest();

		// Enable GAM API services
		googletag.enableServices();
	});
</script>
<!-- End: Advertising Core -->



