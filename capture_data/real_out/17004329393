<!-- End: GAM Ad Slot Definitions | Wonderhowto database Category -->
<!-- Start: Ad Placement -->
<script>
    (function (WHT) {
        WHT.Placements = function (slots, page_type) {
            var self = this;

            this.template = '<div class="{1}" id="{0}"></div>';
            this.cloneTemplate = '<div class="{1}" id="{0}"></div>';
            this.aphTemplate = '<aside class="whtaph {0}" data-unit-type="{0}"></aside>';
            this.width = 1280;
            this.$placeholders = null;
            this.cloneNum = 1;
            this.clonePaused = false;
            this.cloneOneAtATime = false;
            this.listeningToScroll = false;
            this.enableCloning = false;
            this.maxClone = 4;
            this.cloneCount = 0;
            this.minDistanceBetweenPlacements = 1000;
            this.aRenderingPlacements = [];
            this.iPendingPlacementTO = null;
            this.iForceRefreshIfUnfilledAfterSeconds = 0;
            this.iMinAgeForBlankVisiblePlacementRefresh = 5;
            this.bSkipPreBidOnRefresh = false;


                
                this.adSlotPlacementMap = {
                    'inlinetop': 'topwide',
                    'rightrailtop': 'bigbox',
                    'rightrailmid': 'floatbox',
                    'mid1': { name: 'rightbox', ordinal: 1, slotpos: 'left'  },
                    'mid2': { name: 'rightbox', ordinal: 2, slotpos: 'left'  },
                    'mid3': { name: 'rightbox', ordinal: 3, slotpos: 'left'  },
                    'mid4': { name: 'rightbox', ordinal: 4, slotpos: 'left'  },
                    'mid5': { name: 'rightbox', ordinal: 5, slotpos: 'left'  },
                    'mid6': { name: 'rightbox', ordinal: 6, slotpos: 'left'  },
                    'low1': { name: 'rightbox', ordinal: 1, slotpos: 'right' },
                    'low2': { name: 'rightbox', ordinal: 2, slotpos: 'right' },
                    'low3': { name: 'rightbox', ordinal: 3, slotpos: 'right' },
                    'low4': { name: 'rightbox', ordinal: 4, slotpos: 'right' },
                    'low5': { name: 'rightbox', ordinal: 5, slotpos: 'right' },
                    'low6': { name: 'rightbox', ordinal: 6, slotpos: 'right' },
                };
                

            this.adSlotHtmlTemplate = function (slot) {
                if (slot) {
                    var template =
                        `<!-- Start: GAM Ad Slot Placement | ${slot.id} -->
                         <div id="${slot.id}" style="min-width: ${slot.min_width}px; max-height: ${slot.max_height}px; height: auto;">
                            <script>
                                window.googletag = window.googletag ||
                                {
                                    cmd: []
                                };

                                window.googletag.cmd.push(function()
                                {
                                    googletag.display("${slot.id}");
                                });
                            <\/script>
                        <\/div>
                        <!-- End: GAM Ad Slot Placement | ${slot.id} -->`;
                    return template;
                }
                else {
                    return "";
                }
            };

            // Fired once the page loads.
            this.init = function () {
                if (!slots) {
                    console.log('No valid placements available for this page.');
                    return;
                }
                self.$win = $(window);
                self.preparePlaceholders();
                self.addAdSlots();
            };

            this.addAdSlots = function () {
                Object.entries(slots).forEach(([key, slot]) => {

                    // Consult the map to find how to place the slot based on its key.
                    // Placement can either be a string or a object.
                    // If it's a string, it's the name of div the slot needs to be placed into.
                    // If it's an object, it has two keys and optional third key:
                    //             name: the name of the div slot goes in
                    //             ordinal: the div's ordinal, there are multiple possible divs
                    //                         with same name, ordinal determines which one starting
                    //                         from the top to the bottom of the page in order.
                    //             slotpos: if present, determines the slot's position in the div
                    //                      in relation to other possile slots occupying the div with same ordinal
                    var placement = self.adSlotPlacementMap[key];

                    if (placement) {
                        var adSlotHtml = self.adSlotHtmlTemplate(slot);
                        switch (typeof placement) {
                            case 'string':
                                var placeholder = $(`[data-unit-type="${placement}"]`);
                                if (placeholder) {
                                    placeholder.html(adSlotHtml);
                                }
                                else {
                                    //console.log(`no placeholder found for ${placement}`);
                                }
                                break;
                            case 'object':
                                var divs = $(`[data-unit-type="${placement.name}"]`);
                                if (divs.length && placement.ordinal <= divs.length) {
                                    var placeholder = divs[placement.ordinal - 1]
                                    // check to see if need perform slot positioning inside the placeholder
                                    if (placement.slotpos) {
                                        // Check to see if there's already a div inside the placeholder.
                                        var child = $(placeholder).find(':first-child');
                                        if (child.length) {
                                            // A child div already exist, check whether this slot needs to be
                                            // left or right of the child.
                                            switch (placement.slotpos) {
                                                case 'left':
                                                    child.before(adSlotHtml);
                                                    break;
                                                case 'right':
                                                    child.after(adSlotHtml);
                                                    break;
                                            }
                                        }
                                        else {
                                            $(placeholder).html(adSlotHtml);
                                        }
                                    }
                                    else {
                                        $(placeholder).html(adSlotHtml);
                                    }
                                }
                                else {
                                    //console.log(`no placeholders found for ${placement}`);
                                }
                                break;
                        }
                    }
                    else {
                        //console.log(`no placement found for slot ${key}`);
                    }
                });
            };

            // Dynamically adds placeholders to content of varying length:
            this.preparePlaceholders = function () {
                // topwide, bigbox, & skin already exist.

                switch (page_type) {
                    case 'index':
                    case 'homepage':
                        // Dynamically add leftbox placeholders:
                        if ((self.$win.width() >= 768)) {
                            self._addPlaceholders($('#content-container .article-list-mode4:first .article-item:first'),
                                'floatbox', 0);
                            $('#content-container .article-list-mode4:first .article-item:first .settings-container').toggleClass('right-ad-offset', true);
                        } else {
                            self._addPlaceholders($('#content-container .fixed-height-features'),
                                'rightbox', 0, 0, 'whtaph-600');
                        }

                        // Dynamically add rightbox placeholders:
                        self._addPlaceholders($('#content-container .article-list:first .article-item'),
                            'rightbox', 5, 6, 'whtaph-600');

                        break;
                    case 'forum':
                    case 'article':
                        // topwide & bigbox already exist.                        

                        var fAvoidArticleNeghibors = function ($anchor) {
                            var $next = $anchor.next();
                            if ($next.length == 0 || $next.is('aside')) return false;
                            if ($anchor.children().first().is('small') && $anchor.prev().is('aside')) return false;
                            if ($next.children().first().is('small') && $next.next().is('aside')) return false;
                            return true;
                        };

                        // Dynamically add rightbox placeholders:
                        var sSizeClass = (self.width >= 1100) ? 'whtaph-728' : 'whtaph-300';

                        self._addPlaceholders($('.right-col .masked-content > .article-box'),
                            'rightbox', 0, 1, sSizeClass);

                        self._addPlaceholders($('.right-col .comments-box'),
                            'rightbox', 0, 1, sSizeClass);

                        self._addPlaceholders($('.right-col .article-container section'),
                            'rightbox', 0, 2, sSizeClass, fAvoidArticleNeghibors);                                       

                        // If the article starts with an image, remove the first placeholder:
                        if ($('article.article-container').first().find('section').first().children().first().is('figure')) {
                            $('article.article-container').first().find('.whtaph').first().remove();
                        }
                        break;
                }

                self.$placeholders = $('.whtaph');
            };

            // Adds placeholders to a given list using the specified interval:
            this._addPlaceholders = function ($elementList, unitType, startIndex, repeatEveryN, sizeClass, validationFunction) {
                var sWrapStart = '', sWrapEnd = '', bInitHeight = false;
                repeatEveryN = (repeatEveryN && repeatEveryN > 0) ? repeatEveryN : 100000;
                if (startIndex >= $elementList.length && $elementList.length > 0) {
                    startIndex = $elementList.length - 1;
                }
                if (startIndex < 0) startIndex = 0;
                switch (unitType) {
                    case 'leftbox':
                        bInitHeight = true;
                        sWrapStart = '<li{0}>';
                        sWrapEnd = '</li>';
                        break;
                    case 'floatbox':
                        bInitHeight = true;
                        sWrapStart = '<aside{0}>';
                        sWrapEnd = '</aside>';
                        sizeClass = (sizeClass) ? 'whtaph-floatbox ' + sizeClass : 'whtaph-floatbox';
                        break;
                    case 'rightbox':
                        bInitHeight = true;
                        sWrapStart = '<aside{0}>';
                        sWrapEnd = '</aside>';
                        sizeClass = (sizeClass) ? 'whtaph-rightbox ' + sizeClass : 'whtaph-rightbox';
                        break;
                }
                sWrapStart = sWrapStart.format((sizeClass) ? ' class="{0}"'.format(sizeClass) : '');

                // Loop through the list adding placeholders when not already present:
                for (var x = startIndex; x < $elementList.length; x += repeatEveryN) {
                    var $beforeAd = $($elementList[x]);
                    if (unitType == 'floatbox') {
                        if (String($beforeAd.prev().html()).indexOf('whtaph') == -1 && (typeof (validationFunction) != 'function' || validationFunction($beforeAd))) {
                            $beforeAd.before(sWrapStart + self.aphTemplate.format(unitType) + sWrapEnd);
                        }
                    } else {
                        if (String($beforeAd.next().html()).indexOf('whtaph') == -1 && (typeof (validationFunction) != 'function' || validationFunction($beforeAd))) {
                            $beforeAd.after(sWrapStart + self.aphTemplate.format(unitType) + sWrapEnd);
                        }
                    }
                }
            };

            // Checks to see if an article list is inactive:
            this._isArticleListInactive = function ($aphArticleList) {
                if ($aphArticleList.length > 0) {
                    var filterMode = $aphArticleList.data('filter-sort');
                    if (filterMode) {
                        var $activeToggle = $aphArticleList.closest('.content-nav').find('.list-toggler li.active');
                        if ($activeToggle.length > 0 && filterMode != $activeToggle.data('filter-sort')) {
                            return true;
                        }
                    }
                }
                return false;
            };

            // Placeholder stubs
            this.detachFrom = function (el) { };
            this.reAttachTo = function (el) { };
            this.refresh = function (els) { };
            this.clone = function (els) { };
        };
        googletag.cmd.push(function () {
            if (window.ta.gam.tags) WHT.ActivePlacements = new WHT.Placements(window.ta.gam.tags, 'homepage');
        });

    })(window.WHT = window.WHT || {});
</script>
