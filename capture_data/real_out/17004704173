<script type="text/javascript">
    document.addEventListener("DOMContentLoaded", function () {
    if(tc_vars.page_IDH.indexOf('inscription-pro')!==-1 && tc_vars.page_IDH !== 'inscription-pro-Step3'  && tc_vars.page_IDH !== 'decertification-pro-page') {
        
        var scriptUrl1 = "https://cdn.tagcommander.com/4589/tc_MireMonCompte_20.js";
        var scriptUrl2 = "//cdn.tagcommander.com/1491/tc_LaboutiqueLaPoste_3.js";
        

    } else if (tc_vars.page_IDH !== 'inscription-pro-Step3' && tc_vars.page_IDH !== 'decertification-pro-page') {
        
        var scriptUrl1 = "//cdn.tagcommander.com/1491/tc_LaboutiqueLaPoste_1.js";
        var scriptUrl3 = "//cdn.tagcommander.com/1491/tc_LaboutiqueLaPoste_23.js";
        var scriptUrl2 = "//cdn.tagcommander.com/1491/tc_LaboutiqueLaPoste_3.js";
        
    }
        var domElt = identifiedEltToDom(), isPro = tc_vars['is_pro'] == 'true' ? true : false;
        updateValueTCVars(domElt, isPro);
        var params = "";
        if (tc_vars['page_IDH'] && tc_vars['page_IDH'] != '') {
            params += 'cms_page=' + tc_vars['page_IDH'];
        }
        if (tc_vars['product_code'] && tc_vars['product_code'] != '') {
            params += '&product_code=' + tc_vars['product_code'];
        }
        if (tc_vars['category_code'] && tc_vars['category_code'] != '') {
            params += '&category_code=' + tc_vars['category_code'];
        }
        if (tc_vars['store_locator'] && tc_vars['store_locator'] != '') {
            params += '&store_locator=' + tc_vars['store_locator'];
        }

        if (tc_vars['functionnality'] && tc_vars['functionnality'] != '') {
            params += 'functionnality=' + tc_vars['functionnality'];
        }
        if (tc_vars['stepNumber'] && tc_vars['stepNumber'] != '') {
            params += '&stepNumber=' + tc_vars['stepNumber'];
        }
        // Changement des variables page_IDH et new_page_name dans tc_vars avant le chargement de tagCommander Pour la page prelevement paiment dans le cas d'un paiement différe.
        const isMonthlySepa = (document.querySelector('#sepa-mensuel') || document.querySelector('#monthly-sepa-section')) ? 'true' : 'false';
        if(isMonthlySepa == 'true') {
            tc_vars['page_IDH'] = "proPaiementDifferePage";
            tc_vars['new_page_name'] = "paiement_differe";
        }
        //the buster param is here to prevent IE from caching this ajax request, it's a workaround ;) and it's IE XD ..
        ajaxShowCart('/ajax/tagco?buster=' + new Date().getTime() + '&' + params, function (data) {
            // eval response...
            // Create the element...
            if (data) {
                try {
                    var script = document.createElement("script");
                    // Add script content
                    script.innerHTML = data;
                    // Append
                    document.head.appendChild(script);
                    // get and create global vars
                    globalUserData = userData;
                    globalTcVarsAjax = tc_vars_ajax;
                    // update the tagco variables
                    for (var attr in tc_vars_ajax) {
                        tc_vars[attr] = tc_vars_ajax[attr];
                    }
                    loadScript(scriptUrl1, function () {
                        window.dispatchEvent(createCrossEvent('tagco-loaded'));
                    });
                    if (scriptUrl3 !=='undefined'){
                        loadScript(scriptUrl3, function () {
                        });
                    }
                    loadScript(scriptUrl2, function () {
                    });
                    if(tc_vars.env_work !== 'prod'){
                        if (scriptUrl4 !=='undefined'){
                            loadScript(scriptUrl4, function () {
                            });
                        }
                    }
                } catch (error) {
                    console.error('Run ajax before loadScript TC', error);
                }
            }
        });


        $('.mtel-tooltip').hide();

    });

</script>


