    <script>
        var displayWarning = true;
        if ((typeof displayWarning == "boolean") && displayWarning) {
            var IE6 = navigator.userAgent.toLowerCase().indexOf('msie 6') != -1;
            if (IE6) {
                $('.wrapper').hide();
                $('embed, object, select').css({ 'visibility' : 'hidden' });
                $(document).find("body").css({'overflow':'hidden'});
                $('#mask').css({'width':$(document).width()-16,'height':$(document).height(), background: '#fff'}).fadeTo(100,1);
                var id = "#warnwrap";
                $(id).css({position:'absolute', 'z-index': '99'}).css('top', 10).css('left', ($(window).width()-$(id).width())/2).fadeIn(300); 
            } else {
                $('#warnfade')
                    .css({width: $(document).width(), height: $(document).height(), position: "fixed", top: 0, left: '0', 'z-index': '99', background: '#fff', margin: '0 auto'})
                    .show();
                $('#warnwrap')
                    .css({position: "fixed", top: '30px', left: parseInt($(document).width()/2 - 350) + 'px', 'z-index': '100', background: '#fff', margin: '0 auto'})
                    .show();
                $(window).resize(function () { $('#warnwrap').css({left: parseInt($(document).width()/2 - 350) + 'px'}); });
            }
            $('#warn_ent').click(function(e){ 
                e.preventDefault();
                setCookie('warning', 'agree', 365, '/', location.host);
                $(document).find("body").css({'overflow':'auto'});
                $('embed, object, select').css({ 'visibility' : 'visible' });
                $('#warnwrap,#warnfade').hide();
                $('#mask').hide();
                $('.wrapper').show();
            });
            $('#warn_ex').click(function(e){ location.href = '//www.google.com/'; });
        }
    </script>
    
