<script>
    $('[data-privacy-links-toggle]').on('click', function () {
      $(this).toggleClass('inner-svg-rotate-180');
    });

    $('[data-privacy-links-close]').on('click', function () {
      $('[data-privacy-links-toggle]').removeClass('inner-svg-rotate-180');
    })
</script>
