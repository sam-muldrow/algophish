<script>
    (function () {
        checkResolution();
        window.addEventListener('resize', checkResolution);
    })();

    function checkResolution() {
        // Guess if user is on desktop or mobile device. Based on this set and change the link.
        const linkToBlog = window.innerWidth > 800 ? "https://www.bazar.sk/blog/" : "https://m.bazar.sk/blog/";
        document.getElementsByClassName('block-items__blog--link')[0].href = linkToBlog;
    }
</script>
