    <!-- Oauth login:begin -->
    <script src="https://s.aimg.sk/bazar/js/platform/UC.Auth.v0.1.6.js" type="text/javascript"></script>
<script type="text/javascript">
    UC.Auth.init(
        new UC.Auth.Config(
            'https://user.unitedclassifieds.sk',
            'bazarsk',
            'https://www.bazar.sk/api/auth/init/'
        )
    )

    window.addEventListener('storage', event => {
        // When local storage changes, dump the list to
        // the console.
        if (event.key === 'oauth') {
            window.localStorage.removeItem('oauth');
            window.location.reload();
        }
    });

    // bind navigation
    $(() => {
        var oAuthAzet = $('[data-login-oauth-azet]');
        if (oAuthAzet.length > 0) {
            oAuthAzet.on('click', e => {
                e.preventDefault();
                UC.Auth.showLoginDialog('azet');
            })
        }

        var oAuthFacebook = $('[data-login-oauth-facebook]');
        if (oAuthFacebook.length > 0) {
            oAuthFacebook.on('click', e => {
                e.preventDefault();
                UC.Auth.showLoginDialog('facebook-bazarsk');
            })
        }
    });
</script>
    <!-- Oauth login:end -->
