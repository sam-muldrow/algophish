<script type="text/javascript">
    $(document).ready(function() {
        var actionPrivacySettings = $('[data-action-privacy-settings]');
        if (actionPrivacySettings.length > 0) {
            actionPrivacySettings.on('click', function(e) {
                e.preventDefault();
                if (typeof window._sp_ !== "undefined") {
                    window._sp_.gdpr.loadPrivacyManagerModal(916140)
                }
            })
        }
    });

    function loadScript(src, callback) {
        var script = document.createElement('script')
        script.type = 'text/javascript'
        // IE
        if (script.readyState) {
            script.onreadystatechange = function () {
                if (script.readyState === 'loaded' || script.readyState === 'complete') {
                    script.onreadystatechange = null
                    callback()
                }
            }
        }
        // Others
        else {
            script.onload = callback
        }
        script.src = src
        document.head.appendChild(script)
    }

    function enableFacebookPixel() {
        var d = new Date();
        var year = d.getFullYear();
        var month = d.getMonth();
        var day = d.getDate();
        var c = new Date(year + 1, month, day);
        document.cookie = 'fbq=1;expires=' + c.toUTCString() + ';path=' + window.COOKIE_DOMAIN;
    }

    function disableFacebookPixel() {
        document.cookie = "fbq= ; expires = Thu, 01 Jan 1970 00:00:00 GMT"
    }

    function enableLivemonitor() {
        var d = new Date();
        var year = d.getFullYear();
        var month = d.getMonth();
        var day = d.getDate();
        var c = new Date(year + 1, month, day);
        document.cookie = 'livemonitor=1;expires=' + c.toUTCString() + ';path=' + window.COOKIE_DOMAIN;
    }

    function disableLivemonitor() {
        document.cookie = "livemonitor= ; expires = Thu, 01 Jan 1970 00:00:00 GMT"
    }

    function enableNewRelic() {
        var d = new Date();
        var year = d.getFullYear();
        var month = d.getMonth();
        var day = d.getDate();
        var c = new Date(year + 1, month, day);
        document.cookie = 'new_relic=1;expires=' + c.toUTCString() + ';path=' + window.COOKIE_DOMAIN;
    }

    function disableNewRelic() {
        document.cookie = "new_relic= ; expires = Thu, 01 Jan 1970 00:00:00 GMT"
    }

    function includeGoogleAnalytics() {
        loadScript('https://s.aimg.sk/bazar/js/snippet/google/analytics.js');
    }

    function includeGoogleAdSense() {
        return;
        loadScript('https://s.aimg.sk/bazar/js/snippet/google/adsense.js')
    }

    function includeGoogleTagManager() {
        loadScript('https://s.aimg.sk/bazar/js/snippet/google/tag-manager.js')
    }

    function includeGoogleDoubleClick() {
        loadScript('https://s.aimg.sk/bazar/js/snippet/google/doubleclick.js');
    }

    function includeGoogleRemarketing() {
        loadScript('https://s.aimg.sk/bazar/js/snippet/google/remarketing.js')
    }

    function includeGemius() {
        loadScript('https://s.aimg.sk/bazar/js/snippet/gemius.js')
    }

    function includeRsz() {
        loadScript('https://s.aimg.sk/bazar/js/snippet/rsz.js')
    }

    function includeFacebookPixel() {
        loadScript('https://s.aimg.sk/bazar/js/snippet/facebook/pixel.js')
    }

    function includeHotjar() {
        loadScript('https://s.aimg.sk/bazar/js/snippet/hotjar.js')
    }

    //CAR-19825 - temporarily turn off live chat bazar.sk
    /*function includeZendesk() {
        loadScript('https://s.aimg.sk/bazar/js/snippet/zendesk.js')
    }*/
</script>

