<script>
	$(document).scroll( function() {
		var totalScroll = $(document).scrollTop();

		if (totalScroll >= 200) {
			$('[data-click=scroll-top]').addClass("show");
		} else {
			$('[data-click=scroll-top]').removeClass("show");
		}
	});

	$('[data-click=scroll-top]').click(function(e) {
		e.preventDefault();
		$('html, body').animate({
			scrollTop: $("body").offset().top
		}, 500);
	});
</script>

