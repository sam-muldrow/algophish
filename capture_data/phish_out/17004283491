    <script>
      window.fbAsyncInit = function() {
        
          if($('meta[property="fb:app_id"]').length) {
            FB.init({
              appId      : $('meta[property="fb:app_id"]').attr('content'),
              xfbml      : true,
              version    : 'v2.5'
            });
          } else {
            FB.init({
              xfbml      : true,
              version    : 'v2.5'
            });
          }
        
      };

      (function(d, s, id){
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) {return;}
        js = d.createElement(s); js.id = id;
        js.src = "//connect.facebook.net/pt_BR/sdk.js";
        fjs.parentNode.insertBefore(js, fjs);
      }(document, 'script', 'facebook-jssdk'));
    </script>

    
    
<script>
  var sendMetrics = function(event) {
    try {
      var url = '/_events/api/setEvent';

      var now = new Date();

      var data = {
        request: {
          id: uuidv4(),
          environment: 'production'
        },
        store: {
          id: 1855578,
          name: 'Ana Rosa Moda Feminina &amp; Acessórios',
          test_account: false
        },
        device: {
          is_mobile: /Mobi/.test(window.navigator.userAgent),
          user_agent: window.navigator.userAgent,
          ip: '10.0.223.44'
        },
        page: {
          host: window.location.hostname,
          path: window.location.pathname,
          search: window.location.search,
          type: 'home',
          title: document.title
        },
        timestamp: now.toISOString(),
        event
      };
      
      if (window.performance) {
        var [timing] = window.performance.getEntriesByType('navigation');

        data['time'] = {
          server_response: Math.round(timing.responseStart - timing.requestStart)
        };
      }

      var guest_email = "",
          user_email = "";

      if (guest_email) {
        data['user'] = {
          is_guest: true,
          email: guest_email
        };
      } else if (user_email) {
        data['user'] = {
          is_guest: false,
          email: user_email
        };
      }

      try {
        var session_identifier = $.cookie("li_session_identifier");
        if (!session_identifier) {
          session_identifier = uuidv4();
          $.cookie('li_session_identifier', session_identifier, {
            path: '/'
          });
        };
        data['session'] = {
          id: session_identifier
        };
      } catch (err) { }

      try {
        var ab_test_cookie = $.cookie('li_ab_test_running');
        if (ab_test_cookie) {
          var running_tests = JSON.parse(atob(ab_test_cookie));
          if (running_tests.length) {
            data['ab_test'] = running_tests;
          }
        }
      } catch (err) { }

      var utm = {};

      $.each(sessionStorage, function(key, value) {
        if (key.startsWith('utm_')) {
          var name = key.split('_')[1];

          utm[name] = value;
        }
      });

      if (!$.isEmptyObject(utm)) {
        data['session']['utm'] = utm;
      }

      var controller = new AbortController();

      setTimeout(function() {
        controller.abort();
      }, 1000);

      fetch(url, {
        keepalive: true,
        method: 'POST',
        headers: {
          'Content-Type': 'application/json'
        },
        body: JSON.stringify({ data }),
        signal: controller.signal
      });
    } catch (err) { }
  }
</script>

    
<script>
  (function() {
    var initABTestHandler = function() {
      try {
        if ($.cookie('li_ab_test_running')) {
          return
        };
        var running_tests = [];

        
        

        var running_tests_to_cookie = JSON.stringify(running_tests);
        running_tests_to_cookie = btoa(running_tests_to_cookie);
        $.cookie('li_ab_test_running', running_tests_to_cookie, {
          path: '/'
        });

      } catch (err) { }
    }
    setTimeout(initABTestHandler, 500);
  }());
</script>

    
<script>
  $(function() {
    // Clicar em um produto
    $('.listagem-item').click(function() {
      const body = {
        item_id: $(this).attr('data-id'),
        item_name: $(this).find('.nome-produto').text()
      };

      sendMetrics({
        name: 'select_product',
        data: body
      });

      $(document).trigger('li_select_product', [body]);
    });

    // Clicar no WhatsApp
    $('.li-whatsapp a').click(function() {
      const value = 'WhatsApp';

      sendMetrics({
        name: 'start_contact',
        data: value
      });

      $(document).trigger('li_start_contact', [value]);
    });

    
      // Visualizar a página inicial
      sendMetrics({
        name: 'view_home'
      });

      $(document).trigger('li_view_home');
    
  });
</script>

    


  
  
    <script type="text/javascript">
      if('serviceWorker' in navigator) {
        window.addEventListener('load', function() {
          navigator.serviceWorker.register('/service-worker.js');
        });
      }
    </script>
  


