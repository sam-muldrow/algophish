            <script src="/Javascript/ie10-viewport-bug-workaround.js" type="text/javascript"></script>
            <script type="text/javascript">
                BindScroll();
                $(window).resize(function () {
                    BindScroll();
                });
                function BindScroll() {
                    if ($(window).width() < 1024) {
                        $(window).scroll(function () {
                            if ($(window).width() + $(this).scrollLeft() < 1024) {
                                $(".navbar-spread").css("left", 0 - $(this).scrollLeft());
                                //$(".navbar-spread-bottom").css("left", 0 - $(this).scrollLeft());
                            }
                        });
                    } else {
                        $(".navbar-spread").css("left", 0);
                        //$(".navbar-spread-bottom").css("left", 0);
                    }
                }
                $(function () {

                    function resizeHeadWidth() {
                        blowHeadHeight = $(".headbutton").outerHeight() + 20;
                        $(".blowheaddiv").height(blowHeadHeight + 'px');
                    }

                    if ($(document).width() < 888) {
                        resizeHeadWidth();
                    }

                    $(window).resize(resizeHeadWidth);
                });
            </script>
        
