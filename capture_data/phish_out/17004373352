			$(window).scroll(function () {
				var sticky = $('.headhesive--clone'),
      scroll = $(window).scrollTop();

				if (scroll >= 100) sticky.addClass('headhesive--stick');
				else {
					sticky.removeClass('headhesive--stick');
				}
