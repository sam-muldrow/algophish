    <script language="JavaScript" type="text/javascript">
        function DisableScrolling() {
            $("body").css({ "overflow": "hidden" });
        }

        function toggle() {
            var oDiv = document.getElementById('fxm');
            var oBtn = document.getElementById('tst');
            if (oBtn.value == 'SHOW') {
                oDiv.style.display = 'block';
                oBtn.value = 'HIDE'
            }
            else {
                oDiv.style.display = 'none'
                oBtn.value = 'SHOW'
            }
        }


        function toggleIE11BrowserMessage() {
            try {
                $('#IE11BrowserMessage').css('display', 'none');
            } catch (exception) { }
        }
        $(document).ready(function () {
            try {
                if (!!navigator.userAgent.match(/Trident.*rv\:11\./)) {
                    $('#IE11BrowserMessage').removeAttr("style", "display:none;");
                } else {
                    $('#IE11BrowserMessage').css('display', 'none');
                }
            } catch (exception) { }


            const togglePassword = $("#togglePassword");
           

            togglePassword.click(function () {
                let password = $("#ctl00_cphMain_txtPassword");

                // toggle the type attribute
                var nextType = password.attr("type") === "password" ? "text" : "password";

                var newPassword = $("<input type='" + nextType + "' />").attr({ name: password.attr("name"), value: password.val(), class: password[0].className, id: "ctl00_cphMain_txtPassword" }).insertBefore("#ctl00_cphMain_txtPassword");
                password.remove();

                // toggle the icon
                $(this).toggleClass("bi-eye");
            });
        });
    </script>
