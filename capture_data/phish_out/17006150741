<script type="text/javascript">
var abstandRechts = "-7.4em";
var counterK = 0;
var counterF = 0;
document.querySelector('.floating_kontakt > a > span').style.display = "none";
document.querySelector('.floating_kontakt').style.right = abstandRechts;

document.querySelector('.floating_fernwartung > a > span').style.display = "none";
document.querySelector('.floating_fernwartung').style.right = abstandRechts;


var kontakt = false;
var fernWartung = false;

document.querySelector('.floating_kontakt > a > img').setAttribute("onclick", "show1()");
document.querySelector('.floating_fernwartung > a > img').setAttribute("onclick", "show2()");

document.querySelector('.floating_kontakt > a > img').clickOutsideThisElement = function () {hideBtn();};
document.querySelector('.floating_fernwartung > a > img').clickOutsideThisElement = function () {hideBtn();};

function show1() {
  if (kontakt == false) {
    document.querySelector('.floating_kontakt > a > span').style.display = "inline";
    document.querySelector('.floating_kontakt').style.right = "0px";
     document.querySelector('.floating_kontakt > a > img').removeAttribute("onclick");
     document.querySelector('.floating_kontakt > a').setAttribute("onclick", "linkKontakt()");
    kontakt = true;
  } else if (kontakt == true) {
    document.querySelector('.floating_kontakt > a > span').style.display = "none";
    document.querySelector('.floating_kontakt').style.right = abstandRechts;
    kontakt = false;
  }
}

function show2() {

  if (fernWartung == false) {
    document.querySelector('.floating_fernwartung > a > span').style.display = "inline";
    document.querySelector('.floating_fernwartung').style.right = "0px";
    document.querySelector('.floating_fernwartung > a > img').removeAttribute("onclick");
    document.querySelector('.floating_fernwartung > a').setAttribute("onclick", "linkFernwartung()");
    fernWartung = true;
  } else if (fernWartung == true) {
    document.querySelector('.floating_fernwartung > a > span').style.display = "none";
    document.querySelector('.floating_fernwartung').style.right = abstandRechts;
    fernWartung = false;
  }
}

function linkKontakt() {
  if (counterK == 1) {
    location.href = "https://www.ipexx-systems.de/unternehmen/so-erreichen-sie-uns/";
    counterK = 0;
  } else {
    counterK = 1;
  }
}

function linkFernwartung() {
  if (counterF == 1) {
    location.href = "https://www.ipexx-systems.de/it-service/";
    counterF = 0;
  } else {
    counterF = 1;
  }
}

function hideBtn() {
  counterK = 0;
  counterF = 0;
 document.querySelector('.floating_fernwartung > a > span').style.display = "none";
 document.querySelector('.floating_fernwartung').style.right = abstandRechts;
 fernWartung = false;

 document.querySelector('.floating_kontakt > a > span').style.display = "none";
 document.querySelector('.floating_kontakt').style.right = abstandRechts;
 kontakt = false;
 document.querySelector('.floating_kontakt > a > img').setAttribute("onclick", "show1()");
document.querySelector('.floating_fernwartung > a > img').setAttribute("onclick", "show2()");
}
</script>

