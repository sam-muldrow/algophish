        if(!data) data = $this.attr('id');
 
    }
 
    gtag('event', 'click', { event_category: 'Homepage_Slider', event_action: 'click', event_label: data});
 
}); 
			
		}} // End of RevInitScript

		if (window.RS_MODULES.checkMinimal!==undefined) { window.RS_MODULES.checkMinimal();};</script> <script defer src="data:text/javascript;base64,"></script> <script defer src="data:text/javascript;base64,"></script> <script defer src="data:text/javascript;base64,"></script> </body></html>