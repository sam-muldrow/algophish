        <script>
        $(function() {
            $.ajax({
                type  : 'POST',
                url   : 'https://www.aepthailand.com/administrator/api/slider',
                async : false,
                dataType : 'json',
                success : function(data){
                    $('#bg-slide').vegas({
                        delay: 7000,
                        shuffle: true,
                        slides: data,
                        walk: function(s, e){
                            s = null, $(".menu-topbar").find("a").css("color", e.color),$(".menu-topbar").find("li").css("color", e.color), $(".main-nav").find("li").css("color", e.color), $(".menu-a").css("color", e.color),$(".slogan").css("color", e.color)
                        },
                        transitionDuration: 3500,
                        transition: ['fade','fade2']
                    });
                }
            });
        });
        </script>
