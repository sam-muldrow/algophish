  $('.right_bar li').eq(1).hover(function() {
    $(this).stop().find('.rtbar_shwx').show();
  }
                                 , function() {
    $(this).stop().find('.rtbar_shwx').hide();
  }
                                );
  $("div[id^='c_']").attr("loaded","true");
</script>

