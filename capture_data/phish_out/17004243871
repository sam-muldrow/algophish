.categories{
	list-style-type: none;
}
.categories li{
	    padding: 15px;
    border-bottom: 1px solid #ddd;
}
.wppsac-post-slider .slick-slide {
    height: 450px;
}
.wppsac-post-slider .wppsac-post-image-bg img {
  
    height: 450px !important;
}		</style>
		</head>
