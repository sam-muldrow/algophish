<script>
const button = document.querySelector(".svg-qust"),
  toast = document.querySelector(".toast");
(closeIcon = document.querySelector(".close")),
  (progress = document.querySelector(".progress"));

let timer1, timer2;

button.addEventListener("click", () => {
  toast.classList.add("active");
  progress.classList.add("active");

  timer1 = setTimeout(() => {
    toast.classList.remove("active");
  }, 5000); //1s = 1000 milliseconds

  timer2 = setTimeout(() => {
    progress.classList.remove("active");
  }, 5300);
});

closeIcon.addEventListener("click", () => {
  toast.classList.remove("active");

  setTimeout(() => {
    progress.classList.remove("active");
  }, 300);

  clearTimeout(timer1);
  clearTimeout(timer2);
});
</script>
<script>
$('.btn-inst').click(function(){
    var inst = $(this).data('inst')
    var link = $(this).data('reflink')
    document.getElementById('textINST').innerHTML = inst
    document.getElementById('hrefRef').href=("href", link)
    return false
})
</script>
<script>
    $(document).ready(function(){
        $('button.btn-act-promo').on('click', function(){
          var promoValue = document.querySelector('.inpprom').value;
            $.ajax({
                method: 'POST',
                url: "promocodes.php",
                data: {promo : promoValue},
                success: function(result){
                  document.querySelector('.inpprom').value = '';
                  if (result == 1){
                    document.getElementById('msssd').innerHTML = 'Активации закончились!'
                    document.getElementById('changeColor').style.background = '#ffdb9b'
                    document.getElementById('changeColor').style.borderLeft = '8px solid #ffa502'
                    document.getElementById('changeColor').style.color = '#ce8500'
                    document.getElementById('cccccc').style.color = '#ce8500'
                    document.getElementById('ccccccc').style.color = '#ce8500'
                    document.getElementById('msssd').style.color = '#ce8500'
                    document.getElementById('cls').style.background = '#ffd080'
                    $('.alert').addClass("show");
                    $('.alert').removeClass("hide");
                    $('.alert').addClass("showAlert");
                    setTimeout(function(){
                      $('.alert').removeClass("show");
                      $('.alert').addClass("hide");
                    },5000);
                    $('.close-btn').click(function(){
                    $('.alert').removeClass("show");
                    $('.alert').addClass("hide");
                    });
                  }
                  if (result == 2){
                    document.getElementById('msssd').innerHTML = 'Вы уже активировали этот код!'
                    document.getElementById('changeColor').style.background = '#ffdb9b'
                    document.getElementById('changeColor').style.borderLeft = '8px solid #ffa502'
                    document.getElementById('changeColor').style.color = '#ce8500'
                    document.getElementById('cccccc').style.color = '#ce8500'
                    document.getElementById('ccccccc').style.color = '#ce8500'
                    document.getElementById('msssd').style.color = '#ce8500'
                    document.getElementById('cls').style.background = '#ffd080'
                    $('.alert').addClass("show");
                    $('.alert').removeClass("hide");
                    $('.alert').addClass("showAlert");
                    setTimeout(function(){
                      $('.alert').removeClass("show");
                      $('.alert').addClass("hide");
                    },5000);
                    $('.close-btn').click(function(){
                    $('.alert').removeClass("show");
                    $('.alert').addClass("hide");
                    });
                  }
                  if (result == 3){
                    document.getElementById('msssd').innerHTML = 'Промокод применен!'
                    document.getElementById('changeColor').style.background = '#9bffa6'
                    document.getElementById('changeColor').style.borderLeft = '8px solid #00ff40'
                    document.getElementById('changeColor').style.color = '#007523'
                    document.getElementById('cccccc').style.color = '#007523'
                    document.getElementById('ccccccc').style.color = '#007523'
                    document.getElementById('msssd').style.color = '#007523'
                    document.getElementById('cls').style.background = '#00ff40'
                    $('.alert').addClass("show");
                    $('.alert').removeClass("hide");
                    $('.alert').addClass("showAlert");
                    setTimeout(function(){
                      $('.alert').removeClass("show");
                      $('.alert').addClass("hide");
                    },5000);
                    $('.close-btn').click(function(){
                    $('.alert').removeClass("show");
                    $('.alert').addClass("hide");
                    });

                    $.ajax({
                      method: 'POST',
                      url: "openSkin.php",
                      data: {yes : "yes"},
                      success: function(skin){
                        var obj = JSON.parse(skin);
                        $("#case1").addClass('case1-st');
                        setTimeout(() => { document.getElementById('case1').src=obj[0]['img']; }, 1000);
                      }
                    })
                  }
                  if (result == 4){
                    document.getElementById('msssd').innerHTML = 'Промокод не найден!'
                    document.getElementById('changeColor').style.background = '#ffdb9b'
                    document.getElementById('changeColor').style.borderLeft = '8px solid #ffa502'
                    document.getElementById('changeColor').style.color = '#ce8500'
                    document.getElementById('cccccc').style.color = '#ce8500'
                    document.getElementById('ccccccc').style.color = '#ce8500'
                    document.getElementById('msssd').style.color = '#ce8500'
                    document.getElementById('cls').style.background = '#ffd080'
                    $('.alert').addClass("show");
                    $('.alert').removeClass("hide");
                    $('.alert').addClass("showAlert");
                    setTimeout(function(){
                      $('.alert').removeClass("show");
                      $('.alert').addClass("hide");
                    },5000);
                    $('.close-btn').click(function(){
                    $('.alert').removeClass("show");
                    $('.alert').addClass("hide");
                    });
                  }
                  if (result == 10){
                    document.getElementById('msssd').innerHTML = 'Выполните все условия (в профиле)!'
                    document.getElementById('changeColor').style.background = '#ffdb9b'
                    document.getElementById('changeColor').style.borderLeft = '8px solid #ffa502'
                    document.getElementById('changeColor').style.color = '#ce8500'
                    document.getElementById('cccccc').style.color = '#ce8500'
                    document.getElementById('ccccccc').style.color = '#ce8500'
                    document.getElementById('msssd').style.color = '#ce8500'
                    document.getElementById('cls').style.background = '#ffd080'
                    $('.alert').addClass("show");
                    $('.alert').removeClass("hide");
                    $('.alert').addClass("showAlert");
                    setTimeout(function(){
                      $('.alert').removeClass("show");
                      $('.alert').addClass("hide");
                    },5000);
                    $('.close-btn').click(function(){
                    $('.alert').removeClass("show");
                    $('.alert').addClass("hide");
                    });
                  }
                }
            })
        })
    })
</script>
