<script>
    function limit_text($description, $limit) {
    if (str_word_count($description, 0) > $limit) {
        $words = str_word_count($description, 2);
        $pos   = array_keys($words);
        $text  = substr($description, 0, $pos[$limit]) . '...';
    }
    return $description;
}
</script>
