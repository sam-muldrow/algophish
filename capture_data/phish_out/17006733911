        .dropdown-menu, #cell li {
          background-color: transparent !important;
        }
        #cell {
            list-style: none inside;
            margin: 0;
            padding: 0;
            text-align: center;
        }

        #cell li {
            display: block;
            position: relative;
            float: left;
            background: #2879d7;
            /* menu background color */
        }

        #cell li a {
            display: block;
            padding: 0;
            text-decoration: none;
            width: 200px;
            /* this is the width of the menu items */
            line-height: 48px;
            /* this is the hieght of the menu items */
            color: #ffffff;
            /* list item font color */
        }

        #cell li li a {
            font-size: 13px;
            padding: 8px 20px;
            /* this is the width of the menu items */
            line-height: 25px;
        }

        /* smaller font size for sub menu items */

        #cell li:hover {
            background: #2879d7;
        }

        /* highlights current hovered list item and the parent list items when hovering over sub menues */

        #cell ul {
            position: absolute;
            padding: 0;
            left: 0;
            display: none;
            /* hides sublists */
        }

        #cell li:hover ul ul {
            display: none;
        }

        /* hides sub-sublists */

        #cell li:hover ul {
            display: block;
        }

        /* shows sublist on hover */

        #cell li li:hover ul {
            display: block;
            /* shows sub-sublist on hover */
            margin-left: 200px;
            /* this should be the same width as the parent list item */
            margin-top: -35px;
            /* aligns top of sub menu with top of list item */
        }
