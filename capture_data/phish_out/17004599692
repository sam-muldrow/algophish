	// vehicle_models_listing
	// Filter vehicles by category
	$('.model_category').click(function(e) {
		e.preventDefault();
        //debugger;
        var sliderComponent = $(this).parents('.vehicle_models_listing_wrapper');

        $(this).parent('.model_categories_wrapper:not(.make_wrapper)').toggleClass("dropdown-open").toggleClass("dropdown-close");

        var category_id = $(this).data('category');

        sliderComponent.find('.model_category.active').removeClass('active');

        sliderComponent.find('.models_list').addClass('d-none').removeClass('active');

		var active_model_list = sliderComponent.find('.models_list[data-category=' + category_id + ']');
        active_model_list.addClass('active');
        active_model_list.removeClass('d-none');

		//Append the active category text to fake category to simulate dropdown list
        sliderComponent.find('.model_category.active').removeClass('active');
        var active_model_category = sliderComponent.find('.model_category_' + category_id);
        active_model_category.addClass('active');

		active_model_category.parent().parent().find(".selected span").text(active_model_category.first().text());

		var active_models_list = sliderComponent.find('.models_list[data-category="'+ category_id +'"]');

		// Already initiated on the previous list(s), destroy it in order to re initiate it on the actual(s) one(s)
		initialized_sliders = sliderComponent.find(".slick-initialized");
		if (initialized_sliders.length > 0) {
			initialized_sliders.each(function() {
				//$(this).slick('unslick')
			});
		}

		var active_models_list_slider = active_models_list.find(".slick");
		
		//check if the slider is enabled or not
		active_models_list.each(function(a, i) {
            var slick_slider = $(this).find(".slick");
            var slick_slider_mobile = $(this).find(".slick_mobile");

            if (slick_slider.length > 0) {
                var arrows = $(this).find(".dealer_tabs_arrows")[0];
                var dots = $(this).find(".dealer_tabs_arrows .dealer_tabs_dots")[0];
                slideModels(slick_slider, arrows, dots);
            }
            else if (slick_slider_mobile.length > 0) {
                var arrows = $(this).find(".dealer_tabs_arrows")[0];
                var dots = $(this).find(".dealer_tabs_arrows .dealer_tabs_dots")[0];
                slideModelsOnMobile(slick_slider_mobile, arrows, dots);
            } else {
                $(this).find('.model:gt(4)').hide(); // Limit display 5
            }
		});
	});
	if ($(window).width()<768) {
		//Preselect first element to hide the categories dropdown by default
		$('.model_categories .model_category:first-child').trigger('click');
	}

	//We could also have a listing without categories
	$( ".vehicle_models_listing_wrapper" ).each(function() {
		var $model_categories = $(this).find(".model_categories");

        //no categories
		if ($model_categories.length == 0){
			var slick_slider = $(this).find(".slick");
			var slick_slider_mobile = $(this).find(".slick_mobile");
			if (slick_slider.length > 0) {
				var arrows = $(this).find(".dealer_tabs_arrows")[0];
				var dots = $(this).find(".dealer_tabs_arrows .dealer_tabs_dots")[0];
				slideModels(slick_slider, arrows, dots);
			} else if (slick_slider_mobile.length > 0) {

				var arrows = $(this).find(".dealer_tabs_arrows")[0];
				var dots = $(this).find(".dealer_tabs_arrows .dealer_tabs_dots")[0];
				slideModelsOnMobile(slick_slider_mobile, arrows, dots);

			}
		}
	});

    function slickLimitDots($slick) {
        $slick.on('beforeChange', function(event, slick, currentSlide, nextSlide) {
            if (!slick.$dots) {
                return;
            }

            const $dots = slick.$dots.find('.dot');

            const diffEnd = nextSlide - 3 < 0 ? Math.abs(nextSlide - 3) : 0;
            const diffBegin = nextSlide + 4 > $dots.length ? Math.abs($dots.length - nextSlide - 4) : 0;

            $dots.each(function(index, node) {
                const isVisible = index > nextSlide - 4 - diffBegin && index < nextSlide + 4 + diffEnd ;
                $(node).parent().toggle(isVisible);
            });
        });
    }


	function slideModels(elem, arrows, dots) {
		const slickApi = elem.not('.slick-initialized').slick({
			infinite: true,
			slidesToShow: 5,//actual_slides > 5 ? 5 : actual_slides,
			slidesToScroll: 1,
			prevArrow:"<a class='slick-prev'><i class='fas fa-chevron-left'></i></a>",
			nextArrow:"<a class='slick-next'><i class='fas fa-chevron-right'></i></a>",
			customPaging : function(slider, i) {
				return '<a class="dot" data-index="' + (i+1) + '">' + (i+1) + '</a>';
			},
			appendDots: dots,
			appendArrows: arrows,
            responsive: [
                {
                    breakpoint: 1400,
                    settings: {
                        centerMode: true,
                        centerPadding: '10%',
                        slidesToShow: 4,
                        slidesToScroll: 1,
                    }
                },
                {
                    breakpoint: 1200,
                    settings: {
                        centerMode: true,
                        centerPadding: '10%',
                        slidesToShow: 3,
                        slidesToScroll: 1,
                    }
                },
                {
                    breakpoint: 1024,
                    settings: {
                        centerMode: true,
                        centerPadding: '10%',
                        slidesToShow: 2,
                        slidesToScroll: 1,
                    }
                },
                {
                    breakpoint: 768,
                    settings: {
                        centerMode: true,
                        centerPadding: '10%',
                        slidesToShow: 1,
                        slidesToScroll: 1,
                        dots: true,
                        arrows: true,
                    }
                }
            ]
		});
        slickLimitDots(slickApi);

        //Avoiding a buggy slider width since the list was hidden
		elem.slick("refresh");
	}

	function slideModelsOnMobile(elem, arrows, dots) {
		const slickApi = elem.not('.slick-initialized').slick({
			slidesToShow: 1,
			slidesToScroll: 1,
			prevArrow:"<a class='slick-prev'><i class='fas fa-chevron-left'></i></a>",
			nextArrow:"<a class='slick-next'><i class='fas fa-chevron-right'></i></a>",
			customPaging : function(slider, i) {
				return '<a class="dot">' +(i+1) + '</a>';
			},
			appendDots: dots,
			appendArrows: arrows,
			mobileFirst: true,
			dots: true,
			arrows: true,
			responsive: [
				{
						breakpoint: 768,
						settings: 'unslick'
				}
			]
		});

        slickLimitDots(slickApi);

		//Avoiding a buggy slider width since the list was hidden
		if ($(window).width()<768) {
			elem.slick("refresh");
		}
	}

	//If desktop and No tabs we want to display all categories otherwise only the first

    function selectCategoryLink() {
        var selectedCategoryLink = $('.model_category').first();
        if (document.location.hash) {
            var selectedCategory = document.location.hash.substring(1);
            var catId = $("h2[id='" + selectedCategory + "' i]").data('category');
            if (catId) {
                selectedCategoryLink = $("a.model_category[data-category='" + catId + "']");
            }
        }

        if ($(window).width() > 767) {
            $('.model_category:not(.no_tabs)').first().trigger('click');
        } else {
            selectedCategoryLink.first().trigger('click').trigger('click');
        }
    }

    window.addEventListener('hashchange', selectCategoryLink);

    selectCategoryLink();

	//Activate first tab on home page
    $('.home .vehicle_models_listing_wrapper').each(function(i) {
        $(this).find('.model_category').first().trigger('click');
    });

	//Tabs on mobile
	$('.fake_category, .news_fake_category').click(function(e) {
		$('.model_categories_wrapper:not(.make_wrapper)').toggleClass("dropdown-open").toggleClass("dropdown-close");
	});

	//News categories dropdown on mobile
	$('.news_fake_category').click(function(e) {
        $('.model_categories_wrapper').toggleClass("dropdown-open").toggleClass("dropdown-close");
	});

	if ($(window).width()<768) {
		$('.news_fake_category').trigger('click');
	}

	//Setting sidebar parrent height
	$( document ).ready(function() {
		//Sidebar height may change even after the doc is ready (external widget)
		$('body').bind('DOMSubtreeModified', function () {
			AdjustSidebarParrentHeight();
		});
		AdjustSidebarParrentHeight();
	});
	function AdjustSidebarParrentHeight() {
		if ( $( ".sidebar" ).length ) {
			sidebar_height = $( ".sidebar" ).height();
			parent_height = $( ".sidebar" ).parent().outerHeight();
			if (parent_height < sidebar_height) {
				//$( ".sidebar" ).parent().height(sidebar_height);
				$( ".sidebar" ).parent().css("min-height", sidebar_height+"px")
			}
		}
	}
	if (!phpOptions.isFrontPage) {
      var menu_only_sticky = $('#main_menu').hasClass('only_menu_sticky');
      if (menu_only_sticky) {
        var full_header_bottom;
        var menu_top;
        var window_height;
        var window_width;
        var is_header_scrolled = false;

        function updateScroll() {
          full_header_bottom = $('#main_header').outerHeight();
          // Menu sticks to top
          var scroll_top = $(window).scrollTop();
          var current_top = menu_top - scroll_top;

          if (!is_header_scrolled && scroll_top > full_header_bottom) {
            $('#main_header').addClass('scrolled');
            $('#main_menu').addClass('scrolled');
            is_header_scrolled = true;
          } else if (is_header_scrolled && scroll_top <= full_header_bottom) {
            $('#main_header').removeClass('scrolled');
            $('#main_menu').removeClass('scrolled');
            is_header_scrolled = false;
          }
        }

        $(window).scroll(updateScroll);
        updateScroll();
        $(window).resize(updateScroll);
      }
    }

	var disposition_2 = $('#main_header').hasClass('disposition_2');
	var header_only_sticky = $('#main_header').hasClass('only_menu_sticky');
	if(disposition_2 && header_only_sticky){
		var menu_top;
		var window_height;
		var window_width;
		var is_menu_stuck = false;
		function updateScroll()
			{
				topbar_height = $('#main_header .topbar').outerHeight();
				// Menu sticks to top
				var scroll_top = $(window).scrollTop();
				var current_top = menu_top - scroll_top;

				if ( ! is_menu_stuck && scroll_top >= topbar_height ) {
					$('#main_header').addClass('scroll_stuck');
					$('#main_menu').addClass('scroll_stuck');
					is_menu_stuck = true;
				}
				else if ( is_menu_stuck && scroll_top < topbar_height ) {
					$('#main_header').removeClass('scroll_stuck');
					$('#main_menu').removeClass('scroll_stuck');
					is_menu_stuck = false;
				}
				}
			$(window).scroll( updateScroll );
			updateScroll();
			$(window).resize( updateScroll );
	}

	//Readapting departement width based on window's width
	function updateDepartementsPosition()
	{
		if ($(window).width()>767) {
			//Get the site container margin and padding values
			var paddT = $('.header_content').innerWidth() - $('.header_content').width();
			var margT = $('.header_content').outerWidth(true) - $('.header_content').outerWidth();
			//Departments margin left value is half the container margin + padding
			var margin = (paddT / 2) + (margT / 2);
			$(".departments_wrapper .departments").css("margin-left", margin + 'px'); 

			//Caluclate the Departments width based on the body and map width
			const widthBody = $('body').width();
			const widthContainerFluid = $('body .container-fluid:visible').first().width();
			const oneForth = widthContainerFluid / 4;

			$('.departments_wrapper .departments').each(function() {
				var $parent = $(this).parent();
				var $mapElement = $parent.find('.location_map');
				const availableSpace = widthBody - margin;
				if($parent.hasClass("three_departments")) {
					const c = ($(window).width()>1199) ? 3 : 1.5;
					$(this).css("width", (oneForth * c) + "px");
					$mapElement.css("width", (widthBody - margin - (oneForth * c)) + "px");
				} else if ($parent.hasClass("two_departments")) {
					const c = ($(window).width()>1199) ? 2 : 2.5;
					$(this).css("width", (oneForth * c) + "px");
					$mapElement.css("width", (widthBody - margin - (oneForth * c)) + "px");
				} else {
					const c = ($(window).width()>1060) ? 1.5 : 2;
					$(this).css("width", (oneForth * c) + "px");
					$mapElement.css("width", (widthBody - margin - (oneForth * c)) + "px");
				} 
			});
		} else {
			//remove inline css
			$(".departments_wrapper .departments").css("margin-left","");
			$(".departments_wrapper .departments").css("width","");
		}


	}
	$(window).resize(function(){
		updateDepartementsPosition();
	});
	//Trigger click on departement tabs if branches select input exists
	if ( $( "select[name='branch-id']" ).length && $( ".departments_tabs" ).length ) {
		$( "select[name='branch-id']" ).on('change', function(e){
			const branch_id = $(this).val();
			$('.departments_tabs .nav-link[href="#branch-' + branch_id + '"]').trigger('click');
		});
	}
	//Handle dropdown branches
	// Show dropdown menu on click
	$('.departments_tabs .dropdown-toggle').on('click', function() {
		$(this).next('.dropdown-menu').toggle();
	});
	// Hide dropdown menu on click outside
	$(document).on('click', function(e) {
		if (!$(e.target).is('.dropdown-toggle') && !$(e.target).is('.departments_tabs .dropdown-menu *')) {
		$('.departments_tabs .dropdown-menu').hide();
		}
	});
	// Select dropdown option and close dropdown menu
	$('.departments_tabs .dropdown-item').on('click', function() {
		var value = $(this).text();
		var link = $(this).attr("href");
		$('.departments_tabs .dropdown-menu .dropdown-item').removeClass("active");
		$(this).addClass("active");
		$(this).closest('.dropdown').find('.dropdown-toggle').html(value + ' <span class="caret"></span>');
		$('.departments_tabs .nav-link[href="' + link + '"]').trigger('click');
		$('.departments_tabs .dropdown-menu').hide();
		return false;
	});

	$('.readmore-toggle').on('click', function(e){
		e.preventDefault();

		$target = $(this).attr('data-target');

		$('#' + $target).slideDown();

		$(this).addClass('d-none');
		$('.readless-toggle[data-target="' + $target + '"]').removeClass('d-none');
	});

	$('.readless-toggle').on('click', function(e){
		e.preventDefault();

		$target = $(this).attr('data-target');

		$('#' + $target).slideUp();

		$(this).addClass('d-none');
		$('.readmore-toggle[data-target="' + $target + '"]').removeClass('d-none');
	});


  // MAnifacturer filters
  $('.manifacturer-filters a').on('click', function(){
	const filter_type = $(this).hasClass("category") ? "category" : "make";
	
	if($(this).hasClass("active")) {
    	$(this).removeClass('active');
		$(`.manifacturer-filters.${filter_type} a[data-${filter_type}="all"]`).addClass('active');
	} else {
		$(`.manifacturer-filters.${filter_type} a`).removeClass('active');
    	$(this).addClass('active');
	}

	$(`.manifacturer-filters.${filter_type} .fake_category`).find("span").text($(`.manifacturer-filters.${filter_type} a.active`).text());
    
	var make = $(`.manifacturer-filters.make a.active`).data('make');
    var category = $(`.manifacturer-filters.category a.active`).data('category');
    console.log(make, category);

	const category_selector = (!category || category == 'all') ? `[data-category]` : `[data-category*="${category}"]`;
	const make_selector = (!make || make == 'all') ? `[data-make]` : `[data-make="${make}"]`;

	const models_container = $('.vehicle_models_listing_container');
	var initialized_sliders = models_container.find(".slick-initialized");
	if (initialized_sliders.length > 0) {
		initialized_sliders.each(function() {
			$(this).slick('unslick');
			$(this).slick('slickUnfilter');
			$(this).slick('slickFilter',`.model${category_selector}${make_selector}`).slick('refresh');
			$(this).slick('slickGoTo', 0);
		});
	} else {
		$('.vehicle_models_listing_container .model').removeClass("d-flex").addClass("d-none");
   	 	$(`.vehicle_models_listing_container .model${category_selector}${make_selector}`).removeClass("d-none").addClass("d-flex");
	}

	$(`.manifacturer-filters.${filter_type} .model_categories_wrapper`).toggleClass("dropdown-open").toggleClass("dropdown-close");

	return false;
  });

  //Venobox gallery in VI
  $( document ).ready(function() {
	if (typeof VenoBox !== 'undefined') {
		new VenoBox({
			selector: ".venobox-img",
			numeration: true,
		});
	}
  });

    //Complianz custom triggers to open the widget
  jQuery(document).ready(function($) {
		$(".js-complianz-open-widget").on('click', function(e) {
			e.preventDefault();
			$('.cmplz-manage-consent').trigger('click');
		});
		if (window.location.hash === '#cookie-settings') {
			$('.cmplz-manage-consent').trigger('click');
		}
  });
  });


$(window).on("load", function () {
  // Banners slider
	if ($.fn.slick) {
		$(".banner_wrapper.slick .carousel-inner").slick({
