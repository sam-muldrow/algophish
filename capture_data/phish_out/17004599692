	jQuery(function($) {

        window.toggleActionBar = function(name) {
          $('.content li:visible', '#fixed_action_bar').not(name).slideUp();

          $('.toggle_fixed_action_bar.active').removeClass('active');

          if ( $(name).is(':visible') ) {
            $(name).slideUp(200);
            $('.toggle_fixed_action_bar[href="'+name+'"]').removeClass('active');
            $('#fixed_action_bar').removeClass('active');
          }
          else {
            $(name).slideDown(200);
            $('.toggle_fixed_action_bar[href="'+name+'"]').addClass('active');
            $('#fixed_action_bar').addClass('active');
          }

          $(name).find('form').trigger('magnetis.form_visible');
        }

		$(document).on('click', '.toggle_fixed_action_bar', function(e) {
			e.preventDefault();

            toggleActionBar($(this).attr('href'));
		});

		$('.content .close_content', '#fixed_action_bar').click(function(e) {
			e.preventDefault();

			// Slide out all other opened containers
