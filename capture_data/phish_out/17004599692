		console.log('clicked after');
	})
	$('.page-template-lambo-flexible-content .slider.slick').slick({
		slidesToShow: 1,
		dots: true,
		arrows: false,
	});

	var disposition_4 = $('#main_header').hasClass('disposition_4');
	// var disposition_5 = $('#main_header').hasClass('disposition_5');
	if(disposition_4){
		var menu_top;
		var window_height;
		var window_width;
		var is_menu_stuck = false;
		function updateScroll()
			{
				menu_height = $('#main_header').outerHeight();
				// Menu sticks to top
				var scroll_top = $(window).scrollTop();
				var current_top = menu_top - scroll_top;

				if ( ! is_menu_stuck && scroll_top >= menu_height ) {
					$('#main_header').addClass('scroll_stuck');
					$('#main_menu').addClass('scroll_stuck');
					is_menu_stuck = true;
				}
				else if ( is_menu_stuck && scroll_top === 0 ) {
					$('#main_header').removeClass('scroll_stuck');
					$('#main_menu').removeClass('scroll_stuck');
					is_menu_stuck = false;
				}
			}

			$(window).scroll( updateScroll );
			updateScroll();
			$(window).resize( updateScroll );


			var lastScrollTop = 0;
			$(window).scroll(function(event){
				var st = $(this).scrollTop();
				if (st > lastScrollTop){
					$('#main_header').removeClass('scroll_up');
					if($('#main_header').hasClass('scroll_stuck')){
						$('#main_header').addClass('scroll_down');
						$('#main_header #main_menu_5').slideUp();
						$('#main_header .menu-toggle .burger').removeClass('active');
					}
				} else {
					$('#main_header').addClass('scroll_up');
					$('#main_header').removeClass('scroll_down');
				}
				lastScrollTop = st;
			});
	}

	$(document).on('click', 'a[href^="#"]', function (event) {

		$offset = 0;

		if($('#main_header').hasClass('disposition_2')){
			if($('#main_header').hasClass('only_menu_sticky')){
				$header_height = $('#main_header').height();
				$topbar_height = $('.topbar').height();
				$offset = $header_height - $topbar_height;
			} else {
				$header_height = $('#main_header').height();
				$offset = $header_height;
			}
		} else if(!$('#main_header').hasClass('disposition_4')) {
			if($('#main_header').hasClass('only_menu_sticky')){
				$menu_height = $('#main_menu').height();
				$offset = $menu_height;
			} else {
				$header_height = $('#main_header').height();
				$menu_height = $('#main_menu').height();
				$offset = $header_height + $menu_height;
			}
		}

		if($('#notification_bar.fixed').length){
			$notifbar_height = $('#notification_bar.fixed').height();
			$offset = $offset + $notifbar_height;
		}

		if($(this).hasClass('js-scroll-to-anchor')){
			$(':target').css('padding-top', '0');
			$(':target').css('margin-top', '0');
			$('html, body').animate({
				scrollTop: $($.attr(this, 'href')).offset().top - $offset
			}, 500);
		} else {
			$(':target').css('padding-top', $offset + 'px');
			$(':target').css('margin-top', '-' + $offset + 'px');
		}
		
	});

