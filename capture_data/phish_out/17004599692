			$('.toggle_fixed_action_bar.active').removeClass('active');

			$('#fixed_action_bar').removeClass('active');
		});

		function resize() {
			var max = $(window).height() * 0.7;

			$('.content', '#fixed_action_bar').css('max-height', max + 'px');
		}
		$(window).resize();
		resize();

		// Bouncing animation
		if ( $('#fixed_action_bar').length > 0 ) {
			//adding a class to the body if fixed action bar exist
			$('body').addClass('has_fixed_bar');

			var currentBounceTab = false;

			function bounceBar()
			{
				if ( ! currentBounceTab || currentBounceTab.is(':last-child') ) {
					currentBounceTab = $('.action', '#fixed_action_bar').first();
				}
				else {
					currentBounceTab = currentBounceTab.next();
				}

				currentBounceTab.animate({
					'top': -10
				},
				300,
				function() {
					currentBounceTab.animate({
						'top': 0
					},
					300,
					function() {
						setTimeout(bounceBar, 4000);
					});
				});
			}

			setTimeout(bounceBar, 2000);
		}
	});

	// Notification bar
	var notification_bar_closed = sessionStorage.getItem('notification_bar_closed');

	if ( ! notification_bar_closed || $('#notification_bar').hasClass('no_close') ) {
