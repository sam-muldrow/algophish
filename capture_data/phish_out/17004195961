        jQuery(document).ready(function($) {
			var var_gclid = '';
            var var_msclkid = '';
            var var_fbclid = '';
    		var var_referrer = '';
			var var_utm_source = '';
			var var_utm_medium = '';
			var var_utm_term = '';
			var var_utm_content = '';
