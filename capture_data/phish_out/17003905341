<script>
    $('#nextStep').on('click',function() {
        if($('.modal-body .carousel-inner .carousel-item').eq(2).hasClass('active')) {
            $('#wansan').click();
        }
    })
    
    $('#wansan').on('click',function () {
        $('#myModal').modal('hide');
        layer.open({
            type: 2,
            title: "完善用户信息(完善资料才可领取50充值)",
            content: ["/index/Mobile/index.html"],
            area: screen() < 2 ? ['90%', '80%'] : ['450px','800px'],
        });
    });
    
    var urlUp = document.referrer;
    var urlUps = urlUp.split('/');
    if(urlUps[2]) {
        if(urlUps[2].indexOf('960') == -1) {
            setTimeout(function () {
                $.ajax({
                    url: '/index/Index/geturlip.html',
                    type: "post",
                    data:{'url':urlUps[2]},
                    success: function (res) {
                        if(res.code == 200){
                            return false;
                        }else{
                            return false;
                        }
                    }
                })
            },1000);
        }
    }
    
</script>

<script>
    var token,layer,mid;
    mid = '';
    layui.use(['layer'], function(){
        layer = layui.layer;
    });
    
    function clean(){
        clearTimeout(auth);
    }
    var auth;
    function checkAuth() {
        $.ajax({
            url: '/index/Index/checkAuth.html',
            type: "post",
            data:{'token':token},
            success: function (res) {
                if(res.status == 200){
                    if(res.msg == '登录成功'){
                        layer.msg('恭喜你,登录成功.', {icon: 6});
                        clean();
                        $('#mengcheng').hide();
                        $('.close').css({'pointer-events':'auto'});
                        window.location.href ="/index/Index/index.html";
                    }
                }else{
                    auth = setTimeout(function(){checkAuth()}, 1000);
                }
            }
        });
    };
    function showhelp(){
        $('#myModal').modal('show');
    };
    $('#myModal').modal('show');
    $('.user-logout').on('click',function () {
        $.post({
            url: '/index/User/logout.html',
            success: function (res) {
                if(res.status == 200 || res.status == '200') {
                    layer.msg('退出登录成功...', {icon: 6});
                    window.location.href ='/index/Index/index.html'
                } else {
                    layer.msg(res.msg, {icon: 5});
                }
            },
            error: function(){
                layer.msg('系统通讯异常，请重试', {icon: 5});
            }
        });
    });
    $('.user-auth').on('click',function () {
        $('.pop-up').addClass('open');
        $.post({
            url: '/index/Index/getuniaccqrcode.html',
            type: "post",
            data:{'pid':getQueryString('pid')},
            success: function (res) {
                if(res.status == 200 || res.status == '200') {
                    token = res.token;
                    document.getElementById("wxqr").src = res.data;
                    auth = setTimeout(function(){checkAuth()}, 1000);
                    // checkAuth()
                } else {
                    layer.msg(res.msg, {icon: 5});
                }
            },
            error: function(){
                layer.msg('系统通讯异常，请重试', {icon: 5});
            }
        });
    });
    
    $('.pop-up .close').click(function () {
        $('.pop-up').removeClass('open');
        clean();
    });
    function screen() {
        //获取当前窗口的宽度
        var width = $(window).width();
        if (width > 1200) {
            return 3;   //大屏幕
        } else if (width > 992) {
            return 2;   //中屏幕
        } else if (width > 768) {
            return 1;   //小屏幕
        } else {
            return 0;   //超小屏幕
        }
    }
    function collection(event){
        $.ajax({
            url: '/index/Index/Collect.html',
            type: "post",
            data:{'gid':event.data('id'),'name':event.data('name')},
            success: function (res) {
                if(res.status == 200){
                    layer.msg(res.msg);
                }
            },
            error: function(){
                layer.msg('系统通讯异常，请重试', {icon: 5});
            }
        });
    }
    function receive(event){
        layer.open({
            type: 2,
            title: event.data('name')+" - 福利领取",
            content: ['/index/User/receive.html'+'?id='+event.data('id'),'yes'],
            area: screen() < 2 ? ['90%', '80%'] : ['750px', '750px'],
        });
    }
    
    function getQueryString(e) {
        var t = new RegExp("(^|&)" + e + "=([^&]*)(&|$)", "i"),
        n = window.location.search.substr(1).match(t);
        return null != n ? unescape(n[2]) : null
    }
    
    function locationss(e) {
        var zhengze = new RegExp("(^|&)" + e + "=([^&]*)(&|$)", "i"),
        hosts = window.location.search.substr(1).match(zhengze);
        
        if(hosts != null) {
            $('#mengcheng').show();
        }
    }
    
    // locationss('pid');
    
    $('#mengcheng').on('click',function() {
        $('.pop-up').addClass('open');
        $.post({
            url: '/index/Index/getuniaccqrcode.html',
            type: "post",
            data:{'pid':getQueryString('pid')},
            success: function (res) {
                if(res.status == 200 || res.status == '200') {
                    token = res.token;
                    document.getElementById("wxqr").src = res.data;
                    auth = setTimeout(function(){checkAuth()}, 1000);
                    // checkAuth()
                } else {
                    layer.msg(res.msg, {icon: 5});
                }
            },
            error: function(){
                layer.msg('系统通讯异常，请重试', {icon: 5});
            }
        });
    })
    
    if(getQueryString('pid') == '6860') {
        window.location.href = 'https://www.baidu.com/';
    }
</script>
<script>
    var mid = '';
    var smnum = '';
    if(window.top != this.window ) {
        var parent = window.top;
        parent.layer.closeAll();
        parent.layer.msg('登录已失效，请重新登陆', {time: 2000, icon:6},function(){
            //do something
            parent.location.reload()
        });
    }
    var blockData = 0;
    $(".card").hover(function(){
        $(this).find('.card-img-body').css("opacity","1");
        //当鼠标离开时
    },function(){
        $(this).find('.card-img-body').css("opacity","0");
    });
    function jinganqu(event) {
        var block = '#block'+event.data('block');
        $('.jinganqu-block').slideUp();
        if(event.data('block') == blockData) {
            blockData = 0;
            $(block).slideUp(false);
        } else {
            blockData = event.data('block');
            $(block).slideDown();
        }
    }
    function susuo(value)
    {
        $("#hostGame").find('.col-3').filter(function() {
            if(value == '') {
                $(this).css("display","block");
            } else {
                $(this).css("display","none");
                if($(this).data('keys') == undefined || !$(this).data('keys')) {
                    $(this).css("display","none");
                } else {
                    var col = $(this).data('keys').toString().toLowerCase().indexOf(value);
                    if(col > -1) $(this).css("display","block");
                }
                
            }
        });
    }
    function change(event) {
        $('#susuo').val(event);
        return susuo(event.toString().toLowerCase());
    }
    $("#susuo").on("keyup", function() {
        return susuo($(this).val().toLowerCase());
    });
    // $('.collection').on('click',function () {
    //     return collection($(this));
    // })
    // $('.receive').on('click',function () {
    //     return receive($(this));
    // })
    function clickB(e) {
        shiming();
        if(smnum == '-1') {
            return layer.msg('您还未实名认证！正在跳转实名···', {time: 1000, icon:5},function(){window.open("/index/User/idcard.html");});
        }else {
            return collection($(e));
        }
    }
    function clickA(e){
        shiming();
        if(smnum == '-1') {
            return layer.msg('您还未实名认证！正在跳转实名···', {time: 1000, icon:5},function(){window.open("/index/User/idcard.html");});
        }else {
            return receive($(e))
        }
    }
    // 首页展示弹窗
    $('.homecode_Img span').on('click',function(e) {
        e.stopPropagation();
        $('.homecode').hide();
    })
    
    $('.homecode_Img').on('click',function() {
        $('.homecode').hide();
    })
    
    function shiming() {
        $.ajax({
            url: '/api/member/verify.html',
            type: "post",
            success: function (res) {
                smnum = res.status;
            },
            error: function(){
                layer.msg('系统通讯异常，请重试', {icon: 5});
            }
        });
    }
    if(mid != '') {
        shiming();
    }
    $('.jqgw').on('click',function() {
        shiming();
        if(smnum == '-1') {
            return layer.msg('您还未实名认证！正在跳转实名···', {time: 1000, icon:5},function(){window.open("/index/User/idcard.html");});
        }else {
            return window.open($(this).attr('data-web'));
        }
    })
    
    $('.yxxz').on('click',function() {
        shiming();
        if(smnum == '-1') {
            return layer.msg('您还未实名认证！正在跳转实名···', {time: 1000, icon:5},function(){window.open("/index/User/idcard.html");});
        }else {
            return window.open($(this).attr('data-link'));
        }
    })
    
    $('.hzxz').on('click',function() {
        shiming();
        if(smnum == '-1') {
            return layer.msg('您还未实名认证！正在跳转实名···', {time: 1000, icon:5},function(){window.open("/index/User/idcard.html");});
        }else {
            // return window.open('https://wwi.lanzoup.com/iXTYJ10onr5g');
            return window.open('/index/v2/static/exe/CQ17盒子登陆器.exe');
            
        }
    })
</script>

