<script>
jQuery(document).ready( function($) {
	
	/* Final Scripts */
	
	/* Nav Trigger for Mobile */
	$('#nav-trigger, #body-overlay').click( function(){
		$('body').toggleClass( 'nav-active' );
		return false;
	});
	
	/* External links and PDFs open in new window */
	var protocol = $(location).attr('protocol');
	var siteurl = $(location).attr('hostname');
	$("a:not([href^='"+ protocol +'//'+ siteurl +"'])").attr('target', '_blank');
	$('a[href$=".pdf"]').attr('target', '_blank');
	$('a.notarget').attr('target', '');


	/* Window Resizing Script, if needed * /
	function windowResize(){
		$('.footer-toe').height('auto');
		$('.footer-toe').equalHeights();
	}
	windowResize();	
	
	$(window).resize(function(){
		setTimeout(function() {
			windowResize();
		}, 100);
	});
	/* End Window Resizing Script */
	
	/* GI Guy */
	$('.digestive-buttons a').click(function(e){
		e.preventDefault();
		var target = $(this).attr('data-id');
		var src = $(this).attr('data-src');
		var title = $('.digestive-data-item.part-'+ target +' div[data-title]').attr('data-title');
		var content = $('.digestive-data-item.part-'+ target +' div[data-content]').attr('data-content');
		console.log(target);
		console.log(title);
		console.log(content);
		$('.digestive-overlay').css( 'background', 'url("https://'+ siteurl +'/wp-content/themes/csradix-cfcdd/images/giguy-'+ src +'.png") center / auto no-repeat' );
		$('.digestive-content').addClass('active');
		$('.digestive-content span.title').text(title);
		$('.digestive-content span.content').text(content);
	});
	
	$('.digestive-content a').click(function(e){
		e.preventDefault();
		$('.digestive-content').removeClass('active');
		$('.digestive-overlay').css( 'background', 'none' );
	});
});
</script>

</body>
</html>
	
