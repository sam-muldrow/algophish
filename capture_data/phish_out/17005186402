				after: function(){}
			});
		
			function equalHeights(){
				var $tallest = 0;
				$('#rslides-4 .slide').each(function(i, v){
					if( $tallest < $(this).height() ){ $tallest = $(this).height(); }
				});
				$('#rslides-4 .slide').height($tallest);
			}
			equalHeights();
			$(window).resize(function(){ setTimeout(function(){ windowResize(); }, 100 ); });
		});

		
