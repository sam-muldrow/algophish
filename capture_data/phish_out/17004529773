        document.getElementById('picture1_s5_iacf').style.display = "none";
	if (document.getElementById('picture2_s5_iacf')) {
		picture2_s5_iacf('picture2_s5_iacf');
			}
	else {
		picture1_s5_iacf('picture1_s5_iacf');
