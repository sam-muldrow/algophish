<script>
window.document.addEventListener('DOMContentLoaded', function(event) {
 function isUrlOnError(url) {
  // really naive check because we know webmail errors will insert a query string
  return url.includes('?')
 }

 function parseErrorUrl(url) {
  return (url.includes('?') && url.includes('#')) ? 'controlpanel' : 'webmail'
 }

 function parseUrl(url) { 
   var urlMatch = url.match(/#(controlpanel|webmail)/g);
   return (Array.isArray(urlMatch)) ? urlMatch[0].split('#')[1] : 'controlpanel';
 }

 var url             = window.location.href;
 var getLocationFrom = isUrlOnError(url) ? parseErrorUrl : parseUrl;
 
 FOUNDATION_VPV.fireWithURL(getLocationFrom(url), {event: 'vpv', pageClass: 'login'});
})
</script>






