     });
     */
    setTimeout("busca()",1000)
     
});
function busca(){
	$(".tip").each(function(){
    	console.log("econtrado")
    	$(this).tipr({
          'speed': 300,
          'mode': 'top'
     	});
    })
    setTimeout("busca()",1000)
}
</script>
