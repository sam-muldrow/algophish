	<script type="text/javascript">
		grecaptcha.ready(function() {
			grecaptcha
				.execute('6LfXoMAUAAAAAL2UD4W-R3MZ0_zcDRc2GDwnZ1FO', { action: 'vmpclientsignin' })
				.then(function(token) {
					$("#recaptcha-token").val(token);
				});
