		navigation: {
			nextEl: ".module_gallery4 .swiper-button-next",
			prevEl: ".module_gallery4 .swiper-button-prev",
		},
		on: {
			slideChange: function(){
				$(".gallery4_thumbnail").removeClass("active");
				$(".gallery4_thumbnail[swiper-target=" + this.activeIndex + "]").addClass("active");
			}
		}
	});
	
	$("[swiper-target]").on("click", function(){;
		gallert4Swiper.slideTo(parseInt($(this).attr("swiper-target")));
	});
	</script>	
		</div>
	
</div>
</div>

