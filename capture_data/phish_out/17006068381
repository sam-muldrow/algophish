/*=========================*/
/*Make submenu visible on hover*/
@media(min-width: 768px){
.oc-header-13__menu-has-children:hover > .oc-header-13__sub-menu{
	transform: translateY(0);
	opacity: 1;
	visibility: visible;
 }
.oc-header-13__menu-has-children:hover > .oc-header-13__sub-menu2{
	transform: translateY(0);
	opacity: 1;
	visibility: visible;
 }
}</style>
<script type="text/javascript" id="ct_code_block_js_86">document.addEventListener("DOMContentLoaded", function(event) { 
const swiper = new Swiper('.ocs-slider', {
  // Optional parameters
  loop: false,
  slidesPerView: 1,
  autoplay: {
   delay: 8000, //ms
   disableOnInteraction: false,
   },
});
});</script>
<script type="text/javascript" id="ct_code_block_js_532">document.addEventListener( 'DOMContentLoaded', () => {
	const sliderWrapper = document.querySelector('#code_block-532-2').closest('.fancy-slider-5__wrapper');
	const slider = sliderWrapper.querySelector('.oxy-dynamic-list');
	const flkty = new Flickity( slider, {
		autoPlay: false, // 1500 / true / false // set autoplay or turn off here
		prevNextButtons: true, // true / false // show arrows or not
		wrapAround: true,
		on: {
			ready: function() {
				setSliderHeightToMax(this);
			}
		}
	});
	
	function setSliderHeightToMax(slider) {
		slider.cells.forEach(cell => cell.element.style.height = '');

		let heights = slider.cells.map(cell => cell.element.offsetHeight),
			max = Math.max.apply(Math, heights);

		slider.cells.forEach(cell => cell.element.style.height = max + 'px');
	}

	window.addEventListener('resize', function(){
		setSliderHeightToMax(flkty);
	});
	
	
});</script>
