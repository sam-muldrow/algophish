            <!-- Formulaire de recherche -->
			<script type='text/javascript'>
// JavaScript Document
function getXhr(){
                var xhr = null; 
				if(window.XMLHttpRequest) // Firefox et autres
				   xhr = new XMLHttpRequest(); 
				else if(window.ActiveXObject){ // Internet Explorer 
				   try {
			            xhr = new ActiveXObject("Msxml2.XMLHTTP");
			            } catch (e) {
			            xhr = new ActiveXObject("Microsoft.XMLHTTP");
			            }
				}
				else { // XMLHttpRequest non support� par le navigateur 
				   alert("Votre navigateur ne supporte pas les objets XMLHTTPRequest..."); 
				   xhr = false; 
				}
            return xhr;
			}

/*** M�thode qui sera appel�e sur le click du bouton*/
			function go(){
				var xhr = getXhr();
				// On d�fini ce qu'on va faire quand on aura la r�ponse
				xhr.onreadystatechange = function(){
					// On ne fait quelque chose que si on a tout re�u et que le serveur est ok
					if(xhr.readyState == 4 && xhr.status == 200){
						// On se sert de innerHTML pour rajouter les options a la liste
						var leselect;
						leselect = xhr.responseText;
						// On se sert de innerHTML pour rajouter les options a la liste
						document.getElementById('choix_type').innerHTML = leselect;
						//alert(xhr.responseText); 
					}
				}
				// Ici on va voir comment faire du post
				xhr.open("POST","/includes/select_types.php",true);
				// ne pas oublier �a pour le post
				xhr.setRequestHeader('Content-Type','application/x-www-form-urlencoded');
				sel = document.getElementById('Marques');
				vehicules = sel.options[sel.selectedIndex].value;
				xhr.send("Marques="+vehicules);
			}
            
<!--Lors de l'appui sur le bouton rechercher, la page "resultat_recherche.asp" est appel�e -->
function resultat(bouton) {
	if (bouton.name=="valider" )
 	{
      document.recherche.action="annonce_list.php";
      document.recherche.submit();
 	}
}
</script>
 

