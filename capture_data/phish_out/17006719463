<script>

    var _box = {};

    $(window).scroll(function() {
        var win_h = $(document).scrollTop();
        if (win_h >= $('.navbar').height()) {
            $('.navbar').addClass('fixed-strip');
        } else {
            $('.navbar').removeClass('fixed-strip');
        }
    }); //Affix捲動頁面後導覽列動畫滑出

    $(document).ready(function () {
        if ($.isNothing(_box["mem_sn"])==false){
            // 有登入狀態
            $("[wx-role='hintUserName']").html(_box["mem_name"]);
            $("[wx-role='btnLogin']").unbind("click");
            $("[wx-role='btnLogin']").on("click",function(){
                location.href = "/account";
                
            });
            $("[wx-role='btnLogout']").parent().removeClass("hide");
        }else{
            $("[wx-role='hintUserName']").html("");
            $("[wx-role='btnLogin']").unbind("click");
            $("[wx-role='btnLogin']").on("click",function(){
                location.href = "/login";
            });
            $("[wx-role='btnLogout']").parent().addClass("hide");
        }

        $("[wx-role='btnLogout']").on("click",function(){
            $.clog(_box["mem_type"]);
            if ($.isNothing(_box["mem_sn"])==false && (_box["mem_type"]=="DELR" || _box["mem_type"]=="STOR")){
                tsApiUrl = _apiUrl+"dealer/logout";
            }else{
                tsApiUrl = _apiUrl+"customer/logout";
            }
            var api = _gfApi();
                api.query={};
                $.go(tsApiUrl,api,function(res,st,jx){
                    // res.code is a number
                    switch(res.code){
                        case 200:
                            _popWin({   "title":"Bye!! "+_box["mem_name"],
                                        "content":"已經登出 PROGiWeb",
                                        "noCaption":"回到首頁",
                                        "noAction":function(){
                                            location.href = "/";
                                        }
                                    });
                            setTimeout(function(){
                                location.href = "/";
                            },5000);
                        break;
                    }
                });
            return true;

        });

    });

    

</script>


