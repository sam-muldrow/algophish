    <script>
        $(window).scroll(function(event) {
            var scroll = $(window).scrollTop();
            if (scroll >= 50) {
                $(".btn_gototop").addClass("show");
            } else {
                $(".btn_gototop").removeClass("show");
            }
        });//gotoTop:btnshow:Add class when scroll down

        $('.btn_gototop').click(function() {
            $('html, body').animate({
                scrollTop:0
            }, 1000);
        });//gotoTop

    </script>

</footer>

<script>
     AOS.init();

    $(function() {
        if ($(window).width() < 480) { //當視窗小於480時才運作
            $('.navbar a').not('.dropdown-toggle').on('click', function() {
                $('.navbar-toggle').click();
            });
        }
    });//點擊導覽列後自動收合選單

    $(document).ready(function() {
        // dropdown animation
        $('.dropdown').on('show.bs.dropdown', function(e) {
            $(this).find('.dropdown-menu').first().stop(true, true).slideDown(250);
        });
        $('.dropdown').on('hide.bs.dropdown', function(e) {
            $(this).find('.dropdown-menu').first().stop(true, true).slideUp(0);
        });
    });

    $(".golastpage").on("click",function(){
        parent.history.back();
		return false;
    });//回上頁

    $(".fold input").on("click",function(){
		$(".fold").removeClass("fold_active");
		if( $(this).prop("checked")){
			$(this).parents(".fold").addClass("fold_active");
		}
	}); //表單摺疊

    $(".wrap_spinner .btn").on("click tap", function() {

        var spinnerBtn = $(this);
        var spinnerInput = spinnerBtn.siblings("input");
        var oldValue = spinnerBtn.siblings("input").val();


        if (spinnerBtn.hasClass("btn_plus")) {

            var newVal = parseFloat(oldValue) + 1;

        } else {
            if (oldValue > 0) {

                var newVal = parseFloat(oldValue) - 1;
            } else {
                newVal = 0;
            }
        }

        spinnerInput.val(newVal);

        $(".btn_minus").on("click tap", function() {
            if ($(this).siblings("input").val() === '0') {
                $(this).attr("disabled", true);
            }
        });

        $(".btn_plus").on("click tap", function() {
            $(this).siblings("button").removeAttr("disabled");
        });

    }); //input number spinner

    $(".login_input").on("blur",function(){
       if( $(this).val().length == 0 ){
           $(this).addClass("is_empty");
       }else{
           $(this).removeClass("is_empty");
       }
    });//input placeholder>label


</script>

    </body>
