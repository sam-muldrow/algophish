<script>

	$(document)
            .on('show.bs.modal', '.modal', function(event) {
                $(this).appendTo($('body'));
            })
            .on('shown.bs.modal', '.modal.in', function(event) {
                setModalsAndBackdropsOrder();
            })
            .on('hidden.bs.modal', '.modal', function(event) {
                setModalsAndBackdropsOrder();
            });

        function setModalsAndBackdropsOrder() {
            var modalZIndex = 1040;
            $('.modal.in').each(function(index) {
                var $modal = $(this);
                modalZIndex++;
                $modal.css('zIndex', modalZIndex);
                $modal.next('.modal-backdrop.in').addClass('hidden').css('zIndex', modalZIndex - 1);
            });
            $('.modal.in:visible:last').focus().next('.modal-backdrop.in').removeClass('hidden');
        } // bootstrap 多視窗layer


</script>
<script>
	function _popWin(dlgInfo){
		$("#dlg_default_title").html(dlgInfo.title);
		$("#dlg_default_content").html(dlgInfo.content);
		$("#btn_dlgDefault_yes").unbind("click");
		$("#btn_dlgDefault_no").unbind("click");

//			if ($.isNothing(dlgInfo.cmbTrigger)) dlgInfo.cmbTrigger = "";
//			$("#btn_dlgDefault_no").wx("cmb-role",dlgInfo.cmbTrigger);
//			$("#btn_dlgDefault_yes").wx("cmb-role",dlgInfo.cmbTrigger);
//			$("#btn_dlgDefault_no").wx("cmb-key","alt+bks");
//			$("#btn_dlgDefault_yes").wx("cmb-key","alt+enter");

		if (dlgInfo.noCaption!=null && dlgInfo.noCaption!=""){
			$("#btn_dlgDefault_no").html(dlgInfo.noCaption);
			$("#btn_dlgDefault_no").show();
			if (dlgInfo.noAction!=null) $("#btn_dlgDefault_no").on("click",dlgInfo.noAction);
		}else{
			$("#btn_dlgDefault_no").hide();
		}
		$("#btn_dlgDefault_yes").unbind("click");
		if (dlgInfo.yesCaption!=null && dlgInfo.yesCaption!=""){
			$("#btn_dlgDefault_yes").html(dlgInfo.yesCaption);
			$("#btn_dlgDefault_yes").show();
			if (dlgInfo.yesAction!=null) $("#btn_dlgDefault_yes").on("click",dlgInfo.yesAction);
		}else{
			$("#btn_dlgDefault_yes").hide();
		}
		if (typeof(dlgInfo.delay)=="undefined"){
			$("#dlg_default").modal({keyboard: false,show:true,backdrop:"static",focus:true});
			if (dlgInfo.popAction!=null){
				setTimeout(dlgInfo.popAction,400);
			}
		}else{
			setTimeout(function(popAction){
				$("#dlg_default").modal({keyboard: false,show:true,backdrop:"static",focus:true});
			},dlgInfo.delay);
			if (dlgInfo.popAction!=null){
				setTimeout(dlgInfo.popAction,dlgInfo.delay+400);
			}
		}
		if (typeof(dlgInfo.dftBtn)!="undefined"){
			switch(dlgInfo.dftBtn.toLowerCase()){
				case "yes":
					$("#btn_dlgDefault_yes").trigger("focus");
				break;
				case "no":
					$("#btn_dlgDefault_no").trigger("focus");
				break;
			}
		}
	};

	function _popWinClose(){
		$("#dlg_default").modal("hide");
	}
</script>


