                        <!-- SELECT SQL_CALC_FOUND_ROWS `car_code` as car_code, CONCAT(`car_name`,' ',IF(`ps_specval`!='',`ps_specval`,`part_name`)) as part_name_display,`parts`.*,`pi_name` as part_cat_name FROM `car_parts` LEFT JOIN `cars` ON (`car_code`=`cp_car` AND `car_parts`.`_isdel`=0), `parts` LEFT JOIN `part_items` ON (`part_item`= `pi_aliases`) LEFT JOIN `part_spec` ON (`ps_part`=`part_code` AND `ps_specname` = 'name')  WHERE () AND (`part_price`+`part_install_fee`)>0 AND `cp_part`=`part_code` AND `parts`.`_isdel`=0 AND `part_status`>0 AND `car_parts`.`_isdel`=0 GROUP BY CONCAT(`part_code`,`car_code`) ORDER BY `car_code` ASC,`part_code` ASC LIMIT 0,20  --><div>
