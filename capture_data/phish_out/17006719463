<script>

        function windowSize(){
            if ($(window).width() <= 760) {
                $("#tab_search").removeClass("nav-justified");
           }else{
               $("#tab_search").addClass("nav-justified");
           }
        };

        windowSize();

        $(window).resize(function(){
             windowSize();

        });// tab search

        $(".btn_search_collapse").on("click",function(){
            if( $(this).hasClass("collapsed")){
                $(this).text("收合搜尋條件");
            }else{
                $(this).text("展開搜尋條件");
            }
        });

        $(document).ready(function () {
            var tsDatSrc = _prepandJsonObject({"":{"cb_code":"","cb_name":"汽車品牌","_serials":[{"":{"pi_code":"","serial_name":""}}]}},_wx_datsrc.brands);
            _fillOptions("[wx-role='srhField'][wx-field='brand']",tsDatSrc,"cb_code","cb_name","",true);
            
            $("[wx-role='srhField'][wx-field='brand']").on("change",function(){
                $.clog("Go");
                var tsCode = $(this).val();
                if (tsCode=="") return;
                var tsDatSrc = _prepandJsonObject({"":{"serial_code":"","serial_name":"所有車系","_models":[{"":{"model_name":""}}]}},_wx_datsrc.brands[tsCode]["_serials"]);
                _fillOptions("[wx-role='srhField'][wx-field='serial']",tsDatSrc,"serial_code","serial_name","",true);
                
                $("[wx-role='srhField'][wx-field='serial']").unbind("change");
                $("[wx-role='srhField'][wx-field='serial']").on("change",function(){
                    var tsBrand = $("[wx-role='srhField'][wx-field='brand']").val();
                    var tsBrandName = $("[wx-role='srhField'][wx-field='brand'] option:selected").text();
                    var tsSerial = $(this).val();
                    if ($.isNothing(tsSerial)) return;
                    var tsDatSrc = _wx_datsrc.brands[tsBrand]["_serials"][tsSerial]["_models"];
                    tsDatSrc[""] = {"model_name":""};
                    for(var idx in tsDatSrc){
                        tsDatSrc[idx]["model_short_name"] = tsDatSrc[idx]["model_name"].substr(String(tsBrandName+" "+tsSerial+" ").length);
                    }
                    $.clog($.stringify(tsDatSrc));
                    _fillOptions("[wx-role='srhField'][wx-field='model']",tsDatSrc,"model_code","model_short_name","",true);
                    $("[wx-role='srhField'][wx-field='model']").trigger("change");
                });
                
                $("[wx-role='srhField'][wx-field='serial']").trigger("change");
            });
            
            $("[wx-role='srhField'][wx-field='brand']").trigger("change");

            var tsDatSrc = _prepandJsonObject({"":{"pc_code":"","pc_name":"所有產品類型","_items":[{"":{"pi_code":"","pi_name":""}}]}},_wx_datsrc.part_cats_aliases);
            _fillOptions("[wx-role='srhField'][wx-field='part_cat']",tsDatSrc,"pc_code","pc_name","",true);
            $("[wx-role='srhField'][wx-field='part_cat']").on("change",function(){
                var tsCode = $(this).val();
                if ($.isNothing(tsCode)) return;
                var tsDatSrc = _prepandJsonObject({"":{"pi_aliases":"","pi_name":"所有零件類型"}},_wx_datsrc.part_cats_aliases[tsCode]["_items"]);
                _fillOptions("[wx-role='srhField'][wx-field='part_item']",tsDatSrc,"pi_aliases","pi_name","",true);
                $("[wx-role='srhField'][wx-field='part_item']").trigger("change");
            });
            $("[wx-role='srhField'][wx-field='part_cat']").trigger("change");
            
            $(".wrap_partsimg_slt").addClass("hide");
            _fillOptions("[wx-role='sltItemGraphicCat']",_wx_datsrc['mapItemGraphicCat'],"pigc_code","pigc_name","",true);
            $("[wx-role='sltItemGraphicCat']").unbind("change");
            $("[wx-role='sltItemGraphicCat']").on("change",function(){
                var tsCat = $(this).val();
                $("[wx-role='ctrBody'][wx-for='lstItemGraphic'] [wx-reusable]").remove();
                for(var idx in _wx_datsrc['mapItemGraphicCat'][tsCat]["graphics"]){
                    $.clog(">>>>>"+_wx_datsrc['mapItemGraphicCat'][tsCat]["graphics"][idx]["pig_video"]);
                    _wx_datsrc['mapItemGraphicCat'][tsCat]["graphics"][idx]["_has_video_"] = "hide";
                    if ($.isNothing(_wx_datsrc['mapItemGraphicCat'][tsCat]["graphics"][idx]["pig_video"])==false) _wx_datsrc['mapItemGraphicCat'][tsCat]["graphics"][idx]["_has_video_"] = "";
                    $("[wx-role='ctrBody'][wx-for='lstItemGraphic']").append($.reuse("itemGraphic",_wx_datsrc['mapItemGraphicCat'][tsCat]["graphics"][idx]));
                }
                $("[wx-role='btnPickGraphic']").unbind("click");
                $("[wx-role='btnPickGraphic']").on("click",function(){
                    $("#wrap_partsimg").collapse("hide");
                    $(".wrap_partsimg_slt").removeClass("hide");
                    var tsCode = $(this).wx("code");
                    var tsEnd = _wx_datsrc['mapItemGraphicCat'][tsCat]["graphics"][tsCode]["pig_end"];
                    var tsImg = _wx_datsrc['mapItemGraphicCat'][tsCat]["graphics"][tsCode]["pig_img"];
                    var tsTitle = $(this).wx("title");
                    
                    $(".wrap_partsimg_slt [wx-role='preViewBigGraphic']").attr("href",tsImg);
                    $(".wrap_partsimg_slt [wx-role='preViewBigGraphic']").attr("title",tsTitle);
                    $(".wrap_partsimg_slt [wx-role='preViewBigGraphic']").css("background-image","url("+tsImg+")");
                    
                    $.clog("Pick Spec Graphic : "+tsCode+" > "+tsEnd);
                    $("[wx-role='ctrBody'][wx-for='lstItemGraphicSpec'] [wx-reusable]").remove();
                    var specnums = "ABCDEFGHIJKLMNOPQRSTUVWXYZ";
                    var specDefRanges = {"A":"1","B":"10","C":"1","D":"10","E":"20","F":"20","G":"10"};//defRange
                    for(var i=0; i<specnums.length; i++){
                        $.clog("Pick Spec Graphic Num: "+i+" > "+specnums.substr(i,1));
                        var defRange = "";
                        if ($.isNothing(specDefRanges[specnums.substr(i,1)])==false) defRange = specDefRanges[specnums.substr(i,1)];
                        $("[wx-role='ctrBody'][wx-for='lstItemGraphicSpec']").append($.reuse("itemGraphicSpec",{"spec_num":specnums.substr(i,1),"defRange":defRange}));
                        if (specnums.substr(i,1)==tsEnd) break;
                    }
                });
            });
            $("[wx-role='sltItemGraphicCat']").trigger("change");
            $("[wx-role='btnClearPickUpGraphic']").unbind("click");
            $("[wx-role='btnClearPickUpGraphic']").on("click",function(){
                $("#wrap_partsimg").collapse("show");
                $(".wrap_partsimg_slt").addClass("hide");
                $(".wrap_partsimg_slt [wx-role='preViewBigGraphic']").attr("href","");
                $(".wrap_partsimg_slt [wx-role='preViewBigGraphic']").attr("title","");
                $(".wrap_partsimg_slt [wx-role='preViewBigGraphic']").css("background-image","");
            });
            
            $("[wx-role='btnGraphicVideo']").on("click",function(){
//                $("[wx-role='vdoSpecGraphic']").html('<source src="'+$(this).wx("for")+'"></source>' );
                var video = $("[wx-role='vdoSpecGraphic'] video")[0];
                video.src = $(this).wx("for");
                video.load();
                video.play();
            });
            
            
        });

        $('.swipebox').swipebox();
        // 商品照片gallery


</script>
                    </div>

                </div>
            </section>

