        $(function(){
         require(['swiper4.4.2'],function(s){    
             $.ajax({
                type: "get",
                url: "/api/cmscontent/appContentService/getFrontPagination?viewType=1&tenantId="+tenant.id+"&groupId=3&order=1", //后台框架路由
