<script>
var cook_light = FetchCookie('light');	
var but = document.getElementById("light");
var type = document.getElementsByClassName("deancloselight");
var light_text = document.getElementById("light_text");
if(cook_light=='light_night'){
document.getElementsByTagName('body')[0].className = 'light_night';
type[0].classList.add("deanopenlight");
light_text.innerHTML = '开灯';
}else{
document.getElementsByTagName('body')[0].className = '';
}
but.addEventListener('click',function() {if(cook_light=='light_night'){
document.getElementsByTagName('body')[0].className = '';
SetCookie('light','light_day');
cook_light = 'light_day';
type[0].classList.remove("deanopenlight");
light_text.innerHTML = '关灯';
}else{
document.getElementsByTagName('body')[0].className = 'light_night';
SetCookie('light','light_night')
cook_light = 'light_night';
type[0].classList.add("deanopenlight");
light_text.innerHTML = '开灯';
}})
</script>
