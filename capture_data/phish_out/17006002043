          <script>
          
            var Thumbnail = "https://4.bp.blogspot.com/-qeQPQHOR0xo/WoxFlXCdZPI/AAAAAAAAAFE/Gi5oEoQUh60FxaNSMkKkmhna5UW2ctxEgCLcBGAs/s72-c/%25D9%2583%25D9%258A%25D9%2581_%25D8%25A3%25D8%25AB%25D9%2582%25D9%2581_%25D9%2586%25D9%2581%25D8%25B3%25D9%258A.jpg";

            if ( rct_cards !== null && rct_cards.innerText === 'true' ) {
            var BigThumb = Thumbnail.replace("s72-c","s350").replace(/http:\/\//,"https://");
            } else {
            var BigThumb = Thumbnail.replace("s72-c","s350").replace(/http:\/\//,"https://");
            }
            
