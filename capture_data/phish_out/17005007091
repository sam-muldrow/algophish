                <script>
        var start_offset = -1;
        window.onscroll = function() {myFunction()};
        function myFunction() {
            if (start_offset == -1){
                start_offset = document.getElementById("navbar").offsetTop;
                console.log("===============");
                console.log(start_offset);
                console.log("===============");
            }
            if (window.pageYOffset >= start_offset) {
                navbar.classList.add("sticky")
            } else {
                navbar.classList.remove("sticky");
            }
        }
    </script>
    

