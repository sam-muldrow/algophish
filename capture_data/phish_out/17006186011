				<script type="text/javascript">
					setREVStartSize({c: 'rev_slider_7_3',rl:[1240,1024,778,480],el:[600,600,500,400],gw:[800,700,400,300],gh:[600,600,500,400],type:'carousel',justify:'',layout:'fullscreen',offsetContainer:'',offset:'',mh:"0"});
					var	revapi7,
						tpj;
					jQuery(function() {
						tpj = jQuery;
						revapi7 = tpj("#rev_slider_7_3")
						if(revapi7==undefined || revapi7.revolution == undefined){
							revslider_showDoubleJqueryError("rev_slider_7_3");
						}else{
							revapi7.revolution({
								sliderType:"carousel",
								sliderLayout:"fullscreen",
								visibilityLevels:"1240,1024,778,480",
								gridwidth:"800,700,400,300",
								gridheight:"600,600,500,400",
								lazyType:"single",
								perspective:600,
								perspectiveType:"local",
								editorheight:"600,600,500,400",
								responsiveLevels:"1240,1024,778,480",
								carousel: {
									infinity:true,
									maxVisibleItems:5,
									fadeout:false
								},
								progressBar: {
									color:"rgba(255,255,255,0.25)",
									size:8,
									x:0,
									y:0
								},
								navigation: {
									mouseScrollNavigation:"on",
									tabs: {
										enable:true,
										tmp:"<div class=\"tp-tab-content\">  <span class=\"tp-tab-date\">{{param1}}</span>  <span class=\"tp-tab-title\">{{title}}</span></div><div class=\"tp-tab-image\"></div>",
										style:"gyges",
										hide_onleave:true,
										h_align:"left",
										v_align:"center",
										v_offset:0,
										direction:"vertical",
										width:300,
										height:80,
										min_width:220,
										wrapper_padding:0,
										wrapper_color:"rgba(12, 12, 12, 0.14)",
										visibleAmount:6,
										span:true
									}
								},
