<script>
	document.getElementById('js-tomainpage').addEventListener('click', function(e) {
		if (window.location.pathname === '/') {
			var targetPathSplitted = window.location.host.split('.').slice(1);
			if (targetPathSplitted.length > 1) {
				e.preventDefault();
				window.location.href = '//' + targetPathSplitted.join('.');
			}
		}
	});
</script>



