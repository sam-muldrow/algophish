<script>
    function get_random_num(min,max)
    {
        let range = max - min;
        let rand = Math.random();
        return (min + Math.round(rand * range));
    }
    function show(type) {
        if (type === 1) {
            $('body').css('overflow', 'hidden');
            $('.event-menu').css({'display': 'block'})
            $('.event-user').css({'display': 'none'})
            //$('.event-reg').css({'display': 'none'})
            //$('.event-log').css({'display': 'none'})
        }
        if (type === 2) {
            $('body').css('overflow', 'hidden');
            if ($('#user_info').html().length <= 0) {
                let num = get_random_num(100000000,999999999);
                let t = (new Date().getTime()) + '-' + get_cookie('hl_auth:v3') + '-' + num;
                $.ajax({
                    url: "/index/user_info?t=" + t,
                    success: function (evt) {
                        $('#user_info').html(evt)
                        $('.event-menu').css({'display': 'none'})
                        $('.event-user').css({'display': 'block'})
                        //$('.event-reg').css({'display': 'none'})
                        //$('.event-log').css({'display': 'none'})
                    }
                })
            } else {
                $('.event-menu').css({'display': 'none'})
                $('.event-user').css({'display': 'block'})
                //$('.event-reg').css({'display': 'none'})
                //$('.event-log').css({'display': 'none'})
            }
        }
        if (type === 3) {
            //$('.event-menu').css({'display': 'none'})
            //$('.event-user').css({'display': 'none'})
            //$('.event-reg').css({'display': 'block'})
            goto_register();
            //$('.event-log').css({'display': 'none'})
        }
        if (type === 4) {
            //$('.event-menu').css({'display': 'none'})
            //$('.event-user').css({'display': 'none'})
            //$('.event-reg').css({'display': 'none'})
            //$('.event-log').css({'display': 'block'})
            goto_login();
        }
    }

    function xclose(type) {
        $('body').css('overflow', 'scroll');
        $('.event-menu').css({'display': 'none'})
        $('.event-user').css({'display': 'none'})
        $('.event-reg').css({'display': 'none'})
        $('.event-log').css({'display': 'none'})
        if (type == 'notice') {
            //$('.event-notice').css({'display': 'none'})
            //$('.event-notice').remove();
            let index = get_cookie(notice_cookie_index);
            if (index < notices.length) {
                handle_notice_next(index);
                set_cookie(notice_cookie_index, Number(1) + Number(index), 2);
            } else {
                $('.event-notice').css({'display': 'none'})
            }

        }

    }

    $(".dplayer").each(function (item, prop) {
        let jsonStr = $(prop).attr("config");
        let config = JSON.parse(jsonStr);
        config['container'] = prop
        new DPlayer(config);
    });

    function goBack() {
        window.history.length > 1 ? window.history.go(-1) : window.location.href = '/'
    }

</script>

