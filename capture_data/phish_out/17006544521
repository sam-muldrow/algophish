                        let notice_cookie_name = 'hl_notice:v6';
                        let notice_cookie_index = 'hl_notice_index:v6';
                        let today = new Date();
                        let date = today.getFullYear() + '-' + (today.getMonth() + 1) + '-' + today.getDate();
                        let ts = Date.parse(today)/1000;
                        let ct = 5;//显示次数

                        function show_notice(index=0) {
                            //let index = Math.floor((Math.random() * notices.length));
                            let notice = notices[index]
                            let tmp_html = tmp.replace("{ADVERTISER_URL}", notice.advertiser_url).replace("{ADVERTISER_ID}", notice.advertiser_id).replace("{POS_TIP}", notice.pos_tip).replace("{THUMB}", notice.thumb)
                            //document.write(tmp_html)
                            $('#notice_container').html(tmp_html);
                            set_cookie(notice_cookie_index, 1, 2);
                        }
                        function handle_notice(){
                            if (notices.length <= 0) {
                                return;
                            }
                            let info = '';
                            // 不存在
                            if (document.cookie.indexOf(notice_cookie_name) === -1) {
                                show_notice();
                                info = date + '.' + ts + '.' + '1';
                                set_cookie(notice_cookie_name, info, 2);
                                return;
                            }

                            // 时间.时间戳.次数
                            info = get_cookie(notice_cookie_name);
                            let record_info = info.split(".")
                            let record_date = record_info[0];
                            let record_ts = record_info[1];
                            let record_ct = record_info[2];
                            console.log('RDATE:' + record_date + ' RTS:' + record_ts + ' RCT:' + record_ct + ' CT:' + ct)
                            if (record_date !== date) {
                                show_notice()
                                info = date + '.' + ts + '.' + '1';
                                console.log('SET:' + info)
                                set_cookie(notice_cookie_name, info, 2)
                                return;
                            }

                            if (ts - record_ts >= 7200 && record_ct < ct) {
                                show_notice()
                                info = date + '.' + ts + '.' + (parseInt(record_ct) + 1);
                                console.log('SET:' + info)
                                set_cookie(notice_cookie_name, info, 2)
                                return;
                            }
                        }
                        function handle_notice_next(index){
                            let notice = notices[index]
                            let tmp_html = tmp.replace("{ADVERTISER_URL}", notice.advertiser_url).replace("{ADVERTISER_ID}", notice.advertiser_id).replace("{POS_TIP}", notice.pos_tip).replace("{THUMB}", notice.thumb)
