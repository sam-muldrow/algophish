    <script>
        $(document).ready(function () {
            //contactus modal code. Iframe loading made on click to avoid unwanted google stats counting issues on page load
            $("#btnOpenModal").click(function () {
                //alert("Handler for .click() called.");
                $("#iframecontactus").prop("src", "https://contactusform.ecubeapps.com/SwiftForms/CreatePublic/XTcAAA");
                $("#myModal").modal();
            });



            var scrollflag = true;


            $(document.body).on('touchmove', onScroll); // for mobile
            $(window).on('scroll', onScroll);


            function onScroll(e) {
                if ($('.counting-box > div').hasClass('appear-animation')) {

                    if (scrollflag == true) {
                        var data = {};
                        var url = "/Account/Getoverallstastics";
                        $.ajax({
                            url: url,
                            type: "POST",
                            data: data,
                            success: function (ObjSuperAdminViewModel1) {
                                $("#totalusers").text("");
                                $("#totalforms").text("");
                                $("#totalreports").text('');
                                $("#totalcontries").text('');

                                $("#totalusers").text(ObjSuperAdminViewModel1.Users);
                                $("#totalforms").text(ObjSuperAdminViewModel1.FormsCount);
                                $("#totalreports").text(ObjSuperAdminViewModel1.ReportCount);
                                $("#totalcontries").text(ObjSuperAdminViewModel1.Totalcountries);

                                $('.count-this').each(function () {

                                    var val = $(this).text();
                                    var new_val = val - 30;

                                    // Start the counting from a specified number - in this case, 0!
                                    $(this).prop('Counter', new_val).animate({
                                        Counter: $(this).text()
                                    }, {
                                        // Speed of counter in ms, default animation style
                                        duration: 4000,
                                        easing: 'swing',
                                        step: function (now) {
                                            // Round up the number
                                            $(this).text(Math.ceil(now));
                                        }
                                    });

                                });

                            },
                            error: function (xhr, textStatus, errorThrown) {

                                if (xhr.status == 401) {
                                    alert("Session Expired");
                                    location.href = "/Account/login";
                                } else {
                                    alert("error" + xhr.statusText);

                                }

                            }
                        })

                    }

                    e.stopPropagation();
                    scrollflag = false;
                    $('.counter').removeClass('.count-this');
                }
            };


        });

    </script>
    <section class="counters" style="display: none;height: 0;position: absolute;z-index: -999; bottom: 0;">
