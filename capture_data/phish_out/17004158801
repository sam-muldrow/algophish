		$('#search_prod').autocomplete({ 

			 source: req,

			 change: function (event, ui) { 

			 if(!ui.item) {

				 $("#search_prod").val("");

			 }else{

				 $("#cat").val(ui.item.catid);

				 $("#cat1").val(ui.item.catid);

				 $("#subject").val(ui.item.productname);

				 $("#subject1").val(ui.item.productname);

				 $("#cas").val(ui.item.cas);

				 $("#cas1").val(ui.item.cas);

			 }

		} });

			 

	  });
	  
	  
	  $(document).ready(function(){
			
	$('#cat').on('change',function(){
			var id = $(this).val();
			//alert(id);
			if(id){
				$.ajax({
					type:'POST',
					url:'https://www.alkalimetals.com/home/ajaxData',
					data:'id2='+id,
					success:function(html){
						//alert(html); 
						$('#subject').html(html);
					}
				}); 
				$.ajax({
					type:'POST',
					url:'https://www.alkalimetals.com/ajaxCasData',
					data:'id2='+id,
					success:function(htmldata){
						//alert(html); 
						$('#cas').html(htmldata);
					}
				}); 
				}
	});

	$('#subject').on('change',function(){
			var id = $(this).val();
			//alert(id);
			if(id){
				$.ajax({
					type:'POST',
					url:'https://www.alkalimetals.com/home/ajaxData',
					data:'id='+id,
					success:function(html){
						//alert(html);
						$('#cas').html(html);
					}
				});

					$.ajax({
					type:'POST',
					url:'https://www.alkalimetals.com/home/ajaxData',
					data:'id4='+id,
					success:function(html){
						//alert(html);
						$('#cat').html(html);
					}
				});
				}
	});

	$('#cas').on('change',function(){
			var id = $(this).val();
			//alert(id);
			if(!isNaN(id)){
				$.ajax({
					type:'POST',
					url:'https://www.alkalimetals.com/home/ajaxData',
					data:'id1='+id,
					success:function(html){
						//alert(html);
						$('#subject').html(html);
					}
				}); 
				
				$.ajax({
					type:'POST',
					url:'https://www.alkalimetals.com/home/ajaxData',
					data:'id4='+id,
					success:function(vals){
						//alert(html);
						$('#cat').html(vals);
					}
				}); 
			}else{
				alert('No CAS selected!');
			}
	});

	});
	</script>
