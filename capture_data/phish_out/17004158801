<script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script>
    function updateProductDropdown() {
        var selectedCategoryId = document.getElementById('cat').value;

        // Make an AJAX request to the server with the selected category ID
        var xhttp = new XMLHttpRequest();
        xhttp.onreadystatechange = function () {
            if (this.readyState == 4 && this.status == 200) {
                // Parse the JSON data received and update the options of the "product" dropdown
                var productDropdown = document.getElementById('subject');
                //productDropdown.innerHTML = ''; // Clear existing options
                productDropdown.innerHTML = '<option value="">Select by Product Name</option>';
                var products = JSON.parse(this.responseText);
                if (products.length > 0) {
                    products.forEach(function (product) {
                        var option = document.createElement('option');
                        
                        option.value = product.id;
                        option.textContent = product.productname;
                        productDropdown.appendChild(option);
                    });
                } else {
                    // If no products are available for the selected category, display a message or default option
                    var defaultOption = document.createElement('option');
                    defaultOption.value = '';
                    defaultOption.textContent = 'No products available';
                    productDropdown.appendChild(defaultOption);
                }
            }
        };
        xhttp.open("GET", "https://www.alkalimetals.com/Product/get_product_names?category=" + selectedCategoryId, true);
        xhttp.send();
    }
</script>

<script>
    function updateCasDropdown() {
        var selectedProductId = document.getElementById('subject').value;
         //alert(selectedProductId);
        // Make an AJAX request to the server with the selected product ID
        var xhttp = new XMLHttpRequest();
        xhttp.onreadystatechange = function () {
            if (this.readyState == 4 && this.status == 200) {
                // Parse the JSON data received and update the options of the "CAS Number" dropdown
                var casDropdown = document.getElementById('cas');
                casDropdown.innerHTML = ''; // Clear existing options

                var casNumbers = JSON.parse(this.responseText);
                if (casNumbers.length > 0) {
                    casNumbers.forEach(function (cas) {
                        var option = document.createElement('option');
                        option.value = cas.id; // Use the CAS number as the value
                        option.textContent = cas.CAS;
                        casDropdown.appendChild(option);
                    });
                } else {
                    // If no CAS numbers are available for the selected product, display a message or default option
                    var defaultOption = document.createElement('option');
                    defaultOption.value = '';
                    defaultOption.textContent = 'No CAS numbers available for this product';
                    casDropdown.appendChild(defaultOption);
                }
            }
        };
        xhttp.open("GET", "https://www.alkalimetals.com/Product/get_cas_numbers?product=" + selectedProductId, true);
        xhttp.send();
    }

    // Event listener for "Product Name" dropdown change event
    var productDropdown = document.getElementById('subject');
    productDropdown.addEventListener('change', function () {
        updateCasDropdown(); // Call the function to update the CAS dropdown
    });

    // Initial call to populate the CAS dropdown for the selected product on page load
    updateCasDropdown();
</script>

</section>

