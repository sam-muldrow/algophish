<script type="rocketlazyloadscript" data-rocket-type="text/javascript">window.addEventListener('DOMContentLoaded', function() {
	jQuery(document).ready(function() {
		//jQuery('.navigation2').meanmenu();
		jQuery('.sidebar-menu').hover(function() {
			jQuery(this).css("width", "235px");
			jQuery('.menu span').fadeIn()
		}, function() {
			jQuery(this).css("width", "65px");
			jQuery('.menu span').fadeOut()
		});
		$('.navigation2 li').hover(
			function() {
				$(this).addClass('active');
				$('ul', this).stop().slideDown(200);
			},
			function() {
				$(this).removeClass('active');
				$('ul', this).stop().slideUp(200);
			}
		);

		$('.top_header_navigationarea ul li').hover(function() {
				$('ul', this).stop().slideDown(200);
			},
			function() {
				$('ul', this).stop().slideUp(200);
			}
		)

		jQuery('.search-area').click(function() {
			jQuery('.search-field-area').toggle();
		});
		// $( "#datepicker" ).datepicker({ dateFormat: 'dd/mm/yy' });
		$(window).scroll(function() {
			if ($(window).scrollTop() >= 38) {
				$('.second-head-area').addClass('fixed');
				$('.page-wrapper').addClass('visible-title');
			} else {
				$('.second-head-area').removeClass('fixed');
				$('.page-wrapper').removeClass('visible-title');
			}
		});
		$('.top-navigation').click(function() {
			$('.tp-nav-menu').animate({
				'left': '0px'
			});
		});


		$('.close-nav-btn-bx').click(function() {
			$('.tp-nav-menu').animate({
				'left': '-266px'
			});
		});



		$('.responsive-navigation').click(function() {
			$('.service-navigation .esop-navigation').animate({
				'left': '0px'
			});
		});

		$('.esop-close').click(function() {
			$('.service-navigation .esop-navigation').animate({
				'left': '-220px'
			});
		});


		$(document).click(function(e) {
			if (!$(e.target).hasClass("responsive-navigation") &&
				$(e.target).parents(".service-navigation .esop-navigation").length === 0) {
				$('.service-navigation .esop-navigation').animate({
					'left': '-220px'
				});
			}
		});


		$('#myCarousel').carousel({
			interval: 78555
		});
	});
});</script>
