	$(document).ready(function()
	{	
		$(".socialImages").hover(
			function() 
			{
				var src = $(this).children('img').attr("src");
				var tab = src.split('.');
				tronc1 = tab[0];
				tronc2 = tab[1];
				tronc3 = tab[2];
				src = "."+tronc2+"-on."+tronc3;
				$(this).children('img').attr('src', src);
				
			}, function() 
			{
				var src = $(this).children('img').attr("src");
				var tab = src.split('-');
				tronc1 = tab[0];
				tronc2 = tab[1];
				tronc2=".png"
				src = tronc1+tronc2;
				$(this).children('img').attr('src', src);
				
			}
		);
	});
</script>
						
