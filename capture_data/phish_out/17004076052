<script type="text/javascript">

	$(document).ready(function()
	{	
	var flag=true;

		$("#checkVideo").click(function()
		{	
			if(flag==true)
			{
				$("#choiceVideo").fadeIn(1000, function(){});
				$("#encarthover").fadeIn(1000, function(){});
				flag=false;
			}
			else
			{
				$("#choiceVideo").fadeOut(1000, function(){});
				$("#encarthover").fadeOut(1000, function(){});
				flag=true;
			}
		});
		
		
		$(".villeDep").click(function()
		{	
			var idThis = $(this).attr('id');
			$.ajax(
			{
				type: "POST",
				url : "./ajax/meteo.php",
				data:
				{
					reponse: idThis,
				},
				dataType: 'json',
				success: function(data)
				{
					var reponseimg="images/picto_meteo/"+data.ts[0]+".svg";
					var reponsetempe=data.temp[0]+"Â°";
				    $(".highMeteo").children('img').attr('src',reponseimg);
				    $('#affichageTemp').text(reponsetempe);
				    $('#selectVideo').text(data.ville);
				}
			});
			$("#choiceVideo").fadeOut(1000, function(){});
			$("#encarthover").fadeOut(1000, function(){});
			flag=true;
			
		});

	});
	
</script>
