                  <script type="text/javascript">

          $(document).ready(function ()
          {
            $("#formForgot").find("*").css('display', 'none');
            var glob = true;
            $("div#connexion").click(function ()
            {
              $("#finishForgot").css('display', 'none');
              $("div#shadowConnect").fadeIn(300, function () {});

              $("div#moduleConnexion").mousemove(function ()
              {
                if (glob == true)
                  glob = false;
              });
              $("div#moduleConnexion").mouseout(function () {
                glob = true;
              });
              $("div#shadowConnect").click(function ()
              {
                if (glob == true)
                {
                  $("div#shadowConnect").fadeOut(300, function () {});
                  $("#formConnect").fadeIn();
                  $("#formForgot").find("*").css('display', 'none');
                }

              });

            });

            $("#connexionButton").hover(
                    function ()
                    {
                      var src2 = $(this).attr("src");
                      var tab2 = src2.split('.');
                      tronc4 = tab2[0];
                      tronc5 = tab2[1];
                      src2 = tronc4 + "-on." + tronc5;
                      $(this).attr('src', src2);

                    }, function ()
            {
              var src2 = $(this).attr("src");
              var tab2 = src2.split('-');
              tronc4 = tab2[0];
              tronc6 = ".png"
              src2 = tronc4 + tronc6;
              $(this).attr('src', src2);

            }
            );

            $("#connexionButton2").click(function () {
              document.getElementById('formForgot').submit();
            });

            $("#connexionButton2").hover(
                    function ()
                    {
                      var src2 = $(this).attr("src");
                      var tab2 = src2.split('.');
                      tronc4 = tab2[0];
                      tronc5 = tab2[1];
                      src2 = tronc4 + "-on." + tronc5;
                      $(this).attr('src', src2);

                    }, function ()
            {
              var src2 = $(this).attr("src");
              var tab2 = src2.split('-');
              tronc4 = tab2[0];
              tronc6 = ".png"
              src2 = tronc4 + tronc6;
              $(this).attr('src', src2);

            }
            );

            $(".exitConnect").click(function ()
            {
              $("div#shadowConnect").fadeOut(300, function () {});
              $("#formConnect").fadeIn();
              $("#formForgot").find("*").css('display', 'none');
            });

            $("#forgotpassword").click(function ()
            {
              $("#formConnect").css('display', 'none');
              $("#formForgot").find("*").fadeIn(300, function () {});

            });
          });

        </script>
