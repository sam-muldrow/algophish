*/


        /* On replace le bacground bleu claire des partenaires du haut */
        var width_body = $("body").css("width");
        var wb = width_body.replace("px", "") / 2;
        $("div#partenaires_top_all").css("background-position", wb + "px 10px");

        /* Gestion hover pour les partenaires*/
        $("div.logo_partenaire_footer").hover(function ()
        {
          var logo = $(this).attr("id");
          var src = $(this).children("a").children("img").attr("src").split("/");
          var new_src = src[0] + "/" + src[1] + "/foot-on-" + logo;

          $(this).children("a").children("img").attr("src", new_src);
        }, function ()
        {
          var logo = $(this).attr("id");
          var src = $(this).children("a").children("img").attr("src").split("/");
          var new_src = src[0] + "/" + src[1] + "/foot-" + logo;

          $(this).children("a").children("img").attr("src", new_src);
        });
        $("div.logo_partenaire_top_header").hover(function ()
        {
          var logo = $(this).attr("id");
          var src = $(this).children("a").children("img").attr("src").split("/");
          var new_src = src[0] + "/" + src[1] + "/top-on-" + logo;

          $(this).children("a").children("img").attr("src", new_src);
        }, function ()
        {
          var logo = $(this).attr("id");
          var src = $(this).children("a").children("img").attr("src").split("/");
          var new_src = src[0] + "/" + src[1] + "/top-" + logo;

          $(this).children("a").children("img").attr("src", new_src);
        });

        /* Gestion hover reseaux sociaux top */
        $("div#social_top > a > img").hover(function ()
        {
          var src = $(this).attr("src");
          var src_on = src.replace(".png", "-on.png");
          $(this).attr("src", src_on);
        }, function ()
        {
          var src = $(this).attr("src");
          var src_on = src.replace("-on.png", ".png");
          $(this).attr("src", src_on);
        });

        /* Gestion du click dans le menu si on est pas sur le lien */
        $("ul#btnMenu > li").click(function ()
        {
          var dest = $(this).children("a").attr("href");

          if (dest != "#")
            $(location).attr('href', dest);
        });

        /* RAZ champ recherche lors du clique */
        $("input#chp_rec_top").focus(function ()
        {
          var val = $(this).val();

          if (val == "Rechercher...")
            $(this).val("");
        });
        $("input#chp_rec_top").blur(function ()
        {
          var val = $(this).val();

          if (val == "")
            $(this).val("Rechercher...");
        });


        /* RAZ champ recherche lors du clique */
        $("input#chp_rec_top2").focus(function ()
        {
          var val = $(this).val();

          if (val == "Rechercher...")
            $(this).val("");
        });
        $("input#chp_rec_top2").blur(function ()
        {
          var val = $(this).val();

          if (val == "")
            $(this).val("Rechercher...");
        });
        

        var widthFirstImg = $("div.partenaire_aff div:first-child").css("width").replace("px", "") * 1;
				
		setTimeout('animer_partenaire_footer('+(widthFirstImg + 20)+')', 1000);	

        /* GESTION DU HOVER DE L'IMAGE RSS EN FOOTER */
        $("li.img_rss").hover(function ()
        {
          $(this).children("a").children("img").attr("src", "images/rss-footer-on.png");
        }, function ()
        {
          $(this).children("a").children("img").attr("src", "images/rss-footer.png");
        });



        $('.btn-dark-blue').click(function ()
        {
          $('.alert').fadeOut(300, function () {});
          $('#overlay').fadeOut(300, function () {});


          $("#finishForgot").css('display', 'none');
          $("div#shadowConnect").fadeIn(300, function () {});

          $("div#moduleConnexion").mousemove(function ()
          {
            if (glob == true)
              glob = false;
          });
          $("div#moduleConnexion").mouseout(function () {
            glob = true;
          });
          $("div#shadowConnect").click(function ()
          {
            if (glob == true)
            {
              $("div#shadowConnect").fadeOut(300, function () {});
              $("#formConnect").fadeIn();
              $("#formForgot").find("*").css('display', 'none');
            }

          });
        });
        $('.btn-close').click(function ()
        {
          $('.alert').fadeOut(300, function () {});
          $('#overlay').fadeOut(300, function () {});
        });

          $('#moredetails').click(function ()
          {
            $('.alert').fadeIn(300, function () {});
            $('#overlay').fadeIn(300, function () {});
          });
        });

    </script>
