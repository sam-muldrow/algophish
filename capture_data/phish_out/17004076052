<script type="text/javascript">
	$(document).ready(function()
	{
		var curent_actu = 0;
		var chapeau     = new Array("En France on marche sur la tête", "On ne peut laisser les agriculteurs dans une impasse", "Les syndicats fustigent une attente trop longue", "La Maine et Loire en zone règlementée à partir du 18 novembre");
		var images      = new Array("/bibliotheque_image/Actualites/Syndical/Panneau_invers%C3%A9.jpg", "/bibliotheque_image/Actualites/Materiels%20agricole/20120217_pulve.JPG", "/bibliotheque_image/Actualites/Syndical/manif%20nantes%20influenza.jpg", "/bibliotheque_image/Actualites/Animaux/Vache%20viande/Vache%20viande.png");
		var link        = new Array("syndical--fdsea-ja-en-action-actualite-numero-8783.php", 
									"glyphosate--bruxelles-donne-son-feu-vert-pour-10-ans-actualite-numero-8782.php", 
									"influenza-aviaire--frsea-et-ja-des-pays-de-la-loire-ecrivent-au-ministre-de-l-agriculture-actualite-numero-8781.php", 
									"bovins--trois-cas-confirmes-de-mhe-en-vendee-actualite-numero-8780.php");
		
		function change_image(curent_actuA)
		{
			if(curent_actuA < 3)
				curent_actuA++;
			else
				curent_actuA = 0;
			
			$("div.cadre_actu").css("background-image", "url('"+images[curent_actuA]+"')");
			$("a#link_curent_actu").attr("href", link[curent_actuA]);
			
			$("div.fnd_chapeau, div.chapeau").animate(
			{
				right: 500,
			}, 500, function()
			{
				$("div.chapeau").html(chapeau[curent_actuA]);
			});
			$("img.actu_index_suite").animate(
			{
				right: 523,
			}, 500);
			
			$("div.img_actu img").fadeOut(1000, function()
			{
				$("div.img_actu img").attr("src", images[curent_actuA]);
				$("div.img_actu img").fadeIn(100);
			});
			
			$("div.titre_liste_actu").css("background-color", "#34495E");
			$("div.titre_liste_actu a").css("color", "#6084A9");
			$("div.titre_liste_actu a").css("font-weight", "normal");
			
			$("div.fnd_chapeau, div.chapeau").animate(
			{
				right: 45,
			}, 500);
			$("img.actu_index_suite").animate(
			{
				right: 32,
			}, 500);
			
			$("div#titre_actu_"+curent_actuA).css("background-color", "#2C3E50");
			$("div#titre_actu_"+curent_actuA+" a").css("color", "#FFFFFF");
			$("div#titre_actu_"+curent_actuA+" a").css("font-weight", "bold");
			
			return curent_actuA;
		}
		
		var caroussel = setInterval(function()
		{
			curent_actu = change_image(curent_actu);
		}, 9000);
		
		$("div.cadre_actu").hover(function()
		{
			clearInterval(caroussel);
		}, function()
		{
			caroussel = setInterval(function()
			{
				curent_actu = change_image(curent_actu);
			}, 9000);
		});
		
		$("div.titre_liste_actu").mouseenter(function()
		{	
			var id_tab = $(this).attr("id").split("_");
			var id     = id_tab[2];
			
			if(id != curent_actu)
			{
				if(id == 0)
					curent_actuB = 3;
				else
					curent_actuB = id - 1;
				
				curent_actu = curent_actuB;
				
				setTimeout(function()
				{
					if(curent_actuB == curent_actu)
						curent_actu = change_image(curent_actuB);
				}, 300);
			}
		});
		
		$("div.img_actu, div.chapeau, div.titre_liste_actu").click(function()
		{
			var link = $("a#link_curent_actu").attr("href");
			document.location.href = link;
		});
	});
</script>
