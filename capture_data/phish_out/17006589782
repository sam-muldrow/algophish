        		<script type="text/javascript">
		$(function() {
			$( "#ef" ).accordion({
				autoHeight: false,
				collapsible: true
			});
			$( "#tabs" ).tabs();
		});
		
		$(document).ready(function (){ 
			var imgArr1 = [];
			function setGallery1(){
				$("#pikame1 li").each(function(i){
					var img   = $(this).find('a').attr('href');
					var title = $(this).find('img').attr('alt');
					imgArr1[i] 	= 	{
										'href' 	: img,
										'title'	: title
									}
					$(this).find('a').attr("href", "javascript:;");
					$(this).find('img').attr("title", i);
				});
			}
			setGallery1();

			function showGallery1(num){
				$.fancybox(imgArr1, {
					'index'		    : num,
					'type'		    : 'image',
					'speedIn'		:	600, 
					'speedOut'		:	200, 
					'overlayShow'	:	false,
					'titlePosition' : 'over',
					'titleFormat'   : function(title, currentArray, currentIndex, currentOpts) {
						return '<span id="fancybox-title-over">Image ' +  (currentIndex + 1) + ' / ' + currentArray.length + ' ' + title + '</span>';
						}
				});
			}

			function updateImage1(self){
				var num	= self.active.attr("title");
				self.anchor.click(function(){ showGallery1(num) });
			}

			$("#pikame1").PikaChoose({ autoPlay:false, animationFinished:updateImage1 });
		});
		
		/*
		
		$(document).ready(function (){ 
			var a = function(self){ self.anchor.fancybox(); };
			
			$("#pikame" + 1).PikaChoose({
				buildFinished:a,
				carousel:true,
				autoPlay:false
			});
		});
		*/
		
</script>
