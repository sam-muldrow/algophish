        		<script type="text/javascript">
		$(function() {
			$( "#ef" ).accordion({
				autoHeight: false,
				collapsible: true
			});
			$( "#tabs" ).tabs();
		});
		
		$(document).ready(function (){ 
			var imgArr6 = [];
			function setGallery6(){
				$("#pikame6 li").each(function(i){
					var img   = $(this).find('a').attr('href');
					var title = $(this).find('img').attr('alt');
					imgArr6[i] 	= 	{
										'href' 	: img,
										'title'	: title
									}
					$(this).find('a').attr("href", "javascript:;");
					$(this).find('img').attr("title", i);
				});
			}
			setGallery6();

			function showGallery6(num){
				$.fancybox(imgArr6, {
					'index'		    : num,
					'type'		    : 'image',
					'speedIn'		:	600, 
					'speedOut'		:	200, 
					'overlayShow'	:	false,
					'titlePosition' : 'over',
					'titleFormat'   : function(title, currentArray, currentIndex, currentOpts) {
						return '<span id="fancybox-title-over">Image ' +  (currentIndex + 1) + ' / ' + currentArray.length + ' ' + title + '</span>';
						}
				});
			}

			function updateImage6(self){
				var num	= self.active.attr("title");
				self.anchor.click(function(){ showGallery6(num) });
			}

			$("#pikame6").PikaChoose({ autoPlay:false, animationFinished:updateImage6 });
		});
		
		/*
		
		$(document).ready(function (){ 
			var a = function(self){ self.anchor.fancybox(); };
			
			$("#pikame" + 6).PikaChoose({
				buildFinished:a,
				carousel:true,
				autoPlay:false
			});
		});
		*/
		
</script>
                
