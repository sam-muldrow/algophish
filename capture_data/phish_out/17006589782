        		<script type="text/javascript">
		$(function() {
			$( "#ef" ).accordion({
				autoHeight: false,
				collapsible: true
			});
			$( "#tabs" ).tabs();
		});
		
		$(document).ready(function (){ 
			var imgArr2 = [];
			function setGallery2(){
				$("#pikame2 li").each(function(i){
					var img   = $(this).find('a').attr('href');
					var title = $(this).find('img').attr('alt');
					imgArr2[i] 	= 	{
										'href' 	: img,
										'title'	: title
									}
					$(this).find('a').attr("href", "javascript:;");
					$(this).find('img').attr("title", i);
				});
			}
			setGallery2();

			function showGallery2(num){
				$.fancybox(imgArr2, {
					'index'		    : num,
					'type'		    : 'image',
					'speedIn'		:	600, 
					'speedOut'		:	200, 
					'overlayShow'	:	false,
					'titlePosition' : 'over',
					'titleFormat'   : function(title, currentArray, currentIndex, currentOpts) {
						return '<span id="fancybox-title-over">Image ' +  (currentIndex + 1) + ' / ' + currentArray.length + ' ' + title + '</span>';
						}
				});
			}

			function updateImage2(self){
				var num	= self.active.attr("title");
				self.anchor.click(function(){ showGallery2(num) });
			}

			$("#pikame2").PikaChoose({ autoPlay:false, animationFinished:updateImage2 });
		});
		
		/*
		
		$(document).ready(function (){ 
			var a = function(self){ self.anchor.fancybox(); };
			
			$("#pikame" + 2).PikaChoose({
				buildFinished:a,
				carousel:true,
				autoPlay:false
			});
		});
		*/
		
</script>
                
