		$(function() {
			$( "#ef" ).accordion({
				autoHeight: false,
				collapsible: true
			});
			$( "#tabs" ).tabs();
		});
		
		$(document).ready(function (){ 
			var imgArr3 = [];
			function setGallery3(){
				$("#pikame3 li").each(function(i){
					var img   = $(this).find('a').attr('href');
					var title = $(this).find('img').attr('alt');
					imgArr3[i] 	= 	{
										'href' 	: img,
										'title'	: title
									}
					$(this).find('a').attr("href", "javascript:;");
					$(this).find('img').attr("title", i);
				});
			}
			setGallery3();

			function showGallery3(num){
				$.fancybox(imgArr3, {
					'index'		    : num,
					'type'		    : 'image',
					'speedIn'		:	600, 
					'speedOut'		:	200, 
					'overlayShow'	:	false,
					'titlePosition' : 'over',
					'titleFormat'   : function(title, currentArray, currentIndex, currentOpts) {
						return '<span id="fancybox-title-over">Image ' +  (currentIndex + 1) + ' / ' + currentArray.length + ' ' + title + '</span>';
						}
				});
			}

			function updateImage3(self){
				var num	= self.active.attr("title");
				self.anchor.click(function(){ showGallery3(num) });
			}

			$("#pikame3").PikaChoose({ autoPlay:false, animationFinished:updateImage3 });
		});
		
