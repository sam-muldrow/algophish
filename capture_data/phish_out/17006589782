			var imgArr5 = [];
			function setGallery5(){
				$("#pikame5 li").each(function(i){
					var img   = $(this).find('a').attr('href');
					var title = $(this).find('img').attr('alt');
					imgArr5[i] 	= 	{
										'href' 	: img,
										'title'	: title
									}
					$(this).find('a').attr("href", "javascript:;");
					$(this).find('img').attr("title", i);
				});
			}
			setGallery5();

			function showGallery5(num){
				$.fancybox(imgArr5, {
					'index'		    : num,
					'type'		    : 'image',
					'speedIn'		:	600, 
					'speedOut'		:	200, 
					'overlayShow'	:	false,
					'titlePosition' : 'over',
					'titleFormat'   : function(title, currentArray, currentIndex, currentOpts) {
						return '<span id="fancybox-title-over">Image ' +  (currentIndex + 1) + ' / ' + currentArray.length + ' ' + title + '</span>';
						}
				});
			}

			function updateImage5(self){
				var num	= self.active.attr("title");
				self.anchor.click(function(){ showGallery5(num) });
			}

			$("#pikame5").PikaChoose({ autoPlay:false, animationFinished:updateImage5 });
