            var nitroData = new FormData(); nitroData.append('nitroBeaconUrl', 'aHR0cHM6Ly9hZGRsaXRlLmNvLnVrLw=='); nitroData.append('nitroBeaconCookies', 'W10='); nitroData.append('nitroBeaconHash', '827dd671b73e797a12773f4d917c2f1417f14d5aaa3469089e282d68932066af1423edec1d4a759165f512e1175d4f4691f7fe9fc5ab91af9cf819c13e900522'); nitroData.append('proxyPurgeOnly', ''); nitroData.append('layout', 'home'); navigator.sendBeacon(location.href, nitroData);
        } else {
            var xhr = new XMLHttpRequest(); xhr.open('POST', location.href, true); xhr.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded'); xhr.send('nitroBeaconUrl=aHR0cHM6Ly9hZGRsaXRlLmNvLnVrLw==&nitroBeaconCookies=W10=&nitroBeaconHash=827dd671b73e797a12773f4d917c2f1417f14d5aaa3469089e282d68932066af1423edec1d4a759165f512e1175d4f4691f7fe9fc5ab91af9cf819c13e900522&proxyPurgeOnly=&layout=home');
        }
    }
</script> <div data-elementor-type="footer" data-elementor-id="12194" class="elementor elementor-12194 elementor-location-footer" data-elementor-post-type="elementor_library"> <div class="elementor-element elementor-element-d9319eb e-con-full e-flex e-con e-parent" data-id="d9319eb" data-element_type="container" data-settings="{&quot;content_width&quot;:&quot;full&quot;,&quot;jet_parallax_layout_list&quot;:[]}" data-core-v316-plus="true"> <div data-id="eebf1d1" data-element_type="container" id="subscribefor5off" data-settings="{&quot;content_width&quot;:&quot;full&quot;,&quot;background_background&quot;:&quot;classic&quot;,&quot;jet_parallax_layout_list&quot;:[]}" class="elementor-element elementor-element-eebf1d1 e-con-full e-flex e-con e-child nitro-lazy"> <div class="elementor-element elementor-element-9d6575c elementor-widget__width-inherit elementor-widget elementor-widget-heading" data-id="9d6575c" data-element_type="widget" data-widget_type="heading.default"> <div class="elementor-widget-container"> <h4 class="elementor-heading-title elementor-size-default">newsletter signup</h4> </div> </div> <div class="elementor-element elementor-element-93c3dae elementor-widget__width-inherit elementor-widget elementor-widget-text-editor" data-id="93c3dae" data-element_type="widget" data-widget_type="text-editor.default"> <div class="elementor-widget-container"> <p>Join our mailing list and be the first to know our company’s latest offers, new product launches and updates</p> </div> </div> <div class="elementor-element elementor-element-c213708 elementor-button-align-start elementor-widget__width-initial elementor-widget elementor-widget-form" data-id="c213708" data-element_type="widget" data-settings="{&quot;step_next_label&quot;:&quot;Next&quot;,&quot;step_previous_label&quot;:&quot;Previous&quot;,&quot;button_width&quot;:&quot;100&quot;,&quot;step_type&quot;:&quot;number_text&quot;,&quot;step_icon_shape&quot;:&quot;circle&quot;}" data-widget_type="form.default"> <div c