<script>
	jQuery(document).ready(function($){
	if ($("body").hasClass("page-id-261342")) {   
        $(".talent-btn").css("display", "block");
    }else{
		$(".talent-btn").css("display", "none");
	}
		$('.side-menu-nav').children().each(function() {
		  $(this).css('right', '-' + $(this).find('div.content-btn').width() + 'px');
		})
		$('.side-menu-btn').mouseenter(function() {
		  $(this).stop().animate({
			'right': 0
		  }, 600);
		});

		$('.side-menu-btn').mouseleave(function() {
		  $(this).stop().animate({
			'right': '-' + $(this).find('div.content-btn').width() + 'px'
		  }, 600);
		});
		$('.talent-btn').click(function() {
			window.location.href='/careers/open-positions/';
 		});
 		$('.quote-btn').click(function() {
			window.location.href='contact-us';
 		});
		$('.subscribe-btn').click(function() {
			window.location.href='#';
		});
		/* code for blogs */
		$('#blogFeatured').click(function() {
			$("#blogMostRecent").removeClass("active");
			$("#blogFeatured").addClass("active");
			 $(".blog_content").css({"transform":"scale(0)", "height": '0', 'transition': 'all 0.5s ease-in-out'});
			 $(".blog_featured_content").css({"transform":"scale(1)", "height": 'auto', 'transition': 'all 0.5s ease-in-out'});
		});
		$('#blogMostRecent').click(function() {
			$('.blog_featured_content article:lt(6)').show();
			$("#blogMostRecent").addClass("active");
			$("#blogFeatured").removeClass("active");
			 $(".blog_featured_content").css({"transform":"scale(0)", "height": '0', 'transition': 'all 0.5s ease-in-out'});
			 $(".blog_content").css({"transform":"scale(1)", "height": 'auto', 'transition': 'all 0.5s ease-in-out'});
		});
// 		size_li_recent = $(".blog_content article").size();
		size_li_recent = $(".blog_content article").length;
		console.log(size_li_recent);
		rx=6;
		$('.blog_content article:lt(6)').show();
		$('.recent_show_more_blog').click(function () {
		rx= (rx+6 <= size_li_recent) ? rx+6 : size_li_recent;
		$('.blog_content article:lt('+rx+')').show();

		if(rx == size_li_recent){
		$('.recent_show_more_blog').hide();
		}
		});	
		
// 		size_li_feature = $(".blog_featured_content article").size();
		size_li_feature = $(".blog_featured_content article").length;
		fx=6;
		$('.blog_featured_content article:lt(6)').show();
		$('.featured_show_more_blog').click(function () {
		fx= (fx+6 <= size_li_feature) ? fx+6 : size_li_feature;
		$('.blog_featured_content article:lt('+fx+')').show();

		if(fx == size_li_feature){
		$('.featured_show_more_blog').hide();
		}
		});	
		/* code for blogs end */
		/* code for header styling for template page */
		function amountscrolled(){
			var winheight = $(window).height()
			var docheight = $(document).height()
			var scrollTop = $(window).scrollTop()
			var trackLength = docheight - winheight
			var pctScrolled = Math.floor(scrollTop/trackLength * 100) // gets percentage scrolled (ie: 80 NaN if tracklength == 0)
			return pctScrolled;
		}

// 		$(window).on("scroll", function(){
// 			var heightonScroll = amountscrolled();
// 			if(heightonScroll == 0){
// 				$( '#main-header' ).removeClass("et-fixed-header");			   
// 			}else{
// 				$( '#main-header' ).addClass("et-fixed-header");				   
// 			}
// 		})
// 		$(window).load(function() {
// 			var heightonLoad = amountscrolled();
// 			if(heightonLoad == 0){
// 			 		$( '#main-header' ).removeClass("et-fixed-header");				   
// 			   }else{
// 				$( '#main-header' ).addClass("et-fixed-header");				   
// 			}
// 		});
		
		$(window).on("scroll", function(){
// 			var heightonScroll = amountscrolled();
			var heightonScroll = $(window).scrollTop();
			if(heightonScroll == 0){
				$( '#main-header' ).removeClass("et-fixed-header");			   
			}else{
				$( '#main-header' ).addClass("et-fixed-header");				   
			}
		})
// 		$(window).load(function() {
// 			var heightonLoad = amountscrolled();
