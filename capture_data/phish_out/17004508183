<script>	
jQuery(document).ready(function( $ ) {	
	$(".foundation-action-section .et_pb_tabs_controls li").click(function(){
	 var scurrentthetab = jQuery(this).attr('class');
	  var clickscurrentthetab = scurrentthetab.replace("et_pb_tab_active", ""); 
		clickscurrentthetab = clickscurrentthetab.replace(/\s+/g, '');		
	$(".foundation-action-section .et_pb_tabs_controls").removeClass (function (index, className) {
    	return (className.match (/(^|\s)cet_pb_tab\S+/g) || []).join(' ');
	});	
     $(this).parent().addClass('mainul c'+clickscurrentthetab);
   });
	
});
</script>
<script>
(function($) {
    $(document).ready(function(){
        $('.et-readmore-toggle .et_pb_toggle_title').on('click', function(e) {
            if ($(e.target).closest('.et-readmore-toggle').hasClass('et_pb_toggle_open')) {
                $(this).text('Read More');
            } else {
                $(this).text('Read Less');
            }
        });
    });			
})(jQuery);
</script>
<script>
(function($) {
    $(document).ready(function(){
        $('.new-job-position').change( function() {
            var jobslocationtext = $(this).val();
            if(jobslocationtext=='indiacareer'){
                var gotojobslocationurl = 'https://innovacareers.peoplestrong.com/home';
                window.open(gotojobslocationurl, '_blank');
            }else if(jobslocationtext=='singaporecareer'){
                var gotojobslocationurl = 'https://www.voltinternational.com.sg/jobs?query=&amp;location=&amp;commit=&amp;source=statics.teams.cdn.office.net';
                window.open(gotojobslocationurl, '_blank');
            }else if(jobslocationtext=='benecareer'){
                var gotojobslocationurl = 'https://www.voltinternational.be/vacatures?source=statics.teams.cdn.office.net';
                window.open(gotojobslocationurl, '_blank');
            }if(jobslocationtext=='francecareer'){
                var gotojobslocationurl = 'https://www.voltinternational.fr/jobs?query=&amp;location=&amp;commit=&amp;source=statics.teams.cdn.office.net';
                window.open(gotojobslocationurl, '_blank');
            }else if(jobslocationtext=='ukcareer'){
                var gotojobslocationurl = 'https://www.voltinternational.com/jobs?query=&amp;location=&amp;commit=&amp;source=voltinternational.fr';
                window.open(gotojobslocationurl, '_blank');
            }else if(jobslocationtext=='usaintcareer'){
                var gotojobslocationurl = 'https://internalcareers-innovasolutions.icims.com/jobs/search?hashed=-626009968&amp;mobile=false&amp;width=1217&amp;height=500&amp;bga=true&amp;needsRedirect=false&amp;jan1offset=-300&amp;jun1offset=-240';
                window.open(gotojobslocationurl, '_blank');
            }else if(jobslocationtext=='usaworkclientcareer'){
                var gotojobslocationurl = 'https://www.innovasolutions.com/careers-usa/search.php#/jobs?innovasolutions';
                window.open(gotojobslocationurl, '_blank');
            }
            //location.href = gotojobslocationurl;
        });
    });			
})(jQuery);
</script>
