<script>
    jQuery(document).ready(function() {
    jQuery('.services-nav-tab ul.et_pb_tabs_controls li').each(function(){										
        //var servicestab = jQuery(this);
        //var scurrentTab = jQuery(this).attr('class');
        var scurrentTab = jQuery(this).text();  
        var removespacetext = scurrentTab.replace(/ /g,'');
        var orgtabtext = removespacetext.toLowerCase();
        var neworgtabtext	= orgtabtext.replace(/[0-9`~!@#$%^&*()_|+\-=?;:'",.<>\{\}\[\]\\\/]/gi,'');	      
        jQuery(this).attr("id", neworgtabtext);
    });

    //if(window.location.hash) {			
        //jQuery('.services-nav-tab ul.et_pb_tabs_controls li').removeClass('et_pb_tab_active');
        //jQuery('.services-nav-tab .et_pb_all_tabs .et_pb_tab').removeClass("et_pb_active_content et-pb-active-slide");
        //var tabhashid = window.location.hash.substring(1);
        //alert(tabhashid);        
        //var classTab = jQuery('#'+tabhashid).attr('class');
        //alert("hi=="+classTab);			
        //jQuery('.services-nav-tab ul.et_pb_tabs_controls li.'+classTab).addClass('et_pb_tab_active');
        //jQuery('.services-nav-tab .et_pb_all_tabs .et_pb_tab.'+classTab).addClass("et_pb_active_content et-pb-active-slide");	
