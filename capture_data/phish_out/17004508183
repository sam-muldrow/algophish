<script>
jQuery(document).ready(function($){
    //alert("0000000");
     var sloc = window.location.href;
     console.log(sloc);
     var sindex = sloc.indexOf("?");
     console.log(sloc.substr(sindex+1));
     var splitted = sloc.substr(sindex+1).split('&');
     //alert(splitted);
     console.log(splitted);
        if(splitted.length>0){
          var tabparams = splitted[0].split('=');
          //alert(tabparams[1]);
          if(tabparams[0]=='tab'){
                jQuery('.services-nav-tab ul.et_pb_tabs_controls li').removeClass('et_pb_tab_active');
                jQuery('.services-nav-tab .et_pb_all_tabs .et_pb_tab').removeClass("et_pb_active_content et-pb-active-slide");
                var tabclassid = tabparams[1];
                var NclassTab = jQuery('#'+tabclassid).attr('class');
                //alert(NclassTab);
                jQuery('.services-nav-tab ul.et_pb_tabs_controls li.'+NclassTab).addClass('et_pb_tab_active');                     
                jQuery('.services-nav-tab .et_pb_all_tabs .et_pb_tab.'+NclassTab).addClass("et_pb_active_content et-pb-active-slide"); 
			  jQuery('.'+tabclassid).addClass('servicegraybg');	
			  
			  //var headerheight = $(".services-nav-tab").outerHeight();					 
		//$('html, body').animate({ scrollTop: $('#'+tabclassid).offset().top-50 }, 'slow');
		if (jQuery("."+tabclassid).hasClass("servicegraybg") && (jQuery(window).width() < 700)) {
  jQuery('html, body').animate({ scrollTop: jQuery('#s'+tabclassid).offset().top-50 }, 'slow');
} else {
   jQuery('html, body').animate({ scrollTop: jQuery('#'+tabclassid).offset().top-50 }, 'slow'); 
}
                }
          }          
});
</script>
<script>
jQuery(document).ready(function () {
    jQuery('.left-slide-btn-area').click(function(){
        jQuery('.topleft-sidenav').css("width","100%");
        jQuery('.left-slide-btn-area').addClass("brandvideo");
    });
    jQuery('.topleft-sidenav .closebtn, .topleft-sidenav .et_custom_link').click(function(){
        setTimeout(function(){
        jQuery('.topleft-sidenav').css("width","0%");    
        jQuery('.left-slide-btn-area').removeClass("brandvideo");
        }, 100);
    });
});
</script>
