    var mtCounter = jQuery('.mt-counter').find('.elementor-widget-container').text().trim().replaceAll(',','').replace(' MT','');
    mtCounter = parseFloat(1100000)
    var start_date = new Date('2022-10-12 00:00:00')
    var now = new Date()
    var seconds_from_start_date = Math.abs(start_date.getTime() - now.getTime())/1000;
    mtCounter = mtCounter + (seconds_from_start_date*0.00012)
    
    if(mtCounter) {
     setInterval(() => {
        mtCounter = mtCounter + 0.00012;
         jQuery('.mt-counter').find('.elementor-widget-container').each(function() {
             jQuery(this).text(mtCounter.toLocaleString('en-US', {minimumFractionDigits: 5,
  maximumFractionDigits: 5})+ ' MT');
         })
     },1000)    
    }
   
    	     


    
   

