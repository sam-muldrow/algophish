.heateorSssCopyLinkSvg, .heateorSssEmailSvg {
	background-position: left;
}

.heateorSssEmailSvg::before {
  font-weight: bold;
	font-size: 16px;
  content: "Email";
	padding-left: 25px;
	padding-bottom: 10px;
}

.heateor_sss_twitter_tweet, .heateor_sss_linkedin_share{
	width: 75px !important;
}		</style>
		</head>

