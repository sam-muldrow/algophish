        <script type="text/javascript">
          var owl = $('.owl-carousel');
          owl.owlCarousel({
              items:1,
              loop:true,
              margin:10,
              autoplay:true,
              autoplayTimeout:2500,
              autoplayHoverPause:true,
              dots:true
          });
          $('.play').on('click',function(){
              owl.trigger('play.owl.autoplay',[2500])
          })
          $('.stop').on('click',function(){
              owl.trigger('stop.owl.autoplay')
          })

        </script>
