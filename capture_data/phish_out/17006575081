					// $(".mltdat1").click(function(){
					// $('#m_flight_datepicker1').datepicker('show');
					// });

						$("#m_flight_datepicker1").datepicker({
						minDate: 0,
						dateFormat: 'dd-mm-yy',
						numberOfMonths: 2,
						maxDate: "+1y",
						beforeShow: function(input, inst) {
						inst.dpDiv.addClass('notranslate');
						},
						onSelect: function(dateStr) { 

						var coDtSplt = dateStr.split("-");
						var checkOutFrom = new Date(coDtSplt[2], coDtSplt[1] - 1, coDtSplt[0]);
						var currentDate = checkOutFrom.getDate();
						var currentMonth = checkOutFrom.getMonth();
						var currentYear = checkOutFrom.getFullYear();
						var monthNames = ["January", "February", "March", "April", "May", "June",
						"July", "August", "September", "October", "November", "December"
						];
						$('.checkOutDateDiv1').text(currentDate);
						$('.checkOutMonthDiv1').text(monthNames[currentMonth]);
						$('.checkOutYearDiv1').text(currentYear);
						var endDate = $('#m_flight_datepicker1').datepicker('getDate');
						 $("#m_flight_datepicker2").datepicker( "option", "minDate", endDate );
						}
						});

						$(".changeCheckMultidate1").click(function(){
						 $("#m_flight_datepicker1").datepicker('show');
						 });

						});</script> 
					
