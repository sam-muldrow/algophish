        var select_val = '';
        $('.js-example-basic-single').select2();


        $("#flight_datepicker1").datepicker({
            minDate: 0,
            dateFormat: 'dd-mm-yy',
            numberOfMonths: 2,
            maxDate: "+1y",
            beforeShow: function(input, inst) {
                inst.dpDiv.addClass('notranslate');
            },
            onClose: function(dateStr) {
                //create date for the divs...
                if (dateStr != '') {
                    var ciDtSplt = dateStr.split("-");
                    var checkInFrom = new Date(ciDtSplt[2], ciDtSplt[1] - 1, ciDtSplt[0]);
                    var currentDate = checkInFrom.getDate();
                    var currentMonth = checkInFrom.getMonth();
                    var currentYear = checkInFrom.getFullYear();
                    var monthNames = ["January", "February", "March", "April", "May", "June",
                        "July", "August", "September", "October", "November", "December"
                    ];
                    //set dates in the div...
                    $('.checkInDateDiv').text(currentDate);
                    $('.checkInMonthDiv').text(monthNames[currentMonth]);
                    $('.checkInYearDiv').text(currentYear);

                    //set new date in the checkout calender...
                    var date2 = $('#flight_datepicker1').datepicker('getDate', '+1d');
                    console.log(date2);
                    date2.setDate(date2.getDate() + 1);
                    if ($('input[name=trip_type]:checked').val() == 'circle') {
                        $("#flight_datepicker2").datepicker("option", "minDate", date2);
                        $('#flight_datepicker2').datepicker('setDate', date2);
                        $("#flight_datepicker2").datepicker('show');

                        //set new auto date in the checkout div
                        var checkOutDate_auto = date2.getDate();
                        var checkOutMonth_auto = date2.getMonth();
                        var checkOutYear_auto = date2.getFullYear();

                        $('.checkOutDateDiv').text(checkOutDate_auto);
                        $('.checkOutMonthDiv').text(monthNames[checkOutMonth_auto]);
                        $('.checkOutYearDiv').text(checkOutYear_auto);
                    }
                }

            }

        });


        $(".changeCheckIndate").click(function() {
            $("#flight_datepicker1").datepicker('show');
        });


        $("#flight_datepicker2").datepicker({
            minDate: 0,
            dateFormat: 'dd-mm-yy',
            numberOfMonths: 2,
            maxDate: "+1y",
            beforeShow: function(input, inst) {
                inst.dpDiv.addClass('notranslate');
            },
            onSelect: function(dateStr) {

                var coDtSplt = dateStr.split("-");
                var checkOutFrom = new Date(coDtSplt[2], coDtSplt[1] - 1, coDtSplt[0]);
                var currentDate = checkOutFrom.getDate();
                var currentMonth = checkOutFrom.getMonth();
                var currentYear = checkOutFrom.getFullYear();
                var monthNames = ["January", "February", "March", "April", "May", "June",
                    "July", "August", "September", "October", "November", "December"
                ];
                $('.checkOutDateDiv').text(currentDate);
                $('.checkOutMonthDiv').text(monthNames[currentMonth]);
                $('.checkOutYearDiv').text(currentYear);

            }
        });


        $(".changeCheckOutdate").click(function() {
            $("#flight_datepicker2").datepicker('show');
        });

    });


    //Flight source swap
    $(document).ready(function() {
        var select_val = '';
        //alert(select_val);
        $('.js-example-basic-single').select2();
        //$('.js-example-basic-single').select2().select2('val',select_val)
        $(".flight_chnge").click(function() {
            var from = $('#from').val();
            var from_loc_id = $('#from_loc_id').val();

            var to = $('#to').val();
            var to_loc_id = $('#to_loc_id').val();


            $('#from').val(to);
            $('#to').val(from);

            $('#from_loc_id').val(to_loc_id);
            $('#to_loc_id').val(from_loc_id);

            $(".flight_chnge .fa-exchange-alt").toggleClass('rot_arrow');
        });
    });</script> </div>                        
