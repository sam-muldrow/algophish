        var next = $(this).next();
        if (!next.length) {
            next = $(this).siblings(':first');
        }
        next.children(':first-child').clone().appendTo($(this));

        for (var i = 0; i < 2; i++) {
            next = next.next();
            if (!next.length) {
                next = $(this).siblings(':first');
            }

            next.children(':first-child').clone().appendTo($(this));
        }
    });


    $(document).ready(function() {
        $(".flght").click(function() {
            $(".flight_trips").show()
        });

        $(".hotl").click(function() {
            $(".flight_trips").hide()
        });

        $('.start_sub').click(function() {
            event.preventDefault();
            var email = $('#email_id').val();
            var contact = $('#contact_no').val();
            $.ajax({
                type: 'POST',
                url: 'https://esumegh.com/general/email_subscription',
                data: {
                    email: email,
                    contact: contact
                },
                success: function(res) {
                    location.reload();

                }
            });
        });
