	<script>
    function showHideMenu() {
        let menuicon = document.getElementById('menuicon');
        let menupanel = document.getElementById('menupanel');
        if (menuicon.className == 'menuiconopen') {
            menuicon.setAttribute('class','menuiconclose')
            menupanel.setAttribute('class','menupanelopen')
        } else {
            menuicon.setAttribute('class','menuiconopen')
            menupanel.setAttribute('class','menupanelclose')
        }
     }
     document.getElementById('menuicon').addEventListener('click',showHideMenu);
    </script>
    <script>
    function preload(arrayOfImages) {
        $(arrayOfImages).each(function(){
            $('<img/>')[0].src = this;
        });
    }
    preload([
        'images/vapour_pressure_brochure-over.png',
        'images/far_red_brochure-over.png',
        'images/fresh_air_brochure-over.png',
        'images/condenser_brochure-over.png'
    ]);
    </script>
