    function ensure(fn) {
        return fn;
        return function() {
            try {
                return(fn.apply(this, arguments));
            } catch(e) {
                if(window.console && console.error) console.error("Reporting: "+e);
                Raven.captureException(e);
                // throw e;
            }
        };
    }

    $ensure = function(f){
        return jQuery(ensure(f));
    };
    </script>
