                jQuery.ajax({
                url: '/api/upge/cheetah-photo-search/video_details02',
                data: {'query': query,
                        'vsid': id
                },
                success: function(text){
                    jQuery('#details'+id).find('.moreDetails').html(jQuery(text));
                    jQuery('.rslides-box-details').hide();
                    updateHeight();
                    }
                });
    }

