				jQuery('.playlist-col-down').css('height', 'auto');
				jQuery('.share-buttons').show();		
				updateHeight();
				largePalyerOn = false

         	} else {
				playlistWidth = jQuery('.playlist-col').width();
				playlistHeight = jQuery('.playlist-col').height();
				
