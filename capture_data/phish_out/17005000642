	<script type="text/javascript">
	
	var course_buy = 40275.6106;
	var course_sell = 35808.8146;
	var course_buy_uah = 1554638.57;
	var course_sell_uah = 1382220.24;
	var course_buy_usdt_uah = 40.34;
	var course_sell_usdt_uah = 37.98;
	var course_buy_rur = 2416536.64;
	var course_sell_rur = 2148528.88;
	var io_comission = 0.04;

	var count_unprocessed_payment = 0 + 0;

		var p2p_ios;
		var audio_clock = null;

	function updateCourseFromServer() {
		$.get( "/json_data.php", function( data ) {
			course_buy = data.course_buy;
			course_sell = data.course_sell;

			
						if ( typeof(data.count_unprocessed_payment)!="undefined" && data.count_unprocessed_payment!==null && data.count_unprocessed_payment!=count_unprocessed_payment )
				location.reload();

			
						if (typeof(data.p2p_ios)!="undefined" && data.p2p_ios!==null &&
				typeof(p2p_ios)!="undefined" && p2p_ios!==null ) {

				const alertKey = 'play_once_' + data.p2p_ios.unic_str;

				if ((p2p_ios.out_lot_status == 0 && data.p2p_ios.out_lot_status == 1) ||
					(p2p_ios.out_lot_status == 2 && data.p2p_ios.out_lot_status == 3) ||
					(p2p_ios.in_lot_status == 1 && data.p2p_ios.in_lot_status == 2) ||
					p2p_ios.last_lot_4req < data.p2p_ios.last_lot_4req
					) {

					if (!localStorage.getItem(alertKey)) {
						console.log('clock_start '+alertKey);
						localStorage.setItem(alertKey, true);
						audio_clock = new Audio('/img/clock.mp3')
						audio_clock.loop = true;
						audio_clock.play();
					}

					let is_in = p2p_ios.last_lot_4req < data.p2p_ios.last_lot_4req || data.p2p_ios.in_lot_status == 2;

					if (data.p2p_ios.out_lot_status == 1 || data.p2p_ios.out_lot_status == 3)
						document.getElementById("img_p2p_alarm_out").style.display = '';
					if (is_in)
						document.getElementById("img_p2p_alarm_in").style.display = '';

					$('#img_header_alarm').css('display', '');
					$('#a_header_alarm').css('display', '');
					$("#a_header_alarm").attr('href', is_in ? '?p=26' : '?p=27');
				}

				//console.log('check audio: '+alertKey+' '+localStorage.getItem(alertKey)+' '+audio_clock.paused);
								if (audio_clock != null && !audio_clock.paused && !localStorage.getItem(alertKey)) {
					console.log('clock_stop');
					audio_clock.pause();
				}

			}

			p2p_ios = data.p2p_ios; 
			document.getElementById("course_buy_uah").innerHTML = data.course_buy_uah;
			document.getElementById("course_sell_uah").innerHTML = data.course_sell_uah;
		}, "json" );
	}
	setInterval(updateCourseFromServer, 10000);

	</script>

