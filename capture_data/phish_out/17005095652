<script>
  jQuery(document).ready(function( $ ){

	/*   var ua = window.navigator.userAgent;
	//Mozilla/5.0 (Windows NT 10.0; WOW64; Trident/7.0; .NET4.0C; .NET4.0E; InfoPath.3; rv:11.0) like Gecko
    var msie = ua.indexOf(".NET");

    if (msie > 0) // If Internet Explorer, return version number
    {
	  $('.primary-menu > li').css('float', 'right');
	  //console.log("jaber") ; 
    }
    else  // If another browser, return 0
    {
        console.log(ua) ; 
    }

*/
  
	
	
	
	
  var typingTimer;                //timer identifier
var doneTypingInterval =1000;  //time in ms, 5 second for example
var $input = $('#ageInput');

	
	
//on keyup, start the countdown
$input.on('keyup', function () {
  clearTimeout(typingTimer);
  // $input.val().replace('age:','') ; 
   
  typingTimer = setTimeout(doneTyping, doneTypingInterval);
});

//on keydown, clear the countdown 
$input.on('keydown', function () {
  clearTimeout(typingTimer);
});

//user is "finished typing," do something
function doneTyping () {
  //do something

  $input.val('Age:' + $input.val().replace( /^\D+/g, ''));  
 
 } 

});

</script>

