    <script>
        $('.webmap').click(function (){
            layer.open({
                type: 2,
                title: '网站地图',
                shade: 0.3,
                shadeClose: true,
                maxmin: false,
                scrollbar: false,
                area: ['550px', '440px'],
                content: ["/index/index/sitemap.html"]
            });
        });
        // 分页
        $('.page-btn').click(function (){
            var lastPage = Number('');
            var page = $('.page-inner input[name=page]').val();
            if(page>lastPage){
                layer.msg('不能超过最大分页数');
                return false;
            } else {
                var href = $(this).attr('data-href');
                $(this).attr('href',href+page);
            }

        });
    </script>
