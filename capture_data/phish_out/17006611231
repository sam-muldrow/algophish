<script>

    $(document).ready(function () {
	
		if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
			$(".pinner-body[data-num='2']").closest(".rv-module").css("min-height", "auto");
		}
	
    
        $.ajax({

            url: "/_webmoduledata/pinner/settings-2.json",
            dataType: "json",
            success: function (data) {

                var layout = kenesisGetSaveData("layout" , data);
                var size = kenesisGetSaveData("size" , data);
				
				$(".pinner-body[data-num='2']").attr({"data-layout": layout, "data-size": size})
				
                $.ajax({

                    url: "/webmodules/pinner/onsite/pinner-followup.asp",
                    type: "post",
                    data: { 
                        "PinnerNumber": "2",
                        "layout": layout,
                        "size": size,
                        "IMGpath": "/cache/cached_1920x0_Clients1.png"
                    },
                    success: function (data) {

                        $(".pinner-body[data-num='2']").html(data);

                    }

                });
    
            }

        });
    
    });
    
    function kenesisGetSaveData(c, object) {

        var v = "";

        if (typeof object != "undefined") {

            for (var i = 0; i < object.data.length; i++) {

                var objT = object.data[i].type;
                var objC = object.data[i].class;
    
                if (c == objC) {

                    v = object.data[i].value;

                    return v;

                }

            }

        }

        return v;

    }

</script>
