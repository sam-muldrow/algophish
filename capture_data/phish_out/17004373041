    <script>
    $(document).ready(function(){
      var imgIndex = 0;

      bgAutoPlay();
      setInterval(bgAutoPlay, 5000);

      function bgAutoPlay (){
        var ismobile = "desktop";

        if(imgIndex == 15){
          imgIndex = 0;
        }
        $( "."+ ismobile +" .slider_container .img"+ (imgIndex+1)).fadeIn(2000).siblings().not(".img_p1").fadeOut(2000);
        imgIndex++;
      }
    });

    function hashChangeHandler(e) {

      var num = window.location.hash.match(/regpage\/(\d+)$/);
      var pageNum = num ? num[1] : 1;

      // only step 1 use different bg
      if (pageNum == "1") {
        $("div.reg-form").addClass("reg-form-firstpage");
        $(".bg_ghost").css("opacity", 0);
        $(".slider_container div:first-child").css("opacity", 1);
      }else{
        $("div.reg-form").removeClass("reg-form-firstpage");
        $(".bg_ghost").animate({ opacity: 1}, 1000); // can't use fadeIn (mobile no need to show)
        $(".slider_container div:first-child").css("opacity", 0);
      }

      // page 14 Fix couple
      if (pageNum == "15" && $("#form-orientation2").is(":visible"))
      {
        $("div.reg-form").addClass("reg-form-toolong");
      } else{
        $("div.reg-form").removeClass("reg-form-toolong");
      }

      // [Cannot remove] for Back End logic when submit: It would fail when the input[name='action']'s value is more than 10 pages.
      if (pageNum == "16") {
        $('input[name="title"]').focus(function(){
          $("input[name='action']").prop("value","page5");
        });
      }

      $(".submit_alert").hide();
    }

    function btnToggleDisable (option) {
      $("div[data-form-page='"+ option +"'] .actions").addClass("disable");

      $("div[data-form-page='"+ option +"'] .cum_select label input").on("click", function() {
        var checkedNum = $("div[data-form-page='"+ option +"']").find("input:checked").length;
        var $nextBtn   = $("div[data-form-page='"+ option +"']").find("div.actions");

        if (checkedNum == 3) {
          $(this).attr("data-selectedindex","3");
        } else if(checkedNum > 3) {
            $("div[data-form-page='"+ option +"'] .cum_select label input[data-selectedindex='3']").prop("checked",false).attr("data-selectedindex","");
            $(this).attr("data-selectedindex","3");
        } else if(checkedNum < 3) {
           $("div[data-form-page='"+ option +"'] .cum_select label input[data-selectedindex='3']").attr("data-selectedindex","");
        }

        if ( checkedNum > 0 ) {
          $nextBtn.removeClass("disable");
        }else{
          $nextBtn.addClass("disable");
        }
      });
    }

    function isDateValid(y, m, d) {
      var d = new Date(y, m-1, d);
      if(d.getFullYear() == y && d.getMonth()+1 == m) {
          return true;
      } else {
          return false;
      }
    }

    function UpdateDaysByMonthandYear(_year, _month, _day){
      var selectedMonth = _month.val();
      if (selectedMonth != "2")
      {
        selectedMonth = (selectedMonth) ? selectedMonth : "1";
        if(isDateValid("2000", selectedMonth, "31")){
            SetDays(31, _day);
        } else {
            SetDays(30, _day);
        }
      } else {
        var selectedYear = _year.val() || 1980;
        if(isDateValid(selectedYear, selectedMonth, 29)){
            SetDays(29, _day);
        } else {
            SetDays(28, _day);
        }
      }
    }

    function SetDays(_dayInt, _daySelector){
      var days = _daySelector[0].length - 1;
      if(days <= _dayInt){
          for(var i = days+1; i <= _dayInt; i++){
              _daySelector.append($("<option></option>").text(i).val(i));
          }
      } else {
          for(var i = days; i > _dayInt; i--){
              _daySelector[0].removeChild(_daySelector[0][i]);
          }
      }
    }

    function AddAlertBoxAction(){
      var FFN_MC_confirmAlert = (function($) {
          function Unload( alert ) {
              var stopNav = function () {
                  $( window ).off( "beforeunload" );
              };

              if ( alert.trigger && alert.trigger.match( /^(true|1)$/ ) ) {
                  $( "a, button, input", document ).on( "click", stopNav );
                  $( window ).on( "beforeunload", function () {
                      return alert.message
                  } );
              }
          }
          return {
              Unload: Unload
          }
      }(jQuery));

      var FFN = {};
      FFN.MC = FFN.MC || {};
      FFN.MC.Confirm = new FFN_MC_confirmAlert.Unload({
        trigger: "",
        message: "12 members near you are online and ready to fuck."
      });
    }

    $(window).load(function() {
      $('[name="CUPID_body_type"]').after('<div class="indicator"><div class="indicator-mask"></div></div>');

      $("#form-preferred-bodytype .indicator-mask").each(function(){
        $(this).click(function(){
          if($(this).parent().siblings("input").is(":checked")){
            setTimeout(function(){ $("#form-preferred-bodytype label.b00 input").click();},10);
          }
        });
      });

      // adjust bg size
      var winWidth = $(window).width();
      $(".slider_container div").width(winWidth);

      $(window).resize(function() {
        var winWidth = $(window).width();
        $(".slider_container div").width(winWidth);
      });

      //custom select toggle btn disable or not
      btnToggleDisable(9);
      btnToggleDisable(10);

      $( '#form-content form[name="reg"]' ).on( 'keyup keypress', function(e) {
        var keyCode = e.keyCode || e.which;
        if (keyCode === 13) {
          e.preventDefault();
          return false;
        }
      });

      // ------------------ Add event listener
      $(window).on('hashchange', hashChangeHandler);

      $("#bday_month, #bday_year").on("change", function(){
        UpdateDaysByMonthandYear($("#bday_year"), $("#bday_month"), $("#bday_day"));
      });
      $("#bday_month2, #bday_year2").on("change", function(){
        UpdateDaysByMonthandYear($("#bday_year2"), $("#bday_month2"), $("#bday_day2"));
      });

      // ------------------ init
      // Reset birthday (Month-Day-Year)
      UpdateDaysByMonthandYear($("#bday_year"), $("#bday_month"), $("#bday_day"));
      UpdateDaysByMonthandYear($("#bday_year2"), $("#bday_month2"), $("#bday_day2"));

      // Remove useless empty string on validation error messages and tip messages.
      $(".help-block, .note-block").each(function() {
        var blockText = $(this).html();
        if ( blockText !== '' && blockText.trim() === '') {
          $(this).html('');
        }
      });

      // Hide loading page
      $(".wrap-mask").hide();
      hashChangeHandler();
      AddAlertBoxAction();
    });
    </script>
  </body>
