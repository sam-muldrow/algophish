    var CD = CD || {};
    CD.Forms = CD.Forms || {};
    CD.Forms.Events = CD.Forms.Events || {};
    CD.Forms.Reg = CD.Forms.Reg || {};
    jQuery( window ).load(function(){
      $('.wrapper .reg-content').removeClass('vis-hidden');
      //add age cupid value
        $('[data-form-page="1"]').before('<input type="hidden" name="CUPID_min_age" value="">'+
                                         '<input type="hidden" name="CUPID_max_age" value="">');

        var arrAge = []
        $('[data-form-page="8"] button[data-form-submit="next"]').click(function(){
          if($('[data-form-page="8"] input[type="checkbox"]:checked').length > 0) {
            if($(".age_01").is(":checked")) {
              arrAge.push("18");
              arrAge.push("30");
            }
            if($(".age_02").is(":checked")) {
              arrAge.push("25");
              arrAge.push("45");
            }
            if($(".age_03").is(":checked")) {
              arrAge.push("30");
              arrAge.push("60");
            }
            if($(".age_04").is(":checked")) {
              arrAge.push("37");
              arrAge.push("70");
            }
            if($(".age_05").is(":checked")) {
              arrAge.push("46");
              arrAge.push("99");
            }
            var arrAgelength = arrAge.length;
            var ageMin = arrAge[0];
            var ageMax = arrAge[arrAgelength -1];

            $('[name="CUPID_min_age"]').attr("value",ageMin);
            $('[name="CUPID_max_age"]').attr("value",ageMax);

            $('[data-form-page="8"] .next_link').trigger("click");
          }
        });

        //add distance cupid value
        $('[data-form-page="1"]').before('<input type="hidden" name="CUPID_dist_max_value" value="">');

        var arrDistance = []
        $('[data-form-page="10"] button[data-form-submit="next"]').click(function(){
          if($('[data-form-page="10"] input[type="checkbox"]:checked').length > 0) {
            if($(".dis_01").is(":checked")) {
              arrDistance.push("5");
            }
            if($(".dis_02").is(":checked")) {
              arrDistance.push("10");
            }
            if($(".dis_03").is(":checked")) {
              arrDistance.push("20");
            }
            if($(".dis_04").is(":checked")) {
              arrDistance.push("50");
            }
            if($(".dis_05").is(":checked")) {
              arrDistance.push("100");
            }
            var arrDistancelength = arrDistance.length;
            var ageMax = arrDistance[arrDistancelength -1];

            $('[name="CUPID_dist_max_value"]').attr("value",ageMax);

            $('[data-form-page="10"] .next_link').trigger("click");
          }
        });
    })
