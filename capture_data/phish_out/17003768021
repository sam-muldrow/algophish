    jQuery(".yearCharityFooter").keyup(function (e) {
        let year = jQuery(".yearCharityFooter").val();

        yearRegex = /^\d{4}$/;

        if (year == "") {
            jQuery(".errorYearCharityFooter").html("Required");
        } else if (jQuery(this).val().length != 4) {
            jQuery(".errorYearCharityFooter").html("Invalid");
        } else if (!yearRegex.test(year)) {
            jQuery(".errorYearCharityFooter").html("Invalid");
        } else {
            jQuery(".errorYearCharityFooter").html("");
        }

    });

