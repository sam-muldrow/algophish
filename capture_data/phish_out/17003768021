document.addEventListener("DOMContentLoaded", function () {
  // Find the breadcrumb element
  var breadcrumb = document.querySelector(".breadcrumb");

  // Check if the breadcrumb is empty (no child elements)
  if (breadcrumb && !breadcrumb.hasChildNodes()) {
    // Add a unique class to the breadcrumb for styling or hiding
    breadcrumb.classList.add("empty-breadcrumb");
  }
});




// below code is for toggeled 
$(document).ready(function() {
  // Add a click event listener to menu items with submenus
  $('.navbar-default .menu-item-has-children').click(function(event) {
    // Find the submenu within the clicked menu item
    var submenu = $(this).find('.sub-menu');

    // Toggle the submenu's visibility
    submenu.toggle();

    // Close all other open submenus
    $('.navbar-default .menu-item-has-children .sub-menu').not(submenu).hide();
  });

  // Add the arrow indicator to parent menu items
  $('.navbar-default .menu-item-has-children > a').append('<span class="arrow-indicator">â–¾</span>');

  // Prevent the default link behavior for parent menu links
  $('.navbar-default .menu-item-has-children > a').click(function(event) {
    event.preventDefault();
  });
});</script> 
