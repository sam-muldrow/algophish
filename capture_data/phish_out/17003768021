// Get the current page URL
    var host = window.location.hostname;
    var key = 'hadModal_' + host;
    var hadModal = localStorage.getItem(key);

    if (!hadModal) {
      // If 'hadModal' is not found in localStorage for the current URL, trigger the modal to open after a delay.
      setTimeout(function() {
          $(".popup_new_btn").trigger("click");
      }, 2000);
    }

    jQuery('#myModal').on('shown.bs.modal', function () {
      // When the modal is shown, set 'hadModal' in localStorage for the current URL to true.
      localStorage.setItem(key, true);
    })

    setTimeout(function() {
      $( ".custom_popup .close" ).trigger( "click" );
    }, 12000);


  });</script> 
