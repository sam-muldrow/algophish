    jQuery('#txtZipCodeCharityFooter').on('keypress', function (e) {
        let txtZipCode = jQuery(".txtZipCodeCharityFooter").val();


        if (((jQuery(this).val().length) <= 7)) {
            jQuery(".errortxtzipcodeChatiryFooter").html("");
        } else {
            e.preventDefault();
        }

        jQuery('#txtZipCodeCharityFooter').on('keyup', function (e) {
            if ((jQuery(this).val().length) > 0 && (jQuery(this).val().length) < 5) {
                jQuery(".errortxtzipcodeChatiryFooter").html("Invalid");
            } else if (((jQuery(this).val().length) >= 5) && ((jQuery(this).val().length) < 8)) {
                jQuery(".errortxtzipcodeChatiryFooter").html("");
            } else if ((jQuery(this).val().length) == 0) {
                jQuery(".errortxtzipcodeChatiryFooter").html("Required");
            } else {
                jQuery(".errortxtzipcodeChatiryFooter").html("Invalid");
            }
        });


        regexpUs = /^[0-9]{5}(?:-[0-9]{4})?$/;
        regexpCa = /^[ABCEGHJ-NPRSTVXY]\d[ABCEGHJ-NPRSTV-Z][ -]?\d[ABCEGHJ-NPRSTV-Z]\d$/i;
        if ((txtZipCode != "") && (regexpCa.test(txtZipCode)) || (regexpUs.test(txtZipCode))) {
            jQuery(".errortxtzipcodeChatiryFooter").html("");
        }
        else {
            jQuery(".errortxtzipcodeChatiryFooter").html("Invalid");
        }

    });

