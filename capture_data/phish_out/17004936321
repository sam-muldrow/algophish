        function () {
          $(this).find('.sub-nav').toggle();
          $(this).find('.tablet-menu-item').toggleClass('active')
        }
     );
     
     $('.mobile-nav-dropdown').on('click', function() {
        console.log('Menu Clicked');
        $(this).toggleClass('active');
