          $('.hamburger-close').show();
          $('.hamburger').hide();
          $('.nav-menu-link, .mobile-nav-item').show();
          $('#searchNav').hide();
        } else {
          $('.hamburger-close').hide();
          $('.hamburger').show();
          $('.nav-menu-link, .mobile-nav-item').hide();
          $('.sub-nav').hide();
        }
      });
    } else {
      // Desktop Show Navigation Menu on Hover
      $(".nav-menu-link").hover(
        function () {
          if ($(this).hasClass('full-width-nav')) {
            $(this).find('.sub-nav').css('display', 'flex');
          } else {
            $(this).find('.sub-nav').show();
          }
          $('#searchNav').hide();
        },
        function () {
          $(this).find('.sub-nav').hide();
        }
      );
    }
    // Mobile Navigation: Each top-level navigation item should trigger the drop-down 
    if ($(window).width() < 991) {
      $("a.nav-link, a.nav-link-3").bind("click.myclick", function(event) {
        event.preventDefault();
        $(this).unbind(".myclick");
      });
    }
});

// Get URL parameter key
$.urlParam = function(name){
    var results = new RegExp('[\?&]' + name + '=([^&#]*)').exec(window.location.href);
    if(results !== null) {
      return results[1] || 0;
    } else {
      return 0;
    }
}


if($.urlParam('gspk') !== 0) {
  var gspkVar = $.urlParam('gspk');
  $('#field101674723').replaceWith('<input type="text" id="field101674723" name="field101674723" size="50" value="'+ gspkVar +'" class="fsField fsFormatText">');
  $('#field101779847').replaceWith('<input type="text" id="field101779847" name="field101779847" size="50" value="'+ gspkVar +'" class="fsField fsFormatText">');
}

