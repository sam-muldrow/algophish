    'utm_medium': $.urlParam(utmMedium),
    'utm_campaign': $.urlParam(utmCampaign),
    'source_detail': $.urlParam(sourceDetail),
    'refresh': null
  };

  var cookies = [];

  // Run through each parameter in the url that matches the parameters in paramArray
  $.each(paramArray, function(index, value) {

    var regExp = /[a-zA-Z]/g;

    // Check if URL parameter in url exist or not
    //_________________________________
    if(value != null) {

      var getUtmSource = localStorage.getItem(index);

      if(regExp.test(value)){
          // If specific parameters are in the URL
          // Create cookie with those existing URL parameters

          // If the UTM is not already set but exists in the URL
          // Create cookie
            // If gclid exist, get the value from gclid that has already been collected in the <head>
            if(index == utmGCLID) {
              //var gclidValue = JSON.parse(getUtmSource);
              cookies.push('gclid=' + $.urlParam(utmGCLID));
              //console.log('gclidValue ran ' + gclidValue["value"]);
            } else {
              localStorage.setItem(index, value);
              cookies.push(index + '=' + value); 
              console.log(index);
              console.log(value);

              //Apply UTM data parameters to hidden fields
              if($.urlParam(index) !== 0) {
          $('#'+index).val(value);
        }
            }
            console.log(index + ' url parameter detected');

            // Add the url parameters to all links so that the parameters remain in the address bar from page to page
            $("a").click(function(e) {
                e.preventDefault();

                var params = window.location.search,
                    dest = $(this).attr('href') + params;
                    console.log('FinalURL Parameters' + finalURL);

                // in my experience, a short timeout has helped overcome browser bugs
                window.setTimeout(function() {
                    window.location.href = dest;
                }, 100);
            });

            console.log(cookies);

      } else {
          // If No URL Parameters provided, but they exist as cookies
          // Check if cookie exist, if so then get and apply to URL, if not do nothing 

          if(getUtmSource != null){
            if (index == utmGSPK || index == utmGCLID || index == utmSource || index == utmMedium || index == utmCampaign || index == utmContent || index == sourceDetail) {
              //Apply to URL
              if(index == utmGCLID) {
                var gclidValue = JSON.parse(getUtmSource);
                cookies.push(insertParam(index, gclidValue["value"]));
              } else {
                cookies.push(insertParam(index, getUtmSource));  
              }
              console.log(cookies);
              //Apply UTM data parameters to hidden fields
        $('#'+index).val(getUtmSource);
        console.log('value='+getUtmSource);
            }
          }
      }
    } else {
      // If URL parameter in url does NOT exist
      var currentURLQuery = document.location.search + '&';
      if(currentURLQuery == '?tab=platform&' || currentURLQuery == '?tab=docs&' || currentURLQuery == '?tab=sign&') {
        var finalURL = currentURLQuery + cookies.join('&');
      } else {
        var finalURL = cookies.join('&');
      }
      //If cookies array is not empty
      if (cookies.length !== 0) {
        console.log('refresh');
        /*
        if($('#field101674723').length == 1 || $('#field101779847').length == 1) { 
          if($.urlParam('gspk') == 0) {
            //window.location.replace(window.location.href + '?' + finalURL);
          }
        }
        */

        $("a[href*='admin.formstack.com']").click(function(e) {
            e.preventDefault();

            var dest = $(this).attr('href') + '?' + finalURL;
            console.log('this ran = ' + finalURL);

            // in my experience, a short timeout has helped overcome browser bugs
            window.setTimeout(function() {
                window.location.href = dest;
            }, 100);
        });
      }
    } 
    // Will stop running after "last"
  });



  // Insert Parameters in the URL
  //_________________________________
  function insertParam(key, value) {
      key = encodeURIComponent(key);
      //value = encodeURIComponent(value);

      // kvp looks like ['key1=value1', 'key2=value2', ...]
      var kvp = document.location.search.substr(1).split('&');
      let i=0;

      for(; i<kvp.length; i++){
          if (kvp[i].startsWith(key + '=')) {
              let pair = kvp[i].split('=');
              pair[1] = value;
              kvp[i] = pair.join('=');
              break;
          }
      }

      if(i >= kvp.length){
          kvp[kvp.length] = [key,value].join('=');
      }

      kvp.shift();

      // can return this or...
      let params = kvp.join('&');

      // reload page with new params
      //document.location.search = params;
      return params;
  }

} // if 'thank-you' end


// Pass URL Query Params --> 
// All <a> tags containing a certain rel="" -->
$("a[rel~='keep-params']").click(function(e) {
    e.preventDefault();

    var params = window.location.search,
        dest = $(this).attr('href') + params;

    // in my experience, a short timeout has helped overcome browser bugs
    window.setTimeout(function() {
        window.location.href = dest;
    }, 100);
});
  
  
// --- GSPK FSID ---
var fsidSignupLinkModifier = function() {
  if (typeof(URL) === 'undefined'
    || typeof(URLSearchParams) === 'undefined'
    || typeof(localStorage) === 'undefined'
  ) {
    // Don't bother supporting really old browsers
    return;
  }

  document.addEventListener('DOMContentLoaded', function () {
    const localStorageToFsidMappings = {
      gspk: 'gspk',
      gclid: 'gclid',
      utm_content: 'internal_utm_content',
      utm_campaign: 'internal_utm_campaign',
      utm_source: 'internal_utm_source',
      utm_medium: 'internal_utm_medium',
      utm_terms: 'internal_utm_terms',
    };

    const attributesToInclude = {};
    Object.keys(localStorageToFsidMappings).forEach(function(localStorageKey) {
      var fsidKey = localStorageToFsidMappings[localStorageKey];
      var value = localStorage.getItem(localStorageKey);
      if (value) {
        attributesToInclude[fsidKey] = value;
      }
    });

    if (Object.keys(attributesToInclude).length === 0) {
      // Nothing to send along so no need to modify links;
      return;
    }

    document.querySelectorAll('a[fsid-params-passthrough~="true"]').forEach(function(link) {
      var href = link.href;
      var url = new URL(href);
      var searchParams = new URLSearchParams(url.search);

      Object.keys(attributesToInclude).forEach(function(key) {
        var value = attributesToInclude[key];
        searchParams.append(key, value);
      });

      url.search = searchParams.toString();

      link.href = url.toString();
    })
  });
}

fsidSignupLinkModifier();

