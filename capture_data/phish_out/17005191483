	            		rtl: $('body').hasClass('rtl'),
			            items: 5, 
			            responsive: {
			            	1025: {
			            		items: 5			            	},
			            	769: {
			            		items: 4			            	},
							577: {
                                items: 3                            },
			            	0: {
			            		items: 2			            	}
			            },
			            autoplay: true,
													autoplayHoverPause: true,	
									            autoplayTimeout: 5000,
			            dots: false,
			            nav: true,
			           	autoHeight: false,
			            slideBy:  'page',
			            navText:false,
						center: false,
			            loop: true,
						dragEndSpeed: 200,
			            onRefreshed: function() {
			            	$(window).resize();
			            }	
			            
			            
			            
					};

	                owl.owlCarousel(options);

					
		            
				});
