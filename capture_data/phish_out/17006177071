<!--Modal JS Script -->
<script type="text/javascript">
window.onload = () => {
    $('#onload').modal('show');
}
</script>

<script>
var baseUrl = 'http://dispora.bekasikota.go.id/';

kirim_pesan = () => {

    let data = new FormData();

    const contactName = $("#contact-name").val();
    const email = $("#contact-email").val();
    const subjek = $("#subjek").val();
    const message = $("#contact-messgae").val();

    data.append('name', contactName);
    data.append('email', email);
    data.append('subjek', subjek);
    data.append('message', message);

    $("#btn-submit").text("Loading Mohon Tunggu...");

    $.ajax({
        type: "POST",
        url: `${baseUrl}User/kirimKontakKami`,
        cache: false,
        contentType: false,
        processData: false,
        data: data,
        success: function(response) {
            $("#contact-name").val("");
            $("#contact-email").val("");
            $("#subjek").val("");
            $("#contact-messgae").val("");

            $("#btn-submit").text("Kirim");

            swal({
                title: "Berhasil",
                text: "Submit kontak kami berhasil",
                icon: "success",
                button: "Ok",
            });
        },
        error: function(err) {

            $("#btn-submit").text("Kirim");

            swal({
                title: "gagal",
                text: "Silahkan coba lagi",
                icon: "error",
                button: "Ok",
            });
        }
    });
}
</script>

