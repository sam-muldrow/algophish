		    			<button id="playpausebtn"></button>
                        <script>
                            var audio, playbtn, mutebtn, seek_bar;
                            function initAudioPlayer(){
                            	audio = new Audio();
                            	audio.src = "RayatGeet.mp3";
                            	audio.loop = false;
                            	//audio.play();
                            	// Set object references
                            	playbtn = document.getElementById("playpausebtn");
                            	// Add Event Handling
                            	playbtn.addEventListener("click",playPause);
                            	// Functions
                            	function playPause(){
                            		if(audio.paused){
                            		    audio.play();
                            		    playbtn.style.background = "url(img/logo_rayat_geet1.png) no-repeat";
                            	    } else {
                            		    audio.pause();
                            		    playbtn.style.background = "url(img/logo_rayat_geet.png) no-repeat";
                            	    }
                            	}
                            }
                            window.addEventListener("load", initAudioPlayer);
                        </script>
