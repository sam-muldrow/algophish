                            }
                        });

                        this.addEvent('cart:add', function(a, b, c, d) {
                            logDebug("fb_dynamic_ads_debug -> non-ajax add to cart action");
                            var form;
                            var options;
                            if (d == null) {
                                form = c;
                                options = b;
                            } else {
                                form = d[0];
                                options = c;
                            }
                            if (!options.preventSubmit) {
                                if (!form.querySelectorAll('[name=required]')[0]) {
                                    if (form.getAttribute('method') != 'get') {
                                        logDebug("fb_dynamic_ads_debug -> non-ajax add to cart action:");
                                        logDebug(form);
