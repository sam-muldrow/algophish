		/* BS5 active class for listgroup */
		// Get all list-group-item elements
		const listGroupItems = document.querySelectorAll('.list-group-item');
		// Add event listener to each list-group-item
		listGroupItems.forEach(item => {
			item.addEventListener('click', () => {
				// Remove active class from all list-group-items
				listGroupItems.forEach(i => i.classList.remove('active'));
				// Add active class to clicked item
				item.classList.add('active');
			});
		});
		/* BS4 active class for listgroup
		$('.list-group-item-action').on('click', function (e) {
