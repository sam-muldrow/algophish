		var w;
		function LogoutALL() {
			w = window.open("https://www.linkedin.com/m/logout/");
			// like this (with jquery)
			location.href="/v3/____/Logout.asp"
		};
		
		function setSelectedIndex_byval(s, valsearch) {
			// Loop through all the items in drop down list
			for (i = 0; i< s.options.length; i++) {
				if (s.options[i].value == valsearch) { // Item is found. Set its property and exit
					s.options[i].selected = true;
					break;
				}
			}
			return;
		};
		function confirmDelete(anchor) {
			if (confirm('Are you sure?')) {
				return true;
			}
			return false;
		};
		
		
		
		
		
