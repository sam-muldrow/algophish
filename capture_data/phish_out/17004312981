				$this.addClass('active');
			}
		});
		*/
		
		/* BS5 */
		const EditFolderModal = document.getElementById('EditFolderModal')
		if (EditFolderModal) {
			EditFolderModal.addEventListener('show.bs.modal', event => {
				const button = event.relatedTarget 				// Button that triggered the modal
				const var1 = button.getAttribute('data-name4') 				// Extract info from data-bs-* attributes
				const var2 = button.getAttribute('data-desc4') 				// Extract info from data-bs-* attributes
				const var3 = button.getAttribute('data-pri4') 				// Extract info from data-bs-* attributes
				const var4 = button.getAttribute('data-fav4') 				// Extract info from data-bs-* attributes
				const var5 = button.getAttribute('data-id4') 				// Extract info from data-bs-* attributes
				const var6 = button.getAttribute('data-count4') 			// Extract info from data-bs-* attributes
				document.getElementById("name4").value = var1;
				document.getElementById("desc4").value = var2;
				document.getElementById("pri4").checked = false;
				document.getElementById("fav4").checked = false;
				if (var3 == "1") {document.getElementById("pri4").checked = var3};
				if (var4 == "1") {document.getElementById("fav4").checked = var4};
				document.getElementById("id4").value = var5;
				document.getElementById("del4").setAttribute("href", '/v3/Cmd/?Yo8HZ55U9iOXwP6tvytHOFskYKVq6giUnkXojs97l4VkerfpNKecuni7Pqhp=5001&ID=' + var5 + '&R=/v3/Pages/Folders');
				document.getElementById("open4").setAttribute("href", '/v3/Pages/Folder/?C=' + var5 + '&MB=1');
				document.getElementById("count4").innerHTML = var6;
			})
		}
		/* BS4 active class for listgroup
