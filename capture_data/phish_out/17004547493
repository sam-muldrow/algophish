    function configureCommercialDropDownLists(ddl1,ddl2) {
    	
        var rim12 = new Array('155 R12',
							'145/80 R12'
							);
        
        var rim14 = new Array(  '185 R14',
								'195 R14',
								'215 R14C'
						
								);
        var rim15 = new Array(  '195/R 15',
								'205 R16',
								'215/65 R16',
								'217/70 R16',
								'215/75 R15',
								'235/75 R15'
								
								);
        var rim16 = new Array(  '235/70 R16',
								'237/85 R16',
								'700/16 Radial',
								'750/16 Radial'
								
								);
        var rim17 = new Array(  '237/65 R17'
								
								);
        var rim20 = new Array(  '1000/20 Radial',
								'1000/20 Nylon',
								'1000/20 Mining',
								'900/20 Radial',
								'900/20 Nylon',
								'825/20 Nylon'
								
								);
      
        switch (ddl1.value) {
            case 'Rim Size 12':
                ddl2.options.length = 0;
                for (i = 0; i < rim12.length; i++) {
                    createOption(ddl2, rim12[i], rim12[i]);
                }
                break;
            case 'Rim Size 13':
                ddl2.options.length = 0; 
            for (i = 0; i < rim13.length; i++) {
                createOption(ddl2, rim13[i], rim13[i]);
                }
                break;
            case 'Rim Size 14':
                ddl2.options.length = 0;
                for (i = 0; i < rim14.length; i++) {
                    createOption(ddl2, rim14[i], rim14[i]);
                }
                break;
            case 'Rim Size 15':
                ddl2.options.length = 0; 
            for (i = 0; i < rim15.length; i++) {
                createOption(ddl2, rim15[i], rim15[i]);
                }
                break;
            case 'Rim Size 16':
                ddl2.options.length = 0; 
            for (i = 0; i < rim16.length; i++) {
                createOption(ddl2, rim16[i], rim16[i]);
                }
                break;
            case 'Rim Size 17':
                ddl2.options.length = 0; 
            for (i = 0; i < rim17.length; i++) {
                createOption(ddl2, rim17[i], rim17[i]);
                }
                break;
            case 'Rim Size 20':
                ddl2.options.length = 0; 
            for (i = 0; i < rim20.length; i++) {
                createOption(ddl2, rim20[i], rim20[i]);
                }
                break;
                default:
                    ddl2.options.length = 0;
                break;
        }

    }

    function createOption(ddl, text, value) {
    	
        var opt = document.createElement('option');
        opt.value = value;
        opt.text = text;
        ddl.options.add(opt);
    }

     


