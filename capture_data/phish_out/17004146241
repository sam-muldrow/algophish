<script>
   jQuery(document).ready(function($) {
      var stmapdefault = '29.31975043584839, 47.94567234232974';
      var marker = {position:stmapdefault}
      var content = '';    
  
      jQuery('#map_canvas_nfu8').gmap({
         'scrollwheel':false,
         'zoom': 14,
         'center': stmapdefault,
         'mapTypeId':google.maps.MapTypeId.ROADMAP,
         'callback': function() {
            var self = this;
            self.addMarker(marker).on('click', function(){ 
               if(content){
                  self.openInfoWindow({'content': content}, self.instance.markers[0]);
               }                     
            });
         },
         panControl: true
      });
   });
</script></div></div></div></div></div>

