function initMapPhp() {
	// 					var location = {lat: 23.810331, lng: 90.412521};
	// 					var map = new google.maps.Map(document.getElementById("map"), {
	// 						zoom: 8,
	// 						center: location,
	// 						mapTypeId: 'hybrid'
	// 					});
	// 					var marker = new google.maps.Marker({
	// 						position: location,
	// 						map: map
	// 					})
	let country = "";
	try {
		const span1 = document.getElementById("luxer-form__country");
		country = span1.textContent;
	} catch (err) {console.log(err)}
	let city = "";
	try {
		const span2 = document.getElementById("luxer-form__city");
		city = span2.textContent;
	} catch (err) {console.log(err)}
	var lists = [];
	try {
		const mmtotal = document.querySelectorAll('#custom-address-latlong ul li');
		for(var i = 0; i < mmtotal.length; i++){
			const latLongEl = mmtotal[i].querySelectorAll('span');
			const latLongList = [];
			latLongList.push(parseFloat(latLongEl[0].textContent));
			latLongList.push(parseFloat(latLongEl[1].textContent));
			lists.push(latLongList);
		}
	} catch (err) {console.log("Special : ", err)}
	
	console.log(lists);
// 	var lists = {
// 		Bangladesh: {
// 			lat: 51.0543422,
// 			lng: 3.7174243
// 		},
// 		China: {
// 			lat: 22.396427,
// 			lng: 114.109497
// 		},
// 		Korea: {
// 			lat: 37.566536,
// 			lng: 126.977966
// 		}
// 	}
	if(lists.length > 1){
		var mapOptions = {
			center: new google.maps.LatLng(lists[0][0], lists[0][1]),
			zoom: 8,
			zoomControl: true,
			mapTypeControl: false,
			scaleControl: false,
			streetViewControl: false,
			rotateControl: false,
			fullscreenControl: false,
			styles: [{
					featureType: "all",
					elementType: "all",
					stylers: [{
							saturation: -100
						} // <-- THIS
					]
				}, {
					"featureType": "road.highway",
					elementType: "labels",
					stylers: [{
						visibility: "off"
					}]
				}, //turns off highway labels
				{
					"featureType": "road.arterial",
					elementType: "labels",
					stylers: [{
						visibility: "on"
					}]
				}, //turns off arterial roads labels
				{
					"featureType": "road.local",
					elementType: "labels",
					stylers: [{
						visibility: "on"
					}]
				}, //turns off local roads labels
				{
					"featureType": "poi",
					stylers: [{
						visibility: "off"
					}]
				}, {
					"featureType": "transit",
					stylers: [{
						visibility: "off"
					}]
				}
			]
		};
		var map = new google.maps.Map(document.getElementById('map'), mapOptions);
// 		var marker = new google.maps.Marker({
// 			position: new google.maps.LatLng(lists[0][0], lists[0][1]),
// 			map: map
// 		});
		
		for (var i = 0; i < lists.length; i++) {
			var marker = new google.maps.Marker({
				position: new google.maps.LatLng(lists[i][0], lists[i][1]),
				map: map
			});
		}
	}else{
		var mapOptions = {
			center: new google.maps.LatLng(lists[0][0], lists[0][1]),
			zoom: 8,
			zoomControl: true,
			mapTypeControl: false,
			scaleControl: false,
			streetViewControl: false,
			rotateControl: false,
			fullscreenControl: false,
			styles: [{
					featureType: "all",
					elementType: "all",
					stylers: [{
							saturation: -100
						} // <-- THIS
					]
				}, {
					"featureType": "road.highway",
					elementType: "labels",
					stylers: [{
						visibility: "off"
					}]
				}, //turns off highway labels
