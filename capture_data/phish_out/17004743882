function initMapJs() {
	let country = "";
	try {
		const span1 = document.getElementById("country-js");
		country = span1.value;
	} catch (err) {console.log(err)}
	let city = "";
	try {
		const span2 = document.getElementById("city-js");
		city = span2.value;
	} catch (err) {console.log(err)}
	var lists = {
		Bangladesh: {
			lat: 51.0543422,
			lng: 3.7174243
		},
		China: {
			lat: 22.396427,
			lng: 114.109497
		},
		Korea: {
			lat: 37.566536,
			lng: 126.977966
		}
	}
	if (country && city) {
		var choice = lists[country];
		var mapOptions = {
			center: choice,
			zoom: 10,
			zoomControl: true,
			mapTypeControl: false,
			scaleControl: false,
			streetViewControl: false,
			rotateControl: false,
			fullscreenControl: false,
			styles: [{
					featureType: "all",
					elementType: "all",
					stylers: [{
							saturation: -100
						} // <-- THIS
					]
				}, {
					"featureType": "road.highway",
					elementType: "labels",
					stylers: [{
						visibility: "off"
					}]
				}, //turns off highway labels
				{
					"featureType": "road.arterial",
					elementType: "labels",
					stylers: [{
						visibility: "on"
					}]
				}, //turns off arterial roads labels
				{
					"featureType": "road.local",
					elementType: "labels",
					stylers: [{
						visibility: "on"
					}]
				}, //turns off local roads labels
				{
					"featureType": "poi",
					stylers: [{
						visibility: "off"
					}]
				}, {
					"featureType": "transit",
					stylers: [{
						visibility: "off"
					}]
				}
			]
		};
		var map = new google.maps.Map(document.getElementById('map'), mapOptions);
		var marker = new google.maps.Marker({
			position: choice,
			map: map
		})
	}
}
try {
	const btn = document.getElementById("submit-btn");
	btn.addEventListener('click', initMapJs);
} catch (err) {
	console.log(err)
}
			
			
