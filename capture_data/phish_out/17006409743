<script>
jQuery('.gallery_item').click(function (){
	var w = jQuery(window).width();
	if(w < 750){
		
	}else{
		var item = jQuery(this).attr('item');
		var img = jQuery(this).css('background-image');
		
		var clean_image = img.substring(4, (img.length - 1));
		var ci = clean_image.split('"');
		if(ci[1] == null){
			jQuery('.gallery_show_cont').show();
			jQuery('.gallery_show_cont .gallery_show .image').html("");
			jQuery('.gallery_show_cont .gallery_show .image').append('<img src="'+clean_image+'" border="0" />');
			jQuery('.gallery_show_cont .gallery_show .btn .gal_btn_left').attr('item',item);
			jQuery('.gallery_show_cont .gallery_show .btn .gal_btn_right').attr('item',item);
		}else{
			jQuery('.gallery_show_cont').show();
			jQuery('.gallery_show_cont .gallery_show .image').html("");
			jQuery('.gallery_show_cont .gallery_show .image').append('<img src="'+ci[1]+'" border="0" />');
			jQuery('.gallery_show_cont .gallery_show .btn .gal_btn_left').attr('item',item);
			jQuery('.gallery_show_cont .gallery_show .btn .gal_btn_right').attr('item',item);
		}
	}
	
});

jQuery('.gal_btn_left').click(function (){	
	var sum = jQuery('.gallery_item').length - 1;
	var item = jQuery(this).attr('item');
	
	if(item == 0){
		jQuery('.gallery_item').each(function (){
			if(jQuery(this).attr('item') == sum){
				var img = jQuery(this).css('background-image');
				var clean_image = img.substring(4, (img.length - 1));			
				var ci = clean_image.split('"');
				if(ci[1] == null){
					jQuery('.gallery_show_cont').show();
					jQuery('.gallery_show_cont .gallery_show .image').html("");
					jQuery('.gallery_show_cont .gallery_show .image').append('<img src="'+clean_image+'" border="0" />');
					jQuery('.gallery_show_cont .gallery_show .btn .gal_btn_left').attr('item',item - 1);
					jQuery('.gallery_show_cont .gallery_show .btn .gal_btn_right').attr('item',item - 1);
				}else{
					jQuery('.gallery_show_cont').show();
					jQuery('.gallery_show_cont .gallery_show .image').html("");
					jQuery('.gallery_show_cont .gallery_show .image').append('<img src="'+ci[1]+'" border="0" />');
					jQuery('.gallery_show_cont .gallery_show .btn .gal_btn_left').attr('item',item - 1);
					jQuery('.gallery_show_cont .gallery_show .btn .gal_btn_right').attr('item',item - 1);
				}
			}
		});
	}else{
		jQuery('.gallery_item').each(function (){
			if(jQuery(this).attr('item') == (item - 1)){
				var img = jQuery(this).css('background-image');
				var clean_image = img.substring(4, (img.length - 1));			
				var ci = clean_image.split('"');
				if(ci[1] == null){
					jQuery('.gallery_show_cont').show();
					jQuery('.gallery_show_cont .gallery_show .image').html("");
					jQuery('.gallery_show_cont .gallery_show .image').append('<img src="'+clean_image+'" border="0" />');
					jQuery('.gallery_show_cont .gallery_show .btn .gal_btn_left').attr('item',item - 1);
					jQuery('.gallery_show_cont .gallery_show .btn .gal_btn_right').attr('item',item - 1);
				}else{
					jQuery('.gallery_show_cont').show();
					jQuery('.gallery_show_cont .gallery_show .image').html("");
					jQuery('.gallery_show_cont .gallery_show .image').append('<img src="'+ci[1]+'" border="0" />');
					jQuery('.gallery_show_cont .gallery_show .btn .gal_btn_left').attr('item',item - 1);
					jQuery('.gallery_show_cont .gallery_show .btn .gal_btn_right').attr('item',item - 1);
				}
			}
		});
	}
});

jQuery('.gal_btn_right').click(function (){	
	var sum = jQuery('.gallery_item').length - 1;
	var item = parseInt(jQuery(this).attr('item'));
	
	if(item == sum){
		jQuery('.gallery_item').each(function (){
			if(jQuery(this).attr('item') == 0){
				var img = jQuery(this).css('background-image');
				var clean_image = img.substring(4, (img.length - 1));			
				var ci = clean_image.split('"');
				if(ci[1] == null){
					jQuery('.gallery_show_cont').show();
					jQuery('.gallery_show_cont .gallery_show .image').html("");
					jQuery('.gallery_show_cont .gallery_show .image').append('<img src="'+clean_image+'" border="0" />');
					jQuery('.gallery_show_cont .gallery_show .btn .gal_btn_left').attr('item',0);
					jQuery('.gallery_show_cont .gallery_show .btn .gal_btn_right').attr('item',0);
				}else{
					jQuery('.gallery_show_cont').show();
					jQuery('.gallery_show_cont .gallery_show .image').html("");
					jQuery('.gallery_show_cont .gallery_show .image').append('<img src="'+ci[1]+'" border="0" />');
					jQuery('.gallery_show_cont .gallery_show .btn .gal_btn_left').attr('item',0);
					jQuery('.gallery_show_cont .gallery_show .btn .gal_btn_right').attr('item',0);
				}				
			}
		});
	}else{		
		jQuery('.gallery_item').each(function (){			
			if(jQuery(this).attr('item') == (item + 1)){				
				var img = jQuery(this).css('background-image');
				var clean_image = img.substring(4, (img.length - 1));				
				var ci = clean_image.split('"');
				if(ci[1] == null){
					jQuery('.gallery_show_cont').show();
					jQuery('.gallery_show_cont .gallery_show .image').html("");
					jQuery('.gallery_show_cont .gallery_show .image').append('<img src="'+clean_image+'" border="0" />');
					jQuery('.gallery_show_cont .gallery_show .btn .gal_btn_left').attr('item',0);
					jQuery('.gallery_show_cont .gallery_show .btn .gal_btn_right').attr('item',0);
				}else{
					jQuery('.gallery_show_cont').show();
					jQuery('.gallery_show_cont .gallery_show .image').html("");
					jQuery('.gallery_show_cont .gallery_show .image').append('<img src="'+ci[1]+'" border="0" />');
					jQuery('.gallery_show_cont .gallery_show .btn .gal_btn_left').attr('item',0);
					jQuery('.gallery_show_cont .gallery_show .btn .gal_btn_right').attr('item',0);
				}
			}
		});
	}
});

jQuery('.close_gal').click(function (){
	jQuery('.gallery_show_cont').hide();
});
</script>


