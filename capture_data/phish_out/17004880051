	setTimeout(function(){
	
		// Get the timers
		var timers = $('.et_pb_countdown_timer');
		
		timers.each(function(){
			
			var timer = $(this);
			
			// Get the label objects
			var days = timer.find('.days');
			var hours = timer.find('.hours');
			var mins = timer.find('.minutes');
			var secs = timer.find('.seconds');
			
