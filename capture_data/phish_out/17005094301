	var fadeTime = 300;
	$( ".keyword" ).autocomplete({
		appendTo: '#jobLocWrap',
		source: function( request, response ) {  
			$.ajax({
				url: 'https://www.careermarshal.com/home/get_Location',  
				type: 'post',
				dataType: "json",
				data: {
					//keyword: request.term,
					keyword:extractLast( request.term )
				},
				success: function( data ) {
					//response( data );
					response( $.ui.autocomplete.filter(data, extractLast( request.term )));
				}
			});
		},
		/*select: function (event, ui) {
			id = ui.item.id; // Give it a name of the_url, where to navigate to.
			$('.keyword').val(ui.item.name); // save selected id to input
		}*/
		select: function (event, ui) {
			var terms = split( this.value );
			terms.pop();
			terms.push( ui.item.value );
			terms.push( "" );
			this.value = terms.join( ", " );
			return false;
		}
	});
	
	var fadeTime = 300;
	$( ".keywordstitle" ).autocomplete({
		appendTo: '#titleKeyWrap',
		source: function( request, response ) {  
			$.ajax({
				url: 'https://www.careermarshal.com/home/get_Title',  
				type: 'post',
				dataType: "json",
				data: {
					//keyword: request.term,
					keyword:extractLast( request.term )
				},
				success: function( data ) {
					//response( data );
					response( $.ui.autocomplete.filter(data, extractLast( request.term )));
				}
			});
		},
		/*select: function (event, ui) {
			id = ui.item.id; // Give it a name of the_url, where to navigate to.
			$('.keywordstitle').val(ui.item.name); // save selected id to input
		}*/
		select: function (event, ui) {
			var terms = split( this.value );
			terms.pop();
			terms.push( ui.item.value );
			terms.push( "" );
			//this.value = terms.join( ", " );
			//$('.keywordstitle').val(terms.join( ", " ));
			$('.keywordstitle').val(terms.join( "" ));
			return false;
		}
	});
	function ApplyNow(job_id,emp_id,user_id){
		$.ajax({
				url: 'https://www.careermarshal.com/home/apply_now',  
				type: 'post',
				dataType: "html",
				data: {
					job_id:job_id,user_id:user_id,emp_id:emp_id
				},
				success: function( data ) {
					if(data=="unsuccess"){
						alert('You have already applied this job');
					}else{
						alert('You have applied this job successfully');
					}
				}
			});
		
	}
	
	function SavejobNow(job_id,emp_id,user_id){
		$.ajax({
				url: 'https://www.careermarshal.com/home/save_job',  
				type: 'post',
				dataType: "html",
				data: {
					job_id:job_id,user_id:user_id,emp_id:emp_id
				},
				success: function( data ) {
					if(data=="unsuccess"){
						alert('You have already save this job!');
					}else{
						alert('You have save this job successfully.');
					}
				}
			});
		
	}
	 
	$("#Sendmsgcall").click(function(){
		var name=$("#name").val();
		var email=$("#email").val();
		var company_name=$("#company_name").val();
		var location=$("#location").val();
		var message=$("#message").val();
		var phone=$("#phone").val();
		
		$.ajax({
			url: 'https://www.careermarshal.com/home/requrestCallBack',  
			type: 'post',
			dataType: "html",
			data: {
				name:name,email:email,company_name:company_name,location:location,message:message,phone:phone
			},
			success: function( data ) {
				if(data=="0"){
					$("#error").show()
					$("#error").html('error')
					$("#success").hide();
					$("#success").html();
				}else{
					$("#error").hide()
					$("#error").html()
					$("#success").show();
					$("#success").html('Thank you for choosing Career Marshal for your Recruitment and staffing needs. Our team will contact you within 24 hours');
				}
			}
		});
	}) 
