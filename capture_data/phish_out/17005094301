</script>
<script>
function isNumber(evt) {
    evt = (evt) ? evt : window.event;
    var charCode = (evt.which) ? evt.which : evt.keyCode;
    if (charCode > 31 && (charCode < 48 || charCode > 57)) {
        return false;
    }
    return true;
}
</script>
<script>
	function split( val ) {
		return val.split( /,\s*/ );
	}
	function extractLast( term ) {
		return split( term ).pop();
	}
	$(document).ready(function() { 
		setTimeout(function() {  
			$('#login-modal').modal(); 
		}, 5000); 
	});
	$(document).ready(function() {
		  $('.search_jobs').click(function() {
			var experience= $(".experince_jobs").val(); 
            var salary	 =  $(".salary_jobs").val();
			var location =	$(".keyword").val();
			var title	 =	$(".keywordstitle").val();	
			
			if(title ==''){ 
				$('.msgerror').css("color", "#dc1818c9;");
				$('.msgerror').text('Please fill keyword or tittle');
				return false;
			}else{
				var base_url="https://www.careermarshal.com/"; 
				$.ajax({
					url:base_url+'home/jobs/',  
					type: 'post',
					dataType: 'json',
					data: {experience:experience,salary:salary,location:location,title:title},
					success: function( data ) { 
						$("#Jobsection").html(data.msg);
						if(data.msg1 == ''){
							$("#hotJobsection").html('<div class="col-12 text-center alert alert-danger" role="alert">No jobs found.!</div>');
						}else{
							$("#hotJobsection").html(data.msg1);
						}
						
					}
				});
 
			} 
		  });
	});
	$('.clsLocation').click(function() {
		var experience=$(".experince_jobs").val(); 
        var salary=$(".salary_jobs").val();
	    var title=$(".keywordstitle").val();
		var data = { 'services[]' : []};
			 $("input:checked").each(function() {
				data['services[]'].push($(this).val()); 
			 });	
		var location=data['services[]'];
		
		var data = { 'salaries[]' : []};
			 $(".clsSalary:checked").each(function() {
				data['salaries[]'].push($(this).val()); 
			 });
		var Salary=data['salaries[]'];
		
		var base_url="https://www.careermarshal.com/"; 
		 $.ajax({
				url:base_url+'home/search_job_Location/',  
				type: 'post',
				dataType: "html",
				data: {title:title,location:location,salary:salary,experience:experience},
				success: function( data ) {
					$("#Jobsection").html(data);
				}
		});
	});
	
