<script>
    const swiperEl = document.getElementById("cat")
    Object.assign(swiperEl, {
      rewind: true,
      slidesPerView: 6,
      spaceBetween: 10,
      pagination: {
        clickable: true,
      },
      breakpoints: {
        640: {
          slidesPerView: 4,
          spaceBetween: 20,
        },
        768: {
          slidesPerView: 6,
          spaceBetween: 40,
        },
        992: {
          slidesPerView: 7,
          spaceBetween: 40,
        },
        1024: {
          slidesPerView: 10,
          spaceBetween: 50,
        },
      },
    });
    swiperEl.initialize();
</script>

