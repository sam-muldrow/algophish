        function reloadSwipperTop() {

            var swiper = new Swiper('#swipperprop10', {
                // Default parameters
                slidesPerView: 2,
                //init: true,
                spaceBetween: 10,
                paginationClickable: true,
                //pagination: '.swiper-pagination',
                //nextbutton: '.swiper-button-next',
                //prevButton: '.swiper-button-prev',
                navigation: {
                    nextEl: '#nextprop10',
                    prevEl: '#prevprop10',
                },
                pagination: {
                    el: '#paginationprop10',
                    clickable: true,
                },
                loop: false,
                // Responsive breakpoints
                breakpoints: {
                    // when window width is >= 320px
                    320: {
                        slidesPerView: 1,
                        spaceBetween: 0
                    },
                    // when window width is >= 480px
                    480: {
                        slidesPerView: 1,
                        spaceBetween: 0
                    },
                    // when window width is >= 640px
                    640: {
                        slidesPerView: 2,
                        spaceBetween: 30
                    },
                    992: {
                        slidesPerView: 3,
                        spaceBetween: 30
                    }
                }
            })
        }
        function reloadSwipperVentas() {

            var swiper = new Swiper('#swipperVentas', {
                // Default parameters
                slidesPerView: 2,
                //init: true,
                spaceBetween: 10,
                paginationClickable: true,
                //pagination: '.swiper-pagination',
                //nextbutton: '.swiper-button-next',
                //prevButton: '.swiper-button-prev',
                navigation: {
                    nextEl: '#nextVentas',
                    prevEl: '#prevVentas',
                },
                pagination: {
                    el: '#paginationVentas',
                    clickable: true,
                },
                loop: false,
                // Responsive breakpoints
                breakpoints: {
                    // when window width is >= 320px
                    320: {
                        slidesPerView: 1,
                        spaceBetween: 0
                    },
                    // when window width is >= 480px
                    480: {
                        slidesPerView: 1,
                        spaceBetween: 0
                    },
                    // when window width is >= 640px
                    640: {
                        slidesPerView: 2,
                        spaceBetween: 30
                    },
                    992: {
                        slidesPerView: 3,
                        spaceBetween: 30
                    }
                }
            })
        }
        function reloadSwipperArriendo() {

            var swiper = new Swiper('#swipperArriendo', {
                // Default parameters
                slidesPerView: 2,
                //init: true,
                spaceBetween: 10,
                paginationClickable: true,
                //pagination: '.swiper-pagination',
                //nextbutton: '.swiper-button-next',
                //prevButton: '.swiper-button-prev',
                navigation: {
                    nextEl: '#nextArriendo',
                    prevEl: '#prevArriendo',
                },
                pagination: {
                    el: '#paginationArriendo',
                    clickable: true,
                },
                loop: false,
                // Responsive breakpoints
                breakpoints: {
                    // when window width is >= 320px
                    320: {
                        slidesPerView: 1,
                        spaceBetween: 0
                    },
                    // when window width is >= 480px
                    480: {
                        slidesPerView: 1,
                        spaceBetween: 0
                    },
                    // when window width is >= 640px
                    640: {
                        slidesPerView: 2,
                        spaceBetween: 30
                    },
                    992: {
                        slidesPerView: 3,
                        spaceBetween: 30
                    }
                }
            })
        }

        var locations = [];

        var lati = null;
        var long = null;

