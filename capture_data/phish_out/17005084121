        function ValidarRegistro() {
            var pass = document.getElementById("PasswordRegistro").value;
            var pass2 = document.getElementById("PasswordRegistroR").value;
            var rutRegistro = document.getElementById("Rut").value;
            var nombre = document.getElementById("Nombres").value;
            var Apellidos = document.getElementById("Apellidos").value;
            var Apellidos2 = document.getElementById("Apellidos2").value;
            var Terminos = document.getElementById("Terminos");
            var Correo = document.getElementById("EmailRegistro").value;
            var SeleccionTipo = document.getElementById("SeleccionTipo").value;


            document.getElementById("PasswordRegistro").disabled = true;
            document.getElementById("PasswordRegistroR").disabled = true;
            document.getElementById("Rut").disabled = true;
            document.getElementById("Nombres").disabled = true;
            document.getElementById("Apellidos").disabled = true;
            document.getElementById("Terminos").disabled = true;
            document.getElementById("EmailRegistro").disabled = true;
            document.getElementById("SeleccionTipo").disabled = true;
            document.getElementById("ValidarR").disabled = true;
            document.getElementById("Apellidos2").disabled = true;

            $('#errorGeneral').text("");
            document.getElementById("errorGeneral").style.display = "none";

            var passcorrecto,
                pass2correcto, correctoCorreo1,
                rutcorrecto,
                nombrecorrecto,
                apellidocorrecto,
                apellidocorrecto2,
                TerminosCorrecto, ComunaCorrecta = 0;

            if (SeleccionTipo != "0") {
                ComunaCorrecta = 1;
                document.getElementById("ErrorSelect").style.display = "none";
            } else {
                $('#ErrorSelect').text("Campo requerido").css("color", "red");
                document.getElementById("ErrorSelect").style.display = "block";
                ComunaCorrecta = 0;
            }


            var regex2 = /^(?=.*\d)(?=.*[a-záéíóúüñ]).*[A-ZÁÉÍÓÚÜÑ]/;
            //contrseñas regex pass
            if (pass != "") {
                if (regex2.test(pass)) {
                    passcorrecto = 1;
                    document.getElementById("ErrorPass1").style.display = "none";
                } else {
                    passcorrecto = 0;
                    $('#ErrorPass1').text("Password insegura").css("color", "red");
                    document.getElementById("ErrorPass1").style.display = "block";
                }

            } else {
                $('#ErrorPass1').text("Campo requerido").css("color", "red");
                document.getElementById("ErrorPass1").style.display = "block";
                passcorrecto = 0;
            }


            //contrseñas regex pass2
            if (pass2 != "") {
                if (regex2.test(pass2)) {
                    pass2correcto = 1;
                    document.getElementById("ErrorPass2").style.display = "none";
                } else {
                    pass2correcto = 0;
                    $('#ErrorPass2').text("Password insegura").css("color", "red");
                    document.getElementById("ErrorPass2").style.display = "block";
                }

            } else {
                $('#ErrorPass2').text("Campo requerido").css("color", "red");
                document.getElementById("ErrorPass2").style.display = "block";
                pass2correcto = 0;
            }

            //contrseñas iguales
            if (pass != "" && pass2 != "") {
                if (regex2.test(pass2) && regex2.test(pass)) {
                    if (pass == pass2) {

                        passiguales = 1;
                        document.getElementById("ErrorPass2").style.display = "none";
                        document.getElementById("ErrorPass1").style.display = "none";
                    } else {
                        passiguales = 0;
                        $('#ErrorPass1').text("Password no son iguales").css("color", "red");
                        document.getElementById("ErrorPass1").style.display = "block";

                        $('#ErrorPass2').text("Contraseñas no son iguales").css("color", "red");
                        document.getElementById("ErrorPass2").style.display = "block";
                    }
                } else {
                    $('#ErrorPass1').text("Password insegura").css("color", "red");
                    document.getElementById("ErrorPass1").style.display = "block";
                    $('#ErrorPass2').text("Password insegura").css("color", "red");
                    document.getElementById("ErrorPass2").style.display = "block";
                    passiguales = 0;
                }
            }

            if (rutRegistro != "") {
                //alert(rut);
                if (rut == 1) {
                    rutcorrecto = 1;
                    document.getElementById("ErrorRut").style.display = "none";
                } else {
                    rutcorrecto = 0;
                    document.getElementById("ErrorRut").style.display = "block";
                }

            } else {
                $('#ErrorRut').text("Campo requerido").css("color", "red");
                document.getElementById("ErrorRut").style.display = "block";
                rutcorrecto = 0;
            }


            //nonmbre
            if (nombre != "") {
                nombrecorrecto = 1;
                document.getElementById("ErrorNombres").style.display = "none";
            } else {
                $('#ErrorNombres').text("Campo requerido").css("color", "red");
                document.getElementById("ErrorNombres").style.display = "inline-block";
                nombrecorrecto = 0;
            }
            //apellido
            if (Apellidos != "") {
                apellidocorrecto = 1;
                document.getElementById("ErrorApellidos").style.display = "none";
            } else {
                $('#ErrorApellidos').text("Campo requerido").css("color", "red");
                document.getElementById("ErrorApellidos").style.display = "inline-block";
                apellidocorrecto = 0;
            }
            //apellido
            if (Apellidos2 != "") {
                apellidocorrecto2 = 1;
                document.getElementById("ErrorApellidos2").style.display = "none";
            } else {
                $('#ErrorApellidos2').text("Campo requerido").css("color", "red");
                document.getElementById("ErrorApellidos2").style.display = "inline-block";
                apellidocorrecto2 = 0;
            }
            //terminos
            if (Terminos.checked == true) {

                TerminosCorrecto = 1;
                document.getElementById("errorTerminos").style.display = "none";
            } else {
                $('#errorTerminos').text("Debes aceptar los terminos").css("color", "red");
                document.getElementById("errorTerminos").style.display = "block";
                TerminosCorrecto = 0;
            }


            var regex = /^(([^<>()[\]\.,;:\s@\"]+(\.[^<>()[\]\.,;:\s@\"]+)*)|(\".+\"))@(([^<>()[\]\.,;:\s@\"]+\.)+[^<>()[\]\.,;:\s@\"]{2,})$/i;
            //contrseñas regex pass
            if (Correo != "") {
                if (regex.test(Correo)) {
                    document.getElementById("ErrorEmail1").style.display = "none";
                    correctoCorreo1 = 1;
                } else {
                    $('#ErrorEmail1').text("Formato no valido").css("color", "red");
                    document.getElementById("ErrorEmail1").style.display = "block";
                    correctoCorreo1 = 0;

                }

            } else {
                $('#ErrorEmail1').text("Campo requerido").css("color", "red");
                document.getElementById("ErrorEmail1").style.display = "block";
                correctoCorreo1 = 0;
            }

            if (pass2correcto == 1 && passcorrecto == 1 && correctoCorreo1 == 1 &&
                rutcorrecto == 1 && nombrecorrecto == 1 && apellidocorrecto == 1 && apellidocorrecto2 == 1 &&
                TerminosCorrecto == 1 && ComunaCorrecta == 1 && passiguales == 1) {

                grecaptcha.ready(function () {
                    window.grecaptcha.execute('6LfkhMUbAAAAAOqDp8BnxVaj80-go98gnOX6_VH5', { action: 'home' }).then(function (token) {

                        Registro(token);
                    });
                });

                function Registro(tokendata) {
                    var formdata = new FormData();
                    formdata.append("pass", pass);
                    formdata.append("rut", rutRegistro);
                    formdata.append("nombre", nombre);
                    formdata.append("apellidopate", Apellidos);
                    formdata.append("email", Correo);
                    formdata.append("tipo", SeleccionTipo);
                    formdata.append("token", tokendata);
                    formdata.append("apellidomate", Apellidos2);

                    var xhr = new XMLHttpRequest();
                    xhr.onreadystatechange = function () {
                        if (xhr.readyState == 4 && xhr.status == 200) {
                            var json = JSON.parse(xhr.responseText);
                            if (json.ClienteCreado == "ok") {

                                //document.getElementById("PasswordRegistro").disabled = true;
                                //document.getElementById("PasswordRegistroR").disabled = true;
                                //document.getElementById("Rut").disabled = true;
                                //document.getElementById("Nombres").disabled = true;
                                //document.getElementById("Apellidos").disabled = true;
                                //document.getElementById("Terminos").disabled = true;
                                //document.getElementById("EmailRegistro").disabled = true;
                                //document.getElementById("SeleccionTipo").disabled = true;
                                //document.getElementById("ValidarR").disabled = true;
                                //document.getElementById("Apellidos2").disabled = true;


                                document.getElementById("PasswordRegistro").disabled = false;
                                document.getElementById("PasswordRegistroR").disabled = false;
                                document.getElementById("Rut").disabled = false;
                                document.getElementById("Nombres").disabled = false;
                                document.getElementById("Apellidos").disabled = false;
                                document.getElementById("Terminos").disabled = false;
                                document.getElementById("EmailRegistro").disabled = false;
                                document.getElementById("SeleccionTipo").disabled = false;
                                document.getElementById("ValidarR").disabled = false;
                                document.getElementById("Apellidos2").disabled = false;

                                document.getElementById("PasswordRegistro").value = "";
                                document.getElementById("PasswordRegistroR").value = "";
                                document.getElementById("Rut").value = "";
                                document.getElementById("Nombres").value = "";
                                document.getElementById("Apellidos").value = "";
                                document.getElementById("Terminos").value = "";
                                document.getElementById("EmailRegistro").value = "";
                                document.getElementById("SeleccionTipo").value = "";
                                document.getElementById("ValidarR").value = "";
                                document.getElementById("Apellidos2").value = "";
                                document.getElementById("Terminos").checked = false;
                                //document.getElementById("ValidarR").disabled = false;




                                Swal.fire(
                                    'Registro Exitoso!',
                                    'Bienvenido a Capitel',
                                    'success'
                                ).then((result) => {
                                    /* Read more about isConfirmed, isDenied below */
                                    if (result.isConfirmed) {
                                        document.getElementById("btnlogin").click();
                                    } else {
                                        document.getElementById("btnlogin").click();
                                    }
                                })

                            }

                            else if (json.ErrorInsert == "Error") {

                                document.getElementById("PasswordRegistro").disabled = false;
                                document.getElementById("PasswordRegistroR").disabled = false;
                                document.getElementById("Rut").disabled = false;
                                document.getElementById("Nombres").disabled = false;
                                document.getElementById("Apellidos").disabled = false;
                                document.getElementById("Terminos").disabled = false;
                                document.getElementById("EmailRegistro").disabled = false;
                                document.getElementById("SeleccionTipo").disabled = false;
                                document.getElementById("ValidarR").disabled = false;
                                document.getElementById("Apellidos2").disabled = false;


                                $('#errorGeneral').text(json.Mensaje).css("color", "red");
                                document.getElementById("errorGeneral").style.display = "block";


                            } else if (json.DatosErroneos == "Error") {


                            }
                            else if (json.ClienteCaptcha == "Error") {

                                document.getElementById("PasswordRegistro").disabled = false;
                                document.getElementById("PasswordRegistroR").disabled = false;
                                document.getElementById("Rut").disabled = false;
                                document.getElementById("Nombres").disabled = false;
                                document.getElementById("Apellidos").disabled = false;
                                document.getElementById("Terminos").disabled = false;
                                document.getElementById("EmailRegistro").disabled = false;
                                document.getElementById("SeleccionTipo").disabled = false;
                                document.getElementById("ValidarR").disabled = false;
                                document.getElementById("Apellidos2").disabled = false;

                                $('#errorGeneral').text("Captcha incorrecto, intentalo nuevamente").css("color", "red");
                                document.getElementById("errorGeneral").style.display = "block";
                            }


                        } else if (xhr.readyState == 4 && xhr != 200) {
                            document.getElementById("PasswordRegistro").disabled = false;
                            document.getElementById("PasswordRegistroR").disabled = false;
                            document.getElementById("Rut").disabled = false;
                            document.getElementById("Nombres").disabled = false;
                            document.getElementById("Apellidos").disabled = false;
                            document.getElementById("Terminos").disabled = false;
                            document.getElementById("EmailRegistro").disabled = false;
                            document.getElementById("SeleccionTipo").disabled = false;
                            document.getElementById("ValidarR").disabled = false;
                            document.getElementById("Apellidos2").disabled = false;
                            $('#errorGeneral').text(json.Mensaje).css("color", "red");
                            document.getElementById("errorGeneral").style.display = "block";
                        }
                    };

                    xhr.open('POST', '/Login/Registro/');
                    xhr.send(formdata);

                }

            } else {
                document.getElementById("PasswordRegistro").disabled = false;
                document.getElementById("PasswordRegistroR").disabled = false;
                document.getElementById("Rut").disabled = false;
                document.getElementById("Nombres").disabled = false;
                document.getElementById("Apellidos").disabled = false;
                document.getElementById("Terminos").disabled = false;
                document.getElementById("EmailRegistro").disabled = false;
                document.getElementById("SeleccionTipo").disabled = false;
                document.getElementById("ValidarR").disabled = false;
                document.getElementById("Apellidos2").disabled = false;

            }

        }

        /* const $ErroEmailrecupera = $("#EmailUser2");*/
        document.getElementById("EmailUser2").addEventListener("change", function (elemento) {
            var regex = /^(([^<>()[\]\.,;:\s@\"]+(\.[^<>()[\]\.,;:\s@\"]+)*)|(\".+\"))@(([^<>()[\]\.,;:\s@\"]+\.)+[^<>()[\]\.,;:\s@\"]{2,})$/i;
            var data = document.getElementById("EmailUser2").value;
            //contrseñas regex pass
            if (data.value != "") {
                if (regex.test(data)) {
                    document.getElementById("ErroEmailrecupera").style.display = "none";

                } else {
                    document.getElementById("ErroEmailrecupera").innerText = "Formato no valido";
                    document.getElementById("ErroEmailrecupera").style.display = "block";

                }

            } else {
                /* $('#ErroEmailrecupera').text("Campo requerido").css("color", "red");*/
                document.getElementById("ErroEmailrecupera").innerText = "Campo requerido";
                document.getElementById("ErroEmailrecupera").style.display = "block";

            }

        });

        function RecuperaPasswordData() {

            var email = document.getElementById("EmailUser2").value;


                var correctoCorreo1 = 0;
                var regex = /^(([^<>()[\]\.,;:\s@\"]+(\.[^<>()[\]\.,;:\s@\"]+)*)|(\".+\"))@(([^<>()[\]\.,;:\s@\"]+\.)+[^<>()[\]\.,;:\s@\"]{2,})$/i;
                //contrseñas regex pass
                if (email != "") {
                    if (regex.test(email)) {
                        document.getElementById("ErroEmailrecupera").style.display = "none";
                        correctoCorreo1 = 1;
                    } else {
                        $('#ErroEmailrecupera').text("Formato no valido").css("color", "red");
                        document.getElementById("ErroEmailrecupera").style.display = "block";
                        correctoCorreo1 = 0;

                    }

                } else {
                    $('#ErroEmailrecupera').text("Campo requerido").css("color", "red");
                    document.getElementById("ErroEmailrecupera").style.display = "block";
                    correctoCorreo1 = 0;
                }

                if (correctoCorreo1 == 1) {
                     grecaptcha.ready(function () {
                    window.grecaptcha.execute('6LfkhMUbAAAAAOqDp8BnxVaj80-go98gnOX6_VH5', { action: 'home' }).then(function (token) {

                        Recupera(token);
                    });
                });

                    function Recupera(datToken) {
                        var formdata = new FormData();
                        formdata.append("email", email);
                        formdata.append("token", datToken);

                        var xhr = new XMLHttpRequest();
                        xhr.onreadystatechange = function () {
                            if (xhr.readyState == 4 && xhr.status == 200) {
                                var json = JSON.parse(xhr.responseText);
                                if (json.response == "ok") {

                                    document.getElementById("EmailUser2").disabled = false;
                                    document.getElementById("RecuperaPassword").disabled = false;
                                    document.getElementById("spinPass").style.display = "none";
                                    $("#login-Recupera").modal("hide");

                                    //location.reload();
                                    Swal.fire(
                                        'Correcto!',
                                        ""+json.mensaje+"",
                                        'success'
                                    )


                                }

                                else if (json.response == "error") {

                                    document.getElementById("EmailUser2").disabled = false;
                                    document.getElementById("RecuperaPassword").disabled = false;
                                    document.getElementById("spinPass").style.display = "none";

                                    document.getElementById("ErroEmailrecupera").innerText = json.mensaje ;
                                    document.getElementById("ErroEmailrecupera").style.display = "block";


                                }

                            } else if (xhr.readyState == 4 && xhr != 200) {
                                document.getElementById("EmailUser2").disabled = false;
                                document.getElementById("RecuperaPassword").disabled = false;
                                document.getElementById("spinPass").style.display = "none";
                                document.getElementById("ErroEmailrecupera").innerText = "tenenmos problemas, intentalo nuevamente";
                            }
                        };

                        xhr.open('POST', '/Login/CambiarPassword/');
                        xhr.send(formdata);
                    }

                }

            }


        function AbrirModalRecupera() {

            $("#login-Recupera").modal("toggle");
            $("#login-register").modal("hide");
        }

        function Buscarsession(elemento) {

            var xhr = new XMLHttpRequest();
            xhr.onreadystatechange = function () {
                if (xhr.readyState == 4 && xhr.status == 200) {
                    var json = JSON.parse(xhr.response);
                    console.log(json);
                    console.log(xhr.response);
                    if (json.mensaje == "Activo") {
                        location.href = "/Home/PropDetalle?tag_pro=" + elemento.id;
                    }

                }
                else if (xhr.status == 401) {
                    Swal.fire(
                        'Atención!',
                        'Para visualizar una propiedad debes tener una cuenta en Capitel',
                        'warning'
                    )
                }
            };
            xhr.open('POST', '/Login/BuscarSession/');
            xhr.send();
        }

        SessionActiva();
        function SessionActiva() {

            var xhr = new XMLHttpRequest();
            xhr.onreadystatechange = function () {
                if (xhr.readyState == 4 && xhr.status == 200) {
                    var json = JSON.parse(xhr.response);
                    console.log(json);
                    console.log(xhr.response);
                    if (json.mensaje == "Activo") {
                        console.log("Active");
                    }

                }
                else if (xhr.status == 401) {
                    Swal.fire(
                        'Atención!',
                        'Tu session ha finalizado',
                        'warning'
                    ).then((result) => {

                        if (result.isConfirmed) {
                            location.href = "/";
                        } else if (result.isDenied) {
                            location.href = "/";
                        } else {
                            location.href = "/";
                        }
                    })
                }
            };
            xhr.open('POST', '/Login/BuscarSessionData/');
            xhr.send();

        } setInterval(SessionActiva, 60000);

