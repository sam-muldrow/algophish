    <script>

        document.getElementById("ErroEmail").style.display = "none";
        document.getElementById("ErrorPass").style.display = "none";
        document.getElementById("ErrorLogin").style.display = "none";
        document.getElementById("spinLogin").style.display = "none";
        document.getElementById("spinPass").style.display = "none";

            var InputName = document.getElementById("EmailUser");
            var InputPass = document.getElementById("PassUser");
        let rut = 0;
            const $Email = $("#EmailUser");

            $Email.change(function () {
                var regex = /^(([^<>()[\]\.,;:\s@\"]+(\.[^<>()[\]\.,;:\s@\"]+)*)|(\".+\"))@(([^<>()[\]\.,;:\s@\"]+\.)+[^<>()[\]\.,;:\s@\"]{2,})$/i;
                //contrseñas regex pass
                if (InputName.value != "") {
                    if (regex.test(InputName.value)) {
                        document.getElementById("ErroEmail").style.display = "none";

                    } else {
                        $('#ErroEmail').text("Formato no valido").css("color", "red");
                        document.getElementById("ErroEmail").style.display = "block";

                    }

                } else {
                    $('#ErroEmail').text("Campo requerido").css("color", "red");
                    document.getElementById("ErroEmail").style.display = "block";

                }

            });
            const $Pass = $("#PassUser");
            $Pass.change(function () {

                if (InputPass.value != "") {
                    document.getElementById("ErrorPass").style.display = "none";

                } else {
                    $('#ErrorPass').text("Campo requerido").css("color", "red");
                    document.getElementById("ErrorPass").style.display = "block";

                }

            });

            InputName.addEventListener("keyup", function (event) {
                if (event.keyCode === 13) {
                    event.preventDefault();
                    document.getElementById("buttonLogin").click();

                }
            });
            InputPass.addEventListener("keyup", function (event) {
                if (event.keyCode === 13) {
                    event.preventDefault();
                    document.getElementById("buttonLogin").click();
                }
            });


        function LoginUSerData() {
            document.getElementById("EmailUser").disabled = true;
            document.getElementById("PassUser").disabled = true;
            document.getElementById("buttonLogin").disabled = true;
            document.getElementById("Registro").disabled = true;
            document.getElementById("spinLogin").style.display = "inline-block";

                var InputNameValidate = document.getElementById("EmailUser");
                var InputPassValidate = document.getElementById("PassUser");

            var contador1, contador2 = 0;

            var regex = /^(([^<>()[\]\.,;:\s@\"]+(\.[^<>()[\]\.,;:\s@\"]+)*)|(\".+\"))@(([^<>()[\]\.,;:\s@\"]+\.)+[^<>()[\]\.,;:\s@\"]{2,})$/i;
            if (InputNameValidate.value != "") {
                if (regex.test(InputNameValidate.value)) {
                    document.getElementById("ErroEmail").style.display = "none";
                    contador1 = 1;
                } else {
                    $('#ErroEmail').text("Formato no valido").css("color", "red");
                    document.getElementById("ErroEmail").style.display = "block";
                    contador1 = 0;
                }

            } else {
                $('#ErroEmail').text("Campo requerido").css("color", "red");
                document.getElementById("ErroEmail").style.display = "block";
                contador1 = 0;
            }

            if (InputPassValidate.value == "") {
                $('#ErrorPass').text("Campo requerido").css("color", "red");
                document.getElementById("ErrorPass").style.display = "block";
                contador2 = 0;
            } else {
                document.getElementById("ErrorPass").style.display = "none";
                contador2 = 1;
            }

            if (contador1 == 1 && contador2 == 1) {
                /* var tokenreturn = "";*/
                grecaptcha.ready(function () {
                    window.grecaptcha.execute('6LfkhMUbAAAAAOqDp8BnxVaj80-go98gnOX6_VH5', { action: 'home' }).then(function (token) {

                       /* document.getElementById("token").value = await token;*/
                        /*   token = token;*/
                        ValidaL(token);
                    });
                });

                function ValidaL(datToken) {
                    var formdata = new FormData();
                    formdata.append("email", InputNameValidate.value);
                    formdata.append("pass", InputPassValidate.value);
                    formdata.append("token", datToken);

                    var xhr = new XMLHttpRequest();
                    xhr.onreadystatechange = function () {
                        if (xhr.readyState == 4 && xhr.status == 200) {
                            var json = JSON.parse(xhr.responseText);
                            if (json.ClienteLoginsSuccess == "ok") {

                                document.getElementById("EmailUser").disabled = true;
                                document.getElementById("PassUser").disabled = true;
                                document.getElementById("buttonLogin").disabled = true;
                                document.getElementById("Registro").disabled = true;
                                document.getElementById("ErrorPass").style.display = "none";
                                document.getElementById("ErrorLogin").style.display = "none";
                                document.getElementById("spinLogin").style.display = "none";

                                location.reload();


                            }

                            else if (json.ClienteCaptcha == "error") {

                                document.getElementById("EmailUser").disabled = false;
                                document.getElementById("PassUser").disabled = false;
                                document.getElementById("buttonLogin").disabled = false;
                                document.getElementById("Registro").disabled = false;

                                document.getElementById("ErrorLogin").innerHTML = "Captcha detecto problemas, intentalo nuevamente";
                                document.getElementById("ErrorLogin").style.display = "block";
                                document.getElementById("spinLogin").style.display = "none";


                            } else if (json.ClienteLoginsSuccess == "error1") {
                                document.getElementById("EmailUser").disabled = false;
                                document.getElementById("PassUser").disabled = false;
                                document.getElementById("buttonLogin").disabled = false;
                                document.getElementById("Registro").disabled = false;

                                document.getElementById("ErrorLogin").innerHTML = "Usuario no se encuentra registrado.";
                                document.getElementById("ErrorLogin").style.display = "block";
                                document.getElementById("spinLogin").style.display = "none";


                            } else if (json.ClienteLoginsSuccess == "error2") {
                                document.getElementById("EmailUser").disabled = false;
                                document.getElementById("PassUser").disabled = false;
                                document.getElementById("buttonLogin").disabled = false;
                                document.getElementById("Registro").disabled = false;

                                document.getElementById("ErrorLogin").innerHTML = "Datos de usuario no validos.";
                                document.getElementById("ErrorLogin").style.display = "block";
                                document.getElementById("spinLogin").style.display = "none";


                            }
                        } else if (xhr.readyState == 4 && xhr != 200) {
                            document.getElementById("EmailUser").disabled = false;
                            document.getElementById("PassUser").disabled = false;
                            document.getElementById("buttonLogin").disabled = false;
                            document.getElementById("Registro").disabled = false;

                            document.getElementById("ErrorLogin").style.display = "none";
                            document.getElementById("spinLogin").style.display = "none";

                        }
                    };

                    xhr.open('POST', '/Login/LoginResult/');
                    xhr.send(formdata);
                }
            } else {

                document.getElementById("EmailUser").disabled = false;
                document.getElementById("PassUser").disabled = false;
                document.getElementById("buttonLogin").disabled = false;
                document.getElementById("Registro").disabled = false;

                document.getElementById("ErrorLogin").innerHTML = "Ingresa los datos solicitados";
                document.getElementById("ErrorLogin").style.display = "block";
                document.getElementById("spinLogin").style.display = "none";
            }
        }

        //Registro USer
        $(document).ready(function () {
            document.getElementById('PasswordRegistro')
                .addEventListener('input', function (evt) {
                    const campo = evt.target,
                        valido = document.getElementById('PasswordRegistro'),
                        regex = /^(?=.*\d)(?=.*[a-záéíóúüñ]).*[A-ZÁÉÍÓÚÜÑ]/;
                    var pass = $("#PasswordRegistro").val();
                    var pass2 = $("#PasswordRegistroR").val();
                    //Se muestra un texto válido/inválido a modo de ejemplo
                    if (regex.test(campo.value)) {
                        valido.innerText = "";
                        document.getElementById("ErrorPass1").style.display = "none";
                        if (pass == pass2) {

                            document.getElementById("ErrorPass1").style.display = "none";
                            document.getElementById("ErrorPass2").style.display = "none";

                        } else if (pass != pass2) {

                        }
                    } else if (pass = "") {
                        $('#ErrorPass1').text("Password no son iguales").css("color", "red");

                        document.getElementById("ErrorPass1").style.display = "block";

                    } else {

                        $('#ErrorPass1').text("Password no son iguales").css("color", "red");
                        $('#ErrorPass2').text("Password no son iguales").css("color", "red");
                        document.getElementById("ErrorPass1").style.display = "block";
                        document.getElementById("ErrorPass2").style.display = "block";
                    }
                });

        });
        $(document).ready(function () {
            $('#PasswordRegistroR').keyup(function () {
                var pass1 = $('#PasswordRegistro').val();
                var pass2 = $('#PasswordRegistroR').val();

                if (pass1.length >= 6 && pass2.length >= 6) {

                    if (pass1 == pass2) {
                        document.getElementById("ErrorPass2").style.display = "none";
                        document.getElementById("ErrorPass1").style.display = "none";
                    } else {
                        //document.f1.pass2.style.borderColor = "#FF0000";

                        $('#ErrorPass2').text("Password no son iguales").css("color", "red");

                        document.getElementById("ErrorPass2").style.display = "block";
                    }
                }
                else {

                    $('#ErrorPass2').text("Password no son iguales").css("color", "red");

                    document.getElementById("ErrorPass2").style.display = "block";
                }
                if (pass2 == "") {

                }
            });
        });

        const $Correovali = $("#EmailRegistro");
        $Correovali.change(function () {
            var Correo = document.getElementById("EmailRegistro").value;
            var regex = /^(([^<>()[\]\.,;:\s@\"]+(\.[^<>()[\]\.,;:\s@\"]+)*)|(\".+\"))@(([^<>()[\]\.,;:\s@\"]+\.)+[^<>()[\]\.,;:\s@\"]{2,})$/i;
            //contrseñas regex pass
            if (Correo != "") {
                if (regex.test(Correo)) {
                    document.getElementById("ErrorEmail1").style.display = "none";

                    correctoCorreo1 = 1;
                } else {
                    $('#ErrorEmail1').text("Formato Email no valido").css("color", "red");

                }

            } else {
                $('#ErrorEmail1').text("Campo requerido").css("color", "red");
            }
        });


        $("#Rut")
            .rut({ validateOn: 'keyup change' })
            .on('rutInvalido', function () {
                $(this).parents(".control-group").addClass("errorRut")
                $('#ErrorRut').text("Rut No Valido").css("color", "red");
                rut = 0;
            })
            .on('rutValido', function () {
                $(this).parents(".control-group").removeClass("errorRut")
                $('#ErrorRut').text("").css("color", "green");
                rut = 1;
            });

        const $Term = $("#Terminos");

        $Term.change(function () {
            var Terminos = document.getElementById("Terminos");

            if (Terminos.checked == true) {

                document.getElementById("errorTerminos").style.display = "none";
            } else {
                $('#errorTerminos').text("Debes aceptar los terminos").css("color", "red");
                document.getElementById("errorTerminos").style.display = "block";
            }
        });

