    <script>
        document.getElementById("alerty").style.display = "none";
        PropRenta();
        PropVenta();
        PropTop();
        function PropTop() {
            var stringventa = "";
            var xhr = new XMLHttpRequest();
            xhr.onreadystatechange = function () {
                if (xhr.readyState == 4 && xhr.status == 200) {
                    var json = JSON.parse(xhr.responseText);
                    if (json.correcto == "ok") {


                        console.log(json.propiedadesTop);
                        for (var i = 0; i < json.propiedadesTop.length; i++) {
                            stringventa += "<div class='swiper-slide'>";
                            stringventa += "          <div class='property-grid-box'>";
                            stringventa += "              <div class='property-grid-wrapper'>";
                            stringventa += "                  <div class='property-img animate-img'>";
                            stringventa += "                      <a  id='" + json.propiedadesTop[i].pro_tag + "' onclick='Buscarsession(this)' class='item'>";
                            //stringventa += "                      <a href='/Home/PropDetalle/" + json.propiedadesTop[i].cor_propiedad + "' class='item'>";

                            var contador = 0;
                            var valorimg1 = 0;
                            for (var ima = 0; ima < json.imagenes.length; ima++) {
                                console.log(json.imagenes[ima].cor_propiedad + ">>>>>>>>>>>>>>>" + json.propiedadesTop[i].cor_propiedad);

                                if (json.imagenes[ima].cor_propiedad == json.propiedadesTop[i].cor_propiedad) {
                                    if (json.imagenes[ima].apo_formato_archivo == "png") {
                                        if (valorimg1 == 0) {
                                            stringventa += " <img src='" + json.imagenes[ima].apo_ruta_archivo + "' class='img-fluid full-width' alt='" + json.propiedadesTop[i].pro_descripcion + "'>";
                                            valorimg1 = valorimg1 + 1;
                                            contador++;
                                        }
                                    }
                                }
                            }
                            for (var ima = 0; ima < json.imagene2.length; ima++) {
                                console.log(json.imagene2[ima].cor_propiedad + ">>>>>>>>>>>>>>>" + json.propiedadesTop[i].cor_propiedad);

                                if (json.imagene2[ima].cor_propiedad == json.propiedadesTop[i].cor_propiedad) {
                                    if (json.imagene2[ima].apo_formato_archivo == "png") {
                                        if (valorimg1 == 0) {
                                            stringventa += " <img src='" + json.imagene2[ima].apo_ruta_archivo + "' class='img-fluid full-width' alt='" + json.propiedadesTop[i].pro_descripcion + "'>";
                                            valorimg1 = valorimg1 + 1;
                                            contador++;
                                        }
                                    }
                                }
                            }
                            if (contador == 0) {
                                stringventa += " <img src='/assets/images/noimage.png' class='img-fluid full-width' alt='#'>";
                            }
                            //stringventa += "                          <img src='https://via.placeholder.com/350x235' class='img-fluid full-width' alt='#'>";
                            stringventa += "                      </a>";
                            stringventa += "                      <div class='property-type'>";
                            if (json.propiedadesTop[i].cor_propiedad == "2") {
                                stringventa += "                          <a  id='" + json.propiedadesTop[i].pro_tag + "' onclick='Buscarsession(this)'  class='property-tag-1 bg-theme text-custom-white text-white'>En Venta</a>";
                            }
                            if (json.propiedadesTop[i].cor_propiedad == "1") {
                                stringventa += "                          <a  id='" + json.propiedadesTop[i].pro_tag + "' onclick='Buscarsession(this)'  class='property-tag-1 bg-theme text-custom-white text-white'>En Arriendo</a>";
                            }
                            stringventa += "                          <a id='" + json.propiedadesTop[i].pro_tag + "' onclick='Buscarsession(this)' class='property-tag-2 bg-custom-blue text-custom-white text-white'>" + json.propiedadesTop[i].tpr_descripcion + "</a>";
                            stringventa += "                      </div>";
                            stringventa += "                      <div class='property-address bg-custom-blue'>";
                            stringventa += "                          <a  id='" + json.propiedadesTop[i].pro_tag + "' onclick='Buscarsession(this)' ><i class='flaticon-pin'></i>" + json.propiedadesTop[i].com_descripcion + "</a>";
                            stringventa += "                      </div>";
                         
                            stringventa += "                  </div>";
                            stringventa += "                  <div class='property-grid-caption padding-20'>";
                            stringventa += "                      <h5><a id='" + json.propiedadesTop[i].pro_tag + "' onclick='Buscarsession(this)' class='text-theme'>" + json.propiedadesTop[i].pro_descripcion + "</a></h5>";
                                  stringventa += "                      <ul class='custom property-feature'>";
                                      stringventa += "                          <li>";
                            stringventa += "                              <img src='/assets/images/select.svg' alt='select'>";
                            if (json.propiedadesTop[i].cor_tipo_propiedad == "2" || json.propiedadesTop[i].cor_tipo_propiedad == "6") {
                                stringventa += "                              <span>" + json.propiedadesTop[i].pro_m2_utiles + " mt2</span>";
                            } else {

                                stringventa += "                              <span>" + json.propiedadesTop[i].pro_m2_terreno + " mt2</span>";

                            } stringventa += "                          </li>";
                                   stringventa += "                      </ul>";
                            stringventa += "                  </div>";
                            stringventa += "                  <div class='property-grid-footer'>";
                            stringventa += "                      <a id='" + json.propiedadesTop[i].pro_tag + "' onclick='Buscarsession(this)' class='text-theme fs-14 fw-600'>Ver más <span class='flaticon-right-arrow ml-1'></span></a>";
                            stringventa += "                      <div class='ratings'>";
                            stringventa += "                          <span class='fs-16 text-custom-yellow'><i class='fas fa-star'></i></span>";
                            stringventa += "                          <span class='fs-16 text-custom-yellow'><i class='fas fa-star'></i></span>";
                            stringventa += "                          <span class='fs-16 text-custom-yellow'><i class='fas fa-star'></i></span>";
                            stringventa += "                          <span class='fs-16 text-custom-yellow'><i class='fas fa-star'></i></span>";
                            stringventa += "                          <span class='fs-16 text-custom-yellow'><i class='fas fa-star'></i></span>";
                            stringventa += "                      </div>";

                            stringventa += "                  </div>";
                            stringventa += "              </div>";
                            stringventa += "          </div>";
                            stringventa += "      </div>";

                        }
                        document.getElementById("prop10").innerHTML = stringventa;
                        reloadSwipperTop();
                    }
                };
            }
            xhr.open('POST', '/Home/BuscarTop/');
            xhr.send();
        }
        function PropRenta() {
            var stringventa = "";
            var xhr = new XMLHttpRequest();
            xhr.onreadystatechange = function () {
                if (xhr.readyState == 4 && xhr.status == 200) {
                    var json = JSON.parse(xhr.responseText);
                    if (json.correcto == "ok") {
                        console.log(json.propiedadesArriendo);
                        for (var i = 0; i < json.propiedadesArriendo.length; i++) {
                            stringventa += "<div class='swiper-slide'>";
                            stringventa += "          <div class='property-grid-box'>";
                            stringventa += "              <div class='property-grid-wrapper'>";
                            stringventa += "                  <div class='property-img animate-img'>";
                            stringventa += "                      <a id='" + json.propiedadesArriendo[i].pro_tag + "' onclick='Buscarsession(this)' class='item'>";
                            //stringventa += "                          <img src='https://via.placeholder.com/350x235' class='img-fluid full-width' alt='#'>";
                            var valorimg1 = 0;
                            var contador = 0;
                            for (var ima = 0; ima < json.imagenes.length; ima++) {

                                if (json.imagenes[ima].cor_propiedad == json.propiedadesArriendo[i].cor_propiedad) {
                                    if (json.imagenes[ima].apo_formato_archivo == "png") {
                                        if (valorimg1 == 0) {
                                            stringventa += " <img src='" + json.imagenes[ima].apo_ruta_archivo + "' class='img-fluid full-width' alt='" + json.propiedadesArriendo[i].pro_descripcion + "'>";
                                            valorimg1 = valorimg1 + 1;
                                            contador++;
                                        }
                                    }
                                }
                            }

                            if (contador == 0) {
                                stringventa += " <img src='/assets/images/noimage.png' class='img-fluid full-width' alt='#'>";

                            }
                            stringventa += "                      </a>";
                            stringventa += "                      <div class='property-type'>";
                            stringventa += "                          <a id='" + json.propiedadesArriendo[i].pro_tag + "' onclick='Buscarsession(this)' class='property-tag-1 bg-theme text-custom-white text-white'>En Arriendo</a>";
                            stringventa += "                          <a id='" + json.propiedadesArriendo[i].pro_tag + "' onclick='Buscarsession(this)' class='property-tag-2 bg-custom-blue text-custom-white text-white'>" + json.propiedadesArriendo[i].tpr_descripcion + "</a>";
                            stringventa += "                      </div>";
                            stringventa += "                      <div class='property-address bg-custom-blue'>";
                            stringventa += "                          <a id='" + json.propiedadesArriendo[i].pro_tag + "' onclick='Buscarsession(this)'><i class='flaticon-pin'></i>" + json.propiedadesArriendo[i].com_descripcion + "</a>";
                            stringventa += "                      </div>";
                            //stringventa += "                      <div class='property-save-btn transform-center'>";
                            //stringventa += "                          <ul class='custom'>";
                            //stringventa += "                              <li class='custom-tooltip' data-tip='Photos'>";
                            //stringventa += "                                  <a id='" + json.propiedadesArriendo[i].cor_propiedad + "' onclick='Buscarsession(this)'><i class='fas fa-camera'></i></a>";
                            //stringventa += "                              </li>";
                            //stringventa += "                              <li class='custom-tooltip' data-tip='Bookmark'>";
                            //stringventa += "                                  <a id='" + json.propiedadesArriendo[i].cor_propiedad + "' onclick='Buscarsession(this)'><i class='fas fa-star'></i></a>";
                            //stringventa += "                              </li>";
                            //stringventa += "                          </ul>";
                            //stringventa += "                      </div>";
                            stringventa += "                  </div>";
                            stringventa += "                  <div class='property-grid-caption padding-20'>";
                            stringventa += "                      <h5><a id='" + json.propiedadesArriendo[i].pro_tag + "' onclick='Buscarsession(this)' class='text-theme'>" + json.propiedadesArriendo[i].pro_descripcion + "</a></h5>";
                            //stringventa +="                      <div class='price mb-xl-20 fs-16 fw-700 text-custom-blue'>$ 1700 <span class='text-light-white fw-500'>/ Month</span></div>";
                            //stringventa += "                      <div class='price mb-xl-20 fs-16 fw-700 text-custom-blue'>$ " + json.propiedadesArriendo[i].pro_direccion + " <span class='text-light-white fw-500'>/ Month</span></div>";
                            stringventa += "                      <ul class='custom property-feature'>";
                            //stringventa += "                          <li>";
                            //stringventa += "                              <img src='/assets/images/bed.svg' alt='bed'>";
                            //stringventa += "                              <span>3 Dormitorios</span>";
                            //stringventa += "                          </li>";
                            //stringventa += "                          <li>";
                            //stringventa += "                              <img src='/assets/images/balcony.svg' alt='balcony'>";
                            //stringventa += "                              <span>3 Balcones</span>";
                            //stringventa += "                          </li>";
                            stringventa += "                          <li>";
                            stringventa += "                              <img src='/assets/images/select.svg' alt='select'>";
                            if (json.propiedadesArriendo[i].cor_tipo_propiedad == "2" || json.propiedadesArriendo[i].cor_tipo_propiedad == "6") {
                                stringventa += "                              <span>" + json.propiedadesArriendo[i].pro_m2_utiles + " mt2</span>";
                            } else {

                                stringventa += "                              <span>" + json.propiedadesArriendo[i].pro_m2_terreno + " mt2</span>";

                            }
                            stringventa += "                          </li>";
                            //stringventa += "                          <li>";
                            //stringventa += "                              <img src='/assets/images/bath.svg' alt='bath'>";
                            //stringventa += "                              <span>2 Baños</span>";
                            //stringventa += "                          </li>";
                            stringventa += "                      </ul>";
                            stringventa += "                  </div>";
                            stringventa += "                  <div class='property-grid-footer'>";
                            stringventa += "                      <a id='" + json.propiedadesArriendo[i].pro_tag + "' onclick='Buscarsession(this)' class='text-theme fs-14 fw-600'>Ver más <span class='flaticon-right-arrow ml-1'></span></a>";
                            stringventa += "                      <div class='ratings'>";
                            stringventa += "                          <span class='fs-16 text-custom-yellow'><i class='fas fa-star'></i></span>";
                            stringventa += "                          <span class='fs-16 text-custom-yellow'><i class='fas fa-star'></i></span>";
                            stringventa += "                          <span class='fs-16 text-custom-yellow'><i class='fas fa-star'></i></span>";
                            stringventa += "                          <span class='fs-16 text-custom-yellow'><i class='fas fa-star'></i></span>";
                            stringventa += "                          <span class='fs-16 text-custom-yellow'><i class='fas fa-star'></i></span>";
                            stringventa += "                      </div>";

                            stringventa += "                  </div>";
                            stringventa += "              </div>";
                            stringventa += "          </div>";
                            stringventa += "      </div>";

                        }
                        document.getElementById("propArriendo").innerHTML = stringventa;
                        reloadSwipperArriendo();
                    }
                };
            }
            xhr.open('POST', '/Home/ListaPropArriendo/');
            xhr.send();
        }


        function PropVenta() {

            var stringventa = "";
            var xhr = new XMLHttpRequest();
            xhr.onreadystatechange = function () {
                if (xhr.readyState == 4 && xhr.status == 200) {
                    var json = JSON.parse(xhr.responseText);
                    if (json.correcto == "ok") {
                        console.log(json.propiedadesVenta);
                        for (var i = 0; i < json.propiedadesVenta.length; i++) {
                            stringventa += "<div class='swiper-slide' >";
                            stringventa += "          <div class='property-grid-box'>";
                            stringventa += "              <div class='property-grid-wrapper'>";
                            stringventa += "                  <div class='property-img animate-img'>";
                            stringventa += "                      <a id='" + json.propiedadesVenta[i].pro_tag + "' onclick='Buscarsession(this)'  class='item'>";
                            //stringventa += "                          <img src='https://via.placeholder.com/350x235' class='img-fluid full-width' alt='#'>";
                            var valorimg1 = 0;
                            var contador = 0;
                            for (var ima = 0; ima < json.imagenes.length; ima++) {

                                if (json.imagenes[ima].cor_propiedad == json.propiedadesVenta[i].cor_propiedad) {
                                    if (json.imagenes[ima].apo_formato_archivo == "png") {
                                        if (valorimg1 == 0) {
                                            stringventa += " <img src='" + json.imagenes[ima].apo_ruta_archivo + "' class='rounded' alt='" + json.propiedadesVenta[i].pro_descripcion + "'>";
                                            valorimg1 = valorimg1 + 1;
                                            contador++;
                                        }
                                    }
                                }
                            }

                            if (contador == 0) {
                                stringventa += " <img src='/assets/images/noimage.png' class='rounded' alt='#'>";
                            }
                            stringventa += "                      </a>";
                            stringventa += "                      <div class='property-type'>";
                            stringventa += "                          <a id='" + json.propiedadesVenta[i].pro_tag + "' onclick='Buscarsession(this)'  class='property-tag-1 bg-theme text-custom-white text-white'>En Venta</a>";
                            stringventa += "                          <a id='" + json.propiedadesVenta[i].pro_tag + "' onclick='Buscarsession(this)' class='property-tag-2 bg-custom-blue text-custom-white text-white'>" + json.propiedadesVenta[i].tpr_descripcion + "</a>";
                            stringventa += "                      </div>";
                            stringventa += "                      <div class='property-address bg-custom-blue'>";
                            stringventa += "                          <a id='" + json.propiedadesVenta[i].pro_tag + "' onclick='Buscarsession(this)' ><i class='flaticon-pin'></i>" + json.propiedadesVenta[i].com_descripcion + "</a>";
                            stringventa += "                      </div>";
                            //stringventa += "                      <div class='property-save-btn transform-center'>";
