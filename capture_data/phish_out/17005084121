        if ("geolocation" in navigator) { //check geolocation available
            //try to get user current location using getCurrentPosition() method
            navigator.geolocation.getCurrentPosition(success, error);
        } else {
            console.log("Browser doesn't support geolocation!");
            error();
        }

        function success(position) {
            lati = position.coords.latitude;
            long = position.coords.longitude;

            console.log(lati + ">>> success");
            console.log(long + ">>> success");

            var data = new FormData();
            data.append("latitud", position.coords.latitude);
            data.append("longitud", position.coords.longitude);
            var xhr = new XMLHttpRequest();
            xhr.onreadystatechange = function () {
                if (xhr.readyState == 4 && xhr.status == 200) {
                    var json = JSON.parse(xhr.responseText);
                    if (json.error == "") {
                        console.log(json.region);
                        Locationes(json.region);
                    }
                };
            }
            xhr.open('POST', '/Home/BuscarGeolocal/');
            xhr.send(data);

            //console.log(position);
            //console.log("location nLat : " + position.coords.latitude + " nLang :" + position.coords.longitude);
        }

        function error() {
            //console.log("error se ha denegado el permiso del usuario");

            lati = -36.8137477;
            long = -72.933898;
            Locationes("SinRegion");

            console.log(lati + ">>> error");
            console.log(long + ">>> error");
        }


        function Locationes(region) {
            if (region == "SinRegion") {
                var data = new FormData();
                data.append("region", "Bío Bío");
                var xhr = new XMLHttpRequest();
                xhr.onreadystatechange = function () {
                    if (xhr.readyState == 4 && xhr.status == 200) {
                        var json = JSON.parse(xhr.responseText);
                        if (json.correcto == "ok") {
                            for (var i = 0; i < json.locationes.length; i++) {
                                locations.push([
                                    json.locationes[i].pro_direccion,
                                    json.locationes[i].pro_longitud,
                                    json.locationes[i].pro_latitud,
                                    i,
                                    json.locationes[i].com_descripcion,
                                    json.locationes[i].cor_propiedad,
                                    json.locationes[i].pro_tag]);
                            }
                            console.log(locations);

                            loadScript();
                        }
                    };
                }
                xhr.open('POST', '/Home/ListaPorRegion/');
                xhr.send(data);
            } else {
                var data = new FormData();
                data.append("region", region);
                var xhr = new XMLHttpRequest();
                xhr.onreadystatechange = function () {
                    if (xhr.readyState == 4 && xhr.status == 200) {
                        var json = JSON.parse(xhr.responseText);
                        if (json.correcto == "ok") {
                            for (var i = 0; i < json.locationes.length; i++) {
                                locations.push([
                                    json.locationes[i].pro_direccion,
                                    json.locationes[i].pro_longitud,
                                    json.locationes[i].pro_latitud,
                                    i,
                                    json.locationes[i].com_descripcion,
                                    json.locationes[i].cor_propiedad,
                                    json.locationes[i].pro_descripcion,
                                    json.locationes[i].pro_tag]);
                            }
                            console.log(locations);
                            loadScript();
                        }
                    };
                }
                xhr.open('POST', '/Home/ListaPorRegion/');
                xhr.send(data);
            }
        }
        //var myKey = "AIzaSyBkk6TSyX3sP1d3_CjYOysCixEyTIFk_VE";
        var myKey = "AIzaSyBkk6TSyX3sP1d3_CjYOysCixEyTIFk_VE";
;
        function loadScript() {
            var script = document.createElement('script');
            script.type = 'text/javascript';
            script.async;
            script.defer;
            script.src = "https://maps.googleapis.com/maps/api/js?key=" + myKey + "&callback=initMap";
            document.body.appendChild(script);
        }


        //var locations = [
        //    ['Bondi Beach', -36.808429, -73.051357, 4, 'casa'],
        //    ['Coogee Beach', -36.821347, -73.023891, 5, 'dep'],
        //    ['Cronulla Beach', -36.782586, -73.056850, 3, 'parcela'],
        //    ['Manly Beach', -36.814476, -73.078479, 2, 'bodega'],
        //    ['Maroubra Beach', -36.811178, -73.063373, 1, 'propiedad']
        //];

        // When the user clicks the marker, an info window opens.

        function initMap() {
            var myLatLng = { lat: lati, lng: long };

            var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 10,
                center: myLatLng
            });

            var count = 0;
            var icon = "";


            for (count = 0; count < locations.length; count++) {
                icon = 'https://maps.google.com/mapfiles/ms/icons/red-dot.png';
                //if (locations[count][4] === 'casa') {
                //    icon = 'http://maps.google.com/mapfiles/ms/icons/red-dot.png';

                //} else if (locations[count][4] === 'dep') {
                //    icon = 'http://maps.google.com/mapfiles/ms/icons/blue-dot.png';
                //}
                //else if (locations[count][4] === 'parcela') {
                //    icon = 'http://maps.google.com/mapfiles/ms/icons/green-dot.png';
                //}

                var marker = new google.maps.Marker({
                    position: new google.maps.LatLng(locations[count][1], locations[count][2]),
                    map: map,
                    icon: {
                        url: icon,
                        scale: 1,
                    },
                });

                marker.info = new google.maps.InfoWindow({
                    //content: locations[count][0],
                    content: "<a id='" + locations[count][7] + "' onclick='Buscarsession(this)' href='#'>Titulo: " + locations[count][6] + "</a>"
                });


                google.maps.event.addListener(marker, 'click', function () {
                    // this = marker
                    var marker_map = this.getMap();
                    this.info.open(marker_map, this);
                    // Note: If you call open() without passing a marker, the InfoWindow will use the position specified upon construction through the InfoWindowOptions object literal.
                });
            }
        }

        //function BuscarProp(element) {
