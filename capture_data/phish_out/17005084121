                            stringventa += "          </div>";
                            stringventa += "      </div>";

                        }

                        document.getElementById("propVentas").innerHTML = stringventa;
                        reloadSwipperVentas(); reloadSwipperTop();

                    }
                };
            }

            xhr.open('POST', '/Home/ListaPropVenta/');
