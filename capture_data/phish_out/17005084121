                            //stringventa += "                              <li class='custom-tooltip' data-tip='Photos'>";
                            //stringventa += "                                  <a id='" + json.propiedadesVenta[i].cor_propiedad + "' onclick='Buscarsession(this)' ><i class='fas fa-camera'></i></a>";
                            //stringventa += "                              </li>";
                            //stringventa += "                              <li class='custom-tooltip' data-tip='Bookmark'>";
                            //stringventa += "                                  <a id='" + json.propiedadesVenta[i].cor_propiedad + "' onclick='Buscarsession(this)' ><i class='fas fa-star'></i></a>";
                            //stringventa += "                              </li>";
                            //stringventa += "                          </ul>";
                            //stringventa += "                      </div>";
                            stringventa += "                  </div>";
                            stringventa += "                  <div class='property-grid-caption padding-20'>";
                            stringventa += "                      <h5><a  id='" + json.propiedadesVenta[i].pro_tag + "' onclick='Buscarsession(this)'  class='text-theme'>" + json.propiedadesVenta[i].pro_descripcion + "</a></h5>";
                            //stringventa +="                      <div class='price mb-xl-20 fs-16 fw-700 text-custom-blue'>$ 1700 <span class='text-light-white fw-500'>/ Month</span></div>";
                            //stringventa += "                      <div class='price mb-xl-20 fs-16 fw-700 text-custom-blue'>$ " + json.propiedadesVenta[i].pro_direccion + " <span class='text-light-white fw-500'>/ Month</span></div>";
                            stringventa += "                      <ul class='custom property-feature'>";
                            //stringventa += "                          <li>";
                            //stringventa += "                              <img src='/assets/images/bed.svg' alt='bed'>";
