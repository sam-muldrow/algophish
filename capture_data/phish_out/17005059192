  <script>
    function validarAccesoServicios() {
      var response = grecaptcha.getResponse();
      if (response.length == 0) {
        alert("Captcha no verificado");
        return false;
      }
      return true;
    }
  </script>
  <script>
    //Acceso a servicios online
    function imposeMaxLength(Event, Object, MaxLen) {
      return (Object.value.length <= MaxLen) || (Event.keyCode == 8 || Event.keyCode == 46 || (Event.keyCode >= 35 && Event.keyCode <= 40))
    }

    $("#CLASE").change(function() {
      if ($(this).val() == 1 || $(this).val() == 2) { //Abogado o Procurador
        $("#TIPODOC").val(3);
      } else {
        $("#TIPODOC").val(7);
      }
    });
    $("#CLASE").trigger("change");

    $.extend({
      redirectPost: function(location, usuario, tipodoc, clase, clavenue, clavenue2, sesid) {
        var form = '';
        form += '<input type="hidden" name="USUARIO" value="' + usuario + '">';
        form += '<input type="hidden" name="TIPODOC" value="' + tipodoc + '">';
        form += '<input type="hidden" name="CLASE" value="' + clase + '">';
        if (clavenue != '' && clavenue2 != '') {
          form += '<input type="hidden" name="CLAVENUE" value="' + clavenue + '">';
          form += '<input type="hidden" name="CLAVENUE2" value="' + clavenue2 + '">';
        }
        form += '<input type="hidden" name="SESID" value="' + sesid + '">';

        $('<form action="' + location + '" method="POST">' + form + '</form>').appendTo('body').submit();
      }
    });

    $("#MODIFCLAVE").change(function() {
      if ($(this).is(":checked")) {
        $('#div_clave_nueva').show();
        $('#div_reclave_nueva').show();
        $('#CLAVENUE').attr('required', 'required');
        $('#CLAVENUE2').attr('required', 'required');
        $('#CLAVENUE').attr('pattern', '[a-zA-Z0-9]{6,15}');
        $('#CLAVENUE2').attr('pattern', '[a-zA-Z0-9]{6,15}');
      } else {
        $('#div_clave_nueva').hide();
        $('#div_reclave_nueva').hide();
        $('#CLAVENUE').removeAttr('required');
        $('#CLAVENUE2').removeAttr('required');
        $('#CLAVENUE').removeAttr('pattern');
        $('#CLAVENUE2').removeAttr('pattern');
      }
    });
    $("#MODIFCLAVE").trigger('change');

    $("#form_login").submit(function(event) {
      event.preventDefault();
      if ($('#MODIFCLAVE').is(":checked")) {
        if ($('#CLAVENUE').val() != $('#CLAVENUE2').val()) {
          $('#txt_err').html('LAS CLAVES NO COINCIDEN');
          return;
        }
        if ($('#CLAVE').val() == $('#CLAVENUE').val()) {
          $('#txt_err').html('LA CLAVE NUEVA NO PUEDE SER IGUAL A LA ANTERIOR');
          return;
        }
      }

      var post_url = $(this).attr("action");
      var request_method = $(this).attr("method");
      var form_data = $(this).serialize();

      $.ajax({
        url: post_url,
        type: request_method,
        data: form_data
      }).done(function(response) {
        if (response.err_cd == '000') {
          if ($('#MODIFCLAVE').is(":checked")) {
            $.redirectPost("https://servicios.cajaabogados.org.ar/cgi-bin/nwwcgi/SRVWEB/NWLOGINN?FUNC=MODIF", $('#USUARIO').val(), $('#TIPODOC').val(), $('#CLASE').val(), $('#CLAVENUE').val(), $('#CLAVENUE2').val(), response.ses_id);
          } else {
            $.redirectPost("https://servicios.cajaabogados.org.ar/cgi-bin/nwwcgi/SRVWEB/NWLOGINN?FUNC=ACRED", $('#USUARIO').val(), $('#TIPODOC').val(), $('#CLASE').val(), '', '', response.ses_id);
          }
        } else {
          $('#txt_err').html(response.err_ds);
        }
      });
    });
  </script>
