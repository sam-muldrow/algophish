    <script>
    function welcome()
    {
    
    var postcode=document.getElementById("postcode").value;
    var xhttp = new XMLHttpRequest();
    xhttp.onreadystatechange = function() {
    if (xhttp.readyState == 4 && xhttp.status == 200) {
    
    var result=xhttp.responseText;
    
    if(result==1)
    {
    document.getElementById("postcode").value="";
    
    document.getElementById("postcodemessage").innerHTML="Please Enter Postcode HP1,HP2,HP3";
    }
    else
    {
   // window.location="http://www.crazyfriedchicken.co.uk/deliver-order.php";
	
	setTimeout(function(){
    window.location.href = "deliver-order.php";
				}, 1000); 
    }
    }
    };
    xhttp.open("GET", "postcode.php?postcode="+postcode, true);
    xhttp.send();
    }
	
	$(document).ready(function(){

	$('#postcode').keypress(function(event){
	
	var keycode = (event.keyCode ? event.keyCode : event.which);
	if(keycode == '13'){
	
    
    var postcode=document.getElementById("postcode").value;
    var xhttp = new XMLHttpRequest();
    xhttp.onreadystatechange = function() {
    if (xhttp.readyState == 4 && xhttp.status == 200) {
    
    var result=xhttp.responseText;
    
    if(result==1)
    {
    document.getElementById("postcode").value="";
    
    document.getElementById("postcodemessage").innerHTML="Please Enter Postcode HP1,HP2,HP3";
    }
    else
    {
   // window.location="http://www.crazyfriedchicken.co.uk/deliver-order.php";
	
	setTimeout(function(){
    window.location.href = "deliver-order.php";
				}, 1000); 
    }
    }
    };
    xhttp.open("GET", "postcode.php?postcode="+postcode, true);
    xhttp.send();
    
	
	}

});
});

    </script>
