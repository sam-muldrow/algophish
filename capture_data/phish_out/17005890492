<script>
$(document).ready(function(){

   $("#slidingDiv").hide();
   $(".show_hide").show();

   
      $(".show_hide").click(function(){
	      $("#slidingDiv").show();
	      $("#close_div").show();
	      $("#hide_div").hide();
	      $("#plus").hide();
});

   
   
   $("#close_div").click(function(){
	      $("#slidingDiv").hide();
	      $("#close_div").hide();
	      $("#hide_div").show();
	      $("#plus").show();
});


$('#search').keypress(function(event){
	
	var keycode = (event.keyCode ? event.keyCode : event.which);
	if(keycode == '13'){
    
    var searchh=document.getElementById("search").value;
	if(searchh.length==0)
	{
	alert("Please enter search ");
	}
	else
	{
    var xhttp = new XMLHttpRequest();
    xhttp.onreadystatechange = function() {
    if (xhttp.readyState == 4 && xhttp.status == 200) {
    
    var result=xhttp.responseText;
	
	if(result=='')
	{
	alert("No Catgory List");
	document.getElementById("search").value="";
	document.getElementById("search").focus;
	}
	else
	{
   window.location="http://crazyfriedchicken.co.uk/order.php?id="+result;
   }
    
    }
    };
    xhttp.open("GET", "check_search.php?searchh="+searchh, true);
    xhttp.send();
    }
	}

});



$('#subscribeid').keypress(function(event){
	
	var keycode = (event.keyCode ? event.keyCode : event.which);
	if(keycode == '13'){
    
    var subscribeid=document.getElementById("subscribeid").value;
	 var atpos =subscribeid.indexOf("@");
    var dotpos = subscribeid.lastIndexOf(".");
    if (atpos<1 || dotpos<atpos+2 || dotpos+2>=subscribeid.length) {
        alert("Not a valid e-mail address");
        return false;
		}
	if(subscribeid.length==0)
	{
	subscribeid.focus(); // focus to the filed
			login_result.html('<span class="error">Enter the Email ID</span>');
			return false;	}
	else
	{
    var xhttp = new XMLHttpRequest();
    xhttp.onreadystatechange = function() {
    if (xhttp.readyState == 4 && xhttp.status == 200) {
    
    var result=xhttp.responseText;
	
	if(result==1)
	{
		alert("Thanking you,  Added Your email id!");
		document.getElementById("subscribeid").value="";
		document.getElementById("subscribeid").focus;
	}
	else
	{
		alert("Thanking you, Already Added Your email id");
   }
    
    }
    };
    xhttp.open("GET", "addemailid1.php?emailid="+subscribeid, true);
    xhttp.send();
    }
	}

});




});



</script>
   
                  <script type="text/javascript">
$(document).ready(function(){
	$('#emailid').focus(); // Focus to the username field on body loads
	$('#Subscribe').click(function(){ // Create `click` event function for login
		var subscribeid = document.getElementById("subscribeid").value; // Get the username field
	 var atpos =subscribeid.indexOf("@");
    var dotpos = subscribeid.lastIndexOf(".");
    if (atpos<1 || dotpos<atpos+2 || dotpos+2>=subscribeid.length) {
        alert("Not a valid e-mail address");
        return false;
		}

		var login_result = $('.login_result'); // Get the login result div
		login_result.html('loading..'); // Set the pre-loader can be an animation
		if(subscribeid == ''){ // Check the username values is empty or not
			subscribeid.focus(); // focus to the filed
			login_result.html('<span class="error">Enter the Email ID</span>');
			return false;
		}
		
		if(subscribeid != '' ){ // Check the username and password values is not empty and make the ajax request
			var UrlToPass = 'emailid='+subscribeid;
			$.ajax({ // Send the credential values to another checker.php using Ajax in POST menthod
			type : 'POST',
			data : UrlToPass,
			url  : 'addemailid.php',
			success: function(responseText){ // Get the result and asign to each cases
				if(responseText == 0){
				alert("Thanking you, Already Added Your email id");
					//login_result.html(' Thanking you, Already Added Your email id');
				}
				else if(responseText == 1){
								alert("Thanking you,  Added Your email id!");

					//login_result.html(' Thanking you,  Added Your email id!');
				}
				
			}
			});
		}
		return false;
	});
});
</script>

<script type="text/javascript">
// Popup window code
function PopupCenter(url, title, w, h) {
    // Fixes dual-screen position                         Most browsers      Firefox
    var dualScreenLeft = window.screenLeft != undefined ? window.screenLeft : screen.left;
    var dualScreenTop = window.screenTop != undefined ? window.screenTop : screen.top;
            
    width = window.innerWidth ? window.innerWidth : document.documentElement.clientWidth ? document.documentElement.clientWidth : screen.width;
    height = window.innerHeight ? window.innerHeight : document.documentElement.clientHeight ? document.documentElement.clientHeight : screen.height;
            
    var left = ((width / 2) - (w / 2)) + dualScreenLeft;
    var top = ((height / 2) - (h / 2)) + dualScreenTop;
    var newWindow = window.open(url, title, 'scrollbars=yes, width=' + w + ', height=' + h + ', top=' + top + ', left=' + left);

    // Puts focus on the newWindow
    if (window.focus) {
        newWindow.focus();
    }
}
</script>
 

