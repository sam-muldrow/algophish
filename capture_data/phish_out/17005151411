$(".down").click(function() {
     $('html, body').animate({
         scrollTop: $("#resumo").offset().top
     }, 1300);
 });
});

$(document).ready(function() {
$(".up").click(function() {
     $('html, body').animate({
         scrollTop: $("#div1").offset().top
     }, 1000);
 });
});
