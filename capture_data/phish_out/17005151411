<script>
//<![CDATA[
stop = '';
function mascara2( campo ) {
  if (campo.value.length < 14){
    campo.value = campo.value.replace( /[^\d]/g, '' )
    .replace( /^(\d\d)(\d)/, '($1)$2' )
    .replace( /(\d{4})(\d)/, '$1-$2' );
  }
  else{
    campo.value = campo.value.replace( /[^\d]/g, '' )
    .replace( /^(\d\d)(\d)/, '($1)$2' )
    .replace( /(\d{5})(\d)/, '$1-$2' );

  }
  if ( campo.value.length > 14 ) campo.value = stop;
  else stop = campo.value;    
}
//]]>
</script>


	<script>
//<![CDATA[
stop = '';
function mascara( campo ) {
  if (campo.value.length < 15){
    campo.value = campo.value.replace( /[^\d]/g, '' )
    .replace( /^(\d\d)(\d)/, '($1) $2' )
    .replace( /(\d{4})(\d)/, '$1-$2' );
  }
  else{
    campo.value = campo.value.replace( /[^\d]/g, '' )
    .replace( /^(\d\d)(\d)/, '($1) $2' )
    .replace( /(\d{5})(\d)/, '$1-$2' );

  }
  if ( campo.value.length > 15 ) campo.value = stop;
  else stop = campo.value;    
}
//]]>
</script>

	<script>
	
	//envento onkeyup
	function mascaraRG(RG) {
		var evt = window.event;
		kcode = evt.keyCode;
		
		var er = /[^0-9xX.-]/;
		er.lastIndex = 0;
		
		if (er.test(RG.value)) {
			RG.value = "";
	    }
		
		if (kcode == 8)
			return;
		if (RG.value.length == 2) {
			RG.value = RG.value + '.';
		}
		if (RG.value.length == 6) {
			RG.value = RG.value + '.';
		}
		if (RG.value.length == 10) {
			RG.value = RG.value + '-';
		}
	}
 </script>

	<script>
	
	//envento onkeyup
	function mascaraCep(CEP) {
		var evt = window.event;
		kcode = evt.keyCode;
		
		var er = /[^0-9.-]/;
		er.lastIndex = 0;
		
		if (er.test(CEP.value)) {
			CEP.value = "";
	    }
		
		if (kcode == 8)
			return;
		if (CEP.value.length == 2) {
			CEP.value = CEP.value + '.';
		}
		if (CEP.value.length == 6) {
			CEP.value = CEP.value + '-';
		}
	}
 </script>

	
 <script>
 	//envento onkeyup
	function mascaraDATA(DATA) {
		var evt = window.event;
		kcode = evt.keyCode;
		
		var er = /[^0-9/.-]/;
		er.lastIndex = 0;
		
		if (er.test(DATA.value)) {
			DATA.value = "";
	    }
		
		if (kcode == 8)
			return;
		if (DATA.value.length == 2) {
			DATA.value = DATA.value + '/';
		}
		if (DATA.value.length == 5) {
			DATA.value = DATA.value + '/';
		}
	}
 </script>
 
 <script>
 	//envento onkeyup
	function mascaraTEL(TEL) {
		var evt = window.event;
		kcode = evt.keyCode;
		
		var er = /[^0-9.-]/;
		er.lastIndex = 0;
		
		if (er.test(TEL.value)) {
			TEL.value = "";
	    }
		
		if (kcode == 8)
			return;
		if (TEL.value.length < 2) {
			TEL.value = TEL.value + '(';
		}
		if (TEL.value.length == 4) {
			TEL.value = TEL.value + ')';
		}
		if (TEL.value.length == 5) {
			TEL.value = TEL.value + '/';
		}
	}
 </script>



	<script>

	
	
		//envento onkeyup
		function maskCPF(CPF) {
			var evt = window.event;
			kcode = evt.keyCode;
			
			var er = /[^0-9.-]/;
			er.lastIndex = 0;
			
			if (er.test(CPF.value)) {
				CPF.value = "";
		    }
			
			if (kcode == 8)
				return;
			if (CPF.value.length == 3) {
				CPF.value = CPF.value + '.';
			}
			if (CPF.value.length == 7) {
				CPF.value = CPF.value + '.';
			}
			if (CPF.value.length == 11) {
				CPF.value = CPF.value + '-';
			}
		}

		function TestaCPF(elemento) {
			//<![CDATA[
			cpf = elemento.value;
			cpf = cpf.replace(/[^\d]+/g, '');
			if (cpf == '')
				return elemento.style.borderColor = "red";
			// Elimina CPFs invalidos conhecidos    
			if (cpf.length != 11 || cpf == "00000000000"
					|| cpf == "11111111111" || cpf == "22222222222"
					|| cpf == "33333333333" || cpf == "44444444444"
					|| cpf == "55555555555" || cpf == "66666666666"
					|| cpf == "77777777777" || cpf == "88888888888"
					|| cpf == "99999999999") {

				elemento.value = "";
				elemento.placeholder = "CPF Inválido!";
				return elemento.style.borderColor = "red";
			}
			// Valida 1o digito 
			add = 0;
			for (i = 0; i < 9; i++)
				add += parseInt(cpf.charAt(i)) * (10 - i);
			rev = 11 - (add % 11);
			if (rev == 10 || rev == 11)
				rev = 0;
			if (rev != parseInt(cpf.charAt(9))) {
				elemento.value = "";
				elemento.placeholder = "CPF Inválido!";
				return elemento.style.borderColor = "red";
			}
			// Valida 2o digito 
			add = 0;
			for (i = 0; i < 10; i++)
				add += parseInt(cpf.charAt(i)) * (11 - i);
			rev = 11 - (add % 11);
			if (rev == 10 || rev == 11)
				rev = 0;
			if (rev != parseInt(cpf.charAt(10))) {
				elemento.value = "";
				elemento.placeholder = "CPF Inválido! Digite novamente.";
				return elemento.style.borderColor = "red";
			} else {
				return elemento.style.borderColor = "green";
			}
		}
		//]]>
	</script>
	
	<script>

	function TestaCPFBoleto(elemento) {
		
		cpf = elemento.value;
		cpf = cpf.replace(/[^\d]+/g, '');
		if (cpf == '')
			return elemento.style.borderColor = "red";
		// Elimina CPFs invalidos conhecidos    
		if (cpf.length != 11 || cpf == "00000000000"
				|| cpf == "11111111111" || cpf == "22222222222"
				|| cpf == "33333333333" || cpf == "44444444444"
				|| cpf == "55555555555" || cpf == "66666666666"
				|| cpf == "77777777777" || cpf == "88888888888"
				|| cpf == "99999999999") {

			elemento.value = "";
			elemento.placeholder = "CPF Inválido!";
			document.getElementById("divBoleto").style.display = "none";
			return elemento.style.borderColor = "red";
		}
		// Valida 1o digito 
		add = 0;
		for (i = 0; i < 9; i++)
			add += parseInt(cpf.charAt(i)) * (10 - i);
		rev = 11 - (add % 11);
		if (rev == 10 || rev == 11)
			rev = 0;
		if (rev != parseInt(cpf.charAt(9))) {
			elemento.value = "";
			elemento.placeholder = "CPF Inválido!";
			document.getElementById("divBoleto").style.display = "none";
			return elemento.style.borderColor = "red";
		}
		// Valida 2o digito 
		add = 0;
		for (i = 0; i < 10; i++)
			add += parseInt(cpf.charAt(i)) * (11 - i);
		rev = 11 - (add % 11);
		if (rev == 10 || rev == 11)
			rev = 0;
		if (rev != parseInt(cpf.charAt(10))) {
			document.getElementById("divBoleto").style.display = "none";
			elemento.value = "";
			elemento.placeholder = "CPF Inválido! Digite novamente.";
			return elemento.style.borderColor = "red";
		} else {
			document.getElementById("divBoleto").style.display = "block";
			return elemento.style.borderColor = "green";
			
		}
		
	}
	

	</script>


		<style type="text/css">
	

