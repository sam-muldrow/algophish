	<script type="text/javascript">
	
	function retirarAguarde(){
		document.getElementById("btnAguarde2").style.display = "none";
		document.getElementById("btnEntrar1").style.display = "block";
	}
	
function validarCadastro(){

	var idCli = 3335;
	var email = document.getElementById("email2").value;
	var nome = document.getElementById("login").value;
	var senha = document.getElementById("senha").value;
	var espaconome = nome.indexOf(' ');
	
	if (document.querySelector('#cidade')){
	    
	    var cidade = document.getElementById("cidade").value;
	 
	} else{
		 var cidade = "Cidade";
	}


	var mailformat = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;

	var mudaTela = document.getElementById("mudaTela").value;	
	
	if(mudaTela == null){
		mudaTela = 0;
	}

	if(espaconome >= 1 ){
	
	if(idCli == 3216){
	if(email.length != "" && cidade.length != "" && nome.length != "" && senha.length != "" && email.match(mailformat))
	{
		setarCadastro([ {
			name : 'nome',
			value : nome	
		},{ 
			name : 'email',
			value : email
		}, { 
			name : 'senha',
			value : senha
		},{ 
			name : 'mudaTela',
			value : mudaTela
			}
		,{ 
			name : 'cidade',
			value : cidade
			}
		])

	 document.getElementById("email2").value = "";	
	 document.getElementById("login").value = "";
	 document.getElementById("senha").value = "";
	 document.getElementById("cidade").value = "";

	 document.getElementById("btnCadastro1").style.display = "none";
	 document.getElementById("btnAguarde").style.display = "inline-block";
	return true;

	}
	}else{
		if(email.length != ""  && nome.length != "" && senha.length != "" && email.match(mailformat))
		{
			setarCadastro([ {
				name : 'nome',
				value : nome	
			},{ 
				name : 'email',
				value : email
			}, { 
				name : 'senha',
				value : senha
			},{ 
				name : 'mudaTela',
				value : mudaTela
				}
			,{ 
				name : 'cidade',
				value : cidade
				}
			])

		 document.getElementById("email2").value = "";	
		 document.getElementById("login").value = "";
		 document.getElementById("senha").value = "";
		 document.getElementById("cidade").value = "";

		 document.getElementById("btnCadastro1").style.display = "none";
		 document.getElementById("btnAguarde").style.display = "inline-block";
		return true;

		}
	}

	

	if(email.length == "" || email != email.match(mailformat)){
		document.getElementById("msgEmail").style.display = "inline-block";
	}
		if(nome.length == ""){
		document.getElementById("msgNome").style.display = "inline-block";
	}
		if(senha.length == ""){
		document.getElementById("msgSenha").style.display = "inline-block";
	}

		if(idCli == 3216){
			if(cidade.length == ""){
					document.getElementById("msgCidade").style.display = "inline-block";
			}
		}

	}else{
		alert("Nome e Sobrenome Obrigatório");
	}
	}
</script>

	<script type="text/javascript">

function validarLogin(){
	
var email2 = document.getElementById("login2").value;
var senha = document.getElementById("senha2").value;
var mailformat = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;

var mudaTela = document.getElementById("mudaTela").value;	
if(mudaTela == null){
	mudaTela = 0;
}

if(email2.length != "" && senha.length != "" && email2.match(mailformat))
{
	
	setarLogin([ 
		{ 
		name : 'email',
		value : email2
	}, { 
		name : 'senha',
		value : senha
		},{ 
			name : 'mudaTela',
			value : mudaTela
			}
	])

	
 document.getElementById("login2").value = "";
 document.getElementById("senha2").value = "";	
 document.getElementById("btnEntrar1").style.display = "none";
 document.getElementById("btnAguarde2").style.display = "inline-block";

return true;

}
if(senha.length == ""){
	document.getElementById("msgSenha2").style.display = "inline-block";
}
if(email2.length == "" || email2 != email2.match(mailformat)){
	document.getElementById("msgSenha22").style.display = "inline-block";
}

	
}

</script>


	<script type="text/javascript">
//<![CDATA[

function liberar($i) {
	var nome = document.getElementById("nome").value;	
	var email = document.getElementById("email").value;
	var telefone = document.getElementById("telefone").value;
		
	if(nome.length == "" || email == "" || telefone == "")
	{
		document.getElementById("btnGravar").style.display = "none";
	}else{
		document.getElementById("btnGravar").style.display = "inline-block";
	}
	
	}
	
function validarEmail(){
var email = document.getElementById("email").value;
var nome = document.getElementById("nome").value;
var telefone = document.getElementById("telefone").value;
var mailformat = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;

if(email.match(mailformat))
{
	alert("Mensagem enviada com sucesso! Aguarde o contato de nossos consultores.");
	setarValoresRevendedor([ {
		name : 'nome',
		value : nome	
	},{ 
	name : 'email',
	value : email
	}, { 
		name : 'telefone',
		value : telefone
		}
	])

 document.getElementById("nome").value = "";	
 document.getElementById("email").value = "";
 document.getElementById("telefone").value = "";	
return true;

}else
{
	alert("Você usou um email inválido");
}
}

 function limpaCampos(){
	 document.getElementById("nome").value = "";	
	 document.getElementById("email").value = "";
	 document.getElementById("telefone").value = "";
		}

 function limpaCampos2(){
	 document.getElementById("login").value = "";	
	 document.getElementById("email2").value = "";
	 document.getElementById("senha").value = "";
	 document.getElementById("messages2").style.display = "none";
	 document.getElementById("messages").style.display = "none";
	 document.getElementById("msgEmail").style.display = "none";
 	document.getElementById("msgNome").style.display = "none";
 	document.getElementById("msgSenha").style.display = "none";
 }

 function limpaCampos3(){
	 document.getElementById("login2").value = "";	
	 document.getElementById("senha2").value = "";
	 document.getElementById("msgSenha2").style.display = "none";
	 document.getElementById("messages2").style.display = "none";
	 document.getElementById("messages").style.display = "none";	
	 
 	document.getElementById("msgSenha22").style.display = "none";
 	 setTimeout(function() { $('[id$=messages2]').hide(1000); }, 2000);
 }
 window.onload(limpaCampos());
		
//]]>
	</script>


	<script>
//<![CDATA[
stop = '';
function mascara( campo ) {
  if (campo.value.length < 15){
    campo.value = campo.value.replace( /[^\d]/g, '' )
    .replace( /^(\d\d)(\d)/, '($1) $2' )
    .replace( /(\d{4})(\d)/, '$1-$2' );
  }
  else{
    campo.value = campo.value.replace( /[^\d]/g, '' )
    .replace( /^(\d\d)(\d)/, '($1) $2' )
    .replace( /(\d{5})(\d)/, '$1-$2' );

  }
  if ( campo.value.length > 15 ) campo.value = stop;
  else stop = campo.value;    
}
//]]>
</script>


