<script type="text/javascript">
	var customer_id 	=	"";
	function likeThisMerchant(merchantId){
		if(customer_id!=""){
			$.ajax({
	            type:     	'POST',
	            url:      	'model/merchant-like-related-process.php',
	            data:     	'merchantId='+merchantId+'&act=likeMerchant',
	            dataType: 	'html',
	            async:    	false,
	            success:  	function(res){
	                      		var obj = JSON.parse(res);
	                      		if(obj.result=='success'){
	                      			$('[id="heartIconPlane'+merchantId+'"]').hide();
	                      			$('[id="heartIconRed'+merchantId+'"]').show();
	                      			$('[id="heart-counter-main'+merchantId+'"]').html(obj.counter);
	                      		}
	                      	}
	        });
		}else {
			$('#myModal').modal('show');
		}
	}
	function dislikeThisMerchant(merchantId){
		$.ajax({
            type:     	'POST',
            url:      	'model/merchant-like-related-process.php',
            data:     	'merchantId='+merchantId+'&act=unlikeMerchant',
            dataType: 	'html',
            async:    	false,
            success:  	function(res){
                      		var obj = JSON.parse(res);
                      		if(obj.result=='success'){
                      			$('[id="heartIconPlane'+merchantId+'"]').show();
                      			$('[id="heartIconRed'+merchantId+'"]').hide();
                      			$('[id="heart-counter-main'+merchantId+'"]').html(obj.counter);
                      		}
                      	}
        });
	}
	$(document).ready(function () {
        var isChrome = /Chrome/.test(navigator.userAgent) && /Google Inc/.test(navigator.vendor);
        if (window.location.hash && isChrome) {
            setTimeout(function () {
                var hash = window.location.hash;
                window.location.hash = "";
                window.location.hash = hash;
            }, 300);
        }
    });
</script>
