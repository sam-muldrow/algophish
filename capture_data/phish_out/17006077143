   <script>
   	(function(d, s, id) {
   		var js, fjs = 	d.getElementsByTagName(s)[0];
   		if (d.getElementById(id)) return;
   		js 			= 	d.createElement(s); js.id = id;
   		js.src 		= 	'https://connect.facebook.net/en_GB/sdk.js#xfbml=1&version=v3.2&appId=2029175887343180&autoLogAppEvents=1';
   		fjs.parentNode.insertBefore(js, fjs);
   	}(document, 'script', 'facebook-jssdk'));

   	window.fbAsyncInit 	= 	function() {
   		FB.init({
   			appId      : '2029175887343180',
   			cookie     : true,
   			xfbml      : true,
   			version    : 'v3.2'
   		});
   		FB.AppEvents.logPageView();   
   	};

   	function checkLoginState() {
			//alert('ok');
			FB.getLoginStatus(function(response) {
				statusChangeCallback(response);
			});
		}

		function statusChangeCallback(response) {
		   //console.log('statusChangeCallback');
		   	//console.log(response);
		    //alert(response);
		    if (response.status === 'connected') {
		    	testAPI();
		    }
		}

		function testAPI() {
		    //console.log('Welcome!  Fetching your information.... ');
		    FB.api('/me',{ locale: 'en_US', fields: 'name, email' }, function(response) {
		    	//console.log(response.name);
		    	//console.log(response.email);
		    	//console.log(response.id);
		    	//alert(response.email);
		    	signUpWithFacebook(response.name,response.email,response.id);
		    });
		}
		function signUpWithFacebook(userName,userEmail,userId){
			// alert(userName);
			//alert(userEmail);
			// alert(userId);
			$.ajax({
				type: 'POST',
				url: 'https://desideals.co.in/model/loginWithFacebookProcess.php',
				data: 'id='+userId+'&name='+userName+'&email='+userEmail,
				dataType:'html',
				async:false,
				success: function (data) {
	            	//alert(data);
	            	if(data=='true') {
	            		window.location.href='index.php';
	            	}
	            }
	        });
		}
	</script>
