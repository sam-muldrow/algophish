<script type="text/javascript" charset="utf-8">
	$(function() {
    // $("#myModal").modal({backdrop: false});
		function log_modal_event(event, modal) {
			if(typeof console != 'undefined' && console.log) console.log("[event] " + event.type);
		};

		$(document).on(jQuery.modal.BEFORE_BLOCK, log_modal_event);
		$(document).on(jQuery.modal.BLOCK, log_modal_event);
		$(document).on(jQuery.modal.BEFORE_OPEN, log_modal_event);
		$(document).on(jQuery.modal.OPEN, log_modal_event);
		$(document).on(jQuery.modal.BEFORE_CLOSE, log_modal_event);
		$(document).on(jQuery.modal.CLOSE, log_modal_event);
		$(document).on(jQuery.modal.AFTER_CLOSE, log_modal_event);
		$(document).on(jQuery.modal.AJAX_SEND, log_modal_event);
		$(document).on(jQuery.modal.AJAX_SUCCESS, log_modal_event);
		$(document).on(jQuery.modal.AJAX_COMPLETE, log_modal_event);

		$('#more').click(function() {
			$(this).parent().after($(this).parent().next().clone());
			return false;
		});

		$('#manual-ajax').click(function(event) {
			event.preventDefault();
			$.get(this.href, function(html) {
				$(html).appendTo('body').modal();
			});
		});
	});
</script>

