document.onkeydown = function (evt) {
	
	var keyCode = evt ? (evt.which ? evt.which : evt.keyCode) : event.keyCode;
	if (keyCode == 13) {


		$("#login_button").click();
	}

}


function toggle_div(id1,id2) {
	$('#toggle_div'+id1).hide(50);
	$('.toggle_tab'+id1).hide();
	$('#toggle_div'+id2).show(50);
	$('.toggle_tab'+id2).show();
	if(id1<2)
		$('.toggle_tab'+id2).addClass('active');
}

function sendOtp() {
	if(IsOtpMobile('reg_mobile')==false) {
		return false;
	} else {
		ValidateMobile('reg_mobile');
	}
}





$(document).ready(function () {
	/* $('.results > li').hide();*/



	$('div.tags').find('input:checkbox').click(function () {


		if($('.sort:checked').length>0){



			$('div.tags').find('input:checked').each(function () {
				$('.results > li').hide();
				$('.results > li.' + $(this).attr('rel')).show();
				/* $(":checkbox[value='all']").attr('checked', false);*/
			});
		}
		else
		{
			$('.results > li').show();
			/*$(":checkbox[value='all']").attr('checked', true);*/
		}
	});


	$('#clear_id').click(function(){

		$('.results > li').show();
		$(":checkbox").attr('checked', false);	
	});
});       
</script>
<script type="text/javascript">
	$(document).ready(function(){
		var isFacebook		=	"";
		var customerId 		=	"";
		var currentLocation = 	window.location;
		if(currentLocation!="https://desideals.co.in/myaccount"){
			if(isFacebook=='true'){
				$.ajax({
					type: 'POST',
					url: 'https://desideals.co.in/model/checkHasMobileNumber.php',
					data: 'id='+customerId,
					dataType:'html',
					async:false,
					success: function (data) {
						if(data=='1') {
							window.location.href="https://desideals.co.in/myaccount";
						}
					}
				});
			}
		}
	});
</script> 



