                    <label :for="value.id + '-input'">{{ value.settings.label.text }} <span v-if="value.settings.required.is_required == true">*</span></label>
                    <div class="checkbox" v-for="(option, index) in value.settings.options">
                        <label>
                            <input type="checkbox" :name="value.id + '-' + index" :checked="inArray(option.value)" @change="updateQuestionAnswer($event.target.value)" :value="option.value">&nbsp;<span class="form-option-text">{{ option.label }}</span>
