<script type="text/x-template" id="form-file-upload-component">
    <div>
        
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="form-group" v-bind:class="(value.settings.display.classes.container)">
                    <label :for="'file-upload-' + value.id">{{ value.settings.label.text }} <span v-if="value.settings.required.is_required == true">*</span></label>

                    <input
                        :id="'file-upload-' + value.id"
                        type="file"
                        @change="onChange"
                        style="position: absolute; visibility: hidden"
                    >

                    <label class="file-upload-container" :for="'file-upload-' + value.id" tabindex="0">
                        <span class="file-upload-btn">Choose File</span>

                        <span v-if="file">{{ file.name }}</span>
                    </label>
                </div>

                <input type="hidden" :name="value.id" :value="uploadedFileId">

                <p class="text-field-help" style="color: #9e9e9e; font-size: 14px; margin-top: 6px" v-if="value.settings.display.help_text">{{ value.settings.display.help_text }}</p>
            </div>
        </div>

            </div>
</script>

<script>
    Vue.component(
        'form-file-upload-component', {
            template: '#form-file-upload-component',
            props: ['value', 'answers'],
            data: function () {
                return {
                    file: null,
                    uploadedFileId: 0
                }
            },
            methods: {
                onChange: function (e) {
                    if (! e.target.files[0]) {
                        this.file = null

                        this.uploadedFileId = 0

                        return
                    }

                    this.file = e.target.files[0]

                    var fd = new FormData()

                    fd.append('file', this.file, this.file.name)

                    $.ajax({
                        url: '/forms/files/upload',  
                        type: 'POST',
                        data: fd,
                        success: function (data) {
                            this.$emit('answer-update', { questionId: this.value.id, newAnswerValue: data.id });
                            this.uploadedFileId = data.id
                        }.bind(this),
                        processData: false,
                        contentType: false
                    });
                }
            }
        }
    );
</script>    <script type="text/x-template" id="form-submit-button-component">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12" :class="{
        'text-right': value.settings.display.alignment == 'right',
        'text-center': value.settings.display.alignment == 'center',
        'text-left': value.settings.display.alignment == 'left',
    }">
        <button class="btn btn-primary" :class="value.settings.display.classes.element" type="submit">{{ value.settings.display.buttonText }}</button>
    </div>
</script>


<script>
    Vue.component(
        'form-submit-button-component', {
            template: '#form-submit-button-component',
            props: ['value', 'answers'],
            methods: {
                updateQuestionAnswer: function(newValue) {
                    //
                }
            }
        }
    );
</script>
<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            </div>
</div>

<div class="form-3-container" v-cloak>
    <form v-if="! is_loading" id="form-3" @submit="onSubmit" method="post" action="/news-signup/submit?submission_id=2911648&submission_key=6c36e0e0201e4d1786d1c765395ab223" v-if="formTotalQuestions > 0">

        <div class="row" v-if="errors.length">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="alert alert-danger form-alert">
                    <div>Please enter all of the required fields.</div>
                </div>
            </div>
        </div>

        
        
        <div v-if="is_loading" id="form_loading_icon">
            <span class="loading-icon">
                <i class="fa fa-spinner fa-spin"></i>
            </span>
        </div>

        <div class="row">
            <div v-for="(question, index) in questions" :key="question.id" :class="getClassObject(questions[index])">
                <div>
                                            <form-single-line-text-component v-if="question.type == 'single-line-text'" v-model="questions[index]" :answers="answers" v-on:answer-update="onAnswerUpdated">
                        </form-single-line-text-component>
                                            <form-checkbox-component v-if="question.type == 'checkbox'" v-model="questions[index]" :answers="answers" v-on:answer-update="onAnswerUpdated">
                        </form-checkbox-component>
                                            <form-checkbox-list-component v-if="question.type == 'checkbox-list'" v-model="questions[index]" :answers="answers" v-on:answer-update="onAnswerUpdated">
                        </form-checkbox-list-component>
                                            <form-radio-group-component v-if="question.type == 'radio'" v-model="questions[index]" :answers="answers" v-on:answer-update="onAnswerUpdated">
                        </form-radio-group-component>
                                            <form-select-component v-if="question.type == 'select'" v-model="questions[index]" :answers="answers" v-on:answer-update="onAnswerUpdated">
                        </form-select-component>
                                            <form-select-multiple-component v-if="question.type == 'select-multiple'" v-model="questions[index]" :answers="answers" v-on:answer-update="onAnswerUpdated">
                        </form-select-multiple-component>
                                            <form-state-select-component v-if="question.type == 'state-select'" v-model="questions[index]" :answers="answers" v-on:answer-update="onAnswerUpdated">
                        </form-state-select-component>
                                            <form-page-break-component v-if="question.type == 'page-break'" v-model="questions[index]" :answers="answers" v-on:answer-update="onAnswerUpdated">
                        </form-page-break-component>
                                            <form-html-block-component v-if="question.type == 'html-block'" v-model="questions[index]" :answers="answers" v-on:answer-update="onAnswerUpdated">
                        </form-html-block-component>
                                            <form-date-text-component v-if="question.type == 'date-text'" v-model="questions[index]" :answers="answers" v-on:answer-update="onAnswerUpdated">
                        </form-date-text-component>
                                            <form-paragraph-component v-if="question.type == 'paragraph'" v-model="questions[index]" :answers="answers" v-on:answer-update="onAnswerUpdated">
                        </form-paragraph-component>
                                            <form-recaptcha-component v-if="question.type == 'recaptcha'" v-model="questions[index]" :answers="answers" v-on:answer-update="onAnswerUpdated">
                        </form-recaptcha-component>
                                            <form-file-upload-component v-if="question.type == 'file-upload'" v-model="questions[index]" :answers="answers" v-on:answer-update="onAnswerUpdated">
                        </form-file-upload-component>
                                            <form-submit-button-component v-if="question.type == 'submit'" v-model="questions[index]" :answers="answers" v-on:answer-update="onAnswerUpdated">
                        </form-submit-button-component>
                                    </div>
            </div>
        </div>

        <div class="form-controls" style="margin-top: 20px;">

            <div v-if="pagesCount > 1">
                <button class="btn btn-primary" :disabled="currentPage == 1" @click.prevent="previousPage"><i class="fa fa-arrow-left"></i>&nbsp;Previous</button>
                <button class="btn btn-primary" :disabled="currentPage == pagesCount" id="nextBtn" @click.prevent="nextPage" style="float: right; margin-right: 150px;">Next&nbsp;<i class="fa fa-arrow-right"></i></button>
            </div>
        </div>

        <input type="hidden" value="/" name="form_hosted_route">
    </form>

    <div class="form-spinner-container" v-else>
        <svg class="form-spinner" viewBox="0 0 50 50">
            <circle class="path" cx="25" cy="25" r="20" fill="none" stroke-width="5"></circle>
        </svg>
    </div>
</div>

<!-- USED IN VUE INSTANCE -->
<script>

    $(document).ready( function() {
        var app = new Vue({
            el: '.form-' + 3 + '-container',
            data: {
                formId: 3,
                submission: 2911648,
                formKey: '6c36e0e0201e4d1786d1c765395ab223',
                formHostedRoute: '/',
                signatureKey: '725a3b9e866566793e055970b25ecb43115a12ce780988c92ad4b601a4da7d16',
                pagesCount: null,
                currentPage: null,
                oldPage: null,
                questions: [],
                answers: {},
                submission_answers: {},
                hasChanges: false,
                ignoreId: false,
                finalPage: false,
                redirecturl: '',
                formTotalQuestions: 0,
                formTotalAnswers: null,
                currentTotalQuestions: null,
                currentAnswerCount: null,
                is_loading: true,
                categoryName: null,
                progress: null,
                errors: []
            },
            created: function() {
                this.$nextTick(function () {
                    this.getFormPageCount();
                }.bind(this));
            },
            methods: {
                onSubmit: function (e) {
                    var errors = this.validateForm();
                    if (errors.length)  {
                        e.preventDefault()

                        this.errors = errors
                    } else {
                        this.errors = []
                    }

                    if (this.pagesCount > 1) {
                        var formInputs = document.querySelectorAll('.form-' + this.formId + '-container input, .form-' + this.formId + '-container select')

                        formInputs = Array.prototype.filter.call(formInputs, function (el) {
                            return el.id.match(/form_[0-9]+/)
                        }).map(function (el) {
                            return el.id.replace('form_', '')
                        })

                        var answersNotOnPage = []

                        for (var key in this.answers) {
                            var keyNumber = key.replace('question_', '')

                            if (formInputs.indexOf(keyNumber) == -1) {
                                var answerObj = { key: keyNumber, answer: this.answers[key] }
                                answersNotOnPage.push(answerObj)
                            }
                        }

                        answersNotOnPage.forEach(function (answer) {
                            $('#form-' + this.formId).append('<input type="hidden" id="form_' + answer.key + '" name="' + answer.key + '" value="' + answer.answer + '">')
                        }.bind(this))
                    }
                },
                validateForm: function(){
                    var errors = [];

                    this.questions.forEach(function (q) {
                        if (q.settings.required && q.settings.required.is_required && q.showQuestion) {
                            if (! this.answers['question_' + q.id]) {
                                errors.push({ id: q.id, message: 'Missing Field' })
                            } else {
                                if (document.querySelector('[for="' + q.id + '-input"]')) {
                                    document.querySelector('[for="' + q.id + '-input"]').style.color = ''
                                }

                                if (document.getElementById('form_' + q.id)) {
                                    document.getElementById('form_' + q.id).style.borderColor = ''
                                }
                            }
                        }
                    }.bind(this));

                    return errors;
                },
                getClassObject: function (question) {
                    if (! question.settings.display || question.settings.display.width == 'full') {
                        return 'col-xs-12 col-sm-12 col-md-12 col-lg-12'
                    }

                    if (question.settings.display.width == 'half') {
                        return 'col-xs-6 col-sm-6 col-md-6 col-lg-6'
                    }

                    if (question.settings.display.width == 'third') {
                        return 'col-xs-4 col-sm-4 col-md-4 col-lg-4'
                    }
                },
                onAnswerUpdated: function(data) {
                    this.hasChanges = true;

                    this.questions.forEach(function(q, i, arr) {
                        if(q.id == data.questionId) {
                            if (q.type == "checkbox-list" || q.type == "select-multiple") {
                                var answerArray = this.answers['question_' + data.questionId] ? this.answers['question_' + data.questionId].split(';') : [];
                                var index = answerArray.indexOf(data.newAnswerValue);
                                if (index !== -1) {
                                    answerArray.splice(index, 1);
                                } else {
                                    answerArray.push(data.newAnswerValue);
                                }
                                this.answers['question_' + data.questionId] = answerArray.join(';');
                            } else {
                                this.answers['question_' + data.questionId] = data.newAnswerValue;
                            }
                        }
                    }.bind(this));

                    if(typeof this.answers['question_' + data.questionId] == 'undefined') {
                        this.answers['question_' + data.questionId] = {
                            value: ''
                        }
                    }

                    this.parseConditionals();
                    this.$nextTick( function() {
                        bus.$emit('form-done-loading');
                    });
                },
                getFormPageCount: function() {
                    var tags = this.getAllUrlParams().tags;

                    var questionId = '';
                    if (!this.ignoreId) {
                        questionId = this.getAllUrlParams().id
                    }

                    modRequest.request('sa.forms.pagesCount', {}, {
                            form: this.formId,
                            tags: tags ? tags : null,
                            questionId: questionId,
                        },
                        // Success
                        function(response){
                            this.pagesCount = parseInt(response.data.pagesCount);
                            if(!this.currentPage) {
                                this.currentPage = parseInt(response.data.currentPage);
                            }
                        }.bind(this),
                        // Failure
                        function(err) {
                            console.log(err);
                        }.bind(this)
                    );
                },
                saveAndGetFormData: function (pageNumber, validate) {
                    var tags = this.getAllUrlParams().tags;
                    var questionId = '';
                    if (!this.ignoreId) {
                        questionId = this.getAllUrlParams().id;
                    }

                    var shownQuestions = {};
                    // Post which questions are shown for completedness
                    this.questions.forEach(function(question) {
                        shownQuestions[question.id] = question.showQuestion;
                    });
                    let querySubmission = "";
                    if(this.getAllUrlParams().submission_id !== undefined){
                        querySubmission = this.getAllUrlParams().submission_id;
                    }

                    let queryFormKey = "";
                    if(this.getAllUrlParams().submission_key !== undefined){
                        queryFormKey = this.getAllUrlParams().submission_key;
                    }

                    let app = 0;
                    if(this.getAllUrlParams().app !== undefined){
                        app = this.getAllUrlParams().app;
                    }

                    var data = {
                        form: this.formId,
                        pageNumber: pageNumber,
                        submission: this.submission,
                        key: this.formKey,
                        formHostedRoute: this.formHostedRoute,
                        tags: tags,
                        answers: {},
                        querySubmission: querySubmission,
                        queryFormKey: queryFormKey,
                        signatureKey: this.signatureKey,
                        questionId: questionId,
                        shownQuestions: shownQuestions,
                        categoryName: this.categoryName ? this.categoryName : null,
                        allQuestionsAndAnswers: {},
                        validate: validate,
                        memberId: this.getAllUrlParams().pushmemberid,
                        app: app
                    };

                    this.questions.forEach(function (q) {
                        data['allQuestionsAndAnswers'][q.id] = this.answers['question_' + q.id] || ''
                    }.bind(this))

                    if(this.hasChanges) {
                        data.answers = this.answers;
                    }
                    this.is_loading = true;

                    modRequest.request('sa.forms.formsData', {}, data,
                        // Success
                        function(response) {
                            if(response.data.success == false) {
                                this.is_loading = false

                                if (this.currentPage > this.oldPage) {
                                    this.currentPage = this.currentPage - 1
                                } else {
                                    this.currentPage = this.currentPage + 1
                                }

                                this.errors = response.data.errors

                                return false;
                            }

                            this.errors = []

                            this.questions = response.data.questions;
                            // this.answers = response.data.answers;
                            this.submission_answers = response.data.submission_answers;
                            this.redirecturl = response.data.redirecturl;
                            this.formTotalQuestions = response.data.form_total_questions;
                            this.formTotalAnswers = response.data.form_total_answers;
                            this.currentTotalQuestions = response.data.current_total_questions;
                            this.currentAnswerCount = response.data.current_answers;
                            this.is_loading = false;
                            this.ignoreId = true;

                            // Used if category tags exist
                            this.categoryName = response.data.categoryName;

                            this.progress = ((this.currentAnswerCount / this.currentTotalQuestions) * 100).toFixed(0);
                            // To avoid standing out on the front end if there's an issue
                            if(this.progress > 100) {
                                this.progress = 100;
                            }
                            $('#sa-form-progress-bar').css('width', this.progress ? this.progress + '%' : 0 + '%');
                            $('#sa-form-progress').html(this.progress ? this.progress + '%' : 0 + '%');

                            if(typeof response.data.answers !== 'undefined' && !(response.data.answers instanceof Array)) {
                                this.answers = response.data.answers;
                            }

                            this.parseConditionals();

                        }.bind(this),
                        // Failure
                        function(err) {
                            console.log(err);
                            console.log('formsData error');
                        }.bind(this)
                    );
                },
                getAllUrlParams: function(url) {
                    // get query string from url (optional) or window
                    var queryString = url ? url.split('?')[1] : window.location.search.slice(1);

                    // we'll store the parameters here
                    var obj = {};

                    // if query string exists
                    if (queryString) {

                        // stuff after # is not part of query string, so get rid of it
                        queryString = queryString.split('#')[0];

                        // split our query string into its component parts
                        var arr = queryString.split('&');

                        for (var i = 0; i < arr.length; i++) {
                            // separate the keys and the values
                            var a = arr[i].split('=');

                            // set parameter name and value (use 'true' if empty)
                            var paramName = a[0];
                            var paramValue = typeof (a[1]) === 'undefined' ? true : a[1];

                            // (optional) keep case consistent
                            paramName = paramName.toLowerCase();
                            if (typeof paramValue === 'string') paramValue = paramValue.toLowerCase();

                            // if the paramName ends with square brackets, e.g. colors[] or colors[2]
                            if (paramName.match(/\[(\d+)?\]$/)) {

                                // create key if it doesn't exist
                                var key = paramName.replace(/\[(\d+)?\]/, '');
                                if (!obj[key]) obj[key] = [];

                                // if it's an indexed array e.g. colors[2]
                                if (paramName.match(/\[\d+\]$/)) {
                                    // get the index value and add the entry at the appropriate position
                                    var index = /\[(\d+)\]/.exec(paramName)[1];
                                    obj[key][index] = paramValue;
                                } else {
                                    // otherwise add the value to the end of the array
                                    obj[key].push(paramValue);
                                }
                            } else {
                                // we're dealing with a string
                                if (!obj[paramName]) {
                                    // if it doesn't exist, create property
                                    obj[paramName] = paramValue;
                                } else if (obj[paramName] && typeof obj[paramName] === 'string'){
                                    // if property does exist and it's a string, convert it to an array
                                    obj[paramName] = [obj[paramName]];
                                    obj[paramName].push(paramValue);
                                } else {
                                    // otherwise add the property
                                    obj[paramName].push(paramValue);
                                }
                            }
                        }
                    }

                    return obj;
                },
                parseConditionals: function() {

                    // Put this on the next tick to get conditionals to show or hide properly
                    this.$nextTick(function () {
                        // If there are no visible questions on page, skip page
                        var pageHasVisibleQuestions = false;

                        // Loop through each question on the page to see if it should be shown or not based on its conditional_groups setting.
                        this.questions.forEach(function(question, index) {

                            // showQuestion is sent to the DOM at the end of each iteration
                            var showQuestion = false;
                            if(!question.settings.conditional_groups) {
                                showQuestion = true;
                            } else {

                                // Get the outside operator of AND or OR - This ANDS or ORs all of the groups together
                                var outerOperator = question.settings.conditional_groups.operator;

                                // The True or False values to be compared with the outer operator at the end
                                var outerBools = [];

                                // Loop through conditional groups for the question and parse logic
                                question.settings.conditional_groups.groups.forEach(function (group) {
                                    // For each inner group, get the operator AND or OR
                                    var innerOperator = group.operator;

                                    // The True or False values of each group to be compared with the inner operator
                                    var innerBools = [];

                                    // Loop through each group to see if the answer for the given question matches the value
                                    group.rules.forEach(function (rule) {

                                        // Boolean value for the current rule to be added to innerBools array
                                        var ruleBool = false;

                                        // Attempt to find this question in the DOM
                                        var thisQuestion = this.findInArray(this.questions, function(qq) {
                                            return qq.id == rule.question_id;
                                        });
                                        if(!thisQuestion) {
                                            // If the question is not in the DOM, get the current submission's answer for the parent question in the current rule
                                            var submission_answer = this.findInArray(this.submission_answers, function (a) {
                                                return a.question_id == rule.question_id;
                                            });

                                            // Check the rule value against the submission answer
                                            if (!submission_answer) {
                                                ruleBool = false;
                                            } else if (submission_answer.question_type === "checkbox-list" || submission_answer.question_type === "select-multiple") {

                                                // If the submission answer is a checkbox list, see if the answer is in the list
                                                var answerArr = submission_answer.value ? submission_answer.value.split(';') : [];
                                                if (answerArr.indexOf(rule.value) !== -1) {
                                                    ruleBool = true;
                                                }

                                            } else if (submission_answer.value === rule.value) {
                                                ruleBool = true;
                                            }
                                        } else {

                                            // If the question is in the DOM, get its current answer
                                            if (thisQuestion.type === "checkbox-list" || thisQuestion.type === "select-multiple") {

                                                // Check to see if the conditional value is in the array of answers for this checkbox list
                                                var answerArr = this.answers['question_' + rule.question_id] ? this.answers['question_' + rule.question_id].split(';') : [];
                                                if (answerArr.indexOf(rule.value) !== -1) {
                                                    ruleBool = true;
                                                }

                                            } else if (this.answers['question_' + rule.question_id] === rule.value) {
                                                ruleBool = true;
                                            }

                                            // If the question is hidden, hide the current question
                                            if (thisQuestion.showQuestion === false) {
                                                ruleBool = false;
                                            }

                                        }

                                        // Add this rule's boolean value to the inner boolean array
                                        innerBools.push(ruleBool);
                                    }.bind(this));

                                    // Depending on AND or OR, determine the overall boolean value of this group
                                    var innerShowQuestion;
                                    if (innerOperator === "AND") {
                                        innerShowQuestion = true;
                                        innerBools.forEach(function (innerBool) {
                                            if (!innerBool) {
                                                innerShowQuestion = false;
                                            }
                                        });
                                    } else if (innerOperator === "OR") {
                                        innerShowQuestion = false;
                                        innerBools.forEach(function (innerBool) {
                                            if (innerBool) {
                                                innerShowQuestion = true;
                                            }
                                        });
                                    } else {
                                        // No clue why you would get here, but just in case
                                        console.log("ERROR: invalid innerOperator: ");
                                        console.log(innerOperator);
                                    }

                                    outerBools.push(innerShowQuestion);

                                }.bind(this));


                                // Depending on AND or OR, determine whether this question should be shown or not
                                if (outerOperator === "AND") {
                                    showQuestion = true;
                                    outerBools.forEach(function (innerBool) {
                                        if (!innerBool) {
                                            showQuestion = false;
                                        }
                                    });
                                } else if (outerOperator === "OR") {
                                    showQuestion = false;
                                    outerBools.forEach(function (innerBool) {
                                        if (innerBool) {
                                            showQuestion = true;
                                        }
                                    });
                                } else {
                                    // No clue why you would get here, but just in case
                                    console.log("ERROR: invalid outerOperator: ");
                                    console.log(outerOperator);
                                }
                            }

                            // Update conditional rendering for the current question
                            if(!showQuestion) {
                                this.answers['question_' + question.id] = null;
                            } else {
                                pageHasVisibleQuestions = true;
                            }
                            question.showQuestion = showQuestion;
                            bus.$emit('update-conditional-rendering', { id: question.id, showQuestion: showQuestion })
                        }.bind(this));

                        if(!pageHasVisibleQuestions) {
                            if(this.oldPage && this.currentPage < this.oldPage || this.currentPage === this.pagesCount) {
                                this.currentPage--;
                            } else {
                                this.currentPage++;
                            }
                            if(this.currentPage > this.pagesCount) {
                                this.currentPage = this.pagesCount;
                            }
                        }
                        this.$nextTick(function() {
                            $('.multiple-select').select2({
                                closeOnSelect: false
                            });
                        });
                    }.bind(this))
                },
                findInArray: function(arr, callback) {
                    for(var i = 0; i < arr.length; i++) {
                        var match = callback(arr[i])
                        if(match){
                            return arr[i];
                            break;
                        }
                    }
                },
                nextPage: function(){
                    var errors = this.validateForm();
                    if(errors.length == 0){
                        this.currentPage++;
                        this.errors = [];
                    }else{
                        this.errors = errors;
                    }
                },
                previousPage: function(){
                    this.currentPage--;
                }
            },
            watch: {
                errors: function (value) {
                    value.forEach(function (error) {
                        this.$nextTick(function () {
                            if (document.querySelector('[for="' + error.id + '-input"]')) {
                                document.querySelector('[for="' + error.id + '-input"]').style.color = '#dc1818'
                                document.getElementById('form_' + error.id).style.borderColor = '#e57373'
                            }
                        })
                    }.bind(this))
                },
                currentPage: function(newPage, oldPage) {
                    if (oldPage !== null) {
                        this.ignoreId = true;
                    }
                    localStorage.setItem('curPage', newPage);
                    this.oldPage = oldPage;
                    var validate = true;
                    if (this.currentPage == this.oldPage) {
                        validate = false;
                    }
                    this.saveAndGetFormData(newPage, validate);
                }
            }
        });

        $('input[type="tel"]').mask('(999) 999-9999');

        $('input[mask]').each(function () {
            if ($(this).attr('mask').length != '') {
                $(this).mask( $(this).attr('mask'));
            }
        });

        $('.uploadaliasbutton').click( function(e) {

            var fileelementid = $(this).data('id');

            $('#uploadfilebutton-'+fileelementid).click();
            e.preventDefault();
        });
    });
</script>            </div>
          </div>
          <div class="col-lg-4">
            
            <ul class="social-icons style-2">
               
                <li><a href="https://www.facebook.com/African-American-Forum-Inc-269318929765833/" target="_blank"><i class="icon-facebook"></i></a></li>
                                                                                  </ul>

          </div>
        </div>

      </div>

    </div>

   <footer id="footer" class="footer">

      <div class="container wide">

        <!-- main footer -->
        <div class="main-footer">
          
          <div class="row">
        
            <div class="col-lg-3 col-sm-6">
              
              <div class="widget">
                
