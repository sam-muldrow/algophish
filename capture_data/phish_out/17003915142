<script src="https://www.google.com/recaptcha/api.js?render=6LeV-GcbAAAAAGyEvPX_byFnrgssOm3LFYHxTdMv"></script>

<script>
    Vue.component(
        'form-recaptcha-component', {
            template: '#form-recaptcha-component',
            props: ['value', 'answers'],
            data: function () {
                return {
                    token: ''
                }
            },
            mounted: function () {
                $('#form-' + this.$parent.$data.formId).on('submit', function (e) {
                    var that = this

                    e.preventDefault()

                    if (! that.$parent.$data.errors.length) {
                        grecaptcha.ready(function () {
                            grecaptcha.execute('6LeV-GcbAAAAAGyEvPX_byFnrgssOm3LFYHxTdMv').then(function(token) {
                                that.token = token

                                that.$nextTick(function () {
                                    $('#form-' + that.$parent.$data.formId).off('submit')

                                    $('#form-' + that.$parent.$data.formId)[0].submit()
                                })
                            });
                        });
                    }
                }.bind(this))
            }
        }
    );
