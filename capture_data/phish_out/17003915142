<script>
    Vue.component(
        'form-date-text-component', {
            template: '#form-date-text-component',
            props: ['value', 'answers'],
            computed: {
                answer: function() {
                    if(typeof this.answers == 'undefined') {
                        return '';
                    }

                    if(this.answers && this.answers['question_' + this.value.id] != null) {
                        return this.answers['question_' + this.value.id];
                    }

                    return '';
                }
            },
            data: function () {
                return {
                    showQuestion: true
                }
            },
            methods: {
                updateQuestionAnswer: function (newValue) {
                    this.$emit('answer-update', { questionId: this.value.id, newAnswerValue: newValue });
                },
                moveToBeginning: function (e) {
                    this.$nextTick(function () {
                        $(e.target).focus();
                        $(e.target).get(0).setSelectionRange(0,0);
                    })
                }
            },
            created: function(){
                bus.$on('update-conditional-rendering', function (payload) {
                    if (this.value.id == payload.id) {
                        this.showQuestion = payload.showQuestion
                    }
                }.bind(this))
            },
            mounted: function () {
                $('#form_' + this.value.id).mask('99/99/9999', {
                    placeholder: '__/__/____'
                })
            },
            components: {
                'datepicker': vuejsDatepicker
            }
        }
    );
</script>    <style>
    .paragraph-settings-textfield {
        min-height: 75px;
        resize: vertical;
    }
</style>

<script type="text/x-template" id="form-paragraph-component">
    <div class="form-group" v-if="showQuestion">
        
        <label :for="value.id + '-input'">{{ value.settings.label.text }} <span v-if="value.settings.required && value.settings.required.is_required == true">*</span></label>

        <textarea :id="value.id + '-input'" maxlength="255" class="paragraph-settings-textfield form-control" :name="value.id" @input="updateQuestionAnswer($event.target.value)" :placeholder="value.settings.display.placeholder">{{ answer }}</textarea>

        <p class="text-field-help" style="color: #9e9e9e; font-size: 14px; margin-top: 6px" v-if="value.settings.display.help_text">{{ value.settings.display.help_text }}</p>
    </div>
