<script>
    Vue.component(
        'form-checkbox-component', {
            template: '#form-checkbox-component',
            props: ['value', 'answers'],
            computed: {
                isChecked: {
                    get: function() {
                        if(this.value.settings.is_checked === true) {
                            return 'checked';
                        } else {
                            return '';
                        }
                    }
                }
            },
            methods: {
                updateQuestionAnswer: function(newValue) {
                    if (this.value.settings.required.is_required) {
                        this.$emit('answer-update', { questionId: this.value.id, newAnswerValue: newValue ? 'Yes' : '' });
                    } else {
                        this.$emit('answer-update', { questionId: this.value.id, newAnswerValue: newValue ? 'Yes' : 'No' });
                    }
                }
            },
            data: function () {
                return {
                    showQuestion: true
                }
            },
            created: function () {
                bus.$on('update-conditional-rendering', function (payload) {
                    if (this.value.id == payload.id) {
                        this.showQuestion = payload.showQuestion
                    }
                }.bind(this))
            }
        }
    );
</script>    <script type="text/x-template" id="form-checkbox-list-component">
