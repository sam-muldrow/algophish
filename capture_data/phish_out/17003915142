<script>
    Vue.component(
        'form-select-multiple-component', {
            template: '#form-select-multiple-component',
            props: ['value', 'answers'],
            methods: {
                updateQuestionAnswer: function(newValue) {
                    this.$emit('answer-update', { questionId: this.value.id, newAnswerValue: newValue });

                    document.querySelector('input[name="' + this.value.id + '"]').value = this.answers['question_' + this.value.id]
                },
                inArray: function(value) {
                    return this.selected_answers.indexOf(value) !== -1;
                }
            },
            computed: {
                placeholderAttribute: {
                    get: function() {
                        return this.value.settings.display.placeholder;
                    }
                }
            },
            data: function () {
                return {
                    showQuestion: true,
                    selected_answers: []
                }
            },
            created: function () {
                if(this.answers && this.answers['question_' + this.value.id]) {
                    this.selected_answers = this.answers['question_' + this.value.id].split(';');
                } else {
                    this.selected_answers = [];
                }
                bus.$on('update-conditional-rendering', function (payload) {
                    if (this.value.id == payload.id) {
                        this.showQuestion = payload.showQuestion;
                        if (!this.showQuestion) {
                            this.selected_answers = [];
                        }
                    }
                }.bind(this));

            },
            mounted: function() {
                $('.multiple-select').select2({
                    closeOnSelect: false
                });
                $('#multiple-select-' + this.value.id).on('select2:select', function(e) {
                    this.updateQuestionAnswer(e.params.data.text);
                }.bind(this));
            }
        }
    );
