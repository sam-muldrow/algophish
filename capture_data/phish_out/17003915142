            <div id="newsletter" class="newsletter style-2">
              <script>
    window.bus = new Vue
</script>

<script src='https://unpkg.com/v-calendar'></script>

    <script type="text/x-template" id="form-single-line-text-component">
    <div v-if="showQuestion">
        
        <div>
            <div class="form-group" v-bind:class="(value.settings.display.classes.container)">
                <label :for="value.id + '-input'">{{ value.settings.label.text }} <span v-if="value.settings.required.is_required == true">*</span></label>

                <input
                        :id="'form_' + value.id"
                        :name="value.id"
                        :type="shownType"
                        class="form-control"
                        v-bind:class="(value.settings.display.classes.element)"
                        :placeholder="placeholderAttribute"
                        v-on:input="updateQuestionAnswer($event.target.value)"
                        v-bind:value="answer"
                        maxlength="255">

                <p class="text-field-help" style="color: #9e9e9e; font-size: 14px; margin-top: 6px" v-if="value.settings.display.help_text">{{ value.settings.display.help_text }}</p>
            </div>
        </div>

            </div>
</script>

<script>
    Vue.component(
        'form-single-line-text-component', {
            template: '#form-single-line-text-component',
            props: ['value', 'answers'],
            methods: {
                updateQuestionAnswer: function(newValue) {
                    this.$emit('answer-update', { questionId: this.value.id, newAnswerValue: newValue });
                }
            },
            computed: {
                shownType: function () {
                    if (this.value.settings.display.type == 'phone') {
                        return 'text'
                    }

                    return this.value.settings.display.type
                },
                answer: function() {
                    if(typeof this.answers == 'undefined') {
                        return '';
                    }

                    if(this.answers && this.answers['question_' + this.value.id] != null) {
                        return this.answers['question_' + this.value.id];
                    }

                    return '';
                },
                placeholderAttribute: {
                    get: function() {
                        return this.value.settings.display.placeholder;
                    }
                }
            },
            data: function () {
                return {
                    showQuestion: true
                }
            },
            created: function () {
                bus.$on('update-conditional-rendering', function (payload) {
                    if (this.value.id == payload.id) {
                        this.showQuestion = payload.showQuestion
                    }
                }.bind(this))
            },
            mounted: function () {
                if (this.value.settings.display.type === 'phone') {
                    var that = this

                    $('#form_' + this.value.id).mask('(999) 999-9999', {
                        placeholder: '(___) ___-____'
                    })
                }
            }
        }
    );
</script>    <script type="text/x-template" id="form-checkbox-component">
    <div v-if="showQuestion">
        
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="form-group" v-bind:class="(value.settings.display.classes.container)">
                    <label :for="value.id + '-input'">
                        <input type="checkbox" :id="'form_' + value.id" :name="value.id" :checked="isChecked" @change="updateQuestionAnswer($event.target.checked)">&nbsp;{{ value.settings.label.text }} <span v-if="value.settings.required.is_required == true">*</span>
                    </label>
