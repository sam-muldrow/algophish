              <td id="colonne_int_1"><script type="text/javascript">
	$(document).ready(function()
	{
		var curent_actu = 0;
		var chapeau     = new Array("Le Forum instant jeunes agris offrira aux jeunes toutes les informations nécessaires pour élaborer leur projet d'installation le 23 novembre. 
", "La formation sur le parcours de transmission organisée début 2023 par la chambre d’agriculture en a convaincu plus d’un, alors bonne nouvelle :", "La zone régulée couvre dorénavant tout le département et les départements voisins. Avec près de 3 000 foyers recensés en France, recensé à 90", "L’association prim’holstein France fête ses 100 ans en 2023 au Puy du Fou.");
		var images      = new Array("bibliotheque_image/Actualites/2023/Novembre/LAA%20P.28%20(1).png", "bibliotheque_image/Actualites/2023/Novembre/44_IMG_20230209_172251%20(1)%20(1).jpg", "bibliotheque_image/Actualites/2023/Novembre/p9%20VA%20et%2012%20LAA%20-%20Photo%20article%20MHE%20(1)%20(1).jpg", "bibliotheque_image/Actualites/2023/Novembre/VA%20p.07%20(1).png");
		var link        = new Array("magazine-une-nouvelle-edition-un-nouveau-lieu-une-nouvelle-programmation-actualite-numero-124711.php", 
									"transmission--anticiper-pour-bien-preparer-mon-projet-de-transmission--je-me-suis-forme-je-suis-rassure-actualite-numero-124708.php", 
									"maladie-hemorragique-epizootique-les-premiers-foyers-mhe-sur-des-bovins-en-vendee-actualite-numero-124706.php", 
									"primholstein-la-vache-du-centenaire-actualite-numero-124705.php");
		
		function change_image(curent_actuA)
		{
			if(curent_actuA < 3)
				curent_actuA++;
			else
				curent_actuA = 0;
			
			$("div.cadre_actu").css("background-image", "url('"+images[curent_actuA]+"')");
			$("a#link_curent_actu").attr("href", link[curent_actuA]);
			
			$("div.fnd_chapeau, div.chapeau").animate(
			{
				right: 500,
			}, 500, function()
			{
				$("div.chapeau").html(chapeau[curent_actuA]);
			});
			$("img.actu_index_suite").animate(
			{
				right: 523,
			}, 500);
			
			$("div.img_actu img").fadeOut(1000, function()
			{
				$("div.img_actu img").attr("src", images[curent_actuA]);
				$("div.img_actu img").fadeIn(100);
			});
			
			$("div.titre_liste_actu").css("background-color", "#34495E");
			$("div.titre_liste_actu a").css("color", "#6084A9");;
			$("div.titre_liste_actu a").css("font-weight", "normal");
			
			$("div.fnd_chapeau, div.chapeau").animate(
			{
				right: 45,
			}, 500);
			$("img.actu_index_suite").animate(
			{
				right: 32,
			}, 500);
			
			$("div#titre_actu_"+curent_actuA).css("background-color", "#2C3E50");
			$("div#titre_actu_"+curent_actuA+" a").css("color", "#FFFFFF");
			$("div#titre_actu_"+curent_actuA+" a").css("font-weight", "bold");
			
			return curent_actuA;
		}
		
		var caroussel = setInterval(function()
		{
			curent_actu = change_image(curent_actu);
		}, 9000);
		
		$("div.cadre_actu").hover(function()
		{
			clearInterval(caroussel);
		}, function()
		{
			caroussel = setInterval(function()
			{
				curent_actu = change_image(curent_actu);
			}, 9000);
		});
		
		$("div.titre_liste_actu").mouseenter(function()
		{	
			var id_tab = $(this).attr("id").split("_");
			var id     = id_tab[2];
			
			if(id != curent_actu)
			{
				if(id == 0)
					curent_actuB = 3;
				else
					curent_actuB = id - 1;
				
				curent_actu = curent_actuB;
				
				setTimeout(function()
				{
					if(curent_actuB == curent_actu)
						curent_actu = change_image(curent_actuB);
				}, 300);
			}
		});
		
		$("div.img_actu, div.chapeau, div.titre_liste_actu").click(function()
		{
			var link = $("a#link_curent_actu").attr("href");
			document.location.href = link;
		});
	});
</script>

