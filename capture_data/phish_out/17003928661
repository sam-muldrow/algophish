    function isInIframe(){
        try{
            if(window.location != window.parent.location){
                return true;
            }
        }catch(err) {
            return true;
        }
        return false;
    }
    
    if(!isInIframe()){
        window.location = "/app/";
    }
    
    </script>
    
        
    
    
    
