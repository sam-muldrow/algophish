		function fetchResults() {
			let keyword = jQuery('#searchInput').val();
			if (keyword == "") {
				jQuery('#datafetch').html("");
			} else {
				jQuery.ajax({
					url: 'https://www.evently-yours.fr/wp-admin/admin-ajax.php',
					type: 'post',
					data: {
						action: 'data_fetch',
						keyword: keyword,
						lang: 'fr',
					},
					success: function(data) {
						jQuery('#datafetch').html(data);
					}
				});
			}


		}
	</script>

