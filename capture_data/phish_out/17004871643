            <button class="lines-button x responsive_menu_btn menu-button" type="button" role="button" aria-label="Toggle Navigation">
    <span class="lines"></span>
</button>
<script>
    (function ($) {
        $('.lines-button').click(function () {
            if ($(this).hasClass('open')) {
                $(this).removeClass('open');
                $("body").removeClass('responsive-menu-open');
            } else {
                $(this).addClass('open');
                $("body").addClass('responsive-menu-open');
            }
        });

        // close burger menu on click outside
        window.addEventListener('click', function (e) {
            if ((document.getElementById('responsive-header') != null && document.getElementById('responsive-header').contains(e.target)) ||
                    (document.getElementById('full_screen_menu_back') != null && document.getElementById('full_screen_menu_back').contains(e.target))) {  // clicked inside responsive menu

            } else { // clicked outside responsive menu
                $('.lines-button').each(function () {
                    if ($(this).is(":visible")) {
                        if ($(this).hasClass('open')) {
                            $(this).trigger("click");
                        }
                    }
                })
            }
        })
    })(jQuery);
</script><div class="flexnav_container"><ul data-breakpoint="768" id="flexnav_responsive" class="flexnav"><li id="menu-item-103" class="menu-item menu-item-type-post_type menu-item-object-page menu-item-has-children menu-item-103"><a href="https://www.blioumis.gr/%ce%b5%cf%84%ce%b1%ce%b9%cf%81%ce%b5%ce%af%ce%b1/">ΕΤΑΙΡΕΙΑ</a>
