<script type="text/javascript">
    jQuery(document).ready(function () {
        jQuery("#menu-header-support").find('li').addClass('customer-support');
        jQuery(".woocommerce-product-gallery__trigger").html("<i class='ec ec-search' style='color: #333;font-size: 27px;font-weight: bold;'></i>");
        jQuery(".woocommerce-widget-layered-nav-dropdown__submit").html("Filtrer");
        jQuery(".woocommerce-widget-layered-nav-dropdown__submit").css("display", "block");
        jQuery(".yith-wcwl-after-share-section").find("small").html("copiez ce lien pour partager vos favoris !");
        jQuery(".yith-wcwl-after-share-section").find("small").css("display", "block");
        jQuery(".remove-icon[title='Remove']").attr("title", "Retirer");
        jQuery("#menu-header-support a").attr("title", "");
        //jQuery().find(".product-subtitle p").insertAfter(".woocommerce-loop-product__title");

        jQuery("li.product.type-product").each(function (index) {
            $li = jQuery(this);
            $li.find(".product-subtitle").each(function (_index) {
                $product = jQuery(this);
                if($product.find("p").length){
                    $li.find(".woocommerce-loop-product__title").append($product.html())
                }else{
                    $li.find(".woocommerce-loop-product__title").append("<p>" + $product.html() + "</p>")
                }
                $product.remove()
            })
        });
        jQuery(".owl-item").each(function (index) {
            $owl = jQuery(this);
            $owl.find(".product-subtitle").each(function (_index) {
                $product = jQuery(this);
                if ($product.find("p").length) {
                    $owl.find(".woocommerce-loop-product__title").append($product.html())
                } else {
                    $owl.find(".woocommerce-loop-product__title").append("<p>" + $product.html() + "</p>")
                }
                $product.remove()
            })
        });

    });
</script>
