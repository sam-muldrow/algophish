    <style type="text/css">
        
            /* jQuery UI Calendar */
            .ui-datepicker {font-size: 12px;z-index: 9 !important;}
            .ui-datepicker#ui-datepicker-div{font-size: 13px;}
            .ui-datepicker#ui-datepicker-div .ui-widget-header{color: #FFFFFF;background: #FF7500;font-weight: normal;border: 0;margin: -4px -4px 0 -4px;-webkit-border-radius: 1px;-webkit-border-top-left-radius: 4px;-webkit-border-top-right-radius: 4px;-moz-border-radius: 1px;-moz-border-radius-topleft: 4px;-moz-border-radius-topright: 4px;border-radius: 1px;border-top-left-radius: 4px;border-top-right-radius: 4px;}

