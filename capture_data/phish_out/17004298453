<script type="text/javascript">

function eliminar_tramo(tramo){
	$('#tramo'+tramo).remove();
	

	
	tramos_actuales=document.getElementById("motor_tramos").value;
	nexttramo=tramos_actuales-1;
	document.getElementById("motor_tramos").value=nexttramo;
	$('.agregar_tramo_multiple').show();
	
	if(parseInt(tramo)<=2) {	
	document.getElementById("motor_tramos").value=2;
	document.getElementById("motor_multiples_destinos").value=0;	
	}
	


}

var nexttramo = document.getElementById("motor_tramos").value;
function agregar_tramo(){
nexttramo++;

campo = '<div id="tramo' + nexttramo + '"><span class="hide">Tramo ' + nexttramo + '</span><span class="float_right cursor btn-eliminar" onclick="eliminar_tramo(' + nexttramo + ');"><i class="fa fa-minus-circle"></i> Eliminar tramo</span><li class="row_origen"><label for="origen_1">Origen</label><input name="motor_destinos[]" type="text" placeholder="Origen" id="motor_destino_tramo' + nexttramo + '_salida" class="aeropuerto_autocomplete aeropuertos"/></li><li class="row_destino"><label for="destino_1">Destino</label><input name="motor_destinos[]" type="text" placeholder="Destino" id="motor_destino_tramo' + nexttramo + '_llegada" class="aeropuerto_autocomplete aeropuertos"/></li><div class="clear"></div><li class="row_fecha_partida"><label for="fecha_partida">Partida</label><input name="motor_fechas[]" type="text" placeholder="Partida" id="motor_fecha_' + nexttramo + '" class="calendar datepicker_dinamico" readonly="readonly"/></li><div class="clear"></div></div></div>';



if(nexttramo<=5) {


$("#wb_campos").append(campo);

	$("#tramo"+nexttramo+" .aeropuerto_autocomplete").autocomplete({
		source: "https://andviajes.tur.ar/mkte_air/templates/aeropuertos_json.php",
		minLength: 3
    });		
	
	
// reasignacion de calendar
activar_datepicker(nexttramo);

// ajusto valos motor_tramos
document.getElementById("motor_tramos").value=nexttramo;
// copio la info de la llegada del tramo anterior a la nueva salida
tramoanterior=nexttramo-1;
document.getElementById("motor_destino_tramo"+ nexttramo +"_salida").value=document.getElementById("motor_destino_tramo"+ tramoanterior +"_llegada").value;

} 

if(nexttramo==5) {
$('.agregar_tramo_multiple').hide();	
}

}
</script>

<script>
function activar_datepicker(tramo) {
	tramo_anterior=tramo-1;
	tramo_siguiente=tramo+1;	

 $(function() {
			
	if($(window).width()>700)
	var calendar_month_numbers=3;
	else
	var calendar_month_numbers=1;
	
	
    $( "#motor_fecha_"+tramo ).datepicker({
      numberOfMonths: calendar_month_numbers,
      showButtonPanel: true,
	  dateFormat: "dd-mm-yy",
	  minDate: $("#motor_fecha_"+tramo_anterior).datepicker('getDate'),
	  maxDate: new Date(2024,10,12),
	  onSelect: function(dateStr) {
            var min = $(this).datepicker('getDate') || new Date(); // Selected date or today if none
            var max = new Date(2024,10,12); // Add one month
            $('#motor_fecha_'+tramo_siguiente).datepicker('option', {minDate: min, maxDate: max});
	  
  }
    });
	});
}
</script>

