   <link href="https://andviajes.tur.ar/mkte_air/templates/css.php" rel="stylesheet" type="text/css" />
   <link href="https://andviajes.tur.ar/mkte_air/templates/js/Small-Popover-Modal-Plugin-jQuery/popover.css" rel="stylesheet" type="text/css" />
   <link href="https://andviajes.tur.ar/mkte_air/templates/number.css" rel="stylesheet"
   type="text/css" />
   <script type="text/javascript" src="https://andviajes.tur.ar/mkte_air/templates/js/Small-Popover-Modal-Plugin-jQuery/popover.min.js"></script>
   <script src="https://andviajes.tur.ar/mkte_air/templates/js/InputSpinner.js"></script>



 <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBhV0dvglkPVvQsWHTI-xjVmcL_n8t-s8E&libraries=places"></script>  
<style>
.ui-autocomplete { max-height:150px;overflow-y:auto;overflow-x:hidden;}
.ui-autocomplete-term { font-weight: bold; }
</style>

<script type="text/javascript">



$(document).ready(function(){   
						   
	$('.aeropuerto_autocomplete_salida').autocomplete({
		source: "https://andviajes.tur.ar/mkte_air/templates/aeropuertos_json.php?salida=1",
		minLength: 3
	}).data("ui-autocomplete")._renderItem = function( ul, item ) {			
		let txt = String(item.value).replace(new RegExp(this.term, "gi"),"<b>$&</b>");		
		if(item.class){
            return $('<li class="ui-state-disabled"><strong><i class="'+item.class+'"></i> '+item.label+'</strong></li>').appendTo(ul);
        } else {
            return $("<li>")
            .append("<a>" + txt + "</a>")
            .appendTo(ul);
        }
        };
	

	
	$('.aeropuerto_autocomplete').autocomplete({
		source: "https://andviajes.tur.ar/mkte_air/templates/aeropuertos_json.php",
		minLength: 3
	}).data("ui-autocomplete")._renderItem = function( ul, item ) {		
		 let txt = String(item.value).replace(new RegExp(this.term, "gi"),"<b>$&</b>");		
		if(item.class){
            return $('<li class="ui-state-disabled"><strong><i class="'+item.class+'"></i> '+item.label+'</strong></li>').appendTo(ul);
        } else {
            return $("<li>")
            .append("<a>" + txt + "</a>")
            .appendTo(ul);
        }				
        };



	$('.lineas_aereas_autocomplete').autocomplete({
		source: "https://andviajes.tur.ar/mkte_air/templates/lineas_aereas_json.php",
		minLength: 2
	}).data("ui-autocomplete")._renderItem = function( ul, item ) {
        let txt = String(item.value).replace(new RegExp(this.term, "gi"),"<b>$&</b>");		
		if(item.class){
            return $('<li class="ui-state-disabled"><strong><i class="'+item.class+'"></i> '+item.label+'</strong></li>').appendTo(ul);
        } else {
            return $("<li>")
            .append("<a>" + txt + "</a>")
            .appendTo(ul);
        }
        };
	

	

	$('.boton_opciones_avanzadas').click(function() {
		$(this).parent().parent().find('div.opciones_avanzadas').slideToggle('fast');
		/*if($(this).html() == '<i class="fa fa-angle-down"></i> Opciones avanzadas'){ $(this).html('<i class="fa fa-angle-up"></i> Opciones avanzadas'); }
		else { $(this).html('<i class="fa fa-angle-down"></i> Opciones avanzadas'); }*/
		return false;
	});
	
		
		
		
		
		
	
		ocultar_opciones_avanzadas_pc();
	$( "#motor_price_calendar" ).click(function() {
 	 ocultar_opciones_avanzadas_pc();
	});
	
	});


function ocultar_opciones_avanzadas_pc() {
 if ($('#motor_price_calendar').is(':checked')) {
	  $(".row_horario_salida").hide();
	  $(".row_horario_regreso").hide();
	  $(".row_escalas").hide();
	  $(".row_clase").hide();	  
  } else {	  
	  $(".row_horario_salida").show();
	  $(".row_horario_regreso").show();
	  $(".row_escalas").show();
	  $(".row_clase").show();	  
  }	
}

function solo_ida() {
document.getElementById("motor_multiples_destinos").value=0; // reseteo multidestino
if(document.getElementById('input_solo_ida').checked) {
	document.getElementById('motor_tramos').value=1; // reseteo a 1
	$('.li_idayvuelta').hide('fast');
} else {
	document.getElementById('motor_tramos').value=2;
	$('.li_idayvuelta').show('fast');
    }		
}




$(document).ready(function(){
		$("#m_destinos").on( "click", function() {
			$('.multiples_destinos').show(); //muestro mediante clase
			$('.destino_simple').hide();
			$('.li_idayvuelta').hide();
			
		 });
		$("#ida_v").on( "click", function() {
			$('.multiples_destinos').hide(); //muestro mediante clase
			$('.destino_simple').show();
		});
		
		$("#input_solo_ida").on( "click", function() {
			$('.multiples_destinos').hide(); //muestro mediante clase
			$('.li_idayvuelta').hide();
			$('.destino_simple').show(); 
		});
	});

function multiples_destinos() {
	
if(document.getElementById('motor_multiples_destinos').value==0) {
	document.getElementById('motor_multiples_destinos').value=1;
	$('#motor_price_calendar').attr('checked', false);
	
	if(document.getElementById('motor_tramos').value==1)	
	document.getElementById('motor_tramos').value=2;	
	//alert(document.getElementById('motor_tramos').value);
	
	
	
	document.getElementById('motor_destino_tramo1_salida').value=document.getElementById('motor_destino_salida').value;
	document.getElementById('motor_destino_tramo1_llegada').value=document.getElementById('motor_destino_llegada').value;
	
	document.getElementById('motor_destino_tramo2_salida').value=document.getElementById('motor_destino_llegada').value;
	
	document.getElementById('motor_fecha_1').value=document.getElementById('motor_fecha_salida').value;
	document.getElementById('motor_fecha_2').value=document.getElementById('motor_fecha_llegada').value;

	
	
	
		$('#wb_motor .destino_simple').hide('fast');
		$('#wb_motor .li_idayvuelta').hide('fast');
		$('#wb_motor .opciones_avanzadas').hide('fast');
		$('#wb_motor .boton_opciones_avanzadas').hide('fast');
		$('#wb_motor .multiples_destinos').show('fast');
		
} else {
	document.getElementById('motor_multiples_destinos').value=0;
	$('#wb_motor .multiples_destinos').hide('fast');
	$('#wb_motor .opciones_avanzadas').hide('fast');
	$('.boton_opciones_avanzadas').show('fast');
	$('#wb_motor .destino_simple').show('fast');
	$('#wb_motor .li_idayvuelta').show('fast');		

    }	
	
}




function validar_motor() {

	var motor_tramos=document.getElementById("motor_tramos").value;
	var motor_multiples_destinos=document.getElementById("motor_multiples_destinos").value;
	

		
	/////////////////////////////////////////////////////
	// Valido por ONE WAY	
	/////////////////////////////////////////////////////
	
	if(motor_multiples_destinos==0) {
		
	// Destino Salida
	var valor=$(".aeropuertos:eq(0)").val();	
	

	if(valor) {	
	
	if(valor.search(/\[/)<0) {
		alert("Debe indicar un destino de partida correcto");
		return false;
		var error=1;		
	} 	
	
	
	} else {	
		alert("Debe indicar un destino de partida");
		return false;
		var error=1;			
	}


	// Destino Llegada
	var valor=$(".aeropuertos:eq(1)").val();	
	
	if(valor) {	
						 
	if(valor.search(/\[/)<0) {
		alert("Debe indicar un destino de llegada correcto");
		return false;
		var error=1;		
	}} else {		
		alert("Debe indicar un destino de llegada");
		return false;
		var error=1;			
	}
	
	
	
	// Salida Horario
	if($(".calendar:eq(0)").val().length!=10) {
		alert("Debe indicar una fecha valida de partida");
		return false;
		var error=1;		
	}
	
	
	}
	//////////////////////////
	// FIN VALIDACION ONE WAY
	//////////////////////////
	
	/////////////////////////////////////////////////////
	// Valido Fecha de regreso en caso de ser ROUND TRIP
	/////////////////////////////////////////////////////
	if(motor_multiples_destinos==0 && motor_tramos==2) {		
	// Regreso Horario
	if($(".calendar:eq(1)").val().length!=10) {
		alert("Debe indicar una fecha valida de regreso");
		var error=1;
		return false;			
	}	
	}
	///////////////////////////////
	// FIN VALIDACION ROUND TRIP
	///////////////////////////////
	
	
	
	///////////////////////////////////
	// VALIDACION MULTIPLES DESTINOS
	///////////////////////////////////	
	if(motor_multiples_destinos==1) {
		
	// Actualizo las fechas 0 y 1 con los valores 2 y 3
	$(".calendar:eq(0)").val($(".calendar:eq(2)").val());
	$(".calendar:eq(1)").val($(".calendar:eq(3)").val());
	// Actualizo los destinos 0 y 1 
	$(".aeropuerto_autocomplete_salida:eq(0)").val($(".aeropuerto_autocomplete_salida:eq(1)").val());
	$(".aeropuerto_autocomplete:eq(0)").val($(".aeropuerto_autocomplete:eq(1)").val());
	
		
	//////////////////////////
	// LOOP GENERAL DE TRAMOS
	//////////////////////////
	var loop_destinos=1;
	var loop_fecha=1;
	var loop_tramo=0;
	for (var i=0;i<motor_tramos;i++){
	loop_tramo=loop_tramo+1;
	// Primer destino
	loop_destinos=loop_destinos+1;
	
	var valor=$(".aeropuertos:eq("+loop_destinos+")").val();
	
	if(valor) {	

	if(valor.search(/\[/)<0) {
		alert("Debe indicar un destino de origen correcto para el tramo "+loop_tramo);
		return false;
		var error=1;		
	}} else {		
		alert("Debe indicar un destino de origen para el tramo "+loop_tramo);
		return false;
		var error=1;			
	}
	
	// segundo destino
	loop_destinos=loop_destinos+1;
	var valor=$(".aeropuertos:eq("+loop_destinos+")").val();	
	if(valor) {	

	if(valor.search(/\[/)<0) {
		alert("Debe indicar un destino de origen correcto para el tramo "+loop_tramo);
		return false;
		var error=1;		
	}} else {		
		alert("Debe indicar un destino correcto para el tramo "+loop_tramo);
		return false;
		var error=1;			
	}
	
	
	// Fecha
	loop_fecha=loop_fecha+1;
	if($(".calendar:eq("+loop_fecha+")").val().length!=10) {
		alert("Debe indicar una fecha valida de partida para el tramo "+loop_tramo);
		var error=1;
		return false;			
	}	
	
	
	
	}// FIN LOOP GENERAL TRAMOS
	//////////////////////////
		
	}	
	///////////////////////////////
	// FIN VALIDACION MULTIPLES DESTINOS
	///////////////////////////////
	
	
	///////////////////////////////////
	// VALIDACION DE PAX ADULTO	
	// Valido siempre que seleccione 
	// por lo menos un PAX ADULTO
	///////////////////////////////////
		if(document.getElementById("motor_pax_adultos").value==0) {
				document.getElementById("motor_pax_adultos").focus();
				alert("Debe seleccionar por lo menos un pasajero Adulto");
				var error=1;
				return false;
	// FIN VALIDACION PAX ADULTO
			}
			
	// VALIDO QUE LA CANTIDAD DE BEBES NO SEA MAYOR A LA DE ADULTO
	if(document.getElementById("motor_pax_bebes").value>document.getElementById("motor_pax_adultos").value) {
				document.getElementById("motor_pax_bebes").focus();
				alert("La cantidad de Bebes no puede ser superior a la cantidad de adultos");
				var error=1;
				return false;
	// FIN VALIDACION PAX ADULTO
			}

			
			
	// VALIDO QUE LA CANTIDAD DE PAX NO SEA MAYOR A 9	
	var total_pax=Number(document.getElementById("motor_pax_adultos").value)+Number(document.getElementById("motor_pax_menores").value)+Number(document.getElementById("motor_pax_bebes").value);
	if(total_pax>9) {				
				alert("No se pueden seleccionar mas de 9 pasajeros");
				var error=1;
				return false;
	// FIN VALIDACION PAX ADULTO
			}
	
	
	// SUBMIT FINAL	
	if(error!=1) {
		document.getElementById("formulario_mkte_air_motor").submit();		
	}
			
	
}

</script>


