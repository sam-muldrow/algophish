		$(document).ready(function(){	
			
            $('#tipocurso').change(function(){
                var categoria = $('#tipocurso').val();
                $.get('ajax/list-categorias.php', { cat: categoria}, function(options){
                    $('#programa').html(options);
                });
                $.get('ajax/list-destinos.php', { cat: categoria}, function(options){
                    $('#destino').html(options);
                });				
            });			
			
            $('#programa').change(function(){
                var programa = $('#programa').val();
                $.get('ajax/list-destinos2.php', { cat: programa}, function(options){
                    $('#destino').html(options);
                });
            });				
		});	
	</script>

