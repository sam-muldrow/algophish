	function validacion (formulario)
	{
	  if ((formulario.tipocurso.value=="") && (formulario.programa.value=="") && (formulario.destino.value==""))
	  { 
	    alert("No ha seleccionado ninguna opciÃ³n en el buscador. Puede seleccionar un destino o seleccionar tipo de curso");
	    formulario.tipocurso.focus();
	    return false; 
	  }

	  return true;
	}
