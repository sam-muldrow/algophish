<script language="JavaScript">
<!--
function popup_event1(width,height){
	var win_width = width;
	var win_height = height;
	var win_left = 30 //window.screen.width/2 - win_width/2;
	var win_top = 30 //window.screen.height/2 - win_height/2;
	//var win_left = window.screen.width/2 - win_width/2;
	//var win_top = window.screen.height/2 - win_height/2;

	if(cookieVal1("www.doosongift.com_1") != "done"){
		EventPop1.style.left = win_left;
		EventPop1.style.top = win_top;
		EventPop1.style.display = 'inline';
	}
}
function cookieVal1(name){
	var nameOfCookie = name + "=";
	var x = 0;
	while ( x <= document.cookie.length )
	{
		var y = (x+nameOfCookie.length);
		if ( document.cookie.substring( x, y ) == nameOfCookie ) {
			if ( (endOfCookie=document.cookie.indexOf( ";", y )) == -1 )
				endOfCookie = document.cookie.length;
			return unescape( document.cookie.substring( y, endOfCookie ) );
		}
		x = document.cookie.indexOf( " ", x ) + 1;
		if ( x == 0 )
			break;
	}
	return "";
}
function setCookie1( name, value, expiredays ){
    var todayDate = new Date();
    todayDate.setDate( todayDate.getDate() + expiredays );
        document.cookie = name + "=" + escape( value ) + "; path=/;expires=" + todayDate.toGMTString() + ";"
}

function closeWin1(){
	if ( document.quick_form1.Notice.checked ){   //ÇÏ·ç »ý¼º
			setCookie1( "www.doosongift.com_1", "done" , 1);
	}else{
			setCookie1( "www.doosongift.com_1", "" , 0);
	}
	EventPop1.style.display = 'none';
}
function closeWin_pause1(){
       EventPop1.style.display = 'none';
}
