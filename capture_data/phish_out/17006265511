<script type="text/javascript">
	$(function(){


		$("ul.panel li:not("+$("ul.tab li a.selected").attr("name")+")").hide()

		$("ul.tab li a").mouseover(function(){
			$("ul.tab li a").removeClass("selected");
			$(this).addClass("selected");
			$("ul.panel li").hide();
			$($(this).attr("name")).show();
			return false;
		});

	});
</script>


