<script language="javascript">
   function goHome(){
      var indx;
      sel=document.frmGo.objSel;
      indx=sel.selectedIndex;
      if (indx>0){
         win=window.open("");
         win.target="_blank";
         win.location.href=sel.options[indx].value;
         win.document.close();
      }
   }
</script>









