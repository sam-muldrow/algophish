<script>
        jQuery(document).ready(function () {
            var blog_tab = jQuery('.blog_tab').click(function() {
            var blog_attr = jQuery(this).attr('id');
              var el = jQuery('.' + blog_attr).addClass("active_category");
              jQuery('.blog_lists').not(el).removeClass("active_category");
              blog_tab.removeClass('active_tab');
              jQuery(this).addClass('active_tab');
            });
        });
</script>
<script>
        jQuery(".post-excerpt > p").each(function(index, currentText) {
          var texts=jQuery(this).text().split(" ",18).join(" ");
          jQuery(this).text(texts);
        });
</script>
<script>

        jQuery('.select_title').click(function(){
                jQuery(this).parents('.input_text_show').find('.radio_field').show();
                jQuery('.select_title').not(this).parents('.input_text_show').find('.radio_field').hide();
        });

        jQuery('.radio_field').click(function(){
                jQuery(this).hide();
        });
        jQuery('.radio_field label').click(function(){
                jQuery(this).parents('.input_text_show').find('label').removeClass('active');
                var radio_btn_val = jQuery(this).find('input').val();
                jQuery(this).parents('.input_text_show').find('.select_title').val(radio_btn_val);
                jQuery(this).addClass('active');
                jQuery(this).parents('.input_text_show').addClass('active');
        });

        // If an event gets to the body
        jQuery('body').click(function(){
          jQuery('.radio_field').hide();
        });

        // Prevent events from getting pass .popup
        jQuery('.select_title,.radio_field').click(function(e){
          e.stopPropagation();
        });

        // jQuery(document).on('click','.inputs.files .codedropz-upload-handler',function(){
        //      jQuery('.inputs.files .cd-upload-btn').get(0).click();
        // });

        // jQuery('.inputs input[type="date"]').each(function() {
        //      jQuery(this).attr({onfocus:"(this.type='date')",  onblur:"(this.type='text')", placeholder:""});
        // });
        jQuery(function() {
                jQuery('.input_date input').click(function(){
                        jQuery(this).attr('type','date');
                });
                jQuery('.date_only input').each(function() {
                        var date = new Date();

                        var day = date.getDate();
                        var month = date.getMonth() + 1;
                        var year = date.getFullYear();

                        if (month < 10) month = "0" + month;
                        if (day < 10) day = "0" + day;

                        var today = year + "-" + month + "-" + day;
                        jQuery(this).val(today);
                });

                // jQuery('.input_date input').val() = Date();
        });
        
        jQuery(document).on('change', '.input_boxx input', function(){
                var arr = [];
                i = 0;
                jQuery('.input_boxx input').each(function()
                {
                    arr[i++] = jQuery(this).val();
                    //I should store id in an array
                });
                // console.log(arr);

                var total_hrs = 0;
                for (var j = 0; j < arr.length; j++) {
                    total_hrs += parseFloat(arr[j]) || 0;             
                }

                var totalsum = (total_hrs*231.44).toFixed(2);

                jQuery('#total_hours_box input').val(total_hrs);
                jQuery('#total_hours_costs input').val(totalsum);
        });


        jQuery(document).on("keydown", ".wpcf7-form", function(event) { 
            return event.key != "Enter";
        });

