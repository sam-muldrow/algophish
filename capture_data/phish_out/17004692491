<script>
/* Default comment here */ 
jQuery(document).ready(function( $ ){
  $(window).scroll(function(){
  var scroll = $(window).scrollTop();
  if (scroll > 100){
    $("#sticky-menu").addClass('scrolled-me');
  }else{
    $("#sticky-menu").removeClass('scrolled-me');
  }
})
});
</script>
