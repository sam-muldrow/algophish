					$(".mail-btn").attr("data-fancybox", "");
					$(".phone-btn").removeAttr("href");
					$(".phone-btn").attr("data-fancybox", "");
				} else {
					$(".mail-btn").attr("href", mail);
					$(".mail-btn").attr("target", "_blank");
					$(".mail-btn").removeAttr("data-fancybox");
					$(".phone-btn").attr("href", phone);
					$(".phone-btn").attr("target", "_blank");
					$(".phone-btn").removeAttr("data-fancybox");
				}
			});
			$('.mail-btn').on('click', function() {
				$('.mail-popup').show();
				$('.phone-popup').hide();
			});
			$('.phone-btn').on('click', function() {
				$('.phone-popup').show();
				$('.mail-popup').hide();
			});
		});
	});</script>
