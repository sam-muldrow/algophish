							<script type="rocketlazyloadscript">window.addEventListener('DOMContentLoaded', function() {
								jQuery(document).ready(function($) {
									$(".video").click(function() {
										$(this).find('.video-img').fadeOut();
										$(this).find('.play').fadeOut();
										$(this).find('video').addClass('show');
										$(this).find('.pause').show();
									});

									$(document).on('click', '.video-img', function(e) {
										var video = $(this).siblings('video').get(0);
										if (video.paused === true) {
											video.play();
										}
										return false;
									});

									$(document).on('click', '.play', function(e) {
										var video = $(this).siblings('video').get(0);
										if (video.paused === true) {
											video.play();
										}
										return false;
									});

									$(document).on('click', '.pause', function(e) {
										var video = $(this).siblings('video').get(0);
										if (video.paused === false) {
											video.pause();
											$(this).hide();
											$(this).siblings('.resume').show();
											$(this).siblings('.video-img').hide();
											$(this).siblings('.play').hide();
										}

										return false;
									});

									$(document).on('click', '.resume', function(e) {
										var video = $(this).siblings('video').get(0);
										if (video.paused === true) {
											video.play();
											$(this).hide();
											$(this).siblings('.pause').show();
										}
										return false;
									});
									$(".video video").bind('ended', function(e) {
										console.log('stopped');
										$(this).siblings('.pause').hide();
										$(this).siblings('.play').show();
										$(this).siblings('.video-img').show();
										$(this).removeClass('show');
									});
								});
							});</script>
						</div>
