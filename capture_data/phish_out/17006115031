        function GetTimezoneOffset() {
            var offset = new Date().getTimezoneOffset();
            document.getElementById('hfTimezoneOffset').value = offset;
        }
        function TrimUsername() {
            document.getElementById('txtUsername').value = document.getElementById('txtUsername').value.trim();
            document.getElementById('txtPassword').value = document.getElementById('txtPassword').value.trim();
            
            return true;
        }
    </script>
