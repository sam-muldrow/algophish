<script>
function getId(url){var regExp=/^.*(youtu.be\/|v\/|u\/\w\/|embed\/|watch\?v=|\&v=)([^#\&\?]*).*/;var match=url.match(regExp);if(match&&match[2].length==11){return match[2]}else{return'error'}}
$("oembed").each(function(){var url=$(this).attr('url');var id=getId(url);console.log(id);$(this).replaceWith('<div class="embed-responsive embed-responsive-16by9">  <iframe class="embed-responsive-item" src="//www.youtube.com/embed/'+id+'"></iframe></div>')})
</script>

<script>

$('#modalIMG').modal('show');


$(".search-icon").click(function() {
    if ($(".mobile-nav-dip").hasClass("mobile-nav-dip-b")) {
        $(".mobile-nav-dip").removeClass("mobile-nav-dip-b");
        $(".mobile-nav-dip").removeClass("animated");
        $(".search-icon i").removeClass("fa-times");
        $(".search-icon i").addClass("fa-search")
    } else {
        $(".mobile-nav-dip").addClass("mobile-nav-dip-b");
        $(".mobile-nav-dip").addClass("animated");
        $(".search-icon i").removeClass("fa-search");
        $(".search-icon i").addClass("fa-times")
    }
});
function open_mobile_nav() {
    document.getElementById("mobile-menu").style.width = "100%"
}

function close_mobile_nav() {
    document.getElementById("mobile-menu").style.width = "0"
}
</script>
