<script type="text/javascript">
// Función para igualar el alto de componentes
	function equalHeight(group) { 
  	tallest = 0; 
    group.each(function() { 
    	thisHeight = $(this).height(); 
      	if(thisHeight > tallest) { 
        	tallest = thisHeight; 
        } 
    }); 
    group.height(tallest); 
};       
</script>

