<script language="javascript" type="text/javascript">
function validar() {
	var formulario = document.Formulario;	

	if(esVacio(formulario.mail.value)) {
		alert( "ERROR: debe introducir el E-mail." );
		formulario.mail.focus();
		return false;
	} else if(!validarEmail(formulario.mail.value)) {
		alert( "ERROR: no ha introducido una cuenta de E-mail válida." );
		formulario.mail.focus();
		return false;
	} else if(esVacio(formulario.clave.value)) {
		alert( "ERROR: debe introducir su clave de acceso" );
		formulario.clave.focus();
		return false;
	}
}
</script>

