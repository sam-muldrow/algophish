<script type="text/javascript">
   $(document).ready(function(){
         $('body').append('<a href="javascript:void(0)" id="toTop" class=""><i class="fa fa-chevron-up fa-2x"></i></a>');
         $(window).scroll(function () {
            if ($(this).scrollTop() != 0) {
               $('#toTop').fadeIn();
            } else {
               $('#toTop').fadeOut();
            }
         });
       $('#toTop').click(function(){
           $("html, body").animate({ scrollTop: 0 }, 600);
           return false;
       });
   });

   $(function() {
     $('#ChangeToggle').click(function() {
       $('#navbar-hamburger').toggleClass('hidden');
       $('#navbar-close').toggleClass('hidden');
     });

     $('#stockistModel').on('hidden.bs.modal', function (e) {
        $('#div-stockist').addClass('open');
      });
   });

   function changeLanguage(langauge_id){

      $.ajax({
         type: "GET",
         url: "https://www.balloonaa.com/web/controllers.php?language_id="+langauge_id+"&controller=setLanguageId" ,
         success: function(results){
             location.reload();
         },
         cache: false
      });

    }

   $(function(){

         $('#login-nav').submit(function(){

            $.ajax({
               data: $(this).serialize(),
               type: "post",
               url: "https://www.balloonaa.com//web/login.php?action",
               success: function(json){
                   if(json)
                     if(json.status){
                       window.location = "https://www.balloonaa.com/web/manage_languague.php";
                     }else{
                       alert('à¸à¸±à¸à¸à¸µà¸à¸¹à¹à¹à¸à¹à¸à¸²à¸à¹à¸¡à¹à¸à¸¹à¸à¸à¹à¸­à¸');
                     }
               }
            });

            return false;

         });
   });
</script>
