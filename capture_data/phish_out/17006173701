    <script type="text/javascript">
    $().ready(function () {
        if(!document.cookie.match(/cwarning=/))
            $('#cookie-message').removeClass('hidden_overlay');

        if(!document.cookie.match(/signup_ov_dismissed=/) && !window.location.href.match(/\/user\/register/) && !window.location.href.match(/\/user\/signup/))
        {
            let winH = document.body.clientHeight;
            let startO = (winH / 100) * 10;
            window.addEventListener('scroll', function() {
                if(!$('div#signup-ov').is(":visible") && !$('div#signup-ov').hasClass("overlay_dismissed") && pageYOffset >= startO)
                {
                    $('#signup-ov-bg').fadeIn(100, function() {
                        $('#signup-ov-bg').removeClass('hidden_overlay');
                        $('#signup-ov').removeClass('hidden_overlay');
                    });
                }
            });
        }
    });
</script>
