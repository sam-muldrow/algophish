<script type="text/javascript">
/**
 * Anonimus function
 *
 * This function load selection of procedures.
 */

	jQuery.ajax({
		type: 'POST',
		url: 'https://www.dcorpusinternational.com/wp-admin/admin-ajax.php',
		data: {"action": "surgery_procedures", "lang": "es"},
		success: function(procedures){
			if( procedures.error ){
				alert( procedures.error );
			}
			else {
				jQuery.each(procedures, function(index, value) {
					jQuery('#your-procedures').append("<option value=\"" + value + "\">" + value + "</option>");
				});
			}
		}
	});

</script>
