    $(function(){
        function tabs(tabTit,on,tabCon){
            $(tabTit).children('.item').hover(function(){
                $(this).addClass(on).siblings().removeClass(on);
                var index = $(tabTit).children('.item').index(this);
                $(tabCon).children().eq(index).show().siblings().hide();
               // $(tabCon).children().eq(index).css({"position":"inherit","z-index":"1"}).siblings().css({"position":"absolute","right":"99999px","z-index":"-1"});
            });
        };
        tabs(".az_pro_cate","active",".az_pro_list");
        tabs(".az_news_cat","active",".az_news_list");
    });

    var sUserAgent = navigator.userAgent.toLowerCase();
    if (/ipad|iphone|midp|rv:1.2.3.4|ucweb|android|windows ce|windows mobile/.test(sUserAgent)) {
        //移动端
        $('.honor_swiper').each(function(index, item) {
            new Swiper(item, {
                slidesPerView: 2,
                spaceBetween: 10,
                loop: true, observer: true, observeParents: true,
                autoplay: {
                    delay: 1000,
                    disableOnInteraction: false,
                },
                navigation: {
                    nextEl: $(item).siblings('.honor_next').get(0),
                    prevEl: $(item).siblings('.honor_prev').get(0),
                },
            });
        })
    } else {
        //pc端
        $('.honor_swiper').each(function(index, item) {

            new Swiper(item, {
                slidesPerView: 5,
                spaceBetween: 30,
                loop: true, observer: true, observeParents: true,
                autoplay: {
                    delay: 2000,
                    disableOnInteraction: false,
                },
                navigation: {
                    nextEl: $(item).siblings('.honor_next').get(0),
                    prevEl: $(item).siblings('.honor_prev').get(0),
                },
                breakpoints: {
                    1200: {  //当屏幕宽度大于等于320
                        slidesPerView: 4,
                        spaceBetween: 10
                    },
                    1440: {  //当屏幕宽度大于等于768
