<script type="text/javascript">
jQuery(document).ready(function() {
  // Input CUIT

  jQuery('#cuit').on('input', function(ev) {
    var $this = jQuery(this);
    var maxlength = $this.attr('max').length;
    var value = $this.val();
    if (value && value.length >= maxlength) {
      $this.val(value.substr(0, maxlength));
    }
  });

  // Carga para Slick

  (function() {
      var ventana = jQuery(window).outerWidth();

      if(ventana <= 991) {
            jQuery('.slider-gallery').slick({
                 infinite: false
            });
      }
  })();

  // Analizar CP perteneciente a CABA

  jQuery('body').on('click', 'button[name="calc_shipping"]', function() {
    var ubicacion= jQuery('#calc_shipping_state');
    var cp       = jQuery('#calc_shipping_postcode');

    var minimo   = 1000;
    var maximo   = 1599;

    var lista_cp = [];

    for(var i = minimo; i <= maximo; i++) {
      lista_cp.push(i);
    }

    // Verificar CP numérico

    if(/^\d+$/.test(cp.val()) === false) {
      alert('El código postal ingresado no es válido.');
      return false;
    }

    /* Procesar bloque dependiendo del código de ciudad (C = CABA) */

    if(ubicacion.val().includes('C')) {
      // Código para CABA
      // Verificar CP en rango válido

      if(jQuery.inArray(parseInt(cp.val()), lista_cp) === -1) {
        alert('El código postal ingresado no pertenece a la Ciudad Autónoma de Buenos Aires.');
        return false;
      }
    } else {
      // Código para el resto del país
      // Verificar CP en rango válido

      if(jQuery.inArray(parseInt(cp.val()), lista_cp) !== -1) {
        alert('El código postal ingresado pertenece a la Ciudad Autónoma de Buenos Aires.');
        return false;
      }
    }

    return true;
  });
});
</script>
