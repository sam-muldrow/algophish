<script>
    var base_url='https://www.bdfshopping.com/';

    function product_auto(text_id)
    {
        
        var cat_id= $("#cat_id").val();

        var val= $("#"+text_id).val();

       // alert(val);

        //alert(text_id);
        $( "#"+text_id ).autocomplete({
            source: function(request, response) {
             $.ajax({
             
             url:base_url+'search_product/autocomplete_product',
             data:{'request':$("#"+text_id).val(),cat_id:cat_id},
             dataType: "json",
             type: "POST",
             success: function(data){
             response(data);
            
             
             }
             });
             },


        });


    }
</script>
  
