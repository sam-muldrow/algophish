<!-- Sticky Navbar Script -->
<script>
$('#headerSticky').affix({
	offset: {
		// Add .affix when bottom of .header-info is at top of screen
		top: function () {
			return (this.bottom = $('#header').outerHeight(true))
		}
	}
})
</script>

<script>
if (document.documentMode || /Edge/.test(navigator.userAgent)) { // if IE or Edge
    $('body').on("mousewheel", function () {
        if( !$('body').hasClass('modal-open') ){
            // remove default behavior
            event.preventDefault();
            
            //scroll without smoothing
            var wheelDelta = event.wheelDelta;
            var currentScrollPosition = window.pageYOffset;
            window.scrollTo(0, currentScrollPosition - wheelDelta);
        }
    });
}
</script>

<script>
	$('#headerSticky').on('affixed-top.bs.affix', function() {
		if ( isBreakpoint('lg') || isBreakpoint('xl') )
			$('.ds-sticky-cta--shown').css( { 'top':'0', 'z-index':'900' } );
	});

	$('#headerSticky').on('affixed.bs.affix', function() {
		if ( isBreakpoint('lg') || isBreakpoint('xl') )
			$('.ds-sticky-cta--shown').css('top', $('#headerSticky').outerHeight());
	});

	$(window).scroll(function() {
		if ( isBreakpoint('lg') || isBreakpoint('xl') ) {
			if ( $('#headerSticky').hasClass('affix') )
				$('.ds-sticky-cta--shown').css( { 'top':$('#headerSticky').outerHeight(), 'z-index':'2' } );
			if ( $(window).scrollTop() == 0 ) $('.ds-sticky-cta--fixed').css('top', '-100px');
		} else
			$('.ds-sticky-cta--shown').css( { 'top':'0', 'z-index':'900' } );
			if ( $(window).scrollTop() == 0 ) $('.ds-sticky-cta--fixed').css('top', '-100px');
	});

	$(window).resize(function() {
		if ( isBreakpoint('xs') || isBreakpoint('sm') || isBreakpoint('md') )
			$('.ds-sticky-cta--shown').css( { 'top':'0', 'z-index':'900' } );
	});
</script>



