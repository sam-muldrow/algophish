<script type="text/javascript">
	function DataLayerGA3ID(){
		if(window.utag_data) window.utag_data['ga_ua_id'] = 'undefined';
	}
	DataLayerGA3ID();
	$(window).on("load",function(){
		DataLayerGA3ID();
	});
</script>

