Flatsome.behavior('flatsome-filtering', {
	attach: function (context) {
		jQuery('.has-filtering', context).each(function (index, element) {
			var $element = jQuery(element)
			var $grid = $element.find('.row-pzo').imagesLoaded(function () {
				$grid.isotope({itemSelector: '.row-isotope'})
			})

			$element.find('.filter-nav a').click(function (e) {
				var filterValue = jQuery(this).attr('data-filter')
				$element.find('.filter-nav li.active').removeClass('active')
				$(".filter-nav li.zz").addClass("ac")
				jQuery(this).parent().addClass('active')
				$grid.isotope({filter: filterValue})
				e.preventDefault()
			})
		})
	}
})
//]]></script>
<script>//<![CDATA[
        if ($(".filter-nav li.zz").hasClass("active")) {
            $(".row-pzo .row").css("display", "none")
            $(".row-pzo .row:first-child").css("display", "flex")
        }
 		if ($(".filter-nav li.zz").hasClass("ac")){
            $(".row-pzo .row").addClass("block-flex");
        }
  //]]></script>
