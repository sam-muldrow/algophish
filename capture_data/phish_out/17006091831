            <script>
                jQuery(document).ready(function($){
                var postSlider = $(".aps-247183795");
                    postSlider.owlCarousel({
                        margin:20,
                        loop:true,
                        autoWidth:false,
                        responsiveClass:true,
                        dots:false,
                        autoplay:true,

                        autoplayTimeout: 5000,
                        autoplayHoverPause: false,
                        //dotData:true,
                        //dotsEach:true,
                        slideBy:1,

                        responsive:{
                            0 : {
                                items:1,
                            },
                            500: {
                                items:1,
                            },
                            600 : {

                                items:1,

                            },
                            768:{
                                items:1,

                            },
                            1199:{
                                items:1,

                            }
                        }
                    });

                    // custom navigation button for slider
                    // Go to the next item
                    $('.aps-next-1427242540').click(function() {
                        postSlider.trigger('next.owl.carousel');
                    });
                        // Go to the previous item
                    $('.aps-prev-1427242540').click(function() {
                        // With optional speed parameter
                        // Parameters has to be in square bracket '[]'
                        postSlider.trigger('prev.owl.carousel');
                    });

                    // stop on hover but play after hover out
                                            postSlider.hover(
                            function(){
                                postSlider.trigger('stop.owl.autoplay');
                            },
                            function(){
                                postSlider.trigger('play.owl.autoplay');
                            }
                        );
                    

                });
            </script>
