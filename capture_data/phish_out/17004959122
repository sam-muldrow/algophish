                    }
                    if (animateEffect2 == 4) {
                        jQuery(".RW_Im_An_Sl_RAnim" + animateEffect2).css("top", "0%");
                    }
                    zIndex2++;
                    jQuery('.RW_Title_Ef2' + count2)
                        .slideUp(anim_over2);
                    count2--;
                    if (count2 == 0) {
                        count2 = CountShort2;
                    }
                    prevAnim2();
                }
                function next2() {
                    if (y_y2 == true) {
                        return;
                    }
                    if (animateEffect2 == 4) {
                        jQuery(".RW_Im_An_Sl_RAnim" + animateEffect2).css("top",
                            "100%");
                    }
                    zIndex2++;
                    jQuery('.RW_Title_Ef2' + count2)
                        .slideUp(anim_over2);
                    count2++;
                    if (count2 == CountShort2 + 1) {
                        count2 = 1;
                    }
                    nextAnim2();
                }
                function delVideo() {
                    document.querySelector(".rw_delIc_animation").style.display = "none";
                    document.querySelector(".cd-slider-controls").classList.remove("cd-slider-controls_anim");
                    document.querySelector(".RW_Left_Anim_Sl").style.display = "block";
                    document.querySelector(".RW_Right_Anim_Sl").style.display = "block";
                    var icons = document.querySelectorAll(".plIc_animation");
                    var videos = document.querySelectorAll(".rw_animation_video");
                    for (var i = 0; i < icons.length; i++) {
                        icons[i].style.display = "block";
                    }
                    for (var i = 0; i < videos.length; i++) {
                        videos[i].style.display = "none";
                        videos[i].setAttribute("src", "");
                    }
                }
                document.onkeydown = checkKey;
                function checkKey(e) {
                    e = e || window.event;
                    if (e.keyCode == '37') {
                        prev2();
                        delVideo()
                    } else if (e.keyCode == '39') {
                        next2()
                        delVideo()
                    }
                }
                document.querySelector('.cd-slider2').addEventListener('touchstart',
                    handleTouchStart, false);
                document.querySelector('.cd-slider2').addEventListener('touchmove',
                    handleTouchMove, false);
                var xDown = null;
                var yDown = null;
                function handleTouchStart(evt) {
                    xDown = evt.touches[0].clientX;
                    yDown = evt.touches[0].clientY;
                };
                function handleTouchMove(evt) {
                    if (!xDown || !yDown) {
                        return;
                    }
                    var xUp = evt.touches[0].clientX;
                    var yUp = evt.touches[0].clientY;
                    var xDiff = xDown - xUp;
                    var yDiff = yDown - yUp;
                    if (Math.abs(xDiff) > Math.abs(yDiff)) {
                        /*most significant*/
                        if (xDiff > 0) {
                            prev2();
                        } else {
                            next2()
                        }
                    } else {
                        if (yDiff > 0) {
                            /* up swipe */
                        } else {
                            /* down swipe */
                        }
                    }
                    /* reset values */
                    xDown = null;
                    yDown = null;
                };
                var Interval2;
                if (Rich_Web_AnSL_SSh2 == "true") {
                    Interval2 = setInterval(function() {
                            next2();
                        }, (Rich_Web_AnSL_SShChT2 / 10) *
                        CountShort2 * 2)
                    jQuery(".RW_CL_N2").bind("click", function() {
                        next2();
                    })
                    jQuery(".RW_CL_P2").bind("click", function() {
                        prev2();
                    })
                    jQuery(".cd-slider-wrapper2").mouseout(function() {
                        Interval2 = setInterval(function() {
                                next2();
                            }, (Rich_Web_AnSL_SShChT2 / 10) *
                            CountShort2 * 2)
                    }).mouseover(function() {
                        clearInterval(Interval2);
                    })
                } else {
                    jQuery(".RW_CL_N2").bind("click", function() {
                        next2();
                    })
                    jQuery(".RW_CL_P2").bind("click", function() {
                        prev2();
                    })
                }
                jQuery(".RW_Thumb2").click(function() {
                    count2 = parseInt(jQuery(this).attr("name"));
                    if (count2 != thumbCount2) {
                        jQuery('.RW_Title2').slideUp(500);
                    }
                    zIndex2++;
                    if (count2 > thumbCount2) {
                        if (animateEffect2 == 4) {
                            jQuery(".RW_Im_An_Sl_RAnim" + animateEffect2).css(
                                "top", "100%");
                        }
                        nextAnim2();
                    } else if (count2 <
                        thumbCount2) {
                        if (animateEffect2 == 4) {
                            jQuery(".RW_Im_An_Sl_RAnim" + animateEffect2).css(
                                "top", "0%");
                        }
                        prevAnim2();
                    }
                    thumbCount2 = count2;
                    y_y2 = true;
                    setTimeout(function() {
                        y_y2 = false;
                    }, 500)
                })
                resp2();
            }
        })
    }
</script>
<script type="text/javascript">
    function rw_anim_video_cl2(vSrc, el) {
        el.children[0].style.display = "block";
        el.children[0].setAttribute("src", vSrc + "?rel=0&amp;autoplay=1");
        el.children[1].style.display = "none";
        var titles = document.querySelectorAll(".RW_Title");
        for (var i = 0; i < titles.length; i++) {
            titles[i].style.display = "none";
        }
        document.querySelector(".cd-slider-controls").classList.add("cd-slider-controls_anim");
        document.querySelector(".RW_Left_Anim_Sl").style.display = "none";
        document.querySelector(".RW_Right_Anim_Sl").style.display = "none";
        setTimeout(function() {
            document.querySelector(".rw_delIc_animation").style.display = "block";
        }, 1000)
    }
    document.querySelector(".rw_delIc_animation").onclick = function() {
        document.querySelector(".rw_delIc_animation").style.display = "none";
        document.querySelector(".cd-slider-controls").classList.remove("cd-slider-controls_anim");
        document.querySelector(".RW_Left_Anim_Sl").style.display = "block";
        document.querySelector(".RW_Right_Anim_Sl").style.display = "block";
