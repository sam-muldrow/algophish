    jQuery(document).ready(function() {
        var array_dynamic1 = [];
        jQuery(".rw_img_dynamic1").each(function() {
            if (jQuery(this).attr("rel") != "") {
                array_dynamic1.push(jQuery(this).attr("rel"));
            }
        })
        var y_dynamic1 = 0;
        for (i = 0; i < array_dynamic1.length; i++) {
            jQuery("<img class='rw_img_dynamic1' />").attr('src',
                array_dynamic1[i]).on("load", function() {
                y_dynamic1++;
                if (y_dynamic1 == array_dynamic1                    .length) {
                    jQuery(".rSlider_1").fadeIn(1000);
                    jQuery("#RW_Load_Content_Navigation1")
                        .remove();
                }
            })
        }
    })
