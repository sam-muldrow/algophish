<!-- Get Courses -->



<script type="text/javascript">



function getCity(state_id){



$.post("getcourses.php",{stateid:state_id}, function(result){



//alert(result);return false;

var cityHTML = "";

var jsonresult = $.parseJSON(result);

if(parseInt(jsonresult.error) == 1){

alert(jsonresult.msg);

return false;

}

if(jsonresult.msg.length > 0){

cityHTML += "";



for(var i = 0; i < jsonresult.msg.length; i++){                            

cityHTML += '<input style="color:#fff; margin:0px 5px 0px 2px;" name="sub_course[]" type="checkbox" value="'+jsonresult.msg[i].course_category_name+'"><span style="color:#fff; font-size:15px; margin:0px 3px 0px 3px;">'+jsonresult.msg[i].course_category_name+'</span>';                            

}

}else{

cityHTML += '<b style="#fff; font-weight:bold;">No Sub Courses Found Of This Course. Try Later.</b>';

}



$("#stateDivEnq").html(cityHTML);

return false;



});



}



</script>
<script type="text/javascript">
    var flag = 1;
    window.onload = function () {
        OpenBootstrapPopup();
    };
    function OpenBootstrapPopup() {
        //$("#simpleModal").modal('show');
        setTimeout(function() {
            $('#simpleModal').modal('hide');
            if (flag) {
              $('#simpleModal').modal('show');
            } else {
              $('#simpleModal').modal('show')
            }
        }, 1000);
    }
</script>

