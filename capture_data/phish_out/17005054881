<script>

function echeck(str) 

{

	var at="@"

	var dot="."

	var lat=str.indexOf(at)

	var lstr=str.length

	var ldot=str.indexOf(dot)

	if (str.indexOf(at)==-1){

	  alert("Enter email id   e.g  example@gmail.com ")

	   return false

	}



	if (str.indexOf(at)==-1 || str.indexOf(at)==0 || str.indexOf(at)==lstr){

	  alert("Enter Email ID   e.g  example@gmail.com ")

	   return false

	}



	if (str.indexOf(dot)==-1 || str.indexOf(dot)==0 || str.indexOf(dot)==lstr){

	    alert("Enter Email ID   e.g  example@gmail.com ")

	    return false

    }



    if (str.indexOf(at,(lat+1))!=-1){

	    alert("Enter Email ID   e.g  example@gmail.com ")

	    return false

    }



	 if (str.substring(lat-1,lat)==dot || str.substring(lat+1,lat+2)==dot){

	    alert("Enter Email ID   e.g  example@gmail.com ")

	    return false

	 }



	 if (str.indexOf(dot,(lat+2))==-1){

	    alert("Enter Email ID   e.g  example@gmail.com ")

	    return false

	 }

	

	 if (str.indexOf(" ")!=-1){

	    alert("Enter Email ID   e.g  example@gmail.com ")

	    return false

	 }



 	 return true					

}



function pak()

{

	pa=document.deep;

	if(pa.name.value=="")

	{

		alert("Please Enter Your Name");

		pa.name.focus();

		return false;

	}

		

    if ((pa.email.value==null)||(pa.email.value==""))

    {

        alert("Please Enter Valid Email")

        pa.email.focus()

        return false

    }

	

	if (echeck(pa.email.value)==false)

	{

		pa.email.value=""

		pa.email.focus()

		return false

	}

	

	if(pa.phone.value=="")

	{

		alert("Please Enter Your Mobile")

		pa.phone.focus();

		return false;

	}

	

    if(isNaN(pa.phone.value))

    {

        alert("Please Enter Digit In Your Phone");

        pa.phone.focus();

        return false;

    }



    if((pa.phone.value.length)<10)

    {

        alert("Your Phone Number Should Be 10 Digit");

        pa.phone.focus();

        return false;                                             

    }

   

    if((pa.phone.value.length)>10)

    {

        alert("Your Phone Number Should Be 10 Digit");

        pa.phone.focus();

        return false;

    }

	

	if(pa.center.value=="")

	{

		alert("Please Choose Your Center")

		pa.center.focus();

		return false;

	}

	

	if(pa.course.value=="")

	{

		alert("Please Choose Your Course")

		pa.course.focus();

		return false;

	}

	

	if(pa.sub_course.value=="")

	{

		alert("Please Select Atleast One Course")

		pa.sub_course.focus();

		return false;

	}

  

	return true;

}

