	<script>
function makeRolloverImages()
{
	$(function() {
		$('.rollover-images').each(function()
		{
			var newSrc = $(this).data('rollover');
			if(newSrc == 0) return;
	
			var oldSrc;
			$(this).hover(function()
			{
				oldSrc = $(this).attr('src');
				$(this).attr('src', newSrc).stop(true,true).hide().fadeIn(1000);
			}, function()
			{
				$(this).attr('src', oldSrc).stop(true,true).hide().fadeIn(1000);
			});
		});
	});	
}
makeRolloverImages();
</script>







