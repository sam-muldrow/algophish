    <!-- For doctors slide -->
             
        <script type="text/javascript" src="https://www.cardiologypartnerspl.com/resources/assets/js/index.min.js"></script>
        <script>
            // doctors slider
            $( document ).ready(function() {
                $('#projectslider').carousel({ interval: 2000 });
                $('.projectslider .item').each(function(){
                    var itemToClone = $(this);
                    let maxlength = 4;
                    let windowWidth = $( window ).width();
                    if( windowWidth <= 950 && windowWidth >= 757){
                        maxlength = 2;
                    } else if(windowWidth <= 756){
                        maxlength = 1;
                    }
                    for (var i=1;i<maxlength;i++) {
                        itemToClone = itemToClone.next();
                        if (!itemToClone.length) {
                            itemToClone = $(this).siblings(':first');
                        }
                        itemToClone.children(':first-child').clone()
                        .addClass("cloneditem-"+(i))
                        .appendTo($(this));
                    }
                });
            });	
        </script> 
    

