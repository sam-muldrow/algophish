<script>
	var p_hour = 9;
  var p_minutes = '00';
	var d_hour = 9;
  var d_minutes = '00';
	
  var p_time = 900;
	var d_time = 900;

// 	pick up hours
	jQuery('select[name="pickuph"]').change(function () {
      p_hour = jQuery('select[name="pickuph"] option:selected').val();
      checkHour();
     });
  
  jQuery('select[name="pickupm"]').change(function () {
    p_minutes = jQuery('select[name="pickupm"] option:selected').text();
    checkHour();
    });
	
	jQuery('select[name="releaseh"]').change(function () {
		d_hour = jQuery('select[name="releaseh"] option:selected').val();
		checkHour();
	});

  jQuery('select[name="releasem"]').change(function () {
    d_minutes = jQuery('select[name="releasem"] option:selected').text();
    checkHour();
    });
	
	function checkHour() {
    p_time = p_hour + p_minutes;
		d_time = d_hour + d_minutes;
    if(((p_time < 900) || (p_time > 2100)) || ((d_time < 900) || (d_time > 2100)) ){
			if(((p_time < 900) || (p_time > 2100)) && ((d_time < 900) || (d_time > 2100))){
				 jQuery('#result-pickuptime').html('*Supplier will add extra fee '+'<b>'+'80'+'</b>'+' , out of hours office (opening time 9:00 - 21:00). Read more in ').append( '<a href="https://carandgo.gr/rental-terms">Rental Terms</a>' ); 
				 }else{
					 jQuery('#result-pickuptime').html('*Supplier will add extra fee '+'<b>'+'40'+'</b>'+' , out of hours office (opening time 9:00 - 21:00). Read more in ').append( '<a href="https://carandgo.gr/rental-terms">Rental Terms</a>' ); 
				 }
    }else{
			jQuery('#result-pickuptime').html(''); 
		}
  }
</script>
<script>

// 	pick up drop off out of place
// 	

	var option_text = 'Heraklion Airport';
	var option_text_return = 'Heraklion Airport';
	var pickup_state = 0;
	var drop_state = 0;

	jQuery('#modplace').change(function () {
      option_text = jQuery('#modplace option:selected').text();
			if( option_text === 'Heraklion Airport'){
				pickup_state = 0;
				checkPlace();
			}
			if( (option_text !== 'Heraklion Airport') && (option_text !== 'Chania Airport') && (option_text !== 'Chania Port') && (option_text !== 'Chania City') ){
				pickup_state = 2;
				checkPlace();
			}
			if((option_text === 'Chania Airport') || (option_text === 'Chania Port') || (option_text === 'Chania City')){
				pickup_state = 1;
				checkPlace();
			}
      });
	
  jQuery('#modreturnplace').change(function () {
    option_text_return = jQuery('#modreturnplace option:selected').text();
			if( option_text_return === 'Heraklion Airport'){
				drop_state = 0;
				checkPlace();
			}
			if( (option_text_return !== 'Heraklion Airport') && (option_text_return !== 'Chania Airport') && (option_text_return !== 'Chania Port') && (option_text_return !== 'Chania City') ){
				drop_state = 2;
				checkPlace();
			}
			if((option_text_return === 'Chania Airport') || (option_text_return === 'Chania Port') || (option_text_return === 'Chania City')){
				drop_state = 1;
				checkPlace();
			}
      });
		
		function checkPlace(){
			if((pickup_state === 0) && (drop_state === 0)){
				jQuery('#result-pickup-place').text(''); // Heraklion Airport
			}else{
				if((pickup_state === 1) && (drop_state === 0) || (pickup_state === 0) && (drop_state === 1)){
				jQuery('#result-pickup-place').html('*Supplier will add extra fee '+'<b>'+'70'+'</b>'+' , out of office. Read more in ').append( '<a href="https://carandgo.gr/rental-terms">Rental Terms</a>' ); 
				}
				if((pickup_state === 2) && (drop_state === 0) || (pickup_state === 0) && (drop_state === 2) ){
					jQuery('#result-pickup-place').html('*Supplier will add extra fee '+'<b>'+'50'+'</b>'+' , out of office. Read more in ').append( '<a href="https://carandgo.gr/rental-terms">Rental Terms</a>' ); 
				}
				if((pickup_state === 2) && (drop_state === 2)){
					jQuery('#result-pickup-place').html('*Supplier will add extra fee '+'<b>'+'100'+'</b>'+' , out of office. Read more in ').append( '<a href="https://carandgo.gr/rental-terms">Rental Terms</a>' ); 
				}
				if((pickup_state === 1) && (drop_state === 1)){
					jQuery('#result-pickup-place').html('*Supplier will add extra fee '+'<b>'+'140'+'</b>'+' , out of office. Read more in ').append( '<a href="https://carandgo.gr/rental-terms">Rental Terms</a>' ); 
				}
				if((pickup_state === 1) && (drop_state === 2) || (pickup_state === 2) && (drop_state === 1)){
					jQuery('#result-pickup-place').html('*Supplier will add extra fee '+'<b>'+'130'+'</b>'+' , out of office. Read more in ').append( '<a href="https://carandgo.gr/rental-terms">Rental Terms</a>' ); 
				}
			}
		}

	</script>
	</body>
</html>
