    <!-- Scripts -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
    
    <script type="text/javascript">
        $(document).ready(function(){
            
            // Slick slider
            $('.autoplay').slick({
                slidesToShow: 4.5,
                slidesToScroll: 1,
                autoplay: true,
                autoplaySpeed: 2000,
                arrows: false,
                responsive: [
                    {
                        breakpoint: 1024,
                        settings: {
                            slidesToShow: 4.5,
                            slidesToScroll: 1,
                            infinite: true,
                            dots: true
                        }
                    },
                    {
                    breakpoint: 768,
                        settings: {
                            arrows: false,
                            centerMode: true,
                            centerPadding: '40px',
                            slidesToShow: 3
                        }
                    },
                    {
                    breakpoint: 480,
                        settings: {
                            arrows: false,
                            centerMode: true,
                            centerPadding: '40px',
                            slidesToShow: 1
                        }
                    }
                ]
            });
            
    
            // Dark Theme
            var Logo = './images/logo.png'
            var darkLogo = './images/logo-white.png'
            $('.nav-item.moon').click(function(){
                $('body,section,div,nav,li:not(.carousel-indicators li),a*,a *').addClass("dark")
                $('section#main .big-news .carousel-indicators li span').css('color','#222')
                $('section#main .big-news .carousel-caption,.carousel-caption h3').removeClass('dark')
                $('.navbar-brand>img, .logo-footer').attr('src',darkLogo)
            });
            $('.nav-item.sun').click(function(){
                $('*').removeClass('dark')
                $('.navbar-brand>img, .logo-footer').attr('src',Logo)
            });
            
            // dropdown nav
            $('#category-nav .dropdown-menu').hover(function(){
                $(this).siblings('a').addClass('current')
            },function(){
                $(this).siblings('a').removeClass('current')
            });

            // toggle search input
            $('.nav-link.search').click(function(){
                $('form#search .form-group').removeClass("d-none")
                $(this).siblings(".close").removeClass("d-none")
                $(this).addClass("d-none")
            });
            $('.nav-link.close').click(function(){
                $('form#search .form-group').addClass("d-none")
                $(this).addClass("d-none")
                $(this).siblings(".search").removeClass("d-none")
            });
    
            // Sticky Navbar
            window.onscroll = function() {addStick()};
            var navbar = document.getElementById("category-nav");
            var sticky = navbar.offsetTop;
            function addStick() {
                if (window.pageYOffset >= sticky) {
                    navbar.classList.add("sticky")
                } else {
                    navbar.classList.remove("sticky");
                }
            }
        });
    </script>
    
