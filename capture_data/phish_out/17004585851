        $(function () {
            $('#datetimepicker1').datetimepicker({minDate: new Date(), format: 'YYYY/MM/DD'});

            $('[name="filter_arrival"]').on('blur',function(){
                if($('#datetimepicker2').datetimepicker()){
                    $('#datetimepicker2').datetimepicker('destroy');
                }
                var filter_arrival=$(this).val();
                $('#datetimepicker2').datetimepicker({minDate: new Date(filter_arrival), format: 'YYYY/MM/DD'});
            })
        });
    </script>
