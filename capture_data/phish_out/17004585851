  function checkAvailability(){
    var filter_category_id = $('select[name=\'category_id\']').val();
    var filter_arrival = $('input[name=\'filter_arrival\']').val();
    var filter_departure = $('input[name=\'filter_departure\']').val();
    var filter_adults = $('select[name=\'filter_adults\']').val();
    var filter_children = $('select[name=\'filter_children\']').val();
    var url = '?';
    url += 'category_id=' + encodeURIComponent(filter_category_id);
    url += '&arrival=' + encodeURIComponent(filter_arrival);
    url += '&departure=' + encodeURIComponent(filter_departure);
    url += '&adults=' + encodeURIComponent(filter_adults);
    url += '&children=' + encodeURIComponent(filter_children);

    location = "http://aurorabyglenwood.com/book" + url;
  }
  
function datediff(){
	var fromDate = $('input[name=\'arrival\']').val();
  	var toDate = $('input[name=\'departure\']').val();
	from = moment($.trim(fromDate), 'YYYY-MM-DD'); // format in which you have the date
	to = moment($.trim(toDate), 'YYYY-MM-DD');     // format in which you have the date
  
	/* using diff */
	duration = to.diff(from, 'days');
	if (duration==0){
		return '1';
	} else {
		return duration;
	}
}

  function start_booking(product_id, product_title, step){
      $('#start_booking').attr("data-step", step);
      $(".detail-room-"+product_id).modal('hide');
      $('body').css('padding-right',0);

      $('input[name=\'arrival\']').val('');
      $('input[name=\'departure\']').val('');
      $('#bedroom_text').html('0');
      $('input[name=\'bedroom_value\']').val('0');

      $('#day_stay').empty();
      $('#rate_text').html('0');
      $('input[name=\'rate_value\']').val('0');
      $('select[name=\'adults\']').val('0');
      $('select[name=\'children\']').val('0');
      $('select[name=\'optUnit\']').val('0');

      $('input[name=\'available\']').val('0');
      $('input[name=\'booked\']').val('0');
	  
	  	$('#amount_text').html('0');
		$('input[name=\'amount_value\']').val('0');

      $('#booking_form').find('.error').remove();

      $.ajax({
        url: 'http://aurorabyglenwood.com/titles-data',
        type: 'post',
        data: {_token: 'UeYKXTrhRdZ6c6M6e7KyL2xmJMNhPjAtJuSVYGti', category_id: $('select[name=\'categories\']').val()},
        dataType: 'json',
        beforeSend:function(){
          
        },
        complete:function(){

        },
        success: function (json) {
          html = '<option value=""></option>';
          if (json['total']>0) {
              console.log(json['total']);
              
              $('#book_title').html(product_title);  

              for (i = 0; i < json['data'].length; i++) {
                if (product_id==json['data'][i]['id']){
                  html = '<option value="' + json['data'][i]['id'] + '"';
                  html += '>' + json['data'][i]['title'] + '</option>';
                  break;
                } else {
                  html += '<option value="' + json['data'][i]['id'] + '"';
                  html += '>' + json['data'][i]['title'] + '</option>';    
                }
              }
              //console.log(json);  
          }

          $('select[name=\'title\']').html(html);

          if (json['total']>0){
            $("select[name=\'title\']").change();
          }
        }
    });
  }


  hs.graphicsDir = 'http://aurorabyglenwood.com/plugins/highslide/graphics/';
  hs.align = 'center';
  hs.transitions = ['expand', 'crossfade'];
  hs.wrapperClassName = 'dark borderless floating-caption';
  hs.fadeInOut = true;
  hs.dimmingOpacity = .75;

  // Add the controlbar
  if (hs.addSlideshow) hs.addSlideshow({
    //slideshowGroup: 'group1',
    interval: 5000,
    repeat: false,
    useControls: true,
    fixedControls: 'fit',
    overlayOptions: {
      opacity: .6,
      position: 'bottom center',
      hideOnMouseOut: true
    }
  });


    $('.hn-scroll-box').mCustomScrollbar({
     scrollButtons:{enable:true,scrollType:"stepped"},
     keyboard:{scrollType:"stepped"},
     mouseWheel:{scrollAmount:188,normalizeDelta:true},
     theme:"rounded-dark",
     autoExpandScrollbar:true,
     snapAmount:188,
     scrollInertia:100,
     snapOffset:65
    });

</script>
</body>
</html>