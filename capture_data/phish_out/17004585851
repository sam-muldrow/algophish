<script type="text/javascript">
  $(document).ready(function(){
    $('.dropdown-menu .properties li').on("mouseover", function(event){
      var myindex =  $(this).index();
      $('.dropdown-menu .properties-image li').hide();
      $('.dropdown-menu .properties li').removeClass('active');
      $(this).addClass('active');
      $('.dropdown-menu .properties-image li').eq(myindex).show();
    });

    // if($( window ).width() < 992 ) {

    //   $('.navbar-toggle').on( "click", function() {
    //     $('#bs-example-navbar-collapse-1').height('auto');
    //   });
    // }
  });
</script>


