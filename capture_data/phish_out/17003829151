.wpcf7 select {  
    background-color: #fff;
    border: 1px solid #ccc;
    border-radius: 3px;
    margin: 0;
    padding: 4px;
    text-indent: 0px;
    width: 100%;
    z-index: 100;
}
/* Make Contact Form 7 text field full width & responsive */
.wpcf7-textarea { 
	width: 100%;
} 

/* Make Contact Form 7 text field full width & responsive */
.wpcf7-text { 
	width: 100%;
} 

