    slideMargin: 15,
    autoHover: true,
    pause: 4000,
    speed: 500,
    auto: false,
    touchEnabled: false,
    touchEnabled: false
  });

  } else if (windowsize <= 550) {
    var bx_similar = $('#similar_products .related-products.in-tabs-in-carousels ul.productGrid.productGrid--maxCol5').bxSlider({
    slideWidth: 275,
    minSlides: 2,
    maxSlides: 15,
    moveSlides: 3,
    slideMargin: 15,
