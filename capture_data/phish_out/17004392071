<!-- RECENT POSTS FEED HOMEPAGE -->
<script>
setTimeout(function() {
waitForElement(".recent-posts-slider img",function(){
var windowsize = $(window).width();
    if (windowsize <= 800 && windowsize > 550) {
    		$('.recent-posts-slider').bxSlider({
    			slideWidth: 400,
    			minSlides: 2,
    			maxSlides: 2,
    			slideMargin: 10,
    			autoHover: true,
    			pause: 4000,
    			speed: 500,
    			auto: false,
          touchEnabled: false
    });

  } else if (windowsize <= 550) {
      $('.recent-posts-slider').bxSlider({
        slideWidth: 550,
        minSlides: 1,
        maxSlides: 1,
        slideMargin: 10,
        autoHover: true,
        pause: 4000,
        speed: 500,
        auto: false,
        touchEnabled: false
  });

} else {
  $('.recent-posts-slider').bxSlider({
  	slideWidth: 480,
  	minSlides: 3,
  	maxSlides: 3,
  	slideMargin: 15,
  	autoHover: true,
  	pause: 4000,
  	speed: 500,
  	auto: false,
    touchEnabled: false
  });
}
});
}, 1000);
</script>

