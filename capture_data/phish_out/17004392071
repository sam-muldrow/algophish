$( ".brands span.down-arrow" ).on ('click', function() {
	$('.brands-list').toggleClass('active');
	if ( $(".brands-list").hasClass("active") ) {
			$(".brands-list").slideDown();
	} else {
		$(".brands-list").slideUp();
	}
});
</script>

