<script>
// 5 COLUMN CAROUSELS IN TABS
var windowsize = $(window).width();
if (windowsize <= 800 && windowsize > 550) {
    var bx_similar = $('#similar_products .related-products.in-tabs-in-carousels ul.productGrid.productGrid--maxCol5').bxSlider({
    slideWidth: 200,
    minSlides: 4,
    maxSlides: 15,
    moveSlides: 3,
    slideMargin: 15,
    autoHover: true,
    pause: 4000,
    speed: 500,
    auto: false,
