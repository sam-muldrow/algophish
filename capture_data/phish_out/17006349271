<script>
    function defer(method) {
        if (window.jQuery) {
            method();
        } else {
            setTimeout(function () { defer(method) }, 50);
        }
    }

    defer(function () {

        var owl = $('.owl-carousel');

        owl.each(function (i) {
            $(this).attr('id', i + 1);
            var item_count = parseInt($(this).find('.carousel__slide').length);
            var true_false = false;
            if (item_count > 1) {
                true_false = true;
            } else {
                true_false = false;
            }

            $(this).owlCarousel({
                items: 1,
                margin: 0,
                loop: true_false,
                dots: true_false,
                //nav: true_false,
                touchDrag: true_false,
                mouseDrag: true_false,
                lazyLoad: true,
                autoplay: true_false, 
            });
        })

    });
</script>


        



                            
    
        
               



