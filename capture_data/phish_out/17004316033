<script src="https://www.anoregmt.org.br/novo/wp-content/themes/anoregmt-portal/vendor/jquery/jquery-3.2.1.min.js"></script>

<script src="https://www.anoregmt.org.br/novo/wp-content/themes/anoregmt-portal/vendor/animsition/js/animsition.min.js"></script>

<script src="https://www.anoregmt.org.br/novo/wp-content/themes/anoregmt-portal/vendor/bootstrap/js/popper.js"></script>
<script src="https://www.anoregmt.org.br/novo/wp-content/themes/anoregmt-portal/vendor/bootstrap/js/bootstrap.min.js"></script>

<script src="https://www.anoregmt.org.br/novo/wp-content/themes/anoregmt-portal/vendor/owlcarousel/owl.carousel.min.js"></script>

<script src="https://www.anoregmt.org.br/novo/wp-content/themes/anoregmt-portal/js/mask.js"></script>

<script src="https://www.anoregmt.org.br/novo/wp-content/themes/anoregmt-portal/js/jquery.maskMoney.js"></script>

<script src="https://www.anoregmt.org.br/novo/wp-content/themes/anoregmt-portal/js/main.js"></script>
<script>
$.noConflict();
jQuery( document ).ready(function( $ ) {
	
	 $("#Valor").change(function(){
        var associado = $(this).children("option:selected").attr("data-ref");
		if(associado=="s") {
		$("#box-associado").collapse('show');	
		$("#cns").attr("required", true);
		$("#nome-responsavel").attr("required", true);
		$("#cpf-responsavel").attr("required", true);
		} 
		
		if(associado=="n") {
		$("#box-associado").collapse('hide');	
		$("#cns").attr("required", false);
		$("#nome-responsavel").attr("required", false);
		$("#cpf-responsavel").attr("required", false);
			
		}
        //alert(associado);
    });
	
	
	$('#cns').blur(function(){

           /* Configura a requisição AJAX */
           $.ajax({

                url : '/novo/ajax-cartorio.php', /* URL que será chamada */ 

                type : 'POST', /* Tipo da requisição */ 

                data: 'CNJ=' + $('#cns').val(), /* dado que será enviado via POST */

                dataType: 'json', /* Tipo de transmissão */

                success: function(data){

                 $('#RazaoSocial').val(data.RazaoSocial);
				$('#Associado').val(data.Associado);
				
               if(data.Associado=="S") {
                $('#RazaoSocial').val(data.RazaoSocial);
               $('#nome-cartorio').html("<p><span style='color:#060'><strong>Cartório Associado: <br> "+data.RazaoSocial+"</strong></span></p>");
			   $("#box-dados-responsavel").collapse('show');	
			   

				}
				
				 if(data.Associado=="N") {
                $('#RazaoSocial').val(data.RazaoSocial);
                $('#nome-cartorio').html("<p><span style='color:#F00'><strong>Cartório Não Associado, por gentileza escolha outra forma de inscrição.</strong></span></p>");
				 $("#box-dados-responsavel").collapse('hide');	

				}
                   

                }

           });   

   return false;    

   });

	
   /* Executa a requisição quando o campo CEP perder o foco */

   $('#cep').blur(function(){

           /* Configura a requisição AJAX */
           $.ajax({

                url : '/gea/includes/consultar_cep.php', /* URL que será chamada */ 

                type : 'POST', /* Tipo da requisição */ 

                data: 'cep=' + $('#cep').val(), /* dado que será enviado via POST */

                dataType: 'json', /* Tipo de transmissão */

                success: function(data){

                    if(data.sucesso == 1){

						var nome_endereco = data.rua+', '+data.bairro;

                        $('#endereco').val(nome_endereco);

                        $('#cidade').val(data.cidade);

                       
					   estado = data.estado.toLowerCase();
					   
					   //alert(estado);
                        $('#estado').val(estado);						

						

                    }

                }

           });   

   return false;    

   });

   

   

   $('#cpf').blur(function(){

           /* Configura a requisição AJAX */
         ID_PAL =  $('#ID_PAL').val();

           $.ajax({

                url : '/gea/ajax.php?id='+ID_PAL, /* URL que será chamada */ 

                type : 'POST', /* Tipo da requisição */ 

                data: 'CPF=' + $('#cpf').val(), /* dado que será enviado via POST */

                dataType: 'json', /* Tipo de transmissão */

                success: function(data){

                    if(data.sucesso > 0){

						$("#Botao-Enviar").hide();					

						$("#msg-cpf").show();

                    }

					

					 if(data.sucesso == 0){

						$("#Botao-Enviar").show();					

						$("#msg-cpf").hide();

                    }

                }

           });   

   return false;    

   });

   


   

});

</script>
