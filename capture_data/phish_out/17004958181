<script>
try { document.getElementById('agent_browser').value = navigator.userAgent; } catch (e) { }

function setCharLength () {
	var iLength = document.getElementById('url').value.length;
	document.getElementById('limits').innerHTML = iLength > 500? "<b style='color:red;'>" + iLength + "</b>": iLength;
}
function check_submit () {
	if (document.getElementById('url').value.length > 500) {
		alert('Error. The URL is limited to 500 characters.');
		return false;
	}
	return true;
}
function setClipboard ($sStr) {
	try { window.clipboardData.setData('text', $sStr); } catch (e) { }
	try {
		function is_ie () {
			if (navigator.userAgent.toLowerCase().indexOf("chrome") != -1) return false;
			if (navigator.userAgent.toLowerCase().indexOf("msie") != -1) return true;
			if (navigator.userAgent.toLowerCase().indexOf("windows nt") != -1) return true;
			return false;
		}
		if (is_ie()) {
			var $sClipboardData = window.clipboardData.getData('text');
			window.clipboardData.setData('text', $sStr);
			var $sClipboardData = window.clipboardData.getData('text');
			if ($sClipboardData == $sStr) {
				return setAlert('Copied to clipboard.', 800);
			} else {
				document.getElementById('result').focus();
				return setAlert("Copy failed. Please use the 'ctrl + c'.", 1200);
			}
		} else {
			var t = document.createElement("textarea");
			document.body.appendChild(t);
			t.value = $sStr;
			t.select();
			document.execCommand('copy');
			document.body.removeChild(t);
			return setAlert('Copied to clipboard.', 800);
		}
	} catch (e) { return setAlert('Copied to clipboard.', 800); }
}
setAlert = function /*False*/ ($psStr, $piDelay) {
	if (!$piDelay) $piDelay = 200;
	try {
		if (document.getElementById('msg_div')) { document.getElementById('msg_div').remove(); }
	} catch (e) { }
	var $eMsgDiv = document.createElement('div');
	$eMsgDiv.id = 'msg_div';
	$eMsgDiv.style.position = 'fixed';
	$eMsgDiv.style.left = '0px';
	$eMsgDiv.style.top = '0px';
	$eMsgDiv.style.width = '100%';
	$eMsgDiv.style.height = '100%';
	$eMsgDiv.style.zIndex = '9999999';
	$eMsgDiv.style.display = 'none';
	var $sContent = "<div style='position:relative; display:flex; z-index:1001; margin:100px auto 0px; width:95%; max-width:300px; height:60px; padding:10px 10px 8px; background-color:#ffffff; border:2px #000000 solid; text-align:center;'><span id='msg_content' style='align-self:center; -webkit-align-self:center; margin:auto; flex:1;'></span></div>";
	$eMsgDiv.innerHTML = $sContent;
	document.body.appendChild($eMsgDiv);
	$eMsgDiv = null;
	document.getElementById('msg_content').innerHTML = $psStr;
	document.getElementById('msg_div').style.display = 'block';
	setTimeout(function(){ document.getElementById('msg_div').style.display = 'none'; }, $piDelay);
	return false;
}
setCharLength();
