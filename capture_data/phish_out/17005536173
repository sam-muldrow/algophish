<script>
$(document).ready(function() {
  $("#ListTrigger").show();
});
var hide = '<i class="fa fa-minus"></i>';
var show = '<i class="fa fa-plus"></i>';

function showList()
{
	$('#day_list').fadeIn('fast', function() {
		$('#ShowList').replaceWith(' <a href="#" title="\u0421\u043A\u0440\u044B\u0442\u044C\u0020\u0441\u043F\u0438\u0441\u043E\u043A" id="HideList" onclick="hideList(); return false;">'+hide+'</a>');
	});
};

function hideList()
{
	$('#day_list').fadeOut('fast', function() {
		$('#HideList').replaceWith(' <a href="#" title="\u041F\u043E\u043A\u0430\u0437\u0430\u0442\u044C\u0020\u0441\u043F\u0438\u0441\u043E\u043A" id="ShowList" onclick="showList(); return false;">'+show+'</a>');
	});

}
</script>
