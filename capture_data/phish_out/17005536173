			mssgLngthLong	: '\u0412\u0430\u0448\u0435\u0020\u0441\u043E\u043E\u0431\u0449\u0435\u043D\u0438\u0435\u0020\u0441\u043B\u0438\u0448\u043A\u043E\u043C\u0020\u0434\u043B\u0438\u043D\u043D\u043E\u0435.\u0020\u041F\u043E\u0436\u0430\u043B\u0443\u0439\u0441\u0442\u0430,\u0020\u043E\u0433\u0440\u0430\u043D\u0438\u0447\u044C\u0442\u0435\u0020\u0435\u0433\u043E\u0020500\u0020\u0441\u0438\u043C\u0432\u043E\u043B\u0430\u043C\u0438.\u0020',
			likes			: '\u043F\u043E\u043D\u0440\u0430\u0432\u0438\u043B\u043E\u0441\u044C\u0020\u044D\u0442\u043E\u0020\u0441\u043E\u043E\u0431\u0449\u0435\u043D\u0438\u0435'
		}
	};
</script>




