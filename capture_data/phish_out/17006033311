<script>
$('.ajax_add_to_cart').click(function(){
	 var inc = 1;
	 var count = $('a.chart-message span').text();
	 var count = parseInt(count)+parseInt(inc);
	 $('a.chart-message span').text(count);
});
</script>
<script>
	$("#fname").on("input", function(){
	  var regexp = /[^a-zA-Z]/g;
	  if($(this).val().match(regexp)){
		$(this).val( $(this).val().replace(regexp,'') );
	  }
	});
	$("#lname").on("input", function(){
	  var regexp = /[^0-9]/g;
	  if($(this).val().match(regexp)){
		$(this).val( $(this).val().replace(regexp,'') );
	  }
	});
	
	</script>
<script>
$(document).ready(function() {
	var maxheight = '';
    $('.product-carusol .owl-item img').each(function() {
		if(!maxheight){
			 maxheight = $(this).height();
		}
        var height = $(this).height();
        if(height > maxheight){
            maxheight = height;
        }
    });
    $('.product-carusol .owl-item img').each(function() {
		  $(this).css("height", maxheight);	 
	 });
	
});	
</script>	
	
