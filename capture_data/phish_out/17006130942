<script>
	$(function(){
		$("#fullbanner").owlCarousel({
      autoplayTimeout:5000,
			autoplay:true,
			items:1,
			loop:true,
			dots:true,
			nav:true,
			onInitialized:startAnimation
		});
	});

	function startAnimation(e){
		for(i = 0; i < e.item.count; i++){
			active_obj_class = ".owl-item.active .banner-item .banner-caption.id-"+i;
			cur_obj = active_obj_class.replace('.active','');
			item_num = 1;

			$(cur_obj).children().each(function(){
				classes = $(this).data('animation');
				item_name = 'banner-item-'+item_num;
				$(this).addClass(item_name);

				$("style[data-id=for-banner]").append(active_obj_class+' > .'+item_name+'{animation:'+classes+'}');
				item_num++;
			});
		}
	}

</script>
