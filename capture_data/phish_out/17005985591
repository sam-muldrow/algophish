    var timer = setInterval(function(){
      if(jQuery('.trx_addons_message_box_success:contains("Send message complete!")').is(':visible')){
        gtag('event', 'submit', {
          'event_category' : 'form',
          'event_label' : 'contact'
        });
        clearInterval(timer)
      }
    },800)
    })
</script>
