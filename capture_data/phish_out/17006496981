  <script>
    $(function () {
      $('#slides').slidesjs({
        width: 707,
        height: 398,
        pagination: { effect: 'fade' },
        play: {
          auto: true,
          effect: 'fade',
          interval: 3800,
          active: true
        },
        effect: {
          fade: {
            speed: 800
          }
        },
        callback: {
          complete: function (number) {
            if (number == 1) {
              $('#slides .slidesjs-stop').trigger('click');
            }
          }
        }
      });

      $('#slides2').slidesjs({
        width: 227,
        height: 398,
        pagination: { effect: 'fade' },
        play: {
          auto: true,
          effect: 'fade',
          interval: 3800,
          active: true
        },
        effect: {
          fade: {
            speed: 800
          }
        },
        callback: {
          complete: function (number) {
            if (number == 1) {
              $('#slides2 .slidesjs-stop').trigger('click');
            }
          }
        }
      });

      $('#promotion-slide').slidesjs({
        width: 595,
        height: 295,
        pagination: { effect: 'fade' },
        play: {
          auto: true,
          effect: 'fade',
          interval: 3000
        },
        effect: {
          fade: {
            speed: 400
          }
        }
      });

      /*tab*/
      $(".tabc-menu1").show();
      $(".tabc-menu2").hide();
      $(".tabc-menu3").hide();

      $('#menu-tab1').addClass('tab1-hover');
      $('#menu-tab2').addClass('tab2');
      $('#menu-tab3').addClass('tab3');

      $("#menu-tab1").click(function () {
        $(".tabc-menu1").show();
        $(".tabc-menu2").hide();
        $(".tabc-menu3").hide();
        $('#menu-tab1').removeClass('tab1').addClass('tab1-hover');
        $('#menu-tab2').removeClass('tab2-hover').addClass('tab2');
        $('#menu-tab3').removeClass('tab3-hover').addClass('tab3');
      });

      $("#menu-tab2").click(function () {
        $(".tabc-menu1").hide();
        $(".tabc-menu2").show();
        $(".tabc-menu3").hide();
        $('#menu-tab1').removeClass('tab1-hover').addClass('tab1');
        $('#menu-tab2').removeClass('tab2').addClass('tab2-hover');
        $('#menu-tab3').removeClass('tab3-hover').addClass('tab3');
      });

      $("#menu_static1").hover(function () {
        $("#text_static1").toggle();
      });

      $("#menu_static2").hover(function () {
        $("#text_static2").toggle();
      });

      $("#menu_static3").hover(function () {
        $("#text_static3").toggle();
      });

      $("#menu_static4").hover(function () {
        $("#text_static4").toggle();
      });

      $("#menu_static5").hover(function () {
        $("#text_static5").toggle();
      });
    });
  </script>

  <script>
    $(function () {
      $('#overlay').overlay({
        mask: {
          color: '#080d3f',
          loadSpeed: 200,
          opacity: 0.75
        }
      });

      $('#content_cu .testimonial-list ul:not(.no-slide)').slidesjs({
        width: 180,
        height: 175,
        pagination: { effect: 'fade' },
        play: {
          auto: false,
          effect: 'fade'
        },
        effect: {
          fade: { speed: 400 }
        }
      });

      $('.home-success-story-list').slidesjs({
        width: 218,
        height: 187,
        pagination: { active: false },
        play: {
          auto: false,
          effect: 'fade'
        },
        effect: {
          fade: { speed: 400 }
        }
      });

      $('ul.student-star-list li').click(function () {
        $('span', this).toggle();
      });

      $('.slide-box ~ .btn').on('click', function () {
        var $list = $(this).siblings('.slide-box').find('ul'),
          width = $list.children('li').outerWidth(true),
          direction = $(this).hasClass('right-btn') ? '-=' : '+=';

        $list.animate({ 'margin-left': direction + width + 'px' });
      });

      $('.home-grammar .testimonial-list ul:not(.no-slide)').slidesjs({
        width: 222,
        height: 300,
        pagination: { effect: 'fade' },
        play: {
          auto: false,
          effect: 'fade'
        },
        effect: {
          fade: { speed: 400 }
        }
      });

      setInterval(function () {
        $('.click > span').toggle();
      }, 300);
    });
  </script>

