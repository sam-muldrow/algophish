  <style>
    #page-slide {
      display: none;
      position: relative;
    }

    #page-slide .slidesjs-navigation,
    #footer-video-slide .slidesjs-navigation {
      display: none;
    }
  </style>

  <script>
    $(function () {
      $('#page-slide').slidesjs({
        width: 648,
        height: 434,
        pagination: { effect: 'fade' },
        play: {
          auto: true,
          effect: 'fade'
        },
        effect: {
          fade: { speed: 400 }
        }
      });

      $('#footer-video-slide').slidesjs({
        width: 180,
        height: 310,
        pagination: { effect: 'slide' },
        play: {
          auto: true,
          effect: 'slide',
          interval: 3000
        },
        effect: {
          slide: { speed: 400 }
        }
      });

      $('.org-list-wrapper .right-btn').click(function () {
        var w = $('.org-list > li').outerWidth();

        if (parseInt($('.org-list').css('marginLeft')) <= -w * 5) return;

        $('.org-list').animate({ marginLeft: "-=" + w });
      });

      $('.org-list-wrapper .left-btn').click(function () {
        var w = $('.org-list > li').outerWidth();

        if (parseInt($('.org-list').css('marginLeft')) == 0) return;

        $('.org-list').animate({ marginLeft: "+=" + w });
      });

      $('footer .tab a').click(function () {
        $('footer .tab div').removeClass('active');
        $(this).parent().addClass('active');
        $('footer .tab-content > .tc').addClass('hide');
        $($(this).attr('href')).removeClass('hide');
        return false;
      });

      $('#video-overlay').overlay({
        mask: {
          color: '#080d3f',
          loadSpeed: 200,
          opacity: 0.75
        },
        onClose: function (event) {
          $('#video-overlay iframe').attr('src', '');
        }
      });

      $('a[rel=video-overlay]').click(function () {
        $('#video-overlay iframe').attr('src', $(this).attr('href'));
        $('#video-overlay').data('overlay').load();
        return false;
      });

      $('.video-list li, footer .activity-wrapper .tab-content > .tc > ul > .li-tab-content').click(function () {
        $('#video-overlay iframe').attr('src', $(this).attr('src'));
        $('#video-overlay').data('overlay').load();
      });


      $(".eng_hover_courses").hover(function () {
        $(".eng_droupdown_menu_course").slideToggle("slow");
      });

    });
  </script>

