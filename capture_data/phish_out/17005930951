<script type="text/javascript">
jQuery(function($) {

	$(window).scroll(function(){
		var npos = $(window).scrollTop();
		if(npos > 200){
			$('.js_show_top').show();
		} else {
			$('.js_show_top').hide();
		}
	});	
	
	$('.js_to_top').on('click', function(){
		$('body, html').animate({scrollTop : 0},500);
		return false;	
	});
	
});
</script>	

