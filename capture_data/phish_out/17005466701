<script type="rocketlazyloadscript" data-rocket-type="text/javascript" data-rocket-src="https://computermania.com.bd/wp-content/plugins/js_composer/assets/js/dist/js_composer_front.min.js?ver=6.13.0" id="wpb_composer_front_js-js"></script>
<script type="rocketlazyloadscript" data-minify="1" data-rocket-type="text/javascript" data-rocket-src="https://computermania.com.bd/wp-content/cache/min/1/wp-content/themes/woodmart/js/libs/countdown-bundle.js?ver=1700045378" id="wd-countdown-bundle-library-js"></script>
