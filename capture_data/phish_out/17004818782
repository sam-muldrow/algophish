				var time_start;
				$(window).load(
					function() {
						time_start = new Date();
					}
				);
				$(window).unload(
					function() {
						var time_end = new Date();
						var pagetime = new Object;
						pagetime.type = "pagetime";
						pagetime.id_connections = "2511061";
						pagetime.id_page = "3";
						pagetime.time_start = "2023-11-20 13:04:41";
						pagetime.token = "779c36a2ce3820ea5a3663adce9c5db6787a74cb";
						pagetime.time = time_end-time_start;
						$.post("https://bijouxenjade.com/index.php?controller=statistics", pagetime);
					}
				);
			</script>
