$(document).ready(function() {	jQuery.browser={};	(function(){		jQuery.browser.msie=false;		jQuery.browser.version=0;		if(navigator.userAgent.match(/MSIE ([0-9]+)\./)){			jQuery.browser.msie=true;jQuery.browser.version=RegExp.$1;	}})();  $("#owl-banner").owlCarousel({       navigation : false, // Show next and prev buttons      slideSpeed : 300,	  autoPlay : 14000,      paginationSpeed : 400,      singleItem:true,	  beforeMove:beforemoved,	  afterMove:moved,	  afterInit:loadContent,	  transitionStyle : "backSlide",	jsonPath : '/'+virtualPath+'/json/customData.json',	jsonSuccess : customDataSuccess  });   	function customDataSuccess(data){		var content = "";		for(var i in data["items"]){			var img = data["items"][i].img;			var alt = data["items"][i].alt;			content += "<img src=\"" +img+ "\" alt=\"" +alt+ "\">"		}		$("#owl-banner").html(content);	}  	function loadContent(){		var currentIndex = $('div.active').index();		$.get("/f1/0/"+arBannerUNIDs[currentIndex]+"/CurrentTrends?OpenField", function( data ) {  			$("#currentTrend_Content").html(data);  			$("#currentTrend_Content").css({'background':'none'});		});				$.get("/f1/0/"+arBannerUNIDs[currentIndex]+"/Technology?OpenField", function( data ) {  			$("#techPanel_Content").html(data);  			$("#techPanel_Content").css({'background':'none'});		});				$.get("/f1/0/"+arBannerUNIDs[currentIndex]+"/WhereWeAre?OpenField", function( data ) {  			$("#rightPanel_Content").html(data);  			$("#rightPanel_Content").css({'background':'none'});		});			}				function beforemoved(){		if ($.browser.msie){			$("#currentTrend").css({'height':'auto'});			$("#centralPanel").css({'height':'auto'});			$("#rightPanel").css({'height':'auto'});			$("#techPanel").css({'height':'auto'});		}	}		//moved callback    function moved(){						var currentIndex = $('div.active').index();		$("#currentTrend_Content").css({'background-image':'url('+imgLoading.src+')', 'background-position':'center', 'background-repeat':'no-repeat'});		$("#techPanel_Content").css({'background-image':'url('+imgLoading.src+')', 'background-position':'center', 'background-repeat':'no-repeat'});		$("#rightPanel_Content").css({'background-image':'url('+imgLoading.src+')', 'background-position':'center', 'background-repeat':'no-repeat'});				$.get("/f1/0/"+arBannerUNIDs[currentIndex]+"/CurrentTrends?OpenField", function( data ) {			$('#currentTrend_Content').fadeOut('slow', function() {	        		$('#currentTrend_Content').html(data)	        		$('#currentTrend_Content').fadeIn('slow', function(){	        			$("#currentTrend_Content").css({'background':'none'});				});    			});  					});				$.get("/f1/0/"+arBannerUNIDs[currentIndex]+"/Technology?OpenField", function( data ) {			$('#techPanel_Content').fadeOut('slow', function() {	        		$('#techPanel_Content').html(data)	        		$('#techPanel_Content').fadeIn('slow', function(){	        			$("#techPanel_Content").css({'background':'none'});				});    			});  			});				$.get("/f1/0/"+arBannerUNIDs[currentIndex]+"/WhereWeAre?OpenField", function( data ) {			$('#rightPanel_Content').fadeOut('slow', function() {	        		$('#rightPanel_Content').html(data)	        		$('#rightPanel_Content').fadeIn('slow', function() {					resizePanels();					$("#rightPanel_Content").css({'background':'none'});			  	});    			});		});		    }	});function resizePanels(){	if ($.browser.msie){		$("#currentTrend").css({'height':+$("#currentTrend_Parent").height()+'px'});		$("#centralPanel").css({'height':+$("#centralPanel_Parent").height()+'px'});		$("#rightPanel").css({'height':+$("#rightPanel_Parent").height()+'px'});		$("#techPanel").css({'height':+$("#techPanel_Parent").height()+'px'});	}}

