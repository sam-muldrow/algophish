                $(document).ready(function(){
                     $(".postsocial").hide();
                    $(".shrcc").hover(function(){
                       $(this).next().show();
                    });
                    
                     $(".postsocial").hover(function(){
                       $(this).show();
                    });
                    
                    $(".postsocial").mouseleave(function(){
                       $(this).hide();
                    });
                    
                     $(".shrcc").mouseleave(function(){
                       $(".postsocial").hide();
                    });
                    
                    
                });
                
                
</script>
