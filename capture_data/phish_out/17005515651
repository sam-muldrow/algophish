<script>

$(document).ready(function(){
    $(".clickfooter").click(function(){
        $(this).hide();
        $(".clickfooter1").show();
        $(".footericon").css({"margin-left":"0px","transition":"all 1s ease"});
        
});
    
     $(".clickfooter1").click(function(){
        $(this).hide();
        $(".clickfooter").show();
        $(".footericon").css({"margin-left":"200px","transition":"all 1s ease"});
        
});
    
    
     $(".phone").click(function(){
        $(".phonee").stop().slideDown();
    
    });
     $(".phon").click(function(){
        $(".phonee").stop().slideUp();
    });

    
});

</script>



