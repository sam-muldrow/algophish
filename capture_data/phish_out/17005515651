  <script>
  

  
$(function(){
$('select[name="lang1"]').on('change', function(){
var lang = $(this).val();
var language = '';
console.log(lang);
switch(parseInt(lang))
{

case 1 : language = 'select'; break;
case 2 : language = 'english'; short_form = 'en'; break;
case 3 : language = 'french'; short_form = 'fr'; break;
case 4 : language = 'german'; short_form = 'de'; break;
case 5 : language = 'spanish'; short_form = 'es'; break;
case 6 : language = 'hindi'; short_form = 'hi'; break;
case 7 : language = 'portuguese'; short_form = 'pt'; break;
case 8 : language = 'russian'; short_form = 'ru'; break;

case 9 : language = 'mandarian'; short_form = 'en'; break;
case 10 : language = 'chinese'; short_form = 'zh'; break;

case 11 : language = 'italian'; short_form = 'it'; break;

case 12 : language = 'janpaense'; short_form = 'ja'; break;
case 13 : language = 'turkish'; short_form = 'tr'; break;

case 14 : language = 'arabic'; short_form = 'ar'; break;

case 15 : language = 'khmer'; short_form = 'kh'; break;
case 16 : language = 'nepal'; short_form = 'np'; break;

}
console.log(language);
var newForm = jQuery('<form>', {
'action': "http://contribcity.com/LangSwitch/switchLanguage/"+language,
'target': '_top',
'method':'post' 
}).append(jQuery('<input>', {
'name': 'referrer',
'type': 'hidden',
'value': window.location.href,
}));

newForm.appendTo("body").submit();

//index.php/LangSwitch/switchLanguage/german
});

});

</script>
    
     
