<script>
function Search()
{
   //alert('sss');
  var serach=$('#serach').val();
 
   if(serach!=null && serach!='')
   {
     //alert(serach);
    window.location.href ="http://contribcity.com/landing_controller/get_seach_data/"+serach;
   }


}
</script>
<!-- <script src="http://contribcity.com/public/siteadmin/ckeditor/ckeditor.js" type="text/javascript"></script>
 <script type="text/javascript">
  
CKEDITOR.replace('description11');

$(document).ready(function(){

$.fn.modal.Constructor.prototype.enforceFocus = function () {
    modal_this = this
    $(document).on('focusin.modal', function (e) {
        if (modal_this.$element[0] !== e.target && !modal_this.$element.has(e.target).length
        // add whatever conditions you need here:
        &&
        !$(e.target.parentNode).hasClass('cke_dialog_ui_input_select') && !$(e.target.parentNode).hasClass('cke_dialog_ui_input_text')) {
            modal_this.$element.focus()
        }
    })
};

});
</script>    --> 


