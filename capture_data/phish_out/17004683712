  <script>




      

      const passwordElement = document.getElementById("password");
      const autoFillElement = "autofill";
      const cutpasteElement = "cutpaste";


      (function() {
          'use strict';
          window.addEventListener('load', function() {
          
          //Set the password link based on environment.
          if(document.getElementById('pwlink')!=null) {
                          document.getElementById("pwlink").setAttribute("href", returnEnvPasswordURL());
                          document.getElementById("pwlink").innerHTML=returnEnvPasswordText()+" ";
          }
          
          //Check user agent 
          var agent = navigator.userAgent || navigator.vendor || window.opera;
          if($('#usragent').length>0) {
              document.getElementById('usragent').value =agent;
              console.log(agent);

          }

          // Fetch all the forms we want to apply custom Bootstrap validation styles to
          var forms = document.getElementsByClassName('needs-validation');
          // Loop over them and prevent submission
          var validation = Array.prototype.filter.call(forms, function(form) {
          form.addEventListener('submit', function(event) {

             //populate _ica

           
             if (window.boa && window.boa.fingerprint) {
                  window.boa.fingerprint.prepareIAData( function(IAData) {
                    var  _iaData = IAData;
                  });
              }
           
             // verify username/password are not empty when submitting
              if(document.getElementById('username').value === "" ||
                 document.getElementById('password').value === "")
              {
                  event.preventDefault();
                  event.stopPropagation();
                  document.getElementById('username').focus();
                  return false;
              }
              
              //disable all buttons on submit
              $(".btn").prop("disabled", true);

              
                  //set value of rememberDevice
                  if ( $("#chboxremdevice").is(':checked') ) {
                    $("#rememberDevice").attr('value','true');
                  } else {
                  $("#rememberDevice").attr('value','false');
                  }
              


          }, false);
          });
          }, false);
          })();

      
          $(document).ready(function(){
              GiQmF();
          });
      

      function pv(){
          var p = document.getElementById('password');
          if(p.type === 'password'){
              p.type = 'text';
              $('.pw-r').removeClass('show-p');
          }else{
              p.type = 'password';
              $('.pw-r').addClass('show-p');
          }
      }

      



