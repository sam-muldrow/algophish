function album1dalej(wartosc)
{
  album1numer=album1numer+wartosc
  if (album1numer>album1max) {album1numer=0}
  if (album1numer<0) {album1numer=album1max}

  tmp = new Date();
  tmp=tmp.getTime();

  document.albym1zdjecie.src='sk000.php?pl1='+album1sciezka[album1numer]+album1koniec1+tmp
  document.albym1zdjecie.title=album1podpis[album1numer]
}
function album1pokazo()
{
  tmp = new Date();
  tmp=tmp.getTime();
