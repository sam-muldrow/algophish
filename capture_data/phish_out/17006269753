<script language='JavaScript'>
zdw=400
zdl=25
kto=document.getElementById('zp').style
///////

poz0=0 //-1*Math.round(zdw/2)
aa=poz0
bb=poz0
szer=zdl*zdw
szer1=-1*szer 
kto.left=poz0
kto.width=szer+zdw
/////////

function dalej()
{
  

if(aa==bb){bb=aa-zdw};
 aa=aa-50
kto.left=aa;
 
if(aa<=szer1){

aa=poz0;bb=aa ;kto.left=aa; 
//przewinp() 
};
if(aa!=bb){setTimeout("dalej()",30);};
}
////////////
function wstecz()
{

if(aa==bb){bb=aa+zdw};
aa=aa+50
kto.left=aa;
if(aa>=zdw+poz0){
//przewinl()
aa=szer1+zdw+poz0; bb=aa ;kto.left=aa; 
};
if(aa!=bb){setTimeout("wstecz()",30);};
}

 
</script>

 

