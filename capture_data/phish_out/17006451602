        cookie.defaults.expires = 7200;
        cookie.defaults.path = "/";

        var userId = (cookie.get('userId') == undefined) ? 0 : cookie.get('userId');

        return {
            THRESHOLD: 2,
            SPECIAL_IDS: [],
            WID: 1512442891,
            USER_ID: userId
        }
    }
</script>




