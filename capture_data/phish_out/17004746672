                            <script>
                                $(document).ready(function () {
                                    activecourse(62);
                                })
                                function activecourse(a) {
                                    var elemArray1 = document.getElementsByClassName('abc1');
                                    for (var i = 0; i < elemArray1.length; i++) {
                                        var elem = elemArray1[i];
                                        if (a == elem.innerText) {
                                            var span1 = '#pills-qm-tab' + i;
                                            $(span1).removeClass('nav-link');
                                            $(span1).addClass('nav-link active');
                                            if (a != 62) {
                                                document.getElementById("pills-tab").scrollIntoView();
                                            }
                                        }
                                        else {
                                            var span1 = '#pills-qm-tab' + i;
                                            $(span1).removeClass('nav-link active');
                                            $(span1).addClass('nav-link');
                                        }
                                    }
                                    var elemArray = document.getElementsByClassName('abc');
                                    for (var i = 0; i < elemArray.length; i++) {
                                        var elem = elemArray[i];
                                        if (a == elem.innerText) {
                                            var span = '#spanWishlist' + i;
                                            $(span).removeClass('tab-pane fade');
                                            $(span).addClass('tab-pane fade show active');
                                        }
                                        else {
                                            var span = '#spanWishlist' + i;
                                            $(span).removeClass('tab-pane fade show active');
                                            $(span).addClass('tab-pane fade');
                                        }
                                    }
                                }
                            </script>
