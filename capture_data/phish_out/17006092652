$('.inpage').click(function(){ $('#logo2').hide();   $('ul li a.home_tab').css('display', 'inline-block'); $('.about_tab').removeClass('inabpage'); });		

$('ul li a.home_tab').click(function(){   
  $('.pagesection').hide();
  $('ul li a.home_tab').hide();
  $('.about_tab').addClass('inabpage');
  $('.inpage').removeClass('active');
  $('#logo2').show();
    $('.undermenu').fadeOut('slow');
$('.dd_menu').fadeOut('slow');    
$('.contact_tab').addClass('pr-40');  
$('.video-js').removeClass('video-js21');   
  
});

$('.about_tab').click(function(){
$('.inpage').removeClass('active'); $(this).addClass('active');		
$('.dd_menu').fadeOut('slow');  
$('.pagesection').fadeOut('slow');	
$('.aboutsection').fadeIn('slow');
  $('.contact_tab').addClass('pr-40');  
$('.video-js').addClass('video-js21');    
});	
  
  
  
  
$('.service_tab').click(function(){
$('.inpage').removeClass('active'); $(this).addClass('active');	
   
$('.pagesection').fadeOut('slow');		
$('.servicesection').fadeIn('slow');
$('.dd_menu').fadeOut('slow');  
$('.contact_tab').addClass('pr-40');  
  $('.video-js').addClass('video-js21');    
});


  
  

$('.project_tab').click(function(){
$('.inpage').removeClass('active'); $(this).addClass('active');	
 $('.dd_menu>a').addClass('active');	
$('.dd_menu').fadeIn('slow');
$('.pagesection').fadeOut('slow');		
$('.projectsection').fadeIn('slow');
$('.contact_tab').removeClass('pr-40');
$('.video-js').addClass('video-js21');      
});
 
  


$('.blog_tab').click(function(){
$('.inpage').removeClass('active'); $(this).addClass('active');	 
$('.pagesection').fadeOut('slow');	
$('.dd_menu').fadeOut('slow');  
$('.blogsection').fadeIn('slow'); 
$('.contact_tab').addClass('pr-40'); 
  $('.video-js').addClass('video-js21');    
});	
  

$('.contact_tab').click(function(){
$('.inpage').removeClass('active'); $(this).addClass('active');	 
$('.pagesection').fadeOut('slow');		
$('.contactsection').fadeIn('slow');
$('.dd_menu').fadeOut('slow'); 
$('.contact_tab').addClass('pr-40'); 
  $('.video-js').addClass('video-js21');    
});	

  
$('.menu_tab').click(function(){
   $('.undermenu').fadeToggle('slow');
  $('.Commercial').show('fast'); 
$('.Township').show('fast'); 
$('.Residential').show('fast'); 
$('.Government').show('fast');
});	  
  
  

$('.closebtn').click(function(){     
$('.servicedetailbox').hide('fast');
$('.sv1, .sv2, .sv3, .sv4, .sv6, .sv5, .sv7, .sv8').show('fast'); 
}); 

$('.sv1').click(function(){     
$('.svd1').show('fast');
$('.sv1, .sv2, .sv3, .sv4').hide('fast');  
});   

$('.sv2').click(function(){     
$('.svd2').show('fast');
$('.sv1, .sv2, .sv3, .sv4').hide('fast');  
}); 

$('.sv3').click(function(){     
$('.svd3').show('fast');
$('.sv1, .sv2, .sv3, .sv4').hide('fast');  
}); 

$('.sv4').click(function(){     
$('.svd4').show('fast');
$('.sv1, .sv2, .sv3, .sv4').hide('fast');  
});   


$('.closebtn2').click(function(){     
$('.servicedetailbox').hide('fast');
$('.sv1, .sv2, .sv3, .sv4, .sv6, .sv5, .sv7, .sv8').show('fast'); 
 
}); 


$('.sv5').click(function(){     
$('.svd11').show('fast');
$('.sv5, .sv6, .sv7, .sv8').hide('fast');  
});   

$('.sv6').click(function(){     
$('.svd12').show('fast');
$('.sv5, .sv6, .sv7, .sv8').hide('fast');  
}); 

$('.sv7').click(function(){     
$('.svd13').show('fast');
$('.sv5, .sv6, .sv7, .sv8').hide('fast');  
}); 

$('.sv8').click(function(){     
$('.svd14').show('fast');
$('.sv5, .sv6, .sv7, .sv8').hide('fast');   
});   



$('.btn-for-email').click(function(){     
$('.firstbox1').show('fast');
 $('.logoseccon').addClass('logohidem'); 
  $('.firstbox1').removeClass('hideonmobme');
  $('.secondbox').addClass('hideonmobme'); 
}); 

$('.btn-for-talk').click(function(){     
$('.secondbox').show('fast');  
   $('.logoseccon').addClass('logohidem'); 
   $('.secondbox').removeClass('hideonmobme'); 
$('.firstbox1').addClass('hideonmobme'); 
});   
  
// $('.close').click(function(){   
//  $('#playerID').get(0).stopVideo();
// });    
  
$('.c_project').click(function(){   
$('.Commercial').show('fast'); 
$('.Township').hide('fast'); 
$('.Residential').hide('fast'); 
$('.Government').hide('fast');   
});  
  
$('.t_project').click(function(){   
$('.Commercial').hide('fast'); 
$('.Township').show('fast'); 
$('.Residential').hide('fast'); 
$('.Government').hide('fast');   
});
  
$('.r_project').click(function(){   
$('.Commercial').hide('fast'); 
$('.Township').hide('fast'); 
$('.Residential').show('fast'); 
$('.Government').hide('fast');   
}); 
  
$('.g_project').click(function(){   
$('.Commercial').hide('fast'); 
$('.Township').hide('fast'); 
$('.Residential').hide('fast'); 
$('.Government').show('fast');   
});
 $('.close-alert ').click(function(){    
 $('.alert').hide('fast'); 
 }); 
  

$('.letstalk').click(function(){
$('.inpage').removeClass('active'); $('.contact_tab').addClass('active');	 
$('.pagesection').fadeOut('slow');		
$('.contactsection').fadeIn('slow');
$('.dd_menu').fadeOut('slow'); 
$('.contact_tab').addClass('pr-40'); 
  $('.video-js').addClass('video-js21');    
});	
  
  
  
  
