    <script>
        // nastavení cookie a (ne)zobrazení popup
        function checkCookie() {
            let user = getCookie("asparWindow");
            // cookie neexistuje
            if (user != "shown85V_7zuh9Rt") {
                // pokud existuje, zobrazit modal s aktualitou
                let element = document.getElementById('modalNewsAspar');
                if (element) {
                    document.body.className += 'modal-open';
                    element.className += ' show';
                    element.style.display = 'block';
                    element.style.background = 'rgba(0, 0, 0, 0.7)';
                    // vytvořit cookie s platností 1 den
                    user = "shown85V_7zuh9Rt";
                    setCookie("asparWindow", user, 1);
                }
            }
        }

        function closeAsparNews() {
            let element = document.getElementById('modalNewsAspar');
            document.body.classList.remove('modal-open');
            element.classList.remove('show');
            element.style.display = 'none';
        }
    </script>
</body>

