<script type="text/javascript">
    $(window).ready(function() {
      if (sessionStorage.getItem('#myModal')!== 'true') {
        //$('#myModal').modal('show');
        $('#myModal').modal('show',{
              backdrop: 'static',
              keyboard: false,
              show: true
            });

        sessionStorage.setItem('#myModal', 'true')
      }
    });
 
</script>

