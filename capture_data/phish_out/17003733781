        <script>
            var Config = {
                dname      : 'verek.ru',
                captchaKey : '6LeOBCUUAAAAAH12B8bXjdRxiKCPyhBtqdP1DkUE',
                formAction : 'https://www.reg.ru/whois/parking_admin_contact'
