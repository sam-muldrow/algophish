<script language="javascript">
    
function checa(){
	nform = document.getElementById('form_busca');
		if (nform.busca.value == "") {
			alert("O campo BUSCA nï¿½o pode ficar vazio!");
			nform.busca.focus();
			nform.busca.select();
		}else{
			document.getElementById('form_busca').submit();
		}
	}
</script>

