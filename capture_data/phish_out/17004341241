// 	});


    $('#lnk_re_reg_otp').click(function() {
	    var contactno = $('#frm_reg input[name="contactno"]').val();
	    var email = $('#frm_reg input[name="email"]').val();
	    var reg_otp_new = $('#reg_otp').val();

	    $.ajax({
	        type: "POST",
	        url: 'https://www.anupamshaadi.com/home/ressendotp',
	        data: {'contactno': contactno, 'email': email},
	        async: true,
	        dataType: 'json',
	        success: function(result) {
	            if (result.status == 1) {
	                var reg_otp_code = result.otp;

	                $.ajax({
	                    type: "POST",
	                    url: 'https://www.anupamshaadi.com/home/checkotp',
	                    data: {'reg_otp_new': reg_otp_new},
	                    async: true,
	                    dataType: 'json',
	                    success: function(responseText) { 
	                        if (responseText === 1) {
	                            $('#mob_verified').val('1');
	                            $('#buybtn1').click();
	                            $('#buybtn1').attr("disabled", true);
	                            $('#error_reg_otp').hide();
	                        } else {
	                            $('#error_reg_otp').show();
	                        }
	                    }
	                });
	            } else {
	                $('#reg_frm_div').before('<div id="error_reg" class="row m-0">'
	                    + '<span id="error_reg_otp" class="text-danger">' + result.message + '</span>'
	                    + '</div>');
	            }
	        }
	    });
	});

		// build dropdown
	$('.maritialStatus').on('focus', function(){
		var me = $(this);
		$(me).empty();
		$(me).prop('multiple', 'multiple');
    	$.ajax({
			url : "https://www.anupamshaadi.com/ajax/getMaritalStatusDDL",
			type: "POST",				
			dataType: "JSON",
			success: function(data){					
				$.each(data, function(key, value) {   
					$(me).append(value); 
				});

				$(me).multiselect({
					nonSelectedText: 'Select',						
					enableCaseInsensitiveFiltering: true,
					includeSelectAllOption: true,
					buttonWidth: 100,
					maxHeight: 300,
					enableFiltering: true,
					templates: {
						filter: '<li class="multiselect-item filter"><div class="input-group"><input class="form-control multiselect-search" type="text"></div></li>',
						filterClearBtn: '<span class="input-group-btn"><button class="btn btn-default multiselect-clear-filter" type="button"><i class="fas fa-times"></i></button></span>'
					}
				});
			}
		});  
	});

	$('.educationDDL').on('focus', function(){
		var me = $(this);
		$(me).empty();
		$(me).prop('multiple', 'multiple');		
    	$.ajax({
			url : "https://www.anupamshaadi.com/ajax/getEducationDDL",
			type: "POST",				
			dataType: "JSON",
			success: function(data){					
				$.each(data, function(key, value) {   
					$(me).append(value); 
				});

				$(me).multiselect({
					nonSelectedText: 'Select',						
					enableCaseInsensitiveFiltering: true,
					includeSelectAllOption: true,
					buttonWidth: 100,
					maxHeight: 300,
					enableFiltering: true,
					templates: {
						filter: '<li class="multiselect-item filter"><div class="input-group"><input class="form-control multiselect-search" type="text"></div></li>',
						filterClearBtn: '<span class="input-group-btn"><button class="btn btn-default multiselect-clear-filter" type="button"><i class="fas fa-times"></i></button></span>'
					}
				});
			}
		});  
	})

