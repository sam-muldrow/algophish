										$(function() {
											$('input:radio[name="gender"]').click(function(){
												console.log($(this).val());
												if($(this).val()=='Male'){
													$("#birthdate").val('');
													setdobdatepicker(21);
												}else{
													$("#birthdate").val('');
													setdobdatepicker(18);
												}
											});
											$("#birthdate").val('');
											setdobdatepicker(18);
											$('input:radio[name="gender"][value="Male"]').prop('checked',true);
											function setdobdatepicker(age){
												var dtyearRange=(age==18)?'-110:-18':'-110:-21';
												var maxdt = (age==18)?'-18Y':'-21Y';
												var mindt = (age==18)?'-68Y':'-71Y';
												$("#birthdate").datepicker('destroy').datepicker({
													dateFormat : 'dd-mm-yy',
													showOn: "both",
													//buttonImage: "b_calendar.png",
													buttonImageOnly: false,
													buttonText: '<span class="input-group-addon"><i class="fa fa-calendar clr" aria-hidden="true"></i><span>',
													changeMonth: true,
													changeYear: true,
													yearRange: dtyearRange,
													maxDate: maxdt,
													minDate: mindt,
												}); 
											}
											var enforceModalFocusFn = $.fn.modal.Constructor.prototype._enforceFocus;

											$.fn.modal.Constructor.prototype._enforceFocus = function() {};
										});
									</script>
