<script>
	$('#regotp_frm_div').hide();
	$('#error_reg_otp').hide();	
	$('#error_tnc').hide();	
	$('#buybtn1').hide();	
	$('#lnk_login_modal').click(function(){
		$('#firstname').blur();
		$('#lng_email').focus();
	});
	$('#lnk_reg_modal').click(function(){
		$('#error_tnc').hide();	
		$('#reg_frm_div').show();
		$('#regotp_frm_div').hide();
		$('#error_reg_otp').hide();
		$('#error_reg').remove();		
		$('#frm_reg')[0].reset();
		$('#frm_lgn')[0].reset();
		$('#lng_email').blur();
		$('#firstname').focus();
	});
	$('#registerModal').on('shown.bs.modal', function() {
		$('#lng_email').blur();
		$('#firstname').focus();
	});	
	
	$('#loginModal').on('shown.bs.modal', function() {
		$('#firstname').blur();
		$('#lng_email').focus();
	});
	var reg_otp_code = 0;
	$('#btn_reg').click(function(){
		$('#error_reg').remove();
		var chk_status=0;
		if($('#pk1').prop("checked") == true){
			$('#error_tnc').hide();	
			chk_status=1;
		}else{
			$('#error_tnc').show();
		}

		if ($('#frm_reg')[0].checkValidity() && chk_status ==1) {
			$('#error_reg_otp').hide();
			if($('#btn_reg').val()=='Register'){
				var contactno = $('#frm_reg').find('input[name="contactno"]').val();
				var email = $('#frm_reg').find('input[name="email"]').val();
				var txt = $("#g-recaptcha-response").val();
				/* ajax call for otp also check email and contact duplicate */
				$.ajax({
					type: "POST",
					url: 'https://www.anupamshaadi.com/home/registrationotp',
					data: {'contactno':contactno,'email':email, 'g-recaptcha-response':txt,},
					async: true,
					dataType:'JSON',
					success: function(result){
						var response = eval(result);
						if(response.status == 1){
							reg_otp_code = response.otp;
							$('#reg_frm_div').hide();
							$('#regotp_frm_div').show();
							$('#btn_reg').val('Verify OTP');
							$('#error_reg_otp').hide();	
						}else{
							$('#reg_frm_div').before('<div id="error_reg" class="row m-0">'
										+'<span class="text-danger">'+ response.message +'</span>'
									+'</div>');
						}
					}
				});
			}else{
			    
			    var reg_otp_new = $('#reg_otp').val();

					$.ajax({
					    type: "POST",
					    url: 'https://www.anupamshaadi.com/home/checkotp',
					    data: {'reg_otp_new': reg_otp_new},
					    async: true,
					    dataType: 'JSON',
					    success: function(responseText) { 
					        if (responseText == 1) {
					            $('#mob_verified').val('1');
					            $('#buybtn1').click();
					            $('#buybtn1').attr("disabled", true);
					            $('#error_reg_otp').hide();
					        } else {
					            $('#error_reg_otp').show();
					        }
					    }
					});
			    
				// if($('#reg_otp').val() == reg_otp_code){
				// 	$('#mob_verified').val('1');
				// 	$('#buybtn1').click();
				// 	$('#buybtn1').attr("disabled", true);
				// 	$('#error_reg_otp').hide();	
				// }else{
				// 	$('#error_reg_otp').show();	
				// }				
			}
			/*$('frm_reg[id="frm_reg"]').find('#buybtn1').click();
			return false;*/
		}else {
			$("#frm_reg")[0].reportValidity();
		}
	});

// 	$('#lnk_re_reg_otp').click(function(){
// 		/*ajax call*/
// 		var contactno = $('#frm_reg').find('input[name="contactno"]').val();
// 		var email = $('#frm_reg').find('input[name="email"]').val();
// 		/* ajax call for otp also check email and contact duplicate */
// 		$.ajax({
// 			type: "POST",
