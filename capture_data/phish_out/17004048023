<script type="text/javascript">

    function onchangeevent() {
        $('#spanUserExist').hide();
        var emailId = $('#txtEmailId').val();
        if (emailId.length == 0) {
            return;
        }
        $.ajax({
            type: "POST",
            url: '/en/Shared/CheckUserID',
            data: "{'User' : '" + emailId + "'}",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            async: true,
            cache: false,
            error:
         function (x, e) {
             //alert("The call to the server side failed. " + x.responseText);
         },
            success:
        function (result) {
            if (result != null) {
                if (result == 'This Email Address is already registered') {
                    // let user review his entry. Do not clear it.
                    $('#spanUserExist').show();
                    $('#spanUserExist').text('This Email Address is already registered');
                }
                else {
                    $('#spanUserExist').text('');
                }

            }
        }
        });
    }

    function nameLengthvalidation() {
        var firstname = $('#txtFirstName').val();
        var lastname = $('#txtLastNames').val();
        var totallength;
        if (firstname.length > 0 && lastname.length > 0) {
            totallength = firstname.length + lastname.length;
            if(totallength >23) {
                $('#spannamelength').text("FirstName and LastName Cannot Exceed 23 Characters");
                $("#dwaite").hide();
                $("#imgRegister").show();
                e.preventDefault();
                return false;
            }
            else {
                return true;
            }
        }
        
    }

    function SubmitRegister(e) {
        e.preventDefault();
        $('#spannamelength').text("");
        $('#spanUserExist').text('');
        nameLengthvalidation();
        $.post('/en/Shared/RegisterSubmit', $('#registerForm').serialize(), function (data) {
            if (data) {
                $("#imgRegister").show();
                $("#dwaite").hide();
                $('#fade_DIV').hide();
                $('#popup_register').hide();
                e.preventDefault();
                jAlert('Registered Successfully', 'Afrah Travel');
            }
            else {
                alert('User already Exists ', 'Afrah Travel');
            }

        });
    }

    $(document).ready(function () {
        
        $("#imgRegister").click(function (event) {

            if ($("#registerForm").valid()) {
                $("#imgRegister").hide();
                $("#dwaite").show();
                if ($('#spanUserExist').text() == 'This Email Address is already registered') {
                    $("#imgRegister").show();
                    $("#dwaite").hide();
                    return false;
                }
                else {
                    SubmitRegister(event);
                }
                
            }


        });


        $(".close_btn").click(function (event) {

            $('#fade_DIV').slideUp(500)
            $('#popup_register').slideUp(500)
        });

    });

</script>


