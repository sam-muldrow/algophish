    <script type="text/javascript">
        $(document).ready(function () {
            $('#open-pop-up-1').click(function () {
                $('#popup_login').toggle();
                if ($('#popup_login').is(":visible")) {
                    $('#btnloginForm').addClass('login_active');
                    $('#popup_login').show();
                    $('#popup_register').hide();
                }
                else {
                    $('#popup_register').hide();
                    $('#btnloginForm').removeClass('login_active');
                }

                $('#toggle_Forgot').hide();
                $('#btnRegister').removeClass('login_active');
            });
            $('#open-pop-up-2').click(function () {
                if ($('#popup_login').is(":visible")) {
                    $('#btnloginForm').removeClass('login_active');
                    $('#popup_login').hide();
                }
                if ($('#toggle_Forgot').is(":visible")) {
                    $('#btnloginForm').removeClass('login_active');
                    $('#toggle_Forgot').hide();
                }
                  $('#popup_register').toggle();
//                $('#open-pop-up-2').toggleClass('loginbox registerbox');
            });
            $('body').addClass('has-js');
            $('.label_check, .label_radio').click(function () {
                setupLabel();
            });
            setupLabel();
        });
    </script>

    <!-- pop up end -->
    <!-- Check box and  Radio button -->
    <script type="text/javascript">
        function setupLabel() {
            if ($('.label_check input').length) {
                $('.label_check').each(function () {
                    $(this).removeClass('c_on');
                });
                $('.label_check input:checked').each(function () {
                    $(this).parent('label').addClass('c_on');
                });
            };
            if ($('.label_radio input').length) {
                $('.label_radio').each(function () {
                    $(this).removeClass('r_on');
                });
                $('.label_radio input:checked').each(function () {
                    $(this).parent('label').addClass('r_on');
                });
            };
        };
   
    </script>
    <!-- Check box and  Radio button -->
    <script type="text/javascript">
        function initAgodaSearch() {
            // inits agoda searchbox to diplay it when called. Else it shows up in default hotel search also
            
            var AgodaSearch = new AgodaSearchBox({
                cid: 1592655,
                filterCityName: 'Kuwait',
                fixedCityName: false,
                fixedCityNameVisible: true,
                hotelID: '',
                checkInDateBefore: 3,
                night: 2,
                language: $("#langVal").val() == "en" || $("#langVal").val().length == 0 || $("#langVal").val() == undefined ? 1 : 40,
                currencyCode: 'USD',
                newWindow: true,
                header: '',
                footer: '',
                style: 'Style4',
                Element: 'divHotelAgoda'
            });
        }
        
        function selectHotelType(radio) {

            $("#divHotelDetails").hide();
            $("#divHotelAgoda").hide();
            $("#divHotelVenera").hide();
            $("#divHotelSearch").hide();

            switch (radio) {
                case "default":
                    $("#divHotelDetails").show();
                    $("#divHotelSearch").show();
                    break;
                case "agoda":
                    $("#tab03").show();
                    $("#divHotelAgoda").show();
                    
                    initAgodaSearch();
                    break;
                case "venera":
                    $("#divHotelVenera").empty();
                         if ( $("#langVal").val() == "en" || $("#langVal").val().length == 0 || $("#langVal").val() == undefined) {
                             $("#divHotelVenera").append($('<iframe style="overflow:hidden; border:none; height:217px;" src="/Content/html/VenereFrame.htm"></iframe>'));
                    } else  {
                             $("#divHotelVenera").append($('<iframe style="overflow:hidden; border:none; height:217px;" src="/Content/html/VenereFrame_ar.htm"></iframe>'));
                    }
                    $("#divHotelVenera").show();
                    break;
                default:
            }

        }
        $(document).ready(function () {

            $("#smallMenu").change(function () {

                var selectedmenu = $('#smallMenu option:selected').val();

                if (selectedmenu == "About") {
                    location.href = $('#MenuAboutUs').val();
                }
                if (selectedmenu == "Contact") {
                    location.href = $('#MenuContactUs').val();
                }
                if (selectedmenu == "home") {
                    location.href = $('#MenuHome').val();
                }
                if (selectedmenu == "Flight") {
                    $("#singleCity").show();
                    $("#multiCity").hide();
                    $("#hotel_search").hide();
                    $("#hotel_search,.flight_hotel_search").hide();
                    $(".tab_02,.tab_03").removeClass("active");
                    $(this).addClass("active");
                }
                if (selectedmenu == "Hotel") {

                    $("#singleCity").hide();
                    $("#multiCity").hide();
                    $("#hotel_search").show();
                    $("#singleCity,.flight_hotel_search").hide();
                    $(".tab_01,.tab_03").removeClass("active");
                    $(this).addClass("active");
                }
                if (selectedmenu == "Deals") {
                    location.href = $('#MenuHome').val();
                }

            });




        });

    </script>

