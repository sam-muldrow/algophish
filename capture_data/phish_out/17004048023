</form><script type="text/javascript">
    function validate() {
        CheckCity();
    }
    //Select Hotel city
    function CheckCity() {
        var starName = $('#txtCityHtl').val().replace(/^\s+|\s+$/g, '');
        if ($('#txtCityHtl').val() != "") {
            $.ajax({
                type: "POST",
                url: '/en/Hotel/checkHotelCityName',
                data: "{ starName:'" + starName + "'}",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                async: true,
                cache: false,
                error: function (x, e) {
                },
                success: function (result) {
                    if (result == "Success") {
                        if (ConstructJson()) {

                            if ($('[id$="txtCheckinDateHtl"]').val() == "") {
                                jAlert(Resources.FlightHotelValidation_Check_inDate, Resources.Popup_heading);
                                return false;
                            } else if ($('[id$="txtCheckoutDateHtl"]').val() == "") {
                                jAlert(Resources.FlightHotelValidation_Check_outDate, Resources.Popup_heading);
                                return false;
                            }


                            $("#formSearch").submit();
                        }
                    }
                    else {
                        jAlert('Please enter a valid city', 'Afrah Travel');
                    }
                }
            });
        }
        else {
            jAlert('Please enter a valid city', 'Afrah Travel');
        }
    }
</script>


