    <!-----------new field tab script--------------->
    <script type="text/javascript">
        $(document).ready(function () {

            $(".login").click(function () {
                $(".login span").css({ 'color': '#686868' });
                $(".login_popup").slideToggle("slow");

            });



            //main search tab

            $("#flight_tab").click(function () {
                $(".tabs").removeClass("active");
                $(this).addClass("active");
                $(".tab_bottom").hide();
                $("#tab01").show();
            });
            $("#hotel_tab").click(function () {
                $(".tabs").removeClass("active");
                $(this).addClass("active");
                $(".tab_bottom").hide();
                $("#tab02").show();
            });
            $("#hotel_1").click(function () {
                $(".tabs").removeClass("active");
                $(this).addClass("active");
                $(".tab_bottom").hide();
                $("#tab03").show();
                $("#tab02").hide();
                $("#tab01").hide();
                $("#tab04").hide();
            });
            $("#hotel_2").click(function () {
                $(".tabs").removeClass("active");
                $(this).addClass("active");
                $(".tab_bottom").hide();
                $("#tab04").show();
                $("#tab02").hide();
                $("#tab01").hide();
                $("#tab03").hide();
            });
           // tab04








            $(".room_tab_add").click(function () {
                $(".room_tab_add").removeClass("activeclass");
                $(this).addClass("activeclass");
            });


            $("#room01").click(function () {
                $(".room_details_area").hide();
                $("#room_details01").show();
            });



            $("#room02").click(function () {
                $(".room_details_area").hide();
                $("#room_details02").show();
            });




            $("#room03").click(function () {
                $(".room_details_area").hide();
                $("#room_details03").show();
            });



            $("#room04").click(function () {
                $(".room_details_area").hide();
                $("#room_details04").show();
            });



            $("#room05").click(function () {
                $(".room_details_area").hide();
                $("#room_details05").show();
            });




        });
        function change_field() {
            if ($('#roomno :selected').val() == 1) {
                $(".room_tab_add").hide();
                $("#room01").show();

            } else if ($('#roomno :selected').val() == 2) {
                $(".room_tab_add").hide();
                $("#room01").show();
                $("#room02").show();

            } else if ($('#roomno :selected').val() == 3) {
                $(".room_tab_add").hide();
                $("#room01").show();
                $("#room02").show();
                $("#room03").show();

            } else if ($('#roomno :selected').val() == 4) {
                $(".room_tab_add").hide();
                $("#room01").show();
                $("#room02").show();
                $("#room03").show();
                $("#room04").show();

            } else if ($('#roomno :selected').val() == 5) {
                $(".room_tab_add").hide();
                $("#room01").show();
                $("#room02").show();
                $("#room03").show();
                $("#room04").show();
                $("#room05").show();
            }
        }
    </script>
