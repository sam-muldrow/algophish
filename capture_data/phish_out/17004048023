    $(function () {
        $('#chkOneWayS').click(function () {
            if ($(this).is(':checked')) {
                $("#endate").hide();
            }
        });
        $('#chkRoundTripS').click(function () {
            if ($(this).is(':checked')) {
                $("#endate").show();
            }
        });
    });
</script>
