        $(document).ready(function () {
            $(".tab_01").click(function () {
                $("#singleCity").show();
                $("#multiCity").hide();
                $("#hotel_search").hide();
                $("#hotel_search,.flight_hotel_search").hide();
                $(".tab_02,.tab_03").removeClass("active");
                $(this).addClass("active");
            });
            $(".tab_02").click(function () {
                $("#singleCity").hide();
                $("#multiCity").hide();
                $("#hotel_search").show();
                $("#singleCity,.flight_hotel_search").hide();
                $(".tab_01,.tab_03").removeClass("active");
                $(this).addClass("active");
            });
            $(".tab_03").click(function () {
                $("#singleCity").hide();
                $("#multiCity").hide();
                $("#hotel_search").hide();
                $(".flight_hotel_search").show();
                $("#singleCity,.hotel_search").hide();
                $(".tab_01,.tab_02").removeClass("active");
                $(this).addClass("active");
            });
