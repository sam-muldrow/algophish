        <script type="text/javascript">
            $(document).ready(function () {
                function flightArea() {
                    $("#tab01").show();
                    $("#tab02").hide();
                    $("#hotel_tab").removeClass("active");
                    $("#flight_tab").addClass("active");
                }

                function hotelArea() {
                    $("#tab01").hide();
                    $("#tab02").show();
                    $("#flight_tab").removeClass("active");
                    $("#hotel_tab").addClass("active");
                }

                if ('tab1' == "tab1") {
                    flightArea();

                }
                else if ('tab1' == "tab2") {
                    hotelArea();

                }
                else if ('tab1' == "tab3") {
                    $(".tab_03").click();

                }
            });
    
        </script>

