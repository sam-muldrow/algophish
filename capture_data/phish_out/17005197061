        <script>
            $(document).ready(function(){
                var $first = $('.first-column-currency');
                var $second = $('.second-column-currency');
                var $largeLoad = $('.large-load');
                var $body =  $('body');
                var $linksBox = $('.exchange-links');

                var $firstTd = $('.first-td');
                var $secondTd = $('.second-td');
                //father change
                var $submit = $('.left-column-submit');
                var $left = $('.left-table-column');

                $submit.click(function(){
                    var $this = $(this);
                    $submit.removeClass('active');
                    $this.addClass('active');
                    $left.addClass('hidden');
                    $left.each(function(){
                        var $thisLeft = $(this);
                        if($thisLeft.attr('data-id') == $this.attr('data-id')) $thisLeft.removeClass('hidden');
                    })
                });

                res();

                //first currency
                function largeLoad(){
                    $largeLoad.removeClass('hidden');
                }

                setInterval(function(){
                    $('.menu-box').addClass('menu-load');
                    ajaxTableUpdate();
                },100000);

                function ajaxTableUpdate(){
                    var $secondCurrency = $('.second-column-currency.active');
                    var $firstCurrency = $('.first-column-currency.active');
                    var $menu = $('.menu-box');

                    if($secondCurrency.length > 0 && $firstCurrency.length > 0){
                        $('.p2p-change').remove();
                        var $name2 = $secondCurrency.attr('data-name');
                        $name2 = replaceSpaceToTitle($name2);
                        var $title2 = $secondCurrency.attr('data-title');
                        var $id2 = $secondCurrency.attr('data-id');
                        var $name1 = $firstCurrency.attr('data-name');
                        $name1 = replaceSpaceToTitle($name1);
                        var $title1 = $firstCurrency.attr('data-title');
                        var $id1 = $firstCurrency.attr('data-id');
                        var $mainTitle = '';
                        var $url = "/obmen-"+$name1+'-na-'+$name2;

                        window.history.pushState("", "", "/obmen-"+$name1+'-na-'+$name2);
                        var $inputIn = $('#amount-in');
                        var $inputInAmount = $inputIn.val();
                        var $inputOut = $('#amount-out');
                        var $inputOutAmount = $inputOut.val();

                        var $data = {'id1':$id1,'id2':$id2};
                        var $activeLi = $('.nav.nav-tabs li.active');
                        if($activeLi.hasClass('ajax-double'))
                            $data = {'id1':$id1,'id2':$id2,'double':true};

                        else if($activeLi.hasClass('ajax-p2p'))
                            $data = {'id1':$id1,'id2':$id2,'p2p':true};

                        $.ajax({
                            type:'POST',
                            url:'/site/ajax-table',
                            dataType:'html',
                            data:$data ,
                            success:function (request) {
                                $('.before-table').remove();
                                var $link = '<div class="before-table">' +
                                                '<div class="item-box">' +
                                                    '<a href="/obmen-'+replaceSpaceToTitle($name2)+'-na-'+replaceSpaceToTitle($name1)+'" ' +
                                                        ' title="обмен '+$title2+' на '+$title1+' " class="array-reverse"><img src="/image/arrowReverse.png"/> Обратний обмен</a>' +
                                                '</div>'+
                                            '</div>';
                                $('#transitions-block').addClass('hidden');
                                $menu.html(request);
                                $('.main-content-box').before($link);

                                res();
                                var $inputAmountBox = $('#amount-in');
                                var $inputAmountOutBox = $('#amount-out');
                                if($inputInAmount.length > 0){
                                    $inputAmountBox.val($inputInAmount);
                                    inputAmount($inputAmountBox);
                                }
                                if($inputOutAmount.length > 0){
                                    $inputAmountOutBox.val($inputOutAmount);
                                    inputAmount($inputAmountOutBox);
                                }

                                $mainTitle = 'Лучший обмен '+$firstCurrency.html()+' на '+$secondCurrency.html();
                                var $test = new Ya.Metrika({id: 25380812});
                                $test.hit($url, $mainTitle);

                            }
                        });

                        $.ajax({
                            type:'POST',
                            url:'/site/currency-info',
                            dataType:'json',
                            data:$data ,
                            success:function (request) {
                                var $head = $('head');
                                var $menuDef = $('.menu-default-content');
                                var $header = $('.menu-box-header');

                                if(request.content.length > 0){
                                    $menuDef.children('.content-info').children('.content').html(request.footerContent);
                                    $menuDef.children('.content-info').children('.title').children('h2').html(request.footerTitle);
                                    $menuDef.children('.content-info').removeClass('hidden');

                                    $header.children('.header-content').children('.title').children('h1').html(request.title);
                                    $header.children('.header-content').children('.content').addClass('content-close').html(request.content + '<span class="open-content">Показать описание полностью ↓</span>');
                                    $header.children('.header-content').removeClass('hidden');

                                    $menuDef.children('.default-content').addClass('hidden');
                                    $header.children('.default-header-content').addClass('hidden');

                                    $('meta[name=description]').remove();
                                    $head.append( '<meta name="description" content="'+request.description+'">' );
                                    $('meta[name=keywords]').remove();
                                    $head.append( '<meta name="keywords" content="'+request.keywords+'">' );
                                    $('title').remove();
                                    $head.append('<title>'+request.title+'</title>');
                                }
                                else{
                                    $menuDef.children('.default-content').children('.title').children('h2').html('Обмен '+$firstCurrency.text()+' на '+$secondCurrency.text());
                                    $menuDef.children('.default-content').removeClass('hidden');

                                    $header.children('.default-header-content').children('.title').children('h1').html('Обмен '+$firstCurrency.text()+' на '+$secondCurrency.text());
                                    $header.children('.default-header-content').removeClass('hidden');

                                    $menu.children('.content-info').addClass('hidden');
                                    $header.children('.header-content').addClass('hidden');

                                    $menuDef.children('.content-info').addClass('hidden');
                                    $menuDef.children('.default-content').removeClass('hidden');

                                    $('meta[name=description]').remove();
                                    $head.append( '<meta name="description" content="Лучший обмен '+$firstCurrency.html()+' на '+$secondCurrency.html()+'">' );
                                    $('meta[name=keywords]').remove();
                                    $head.append( '<meta name="keywords" content="Лучший обмен '+$firstCurrency.html()+' на '+$secondCurrency.html()+'">' );
                                    $('title').remove();
                                    $head.append( '<title>Лучший обмен '+$firstCurrency.html()+' на '+$secondCurrency.html()+'</title>');
                                }

                                $name1 = replaceTitleToSpace($name1);
                                $name2 = replaceTitleToSpace($name2);
                                $('.currency-out-select').val($name1);
                                $('.currency-in-select').val($name2);

                                $linksBox.html('<a href="/obmen-'+request.linkNext.next.in.title+'-na-'+request.linkNext.next.out.title+'"' +
                                    '>Обмен '+request.linkNext.next.in.name+' на '+request.linkNext.next.out.name+'</a><br />');
                                $linksBox.append('<a href="/obmen-'+request.linkNext.previous.in.title+'-na-'+request.linkNext.previous.out.title+'"' +
                                    '>Обмен '+request.linkNext.previous.in.name+' на '+request.linkNext.previous.out.name+'</a>');

                                smallReserve();
                                $menu.removeClass('menu-load');
                                $largeLoad.addClass('hidden');
                            }
                        })

                    }
                }


                $first.click(function(){
                    var $this = $(this);
                    zeroing();
                    $second.removeClass('can-be-active').removeClass('non-active');
                    $secondTd.removeClass('non-active');
                    $first.removeClass('active');
                    firstColumnLight($this);
                    var $firstCurrency = $('.first-column-currency.active');
                    var $secondCurrency = $('.second-column-currency.active');
                    if($secondCurrency.length > 0 && $firstCurrency.length > 0){
                        largeLoad();
                        ajaxTableUpdate();
                    }
                });

                function firstColumnLight($this){
                    $this.addClass('active');
                    var $children = $this.attr('data-children');
                    var $array = JSON.parse('['+$children+']');
                    $second.each(function(){
                        var $another = $(this);
                        var $id = $another.attr('data-id');
                        $.each($array,function($k,$v){
                            if($v == $id) $another.addClass('can-be-active');
                        });
                        if(!$another.hasClass('can-be-active')) $another.addClass('non-active');
                    });

                    $('.second-td').each(function(){
                        var $parent = $(this);
                        var $tdHidden = true;
                        $parent.children().each(function(){
                            var $children = $(this);
                            if($children.hasClass('second-column-currency') && !$children.hasClass('non-active'))
                                $tdHidden = false;
                        });
                        if($tdHidden)
                            $parent.addClass('non-active');
                    });
                }

                function zeroing(){
                    var $amountInInput = $('.amount-in-currency');
                    var $amountOutInput = $('.amount-out-currency');
                    $amountInInput.val('');
                    $amountOutInput.val('');
                }

                $second.click(function(){
                    var $this = $(this);
                    zeroing();

                    $first.removeClass('can-be-active').removeClass('non-active');
                    $firstTd.removeClass('non-active');

                    $second.removeClass('active');
                    $this.addClass('active');
                    secondColumnLight($this);
                    var $firstCurrency = $('.first-column-currency.active');
                    var $secondCurrency = $('.second-column-currency.active');
                    if($secondCurrency.length > 0 && $firstCurrency.length > 0){
                        largeLoad();
                        ajaxTableUpdate();
                    }
                });

                function secondColumnLight($this){
                    $first.removeClass('can-be-active').removeClass('non-active');
                    var $children = $this.attr('data-children');
                    var $array = JSON.parse('['+$children+']');
                    $first.each(function(){
                        var $another = $(this);
                        var $id = $another.attr('data-id');
                        $.each($array,function($k,$v){
                            if($v == $id) $another.addClass('can-be-active');
                        });
                        if(!$another.hasClass('can-be-active')) $another.addClass('non-active');
                    });
                    $('.first-td').each(function(){
                        var $parent = $(this);
                        var $tdHidden = true;
                        $parent.children().each(function(){
                            var $children = $(this);
                            if($children.hasClass('first-column-currency') && !$children.hasClass('non-active'))
                                $tdHidden = false;
                        });
                        if($tdHidden)
                            $parent.addClass('non-active');
                    });
                }

                $('.column-bottom-open').click(function(){
                    var $this = $(this);
                    var $open = $this.hasClass('this-important');
                    var $currencyBox = $('.currency-box');
                    var $status = 'true';
                    if($open){
                        $this.removeClass('this-important').addClass('this-non-important');
                        $currencyBox.removeClass('this-important').addClass('this-non-important');
                        $this.children('span').html('свернуть');
                        $status = 'false';
                    }
                    else{
                        $this.addClass('this-important').removeClass('this-non-important');
                        $currencyBox.addClass('this-important').removeClass('this-non-important');
                        $this.children('span').html('развернуть');
                    }

                    $.ajax({
                        'url':'/site/change-open-status',
                        'type':'POST',
                        'dataType':'json',
                        'data':{'status':$status}
                    })
                });

                //second column
                function clickNotTable($currencyTitleIn,$currencyTitleOut){
                    var $first = $('.first-column-currency');
                    $first.removeClass('active').removeClass('can-be-active').removeClass('non-active');
                    var $second = $('.second-column-currency');
                    $second.removeClass('active').removeClass('can-be-active').removeClass('non-active');
                    $first.each(function(){
                        var $this = $(this);
                        var $val = $this.attr('data-name');
                        $val = replaceSpaceToTitle($val);
                        if($val == $currencyTitleIn)
                            $this.click();
                    });
                    $second.each(function(){
                        var $this = $(this);
                        var $val = $this.attr('data-name');
                        $val = replaceSpaceToTitle($val);
                        if($val == $currencyTitleOut)
                            $this.click();
                    })
                }

                $('.currency-insert').on('change',function(){
                    var $currencyTitleIn = $('.currency-in-select.currency-insert.input-md.select2-offscreen').val();
                    $currencyTitleIn = replaceSpaceToTitle($currencyTitleIn);
                    var $currencyTitleOut = $('.currency-out-select.currency-insert.input-md.select2-offscreen').val();
                    $currencyTitleOut = replaceSpaceToTitle($currencyTitleOut);
                    if($currencyTitleIn.length > 0 || $currencyTitleOut.length > 0)
                        clickNotTable($currencyTitleIn,$currencyTitleOut);
                });

                function replaceSpaceToTitle($amount){
                    var $res = $amount.replace(' ','-');
                    if($res.indexOf(' ') > 0)
                        return replaceSpaceToTitle($res);
                    else
                        return $res;
                }

                function replaceTitleToSpace($amount){
                    var $res = $amount.replace('-',' ');
                    if($res.indexOf('-') > 0)
                        return replaceSpaceToTitle($res);
                    else
                        return $res;
                }

                //third currency
                $('.best-choice-td').click(function(){
                    var $this = $(this);
                    var $currencyTitleIn = $this.attr('data-currency-in');
                    $currencyTitleIn = replaceSpaceToTitle($currencyTitleIn);
                    var $currencyTitleOut = $this.attr('data-currency-out');
                        $currencyTitleOut = replaceSpaceToTitle($currencyTitleOut);
                    clickNotTable($currencyTitleIn,$currencyTitleOut);
                });

                //currency table
                $body.on('change','.amount',function(){
                    var $this=$(this);
                    inputAmount($this);
                });
                $('.menu-box').keyup(function(event){
                    var $box = $(this);
                    var $amountInInput = $('.amount-in-currency');
                    var $amountOutInput = $('.amount-out-currency');
                    if($amountInInput.is(':focus'))  $amountOutInput.val('');
                    else $amountInInput.val('');

                    var $in = $('#amount-in');
                    var $amountIn = $in.val();
                    if($amountIn.length > 0)inputAmount($in);

                    var $out = $('#amount-out');
                    var $amountOut = $out.val();
                    if($amountOut.length > 0) inputAmount($out);
                });
                $('.menu-box').click(function(event){
                        var $box = $(this);
                        var $com = $('.commission');
                        var $amountInInput = $('.amount-in-currency');
                        var $amountOutInput = $('.amount-out-currency');
                        if($amountInInput.is(':focus'))  $amountOutInput.val('');
                        else if ($amountOutInput.is(':focus'))  $amountInInput.val('');
                        if ($com.prop("checked")) {
                            $com.addClass('checked');
                        } else  {
                            $com.removeClass('checked');
                        }
                        var $in = $('#amount-in');
                        var $amountIn = $in.val();
                        if($amountIn.length > 0)inputAmount($in);

                        var $out = $('#amount-out');
                        var $amountOut = $out.val();
                        if($amountOut.length > 0) inputAmount($out);
                });
                function inputAmount($this) {
                    var $amount = $this.val();
                    var $com = $('.commission');
                    var $row = $('.row-table');
                    if($amount.match(/^\d+$/)) {
                        if ($com.hasClass('checked')) {
                            if ($this.attr('id') == 'amount-in') {
                                $row.each(function ($key, $val) {
                                    $amount = $this.val();
                                    var
                                        $minFee = $($val).find('.additionalMinFee').attr('data-currency-in'),
                                        $fromFee = $($val).find('.additionalFromFee').attr('data-currency-in'), //$amount
                                        $toFee = $($val).find('.additionalToFee').attr('data-currency-in'),
                                        $currency = $($val).find('td.amount-out-td').attr('data-currency-in'),
                                        $name = $($val).find('td.amount-out-td').attr('data-currency-name'),
                                        $fix = 0,
                                        $fixed = 0;
                                    if (!isNaN(parseFloat($minFee))) {
                                        if ($minFee.search($name)<0) {
                                            $amount = $amount - parseFloat($minFee);
                                        } else {
                                            $fix = parseFloat($minFee);
                                        }
                                    }
                                    if (!isNaN(parseFloat($fromFee))) {
                                        if ($fromFee.search('%')<0) {
                                            $amount = $amount - parseFloat($fromFee);
                                        } else {
                                            $amount = $amount - ($amount * parseFloat($fromFee) / 100);
                                        }
                                    }
                                    $fixed = $amount * $currency - $fix;
                                    if (!isNaN(parseFloat($toFee))) {
                                        if ($toFee.search('%')<0) {
                                            $fixed = $fixed - parseFloat($toFee);
                                        } else {
                                            $fixed = $fixed - ($fixed * parseFloat($toFee) / 100);
                                        }
                                    }
                                    if ($amount<0) {
                                        $amount = 0;
                                    }
                                    if ($fixed<0) {
                                        $fixed = 0;
                                    }
                                    $($val).find('td.amount-in-td .amount').html($amount);
                                    $($val).find('td.amount-out-td .amount').html(($fixed).toFixed(4));
                                });
                                $('#amount-out').val('');
                            }
                            else if ($this.attr('id') == 'amount-out') {
                                $row.each(function ($key, $val) {
                                    var
                                        $minFee = $($val).find('.additionalMinFee').attr('data-currency-in'),
                                        $toFee = $($val).find('.additionalFromFee').attr('data-currency-in'), //$amount
                                        $fromFee = $($val).find('.additionalToFee').attr('data-currency-in'),
                                        $currency = $($val).find('td.amount-out-td').attr('data-currency-in'),
                                        $name = $($val).find('td.amount-out-td').attr('data-currency-name'),
                                        $fix = 0,
                                        $fixed = 0;
                                    if (!isNaN(parseFloat($minFee))) {
                                        if ($minFee.search($name)<0) {
                                            $fix = parseFloat($minFee);
                                        } else {
                                            $fix = parseFloat($minFee) / $currency ;
                                        }
                                    }

                                    if (!isNaN(parseFloat($fromFee))) {
                                        if ($fromFee.search('%')<0) {
                                            $fix = $fix + parseFloat($minFee) / $currency;
                                        } else {
                                            $fix = $fix + ($amount * parseFloat($fromFee) / 100) / $currency;
                                        }
                                    }
                                    if (!isNaN(parseFloat($toFee))) {
                                        if ($toFee.search('%')<0) {
                                            $fix = $fix + parseFloat($toFee);
                                        } else {
                                            $fix = $fix + ($fixed * parseFloat($toFee) / 100);
                                        }
                                    }
                                    $fixed = $amount / $currency + $fix;
                                    if ($fixed<0) {
                                        $fixed = 0;
                                    }
                                    $($val).find('td.amount-in-td .amount').html(($fixed).toFixed(4));
                                    $($val).find('td.amount-out-td .amount').html($amount);
                                });
                                $('#amount-in').val('');
                            }
                        }
                        else {
                                if ($this.attr('id') == 'amount-in') {
                                    $row.each(function ($key, $val) {
                                        var $currency = $($val).find('td.amount-out-td').attr('data-currency-in');
                                        $($val).find('td.amount-in-td .amount').html($amount);
                                        $($val).find('td.amount-out-td .amount').html(($amount * $currency).toFixed(4));
                                    });
                                    $('#amount-out').val('');
                                }
                                else if ($this.attr('id') == 'amount-out') {
                                    $row.each(function ($key, $val) {
                                        var $currency = $($val).find('td.amount-out-td').attr('data-currency-in');
                                        $($val).find('td.amount-in-td .amount').html(($amount / $currency).toFixed(4));
                                        $($val).find('td.amount-out-td .amount').html($amount);
                                    });
                                    $('#amount-in').val('');
                                }
                        }
                        smallReserve();
                    }
                    else
                    {
                        var $in = $('#amount-in');
                        alert('Проверьте на правильность ввода поля! Поля поддерживают только числа');
                        $('#amount-out').val('');
                        $in.val(1);
                        inputAmount($in);
                    }
                }

                function replaceSpace($amount){
                    var $res = $amount.replace(' ','');
                    if($res.indexOf(' ') > 0)
                        return replaceSpace($res);
                    else
                        return $res;
                }

                function res(){
                    var $sum = parseFloat(0);
                    var $sumA = parseFloat(0);
                    var $k=0;
                    var $val = parseFloat(0);
                    var $valA = parseFloat(0);

                    var $currencyName = $('.second-column-currency.active').html();
                    if(typeof $currencyName == 'undefined') $currencyName = $('.currencyName2').html();

                    var $sumBalance = parseFloat(0);
                    var $sumBalanceVal = parseFloat(0);
                    if(typeof $currencyName != 'undefined'){
                        $('.information-table .reserve-amount').each(function()
                        {
                            $k++;
                            $val = replaceSpace($(this).find('.color-dark').html());
                            $val = $val.replace(',','.');
                            $sum = (parseFloat($sum) + + parseFloat($val)).toFixed(4);

                            var $amountBox = $(this).siblings('.amount-out-td');
                            $sumBalanceVal = replaceSpace($amountBox.attr('data-currency-in'));
                            $sumBalance = (parseFloat($sumBalance) + + (parseFloat($sumBalanceVal)*parseFloat($val))).toFixed(4);
                        });

                        var $box = $('.m-hint');
                        $box.html('<div class="bottom"></div>'+
                                    '<span title="Статистика по обменникам Яндекс.Деньги - WMB">Всего по направлению работает <span style="font-weight: bold" class="bt-count">'+$k+'</span> '+findByExchanger($k)+'.'+
                                    '<br>Суммарный резерв обменников: <span style="font-weight: bold" class="bt">'+$sum+'</span> <span class="currencyName">'+$currencyName+'</span>.'+
                                    '<span title="Средний арифметический взвешенный курс">Средневзвешенный курс обмена: <span style="font-weight: bold" class="bt-currency">'+
                                        parseFloat($sumBalance/$sum).toFixed(4)+' '+$currencyName+'</span></span>'+
                                  '</span>');
                        $box.removeClass('hidden');
                    }
                }

                function findByExchanger($key){
                    $key = parseInt($key);
                    var $res = '';
                    var $key1 = $key%10;

                    if($key1 == 1)
                        $res = "надежный обменный пункт";
                    else if($key1 < 5 )
                        $res = 'надежных обменных пункта';
                    else
                        $res = 'надежных обменных пунктов';
                    return $res;
                }

                function smallReserve(){
                    var $tr = $('.information-table.main-table tr.row-table');
                    $tr.removeClass('small-balance');
                    $tr.each(function(){
                        var $this = $(this);
                        var $amount = $this.children('.amount-out-td').children('noindex').children('a').children('.amount').html();
                        var $balance = $this.children('.reserve-amount').children('noindex').children('a').children('.color-dark').html();
                        $balance = replaceSpace($balance);
                        $amount = replaceSpace($amount);
                        $balance = parseFloat($balance.replace('.',','));
                        $amount = parseFloat($amount.replace('.',','));
                        if($amount > $balance)
                            $this.addClass('small-balance');

                    });
                }

                //default currency
                
                //double
                $body.on('click','.row-table',function(){
                    var $row = $('.row-table');
                    var $prev = $('.about-prev');
                    var $this = $(this);
                    var $id = $this.attr('data-table');
                    var $subTitle = $('.about'+$id);
                    if($this.hasClass('active')){
                        $this.removeClass('active');
                        $prev.addClass('hidden');
                    }
                    else{
                        $row.removeClass('active');
                        $prev.addClass('hidden');
                        $this.addClass('active');
                        $subTitle.removeClass('hidden');
                    }
                });
            });
        </script>
            <script>
                $('body').on('click','.nav.nav-tabs li',function(){
                    var $this = $(this);
                    var $loading = $('.information-table tr td .loading');
                    if($this.hasClass('ajax-double') && $loading.hasClass('loading')){
                        var $in = $this.attr('data-currency-in');
                        var $out = $this.attr('data-currency-out');

                        var $data = {'currencyIn':$in,'currencyOut':$out};
                        $.ajax({
                            type:'POST',
                            url:'/site/load-double-ajax',
                            dataType:'html',
                            data:$data ,
                            success:function (request){
                                var $double = $('.double-for-replace');
                                $double.html(request);
                            }
                        });
                    }
                });
            </script>

        </div>
</div>        </div>
