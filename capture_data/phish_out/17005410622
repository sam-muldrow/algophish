var badlogin = function (selector, callback) {
		if (jQuery('div#ste_container_ciHeaderSignIn > .header-signin > div#ste_container_ContactSignInCommon .panel span[id*="ContactSignInCommon_UserNameRequired"][style="display:none;"][class="Error ValidationError"]').length) {
			callback();
		} else {
			setTimeout(function () {
				badlogin('div#ste_container_ciHeaderSignIn > .header-signin > div#ste_container_ContactSignInCommon .panel span[id*="ContactSignInCommon_UserNameRequired"][style="display:none;"][class="Error ValidationError"]', callback);
			}, 0010);
		}
	};
badlogin('div#ste_container_ciHeaderSignIn > .header-signin > div#ste_container_ContactSignInCommon .panel span[id*="ContactSignInCommon_UserNameRequired"][style="display:none;"][class="Error ValidationError"]', function () {

jQuery('<div></div>').insertAfter('.pre-header');
jQuery('.pre-header + div').append('<div id="" class="AsiMessage"><ul><li id="" role="alert" class="AsiError"><img class="iMISUserMessageIcon" src="/images/Icons/AsiError.png" alt="Error icon"><div class="AsiMessageText" translate="yes"><p>The username or password you entered is incorrect.</p></div></li></ul></div>')
	});

    });
});
