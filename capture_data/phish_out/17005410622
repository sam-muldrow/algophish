// function to enable sticky navigation 
	//-----------------------------
	
		
	// Sticky Navigation START
	window.onscroll = function () {
		scrollNav()
	};
	// on scroll add classes to nav at certain locations
	function scrollNav() {
		if (window.pageYOffset >= 200) {
			jQuery('header#hd').addClass("prepresticky");
		} else {
			jQuery('header#hd').removeClass("prepresticky");
		}
		if (window.pageYOffset >= 300) {
			jQuery('header#hd').addClass("presticky");
		} else {
			jQuery('header#hd').removeClass("presticky");
		}
		if (window.pageYOffset >= 400) {
			jQuery('header#hd').addClass("sticky");
		} else {
			jQuery('header#hd').removeClass("sticky");
		}
	}
		
var navHeight = jQuery('header#hd').height(); 
 
		jQuery('header#hd + div[id*="PageHeading"]').css("margin-top", navHeight + "px");
		jQuery('header#hd + div#HomePageContent + div#masterContentArea').css("margin-top", navHeight + "px");
jQuery('header#hd:not(.sticky) .header-bottom-container .RadMenu.RadMenu_Causeis .rmRootGroup > .rmItem > .rmLink + .rmSlide').css("margin-top", navHeight - 0 + "px");
		jQuery('header#hd.sticky .header-bottom-container .RadMenu.RadMenu_Causeis .rmRootGroup > .rmItem > .rmLink + .rmSlide').css("margin-top", navHeight - 0 + "px");
				jQuery('header#hd +  div#internalPageContentAreaBanner + div#masterContentArea').css("margin-top", navHeight - 0 + "px");
				jQuery('header#hd  + HeaderRow + div#masterContentArea').css("margin-top", navHeight - 0 + "px");
				jQuery('header#hd  + div#masterContentArea').css("margin-top", navHeight - 0 + "px");
// observe changes to elements
		(function (jQuery) {
			var MutationObserver = window.MutationObserver || window.WebKitMutationObserver || window.MozMutationObserver;
			jQuery.fn.attrchange = function (callback) {
				if (MutationObserver) {
					var options = {
						subtree: false,
						attributes: true
					};
					var observer = new MutationObserver(function (mutations) {
						mutations.forEach(function (e) {
							callback.call(e.target, e.attributeName);
						});
					});
					return this.each(function () {
						observer.observe(this, options);
					});
				}
			}
		})(jQuery);	
 jQuery('header#hd').attrchange(function(attrName) {
        // which attribute do you want to observe
        if (attrName == 'class') {
		//apply the height as a margin-top to the PageHeading ipart, or master content area
var navHeight = jQuery('header#hd').height();
		jQuery('header#hd.sticky + div[id*="PageHeading"]').css("top", navHeight + "px");
		jQuery('header#hd.sticky + div#HomePageContent + div#masterContentArea').css("top", navHeight + "px");
		jQuery('header#hd').css("top", "0" + "px");
		jQuery('header#hd.prepresticky').css("top", "-" + navHeight + "px");
	jQuery('header#hd.presticky').css("top", "-" + navHeight + "px");
jQuery('header#hd.sticky').css("top", "0" + "px");
jQuery('header#hd:not(.sticky) .header-bottom-container .RadMenu.RadMenu_Causeis .rmRootGroup > .rmItem > .rmLink + .rmSlide').css("margin-top", navHeight - 0 + "px");
		jQuery('header#hd.sticky .header-bottom-container .RadMenu.RadMenu_Causeis .rmRootGroup > .rmItem > .rmLink + .rmSlide').css("margin-top", navHeight - 0 + "px");
		
					jQuery('header#hd +  div#internalPageContentAreaBanner + div#masterContentArea').css("margin-top", navHeight - 0 + "px");
				jQuery('header#hd  + HeaderRow + div#masterContentArea').css("margin-top", navHeight - 0 + "px");
				jQuery('header#hd  + div#masterContentArea').css("margin-top", navHeight - 0 + "px");
	  }
    });
	// Sticky Navigation END

	jQuery(".noclick").removeAttr("href");


jQuery('.header-bottom-container').bind("mouseover", "touchstart", function (e) {
	// no click primary nav - ipad navigation START
	// ipad indicator and solution to ipad drop down problem
	// requires setup of desktop indicator - see end of document
	// "ipad indicator" is at the bottom as html and css
	// check the size of window on load and resize
	// run test on initial page load
	checkSize();
	// run test on resize of the window
	jQuery(window).resize(checkSize);
	// Function for the css rule
	// if ipad indicator is visible
	function checkSize() {
		if (jQuery("#ipad-indicator").css("display") == "inline-block") {
			// target each nav ul li item
			jQuery(".RadMenu.RadMenu_Causeis .rmRootGroup > .rmItem > .rmLink").each(function () {
				// when top level nav item is "touched"
				jQuery(this).bind("touchstart", function (e) {
					if (jQuery(this).parent().hasClass("ipadexpand")) {
						jQuery(this).parent().toggleClass('ipadexpand');
					} else {
						jQuery(".RadMenu.RadMenu_Causeis .rmRootGroup > .rmItem").removeClass('ipadexpand');
						// remove parent class that is not yet applied
						// add class clicked to nav ul li item
						jQuery(this).parent().addClass('ipadexpand');
						// if nav ul li item is clicked, add class that activates drop down, otherwise remove drop down activation
					}
					// remove class that activates drop down from all each time there is a touch - this ensures that the most recent clicked item is cleared after another nav item is touched
				});
			});
		}
	}
});

jQuery(".RadMenu.RadMenu_Causeis .rmTwoLevels.rmItem .rmLink span.rmToggle").each(function () {
	// when top level nav item is "touched"
	jQuery(this).children('span.rmIcon').bind("mousedown", "touchstart", function (e) {
		// remove class that activates drop down from all each time there is a touch - this ensures that the most recent clicked item is cleared after another nav item is touched
		// remove parent class that is not yet applied
		jQuery(this).parent().parent().parent().toggleClass('clicked');
		// add class clicked to nav ul li item
		// if nav ul li item is clicked, add class that activates drop down, otherwise remove drop down activation
	});
});
jQuery('div#masterContentArea').bind("mouseover", "touchstart", function (e) {
	jQuery("li.rmItem > div.rmSlide > ul > li.rmItem.clicked").removeClass('clicked');
jQuery(".RadMenu.RadMenu ul.rmHorizontal > li.rmItem.ipadexpand").removeClass('ipadexpand');

});

 


