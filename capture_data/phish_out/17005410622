// wait for navigation to fully loaded
var waitForEl = function (selector, callback) {
	if (jQuery('ul.rmRootGroup.rmToggleHandles.rmHorizontal').length) {
		callback();
	} else {
		setTimeout(function () {
			waitForEl('ul.rmRootGroup.rmToggleHandles.rmHorizontal', callback);
		}, 100);
	}
};
waitForEl('ul.rmRootGroup.rmToggleHandles.rmHorizontal', function () {
	// once it is
	// check that profile iqa is loaded
	var profileiqa = function (selector, callback) {
		if (jQuery('div#ste_container_ciLogindetails .RadGrid.RadGrid_MetroTouch table').length) {
			callback();
		} else {
			setTimeout(function () {
				profileiqa('div#ste_container_ciLogindetails .RadGrid.RadGrid_MetroTouch table', callback);
			}, 0010);
		}
	};
	// once it is
	profileiqa('div#ste_container_ciLogindetails .RadGrid.RadGrid_MetroTouch table', function () {
		// if user is on a mobile
		if (jQuery("#mobile-indicator").css("display") == "inline-block") {
			// move elements or remove iqa for logged in users
			// if user is not logged in
			var isAnonymous = JSON.parse(jQuery("#__ClientContext").val()).isAnonymous;
			if (isAnonymous) {

 

				//remove link from top log in button 
				jQuery('a.UtilitySection.UtilityAuthenticationLink').removeAttr('href');
				// when it is clicked
				jQuery('a.UtilitySection.UtilityAuthenticationLink').bind("mousedown", "touchstart", function (e) {
					// toggle the collapabe sign in ipart
					jQuery('.header-signin .panel-heading.Distinguish a.panel-heading-collapse[data-toggle="collapse"]').click();
				});
				//remove the iqa		
				// jQuery('div#ste_container_ciLogindetails').remove();
jQuery('div#ste_container_ciLogindetails').insertAfter('div#ste_container_ciUtilityNavigationThree');
				console.log("user is logged out");
			}




 else {
				// if user is logged in
jQuery('header#hd > .pre-header > div#preheader-bar > div').addClass('noflex');
				// remove login message from iqa
				jQuery('div#ste_container_ciLogindetails .RadGrid.RadGrid_MetroTouch table > tbody > tr >td > .loggedout').hide();
				// prepends profile image from footer to top nav
				jQuery('div[id*="ProfilePic_ResultsGrid"] img').clone().prependTo('div#ste_container_ciLogindetails .RadGrid.RadGrid_MetroTouch table > tbody > tr >td > .loggedin .image');
				// move username and photo to preheader
				jQuery('div#ste_container_ciLogindetails .RadGrid.RadGrid_MetroTouch table > tbody > tr >td > .loggedin .user').clone().insertAfter('header#hd > .pre-header > .preheader-bar-container a.UtilitySection.UtilityAuthenticationLink');
				// move profile details and photo before first nav item 
				jQuery('div#ste_container_ciLogindetails .RadGrid.RadGrid_MetroTouch table > tbody > tr >td > .loggedin').insertBefore('.RadMenu.RadMenu_Causeis .rmRootGroup > .rmItem.rmFirst');
				// add sign out link to details 
				jQuery('header#hd > .pre-header > .preheader-bar-container a.UtilitySection.UtilityAuthenticationLink').clone().insertAfter('header#hd #preheader-bar > div > div:nth-of-type(3) .user .welcome');
				// remove the iqa
				jQuery('div#ste_container_ciLogindetails').remove();
jQuery('div#ste_container_ciLogindetails').insertAfter('div#ste_container_ciUtilityNavigationThree');
				console.log("user is logged in");
			}
			console.log("user is on a mobile");
		}
		// if user is not on a mobile
		else {
			// if user is not logged in
			var isAnonymous = JSON.parse(jQuery("#__ClientContext").val()).isAnonymous;
			if (isAnonymous) {

				//remove the iqa		
				jQuery('div#ste_container_ciLogindetails .RadGrid.RadGrid_MetroTouch table > tbody > tr >td > .loggedin').hide();
		jQuery('<img id="login" src="/images/Icons/svg/profile.svg" />').prependTo('div#ste_container_ciLogindetails .RadGrid.RadGrid_MetroTouch table > tbody > tr >td > .loggedout .image');
	

jQuery('header#hd > .pre-header > .preheader-bar-container > div> div:nth-child(4)').remove()

				console.log("user is logged out");

			} else {
				// if user is logged in
				// remove login message from iqa
jQuery('div#ste_container_ciLogindetails .RadGrid.RadGrid_MetroTouch table > tbody > tr >td > .loggedout').hide();
		// prepends image from footer to top nav
		jQuery('div[id*="ProfilePic_ResultsGrid"] img').clone().prependTo('div#ste_container_ciLogindetails .RadGrid.RadGrid_MetroTouch table > tbody > tr >td > .loggedin .image');
	
				console.log("user is logged in");
			}
			console.log("user is on a desktop");
		}
		console.log("profile iqa is loaded");
