<script>

jQuery(document).ready(function () {
	jQuery('#Accessibility-button').on('click', function () {
		jQuery('button#INDmenu-btn').click();
	});
	setTimeout(function () {
		// Remove entries from language selector, refresh page when English is clicked
		jQuery('.goog-te-menu-frame').contents().find('a.goog-te-menu2-item span.text').each(function () {
						if (!jQuery(this).is(':contains("French"),:contains("Chinese (Simplified)"), :contains("English"), :contains("German"), :contains("Italian"), :contains("Greek"), :contains("Japanese"), :contains("Spanish"), :contains("Lao"), :contains("Malay"), :contains("Arabic")')) {
				jQuery(this).parent().parent().remove();
			}
		});
jQuery('.goog-te-menu-frame').contents().find('a.goog-te-menu2-item span.text').each(function () {
						if (jQuery(this).is(':contains("Malayalam")')) {
				jQuery(this).parent().parent().remove();
			}
		});
		// Remove entries from language selector, when the title attr is updated - from switching a language 
		(function (jQuery) {
			var MutationObserver = window.MutationObserver || window.WebKitMutationObserver || window.MozMutationObserver;
			jQuery.fn.attrchange = function (callback) {
				if (MutationObserver) {
					var options = {
						subtree: false,
						attributes: true
					};
					var observer = new MutationObserver(function (mutations) {
						mutations.forEach(function (e) {
							callback.call(e.target, e.attributeName);
						});
					});
					return this.each(function () {
						observer.observe(this, options);
					});
				}
			}
		})(jQuery);
		jQuery('.goog-te-menu-frame').each(function () {
			// which element do you want to observe
			jQuery(this).attrchange(function (attrName) {
				// which attribute do you want to observe
				if (attrName == 'title') {
					// do something
					jQuery('.goog-te-menu-frame').contents().find('a.goog-te-menu2-item span.text').each(function () {
						// if items are not the below
						if (!jQuery(this).is(':contains("French"),:contains("Chinese (Simplified)"), :contains("English"), :contains("German"), :contains("Italian"), :contains("Greek"), :contains("Japanese"), :contains("Spanish"), :contains("Lao"), :contains("Malay"), :contains("Arabic")')) {
							// remove them 
							jQuery(this).parent().parent().remove();
						} else {
							// do nothing
						}
					});
jQuery('.goog-te-menu-frame').contents().find('a.goog-te-menu2-item span.text').each(function () {
						if (jQuery(this).is(':contains("Malayalam")')) {
				jQuery(this).parent().parent().remove();
			}
		});
				}
			});
		});
		// check if the language has been chnaged to english
		jQuery('div#ste_container_googletranslate').on('click', function () {
			jQuery('.isItEnglish').each(function () {
				if (jQuery('html.isItEnglish[lang="en"]').length) {
					//  jQuery.removeCookie("googtrans");
					jQuery('iframe.goog-te-menu-frame').remove();
					setTimeout(function () {
						location.reload();
					}, 0300);
				}
			});
			jQuery('body#MainBody').parent().addClass('isItEnglish');
