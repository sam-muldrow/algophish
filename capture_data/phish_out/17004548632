<script type="text/javascript">
  $(document).ready(function() {
    $(".gallery-thumb").on("click", function() {
      var idGaleria = $(this).data("galeria");
      $.get( "./includes/ajax/galeria.php?id=" + idGaleria, function( data ) {
        var items = JSON.parse(data);
        $.fancybox.open(items, {
          loop : true,
          clickOutside: "close",
        });
      });
    });
  });
</script>
