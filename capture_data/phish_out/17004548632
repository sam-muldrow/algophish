		$(function(){
		    var md = new MobileDetect(window.navigator.userAgent);
		    if(md.phone()){
		    	$("body").addClass('is_phone');
		    }
		    if(md.tablet()){
		    	$("body").addClass('is_tablet');
		    }
		    if(md.mobile()){
		    	$("body").addClass('is_mobile');
		    }

		});
	</script>


	
