  resizeContent();
});

function resizeContent() {
	setTimeout(
	function() {
		$height = jQuery("#top-long .tf-module .moduletable_products .owl-item").height();
		jQuery('#top-long .tf-module .moduletable_style8').height($height);
	}, 500);
}

