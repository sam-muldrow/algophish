    checkIgnore: function checkIgnore(isUncaught, args, payload) {
    // Only ingore error if message is "ResizeObserver loop limit exceeded"
    // Link to Rollbar error page: https://rollbar.com/Typeform/public-main-site/items/22/?person_page=0&#traceback
    // TODO: Remove this block once error has been solved in Ginger
    var RESIZE_OBSERVER_ERROR_MESSAGE = "ResizeObserver loop limit exceeded";
    var MINIFIED_REACT_ERROR_418 = "Minified React error #418";
    var MINIFIED_REACT_ERROR_423 = "Minified React error #423";
