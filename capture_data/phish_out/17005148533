<script type="text/javascript">
  
    $("#txtSearch").focus(function () {
        var defaultvalue = 'Nhập từ khóa tìm kiếm ...';
        if ($(this).val() == defaultvalue) {
            $(this).val("");
        };
    });
    $("#txtSearch").blur(function () {
        var defaultvalue = 'Nhập từ khóa tìm kiếm ...';
        if (!$(this).val()) {
            $(this).val(defaultvalue);
        };
    });
    
