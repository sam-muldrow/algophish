	<h1 style="display: none">CÔNG TY CỔ PHẨN TƯ VẤN ĐẦU TƯ VÀ THIẾT KẾ XÂY DỰNG VIỆT NAM</h1>
   <div class="Slide_PC2">
       

<link href="/Controls/Adv/AdvSlideResponesiveHome/css.css" rel="stylesheet" />
<script src="https://cdcjsc.vn/Controls/Gallery/jquery.touchwipe.1.1.1.js"></script>


<div id="AdvSlideResponesiveHome">  
    <a href="javascript:void(0)" class="prev"></a>
    <div class="Baocontent">
         <a data-thutu='thutudau' class='current itemSlide itemSlidethutudau' style='opacity:1;z-index:10;background: url(https://cdcjsc.vn//FileUpload/Images/artboard_1.jpg)' href='javascript:void(0)'  target='_self     '> <span class='caption_title_line'><h2>12 bo nganh 2</h2></span></a><a data-thutu='' class=' itemSlide itemSlide' style='opacity:0;z-index:-1;background: url(https://cdcjsc.vn//FileUpload/Images/artboard_2.jpg)' href='javascript:void(0)'  target='_self     '> <span class='caption_title_line'><h2>12 bo nganh 1</h2></span></a><a data-thutu='' class=' itemSlide itemSlide' style='opacity:0;z-index:-1;background: url(https://cdcjsc.vn//FileUpload/Images/g11.jpg)' href='javascript:void(0)'  target='_self     '> <span class='caption_title_line'><h2>VOV</h2></span></a><a data-thutu='' class=' itemSlide itemSlide' style='opacity:0;z-index:-1;background: url(https://cdcjsc.vn//FileUpload/Images/bo_ngoai_giao.jpg)' href='javascript:void(0)'  target='_self     '> <span class='caption_title_line'><h2>Bo Ngoai Giao</h2></span></a><a data-thutu='' class=' itemSlide itemSlide' style='opacity:0;z-index:-1;background: url(https://cdcjsc.vn//FileUpload/Images/1shop_cam_16_294_copy.jpg)' href='javascript:void(0)'  target='_self     '> <span class='caption_title_line'><h2>Van phong chinh phu</h2></span></a><a data-thutu='' class=' itemSlide itemSlide' style='opacity:0;z-index:-1;background: url(https://cdcjsc.vn//FileUpload/Images/13pctt__night.png)' href='javascript:void(0)'  target='_self     '> <span class='caption_title_line'><h2>tay do</h2></span></a><a data-thutu='' class=' itemSlide itemSlide' style='opacity:0;z-index:-1;background: url(https://cdcjsc.vn//FileUpload/Images/main.jpg)' href='javascript:void(0)'  target='_self     '> <span class='caption_title_line'><h2>Thap tai chinh quoc te</h2></span></a><a data-thutu='' class=' itemSlide itemSlide' style='opacity:0;z-index:-1;background: url(https://cdcjsc.vn//FileUpload/Images/cdc1.jpg)' href='javascript:void(0)'  target='_self     '> <span class='caption_title_line'><h2>Trung tam hoi nghi quoc gia</h2></span></a><a data-thutu='' class=' itemSlide itemSlide' style='opacity:0;z-index:-1;background: url(https://cdcjsc.vn//FileUpload/Images/cdc3.jpg)' href='javascript:void(0)'  target='_self     '> <span class='caption_title_line'><h2>Nha Quoc hoi 2</h2></span></a><a data-thutu='' class=' itemSlide itemSlide' style='opacity:0;z-index:-1;background: url(https://cdcjsc.vn//FileUpload/Images/cdc2.jpg)' href='javascript:void(0)'  target='_self     '> <span class='caption_title_line'><h2>Nha Quoc hoi</h2></span></a><a data-thutu='thutucuoi' class=' itemSlide itemSlidethutucuoi' style='opacity:0;z-index:-1;background: url(https://cdcjsc.vn//FileUpload/Images/cdc4.jpg)' href='javascript:void(0)'  target='_self     '> <span class='caption_title_line'><h2>Trung tam the duc the thao Vinh Phuc</h2></span></a>
    </div>
     <a href="javascript:void(0)" class="next"></a>
</div>

<script type="text/javascript">
   
    $("#AdvSlideResponesiveHome .Baocontent").touchwipe({
        wipeLeft: function () { VuotTrai(); },
        wipeRight: function () { VuotPhai(); },
        min_move_x: 20,
        min_move_y: 20,
        preventDefaultEvents: false
    });

    function VuotTrai() {
        var vitri = $("#AdvSlideResponesiveHome .Baocontent .current").attr("data-thutu");
        if (vitri == "thutudau") {
            $("#AdvSlideResponesiveHome .Baocontent .itemSlidethutucuoi").addClass("current");
            $("#AdvSlideResponesiveHome .Baocontent .itemSlidethutudau").removeClass("current");
            $("#AdvSlideResponesiveHome .Baocontent .itemSlidethutucuoi").animate({ "opacity": 1, "z-index": 10 }, 1000);
            $("#AdvSlideResponesiveHome .Baocontent .itemSlidethutudau").animate({ "opacity": 0, "z-index": -1 }, 1000);
        }
        else {
            $("#AdvSlideResponesiveHome .Baocontent .current").each(function () {
                $(this).prev().addClass("current");
                $(this).removeClass("current");
                $(this).prev().animate({ "opacity": 1, "z-index": 10 }, 1000);
                $(this).animate({ "opacity": 0, "z-index": -1 }, 1000);
            });
        }
    }
    
    function VuotPhai() {
        var vitri = $("#AdvSlideResponesiveHome .Baocontent .current").attr("data-thutu");
        if (vitri == "thutucuoi") {
            $("#AdvSlideResponesiveHome .Baocontent .itemSlidethutudau").addClass("current");
            $("#AdvSlideResponesiveHome .Baocontent .itemSlidethutucuoi").removeClass("current");
            $("#AdvSlideResponesiveHome .Baocontent .itemSlidethutudau").animate({ "opacity": 1, "z-index": 10 }, 1000);
            $("#AdvSlideResponesiveHome .Baocontent .itemSlidethutucuoi").animate({ "opacity": 0, "z-index": -1 }, 1000);
        }
        else {
            $("#AdvSlideResponesiveHome .Baocontent .current").each(function () {
                $(this).next().addClass("current");
                $(this).removeClass("current");
                $(this).next().animate({ "opacity": 1, "z-index": 10 }, 1000);
                $(this).animate({ "opacity": 0, "z-index": -1 }, 1000);
            });
        }
    }

    $('#AdvSlideResponesiveHome .prev').hover(function () { //mouse enter
        $("#AdvSlideResponesiveHome").addClass('pauseInterval');
        },function() { //mouse leave
            $("#AdvSlideResponesiveHome").removeClass('pauseInterval');
        }
    );
    
    $('#AdvSlideResponesiveHome .next').hover(function () { //mouse enter
        $("#AdvSlideResponesiveHome").addClass('pauseInterval');
    }, function () { //mouse leave
        $("#AdvSlideResponesiveHome").removeClass('pauseInterval');
    }
    );

    $(document).ready(function () {
        var biencounthome = $("#AdvSlideResponesiveHome .Baocontent .itemSlide").size();
        if (biencounthome > 1) {
            SliderHome();
		}     		
    });
    // Auto chay slide
    function SliderHome() {
        setInterval(function () {
            if (!$('#AdvSlideResponesiveHome').hasClass('pauseInterval')) {
                var vitri = $("#AdvSlideResponesiveHome .Baocontent .current").attr("data-thutu");
                if (vitri == "thutucuoi") {
                    $("#AdvSlideResponesiveHome .Baocontent .itemSlidethutudau").addClass("current");
                    $("#AdvSlideResponesiveHome .Baocontent .itemSlidethutucuoi").removeClass("current");
                    $("#AdvSlideResponesiveHome .Baocontent .itemSlidethutudau").animate({ "opacity": 1, "z-index": 10 }, 1000);
                    $("#AdvSlideResponesiveHome .Baocontent .itemSlidethutucuoi").animate({ "opacity": 0, "z-index": -1 }, 1000);
                }
                else {
                    $("#AdvSlideResponesiveHome .Baocontent .current").each(function () {
                        $(this).next().addClass("current");
                        $(this).removeClass("current");
                        $(this).next().animate({ "opacity": 1, "z-index": 10 }, 1000);
                        $(this).animate({ "opacity": 0, "z-index": -1 }, 1000);
                    });
                }
            }
        }, 5000);
    }
    // Su kien nut Prev
    $("#AdvSlideResponesiveHome .prev").click(function() {
        var vitri = $("#AdvSlideResponesiveHome .Baocontent .current").attr("data-thutu");
        if (vitri == "thutudau") {
            $("#AdvSlideResponesiveHome .Baocontent .itemSlidethutucuoi").addClass("current");
            $("#AdvSlideResponesiveHome .Baocontent .itemSlidethutudau").removeClass("current");
            $("#AdvSlideResponesiveHome .Baocontent .itemSlidethutucuoi").animate({ "opacity": 1, "z-index": 10 }, 1000);
            $("#AdvSlideResponesiveHome .Baocontent .itemSlidethutudau").animate({ "opacity": 0, "z-index": -1 }, 1000);
        }
        else {
            $("#AdvSlideResponesiveHome .Baocontent .current").each(function () {
                $(this).prev().addClass("current");
                $(this).removeClass("current");
                $(this).prev().animate({ "opacity": 1, "z-index": 10 }, 1000);
                $(this).animate({ "opacity": 0, "z-index": -1 }, 1000);
            });
        }
    });
    // Su kien nut Next
    $("#AdvSlideResponesiveHome .next").click(function () {
        var vitri = $("#AdvSlideResponesiveHome .Baocontent .current").attr("data-thutu");
        if (vitri == "thutucuoi") {
            $("#AdvSlideResponesiveHome .Baocontent .itemSlidethutudau").addClass("current");
            $("#AdvSlideResponesiveHome .Baocontent .itemSlidethutucuoi").removeClass("current");
            $("#AdvSlideResponesiveHome .Baocontent .itemSlidethutudau").animate({ "opacity": 1, "z-index": 10 }, 1000);
            $("#AdvSlideResponesiveHome .Baocontent .itemSlidethutucuoi").animate({ "opacity": 0, "z-index": -1 }, 1000);
        }
        else {
            $("#AdvSlideResponesiveHome .Baocontent .current").each(function () {
                $(this).next().addClass("current");
                $(this).removeClass("current");
                $(this).next().animate({ "opacity": 1, "z-index": 10 }, 1000);
                $(this).animate({ "opacity": 0, "z-index": -1 }, 1000);
            });
        }
    });

</script>

    </div>
