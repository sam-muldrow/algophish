  <script>
    jQuery(function() {
      var showcase = jQuery("#showcase")

      showcase.Cloud9Carousel( {
        yOrigin: 0,
        yRadius: -60,
		autoPlay:true,
        itemClass: "cloud9-item",
        buttonLeft: jQuery(".testimonial_items.left"),
        buttonRight: jQuery(".testimonial_items.right"),
        bringToFront: true,
        onLoaded: function() {
          showcase.css( 'visibility', 'visible' )
          showcase.css( 'display', 'none' )
          showcase.fadeIn( 1500 )
        }
      } )

      //
      // Simulate physical button click effect
      //
      jQuery('.testimonial_items').click( function( e ) {
        var b = jQuery(e.target).addClass( 'down' )
        setTimeout( function() { b.removeClass( 'down' ) }, 80 )
      } )

      jQuery(document).keydown( function( e ) {
        //
        // More codes: http://www.javascripter.net/faq/keycodes.htm
        //
        switch( e.keyCode ) {
          /* left arrow */
          case 37:
            jQuery('.testimonial_items.left').click()
            break

          /* right arrow */
          case 39:
            jQuery('.testimonial_items.right').click()
        }
      } )
    })
    jQuery("#carousel").data("carousel").go( 3 );
  </script>
