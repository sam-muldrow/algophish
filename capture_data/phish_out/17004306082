   const dropDownItems = document.querySelectorAll('li.nav-item.dropdown');

   dropDownItems.forEach((item) => item.addEventListener('mouseenter', () => {
      item.classList.add('active')
   }));

   dropDownItems.forEach((item) => item.addEventListener('mouseleave', () => {
      item.classList.remove('active')
   }));
</script>


