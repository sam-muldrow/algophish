    $j(function() {
        $j(document).foundation();
                ouibounce(false, {
            sensitivity: 20,
            timer: 4000,
            delay: 100,
            cookieExpire: 30,
            cookieName: 'ExitIntentPopupMakeup',
            sitewide: true,
            callback: function() {
                $j('#exitintent-popup').foundation('reveal', 'open');
            }
        });
                    });
</script>
