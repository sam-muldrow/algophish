<script>
    function refreshCaptcha() {
        $.ajax({
            url: "https://apsconsultant.vn/apply/refereshcapcha",
            type: "get",
            dataType: "html",
            success: function (json) {
                $(".refereshrecapcha").html(json);
            },
            error: function (data) {
                alert("Try Again.");
            },
        });
    }
</script>




