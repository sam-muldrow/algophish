 <script>
  const preorderCustom = {
      render: function(){
          this.initSelectors()
          this.handleEvents()
      },
      initSelectors: function(){
          window.GloboPreorderParams = window.GloboPreorderParams || {};
          window.GloboPreorderParams.selectors = window.GloboPreorderParams.selectors || {};
          window.GloboPreorderParams.selectors.productForm = ["form.t4s-form__product"];
          window.GloboPreorderParams.selectors.collectionProductForms = "";
          window.GloboPreorderParams.selectors.productPositionBis = ".gPositionBis";
          window.GloboPreorderParams.selectors.quickViewActivator = ".t4s-pr-item-btn";
          window.GloboPreorderParams.selectors.quickViewProductForm = ".t4s-product-quick-view form.t4s-form__product";
      },
      handleEvents: function(){
          document.addEventListener('globo.preorder.show.bis', e => {
              const form = e.detail.form;
              form.addClass('gBisShowing')
          })
          document.addEventListener('globo.preorder.show.addtocart', e => {
              const form = e.detail.form;
              window.Globo.Preorder.appSetInterval(function(){
                  let bisBtn = form.find("#Globo-Back-In-Stock")
                  if(bisBtn.length == 0){
                      form.removeClass('gBisShowing')
                  }
              }, 500, 3)
          })
          document.addEventListener('globo.preorder.show.preorder', e => {
              const form = e.detail.form;
              window.Globo.Preorder.appSetInterval(function(){
                  let bisBtn = form.find("#Globo-Back-In-Stock")
                  if(bisBtn.length == 0){
                      form.removeClass('gBisShowing')
                  }
              }, 500, 3)
          })
      }
  }
  preorderCustom.render()
</script>
<style>
  .gBisShowing button[disabled="disabled"]:not(.gPreorderBtn).t4s-product-form__submit{
      display: none;
  }
  .Globo-header-close{
      top: 15px !important;
  }
</style>
  <div id="shopify-block-5731609403698768836" class="shopify-block shopify-app-block">
