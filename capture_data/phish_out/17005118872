        function autoptJsFilesInit() {
            lzLoadItemF = function (lLItemId) {
                let lCurId = document.getElementById(lLItemId);
                if (lCurId) {
                    if (lCurId.getAttribute("data-src")) {
                        lCurId.setAttribute("src", lCurId.getAttribute("data-src"));
                        lCurId.removeAttribute("data-src");
                    }
                }
            }
            lLItemHrefF = function (lItIdHref) {
                let lCIdHref = document.getElementById(lItIdHref);
                if (lCIdHref) {
                    if (lCIdHref.getAttribute("data-href")) {
                        lCIdHref.setAttribute("href", lCIdHref.getAttribute("data-href"));
                        lCIdHref.removeAttribute("data-href");
                    }
                }
            }
            var event;
            if (typeof (Event) === 'function') {
                event = new Event('load');
            } else {
                event = document.createEvent('Event');
                event.initEvent('load', true, true);
            }


            scdParam = false;
            function lLoadImgTwo(imagesClassN, scdParam) {
                let lImgsTwoArr = document.querySelectorAll('.' + imagesClassN);
                Object.keys(lImgsTwoArr).forEach(function (key) {
                    if (lImgsTwoArr[key].dataset.src) {
                        lImgsTwoArr[key].src = lImgsTwoArr[key].dataset.src;
                        lImgsTwoArr[key].classList.add("lazzzy-loaded");
                        lImgsTwoArr[key].classList.remove(imagesClassN);
                        lImgsTwoArr[key].classList.remove("invisible-class");

                        if (scdParam) {
                            org_html = lImgsTwoArr[key].outerHTML;
                            new_html = '<div class="fluid-width-video-wrapper" style="padding-top: 56.25%;">' + org_html + "</div>";
                            lImgsTwoArr[key].outerHTML = new_html;
                        }

                        /*setTimeout(function() {
                         window.dispatchEvent(event);
                         },900);*/

                    }
                    if (lImgsTwoArr[key].dataset.srcset) {
                        lImgsTwoArr[key].src = lImgsTwoArr[key].dataset.srcset;
                    }

                });


            }

            setTimeout(function loadAllJsFilesNow() {

                lzLoadItemF("gtag-js");
                lzLoadItemF("stripe-js");


                setTimeout(function () {
                    if (typeof inlineGtmJs === "function") {
                        inlineGtmJs();
                    }
                }, 800);

                lzLoadItemF("lazysizes-js");




                let filteredJSfiles = document.getElementById("autoptimize_filtered_js_script");
                if (filteredJSfiles) {
                    if (filteredJSfiles.getAttribute("data-src")) {
                        filteredJSfiles.setAttribute("src", filteredJSfiles.getAttribute("data-src"));
                        filteredJSfiles.removeAttribute("data-src");
                    }
                }


                /*===================================================*/

                function linkDicsName(linkClassN) {
                    let lImgsTwoArr = document.querySelectorAll('.' + linkClassN);

                    Object.keys(lImgsTwoArr).forEach(function (key) {

                        ariaLabelVal = lImgsTwoArr[key].childNodes[1].innerText;

                        lImgsTwoArr[key].childNodes[0].childNodes[0].setAttribute('aria-label', ariaLabelVal);
                        lImgsTwoArr[key].childNodes[1].childNodes[0].setAttribute('aria-label', ariaLabelVal);
                        lImgsTwoArr[key].childNodes[3].childNodes[1].setAttribute('aria-label', ariaLabelVal);


                    });
                }

                currentURL = window.location.href;

                /*===================================================*/

                var loadCheck = false;
                var loadCheckFunc = (function () {
                    return function () {
                        if (!loadCheck) {
                            loadCheck = true;
                            if (typeof freecheckWrapperFunc === "function") {
                                freecheckWrapperFunc();
                            }
                        }
                    };
                })();

                testforjQuery = function () {
                    if (window.jQuery) {
                        loadCheckFunc();
                    } else {
                    }
                }

                for (i = 1; i <= 100; ++i) {
                    setDelay(i);
                }

                function setDelay(i) {
                    setTimeout(function () {
                        /*console.log(i);*/
                        testforjQuery();
                    }, 100 * i);
                }


                /*===================================================*/



                setTimeout(function loadAllinlineJsFilesNow() {

                    setTimeout(function () {
                        window.dispatchEvent(event);
                    }, 1500);

                    loadAllCommands = function () {

                        if (!document.getElementById('admin-bar-js')) {
                            var spanEl = document.createElement("span");
                            spanEl.id = "admin-bar-js";
                            document.body.appendChild(spanEl);
                            setTimeout(function () {
                                lzLoadItemF("admin-bar-js");
                            }, 250);
                        }
                    }


                    var exctd = false, exctdTwo = false;
                    ldTwo = function () {
                        lLItemHrefF("dashicons-css");
                        lLoadImgTwo("yvid");
                        lLoadImgTwo("g-docs-iframe");
                        lLItemHrefF("etmodules-theme-css");
                        lLItemHrefF("theme-fonts-css");
                        lzLoadItemF("stripeobjects-js");
                        lzLoadItemF("lazysizes-js");

                        if (document.fonts.check("16px ETmodules")) {
                            setTimeout(function () {
                                if (document.getElementById("bodyid")) {
                                    document.getElementById("bodyid").classList.remove("lzcls");
                                    document.getElementById("top-menu-nav").classList.remove("lzmenu-after");

                                }
                            }, 350);
                        }

                        var innerFunc = (function () {
                            return function () {
                                if (!exctd) {
                                    exctd = true;
                                    exctdTwo = true;
                                    setTimeout(function () {
                                        lzLoadItemF("init-gmap-js");
                                    }, 1000);

                                    setTimeout(function () {
                                        if (typeof gtagPush === "function") {
                                            gtagPush();
                                        }
                                        if (typeof provideGtagPluginFunc === "function") {
                                            provideGtagPluginFunc();
                                        }
                                        if (typeof fbTrackFunc === "function") {
                                            fbTrackFunc();
                                        }
                                        if (typeof linkedInOne === "function") {
                                            linkedInOne();
                                            setTimeout(function () {
                                                snapLicdnF();
                                            }, 1500);
                                        }
                                    }, 1200);
                                }
                            };
                        })();

                        innerFunc();

                    }

                    document.addEventListener("scroll", function () {
                        loadAllCommands();
                        ldTwo();
                    });
                    document.addEventListener("mouseover", function () {
                        loadAllCommands();
                        ldTwo();
                    });
                    document.addEventListener("resize", function () {
                        loadAllCommands();
                    });
                    window.addEventListener('touchstart', function () {
                        loadAllCommands();
                        ldTwo();
                    });


                }, 350);   /*loadAllinlineJsFilesNow func end*/

            }, 1900);




            setTimeout(function scrollToHashTagNow() {
                UrlStr = window.location.href;
                if (UrlStr.includes("#")) {
                    idAtTheUrl = UrlStr.substr(UrlStr.indexOf("#") + 1);
                    if (document.getElementById(idAtTheUrl)) {
                        let elmntToScroll = document.getElementById(idAtTheUrl);
                        elmntToScroll.scrollIntoView();
                    }
                }
            }, 2450);


        }

        /*========*/

        triggerEvents = function () {
            if (!document.getElementById('span-load-event-happend')) {
                var spanEl = document.createElement("span");
                spanEl.id = "span-load-event-happend";
                document.body.appendChild(spanEl);
                setTimeout(function () {
                    autoptJsFilesInit();
                }, 250);
            }
        }


        document.addEventListener("DOMContentLoaded", function (event) {
            triggerEvents();
        });
