	    //checkbox rgpdCookies02 
		if (document.getElementById('rgpdCookies02').checked) {
	            console.log("está checked");
	            setCookie('rgpdCookies02','true','360');
	    } else {
	            //console.log("não está checked");
	            setCookie('rgpdCookies02','false','360');
	    }

