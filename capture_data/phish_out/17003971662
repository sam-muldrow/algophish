$(document).ready(function(){
	$('#testimonials .slide');
	setInterval(function(){
		$('#testimonials .slide').filter(':visible').fadeOut(2000,function(){
			if($(this).next('li.slide').size()){
				$(this).next().fadeIn(7000);
			}
			else{
				$('#testimonials .slide').eq(0).fadeIn(2000);
			}
		});
	},7000);	
});	
