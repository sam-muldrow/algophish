var isuwc,repuwc
isuwc = document.location.href.search("/uwc");
if (isuwc == -1){
        repuwc=document.location.href.replace('?','/');
        repuwc=repuwc.replace('#','/');
        document.location.href=repuwc+'uwc/auth';
}
