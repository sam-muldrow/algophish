      <div class="x-modal-content" role="document" aria-label="Modal Content" data-x-toggleable-content="e1170-9" >
        <script type="text/cs-toggle-template"><style>.embed-container { position: relative; padding-bottom: 56.25%; height: 0; overflow: hidden; max-width: 100%; } .embed-container iframe, .embed-container object, .embed-container embed { position: absolute; top: 0; left: 0; width: 100%; height: 100%; }</style><div class='embed-container'><iframe src='https://www.youtube.com/embed/ly69s69q6sI' frameborder='0' allowfullscreen></iframe></div></script>      </div>
