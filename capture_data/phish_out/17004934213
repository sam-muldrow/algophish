<script type="text/javascript">
	jQuery(document).ready(function($){
		var $tag_id = $('#so_listing_tabs_246'), 
		parent_active = 	$('.items-category-28', $tag_id),
		total_product = parent_active.data('total'),
		tab_active = $('.ltabs-items-inner',parent_active),
		nb_column0 = 4,
		nb_column1 = 3,
		nb_column2 = 2,
		nb_column3 = 2,
		nb_column4 = 1;
		tab_active.owlCarousel2({
			rtl: false,
			nav: true,
			dots: false,	
			margin: 0,
			loop:  false,
			autoplay: false,
			autoplayHoverPause: false,
			autoplayTimeout: 5000,
			autoplaySpeed: 1000,
			mouseDrag: true,
			touchDrag: true,
			navRewind: true,
			navText: [ '', '' ],
			responsive: {
				0: {
					items: nb_column4,
					nav: total_product <= nb_column4 ? false : true,
				},
				480: {
					items: nb_column3,
					nav: total_product <= nb_column3 ? false : true,
				},
				768: {
					items: nb_column2,
					nav: total_product <= nb_column2 ? false : true,
				},
				992: {
					items: nb_column1,
					nav: total_product <= nb_column1 ? false : true,
				},
				1200: {
					items: nb_column0,
					nav: total_product <= nb_column0 ? false : true,
				},
				1400: {
					items: 5,
					
				}
			}
		});
	});
