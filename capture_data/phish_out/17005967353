<script>
    jQuery(document).on("scroll", function () {
        if (jQuery(document).scrollTop() > 100) {
            jQuery("#stickymenu").removeClass("main-menu").addClass("main-menu-scroll");
                jQuery(".mega-menu").css("top" , "120px");
            
        } else {
            jQuery("#stickymenu").removeClass("main-menu-scroll").addClass("main-menu");
                jQuery(".mega-menu").css("top" , "160px");
                jQuery(".mega-menu").css("z-index" , "125");
        }

    });
    jQuery(document).ready(function () {
         jQuery(".mega-menu").css("top" , "160px");
         jQuery(".mega-menu").css("z-index" , "125");
    });
</script>

