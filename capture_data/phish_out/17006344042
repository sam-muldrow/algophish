<section id="banner-parallax" class="parallax-iriz" data-speed="3" style="background-image: url(https://www.earthzoo.com.br/image/cache/catalog/banners/leao-1920x1280.jpg);">
  <article>
    <header>
            <h1>#VistaEarthZoo</h1>
          </header>
  </article>
</section>

<script type="text/javascript">

    $(function(){

    //alert($(".parallax-iriz").offset().top);
    //alert($(window).height());
    //alert($(window).scrollTop());
    //alert( $(window).scrollTop() - ($(".parallax-iriz").offset().top - $(window).height()));

      $('.parallax-iriz').each(function(){

          var $obj = $(this);
          var speed = 3

          if($(window).width() < 1199)
          {
                speed = 6;
          }
          
          if($(window).width() < 1023)
          {
                speed = 8;
          }

          $(window).scroll(function() {
            var topo = $(window).scrollTop() - ($(".parallax-iriz").offset().top - $(window).height())
            var yPos = -(topo / speed);      
            var bgpos = 'center '+ yPos + 'px';       

            if($(window).width() < 767)
            {
              var bgpos = 'center center';       
            }

            $obj.css('background-position', bgpos );     
          });

      });
    });
</script>
        
