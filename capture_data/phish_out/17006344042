                    <script type="text/javascript">

                        function quickBox(product_id, slider) {
                            jQuery('#quickview-'+product_id+'-'+slider+' .quickviewblock .large-image').html(jQuery('.main-image-'+product_id + ' .first-main-image-'+product_id+':first').clone());
                            jQuery('#quickview-'+product_id+'-'+slider+' .quickviewblock .advanced_color').html(jQuery('.advanced_option-'+product_id).clone());

                            jQuery('.fancybox').fancybox({
                                "closeBtn": false,
                                hideOnContentClick : false,
                                //margin:0,
                                //padding:0,
                                autoDimensions: true,
                                showTitle: false,
                                scrolling: 'auto',
                                maxWidth: 800,
                                maxHeight: 800,
                                fitToView: false,
                                width: "84%",
                                //height: "70%",
                                //height:500,
                                autoSize: false,
                                closeClick: true,

                                onComplete: function(){
                                    jQuery('.fancybox-default').load(function () {
                                        jQuery('.fancybox-default').height(jQuery(this).contents().height());
                                        jQuery.fancybox.resize();
                                    });


                                }


                            });
                        }

                        function quickBoxClose(product_id, slider) {
                            $.fancybox.close();
                            jQuery('#quickview-'+product_id+'-'+slider+' .quickviewblock .advanced_color .advanced_option-'+product_id).remove();
                            $('.quickviewblock .product-images-cell .first-main-image').remove();
                        }

                    </script>
