    <script>
        const s_top_hide = document.querySelector(".scroll_top_hide")
        const white_top_menu = document.querySelector(".top_menu_wt")
        $(window).scroll(function () {
            var scroll = $(window).scrollTop();
            if (scroll > 80) {
                s_top_hide.style.display = "block"
                document.querySelector(".page_top_links_dv").style.display = "none"
                document.querySelector(".page_top_links_dv_after").style.display = "block"

            } else {
                s_top_hide.style.display = "none"
                document.querySelector(".page_top_links_dv").style.display = "block"
                document.querySelector(".page_top_links_dv_after").style.display = "none"
            }
        });
        const offcanvas_body = document.querySelector(".canvas_right")

    </script>
    <script>
        const tech_btn = document.querySelector(".Tech_btn_toggle")
        const tech_numbtn = document.querySelector(".search_glass")
        const tech_consult = document.querySelector(".Technology_conslt")
        const excel_consalting_btn = document.querySelector(".excel_consalting_btn_toggle")
        const excel_conslt = document.querySelector(".excel_conslt_")
        const search_btn = document.querySelector(".search_glass")
        const search_content_top = document.querySelector(".search_sec_in_topmenu")
        const close_exe_mark = document.querySelector(".close_exe_mark")
        const canvas_bar = document.querySelectorAll(".three_bars_top_menu")
        const body = document.querySelector("body")


        for (i = 0; i < 6; i++) {
            canvas_bar[i].addEventListener("click", () => {
                offcanvas_body.classList.toggle("canvas_right_aft")

                if (!offcanvas_body.classList.contains("canvas_right_aft")) {
                    body.classList.remove("stop_scroll")
                } else {
                    body.classList.add("stop_scroll")
                }
                tech_consult.classList.remove("Technology_conslt_aft");
                excel_conslt.classList.remove("excel_conslt_aft");
                search_content_top.classList.remove("search_sec_in_topmenu_aft");
            });
        }
        close_exe_mark.addEventListener("click", () => {
            offcanvas_body.classList.remove("canvas_right_aft")
            body.classList.remove("stop_scroll")
        });
    </script>
