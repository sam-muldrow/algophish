    <script type="text/javascript">
        generateCaptcha();
        function generateCaptcha() {
            if (document.getElementById("txtCaptchaCode")) {
                document.getElementById("txtCaptchaCode").value = "";
                var randomNr1 = 0;
                var randomNr2 = 0;
                var totalNr = 0;
                randomNr1 = Math.floor(Math.random() * 10);
                randomNr2 = Math.floor(Math.random() * 10);
                $('#randnum1').val(randomNr1);
                $('#randnum2').val(randomNr2);
                totalNr = randomNr1 + randomNr2;
                document.getElementById("image").value = totalNr;
            }
            if (document.getElementById("txtCaptchaCode1")) {
                document.getElementById("txtCaptchaCode1").value = "";
                var randomNr1 = 0;
                var randomNr2 = 0;
                var totalNr = 0;
                randomNr1 = Math.floor(Math.random() * 10);
                randomNr2 = Math.floor(Math.random() * 10);
                $('#1randnum1').val(randomNr1);
                $('#1randnum2').val(randomNr2);
                totalNr = randomNr1 + randomNr2;
                document.getElementById("image1").value = totalNr;
            }
            if (document.getElementById("txtCaptchaCode2")) {
                document.getElementById("txtCaptchaCode2").value = "";
                var randomNr1 = 0;
                var randomNr2 = 0;
                var totalNr = 0;
                randomNr1 = Math.floor(Math.random() * 10);
                randomNr2 = Math.floor(Math.random() * 10);
                $('#2randnum1').val(randomNr1);
                $('#2randnum2').val(randomNr2);
                totalNr = randomNr1 + randomNr2;
                document.getElementById("image2").value = totalNr;
            }
            if (document.getElementById("txtCaptchaCode3")) {
                document.getElementById("txtCaptchaCode3").value = "";
                var randomNr1 = 0;
                var randomNr2 = 0;
                var totalNr = 0;
                randomNr1 = Math.floor(Math.random() * 10);
                randomNr2 = Math.floor(Math.random() * 10);
                $('#3randnum1').val(randomNr1);
                $('#3randnum2').val(randomNr2);
                totalNr = randomNr1 + randomNr2;
                document.getElementById("image3").value = totalNr;
            }
            if (document.getElementById("txtCaptchaCode4")) {
                document.getElementById("txtCaptchaCode4").value = "";
                var randomNr1 = 0;
                var randomNr2 = 0;
                var totalNr = 0;
                randomNr1 = Math.floor(Math.random() * 10);
                randomNr2 = Math.floor(Math.random() * 10);
                $('#4randnum1').val(randomNr1);
                $('#4randnum2').val(randomNr2);
                totalNr = randomNr1 + randomNr2;
                document.getElementById("image4").value = totalNr;
            }           
        }
