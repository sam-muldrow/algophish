    <script>
        function animate() {
            var fadeup = document.querySelectorAll(".fade_up_r");
            var fdeup = document.querySelectorAll(".fde_up")
            for (var i = 0; i < fadeup.length; i++) {
                var windowheight = window.innerHeight;
                var elementtop = fadeup[i].getBoundingClientRect().top;
                var elementvisible = 0;
                if (elementtop < windowheight - elementvisible) {
                    fadeup[i].classList.add("activating_r");
                } else {
                    fadeup[i].classList.remove("activating_r");
                }
            }
            for (var i = 0; i < fdeup.length; i++) {
                var windowheight = window.innerHeight;
                var elementtop = fdeup[i].getBoundingClientRect().top;
                var elementvisible = 0;
                if (elementtop < windowheight - elementvisible) {
                    fdeup[i].classList.add("activating_r_up");
                } else {
                    fdeup[i].classList.remove("activating_r_up");
                }
            }
        }
        window.addEventListener("scroll", animate);
    </script>


    <script>
        const animate_1 = () => {
            var zmon = document.querySelectorAll(".tab_nav_cnt_fld");
            var scroll = window.pageYOffset;

            for (var i = 0; i < zmon.length; i++) {
                if (scroll > 600) {
                    zmon[i].classList.add("activeing");
                } else {
                    zmon[i].classList.remove("activeing");
                }
                //console.log(scroll)
            }
        }

        window.addEventListener("scroll", animate_1);
    </script>
    <script>
        $(document).ready(function () {
            var navHeight = $('nav').outerHeight();
            $(window).scroll(function () {
                var position = $(this).scrollTop();
                $('section').each(function () {
                    var sectionHeight = $(this).outerHeight();
                    var sectionTop = $(this).offset().top - navHeight;
                    if (position >= sectionTop && position < sectionTop + sectionHeight) {

                        $('nav a[href="#' + $(this).attr('id') + '"]').addClass('active');
                    }
                    else {
                        $('nav a[href="#' + $(this).attr('id') + '"]').removeClass('active');
                    }
                });
            });
        });

    </script>
