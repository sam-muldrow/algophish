    <script type="text/javascript">
        $(document).ready(function () {

            if (window.location.href.indexOf('schedule') > -1) {
                if (Math.min(window.screen.width, window.screen.height) < 768) {
                    document.getElementById('traning_sche_accord').click();
                }
            }
            if (window.location.href.indexOf('certificate') > -1) {
                if (Math.min(window.screen.width, window.screen.height) < 768) {
                    document.getElementById('certification_accord').click();
                }
            }
        });
    </script>
    <script>
        $('select').on('change', function () {
            if ($(this).val() == 1) {
                return $(this).css('color', '#d1d1d1');
            } else {
                $(this).css('color', 'black');
                $(this).css('font-weight', '400');
            }
        });
    </script>

    <script type="text/javascript">
        $('.collapse').on('shown.bs.collapse', function (e) {
            var $card = $(this).closest('.accordion-item');
            var $open = $($(this).data('parent')).find('.collapse.show');

            var additionalOffset = 0;
            if ($card.prevAll().filter($open.closest('.accordion-item')).length !== 0) {
                additionalOffset = $open.height();
            }
            $('html,body').animate({
                scrollTop: $card.offset().top - additionalOffset - 45
            }, 10);
        });
        function showSchedule() {
            if (Math.min(window.screen.width, window.screen.height) < 768) {
                document.getElementById('traning_sche_accord').click();
                $('html, body').animate({
                    scrollTop: $('#traning_sche_accord').offset().top - 40
                });
            } else {
                $('html, body').animate({
                    scrollTop: $('#schedule').offset().top - 40
                });
            }
        }
        function showCertificate() {
            if (Math.min(window.screen.width, window.screen.height) < 768) {
                document.getElementById('certification_accord').click();
            }
        }

    </script>
    <script type="text/javascript">
        function DoSetActiveClass(strActivePage) {
            document.getElementById('liFullStackBIActive').removeAttribute('class');
            document.getElementById('liVBAActive').removeAttribute('class');
            document.getElementById('liExcelActive').removeAttribute('class');
            document.getElementById('liMISActive').removeAttribute('class');
            if (strActivePage == 'FullStackBI') {
                document.getElementById('liFullStackBIActive').setAttribute('class', 'active');
            }
            else if (strActivePage == 'VBA') {
                document.getElementById('liVBAActive').setAttribute('class', 'active');
            }
            else if (strActivePage == 'Excel') {
                document.getElementById('liExcelActive').setAttribute('class', 'active');
            }
            else if (strActivePage == 'MIS') {
                document.getElementById('liMISActive').setAttribute('class', 'active');
            }
        }

    </script>
    
