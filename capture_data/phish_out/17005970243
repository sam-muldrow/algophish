							itemsDesktop : [1199,3],
							itemsDesktopSmall : [991,3],
							itemsTablet: [767,2],
							itemsMobile : [480,1],
							rewindNav : false,
							autoPlay :  false,
							stopOnHover: false,
							pagination : false,
							addClassActive : true,
						});
