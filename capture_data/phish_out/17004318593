                                                $form.removeClass('was-validated');

                                                $form.find('[id="inputPassword"]').siblings('.invalid-feedback').remove();
                                                $form.find('[id="inputPassword"]').removeClass('is-invalid');
                                            },
                                            complete: function () {
                                                $form.find('[id="btnLoginLoad"]').addClass('d-none');
                                            },
                                            success: function (data, textStatus, jqXHR) {
                                                //window.location.href = '//';
                                                window.location.reload();
                                            },
                                            error: function (jqXHR, textStatus, errorThrown) {
                                                $form.addClass('was-validated');

                                                $form.find('[id="btnLoginLoad"]').addClass('d-none');
                                                $form.find('[id="BtFormLogin"]').addClass('follow');

                                                var data = jqXHR.responseJSON;

                                                if (data && data.error) {
                                                    $form.find('[id="inputPassword"]').addClass('is-invalid');
                                                    $form.find('[id="inputPassword"]').after('<div class="invalid-feedback">' + data.error + '</div>');
                                                }
                                            }
                                        });
                                    });
                                });
                            </script>
                        </li>
                    </ul>
                </div>
                <div class="col-3 col-md-4">
                    <ul class="idiomais">
                        <h2 class="d-none d-md-inline-block">Português/BR</h2>
                        <li>
                            <a href="https://anpad.org.br/" class="active">
                                <img src="https://anpad.org.br/wp-content/themes/anpad/assets/img/br.png" alt="Português/BR | Anpad"/>
                            </a>
                        </li>
                        <li>
                            <a href="https://anpad.org.br/en/" class="">
                                <img src="https://anpad.org.br/wp-content/themes/anpad/assets/img/us.png" alt="English/US | Anpad"/>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>

