                                $(function () {
                                    $('.dropdown-menu').on("click.bs.dropdown", function (e) {
                                        e.stopPropagation();
                                    });

                                    $('[id="formLoginLogin"]').submit(function (e) {
                                        e.stopPropagation();
                                        e.preventDefault();

                                        var $form = $(this);

                                        $.ajax({
                                            url: $form.prop('action'),
                                            type: $form.prop('method'),
                                            dataType: 'json',
                                            data: $form.serialize(),
                                            beforeSend: function () {
