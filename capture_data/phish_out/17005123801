/* Custom JS */

(function($,window){

	let iLaw = window.iLaw || {};
		
	const 
		brxs = parseFloat( getComputedStyle(document.documentElement).getPropertyValue('--br-xs') ) || 0,
		brsm = parseFloat( getComputedStyle(document.documentElement).getPropertyValue('--br-sm') ) || 768,
		brmd = parseFloat( getComputedStyle(document.documentElement).getPropertyValue('--br-md') ) || 1200,
		brlg = parseFloat( getComputedStyle(document.documentElement).getPropertyValue('--br-lg') ) || 1400,
		brxl = parseFloat( getComputedStyle(document.documentElement).getPropertyValue('--br-xl') ) || 1800;

	function isMobile(br){ 
		br = br || brmd;
		return window.innerWidth < br;
	}

	/* Accordions
		--------------------------------------------------------------------------------------- */
		/*
