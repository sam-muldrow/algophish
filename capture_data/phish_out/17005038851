                ( function ( $ ) {
                    'use strict';
                    $( document ).on( 'ready', function () {
                        var sub_lg_menu_parent = $( '.rhea-lg-menu-e707eac ul.sub-menu' )
                        .prev( 'a' );
                        sub_lg_menu_parent.append( '' );
                        var sub_menu_parent = $( '.rhea-inner-e707eac ul.sub-menu' )
                        .parent();
                        sub_menu_parent.prepend( '<i class="rhea-menu-icon fas fa-angle-down rh_menu__indicator"></i>' );
                        $( '.rhea-inner-e707eac .rhea-elementor-nav-menu > li .rh_menu__indicator' )
                        .on( 'click', function ( e ) {
                            e.preventDefault();
                            $( this ).parent().children( 'ul.sub-menu' ).slideToggle();
                            $( this ).toggleClass( 'rh_menu__indicator_up' );
                        } );
                        $( '.rhea-bars-e707eac' ).on( 'click', function () {
                            $( this ).addClass( 'rhea-nav-is-open' );
                            $( '.rhea-inner-e707eac' ).addClass( 'rhea-show-nav' );
                        } );
                        $( '.rhea-fa-close-nav' ).on( 'click', function () {
                            $( this )
                            .parent( '.rhea-inner-e707eac' )
                            .removeClass( 'rhea-show-nav' );
                        } );
                        //Hide Mobile Menu after item click
                        $( '.rhea-nav-menu-responsive-inner a' ).on( 'mouseup', function () {
                            $( this ).closest( '.rhea-inner-e707eac' )
                            .removeClass( 'rhea-show-nav' );
                        } );

                        $( document ).on( 'click', function ( e ) {
                            let container = $( ".rhea-bars-e707eac,.rhea-bars-e707eac i,.rhea-inner-e707eac .rhea-elementor-nav-menu" );
                            if ( ! container.is( e.target ) && container.has( e.target ).length === 0 ) {
                                $( '.rhea-inner-e707eac' )
                                .removeClass( 'rhea-show-nav' );
                            }
                        } );

                    } );
                } )( jQuery );


            </script>
