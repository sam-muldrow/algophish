// Post Carousel Lite scripts: 
jQuery(document).ready(function(){	
function uc_uc_card_post_carousel_elementor71380_start(){
  
  var objCarousel = jQuery('#uc_uc_card_post_carousel_elementor71380 .owl-carousel');
  var objWrapper = jQuery('#uc_uc_card_post_carousel_elementor71380');
  	
			objCarousel.owlCarousel({
                loop: true,      
                navText: ["<i class='fas fa-arrow-left'></i>","<i class='fas fa-arrow-right'></i>"],
                nav: true,
				dots: true,                                    
                autoplay: true,
                rtl: false,
                paddingType: "none",
                autoplayTimeout: 4000,
                autoplayHoverPause: true,
                smartSpeed: 1000,
                margin: 30,                                           
                responsive : {
					0 : {
						items:1,
                        slideBy: 1,
                        
                        	            
