						items:3,
                        slideBy: 1,
                                    
                        	            
					}
				}
			});
  
	objWrapper.on("uc_ajax_sethtml",function(event, htmlItems, isAppend){
      	
      	objCarousel.trigger("replace.owl.carousel",htmlItems);
      	objCarousel.trigger("refresh.owl.carousel");
         
    });
  
    
  
  
}if(jQuery("#uc_uc_card_post_carousel_elementor71380").length) uc_uc_card_post_carousel_elementor71380_start(); else
	jQuery( document ).on( 'elementor/popup/show', () => { if(jQuery("#uc_uc_card_post_carousel_elementor71380").length) uc_uc_card_post_carousel_elementor71380_start();});
