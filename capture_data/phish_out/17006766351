<script>
jQuery(document).ready(function() {
    var width = jQuery('body').width();
    if (width <= 1024) {
        jQuery('.service-main-menu > .mega-sub-menu > li > .mega-menu-link').click(function(e) {
            var UlElement = jQuery(this);
            var UlElementSiblings = jQuery(this).siblings(".mega-sub-menu");
            if (!(UlElementSiblings.hasClass('close'))) {
                UlElementSiblings.toggleClass('close').slideToggle();
                e.preventDefault();
            } else {
                return true;
            }
        })

        jQuery('.service-main-menu > .mega-sub-menu > li > .mega-menu-link > .mega-indicator').click(function(
        e) {
            var UlElement = jQuery(this).parent();
            var UlElementSiblings = jQuery(this).parent().siblings(".mega-sub-menu");
            UlElementSiblings.toggleClass('close').slideToggle();
        })
    }

});
