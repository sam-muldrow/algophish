<script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script>
jQuery(function() {

    var siteObject = {
        "https://www.accely.com/": 10000,
        "https://www.accely.com/blog/": 5000,
    }

    jQuery(window).on('load', function() {
        if (siteObject[window.location.href] != undefined) {
            setTimeout(function() {
                jQuery('#exampleModalCenter').modal();
            }, siteObject[window.location.href]);
        }
    });

    jQuery(document).ready(function() {

        jQuery(".navbar-toggler").click(function() {
            jQuery(".search").toggleClass("hide");
        });

        // jQuery(function() {

        jQuery('#search-button').on('click', function(e) {

            e.preventDefault();
            jQuery('#search-input-container').toggleClass('hdn')
            return false;

        });

        // });
        //jQuery(function() {

        jQuery(".scroll").click(function() {

            jQuery("html,body").animate({
                scrollTop: jQuery(".top_header").offset().top
            }, "1000");

            return false
        });

        //});
    });

    jQuery(window).scroll(function() {
        var noScroll = jQuery(this).scrollTop();
        if (jQuery(this).width() > 767) {
            if (noScroll >= 40) {
                jQuery('.header').addClass('fixedHeader');
            } else {
                jQuery('.header').removeClass('fixedHeader');
            }
        }
    });

    jQuery(window).scroll(function(event) {

        var length_about = jQuery('.about').length;

        if (length_about > 0) {

            var eT = jQuery('.about').offset().top - 100,
                wSt = jQuery(this).scrollTop();

            if (wSt > eT) {

                /****  js for counter on homepage  *****/
                jQuery('.con_list p span').each(function() {

                    jQuery(this).prop('Counter', 0).animate({
                        Counter: jQuery(this).text()
                    }, {
                        duration: 4000,
                        easing: 'swing',
                        step: function(now) {
                            jQuery(this).text(Math.ceil(now));
                        }
                    });
                });

                /****  js for counter on homepage  *****/
                //detach scroll event handler, as we dont want it to fire again
                jQuery(this).off(event);
            }
        }
    });

    /*counter js*/
    jQuery('.count').each(function() {
        jQuery(this).prop('Counter', 0).animate({
            Counter: jQuery(this).text()
        }, {
            duration: 4000,
            easing: 'swing',
            step: function(now) {
                jQuery(this).text(Math.ceil(now));
            }
        });
    });
});
/*counter js end*/
 </script>
<script type="rocketlazyloadscript" data-rocket-type="text/javascript">

	jQuery(document).ready(function($) {
		// var type = jQuery('#categoryname').data('cate');
		// console.log(type);
		// console.log(category_id);
        // var total_post_count = jQuery('#post').data('count');

        var total_post_count = parseInt(jQuery('#post').data('count'));
        var post_display_count = parseInt(jQuery('#post').data('post-display'));

      //  console.log('total_post_count: '+total_post_count);

		jQuery('#load_more').click(function(){
			var page = jQuery('#page_number').val();
			var type = jQuery('.tab-list .active').attr('data-cate');
			var category_id = jQuery('#sub_cat_name').data('id');
			//var category_indsol = jQuery('#displaycategory').data('indsol');
			//console.log('Case study load more btn click');
			 var category_arr = [];
              $.each($("input[name='catnamebox-"+type+"']:checked"), function(){
                  category_arr.push($(this).val());
              });
		  	page++;


			fetch_data_casestudy(page,type,category_arr,'load_more');


		});
	});

	function fetch_data_casestudy(page,type,category_arr,view){

			jQuery('#page_number').val(page);

		var ajaxurl ="https://www.accely.com/wp-admin/admin-ajax.php";
		var data = {
			'action': 'my_action',
			'page': page,
			'type': type,
			'category_id': category_arr

		};

		// since 2.8 ajaxurl is always defined in the admin header and points to admin-ajax.php
		jQuery.post(ajaxurl, data, function(response) {

			if(view == "load_more") {


			if(jQuery.trim(response) != '') {
                jQuery("#post-"+type).append(response);
                page++;
            } else {
                jQuery('#load_more').hide();
            }



			} else {
					jQuery("#-"+type).append(response);

				jQuery(".main_contain_data_"+type).remove();
				//jQuery(".cate-posts-"+type).remove();
 				jQuery("#post-"+type).html(response);

 			}

 			//load more
 			jQuery("#displaycategory").append(data);
 			jQuery("#retail-tab-0").append(data); 		 //industy and solution
 		//	jQuery("#retail-tab-1").append(data);
		});

	}
	</script>
