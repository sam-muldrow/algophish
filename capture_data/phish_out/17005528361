var runs;
runs=0;
setInterval(function(){



jQuery('.cart_totals .shipping').hide();
jQuery('.cart_totals .tax-total').hide();
jQuery('.cart_totals .order-total').hide();

if(jQuery('.wc-variation-selection-needed').length && jQuery('#pls').length < 1){
	jQuery('#product-addons-total').prepend('<span id="pls">Please select options to enable ordering</span>');
}else{
	if(jQuery('#pls').length > 1)jQuery('#pls').remove()
};



jQuery('.shipping-calculator-button').hide();




		//hide delivery & pickup
//		jQuery('.shipping_method').attr('checked',false);
		//alert('nocakes');
		if(runs==0)jQuery('#shipping_method .shipping_method').first().click();
		
		jQuery('#shipping_method_0_table_rate353').parent().hide();
		jQuery('#shipping_method_0_table_rate354').parent().hide();


		jQuery('.shipping label').each(function(i){
		   if(jQuery(this).html().indexOf("Local Delivery")>-1)jQuery(this).parent().hide()
		   if(jQuery(this).html().indexOf("Pickup")>-1)jQuery(this).parent().hide()
		   if(jQuery(this).html().indexOf("Standard")>-1)jQuery(this).parent().show()
		   if(jQuery(this).html().indexOf("Expedited")>-1)jQuery(this).parent().show()

		  	if(runs==0)if(jQuery(this).html().indexOf("Standard")>-1)jQuery(this).parent().find('input').click();

		     //alert(jQuery(this).html());
		})
		runs=runs+1





		

jQuery('[name=aim_ccexpdate]').css('width','60px')
jQuery('[name=aim_ccvnumber]').css('width','60px')


jQuery('[for=do-you-accept-the-terms-and-conditions-of-the-order]').html('Do you accept the <a href="../../terms-conditions/" target="_blank">terms and conditions</a> of the order?*');

