<script>
var today = new Date();
var curHour = today.getHours();
var timez;
timez = 1; 
if(curHour>11) timez = 2;

var day = today.getDay();

if(curHour>11 && day == 5) timez = 4;


var today = new Date();
var curHour = today.getHours();
var timez2;
timez2 = 7; 
if(curHour>11) timez2 = 8;

var day = today.getDay();

if(curHour>11 && day == 5) timez2 = 10;

