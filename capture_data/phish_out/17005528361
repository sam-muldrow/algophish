<script>
// Temp Remove 12/30 as a delivery option.
// 
function checkMonth() {
  // Select the ui-datepicker-div element
  var datepickerDiv = document.querySelector('#ui-datepicker-div');
  
  // Select the td elements within the ui-datepicker-div element
  var tdElements = datepickerDiv.querySelectorAll('td');
  
  // Iterate through the td elements
  for (var i = 0; i < tdElements.length; i++) {
    // Get the current td element
    var tdElement = tdElements[i];
    
    // Check if the td element has a data-month attribute with a value of "11" and an innerHTML of "30"
    if (tdElement.hasAttribute('data-month') && tdElement.getAttribute('data-month') === '11' && tdElement.innerText === '30') {
      // The td element has a data-month attribute with a value of "11" and an innerHTML of "30"
      // Add the classes ui-datepicker-unselectable and ui-state-disabled to the td element
      tdElement.classList.add('ui-datepicker-unselectable', 'ui-state-disabled');
    }
  }

  var prevButton = document.querySelector(".ui-datepicker-prev");
  var nextButton = document.querySelector(".ui-datepicker-next");
  prevButton.addEventListener('click', checkMonth);
  nextButton.addEventListener('click', checkMonth);
}

function init() {
  // Select the element with the id of addon-253-delivery-pickup-date-5
  var element = document.querySelector('.hasDatepicker');

  // Add a click event listener to the element
  element.addEventListener('click', checkMonth);
}

// Call the init function after the HTML document has finished loading
document.addEventListener('DOMContentLoaded', init);
</script>

