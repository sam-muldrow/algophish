jQuery('[class*="pickup-date"] input').datepicker({ 
	

	

	dateFormat: 'mm-dd-yy', minDate: timez, maxDate: 700,

	beforeShowDay: function(date){
	str = jQuery.datepicker.formatDate('mm-dd-yy',date);
	day = date.getDay();
	return [day!=6&&day!=0 && array.indexOf(str) == -1,''];
}


});


jQuery('#delivery_date').datepicker({dateFormat: 'mm-dd-yy', minDate: timez2, maxDate: 700,
	beforeShowDay: function(date){
	str = jQuery.datepicker.formatDate('mm-dd-yy',date);
	day = date.getDay();
	return [day!=6&&day!=0 && array.indexOf(str) == -1,''];
}
});

	jQuery('[class*="pickup-date"] input, #delivery_date').attr('autocomplete', 'off');

