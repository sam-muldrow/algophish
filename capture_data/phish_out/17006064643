  $(function(){
    $('.left').easyTicker({
      direction: 'up',
      visible: 3,
      interval: 2500,
      controls: {
        up: '.btnUp',
        down: '.btnDown',
        toggle: '.btnToggle'
      }
    });

    var galleryTop = new Swiper('.gallery-top', {
      slidesPerView: 1,
      loop: true,
      loopedSlides: 50,
      navigation: {
        nextEl: '.swiper-button-next',
        prevEl: '.swiper-button-prev',
      },
    });
    var galleryThumbs = new Swiper('.gallery-thumbs', {
      direction: 'vertical',
      slidesPerView: 5,
      slideToClickedSlide: true,
      spaceBetween: 10,
      loopedSlides: 50,
      loop: true,
    });
    var galleryTopnav =  new Swiper('.gallerynav', {
      slidesPerView: 1,
      loop: true,
      loopedSlides: 50,
      navigation: {
        nextEl: '.swiper-button-next',
        prevEl: '.swiper-button-prev',
      },
      autoplay: {
        delay: 2500,
        disableOnInteraction: false,
      },
      pagination: {
        el: ".swiper-pagination",
      },
    });

    galleryTop.controller.control = galleryThumbs;
    galleryThumbs.controller.control = galleryTop;
  });

  </script>

