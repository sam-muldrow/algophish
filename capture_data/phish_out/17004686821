<script type="text/javascript">
  function preview(url){
    var modal = $("#modal_img_preview");
    modal.find(".modal-body").html('<img src="'+url+'" style="width: 100%;">');
    modal.find(".modal-footer").html('<a href="'+url+'" download class="btn btn-danger"><span class="ion-ios-download"></span> Download</a>');
    modal.modal("show");
  }
