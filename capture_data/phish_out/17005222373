<script>
	var limitFlatPicker = limitFlatPicker || {};
	document.addEventListener('DOMContentLoaded', function(event) {
		var $ = jQuery;
		//webotic_stticky_header();
		$( window ).scroll(function() {
			//webotic_stticky_header();
		});
		$("#main-menu-burger").click(function(){
			if ($(this).hasClass("is-active")){
				$("#main-menu-burger").parent().addClass("is-active");
				$("#main-header .logo-top").fadeOut(200);
				setTimeout(function(){
					$("#side-menu-bar").addClass('show');
				},200);
			}else{
				$("#side-menu-bar").removeClass('show');
				$("#main-menu-burger").parent().removeClass("is-active");
				setTimeout(function(){
					$("#main-header .logo-top").fadeIn(200);
				},300);
			}
		});
		$("#side-menu-bar .elementor-nav-menu--main li a").click(function(){
			$("#main-menu-burger").removeClass("is-active");
			$("#main-menu-burger").parent().removeClass("is-active");
			$("#side-menu-bar").removeClass('show');
			setTimeout(function(){
				$("#main-header .logo-top").fadeIn(200);
			},300);			
		});
		// Home Page Date Validation
		limitFlatPicker = {
			defaultSettings: {
				selector: '.flatpickr-input',
				minDate: false,
				maxDate: false,
			},
			settings: {},
			init: function( options ) {
				this.settings = $.extend( this.defaultSettings, options );
				if ( this.settings.minDate || this.settings.maxDate ) {
					this.waitForFlatpicker( this.callback );
				}
			},
			waitForFlatpicker: function( callback ) {
				if ( typeof window.flatpickr !== 'function' ) {
					setTimeout( function() { limitFlatPicker.waitForFlatpicker( callback ) }, 100 );
				}
				callback();
			},
			modifyPicker: function( picker, settings ) {
				flatpickr( picker ).set( settings );
			},
			callback: function() {
				var self = limitFlatPicker;
				$( self.settings.selector ).each( function() {
					var picker = $( this )[0],
						pickerSettings = {};

					if ( self.settings.minDate ) {
						pickerSettings['minDate'] = self.settings.minDate;
					}
					if ( self.settings.maxDate ) {
						pickerSettings['maxDate'] = self.settings.maxDate;
					}
					self.modifyPicker( picker, pickerSettings );
				} );
			}
		}
		var aWeekAgo = new Date(),
			aWeekFromToday = new Date();
		aWeekAgo.setDate( aWeekAgo.getDate() +1 );
		aWeekFromToday.setDate( aWeekFromToday.getDate() + 365 );

		limitFlatPicker.init( {
			minDate: aWeekAgo,
			maxDate: aWeekFromToday
		} );
	});
	function webotic_stticky_header(){
		var $ = jQuery;
		var h = $("#main-header");
		if (!h.hasClass("steacky")) {
			if ($(window).scrollTop() > 200)
				h.addClass("steacky");
		} else {
			if ($(window).scrollTop() < 200)
				h.removeClass("steacky");
			if ($(window).scrollTop() >= 1250)
				h.addClass("steacky-active");
			if ($(window).scrollTop() < 1250)
				h.removeClass("steacky-active");
		}
	}
</script>
