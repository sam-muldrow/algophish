                    logLevel: 'ERROR',
                    attribution: 'AmazonStoresLivePage:UnsupportedScriptError',
                };

                window.ueLogError(exception, additionalInfo);
                return;
            }

            // otherwise, report to parent onError
            if (originalOnError) {
                originalOnError.apply(window, arguments);
            }
        }
    }();
