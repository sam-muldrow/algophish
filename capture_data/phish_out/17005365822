</script><script>
/*
The code below behaves like this:
- Checks all fields with class lib_intlTelInput
- Loads the intlTelInput mask on all fields with class lib_intlTelInput
- Checks if the number of char in the field is equal to the number of char in the placeholder
- Check the field and add custom css in the field with error
- Updates the mask when clicking on the country select
*/
jQuery(document).ready(function($) {
    
    //script adapted from https://github.com/jackocnr/intl-tel-input/issues/921
    var ary = Array.prototype.slice.call(document.querySelectorAll('.lib_intlTelInput'));
        
    ary.forEach(function(el) {
        PhoneDisplay(el);
    });

    function PhoneDisplay(input) {

        // associate variables to span tags by ID
        errorMsg = document.querySelector("#error-msg"),
        validMsg = document.querySelector("#valid-msg");

        // here, the index maps to the error code returned from getValidationError - see readme
        var errorMap = ["Número inválido", "Invalid country code", "Número curto", "Número longo", "Número inválido", "Indefinido"];

        // call lib to inputs fields
        var iti = window.intlTelInput(input, {
            initialCountry: 'br',
            placeholderNumberType: 'MOBILE',
            preferredCountries: ['br', 'us', 'gb'],
            utilsScript: 'https://clubedovalor.com.br/net-content/plugins/cubeat-cdv-extension/assets/js/lib/utils.js',
        });

        var reset = function() {
            input.classList.remove("error");
            errorMsg.innerHTML = "";
            errorMsg.classList.add("hide");
            validMsg.classList.add("hide");
        };

        jQuery(input).blur(function(event) {
            // Get a reference to the input element that triggered the blur event
            reset();

            //Get element on blur 
            var targetInput = event.target;
            
            // Get error code
            var errorCode = iti.getValidationError();
            
            // Get the maxlength attribute of the input element
            var maxLength = targetInput.getAttribute('maxlength');

            // Get a reference to the submit button
            var submitButton = $('button[type=submit]');

            // Check value in input element
            if (input.value.trim()) {
                if (input.value.length !=  maxLength ) {
                    input.classList.add("error");
                    
                    errorMsg.innerHTML = errorMap[errorCode];
                    errorMsg.classList.remove("hide");
                    
                    // Disable the submit button
                    submitButton.prop('disabled', true);
                    event.preventDefault();
                    targetInput.focus();
                } else {
                    
                    // Re-enable the submit button
                    submitButton.prop('disabled', false);
                }
            }
            
        });
        
        //Execute function on change country intlTelInput #1
        jQuery(input).on('countrychange', function(event) {

            // Get the selected country data to know which country is selected.
            var selectedCountryData = iti.getSelectedCountryData();

            // Get an example number for the selected country to use as placeholder.
            newPlaceholder = intlTelInputUtils.getExampleNumber(selectedCountryData.iso2, true, intlTelInputUtils.numberFormat.INTERNATIONAL),

            // Reset the phone number input.
            iti.setNumber('');
            errorMsg.classList.add("hide");

            // Convert placeholder as exploitable mask by replacing all 1-9 numbers with 0s
            mask = newPlaceholder.replace(/[1-9]/g, '0');

            // Apply the new mask for the input
            jQuery(this).mask(mask);

        });

        // When the plugin loads for the first time, we have to trigger the "countrychange" event manually, 
        // after making sure that the plugin is fully loaded by associating handler to the promise of the 
        // plugin instance.
        iti.promise.then(function() {
                jQuery(input).trigger('countrychange');
        });

    }

    // apply lib on popup show
    jQuery( document ).on( 'elementor/popup/show', () => {
        
        //script adapted from https://github.com/jackocnr/intl-tel-input/issues/921
        var ary = Array.prototype.slice.call(document.querySelectorAll('.lib_intlTelInput'));
            
        ary.forEach(function(el) {
            PhoneDisplay(el);
        });

    } );

});
</script><script>
/*
The code below behaves like this:
- Checks all fields with class lib_intlTelInput
- Loads the intlTelInput mask on all fields with class lib_intlTelInput
- Checks if the number of char in the field is equal to the number of char in the placeholder
- Check the field and add custom css in the field with error
- Updates the mask when clicking on the country select
*/
jQuery(document).ready(function($) {
    
    //script adapted from https://github.com/jackocnr/intl-tel-input/issues/921
    var ary = Array.prototype.slice.call(document.querySelectorAll('.lib_intlTelInput'));
        
    ary.forEach(function(el) {
        PhoneDisplay(el);
    });

    function PhoneDisplay(input) {

        // associate variables to span tags by ID
        errorMsg = document.querySelector("#error-msg"),
        validMsg = document.querySelector("#valid-msg");

        // here, the index maps to the error code returned from getValidationError - see readme
        var errorMap = ["Número inválido", "Invalid country code", "Número curto", "Número longo", "Número inválido", "Indefinido"];

        // call lib to inputs fields
        var iti = window.intlTelInput(input, {
            initialCountry: 'br',
            placeholderNumberType: 'MOBILE',
            preferredCountries: ['br', 'us', 'gb'],
            utilsScript: 'https://clubedovalor.com.br/net-content/plugins/cubeat-cdv-extension/assets/js/lib/utils.js',
        });

        var reset = function() {
            input.classList.remove("error");
            errorMsg.innerHTML = "";
            errorMsg.classList.add("hide");
            validMsg.classList.add("hide");
        };

        jQuery(input).blur(function(event) {
            // Get a reference to the input element that triggered the blur event
            reset();

            //Get element on blur 
            var targetInput = event.target;
            
            // Get error code
            var errorCode = iti.getValidationError();
            
            // Get the maxlength attribute of the input element
            var maxLength = targetInput.getAttribute('maxlength');

            // Get a reference to the submit button
            var submitButton = $('button[type=submit]');

            // Check value in input element
            if (input.value.trim()) {
                if (input.value.length !=  maxLength ) {
                    input.classList.add("error");
                    
                    errorMsg.innerHTML = errorMap[errorCode];
                    errorMsg.classList.remove("hide");
                    
                    // Disable the submit button
                    submitButton.prop('disabled', true);
                    event.preventDefault();
                    targetInput.focus();
                } else {
                    
                    // Re-enable the submit button
                    submitButton.prop('disabled', false);
                }
            }
            
        });
        
        //Execute function on change country intlTelInput #1
        jQuery(input).on('countrychange', function(event) {

            // Get the selected country data to know which country is selected.
            var selectedCountryData = iti.getSelectedCountryData();

            // Get an example number for the selected country to use as placeholder.
            newPlaceholder = intlTelInputUtils.getExampleNumber(selectedCountryData.iso2, true, intlTelInputUtils.numberFormat.INTERNATIONAL),

            // Reset the phone number input.
            iti.setNumber('');
            errorMsg.classList.add("hide");

            // Convert placeholder as exploitable mask by replacing all 1-9 numbers with 0s
            mask = newPlaceholder.replace(/[1-9]/g, '0');

            // Apply the new mask for the input
            jQuery(this).mask(mask);

        });

        // When the plugin loads for the first time, we have to trigger the "countrychange" event manually, 
        // after making sure that the plugin is fully loaded by associating handler to the promise of the 
        // plugin instance.
        iti.promise.then(function() {
                jQuery(input).trigger('countrychange');
        });

    }

    // apply lib on popup show
    jQuery( document ).on( 'elementor/popup/show', () => {
        
        //script adapted from https://github.com/jackocnr/intl-tel-input/issues/921
        var ary = Array.prototype.slice.call(document.querySelectorAll('.lib_intlTelInput'));
            
        ary.forEach(function(el) {
            PhoneDisplay(el);
        });

    } );

});
</script><script>
    
//List all links on popup and concatenate slug in utm_content
jQuery(document).ready(function($) {
    
    // Get the current slug from the URL
    const currentSlug = window.location.pathname;
    
    // Check and define slug variable
    const slug = (currentSlug === '/') ? '-site-home' : '-site-' + currentSlug.slice(1, -1);
    
    //Loop to concatenate slug in each button parameter utm_content
    $('.botao_saibamais_popup .elementor-button-link').each(function(index, element) {
        
        // Get the href attribute value
        let href = $(this).attr('href');
        
        // Replace "#" with "&" in the URL
        let url = href.replace("#", "&");
        
        // Extract the domain and remove everything after the "?"
        const getContentBeforeQuestionMark = (url) => {
            const index = url.indexOf('?');
            const result = (index !== -1) ? url.substring(0, index) : url;
            return result;
        }
        
        let domainUrl = getContentBeforeQuestionMark(url);
        let testar = href.includes("-site-");
      
        if (!testar) {
            
            const parametros = {};
            
            // Remove the domain and the initial part of the URL
            let queryString = url.substring(url.indexOf('?') + 1);
            
            // Split parameters by "&"
            let parametrosArray = queryString.split('&');
            
            // Loop through each parameter
            for (let i = 0; i < parametrosArray.length; i++) {
                let parametro = parametrosArray[i].split('=');
                let nome = decodeURIComponent(parametro[0]);
                let valor = decodeURIComponent(parametro[1]);
                
                // Store the parameter and value in the parametros object
                parametros[nome] = valor;
            }
            
            // Concatenate all parameters and values
            let parametrosConcatenados = "";
            for (let key in parametros) {
                if (key === "utm_content") {
                    parametrosConcatenados += key + "=" + parametros[key] + slug + "&";
                } else {
                    parametrosConcatenados += key + "=" + parametros[key] + "&";
                }
            }
            parametrosConcatenados = domainUrl + "?" + parametrosConcatenados.slice(0, -1);
            
            $(this).attr('href', parametrosConcatenados);
            parametrosConcatenados = "";
            
        }
    
    });

});

</script><script type="text/javascript" id="eae-main-js-extra">
