	jQuery(document).ready(function() {
		var vids = jQuery("#slideshow456 .scroll-item");
		for(var i = 0; i < vids.length; i+=3) {
		  vids.slice(i, i+3).wrapAll('<div class="group_items"></div>');
		}
		jQuery(function() {
			jQuery('#slideshow456').cycle({
				fx:     'scrollHorz',
				timeout: 3000,
				pager:  '#nav456',
				slideExpr: '.group_items',
				speed: 300,
				slideResize: false,
				pause: true
			});
		});
  });
