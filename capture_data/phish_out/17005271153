<script>
    $(function(){
    $('#area').change(function(){
       var selected = $(this).find('option:selected');
       var shipPrice = selected.data('ship');
       var quantity  = $('#quantity').val();
       var price  = $('#price').val();

       var subTotal = parseInt(price)*parseInt(quantity);
       var totalPrice = (parseInt(price)*parseInt(quantity)) + parseInt(shipPrice);


       $('#subTotal').html('৳'+subTotal);
       $('#shipPrice').html('৳'+shipPrice);
       $('#totalPrice').html('৳'+totalPrice);


//alert(name);

//$('#ptype').html(ptype);




    });
});

    </script>
    <script>
         $('#quantity').change(function(){
       var selected = $('#area').find('option:selected');
       var shipPrice = selected.data('ship');
       var quantity  = $(this).val();
       var price  = $('#price').val();


       var subTotal = parseInt(price)*parseInt(quantity);
       var totalPrice = (parseInt(price)*parseInt(quantity)) + parseInt(shipPrice);


       $('#subTotal').html('৳'+subTotal);
       $('#shipPrice').html('৳'+shipPrice);
       $('#totalPrice').html('৳'+totalPrice);


//alert(name);

//$('#ptype').html(ptype);




    });
    </script>
