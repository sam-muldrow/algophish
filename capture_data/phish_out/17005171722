<style type="text/css">

#map_canvas {

	width:100%;
	height: 500px;
}

</style>

<script type="text/javascript">
	
 var total = 0;
 var pricetotal = 0;
 var startpunkt_foremail = '';
 var endpunkt_foremail = '';
 
function getXmlHttp() {
    var xmlhttp;
    try {
      xmlhttp = new ActiveXObject("Msxml2.XMLHTTP");
    } catch (e) {
    try {
      xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
    } catch (E) {
      xmlhttp = false;
    }
    }
    if (!xmlhttp && typeof XMLHttpRequest!='undefined') {
      xmlhttp = new XMLHttpRequest();
    }
    return xmlhttp;
  }

var directionDisplay;
  var directionsService = new google.maps.DirectionsService();
  var map;


  function initialize() {
	var shirota = 54.64885394;
	var dolgota = 21.3497925;
    directionsDisplay = new google.maps.DirectionsRenderer();
    var myOptions = {
      zoom:9,
      mapTypeId: google.maps.MapTypeId.ROADMAP,
      center: new google.maps.LatLng(shirota, dolgota)
    }
    map = new google.maps.Map(document.getElementById("map_canvas"), myOptions);
	
    directionsDisplay.setMap(map);
	
	

    google.maps.event.addListener(directionsDisplay, 'directions_changed', function() {

      computeTotalDistance(directionsDisplay.directions);

    });    
	
  }
  
  function calcRoute() {
	  var startmod = '';
	  if (startmod == '') {
    var start = document.getElementById("name_street").value;
    } else {
	    var start = startmod;
    }
    var end = document.getElementById("end").value;
    var request = {
        origin:start, 
        destination:end,
        travelMode: google.maps.DirectionsTravelMode.DRIVING
    };
    directionsService.route(request, function(response, status) {
      if (status == google.maps.DirectionsStatus.OK) {
        directionsDisplay.setDirections(response);
      }
    });
  }
  
  function computeTotalDistance(result) {

   

    var myroute = result.routes[0];

    for (i = 0; i < myroute.legs.length; i++) {

      total += myroute.legs[i].distance.value;
	
    }

    total = total / 1000.
    var totalfinal = 0;
    var fixkilomprice = 30000; // Цена на фиксировванные километры
 
    var totalminusfree = total-0; // Количество километров за вычетом бесплатных
    var fixkillometres = 30; // Количество километров за фиксированную плату
    totalfinal = totalminusfree - fixkillometres; // Количество километров без фиксированных
    
    var okrmath = 1+0;
    
    
    if (totalminusfree >= fixkillometres) {
    ///Если указано в настройках, то округляем
    	pricetotal = totalfinal*150 + fixkilomprice;
  		  if (okrmath == 1) {
  		  	pricetotal = Math.floor (totalfinal*150 + fixkilomprice) ;
  		  	total = Math.floor(total);
  		  	}
  		 
    }
     if (totalminusfree <= fixkillometres) {
	   pricetotal = fixkilomprice;
	   if (okrmath == 1) {
		   total = Math.floor(total);
		   pricetotal = Math.floor(pricetotal);
	   }
    }
    
      
      var checkdost = '1';
      if (checkdost == 1) {
      if (pricetotal <= 0) {
	    	 pricetotal = 'Ваша цена сегодня:' + "<span class='prtype'>Бесплатно</span>";
			
	     } else {
		    pricetotal = 'Ваша цена сегодня:' + '<span class="prtype">' + pricetotal + '</span>' + ' ' + "<span class='prtype'>Руб.</span>";
		      
	     }
	     document.getElementById("totalprice").innerHTML =  pricetotal;
	     }
      
	total = 'Расстояние:' + '<span class="totalkm" id="totalkmid">' + total + '</span>' + '<span class="totalkm">' + ' ' +  'км</span>';

    document.getElementById("total").innerHTML = total;
       
    		
		document.getElementById('formemail').style.display = 'block';
		document.getElementById('raschetform').style.display = 'none';
		document.getElementById("titlezakaz").innerHTML = '<h3 class="zakaztitle">Оформление заказа</h3>';
		startpunkt_foremail = 'Адрес погрузки (Страна, Город, Улица, Дом)' + ': ' + document.getElementById('name_street').value;
		endpunkt_foremail = 'Адрес выгрузки  (Страна, Город, Улица, Дом)' + ': ' + document.getElementById('end').value;
		
;	                          
		
		
  }   


  function returnRaschet() {
	  document.getElementById('formemail').style.display = 'none';
	  document.getElementById('raschetform').style.display = 'block';
	  document.getElementById("titlezakaz").innerHTML = '';
	  total = 0;
	  pricetotal = 0;
	  startpunkt_foremail = '';
	  endpunkt_foremail = '';
  }
  
  
  
  function sendForm () {
	  
	 email_subject = 'заказ перевозки с сайта ЦЕНТРПРОДУКТ.РФ';
	 emailadmin = 'centrprodukt@gmail.com';
	 langpreme = 'ru-RU';
	 messagetext = 'Поступил заказ на доставку. Данные заказа:';
	 textforuser = 'От вас поступил заказ. Данные заказа: ';
	 emailforuser_on = '1';
	 numberzakaz_on =  '1';
	 numberzakaz_title = 'Номер заказа: ';
	 
	  
		
		datafrom_rast = document.forms["formzakaz"].elements["datafrom_rast"].value;
		datato_rast = document.forms["formzakaz"].elements["datato_rast"].value;

		
		if (!datafrom_rast) {
			
			datafrom_rast = '';
			
		}
		else {
			datafrom_rast = 'Дата отправления:  ' + datafrom_rast;
		}
		
		if (!datato_rast) {
			
			datato_rast = '';

		}
		else {
			datato_rast = 'Дата прибытия:  ' + datato_rast;
		}
		
		
		
	phonezakaz_rast = document.forms["formzakaz"].elements["phonezakaz_rast"].value;
	emailzakaz_rast = document.forms["formzakaz"].elements["emailzakaz_rast"].value;
	
	// Проверяем заполнено ли поле email в админ панели модуля
	
	if (!emailadmin) {
		document.getElementById("messagezakaz").innerHTML = 'В настройках модуля не указан email администратора. Отправка заказа невозможна!';
		return;
	}
	
	// Проверяем заполнено ли поле email пользователем
	
	if (!emailzakaz_rast) {
		document.getElementById("messagezakaz").innerHTML = 'Не заполнено поле Email';
		return;
	}
	
	var re = /^[\w-\.]+@[\w-]+\.[a-z]{2,4}$/i;
    var valid = re.test(emailzakaz_rast);
    
    // Проверяем поле на валидность
    
    if (!valid) {
	    document.getElementById("messagezakaz").innerHTML = 'Не корректно заполнено поле Email';
		return;
    }
	
	// Проверяем заполнено ли поле телефон
	
	if (!phonezakaz_rast) {
		document.getElementById("messagezakaz").innerHTML = 'Не заполнено поле телефон';
		return;
	}
	
	var rephone = /^((8|\+7)[\- ]?)?(\(?\d{3}\)?[\- ]?)?[\d\- ]{7,10}$/i;
    var valid = rephone.test(phonezakaz_rast);
    
    // Проверка телефона на валидность
    
    if (!valid) {
	    document.getElementById("messagezakaz").innerHTML = 'Некорректно заполнено поле телефон';
		return;
    }
	
	phonezakaz_rast = 'Ваш телефон:  ' + phonezakaz_rast;
	
	
	var xmlhttp = getXmlHttp(); 
    xmlhttp.open('POST', 'https://centrprodukt.com/modules/mod_raschetrastoyaniy/form.php', true); 
    xmlhttp.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded'); 
    
    // Отправка данных
    
    
  
    xmlhttp.send("startpunkt_foremail=" + encodeURIComponent(startpunkt_foremail) + "&endpunkt_foremail=" + encodeURIComponent(endpunkt_foremail) + "&total=" + encodeURIComponent(total) + "&pricetotal=" + pricetotal + "&datafrom_rast=" + encodeURIComponent(datafrom_rast) + "&datato_rast=" + encodeURIComponent(datato_rast) + "&phonezakaz_rast=" + encodeURIComponent(phonezakaz_rast) + "&emailzakaz_rast=" + encodeURIComponent(emailzakaz_rast) + "&email_subject=" + encodeURIComponent(email_subject) + "&emailadmin=" + encodeURIComponent(emailadmin) + "&messagetext=" + encodeURIComponent(messagetext) + "&textforuser=" + encodeURIComponent(textforuser) + "&emailforuser_on=" + encodeURIComponent(emailforuser_on) + "&numberzakaz_on=" + encodeURIComponent(numberzakaz_on) + "&numberzakaz_title=" + encodeURIComponent(numberzakaz_title)); 
    

    xmlhttp.onreadystatechange = function() { 
      if (xmlhttp.readyState == 4) { 
        if(xmlhttp.status == 200) { 
          document.getElementById("formemail").innerHTML = '<div class="thanksmessage">Спасибо за ваш заказ! Наши менеджеры свяжутся с вами в ближайшее время!</div><div class="returntomod"><a href="https://centrprodukt.com:443/" class="retunlink">Закрыть</a>';
          document.getElementById('raschetform').style.display = 'none';
          document.getElementById('map_canvas').style.display = 'none';
          document.getElementById('directionsPanel').style.display = 'none';
          document.getElementById("messagezakaz").style.display = 'none';
        }
      }
    };
  }
           


</script>
