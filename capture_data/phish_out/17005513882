      <script type="text/javascript">
				var time_start;
				$(window).on("load", function (e) {
					time_start = new Date();
				});
				$(window).on("unload", function (e) {
					var time_end = new Date();
					var pagetime = new FormData();
					pagetime.append("type", "pagetime");
					pagetime.append("id_connections", "6301411");
					pagetime.append("id_page", "2");
					pagetime.append("time_start", "2023-11-21 08:23:09");
					pagetime.append("token", "3ce016b58cb8ff81f691ac2113b47ecfba2eb658");
					pagetime.append("time", time_end-time_start);
					pagetime.append("ajax", "1");
					navigator.sendBeacon("https://contecom.it/index.php?controller=statistics", pagetime);
				});
			</script> 
    
  </body>

</html>
