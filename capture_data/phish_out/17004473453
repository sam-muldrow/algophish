<script async src="https://www.googletagmanager.com/gtag/js?id=G-RSLF90XLYZ"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  // Функция для получения актуальной цены
  function getCurrentPrice(selector) {
    var priceElement = document.querySelector(selector);
    var priceText = priceElement ? priceElement.textContent.trim() : '';
    return parseFloat(priceText.replace(/[^\d.]/g, '')) || 0; // Возвращаем 0, если цена не найдена
  }

  // Функция для передачи данных о товаре в аналитический инструмент
  function sendViewItemEvent(price) {
    if (!isNaN(price) && price > 0) {
      gtag('event', 'view_item', {
        'send_to': 'G-RSLF90XLYZ',
        'event_category': 'product',
        'value': price,
      });
    }
  }

  // Получение цены товара
  var regularPrice = getCurrentPrice('.autocalc-product-price');
  var specialPrice = getCurrentPrice('.autocalc-product-special');

  // Передача параметра view_item в аналитический инструмент для обычной цены
  sendViewItemEvent(regularPrice);

  // Передача параметра view_item в аналитический инструмент для цены с акцией
  sendViewItemEvent(specialPrice);

  // Обработчик события для кнопки "Добавить в корзину"
  document.getElementById('button-cart').addEventListener('click', function() {
    // Получение информации о товаре, который будет добавлен в корзину
    var productId = '42'; // Идентификатор товара
    var productName = 'Название товара'; // Название товара
    var productPrice = getCurrentPrice('.autocalc-product-price'); // Цена товара

    // Обновление информации о цене в разметке сайта
    var cartTotalElement = document.getElementById('cart-total');
    var currentCartTotal = cartTotalElement.innerHTML;
    var newCartTotal = currentCartTotal.replace(/\(\d+UAH\)/, '(' + productPrice + 'UAH)');
    cartTotalElement.innerHTML = newCartTotal;

    // Вызов функции добавления товара в корзину в OpenCart 2.3
    $.ajax({
      url: 'index.php?route=checkout/cart/add',
      type: 'post',
      data: 'product_id=' + productId + '&quantity=1',
      dataType: 'json',
      success: function(json) {
        // Обработка успешного добавления товара в корзину
        if (json['success']) {
          // Ваш код обработки успешного добавления товара в корзину
          console.log('Товар успешно добавлен в корзину');
          // Передача параметра add_to_cart в аналитический инструмент
          sendAddToCartEvent(productPrice);
        }
      }
    });

    // Функция для передачи данных о добавленном товаре в аналитический инструмент
    function sendAddToCartEvent(price) {
      gtag('event', 'add_to_cart', {
        'send_to': 'G-RSLF90XLYZ',
        'event_category': 'ecommerce',
        'value': price,
        'items': [{
          'id': productId,
          'name': productName,
          'price': price,
          'quantity': 1
        }]
      });
    }

   // Передача параметра add_to_cart в аналитический инструмент
    sendAddToCartEvent(productPrice);
  });

// Переменная для хранения суммы заказа
var orderTotal = 0;

// Обработчик события для успешного оформления заказа
function handleOrderCompletion() {
  // Проверка, что сумма заказа больше 0
  if (orderTotal > 0) {
    // Вызов функции отправки данных о заказе в аналитический инструмент
    sendPurchaseEvent(orderTotal);
  } else {
    console.log('Сумма заказа не определена или меньше или равна 0.'); // Отладочный вывод
  }
}

// Получение информации о заказе и обновление значения суммы заказа
function updateOrderTotal() {
  var pricesElements = document.querySelectorAll('.prices');
  var lastPriceElement = pricesElements[pricesElements.length - 1];
  var orderTotalMatch = lastPriceElement.innerText.match(/(\d+(?:\.\d+)?) грн/);
  if (orderTotalMatch) {
    orderTotal = parseFloat(orderTotalMatch[1]);
  } else {
    console.log('Не удалось извлечь сумму заказа.'); // Отладочный вывод
  }
}

// Вызов функции updateOrderTotal при загрузке страницы
document.addEventListener('DOMContentLoaded', updateOrderTotal);

// Добавление Mutation Observer для отслеживания изменений в .prices
var observer = new MutationObserver(updateOrderTotal);
var pricesElements = document.querySelectorAll('.prices');
var lastPriceElement = pricesElements[pricesElements.length - 1];
observer.observe(lastPriceElement, { childList: true, subtree: true });

// Вызов функции handleOrderCompletion при успешном оформлении заказа (для тестирования)
handleOrderCompletion();

// Функция для передачи данных о заказе в аналитический инструмент
function sendPurchaseEvent(total) {
  gtag('event', 'purchase', {
    'send_to': 'G-XDCFZELT42',
    'event_category': 'ecommerce',
    'value': total,
    'currency': 'UAH'
  });
}


</script>

