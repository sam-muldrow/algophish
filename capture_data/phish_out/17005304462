      <script>
        var slideIndex = 0;

        showGood();
        function showGood() {
          var i;
          var slides = document.getElementsByClassName("btm_bx1");
          for (i = 0; i < slides.length; i++) {
            slides[i].style.display = "none";
          }
          slideIndex++;
          if (slideIndex > slides.length) {slideIndex = 1}
          slides[slideIndex-1].style.display = "block";
          setTimeout(showGood, 3500); // Change image every 2 seconds
        }

        showBetter();
        function showBetter() {
          var i;
          var slides = document.getElementsByClassName("btm_bx2");
          for (i = 0; i < slides.length; i++) {
            slides[i].style.display = "none";
          }
          slideIndex++;
          if (slideIndex > slides.length) {slideIndex = 1}
          slides[slideIndex-1].style.display = "block";
          setTimeout(showBetter, 3200); // Change image every 2 seconds
        }

        showBest();
        function showBest() {
          var i;
          var slides = document.getElementsByClassName("btm_bx3");
          for (i = 0; i < slides.length; i++) {
            slides[i].style.display = "none";
          }
          slideIndex++;
          if (slideIndex > slides.length) {slideIndex = 1}
          slides[slideIndex-1].style.display = "block";
          setTimeout(showBest, 3000); // Change image every 2 seconds
        }
      </script>

    
