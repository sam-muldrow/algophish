<script type="rocketlazyloadscript" data-rocket-type="text/javascript" id="sidr-js-after">window.addEventListener('DOMContentLoaded', function() {
/* <![CDATA[ */
						jQuery(document).ready(function($){
						    $(".flyout-trigger-id-8516").sidr({
								name: "flyout-content-id-8516",
								side: "right",
								displace: false,
								speed: 300,
								onOpen: function(name) {
									$(".flyout-overlay").fadeIn(300).toggleClass("active");
									$("#flyout-content-id-8516").toggleClass("flyout-open");
								    $(".flyout-overlay, .flyout-close").on("click", function() {
										$.sidr("close", "flyout-content-id-8516");
									});
								},
								onClose: function(name) {
									$(".flyout-overlay").fadeOut(300).toggleClass("active");
									$("#flyout-content-id-8516").toggleClass("flyout-open");
								}
						    });
						});
					

