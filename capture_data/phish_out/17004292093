	function resizeHero()
	{
		var width = jQuery(window).width();
	    var height = (width * 773) / 1920;
	    jQuery("#carouselHacked.carousel, #carouselHacked .carousel-inner, #carouselHacked .carousel-inner .item, #carouselHacked .carousel-inner .item .carousel-overlay, #slideshow").css('height', height + 'px');
	}
  });</script>
