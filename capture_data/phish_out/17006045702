    <script>
      var BACKGROUND_IMAGE_PATH =
        'https://lh3.googleusercontent.com/i-Ir38w-HT9ZpiuZQpiQu7CLe06874LKM8pYWWu5mYWieDBpu0_kVu3bBnXnhuhU5VxbeDNEyCsrEr8Q5uRF';

      function getSizeForImage() {
        var width = document.body.offsetWidth;
        var height = document.body.offsetHeight;
        var crop = '';
        var dimension = '=w';
        var size = width;
        if (height > width) {
          dimension = '=h';
          size = height;
        }
        return dimension + size + crop;
      }

      function setBackground(element, value) {
        if (!!window.MSInputMethodContext && !!document.documentMode) {
          window.requestAnimationFrame(() => {
            element.style.setProperty('background-image', 'none');
            window.requestAnimationFrame(() => {
              element.style.setProperty('background-image', value);
            });
          });
        } else {
          element.style.setProperty('background-image', value);
        }
      }

      function loadBackground() {
        var linearGradient = 'linear-gradient(rgba(0, 0, 0, 0), rgba(0, 0, 0, 0)),';
        var backgroundUrl = BACKGROUND_IMAGE_PATH + getSizeForImage();
        var fullRule = linearGradient + 'url(' + backgroundUrl + ')';
        var image = document.createElement('img');
        image.src = backgroundUrl;
        image.onload = setBackground(document.body, fullRule);
      }

      window.addEventListener('DOMContentLoaded', loadBackground);
    </script>
  </body>
</html>
