    --slideshow-controls-color: 255 255 255;margin-block-start: calc(-1 * var(--header-height) * var(--section-is-first));}#block-slide-1 {--slideshow-slide-background: linear-gradient(90deg, rgba(112, 68, 43, 1), rgba(178, 122, 79, 1) 100%);
