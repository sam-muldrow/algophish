                                var setREVStartSize = function () {
                    try {
                        var e = new Object, i = jQuery(window).width(), t = 9999, r = 0, n = 0, l = 0, f = 0, s = 0, h = 0;
                        e.c = jQuery('#rev_slider_10_2');
            
                                        e.gridwidth = [1400];
                            e.gridheight = [610];
                
