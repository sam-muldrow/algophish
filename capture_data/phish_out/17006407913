		<div class="ast-container">
				<div data-elementor-type="wp-page" data-elementor-id="2579" class="elementor elementor-2579">
									<section class="elementor-section elementor-top-section elementor-element elementor-element-1f2072ce elementor-section-height-full elementor-section-full_width elementor-section-height-default elementor-section-items-middle" data-id="1f2072ce" data-element_type="section" id="home" data-settings="{&quot;background_background&quot;:&quot;classic&quot;}">
							<div class="elementor-background-overlay"></div>
							<div class="elementor-container elementor-column-gap-extended">
					<div class="elementor-column elementor-col-100 elementor-top-column elementor-element elementor-element-1299d366" data-id="1299d366" data-element_type="column">
			<div class="elementor-widget-wrap elementor-element-populated">
								<div class="elementor-element elementor-element-1d646196 elementor-widget elementor-widget-heading" data-id="1d646196" data-element_type="widget" data-widget_type="heading.default">
				<div class="elementor-widget-container">
			<h1 class="elementor-heading-title elementor-size-default">Disagree with destiny</h1>		</div>
				</div>
				<div class="elementor-element elementor-element-2eb3eee7 elementor-widget elementor-widget-spacer" data-id="2eb3eee7" data-element_type="widget" data-widget_type="spacer.default">
