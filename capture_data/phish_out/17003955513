<script>
	var url = "/component/jbusinessdirectory/?Itemid=437";
	var minLetters = 2;
	var keyword;

	function checkSearch941(){
		jQuery("#dir-container-941 #searchkeyword").removeClass("required");
					if(document.getElementById('searchkeyword') && jQuery("#dir-container-941 #searchkeyword").val().length == 0){
				jQuery("#dir-container-941 #searchkeyword").focus();
				jQuery("#dir-container-941 #searchkeyword").addClass("required");
				return false;
			}
		
		if(document.getElementById('searchkeyword') && document.getElementsByClassName('checkNumberChars-941') && jQuery("#dir-container-941 .checkNumberChars-941").val().length > 0 && minLetters!=0) {
            if(!checkNumberLetters(941)) {
            	alert('Le nombre minimum de lettres de recherche doit être au moins 2');
                return false;
            }
        }

		jQuery("#dir-container-941 #categories-1").removeClass("required");
		jQuery("#dir-container-941 #categories_1_chosen").removeClass("required");
					var foo = jQuery("#dir-container-941 #categories-1");
			if (foo) {
				if (foo.find(":selected").index() == 0) {
					jQuery("#dir-container-941 #categories-1").focus();
					jQuery("#dir-container-941 #categories-1").addClass("required");
					jQuery("#dir-container-941 #categories_1_chosen").addClass("required");
					return false;
				}
			}
		
		jQuery("#dir-container-941 #typeSearch").removeClass("required");
		jQuery("#dir-container-941 #typeSearch_chosen").removeClass("required");
					var foo = jQuery("#dir-container-941 #typeSearch");
			if (foo) {
				if (foo.find(":selected").index() == 0) {
					jQuery("#dir-container-941 #typeSearch").focus();
					jQuery("#dir-container-941 #typeSearch").addClass("required");
					jQuery("#dir-container-941 #typeSearch_chosen").addClass("required");
					return false;
				}
			}
		
        jQuery(".zipcode-941").removeClass("required");
        
		jQuery("#dir-container-941 #countrySearch").removeClass("required");
		jQuery("#dir-container-941 #countrySearch_chosen").removeClass("required")
		
        jQuery("#dir-container-941 #provinceSearch").removeClass("required");
        jQuery("#dir-container-941 #provinceSearch_chosen").removeClass("required")
        
		
        //loading button animation if validation is OK
        jQuery("#dir-container-941 .search-dir-button").addClass("loader");
        
		return true;
	}

	function checkNumberLetters(token) {
	    var keyword = jQuery("#dir-container-"+token +" .checkNumberChars-"+token).val();
	    if(minLetters !== undefined){
            return keyword.length >= minLetters;
        }else{
            var minLettersCheck = 2;
            return keyword.length >= minLettersCheck;
        }
    }

	window.addEventListener('load', function(){
		        jQuery(".chosen-select").chosen({width:"95%", disable_search_threshold: 5, search_contains: true, placeholder_text_single: "Sélectionnez une option" , placeholder_text_multiple: "Sélectionnez une option"});

        
		    		jQuery("#dir-container-941 #searchkeyword").autocomplete({
    			source: "/component/jbusinessdirectory/?task=categories.getCategories&amp;type=1&amp;Itemid=437",
    			minLength: 0,
    			search: function( event, ui ) {
    				  jQuery("#dir-container-941 #keyword-search-suggestion").slideUp(500);
        			},
    			select: function( event, ui ) {
                    jQuery('#dir-container-941 #categorySuggestion').val(ui.item.id);
                    keyword = ui.item.label;
    				jQuery(this).val(ui.item.label);
    				return false;
    			}
    		});
    
            jQuery("#searchkeyword").change(function() {
                var searchKeyword = jQuery("#searchkeyword").val();
                if (searchKeyword != keyword){
                    jQuery('#categorySuggestion').val("");
                }
            });
		
        jQuery(".zipcode-941").change(function() {
        	jQuery('#dir-container-941 #regionSearch').val('');
    		jQuery('#dir-container-941 #citySearch').val('');
          
        });
	

        jQuery(".zipcode-941").focusin(function() {
        	jQuery("#dir-container-941 #location-search-suggestion").slideDown(500);
        });
        jQuery(".zipcode-941").focusout(function() {
        	jQuery("#dir-container-941 #location-search-suggestion").slideUp(500);
        });

        jQuery("#dir-container-941 #searchkeyword").focusin(function() {
            jQuery("#dir-container-941 #keyword-search-suggestion").slideDown(500);
        });
        jQuery("#dir-container-941 #searchkeyword").focusout(function() {
            jQuery("#dir-container-941 #keyword-search-suggestion").slideUp(500);
        });

            });

    function initializeSearchAutocomplete941(preventSubmit) {
    	jbdUtils.initializeSearchAutocomplete(941, preventSubmit);
    }

    function disableEmptyFields() {
        //disable all empty fields to have a nice url
        jQuery('#dir-container-941').submit(function() {
            jQuery(':input', this).each(function() {
                this.disabled = !(jQuery(this).val());
            });

            jQuery('#dir-container-941 select').each(function() {
                if(!(jQuery(this).val()) || jQuery(this).val()==0 && !jQuery(this).hasClass('required')){
                    jQuery(this).attr('disabled', 'disabled');
                }
            });
        });
    }

    function enableGeoLocation941(){
        if (navigator.geolocation) {
            navigator.geolocation.getCurrentPosition(setGeoLocation941);
            jQuery("#loading-geo-locaiton").show();
            jQuery(".la la-bullseye").addClass("dir-beat-animation");
        }
        jQuery("#enable-geolocation-941").addClass("active");
        jQuery("#disable-geolocation-941").removeClass("active");
        jQuery("#dir-container-941 #geolocation").val(1);
    }

    function disableGeoLocation941(){
        jQuery("#enable-geolocation-941").removeClass("active");
        jQuery("#disable-geolocation-941").addClass("active");
        jQuery("#dir-container-941 #geolocation").val(0);
        jQuery("#loading-geo-locaiton-"+token).hide();
        jQuery(".la la-bullseye").removeClass("dir-beat-animation");
        jQuery("#dir-container-941 #geo-latitude").val('');
        jQuery("#dir-container-941 #geo-longitude").val('');
        jQuery("#dir-container-941 #geo-country").val('');
    }

	function setGeoLocation941(position){
		jQuery("#loading-geo-locaiton").hide();
		jQuery(".la la-bullseye").removeClass("dir-beat-animation");
		var latitude = position.coords.latitude;
		var longitude = position.coords.longitude;
		jQuery("#dir-container-941 #geo-latitude").val(latitude);
		jQuery("#dir-container-941 #geo-longitude").val(longitude);
        codeLatLng(latitude,longitude);
	}

    function codeLatLng(lat, lng) {
	    var location = [];
	    location.latitude  = lat;
	    location.longitude = lng;

        var callback = function(country_code) {
            jQuery("#geo-country").val(country_code);
        };

        JBDMapHelper.reverseGeocoding(location, callback);
    }

	
	
	
	function showCategorySelect941(level, parentId, id){
        var urlGetCategories = "/component/jbusinessdirectory/?task=categories.getSubcategoriesByParentIdAjax&amp;Itemid=437";
		var selectedCat;
		if(parentId===undefined)
			 selectedCat = jQuery('#dir-container-941 #categories-'+level).find(":selected").val();
		else
			 selectedCat = parentId;

		var categoryType = 1;
		jQuery('#dir-container-941 #categorySearch').val(id);

		if(selectedCat != 0) {
							jQuery.ajax({
					type: "GET",
					url: urlGetCategories,
                    data: {parentId: selectedCat, categoryType: categoryType, level: level, categoryId: id, token: '941', option:'com_jbusinessdirectory'},
                    dataType: 'json',
					success: function (data) {
						jQuery('#dir-container-941 .categories-form-field').each(function () {
							if (jQuery(this).attr('id') > level)
								jQuery(this).remove();
						});
						if (data != null){
						    if (jQuery('#dir-container-941 .categories-form-field').length == 1) {
                                jQuery('#categories-container-941').after(data);
                            } else {
                                jQuery('#dir-container-941 .categories-form-field').last().after(data);
                            }

													}
					}
				});
			
			if(parentId === undefined)
				jQuery('#dir-container-941 #categorySearch').val(selectedCat);
			else
				jQuery('#dir-container-941 #categorySearch').val(id);
		}
	}

	function setSuggestedCategory(name, value){
		jQuery('#dir-container-941 #searchkeyword').val(name);
		jQuery('#dir-container-941 #categorySuggestion').val(value);
		jQuery('#dir-container-941 #suggestionType').val(4);
	}

	function setSuggestedRegion(name, value){
		jQuery('#dir-container-941 #regionSearch').val(name);
		jQuery('#dir-container-941 #citySearch').val("");
		jQuery('#dir-container-941 #zipcode').val(name);
	}

	function setSuggestedCity(name, value){
		jQuery('#dir-container-941 #citySearch').val(name);
		jQuery('#dir-container-941 #regionSearch').val("");
		jQuery('#dir-container-941 #zipcode').val(name);
	}

	</script></div></div></div></div></div></div></div></div></div></div><section id="section-id-1542210153165" class="sppb-section" ><div class="sppb-row-overlay"></div><div class="sppb-row-container"><div class="sppb-row"><div class="sppb-col-md-4" id="column-wrap-id-1542210153162"><div id="column-id-1542210153162" class="sppb-column" ><div class="sppb-column-addons"></div></div></div><div class="sppb-col-md-4" id="column-wrap-id-1542210153163"><div id="column-id-1542210153163" class="sppb-column" ><div class="sppb-column-addons"></div></div></div><div class="sppb-col-md-4" id="column-wrap-id-1542210153164"><div id="column-id-1542210153164" class="sppb-column" ><div class="sppb-column-addons"></div></div></div></div></div></section><section id="section-id-1588770755482" class="sppb-section" ><div class="sppb-row-container"><div class="sppb-section-title sppb-text-center"><h1 class="sppb-title-heading">ANIMATIONS COMMERCIALES A VENIR</h1></div><div class="sppb-row"><div class="sppb-col-md-12" id="column-wrap-id-1588770755481"><div id="column-id-1588770755481" class="sppb-column" ><div class="sppb-column-addons"><div id="sppb-addon-wrapper-1588770755478" class="sppb-addon-wrapper"><div id="sppb-addon-1588770755478" class="clearfix "     ><div class="sppb-addon sppb-addon-text-block sppb-text-center "><h2 class="sppb-addon-title">Découvrez les animations et opérations commerciales de l'ACAPIC</h2><div class="sppb-addon-content"></div></div></div></div></div></div></div></div></div></section><section id="section-id-1690466673773" class="sppb-section" ><div class="sppb-row-container"><div class="sppb-row"><div class="sppb-col-md-6" id="column-wrap-id-1690466673771"><div id="column-id-1690466673771" class="sppb-column" ><div class="sppb-column-addons"><div id="sppb-addon-wrapper-1695040637852" class="sppb-addon-wrapper"><div id="sppb-addon-1695040637852" class="clearfix "     ><div class="sppb-addon sppb-addon-feature  "><div class="sppb-addon-content sppb-text-center"><sp