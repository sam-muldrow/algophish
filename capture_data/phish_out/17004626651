			<script src="/templates/sproutD/js/jquery-3.5.1.min.js"></script>
			
<script>
				
//получаем курсы битка и выводим их
$.getJSON( "https://api.coindesk.com/v1/bpi/currentprice/rub.json", function( data) {

   let rubCurr = parseInt(data.bpi.RUB.rate_float);
   $('#rubCurr').html(numberWithSpaces(rubCurr));

});

//форматирование числа в французской нотации
function numberWithSpaces(x) {
    var parts = x.toString().split(".");
    parts[0] = parts[0].replace(/\B(?=(\d{3})+(?!\d))/g, " ");
    return parts.join(".");
}
				
				
</script>           
        </div>

