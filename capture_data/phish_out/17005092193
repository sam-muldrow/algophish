	if(document.getElementById("hc")) {
    document.getElementById("helpcenter").style.display = "block";
	document.getElementById("bga").style.background = "#FFF url('https://www.cardpresso.com/images/backgrounds/bgHelpCenter.jpg') no-repeat left top fixed";
	} 
}
