function editionsFunction() {
    var x = document.URL;
	if (x == "https://www.cardpresso.com/editions-cp") {
	document.getElementById("editions").style.display = "block";
	document.getElementById("bga").style.background = "#FFF url('https://www.cardpresso.com/images/backgrounds/bgEditions.jpg') no-repeat left top fixed";
    }
	else if (x == "https://www.cardpresso.com/editions-cp#") {
	document.getElementById("editions").style.display = "block";
	document.getElementById("bga").style.background = "#FFF url('https://www.cardpresso.com/images/backgrounds/bgEditions.jpg') no-repeat left top fixed";
    }
	else {
	document.getElementById("editions").style.display = "none";
    }
}	

//HELPCENTER
