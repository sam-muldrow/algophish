				<article id="post-74" class="post-74 page type-page status-publish hentry">

				
					<div class="entry-content">
					<div class="et_pb_section et_pb_fullwidth_section  et_pb_section_0 et_section_regular">
				
				
					
					<div class="et_pb_module et_pb_slider  et_pb_fullwidth_slider_0">
				<div class="et_pb_slides">
					 <div class="et_pb_slide et_pb_bg_layout_dark et_pb_media_alignment_center et_pb_slide_0 et-pb-active-slide" style='background-color:#ffffff;background-image: url(https://ariadnenetwork.eu/wp-content/uploads/2017/07/home_pge_image.jpg);'>
				
				
				
