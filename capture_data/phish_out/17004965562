      <script>
$('#busqueda').keyup(function() {
                
                //Si value esta vacio
                if (jQuery(this).val().length == 0)  {
                    
                    //Mostramos el elemento
                    jQuery('.div1').hide();
                }
                else
                {
                    //Ocultamos el elemento
                    jQuery('.div1').show();   
                }
                
            }).keyup();
</script>
   
          <script>
jQuery(obtener_registros());

function obtener_registros(alumnos)
{
	jQuery.ajax({
		url : 'consulta.php',
		type : 'POST',
		dataType : 'html',
		data : { alumnos: alumnos },
		})

	.done(function(resultado){
		jQuery("#tabla_resultado").html(resultado);
	})
}

jQuery(document).on('keyup', '#busqueda', function()
{
	var valorBusqueda=jQuery(this).val();
	if (valorBusqueda!="")
	{
		obtener_registros(valorBusqueda);
	}
	else
		{
			obtener_registros();
		}
});

</script>  
            
		 
