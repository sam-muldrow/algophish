  var skrollrSingleton;

  $(function() {
    if ($(window).width() > 640) {
      skrollrSingleton = skrollr.init();
    }

    if (skrollrSingleton.isMobile()) {
      skrollrSingleton.destroy();
    }

    $(window).on('resize', function () {
      skrollrSingleton = skrollr.init();
      if ($(window).width() <= 640 || skrollrSingleton.isMobile()) {
        if (skrollrSingleton) {
          skrollrSingleton.destroy();
        }
      }
    });
  });

</script><script src=//cdn.jsdelivr.net/jquery.scrollto/2.1.2/jquery.scrollTo.min.js></script><script src=https://cdnjs.cloudflare.com/ajax/libs/foundation/5.5.2/js/foundation.min.js type=text/javascript></script><script src=https://cdnjs.cloudflare.com/ajax/libs/foundation/5.5.2/js/foundation/foundation.clearing.min.js type=text/javascript></script><script src=https://cdnjs.cloudflare.com/ajax/libs/foundation/5.5.2/js/foundation/foundation.orbit.js type=text/javascript></script><script type=text/javascript src=//cdn.jsdelivr.net/jquery.slick/1.5.9/slick.min.js></script><script>
	var containerWidth = {}
	function shrinkHeaderMenu()
	{

		$("section.header-simple").map( function() {
			if(this.id){
				if ($("#" + this.id + ' .chord--header-simple__link-container').parent().is($("#" + this.id + ' .top-bar-section .right'))) {
					containerWidth[this.id] = $("#" + this.id + ' .chord--header-simple__link-container').width()
				}
				// console.log($("#" + this.id + ' .chord--header-simple__title-area').width() + ' + ' + containerWidth[this.id])
				if ($("#" + this.id + ' .menu-icon').is(':hidden') && $("#" + this.id + ' .chord--header-simple__title-area').width() + containerWidth[this.id] > $("#" + this.id + ' .chord--header-simple__nav').width()){
					// console.log("bigger than " + $("#" + this.id + ' .chord--header-simple__nav').width())
					if ( $("#" + this.id + ' .chord--header-simple__link-container').parent().is($("#" + this.id + ' .top-bar-section .right')) ) {
						// console.log("compress")
						$("#" + this.id + ' .chord--header-simple__link-container').wrap('<li class="has-dropdown chord--header-simple__link"></li>').wrap('<ul class="dropdown"></ul>')
						$("#" + this.id + ' .has-dropdown.chord--header-simple__link').prepend('<a href="#">'+ $("#" + this.id + ' .chord--header-simple__menu>a>span').text() +'</a>')
					}
				} else {
					// console.log("smaller than " + $("#" + this.id + ' .chord--header-simple__nav').width())
					if ( !$("#" + this.id + ' .chord--header-simple__link-container').parent().is($("#" + this.id + ' .top-bar-section .right')) ) {
						// console.log("expand")
						var element = $("#" + this.id + ' .chord--header-simple__link-container').filter(":first").detach()
						$("#" + this.id + ' .top-bar-section .right').empty()
						$("#" + this.id + ' .top-bar-section .right').append(element)
						containerWidth[this.id] = $("#" + this.id + ' .chord--header-simple__link-container').width()
					}
				}
			}
		})

	}
	function repeatShrink()
	{
		var handle = window.setInterval(shrinkHeaderMenu, 1000);
	}
	$(document).ready(function() {
		repeatShrink();
		shrinkHeaderMenu();
	});
