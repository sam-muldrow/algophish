<section id="home-cases" class="block-businesscases alignfull has-background has-primary-100-background-color py-8 lg:py-16">

    <div class="container">

                    <h2 class="mb-8">
                Business Cases            </h2>
        
        
        <div class="grid gap-8 lg:grid-cols-3">

            
                
<article id="post-1121" class="post-1121 business-case type-business-case status-publish has-post-thumbnail hentry bc-category-car-park-charge">

    <div class="bg-white group hover:shadow-sm rounded-lg overflow-hidden h-full hover:scale-105 transition-all duration-300 ease-in-out">

        <div class="aspect-4/3 relative overflow-hidden">
            <a href="https://circontrol.com/business-case/tele2-arena-parking-manages-160-ev-charging-points-with-dlm-premium/" class="w-full h-full block absolute inset-0" title="Tele2 Arena Parking manages 160 EV charging points with DLM premium">
            
