<!-- GET QUOTE MODAL -->
<!-- https://www.codexworld.com/bootstrap-modal-popup-form-submit-jquery-ajax-php/ -->
<script>
function submitContactForm(){
var reg = /^[A-Z0-9._%+-]+@([A-Z0-9-]+\.)+[A-Z]{2,4}$/i;
var name = $('#ViewName').val();
var mobile = $('#ViewMobile').val();
var email = $('#ViewEmail').val();
var message = $('#ViewDetail').val();
var solutions = $('#ViewSolutions').val();
var address = $('#ViewAddress').val();
var urgency = $('#ViewUrgency').val();

if(name.trim() == '' ){
    alert('Please enter your name.');
    $('#ViewName').focus();
    return false;
}else if(mobile.trim() == '' ){
    alert('Please enter your mobile number.');
    $('#ViewMobile').focus();
    return false;
}else if(email.trim() == '' ){
    alert('Please enter your email.');
    $('#ViewEmail').focus();
    return false;
}else if(email.trim() != '' && !reg.test(email)){
    alert('Please enter valid email.');
    $('#ViewEmail').focus();
    return false;
}else{
    $.ajax({
        type:'POST',
        url:'submit_quote.php',
        data:'contactFrmSubmit=1&name='+name+'&mobile='+mobile+'&email='+email+'&solutions='+solutions+'&message='+message+'&address='+address+'&urgency='+urgency,
        beforeSend: function () {
            $('.submitBtn').attr("disabled","disabled");
            $('.modal-body').css('opacity', '.5');
        },
        success:function(msg){
            if(msg == 'ok'){
                $('#ViewName').val('');
                $('#ViewMobile').val('');
                $('#ViewEmail').val('');
                $('#ViewSolutions').val('');
                $('#ViewDetail').val('');
                //window.location.href = 'successful-viewing.php'; //DONT DIRECT AWAY
                $('.statusMsg').html('<div style="color:#006400;" align="center"><b>QUOTE REQUEST SUBMITTED SUCCESSFULLY.</b><br/>We will be in touch as soon as possible.</div>');

                //refresh the page after 4 seconds
                $(document).ready(function() {
                    window.setTimeout(function(){window.location.href = "index.html"},7000);
                });
                
            }else{
                $('.statusMsg').html('<div style="color:red;" align="center">Some problem occurred, please refresh this Page and try again.</div>');
            }
            $('.submitBtn').removeAttr("disabled");
            $('.modal-body').css('opacity', '');
        }
    });
}
}
</script>

<!-- GET QUOTE MODAL -->


<!-- CONTACT US FORM -->
<script>
function submitContactForm2(){
var reg = /^[A-Z0-9._%+-]+@([A-Z0-9-]+\.)+[A-Z]{2,4}$/i;
var name2 = $('#ContactName').val();
var phone2 = $('#ContactPhone').val();
var email2 = $('#ContactEmail').val();
var message2 = $('#ContactMessage').val();
var department2 = $('#ContactDepartment').val();

if(name2.trim() == '' ){
    alert('Please enter your Name.');
    $('#ContactName').focus();
    return false;
}else if(phone2.trim() == '' ){
    alert('Please enter your Phone Number.');
    $('#ContactPhone').focus();
    return false;
}else if(department2.trim() == '' ){
    alert('Select the Department to handle your Feedback.');
    $('#ContactDepartment').focus();
    return false;
}else if(email2.trim() == '' ){
    alert('Please enter your Email.');
    $('#ContactEmail').focus();
    return false;
}else if(email2.trim() != '' && !reg.test(email2)){
    alert('Please enter valid Email.');
    $('#ContactEmail').focus();
    return false;
}else{
    $.ajax({
        type:'POST',
        url:'submit_feedback.php',
        data:'contactFrmSubmit2=1&name='+name2+'&phone='+phon2+'&email='+email2+'&department='+department2+'&message='+message2,
        beforeSend: function () {
            $('.submitBtn').attr("disabled","disabled");
            $('.modal-body').css('opacity', '.5');
        },
        success:function(msg){
            if(msg == 'ok'){
                $('#ContactName').val('');
                $('#ContactPhone').val('');
                $('#ContactEmail').val('');
                $('#ContactDepartment').val('');
                $('#ContactMessage').val('');
                //window.location.href = 'successful-viewing.php'; //DONT DIRECT AWAY
                $('.statusMsg').html('<div style="color:#006400;" align="center"><b>FEEDBACK SUBMITTED SUCCESSFULLY.</b><br/>We will be in touch with you as soon as possible.</div>');

                //refresh the page after 4 seconds
                $(document).ready(function() {
                    window.setTimeout(function(){window.location.href = "index.html"},7000);
                });
                
            }else{
                $('.statusMsg').html('<div style="color:red;" align="center">Some problem occurred, please refresh this Page and try again.</div>');
            }
            $('.submitBtn').removeAttr("disabled");
            $('.modal-body').css('opacity', '');
        }
    });
}
}
</script>
