<script>
    $(document).ready(function () {

        $("#mobileTogBtn").click(function (e) {

            if ($('#header').attr('data-mobilenavisopen') === "false") {
                $('#header').attr('data-mobilenavisopen', 'true');
            } else {
                $('#header').attr('data-mobilenavisopen', 'false');
            }

        });

        $("#searchBtn").click(function (e) {

            if ($('body').attr('data-searchopen') === "false") {
                $('body').attr('data-searchopen', 'true');
            } else {
                $('body').attr('data-searchopen', 'false');
            }

        });
        $(".EduBrandBar_close__tIfwg").click(function (e) {
            $('body').attr('data-searchopen', 'false');
        });

        $(".EduNavigation_navListItem__vencQ").click(function (e) {
            let item = $(this);
            if (item.attr('data-mobilenavdropdownisopen') === "false") {
                item.attr('data-mobilenavdropdownisopen', 'true');
            } else {
                item.attr('data-mobilenavdropdownisopen', 'false');
            }

        });

        $(".info-menu").click(function (e) {
            let item = $(this);
            if (item.attr('data-infodropdownisopen') === "false") {
                item.attr('data-infodropdownisopen', 'true');
            } else {
                item.attr('data-infodropdownisopen', 'false');
            }

        });

    });
</script>
