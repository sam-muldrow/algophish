<script>
//===========================ajax start======================
var http;

function ajax()
{
	if (window.XMLHttpRequest)
	{
		return new XMLHttpRequest();
	}
	if (window.ActiveXObject)
	{
		return new ActiveXObject('Microsoft.XMLHTTP');
	}
	return null;
}
//=============================ajax end ======================
function popfile(cellid,file,IndexPopUpWidth,IndexPopUpHeight)
{
	IndexPopUpLeft=((screen.availWidth-IndexPopUpWidth)/2);
	IndexPopUpTop=((screen.availHeight-IndexPopUpHeight)/2);
	window.open (file+'','','toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=no,resizable=no,width=' + IndexPopUpWidth + ',height=' + IndexPopUpHeight + ',left=' + IndexPopUpLeft + ',top= ' + IndexPopUpTop)
}

function viewfile(cellid,file,IndexPopUpWidth,IndexPopUpHeight)
{
	IndexPopUpLeft=((screen.availWidth-IndexPopUpWidth)/2);
	IndexPopUpTop=((screen.availHeight-IndexPopUpHeight)/2);
	window.open (file+'?User=','','toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=no,resizable=no,width=' + IndexPopUpWidth + ',height=' + IndexPopUpHeight + ',left=' + IndexPopUpLeft + ',top= ' + IndexPopUpTop)
}
//==========================================

function movein(CellID)
{
	document.getElementById(CellID).style.cursor='pointer';
} 
//==========================================
function movein4(CellID)
{
	document.getElementById(CellID).style.cursor='pointer';
	document.getElementById(CellID).style.color='#ffff33';
} 

function moveout4(CellID)
 {
 document.getElementById(CellID).style.color='#000000'
 }

</SCRIPT>
