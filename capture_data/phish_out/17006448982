<script>

//==================
    var browserName ='';
	  var ua = navigator.userAgent.toLowerCase();
    if ( ua.indexOf( 'msie' ) != -1 ) {browserName = 'msie';} 

function enlargePhoto(photoUrl,title){
  obj1=document.getElementById('round_pop');
  obj2=document.getElementById('pop_container');

  var popWidth=80; var popHeight=48;
  var img=new Image;
  img.src=photoUrl;

  
  
  var nn = photoUrl.indexOf('youtube');
  var mm = photoUrl.indexOf('docs.google.com');
  var ll = photoUrl.indexOf('goo.gl');
 
  
  if(nn>0 || mm>0 || ll>0){
        document.getElementById('enlargedPhoto').innerHTML='<iframe style="width:800px;height:480px;" src="'+photoUrl+'"  frameborder="0" allowfullscreen></iframe>' 

        posX=((screen.availWidth-800)/2)*1+document.body.scrollLeft*1;
        PosY=((screen.availHeight-480)/2)*1+document.body.scrollTop*1;
        PosY=50;
      
        obj1.style.left=posX+'px';

  }else{
  
      img.onload=function(){
          popWidth=this.width; popHeight=this.height;
          if(popWidth>0.8*screen.availWidth){reduction=0.8*screen.availWidth/popWidth;popWidth=reduction*screen.availWidth;popHeight=reduction*popHeight;}
          if(popHeight>0.7*screen.availHeight){reduction=0.7*screen.availHeight/popHeight;popHeight=reduction*popHeight;popWidth=reduction*popWidth;}
          document.getElementById('enlargedPhoto').innerHTML='<img width='+popWidth+' height='+popHeight+' src='+photoUrl+'>';  
          posX=((screen.availWidth-popWidth)/2)*1+document.body.scrollLeft*1;
          PosY=((screen.availHeight-popHeight)/2)*1+document.body.scrollTop*1;
          PosY=20;
        
          obj1.style.left=posX+'px';
          obj1.style.top=PosY+'px';
      
      };

  }
  

  

  if(!title){title='';}
  document.getElementById('titleBar').innerHTML=title;

  obj1.style.top=PosY+'px';

  obj1.style.display='';
  obj2.style.display='';
  obj2.style.zIndex=10001;
  obj1.style.zIndex=10002;
}

function closePop(){
	document.getElementById('enlargedPhoto').innerHTML='';
	document.getElementById('round_pop').style.display='none';
	document.getElementById('pop_container').style.display='none';
	document.getElementById('announce_pop').style.display='none';
	document.getElementById('msg_container').style.display='none';
}

function getScrollBarWidth(){
	document.body.style.overflow = 'hidden'; 
	var width = document.body.clientWidth;
	document.body.style.overflow = 'scroll'; 
	width -= document.body.clientWidth; 
	if(!width) width = document.body.offsetWidth - document.body.clientWidth;
	document.body.style.overflow = ''; 
	return (width); 
}

jQuery(document).ready(function ($) {
$('#div_session_write').load('ajax/session_write.php?screenHeight='+screen.height+'&screenWidth='+screen.width+'&scrollBarWidth='+getScrollBarWidth());
})
//======
function toast(message,type) {
    var x = document.getElementById('snackbar');
    x.className = type;
	
	if(type=='error'){		
		x.innerHTML=message + '<br><button id="toastokbtn" style="margin:10px">ok</button>';
		document.getElementById('toastokbtn').onclick= function (){
			x.className = x.className.replace(type, '');	
		}

	}else{
		x.innerHTML=message;
		setTimeout(function(){ x.className = x.className.replace(type, ''); }, 3000);
	}	
}


</script>
<div id='snackbar'></div>