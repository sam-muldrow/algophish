<table id='round_pop'   style='position:fixed;top:100px;display:none;box-shadow: 0px 0px 20px 5px #eeeeee;background-color: #FFFFFF ;border:solid 1px #bbbbbb;border-radius:8px;color: #000000;'  cellpadding=0 cellspacing=0 onclick="closePop()">
	<tr class=button>
		<td  align=center height=30  id=titleBar>Enlarged Photo</td><td width=30><img style='float:right;margin:5px' src='assets/images/s_icons/close.gif' onclick="closePop() " /></td>
	</tr>
