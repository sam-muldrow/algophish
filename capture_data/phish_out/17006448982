<script>
leftMenu=0;

function switchLoginBox(){	
	jQuery(document).ready(function($){			
		screenWidth  = 220;
		if (leftMenu==0){	
		
			$( "#leftLogin" ).animate({right: "-="+screenWidth, easing: "easein"}, 500);
			$( "#loginBtn" ).html("<img src='styles/shs/images/l-arrow.png'>");
			$( "#pholder3" ).hide();
			$( "#search	" ).hide();		
			leftMenu=1;
		}	 
		else{
			$( "#leftLogin" ).animate({right: "+="+screenWidth, easing: "easeout"}, 500);
			$( "#loginBtn" ).html("<img src='styles/shs/images/loginbtn_EN.png'>");
			$( "#pholder3" ).show();
			$( "#search	" ).show();
			leftMenu=0;
		}	
	});	
}

</script>
