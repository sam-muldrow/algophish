      <script language="javascript">
         // Create a clone of the menu, right next to original.
         jQuery(function($){
         	var nav = $('#Header');
                     $(window).scroll(function () {
                         if ($(this).scrollTop() > 100) {
                            nav.removeClass('notscrolled').addClass("scrolled");
                         } else {
                            nav.removeClass('scrolled').addClass("notscrolled");
                         }
                     });
         			
         $('.toogleMenu').click(
           function () {
         	   $("#Header").toggleClass("mob-clicked");
         	  
         	  $( ".mobileMenu" ).slideToggle( "slow");
         	  
         	    $( "#menu-w" ).toggleClass('abs');
         		
         		  $(document).scrollTo('#menu-w');
         		 
         	  
         	  return false;;
         	  
         });
         			
         			
         });
      </script>
