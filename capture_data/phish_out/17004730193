<script>

    function DisplayTSHQLicenseAgreementPopup() {
        $("#tshqagreement").css("display", "block");
        Snackbar(".tshqagreement-text", function (e) {
            if (e[0].lastChild.id == "tshqagreement")
            {
                document.cookie = 'tshqagreement=; expires=Thu, 01 Jan 1970 00:00:01 EST;';
            }
        });
    }

    function AcceptTSHQLicenseAgreement() {
        var sf = $.ServicesFramework();
        var serviceUrlPath = sf.getServiceRoot('sRegMemberServices');

        crudServiceBaseUrl = serviceUrlPath + "Customer/";
        $.ajax({
            type: 'POST',
            url: crudServiceBaseUrl + "AcceptTSHQLicenseAgreementForUser",
            data: { InstanceKey: $(".json-InstanceKeyForSnackbar").val(), PortalId: parseInt($(".json-PortalIdForSnackbar").val()), UserId: parseInt($(".json-CustomerIdForSnackbar").val()), Username: $(".json-CurrentUsernameForSnackbar").val(), IsAccepted: true },
            success: function (response) {
                if (response) {}
            },
            error: function (xhr) {}
        });
    }

</script>

