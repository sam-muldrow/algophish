    <script>
        jQuery(document).ready(function ($) {

            if (Cookies.get('cookiesAccepted') === undefined) {
                $("#popupCookies").removeClass('popupClosed');
            }

            var date = new Date();
            date.setTime(date.getTime() + (365 * 24 * 60 * 60 * 1000));

            $("#popupCookies .closePopup").click(function () {
                Cookies.set('cookiesAccepted', 'Yes', {
                    expires: date,
                    path: '/',
                    secure: true
                });
                $("#popupCookies").remove();
            });

        });
    </script>
