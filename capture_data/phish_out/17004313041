        <div class="modal-content" style="width: 100%; height: 100%; margin: auto;">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body" style="width: 100%; height: 90%; padding: 10px 40px 0px 40px;">
                <iframe src="" style="width: 100%; height: 100%; border: 0;">

                </iframe>
            </div>
        </div>
    </div>
</div>


<script src="/assets/c455921a/js/bootstrap.js?v=1700146229"></script>
<script src="/arone/js/script.js?v=1700146160"></script>
<script>jQuery(function ($) {
    $(document).ready(function(){
        window.closeModal = function(){
            $("#exampleModal").modal('hide');
        };
        
        $(document).on("click","button[data-toggle=modal]",function(){
            $("#exampleModal").find('iframe').attr('src',$(this).attr("data-src"));
        });

        $('#exampleModal').on('hidden.bs.modal', function () {
            $("#exampleModal").find('iframe').attr('src','');
        });   
    });
});</script>

</body>

</html>
