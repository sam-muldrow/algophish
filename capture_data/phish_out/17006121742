<script type="text/javascript">
	if (window.location.hash && window.location.hash == '#_=_') {
		if (window.history && history.pushState) {
			window.history.pushState("", document.title, window.location.pathname);
		} else {
			window.location.hash = '';
		}
	}

	$(function() {
		$('[data-toggle="tooltip"]').tooltip({ html: true });
		$('textarea').keyup(function() {
			autoresizeTextarea(this);
		});
	});

	/* trim form on submit! */
	$(document).ready(function() {
		var prevent = true, requiredEmptyFields = false;

		//unbind the original
		$('button[type=submit]').unbind('click');

		//bind the modified one
		$('button[type=submit]').click(function() {
			if (prevent === true) {
				var $form = $(this).closest("form");
				$form.find('select, textarea, input').each(function() {
					if ($(this).prop('required') && $(this).prop('type') !== 'file') {
						$(this).val($.trim($(this).val()));
						if ($(this).val().length === 0) {
							prevent = true;
							requiredEmptyFields = true;
						}
					}
				});

				if (requiredEmptyFields === false) {
					prevent = false;
					$(this).bind('click');

				} else {
					requiredEmptyFields = false;
					alert('Fehler! Bitte füllen Sie alle Formularfelder korrekt aus.');
				}
			} else {
				$(this).bind('click');
			}
		});
	});
</script>

