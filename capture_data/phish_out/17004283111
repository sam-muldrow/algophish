					    include($wordpress_header);		  					     
					    query_posts('posts_per_page=4');					    
					    if ( have_posts() ) : while ( have_posts() ) : the_post();					    	
						    //The Loop
						    echo '<img  width=20 src="images/dot-white.gif" alt="dot image"/><a class="site" target="_blank" href="';
						    the_permalink();
						    echo '"><font face="Verdana" size="2">';      
						    the_title();
						    echo '</font></a><br><br>';
						    endwhile;
					    else:
					   		endif;
					    //Reset Query
					    wp_reset_query();
					   }
					   echo '<p style="text-align:right"><a class="site" target="_blank" href="https://www.anandsoft.com/blog">more news..</a></p>';
					   ?>
					<h4 style="text-align: center"><strong>Featured Products</strong></h4>
					<br>
					<img alt="dot image" src="images/dot-white.gif" width="20" />
					<a class="site" href="https://www.anandsoft.com/router-simulator/router-simulator.html">
					CCNA Network Simulator</a> <br><br>
					<img alt="dot image" src="images/dot-white.gif" width="20" />
					<a class="site" href="https://www.simulationexams.com/exam-details/ccna.htm">
					CCNA Practice Tests With NetSim</a> <br><br>
					<img alt="dot image" src="images/dot-white.gif" width="20" />
					<a class="site" href="https://www.simulationexams.com/exam-details/aplus-core1.htm">
					A+ Core 1 exam 220-1001</a> <br><br></div>
