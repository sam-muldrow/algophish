    </footer><div id="meudiv"></div><script>
                window.onscroll = function() {
                    myFunction()
                };

                function myFunction() {
                    var navbar = document.getElementById("banner");

                    if (document.body.scrollTop > 100 || document.documentElement.scrollTop > 100) {
                        navbar.className = "w3-center w3-hide";
                    } else {
                        navbar.className = navbar.className.replace(" w3-hide", " ");
                    }
                }
                // Script for side navigation
                function w3_open() {
                    var x = document.getElementById("mySidebar");
                    var dvOrigem = document.getElementById('portal-column-one');
                    var dvDest = document.getElementById('menu_lateral')
                    dvDest.innerHTML = dvOrigem.innerHTML;
                    x.style.width = "300px";
                    x.style.paddingTop = "10%";
                    x.style.display = "block";
                }

                // Close side navigation
                function w3_close() {
                    document.getElementById("mySidebar").style.display = "none";
                }

                
                function openNav() {
                    var x = document.getElementById("navDemo");
                    
                    if (x.className.indexOf("w3-show") == -1) {
                        x.className += " w3-show";
                    } else {
                        x.className = x.className.replace(" w3-show", "");
                    }
                }
        </script></body>
