  <script>

    Static =  { };
    Static.CONTEXT = {};

    Y = YUI(YUI_CONFIG).use('squarespace-common', 'squarespace-system-page', function(Y) {

      var message;

      if (Static.CONTEXT.removed) {

        message = '<div class="bigtext"><div class="title">SITE DELETED</div><div class="description">This site has been deleted by the owner.</div></div>';

      } else {

        message = '<div class="bigtext"><div class="title">No Such Website</div><div class="description">The website you requested does not exist.</div></div>';

      }

      LB = new Y.Squarespace.Lightbox({
        disableNormalClose: true,
        clickAnywhereToExit: false,
        content: message,
        margin: 100,
        height: 90
      });

      LB.show();

    });

