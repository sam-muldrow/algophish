<script src="https://cogentdatasolutions.com/wp-content/themes/pindol/js/jquery.validate.js" type="text/javascript"></script>
<script src="https://cogentdatasolutions.com/wp-content/themes/pindol/js/jquery.placeholder.js" type="text/javascript"></script>

<!--script for contact-us div font start here-->
<script type="text/javascript">
    $.noConflict(); 
    jQuery(document).ready(function($){
  
		$('input, textarea').placeholder();
    	$(".contactus-banner-div-left-side-socilaicons").click(function(){

		    if ($('.right-side').is(':visible')) {

				$('.contactus-banner-div-left-side-socilaicons').css('background-image', 'url("https://142.4.19.96/~cogent/dev/wp-content/themes/pindol/images/cb-open.png")');

				} else {

				$('.contactus-banner-div-left-side-socilaicons').css('background-image','url("https://142.4.19.96/~cogent/dev/wp-content/themes/pindol/images/close-contact-form.png")');

				}

			$(".right-side").animate({

		    	width:'toggle'

		    });

  		});

	  	$(".main-content").click(function(){

		    $('.contactus-banner-div-left-side-socilaicons').css('background-image', 'url("https://142.4.19.96/~cogent/dev/wp-content/themes/pindol/images/cb-open.png")');

		    $(".right-side").animate({

	              width:'hide'
	        });
	   
	    });	

    	$("#contact-form").validate({
            rules: {
                fName: {           //input name: fullName
                    required: true,   //required boolean: true/false
                    minlength: 2,       
                },
                lName: {           //input name: fullName
                    required: true,   //required boolean: true/false
                    minlength: 2,       
                },
                email: {              //input name: email
                    required: true,   //required boolean: true/false
                    email: true       //required boolean: true/false
                }
                
               
            },
            
            submitHandler: function(form) {
            	 
                   $(form).ajaxSubmit({
                            url:"echo/html",
                           type:"GET",
                        success: function(response){
                           $('#contact-form').hide();
                            $('#sent').show();       
                        }
                    });
            }

        }); 

    		
	    $('#popupBoxClose').click( function() {			
	    	unloadPopupBox();
	    	$('#contact-form #download').val('');
	    	setCookie('cds_download_url', '', 0);
	    	$("label.error").hide();
	    	$(".error").removeClass("error");
	    	$('#contact-form')[0].reset();

	    });    

	    function unloadPopupBox() {	
	    	$('#popup_box').fadeOut("slow");
	    	$(".main-content, .contactus-banner-div").css({ 		
	    		"opacity": "1"  
	    	}).fadeIn("slow"); 
	    }	
    
	    $('.custom_downloads').click( function(e) {	// To Load the Popupbox
	    	e.preventDefault();
	    	var post_id = $( this ).attr( "href" );	
	    	$('#contact-form #download').val('');

	    	$('#contact-form #download').val( post_id );
	    	setCookie('cds_download_url', post_id,1);
	    	
	    	//$('#popup_box').fadeIn("slow");
	    	$(".main-content, .contactus-banner-div").css({ // this is just for style
	    		"opacity": "0.3"  
	    	}); 
	    	$('#popup_box').css({ // this is just for style
	    		"opacity": "1"  
	    	}).fadeIn("slow"); 

	    });

    function getCookie(c_name) {

		var c_value = document.cookie;
		var c_start = c_value.indexOf(" " + c_name + "=");
		if (c_start == -1) {
			c_start = c_value.indexOf(c_name + "=");
		}
		if (c_start == -1) {
			c_value = null;
		} else {
			c_start = c_value.indexOf("=", c_start) + 1;
			var c_end = c_value.indexOf(";", c_start);
			if (c_end == -1) {
				c_end = c_value.length;
			}
			c_value = unescape(c_value.substring(c_start,c_end));
		}
		return c_value;
	}

	function setCookie(c_name,value,exdays) {
		var exdate=new Date();
		exdate.setDate(exdate.getDate() + exdays);
		var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString())+"; path=/";;
		document.cookie=c_name + "=" + c_value;
	}

    			

});

</script>

