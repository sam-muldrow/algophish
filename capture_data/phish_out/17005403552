<script>
	jQuery(function() {
		jQuery('#toggle-footer').click(function() {
			var current_class = jQuery(this).attr('class');

			if( 'show-arrow-up' == current_class ) current_class = 'show-arrow-down';
			else current_class = 'show-arrow-up';

			jQuery('#Footer .footerwidgets').slideToggle();
			jQuery(this).attr('class', current_class);

		})
	})
</script>

