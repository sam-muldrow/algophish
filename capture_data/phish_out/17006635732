<script type="text/javascript">
function validation(frm)
{
	if (frm.c_name.value=="")
	{
		
		frm.c_name.focus();
		frm.c_name.style.borderColor="#FF0000";
		return false;
	}
	else if (frm.c_email.value=="")
	{
		
		frm.c_email.focus();
		frm.c_email.style.borderColor="#FF0000";
		return false;
	}
	else if (frm.c_contact.value=="")
	{
		frm.c_contact.focus();
		frm.c_contact.style.borderColor="#FF0000";
		return false;
	}
	else if (frm.c_message.value=="")
	{
		frm.c_message.focus();
		frm.c_message.style.borderColor="#FF0000";
		return false;
	}
	else if(frm.captcha.value == "")
	{
		frm.captcha.focus();
		frm.captcha.style.borderColor="#FF0000";
		return false;
	}
	else 
	{
		return true;
	}
}
function chklenght(names)
{
	if (names.length!=0)
	{
		names.style.borderColor="#999999";
	}

}
function Phone(evt)
{
	 var charCode = (evt.which) ? evt.which : event.keyCode

	 if ((charCode >= 97 && charCode <=122) || ((charCode >= 65 && charCode <=90)))
	 {
		return false;
	 }
	 else if((charCode>=48 && charCode <=57) || charCode==43 || charCode==32 || charCode==45 || charCode==40 || charCode==41 || charCode==8)
	 {
		return true;
	 }
	 else
	 {
		return false;
	 }

	
}
</script>

