        jQuery(document).ready(function($){

            jQuery('#wpcs_tab_2907').click(function($){

                if( ! (jQuery('#wpcs_content_main_2907').hasClass('is_open')) ){

                    // Open slider
                    wpcs_open_slider_2907();

                } else {

                    // close slider
                    wpcs_close_slider_2907();

                }

            });

            jQuery("#wpcs_overlay_2907, #wpcs_close_slider_2907").click(function(){
                wpcs_close_slider_2907();
            });

            
        });

        function wpcs_open_slider_2907(do_repeat){

            do_repeat = typeof do_repeat !== 'undefined' ? do_repeat : 0 ;

            if( do_repeat !== 0 ){
                jQuery('#wpcs_content_main_2907').addClass('do_repeat');
                jQuery( "#wpcs_content_main_2907" ).data( "interval", do_repeat );
            }

            if( ! (jQuery('#wpcs_content_main_2907').hasClass('is_open')) && !(jQuery('#wpcs_content_main_2907').hasClass('is_opening')) ){

                // hide tap
                jQuery('#wpcs_tab_2907,.wpcs_tab').fadeTo("slow", 0);

                jQuery('#wpcs_content_main_2907').addClass('is_opening');

                jQuery("#wpcs_overlay_2907").addClass('wpcs_overlay_display_cross');

                jQuery( "#wpcs_overlay_2907").fadeIn('fast');

                // PRO FEATURE - PUSH BODY
                
                jQuery('#wpcs_content_main_2907').addClass('is_open');

                jQuery( "#wpcs_content_main_2907" ).animate({
                    opacity: 1,
                right: "+=500"
            }, 250 , function() {

                    // hide tap
                    jQuery('#wpcs_tab_2907,.wpcs_tab').fadeTo("slow", 0);

                    // Trigger some thing here once completely open
                    jQuery( "#wpcs_content_inner_2907").fadeTo("slow" , 1);

                    // Remove is_opening class
                    jQuery('#wpcs_content_main_2907').removeClass('is_opening');

                });

            }

        }

        function wpcs_close_slider_2907(){

            if( (jQuery('#wpcs_content_main_2907').hasClass('is_open')) && !(jQuery('#wpcs_content_main_2907').hasClass('is_closing')) ) {

                jQuery("#wpcs_overlay_2907").removeClass('wpcs_overlay_display_cross');

                jQuery('#wpcs_content_main_2907').addClass('is_closing');

                jQuery("#wpcs_content_main_2907").animate({
                right:
                "-=500"
            }
            , 250 ,
                function () {

                    // Trigger some thing here once completely close
                    jQuery("#wpcs_content_main_2907").fadeTo("fast", 0);
                    jQuery("#wpcs_content_inner_2907").slideUp('fast');
                    jQuery("#wpcs_overlay_2907").fadeOut('slow');
                    jQuery('body').removeClass('fixed-body');

                    //  Removing is_open class in the end to avoid any confliction
                    jQuery('#wpcs_content_main_2907').removeClass('is_open');
                    jQuery('#wpcs_content_main_2907').removeClass('is_closing');


                    // display tap
                    jQuery('#wpcs_tab_2907,.wpcs_tab').fadeTo("slow", 1);

                });

                if( (jQuery('#wpcs_content_main_2907').hasClass('do_repeat')) ) {
                    setTimeout(function () {
                        wpcs_open_slider_2907(0);
                    }, 0 );
                }

            }

        }
            </script>
