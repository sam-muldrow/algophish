			<script>
$=jQuery;	
	//~ document.getElementsByClassName("banCls").style.display="none";
		$(".banCls").css("display","none");
		$(".defaultCls").css("display","block");
	function showBrand(id)
	{
$(".banCls").removeClass("selectedCls");
		$(".banCls").css("display","none");
		//document.getElementsByClassName("banCls").style.display="none";
		document.getElementById("ban-"+id).style.display="block";
		$(".showBan").removeClass("selectedCls");
		document.getElementById("showBan"+id).classList.add("selectedCls");;
	}
	
	$(document).ready(function(){
	$(".banCls").css("display","none");
$("#showBan1").addClass("selectedCls");
		$(".defaultCls").css("display","block");
		//~ $(".showBan").click(function(){
			//~ //$(".banCls").css("display","none");
			//~ //$(this).css("display","block");
		//~ });
	});
	var myIndex = 0;
carousel();

function carousel() {
    var i;
    var x = document.getElementsByClassName("banCls");
    for (i = 0; i < x.length; i++) {
       x[i].style.display = "none";  
    }
    myIndex++;
    if (myIndex > x.length) {myIndex = 1} 
    $(".showBan").removeClass("selectedCls");
	document.getElementById("showBan"+myIndex).classList.add("selectedCls");;   
    x[myIndex-1].style.display = "block";  
    setTimeout(carousel, 3000); // Change image every 2 seconds
}
</script>

