<script type="text/javascript">
    $(function(){
        $('#zztywz').find("a").each(function(){
            par($(this).text())
        })
    })

    function par(t){
        $(".j-navbar-nav").children("li").each(function(){
            if(t == $(this).find("a").eq(0).text()){
                $(this).addClass("active")
            }
        })
    }


</script>

