<script type="text/javascript" src="/js/idangerous.swiper2.7.6.min.js"></script>
<script type="text/javascript" src="/js/idangerous.swiper.progress.js"></script>
<script>
    $(function(){

        if($(".j_index_tab4").find("li").length <1){
            $(".j_index_tab4").find("a").each(function(){
                $(this).hide();
            })
        }
        var mySwiper = new Swiper ('.swiper-container', {
            pagination: '.pagination',
            paginationClickable :true,
            preventClicks:false,
            autoplay : 3000,
            speed:1500,
            loop:true,
            simulateTouch:false,
            progress:true,

            onProgressChange: function(swiper){
                for (var i = 0; i < swiper.slides.length; i++){
                    var slide = swiper.slides[i];
                    var progress = slide.progress;
                    var translate = progress*swiper.width;
                    var opacity = 1 - Math.min(Math.abs(progress),1);
                    slide.style.opacity = opacity;
                    swiper.setTransform(slide,'translate3d('+translate+'px,0,0)');
                }
            },

            onTouchStart:function(swiper){
                for (var i = 0; i < swiper.slides.length; i++){
                    swiper.setTransition(swiper.slides[i], 0);
                }
            },
            onSetWrapperTransition: function(swiper, speed) {
                for (var i = 0; i < swiper.slides.length; i++){
                    swiper.setTransition(swiper.slides[i], speed);
                }
            },

            onSlideChangeStart: function(Swiper){
                // 暂停所有视频
                var videos = document.getElementsByTagName('video');
                for (var i = videos.length - 1; i >= 0; i--) {
                    (function(){
                        var p = i;
                        videos[p].addEventListener('play',function(){
                            pauseAll(p);
                        })
                    })()
                }
                function pauseAll(index){
                    for (var j = videos.length - 1; j >= 0; j--) {
                        if (j!=index) videos[j].pause();
                    }
                }
                // 判断swiper-slide-active是否有视频
                if($(".banner-container .swiper-slide-active video").length>0){
                    // 停止自动切换
                    Swiper.stopAutoplay();
                    // 动态增加id
                    $(".banner-container .swiper-slide-active video").attr("id","video_01");

                    var _video=document.getElementById("video_01");

                    // 播放视频
                    _video.play();
                    // 切换后重新播放视频
                    _video.currentTime = 0;
                    // 静音
                    _video.volume = 0;
                    // 监听视频播放结束
                    _video.addEventListener('ended', function () {
                        //Swiper.slideNext();
                        //重新开始轮播banner
                        Swiper.startAutoplay();
                    });
                }
            },
            onSlideChangeEnd: function(swiper){
                $(".banner-container .swiper-slide-active video").attr("id","");//动态移除id
            },


        });
        var touchSwiper = new Swiper ('.swiper_touch', {
            pagination: '.pagination_touch',
            paginationClickable :true,
            autoplay : 3000,
            speed:1500,
            loop:true,
            progress:true,
            onProgressChange: function(swiper){
                for (var i = 0; i < swiper.slides.length; i++){
                    var slide = swiper.slides[i];
                    var progress = slide.progress;
                    var translate = progress*swiper.width;
                    var opacity = 1 - Math.min(Math.abs(progress),1);
                    slide.style.opacity = opacity;
                    swiper.setTransform(slide,'translate3d('+translate+'px,0,0)');
                }
            },
            onTouchStart:function(swiper){
                for (var i = 0; i < swiper.slides.length; i++){
                    swiper.setTransition(swiper.slides[i], 0);
                }
            },

            onSetWrapperTransition: function(swiper, speed) {
                for (var i = 0; i < swiper.slides.length; i++){
                    swiper.setTransition(swiper.slides[i], speed);
                }
            },


        })

        var caseSwiper = new Swiper('.in_container',{
            centeredSlides : false,
            slidesPerView : '3',
            slidesPerGroup : 1,
            loop : true,
            autoplay : 5000,
            initialSlide :1,
        })

        $('.swiper-button-prev').on('click', function(e){
            e.preventDefault()
            caseSwiper.swipePrev()
        })

        $('.swiper-button-next').on('click', function(e){
            e.preventDefault()
            caseSwiper.swipeNext()
        })

    })

</script>
<script type="text/javascript" src="/js/jquery.fancybox.pack.js"></script>
<script src="/js/jquery.roundabout.min.js"></script>

<script type="text/javascript">
    $(document).ready(function(){
        $('.roundabout_box ul').roundabout({
            duration: 1000,
            minScale: 0.6,
            autoplay: false,
            minOpacity: 0,
            maxOpacity: 1,
            reflect: true,
            startingChild: 1,
            responsive: true,
            btnPrev: ".ban_r_btn", // 右按钮
            btnNext: ".ban_l_btn", // 左按钮
            enableDrag: false,
        });
    });
</script>

<script>
    $('.videolist').each(function(){ //遍历视频列表
        $(this).hover(function(){ //鼠标移上来后显示播放按钮
            $(this).find('.videoed').show();
        },function(){
            $(this).find('.videoed').hide();
        });
        $(this).click(function(){ //这个视频被点击后执行
            var img = $(this).attr('vpath');//获取视频预览图
            var video = $(this).attr('ipath');//获取视频路径
            $('.videos').html("<video id=\"video\" poster='"+img+"' style='width: 785px' src='"+video+"' preload=\"auto\" controls=\"controls\" ></video><img onClick=\"close1()\" class=\"vclose\" src=\"../\images/\gb.png\" width=\"25\" height=\"25\"/>");
            $('.videos').show();
        });
    });

    function close1(){
        var v = document.getElementById('video');//获取视频节点
        $('.videos').hide();//点击关闭按钮关闭暂停视频
        v.pause();
        $('.videos').html();
    }
</script>
<script src="/js/jquery.fullPage.js"></script>
<script>
    $(function(){
        $('#fullPage').fullpage({
            'navigation': true,
            verticalCentered: true,

        });

        $(window).resize(function(){
            autoScrolling();
        });

        function autoScrolling(){
            var $ww = $(window).width();
            if($ww < 1024){
                $.fn.fullpage.setAutoScrolling(false);
                $.fn.fullpage.setFitToSection(false);
            } else {
                $.fn.fullpage.setAutoScrolling(true);
                $.fn.fullpage.setFitToSection(true);
            }
        }

        autoScrolling();
    });

</script>
<script>
    $('.news_index .am-tabs-nav').on('mousemove', 'a', function() {
        this.click();
    });
</script>

