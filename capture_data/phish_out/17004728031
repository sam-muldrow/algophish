<!-- РОИСТАТ -->


<script>
    const ROISTAT_AB_COOKIE = "roistat_ab";
    const ROISTAT_AB_SUBMITTED_COOKIE = "roistat_ab_submit";
    const COOKIE_EXPIRE = 7*24*60*60;
    const COOKIE_CONFIG = { expires: COOKIE_EXPIRE, path: '/'};
    var testsConfig = [
    	{
    		'id': 'myTest1',
    		'variants': ['a','b']
    	},
    	{
    		'id': 'mobileMenu',
    		'variants': ['tile','list']
    	}

    
    ];
    var testValues = [];
    var roistatAbCookieValues = [];
    //Работаем только, если нет куки, что уже применили настройки теста
    if (getCookieValue(ROISTAT_AB_SUBMITTED_COOKIE) != '1') {
        
        for (var i = 0; i < testsConfig.length; i++) {
            var testId = testsConfig[i].id;
            var testValue = getCookieValue(testId);
            if (typeof testValue === 'undefined') {
                testValue = testsConfig[i].variants[getRandomInt(0,testsConfig.length-1)];
                setCookieValue(testId,testValue,COOKIE_CONFIG);
            } else {
                testValue = getCookieValue(testId);
            }
            testValues.push({'id' : testId, 'value' : testValue});
        }
        // Устанавливаем куку Roistat для АБ-тестов
        for (var i = 0; i < testValues.length; i++) {
            roistatAbCookieValues.push(testValues[i].id+":"+testValues[i].value);
        }
        setCookieValue(ROISTAT_AB_COOKIE,roistatAbCookieValues.join(','),COOKIE_CONFIG);
        //Устанавливаем куку, что мы применили настройки теста
        setCookieValue(ROISTAT_AB_SUBMITTED_COOKIE,'1',COOKIE_CONFIG);
    } 
    //Вспомогательные функции    
    function getRandomInt (min,max) {min = Math.ceil(min);max = Math.floor(max);return Math.floor(Math.random() * (max - min + 1)) + min;}
    function getCookieValue (name) {var matches = document.cookie.match(new RegExp("(?:^|; )" + name.replace(/([\.$?*|{}\(\)\[\]\\\/\+^])/g, '\\$1') + "=([^;]*)"));return matches ? matches[1] : undefined;};
    function setCookieValue (name, value, options) {options = options || {};var expires = options.expires;if (typeof expires == "number" && expires) {var d = new Date();d.setTime(d.getTime() + expires*1000);expires = options.expires = d;}if (expires && expires.toUTCString) { options.expires = expires.toUTCString();}value = encodeURIComponent(value);var updatedCookie = name + "=" + value;for(var propName in options) {updatedCookie += "; " + propName;var propValue = options[propName];    if (propValue !== true) { updatedCookie += "=" + propValue;}}document.cookie = updatedCookie;};
    
    console.log('test');
    // переписать куку
        
    $(document).on('click', '.one_column', function () {

        document.cookie = "mobileMenu=tile";

        });

    $(document).on('click', '.two_column', function () {

        document.cookie = "mobileMenu=list";
    
        });


    
    
    
    
    
    // функция для получения значения куки
    function getCookie(name) {
        var cookie = " " + document.cookie;
        var search = " " + name + "=";
        var setStr = null;
        var offset = 0;
        var end = 0;
        if (cookie.length > 0) {
            offset = cookie.indexOf(search);
            if (offset != -1) {
                offset += search.length;
                end = cookie.indexOf(";", offset)
                if (end == -1) {
                    end = cookie.length;
                }
            setStr = unescape(cookie.substring(offset, end));
            }
        }   
        return(setStr);
    }
    
    
</script>





        
      
        
<!-- электронная комерция. ---> 



<script>

const activeElements = document.querySelectorAll('.wa-coupon-section');

// Функция для проверки наличия класса "is-active'" и добавления необходимых действий
function checkActiveElement(element) {
  if (element.classList.contains('is-active')) {
  
 //   console.log('Класс "is-active" был добавлен');
    activeCoupon = document.getElementsByName('coupon_code')[0].value;
    console.log('Купон '+activeCoupon);
    
    
    dataLayer.push({
   "ecommerce": {
       "currencyCode": "RUB",    
       "add": {
           "products": [
               {
                   "id": "43521",
                   "name": activeCoupon,
                   "price": 0,
                   "brand": "Яндекс / Яndex",
                   "category": "Купоны",
                   "quantity": 1
               }
           ]
       }
   }
});
    
   }
}

// Проверяем все элементы с классом "wa-coupon-sectione" на наличие класса "is-active"
activeElements.forEach(function(element) {
  checkActiveElement(element);
});

// Наблюдатель за изменениями атрибутов элементов
const observer = new MutationObserver(function(mutationsList) {
  for (let mutation of mutationsList) {
    // Проверяем только изменения классов
    if (mutation.type === 'attributes' && mutation.attributeName === 'class') {
      // Проверяем измененный элемент на наличие класса "is-active"
      checkActiveElement(mutation.target);
    }
  }
});

// Запускаем слежение за изменениями атрибутов для всех элементов с классом "wa-active-state"
activeElements.forEach(function(element) {
  observer.observe(element, { attributes: true });
});

</script>







 

</head>

