        <style>
            .comfortbuy-horizontal-tree li:last-child {
                flex-grow:1;
                border-right: none;
            }
        </style>
        <script type="text/javascript">
            //$('.comfortbuy-horizontal-tree').css('visibility', 'hidden');
            
            /*Скрипт для работы меню во вкладке "КАТАЛОГ"*/
            let exemplу = document.getElementById('horizontal-category-wrap');
            let menu = document.querySelector('.nav-bar-category');
            let menuToggle = document.querySelector('.comfortbuy-horizontal-tree li');
            let menuIsShow = false; 

            function closeMenu(evt) {
                menuToggle.classList.remove('collapse');
                menu.classList.add('d-none');
                menuIsShow = false;
            }

            function openMenu(evt) {
                menuToggle.classList.add('collapse');
                menu.classList.remove('d-none');
                menuIsShow = true;
            }

            menu.addEventListener('mouseleave', (evt) => {
                if(evt.relatedTarget.parentNode === menuToggle) {
                    return;
                }
                closeMenu();
            });

            menuToggle.addEventListener('mouseenter', (evt) => {
                if (menuIsShow === false) {
                    openMenu();
                    exemplу.addEventListener('mouseleave', closeMenu);
                }
            });


            let categories = document.querySelectorAll('.category-wrapper');
            categories.forEach((category) => {
                let sub = category.querySelector('.subcategory-list');

                if (sub) {
                    let isInter = false;
                    let openMenuTimeOut;

                    function openSubMenu() {
                        sub.style.display = 'block';
                    }

                    function closeSubMenu() {
                        sub.style.display = 'none';
                    }

                    sub.addEventListener('mouseenter', () => {
                        isInter = true;
                    });

                    sub.addEventListener('mouseleave', () => {
                        isInter = false;
                    });

                    category.addEventListener('mouseenter', () => {
                        openMenuTimeOut = setTimeout(openSubMenu, 300);
                    });

                    category.addEventListener('mouseleave', (evt) => {
                        clearTimeout(openMenuTimeOut);
                        setTimeout(function() {
                            if (isInter) {
                                return;
                            }
                            closeSubMenu();
                        }, 300);
                    });
                }
            });    
        </script>
    </div>

    
    	        </div>
    	        
    	    </div>
    	    
    	    <div class="clear-both"></div>
	    </div>
	</div>

