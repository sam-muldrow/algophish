                    loop:true,
                    stopOnHover:true
                });
            }
    });
    function color_event() {
        $('.day_event').each(function () {
            var color = $(this).attr('data-event');
            $(this).parent().parent().css('background-color', color);
        })
    }

    function inscription_form()
    {
        $.post($("#inscription_form").attr('action'),$("#inscription_form").serialize(),function(data){
            var id = $("#inscription_form").find('#competition_id').val();
            if(data == "ok")
            {
                $('#compet_'+id).append('<div class="alert alert-success">Votre demande d\'inscription à été transmise, vous allez recevoir une confirmation par email</div>');
                $('#modal_inscription').find('input').val("");
                $('#modal_inscription').modal('hide');
            }
            else if (data == "captcha_vide")
            {
                $(".modal-body .error").html('<div class="alert alert-warning">Veuillez remplir le captcha</div>');
            }
            else
            {
                $('#compet_'+id).append('<div class="alert alert-warning">Une erreur s\'est produite, veuillez réessayer plus tard svp.</div>');
                $('#modal_inscription').find('input').val("");
                $('#modal_inscription').modal('hide');
            }
        });
    }
</script>
