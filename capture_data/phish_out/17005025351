<script type="text/javascript">
var old="";
var cur = 0;
var bann_img = new Array(); 
var bann_url = new Array(); 
var bann_target = new Array(); 
function menu(n){
    cur = n;
    var bann = document.getElementById('bann_img');
    var onimg = document.getElementById('on_'+cur);
    var bann_lnk = document.getElementById('bann_lnk');
    bann.src = bann_img[cur];
    bann_lnk.href = bann_url[cur];
    bann_lnk.target = bann_target[cur];
    if(old!=""||old=="0") document.getElementById('on_'+old).src = "images/off.gif";
    onimg.src = "images/on.gif";
    old = cur;
}

function ch_menu() {
    cur++;
    if(cur>=5) cur = 0;
    menu(cur);
}
