var m_old='';
var m_cur = 0;
function m_menu(name){
    m_cur = name;
    m_submenu=eval("menu"+name+".style");
    if(m_old!=m_submenu)
    {
        if(m_old!='')
        {
            m_old.display='none';
        }
        m_submenu.display='block';
        m_old=m_submenu;
    }
}
var e_old='';
var e_cur = 0;
function e_menu(name){
    e_cur = name;
