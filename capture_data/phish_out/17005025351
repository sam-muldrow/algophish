        <td align="right" valign="top"><script>
var n_old='';
var n_cur = 0;
function news(name){
    n_cur = name;
    n_submenu=eval("news"+name+".style");
    if(n_old!=n_submenu)
    {
        if(n_old!='')
        {
            n_old.display='none';
        }
        n_submenu.display='block';
        n_old=n_submenu;
    }
}
