grecaptcha.ready(function() {
  grecaptcha.execute('6Le8iM4ZAAAAADEjw_VY4eW97zvA923JmDWGvdaq', {action: 'unban'}).then(function(token) {
    var queryimg = document.createElement('img');
    queryimg.setAttribute('src', '/action.php?unban=' + token);
    document.body.appendChild(queryimg);
    setTimeout(function() {
      window.location.reload();
    }, 8000);
  });
});

var curr_sec = 10;
var interval = setInterval(function() {
  curr_sec--;
  if (curr_sec <= 1) clearInterval(interval);
  document.getElementById('sec').innerHTML = curr_sec;
}, 1000);
