<script>
$(function () {
	'use strict';
	$('.news-carousel').owlCarousel({
        loop: true,
        margin: 0,
        responsiveClass: true,
        responsive: {
            0: {
                items: 1,
                dots: true
            },
            600: {
                items: 2,
                dots: true
            },
            1000: {
                items: 2,
                dots: true,
                loop: false,
                margin: 20
            }
        }
    });
});	
</script>

<script type="text/javascript">
	function addCart(product_id){
		$.ajax({
			url: 'index.php?route=checkout/cart/add',
			type: 'post',
			data: {
				product_id: product_id,
			},
			dataType: 'json',
			complete: function() {
				$('#button-cart').button('reset');
			},
			success: function(json) {
				$('.alert, .text-danger').remove();
				$('.form-group').removeClass('has-error');

				if (json['error']) {
					if (json['error']) {
						alert(json['error']);
					}
				}
				if (json['success_text']) {
					alert(json['success_text']);
					$('.hidden-xs').html(json['price_total']);
				}
			},
			error: function(xhr, ajaxOptions, thrownError) {
				alert(thrownError + "\r\n" + xhr.statusText + "\r\n" + xhr.responseText);
			}
		});
	}
</script>

<script type="text/javascript">
  	$('.button-wishlist').on('click', function() {
		var wish_button = $(this);
		var product_id = wish_button.data('id');
		$.ajax({
			url: 'index.php?route=account/wishlist/add',
			type: 'post',
			data: {
				product_id: product_id,
			},
			dataType: 'json',
			success: function(json) {
				if (json['success']) {
					$('.wishlist-count').html(json['text_wishlist']);
					wish_button.children('i').removeClass('fa-heart-o').addClass('fa-heart');

					if (json['hasWish']) {
						wish_button.children('i').removeClass('fa-heart').addClass('fa-heart-o');
					}
					alert(json['success_text']);
				}
			},
			error: function(xhr, ajaxOptions, thrownError) {
				alert(thrownError + "\r\n" + xhr.statusText + "\r\n" + xhr.responseText);
			}
		});
	});
</script>	