$(".consult_toolbar").click(function(){
  $(".rbt_toolbar").toggleClass("rbt_sidearea_addrad") 
  $(".rbt_toolbar").removeClass("rbt_sidearea_addwhite") 
  
  if($( ".rbt_toolbar" ).hasClass( "rbt_sidearea_addrad"))
  {
	   $("#multiscroll-nav").addClass ("multiscrollhide");	  
  }
  else
  {
	 $("#multiscroll-nav").removeClass ("multiscrollhide");
  } 
   
   $(".edgtf-side-menu-button-opener").toggleClass ("multimenunav");
  return false;
});

$(".contact_toolbar").click(function(){
  $(".rbt_toolbar").removeClass("rbt_sidearea_addrad")
  $(".rbt_toolbar").toggleClass("rbt_sidearea_addwhite")  
  
  if($( ".rbt_toolbar" ).hasClass( "rbt_sidearea_addwhite"))
  {
	   $("#multiscroll-nav").addClass ("multiscrollhide");	  
  }
  else
  {
	 $("#multiscroll-nav").removeClass ("multiscrollhide");
  } 
   
   $(".edgtf-side-menu-button-opener").toggleClass ("multimenunav");
  return false;
});


