}
} else {
lsjQuery("#layerslider_14").layerSlider({sliderVersion: '6.8.4', type: 'fullwidth', skin: 'v6', navStartStop: false, showCircleTimer: false, skinsPath: 'https://www.bsvclaims.com/wp-content/plugins/LayerSlider/static/layerslider/skins/'});
