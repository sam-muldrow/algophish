		jQuery(document).bind('copy', function(e) {

			e.preventDefault();

			alert('Todos os textos, fotos, artes e vídeos do site estão protegidos pela legislação brasileira sobre direito autoral. Não reproduza o conteúdo do site em qualquer meio de comunicação, eletrônico ou impresso, sem autorização.');

		});

	});
</script>



</body>



</html>