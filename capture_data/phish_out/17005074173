        <script>
      function formatState (state) {
  if (!state.id) {
    return state.text;
  }
  var baseUrl = "https://www.campingmiraflores.it/img/lang";
  var $state = $(
    '<span><img style="width:25px" src="' + baseUrl + '/' + state.flag + '.png" class="img-flag" /> ' + state.label + '</span>'
  );
  return $state;
};
    </script>
