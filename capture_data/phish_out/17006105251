			  var slides_c = document.getElementsByClassName("mySlides");
			  for (i = 0; i < slides_c.length; i++) {
				slides_c[i].style.display = "none";
			  }
			  slideIndex_c++;
			  if (slideIndex_c > slides_c.length) {slideIndex_c = 1}
