			var slideIndex_c = 0;
			showSlides_c();

			function showSlides_c() {
			  var i;
			  var slides_c = document.getElementsByClassName("mySlides");
			  for (i = 0; i < slides_c.length; i++) {
				slides_c[i].style.display = "none";
			  }
			  slideIndex_c++;
			  if (slideIndex_c > slides_c.length) {slideIndex_c = 1}
			  slides_c[slideIndex_c-1].style.display = "block";
			  setTimeout(showSlides_c, 3000); // Change image every 3 seconds
			}
		
</script><div class="column block">
