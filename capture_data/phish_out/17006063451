			jQuery(window).scroll(function() {
				if (jQuery(this).scrollTop()>300)
				 {
					jQuery('.tovelos').show(1000);
				 }
				else
				 {
				  jQuery('.tovelos').hide(1000);
				 }
			 });
			 
			jQuery("a[href='#top']").click(function() {
			  jQuery("html, body").animate({ scrollTop: 0 }, "slow");
			  return false;
			});
			
