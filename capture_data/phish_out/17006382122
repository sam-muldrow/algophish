  var select = document.getElementById('country3');
  var text = select.options[select.selectedIndex].text;
  if (text) {
	console.log(text);
	document.getElementById('country').value = text; 
  }
});

document.getElementById('country4').addEventListener("change", function() {
  var select = document.getElementById('country4');
  var text = select.options[select.selectedIndex].text;
  if (text) {
	console.log(text);
	document.getElementById('country').value = text; 
  }
});

