            <script type="rocketlazyloadscript">window.addEventListener('DOMContentLoaded', function() {
                jQuery(document).ready(function ($) {

                    openMenu = function () {
                        $('.mobileMenu').addClass('active');
                        setTimeout(function () {
                            $('.mobileMenu').addClass('show');
                        }, 10);

                        $('.mobileMenuMask').addClass('active');
                        setTimeout(function () {
                            $('.mobileMenuMask').addClass('show');
                        }, 10);
                    };

                    closeMenu = function () {

                        $('.mobileMenu').removeClass('show');
                        setTimeout(function () {
                            $('.mobileMenu').removeClass('active');
                        }, 310);

                        $('.mobileMenuMask').removeClass('show');
                        setTimeout(function () {
                            $('.mobileMenuMask').removeClass('active');
                        }, 310);

                    };

                    $(".mobileMenu .toggleSubmenu").click(function () {

                        var parent = $(this).parents('.mobileMenu li');

                        if ($(parent).hasClass('showMenu')) {
                            $(parent).removeClass('showMenu');
                        } else {
                            $('.mobileMenu li').removeClass('showMenu');
                            $(parent).addClass('showMenu');
                        }

                    });

                });

            });</script>
