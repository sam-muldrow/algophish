        $(window).on('scroll', function() {
            var y = $(this).scrollTop();
            if (y >= wheight) {
                $("#sidenav").addClass("bg-white");
                $(".sidebar-logo").css('opacity', '1');
            } else {
                $("#sidenav").removeClass("bg-white");
                $(".sidebar-logo").css('opacity', '0')
            }
        });

