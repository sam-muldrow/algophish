         grecaptcha.ready(function() {
             grecaptcha.execute('6Lc2-RIaAAAAAHjRI6Li5LazF3zL5eTnVPINHupR', {action: 'contact'}).then(function(token) {
                if (token) {
                  document.getElementById('recaptcha').value = token;
                }
             });
         });
</script>


            




