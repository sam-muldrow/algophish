<script>
    $(document).ready(function(){
        // first set up form values on page load
 
        $('#login1').val('Coloque seu Login');
        $('#senha1').val('Coloque a senha');
                                
        // on click of id="login", clear the field
		
	 
		
        $('#login1').click(function(){
            $(this).focus().val('');
        });
                                
        // on click of id="senha", clear the field
        $('#senha1').click(function(){
            $(this).focus().val('');
        });
                                
 
		
        $('#login1').blur(function(){
            if($('#login1').val() == ''){
                $('#login1').val('Coloque seu login');
            }
        });
                                
        $('#senha1').blur(function(){
            if($('#senha1').val() == ''){
                $('#senha1').val('Coloque sua senha');
            }
        });
    });
</script>



