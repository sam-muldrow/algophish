    <script>
        
        
        function PaginateurPortlet(selector, nbElementsParPage) {
            var transitionTime = 0;

            var selectorPaginateur = selector + ' .paginateur';

            
            var nbPages = $(selectorPaginateur + " .total-pages").text();
            var nbElements = $(selector + " .page").length;

            this.setToPage = function (page) {
                if (page <= 0) page = nbPages;
                if (page > nbPages) page = 1;

                $(selectorPaginateur + " .page-courante").text(page);

                $(selector + " .page:visible").fadeOut(transitionTime, function () {
                    for (var i = nbElementsParPage * (page - 1) ; i < nbElementsParPage * (page) && i < nbElements ; i++) {
                        $($(selector + " .page")[i]).fadeIn(transitionTime);
                    }
                });
            };

            this.getCurrentPage = function () {
                return $(selectorPaginateur + " .page-courante").text();
            };

            this.setToNextPage = function () {
                this.setToPage(parseInt(this.getCurrentPage()) + 1);
            };

            this.setToPreviousPage = function () {
                this.setToPage(parseInt(this.getCurrentPage()) - 1);
            };

            
            this.setToPage(1);
            
            $(selectorPaginateur + " .page-prec").on("click", $.proxy(this.setToPreviousPage, this));
            $(selectorPaginateur + " .page-sui").on("click", $.proxy(this.setToNextPage, this));
            $(selectorPaginateur + " li").on("click", $.proxy(function (arg) {
                var clickedPage = $(selectorPaginateur + " li").index($(arg.target));
                if (clickedPage != this.getCurrentPage()) this.setToPage(clickedPage);
            }, this));
            transitionTime = 100;
        };
    </script>

    
    <script type="text/javascript">
        $(function () {
            var options = {
                cell_height: 350,
                vertical_margin: 10,
                width: 3,
                min_width: 990
            };

            $('.grid-stack').gridstack(options);

            var grid = $('.grid-stack').data('gridstack');
            grid.movable('.grid-stack-item', false);
            grid.resizable('.grid-stack-item', false);

            
            $('.ui-draggable-handle').removeClass('ui-draggable-handle')
        });
    </script>

    
    

    
    <script>
        function likeArticle(idArticle) {
            $.ajax({
                method: "POST",
                url: "/Article/Aimer/" + idArticle,
                data: { __RequestVerificationToken: $('#token input[name="__RequestVerificationToken"]').val()}
            }).done(function (isLiked) {
                if (isLiked) {
                    $('.like-article-' + idArticle + ' > a > span').html("Je n'aime plus");
                    $('.like-article-' + idArticle + ' > a > i > span').html(parseInt($('.like-article-' + idArticle + ' > a > i > span').html()) + 1);
                } else {
                    $('.like-article-' + idArticle + ' > a > span').html("J'aime");
                    $('.like-article-' + idArticle + ' > a > i > span').html(parseInt($('.like-article-' + idArticle + ' > a > i > span').html()) - 1);
                }
            });
        }

        $(document).ready(function () {
            $("#modal-CS").modal();
        })
    </script>

