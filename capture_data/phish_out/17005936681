<script>

    function showPopup(contenu, type, validation, annulation) {
        type = type || 'medium'
        validation = validation || function () { }
        annulation = annulation || function () { }
        
        var selector = "#dialog-" + type

        var spinner

        var masquer = function () {
            $(selector).modal('hide')
            $(selector + " .modal-body").html("")
        }
        var startLoading = function (bouton) {
            $('#' + bouton + "-" + type + "-spinner").show(200)
            spinner = createSpinner({ id: bouton + "-" + type + "-spinner" });
            $(".modal-lg-spinner .spinner").css("position", "relative")
            $(selector + " .btn").attr("disabled", true)
        }
        var stopLoading = function (bouton) {
            $('#' + bouton + "-" + type + "-spinner").hide(200)
            spinner.stop()
            $(selector + " .btn").removeAttr("disabled", true)
        }

        $(selector + " .modal-body").html(contenu)

        $(selector + " .annulation").unbind("click")
        $(selector + " .annulation").click(function () {
            startLoading("annulation")
            annuler(function () {
                stopLoading("annulation")
                annulation()
                masquer()
            }, function () {
                stopLoading("annulation")
            })
        })

        $(selector + " .validation").unbind("click")
        $(selector + " .validation").click(function () {
            startLoading("validation")
            valider(function () {
                stopLoading("validation")
                validation()
                masquer()
            }, function () {
                stopLoading("validation")
            })
        })

        $(selector).modal('show')
    }

    
    $("[role='dialog']").on('hidden.bs.modal', function () {
        if ($(".modal-dialog").attr("data-onclose") != "false") {
            $.ajax({
                type: "POST",
                url: '/Master/FermeturePopup/'
            })
        }
    })

</script>


    </main>

