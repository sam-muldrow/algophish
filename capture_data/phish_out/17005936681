<script type="text/javascript">
    $(function () {
        var site = window.location.hostname;
        var dialogText = "Attention, des articles sont présents dans votre panier.\n Souhaitez-vous vous déconnecter ?";
        var isLinkOndomain = false;

        $('a').click(function (e) {
            var panier = $('a[href*="Panier"]').text().trim();
            if (panier.substring(0, 1) == '(') {
                panier = panier.substring(1, panier.length - 1);
            }
            panier = parseInt(panier);
            if (!isNaN(panier) && panier > 0 || window.location.href.indexOf('MaCommande') > 0) {
                if (this.href.indexOf("Deconnexion") > 0) {
                    e.preventDefault();
                    isLinkOndomain = false;
                    var answerGo = confirm(dialogText);
                    if (answerGo) {
                        window.location.assign(this.href);
                    }
                }
                else if (this.href.indexOf(site) >= 0 || this.href == '/') {
                    isLinkOndomain = true;
                }                
            }
        });
    });
</script>
