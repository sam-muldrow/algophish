            this.page.api_key = 'kgLGW0evKY3stsTyDzF6Uv5Etlrt7MzZ7AWQESkvyZPev49GJCrbtByseVYlfQj6';

            // This adds the custom login/logout functionality
            this.sso = {
                name:   "ISMG",
