<script type='text/javascript' id='contact-form-7-js-extra'>
/* <![CDATA[ */
var wpcf7 = {"apiSettings":{"root":"http:\/\/www.comfortdiner.com\/wp-json\/contact-form-7\/v1","namespace":"contact-form-7\/v1"}};
