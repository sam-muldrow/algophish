		var TwitterShareCB_261424 = function(event){
			if(event && event.type == 'tweet'){
				// console.log("socialCB('twitter')");
				
			}
		};
		window.TwitterTweetHandlers = window.TwitterTweetHandlers || [];
		var TwitterTweetCallback = function(event){
			if( $(event.target).data("twitter-callback") ){
				var cb = $(event.target).data("twitter-callback");
				eval(cb+"(event);");
			}
		};


		if(!window.twttr){
			window.twttr = (function(d, s, id) {
				var js, fjs = d.getElementsByTagName(s)[0],
					t = window.twttr || {};
				if (d.getElementById(id)) return;
				js = d.createElement(s);
				js.id = id;
				js.src = "https://platform.twitter.com/widgets.js";
				fjs.parentNode.insertBefore(js, fjs);

				t._e = [];
				t.ready = function(f) {
					t._e.push(f);
				};

				return t;
			}(document, "script", "twitter-wjs"));

			twttr.ready(function (twttr) {
				if(!window.TwitterEventsBinded){
					twttr.events.bind('click', TwitterTweetCallback);
					twttr.events.bind('tweet', TwitterTweetCallback);
					window.TwitterEventsBinded = true;
				}
			});
		}else{
			twttr.ready(function (twttr) {
				if(!window.TwitterEventsBinded){
					twttr.events.bind('click', TwitterTweetCallback);
					twttr.events.bind('tweet', TwitterTweetCallback);
					window.TwitterEventsBinded = true;
				}
			});
		}



		var TwitterShareCallback__e72300bd0b98b73076633ee0a612bae9 = TwitterShareCB_261424;
