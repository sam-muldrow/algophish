			var MailOfferPagePopup = function(url){
				var popup = $("#mailofferpagepopupholder");
				if(!popup.length){
					$("body").append('<div id="mailofferpagepopupholder"></div>');
					var popup = $("#mailofferpagepopupholder");
				}

				$.get(url)
					.done(function(resp){
						popup.html(resp).popmeup({
							topMargin:75,
							closeButtonSelector : ".kapat",
							closeOnBackgroundClick:true,
							closeOnEscDown:true,
							closeCallback : function(elem){
									elem.empty();
								}
						}).trigger("popmeup:reposition");
					});
			};
				var shareCounters = (function(){
					var inject = function(url){
						var script = document.createElement('script');
						script.async = true;
						script.src = url;
						document.body.appendChild(script);
					};

					return {
						facebook : function(url, cbName){
							inject('https://graph.facebook.com/?id=' + url + '&callback=' + cbName);
						},
						twitter : function(url, cbName){
							// inject('https://cdn.api.twitter.com/1/urls/count.json?url=' + url + '&callback=' + cbName);
						}
					};
				})();

				var twitterCounterCB = function(res){
					var counter = res.count && !isNaN(res.count) ? res.count : 0;
					$("#shareOfferPage-twitter-count")
							.data('counter', counter)
							.text('('+ counter+')');
						updateTotalShareCount();
				};

				var facebookCounterCB = function(res){
					var counter = res.shares && !isNaN(res.shares) ? res.shares : 0;
					$("#shareOfferPage-facebook-count")
							.data('counter', counter)
							.text('('+ counter+')');
						updateTotalShareCount();
				};

				var updateTotalShareCount = function(){
					var total = 0;
					total += parseInt($("#shareOfferPage-twitter-count").data("counter"));
					total += parseInt($("#shareOfferPage-facebook-count").data("counter"));
					if(total == 0){
						$("#shareOfferPage-total-count")
							.data("counter", total)
							.text(TranslateJS.t('social', "Hiç paylaşılmadı"));
					}
					else{
						$("#shareOfferPage-total-count")
							.data("counter", total)
							.text(TranslateJS.t('social', ":total kere paylaşıldı", {":total":total}));
					}
				};
				var refreshAgeInfoWidget = function(name, el){
					if(el.hasClass("processing"))
						return false;

					el.addClass("processing").text( TranslateJS.t('domains', 'Yükleniyor...') );
					$.post('/site/refreshAgeInfo', {domain:name})
						.done(function(resp){
							resp = $.parseJSON(resp);
							if(resp.age != undefined && !isNaN(resp.age)){
								el.parent().text(resp.age);
							}
							else if(resp.tryAgain){
								el.text( TranslateJS.t('domains', 'Sonra tekrar deneyiniz.')).removeClass("processing");
							}
							else{
								el.parent().text(TranslateJS.t('domains',"Yaş bilgisine ulaşılamıyor."));
							}
						})
						.fail(function(err){
							el.text(TranslateJS.t('domains', "Tekrar deneyiniz")).removeClass("processing");
						});
						// .always();
				};


			function SoLPopup(scenario){
				var url = scenario == 'login' ? '/site/signuporlogin?scenario=login&return_url=%2Fakguler.com%3Futm_source%3Dakguler.com%26utm_medium%3Dpark%26utm_campaign%3D1' : '/site/signuporlogin?scenario=signup';
				if( !$('#SoLPopupHolder').length )
					$('body').append('<div id="SoLPopupHolder" style="min-width:434px;"></div>');

				$('#SoLPopupHolder')
					.load( url + '&' + Math.random() )
					.popmeup({
						closeButtonSelector : ".kapat",
						backgroundRGBA : "255,255,255,0.7",
						closeOnBackgroundClick:false,
						closeCallback : function(elem){ elem.empty(); }
					})
					.trigger('popmeup:reposition');
			};

			 var alertFallback = false;
			 if (typeof console === "undefined" || typeof console.log === "undefined") {
			 console = {};
			 if (alertFallback) {
				 console.log = function(msg) {
						alert(msg);
				 };
			 } else {
				 console.log = function() {};
			 }
			 }

			 function loginPopup(){SoLPopup('login');}
			 function uyeOlPopup(){SoLPopup('signup');}

		
var backToSearchResults = function(url){
	$.cookie("backToSearchResultsNotified", 1, {expires:30});
	setTimeout(function(){
		window.location.href = url;
	}, 100);
};
jQuery(function($) {

	$(".idn-tooltip").tipTip({attribute:"data-tooltip", defaultPosition:"top", delay:0, maxWidth:"250px"});

			$(document).on("click", ".buyukKucukHarfCevir", toggleCaseWidgetClicked);
			$(document).on("mousedown", ".buyukKucukHarfCevir", function(e){e.preventDefault();});
			toggleCaseWidgetReady();
			shareCounters.facebook('https://www.alanadlari.com/akguler.com', 'facebookCounterCB');
			shareCounters.twitter('https://www.alanadlari.com/akguler.com', 'twitterCounterCB');


			$("#alanadlaricomAramaForm").submit( function(){
				var $form = $(this);
				var $div = $("#gelismisArama");
				var serialized = $form.serialize();

				var v = $form.find("input[name=keyword]").val();
				v = v.split(".");
				v = v[0];
				v = v.replace("ı","i").replace("ş","s").replace("ğ","g").replace("ü","u").replace("ö","o").replace("ç","c").replace("İ","I").replace("Ş","S").replace("Ğ","G").replace("Ü","U").replace("Ö","O").replace("Ç","C");
				v = v.replace(/\W\s/gi, "");

				if( v == 'anahtar kelime giriniz' )
					v = "";

				$form.find("input[name=keyword]").val( v );

				var targeturl = "/arama?"+ $("#alanadlaricomAramaForm").serialize() +"&"+ $("#alanadlaricomGelismisAramaForm").serialize();
				window.location.href = targeturl;



				return false;
			});


			$(".aramayiSifirla").click(function(){
				$("#alanadlaricomAramaForm").find("input[name=keyword]").val( "" );
				$("#alanadlaricomGelismisAramaForm").find("input[name=portfolio]").remove();
				$("input[name='filters[ktype]']").each( function(){
					$(this).prop("checked", false);
					$(this).closest("span").find("a").removeClass("jqTransformChecked");
				});
			});

		

				$(".hasTooltip").tipTip({attribute:"data-tooltip", defaultPosition:"right", delay:0});
				$(".hasTooltip.keepAlive").tipTip({attribute:"data-tooltip", defaultPosition:"right", keepAlive:true, delay:0});
				$(".hasTooltip.top").tipTip({attribute:"data-tooltip", defaultPosition:"top", delay:0});
				$(".hasTooltip.backToSearchResults").tipTip({attribute:"data-tooltip", defaultPosition:"top", delay:0, keepAlive:true});


				if($(".backToSearchResults").length){
					if($.cookie("backToSearchResultsNotified") === null){
						$(".backToSearchResults").trigger("mouseenter");
					}
				}

			

			$(document).on('click','#uyeOlPopupTrigger, .uyeOlPopupTrigger', function(e){
				e.preventDefault();
				SoLPopup('signup');
			});
			$(document).on('click','#loginPopupTrigger, .loginPopupTrigger', function(e){
				e.preventDefault();
				SoLPopup('login');
			});


		

		$.fn.popmeup.defaultSettings.zIndexBase = 4100;
		getLastTwit('alanadlari');
	
		$("span.changeApplicationLanguage:not(.selected)").on('click', function(e){
			var lang = $(this).data('language');
			if(lang){
				var form = '<form method="post" action="'+ window.location.href +'" style="display:none;">';
				form += '<input type="hidden" name="_changeLanguage" value="'+lang+'">';
				form += '<input type="submit">';
				form += '</form>';
				form = $(form);
				$("body").append(form);
				form.submit();
			}
		});
});
