	function offerFormDivToggle(){
		if( $("#offerFormDiv").is(":visible") ){
			$("#offerFormDiv").hide();
		}
		else{
			$("#offerFormDiv").show();
		}
	}


			var toggleCaseWidgetReady   = function(){
				$(".buyukKucukHarfCevir").each(function(i,e){
					e = $(e);
					toggleCaseWidgetInspectElement(e);
					e = $(e);
					var currentIndex = e.data("current-case");
					var dataMap = e.data("toggle-case-map");


					$(".in-ortaBlok15-domainwp-sol h2:first b").text(dataMap[currentIndex].value);
					e.find("span:first").text(dataMap[currentIndex].text);
					if(typeof SIGDIR == "function")
						SIGDIR($(".in-ortaBlok15-domainwp-sol h2:first"), 36, 330);
				});
			};

			var toggleCaseWidgetClicked = function(event){
				event.preventDefault();
				var e = $(this);
				var currentIndex = parseInt(e.data("current-case"));
				var dataMap = e.data("toggle-case-map");
				var nextIndex = dataMap[currentIndex+1] ? currentIndex+1 : 0;
				e.data("current-case", nextIndex);


				$(".in-ortaBlok15-domainwp-sol h2:first b").text(dataMap[nextIndex].value);
				e.find("span:first").text(dataMap[nextIndex].text);

				if(typeof SIGDIR == "function")
					SIGDIR($(".in-ortaBlok15-domainwp-sol h2:first"), 36, 330);
			};

			var toggleCaseWidgetInspectElement = function(el){
				if(el.data("current-case") == undefined){
					el.data("current-case", 0);
					var dataPretty    = el.data("pretty");
					var dataLowercase = el.data("lowercase");
					var dataUppercase = el.data("uppercase");

					if(dataPretty != dataLowercase){
						dataMap = [
							{value: dataLowercase, text: TranslateJS.t('domains', "Utf-8")},
							{value: dataPretty, text: TranslateJS.t('domains', "Büyük Harf")},
							{value: dataUppercase, text: TranslateJS.t('domains', "Küçük Harf")}
						];
					}
					else{
						dataMap = [
							{value: dataLowercase, text: TranslateJS.t('domains', "Büyük Harf")},
							{value: dataUppercase, text: TranslateJS.t('domains', "Küçük Harf")}
						];
					}
					el.data("toggle-case-map", dataMap);
				}
			};

