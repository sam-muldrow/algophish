<p style="text-align: center; padding: 0 20px;"><a href="/categoria/depoimentos/">Meu nome é Oleontina do Carmo Batschauer, tenho 79 anos, sofria de dores na coluna há 19 anos, dia e noite sem posição para dormir. Foi agravando de tal forma que…</a></p>
</div>
<div class="imageframe-align-center"><a href="/categoria/depoimentos/"><span class="fusion-imageframe imageframe-none imageframe-1 hover-type-none"><img class="img-responsive wp-image-35509" title="" src="/wp-content/uploads/2020/06/oleontina-d.jpg?x57771" /></span></a></div>
</div></div>
</div>
</div>
<script type='text/javascript'>
	jQuery(document).ready(function() {
		jQuery('#slider_35679').owlCarousel({
			responsive:{
				0:{ items:1 },
				480:{ items:1 },
				768:{ items:2 },
				980:{ items:2 },
				1200:{ items:3 },
				1500:{ items:3 }
			},
			autoplay : true,
			autoplayTimeout : 5000,
			autoplayHoverPause : true,
			smartSpeed : 200,
			fluidSpeed : 200,
			autoplaySpeed : 200,
			navSpeed : 200,
			dotsSpeed : 200,
			loop : true,
			nav : false,
			navText : ['Previous','Next'],
			dots : true,
			responsiveRefreshRate : 200,
			slideBy : 1,
			mergeFit : true,
			autoHeight : false,
			mouseDrag : false,
			touchDrag : false
		});
		jQuery('#slider_35679').css('visibility', 'visible');
		sa_resize_slider_35679();
		window.addEventListener('resize', sa_resize_slider_35679);
		function sa_resize_slider_35679() {
			var min_height = '50';
			var win_width = jQuery(window).width();
			var slider_width = jQuery('#slider_35679').width();
			if (win_width < 480) {
				var slide_width = slider_width / 1;
			} else if (win_width < 768) {
				var slide_width = slider_width / 1;
			} else if (win_width < 980) {
				var slide_width = slider_width / 2;
			} else if (win_width < 1200) {
				var slide_width = slider_width / 2;
			} else if (win_width < 1500) {
				var slide_width = slider_width / 3;
			} else {
				var slide_width = slider_width / 3;
			}
			slide_width = Math.round(slide_width);
			var slide_height = '0';
			if (min_height == 'aspect43') {
				slide_height = (slide_width / 4) * 3;				slide_height = Math.round(slide_height);
			} else if (min_height == 'aspect169') {
				slide_height = (slide_width / 16) * 9;				slide_height = Math.round(slide_height);
			} else {
				slide_height = (slide_width / 100) * min_height;				slide_height = Math.round(slide_height);
			}
			jQuery('#slider_35679 .owl-item .sa_hover_container').css('min-height', slide_height+'px');
		}
		var owl_goto = jQuery('#slider_35679');
		jQuery('.slider_35679_goto1').click(function(event){
			owl_goto.trigger('to.owl.carousel', 0);
		});
		jQuery('.slider_35679_goto2').click(function(event){
			owl_goto.trigger('to.owl.carousel', 1);
		});
		jQuery('.slider_35679_goto3').click(function(event){
			owl_goto.trigger('to.owl.carousel', 2);
		});
		jQuery('.slider_35679_goto4').click(function(event){
			owl_goto.trigger('to.owl.carousel', 3);
		});
		jQuery('.slider_35679_goto5').click(function(event){
			owl_goto.trigger('to.owl.carousel', 4);
		});
		jQuery('.slider_35679_goto6').click(function(event){
			owl_goto.trigger('to.owl.carousel', 5);
		});
		var resize_35679 = jQuery('.owl-carousel');
		resize_35679.on('initialized.owl.carousel', function(e) {
			if (typeof(Event) === 'function') {
				window.dispatchEvent(new Event('resize'));
			} else {
				var evt = window.document.createEvent('UIEvents');
				evt.initUIEvent('resize', true, false, window, 0);
				window.dispatchEvent(evt);
			}
		});
	});
</script>
