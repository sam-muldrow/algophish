                                                            var inputTele = document.querySelector("#txtDonarMobile_" + '64d9b8f42053f92ca2745782');

                                                            txtDonarMobileObj = window.intlTelInput(inputTele, {
                                                                allowDropdown: true,
                                                                separateDialCode: true,
                                                                initialCountry: "kw",
                                                                utilsScript: "/content/intlTelInput/utils.js",
                                                            });

                                                            document.getElementById("txtDonarMobile_" + '64d9b8f42053f92ca2745782').addEventListener('blur', function () {
                                                                document.getElementById("txtDonarMobile_" + '64d9b8f42053f92ca2745782').value = toEnglishNumber(document.getElementById("txtDonarMobile_" + '64d9b8f42053f92ca2745782').value);
