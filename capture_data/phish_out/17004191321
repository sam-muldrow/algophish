    <script>
                            var enableDonation6551f3744939b5b807b51524 = true;

                            document.addEventListener("DOMContentLoaded", function (event) {
                            if (document.getElementById("DonationAmount_6551f3744939b5b807b51524") != null) {
                            $("#DonationAmount_6551f3744939b5b807b51524").on('focusout', function () {
                            if ($("#DonationAmount_6551f3744939b5b807b51524").val() == "0") {
                            $("#DonationAmount_6551f3744939b5b807b51524").val("");
                            } else {
                            $("#DonationAmount_6551f3744939b5b807b51524").val(toEnglishNumber($("#DonationAmount_6551f3744939b5b807b51524").val()));
                            }
                            }).TouchSpin({
                            min: 0,
                            max: 1000000000,
                            forcestepdivisibility: 'round',
                            buttondown_class: "btn btn-outline-white",
                            buttonup_class: "btn btn-outline-white",
                            buttondown_txt: '<i class="bi bi-dash-lg"></i>',
                            buttonup_txt: '<i class="bi bi-plus-lg"></i>'
                            }).on('touchspin.on.startspin', function () {
                            if (enableDonation6551f3744939b5b807b51524) {
                            $("#predefined-values-div-6551f3744939b5b807b51524").find("a").removeClass("active");
                            $("#preAmount_6551f3744939b5b807b51524").val("0");
                            $("#EnteredPrice_6551f3744939b5b807b51524").val($("#DonationAmount_6551f3744939b5b807b51524").val());

                            if ($("#DonationCount_6551f3744939b5b807b51524").length > 0) {
                            $("#DonationCount_6551f3744939b5b807b51524").val("1");
                            }

                            attribute_change_handler_6551f3744939b5b807b51524();
                            }
                            }).on('blur', function () {
                            if (enableDonation6551f3744939b5b807b51524) {
                            $("#predefined-values-div_6551f3744939b5b807b51524").find("a").removeClass("active");
                            $("#preAmount_6551f3744939b5b807b51524").val("0");
                            $("#EnteredPrice_6551f3744939b5b807b51524").val($("#DonationAmount_6551f3744939b5b807b51524").val());

                            if ($("#DonationCount_6551f3744939b5b807b51524").length > 0) {
                            $("#DonationCount_6551f3744939b5b807b51524").val("1");
                            }

                            attribute_change_handler_6551f3744939b5b807b51524();
                            }
                            });
                            }
                            });


                            function setPredefinedAmount6551f3744939b5b807b51524(obj, objVal, productId) {

                            if (objVal != null && objVal != "") {
                            var childrens = document.getElementById("predefined-values-div-" + productId).querySelectorAll('a');

                            for (var i = 0; i < childrens.length; i++) {
                            childrens[i].classList.remove("active");
                            }

                            obj.classList.add("active");

                            document.getElementById("preAmount_" + productId).value = objVal;
                            document.getElementById("EnteredPrice_" + productId).value = "0";
                            eval("attribute_change_handler_" + productId + "();");
                            }
                            }

                            function attribute_change_handler_6551f3744939b5b807b51524() {

                            var form = document.getElementById('homepage-product-details-form-6551f3744939b5b807b51524');

                            var tempFormData = new FormData(form);
                            axios({
                            url: "/actioncart/productdetails_attributechange?productId=6551f3744939b5b807b51524",
                            method: 'post',
                            data: tempFormData,
                            headers: {
                            'Accept': 'application/json',
                            'Content-Type': 'application/json',
                            'X-Response-View': 'Json'
                            }
                            }).then(function (response) {
                            if (response.data != null) {
                            if (document.getElementById('DonationAmount_6551f3744939b5b807b51524') != null) {

                            if (response.data.price == 0) {
                            document.getElementById('DonationAmount_6551f3744939b5b807b51524').value = "";
                            } else {
                            document.getElementById('DonationAmount_6551f3744939b5b807b51524').value = response.data.price;
                            }
                            }

                            if (response.data.message) {

                            if (response.data.message != null && response.data.message.length > 0) {
                            for (var i = 0; i < response.data.message.length; i++) {
                            displayBarNotification(response.data.message[i], 'error', 3500);
                            }
                            }
                            }

                            if (response.data.pictureUrl) {
                            document.getElementById('homepagemain-product-img-6551f3744939b5b807b51524').setAttribute("src", response.data.pictureUrl);
                            }

                            if (response.data.allowAnyDonationAmount) {
                            enableDonation6551f3744939b5b807b51524 = response.data.allowAnyDonationAmount;
                            document.getElementById('DonationAmount_6551f3744939b5b807b51524').removeAttribute("disabled");
                                
                                    document.getElementById('predefined-values-div-6551f3744939b5b807b51524').classList.remove("d-none");
                                                            } else {
                            document.getElementById('DonationAmount_6551f3744939b5b807b51524').setAttribute("disabled", "disabled");
                                
                                    document.getElementById('predefined-values-div-6551f3744939b5b807b51524').classList.add("d-none");
                                                            }

                            }
                            }).catch(function (error) {
                            alert(error);
                            });
                            }

                            document.addEventListener("DOMContentLoaded", function (event) {
                            attribute_change_handler_6551f3744939b5b807b51524();
                            ;
                            });

                        </script>
