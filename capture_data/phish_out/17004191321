<script>
                        var enableDonation64d9b8f42053f92ca2745782 = true;
                        function checkDonateForm(productId, randomId, urladd, formselector, txtPhoneId, PhoneId) {

                            var showMobile = document.getElementById("ShowMobileNumberField_" + productId).value;

                            document.getElementById(PhoneId).value = "";

                            if (showMobile) {
                                var valid = true;
                                var errorMsg = "";
                                var errorMap = ["رقم غير صحيح", "كود محاولة كوب خاطئ", "رقم التليفون صغير", "رقم الهاتف اطول من المسموح", "رقم غير صحيح"];

                                document.getElementById("spPhone_" + productId).innerHTML = "";

                                if (document.getElementById(txtPhoneId).value != "") {
                                    if (!txtDonarMobileObj.isValidNumber()) {
                                        var errorCode = txtDonarMobileObj.getValidationError();
                                        errorMsg = errorMap[errorCode];

                                        document.getElementById("spPhone_" + productId).innerHTML = errorMap[errorCode];
                                        document.getElementById("spPhone_" + productId).classList.remove("field-validation-valid");
                                        document.getElementById("spPhone_" + productId).classList.add("field-validation-error");

                                        valid = false;
                                    } else {
                                        document.getElementById(PhoneId).value = txtDonarMobileObj.getNumber().replace(/\s/g, '');
                                        document.getElementById("spPhone_" + productId).classList.add("field-validation-valid");
                                        document.getElementById("spPhone_" + productId).classList.remove("field-validation-error");
                                    }
                                }

                                if (!valid) {
                                    return;
                                }
                            }

                            var productRandomId = productId + randomId;

                            AxiosCart.addproducttocart_details(urladd, formselector, productRandomId);
                        }

                        function setShareValue(hasShare, obj, objVal, productId) {

                            var isSharesUsed = hasShare;

                            if (isSharesUsed && objVal != null && objVal != "") {
                                document.getElementById("EnteredPrice_" + productId).value = "0";
                                document.getElementById("ShareId_" + productId).value = objVal;

                                var childrens = document.getElementById("share-values-div-" + productId).querySelectorAll('a');

                                for (var i = 0; i < childrens.length; i++) {
                                    childrens[i].classList.remove("active");
                                }

                                obj.classList.add("active");


                                    eval("attribute_change_handler_" + productId + "();");
                            }
                        }


                            document.addEventListener("DOMContentLoaded", function (event) {
                                if (document.getElementById("DonationAmount_64d9b8f42053f92ca2745782") != null) {
                                    $("#DonationAmount_64d9b8f42053f92ca2745782").on('focusout', function () {
                                        if ($("#DonationAmount_64d9b8f42053f92ca2745782").val() == "0") {
                                            $("#DonationAmount_64d9b8f42053f92ca2745782").val("");
                                        } else {
                                            $("#DonationAmount_64d9b8f42053f92ca2745782").val(toEnglishNumber($("#DonationAmount_64d9b8f42053f92ca2745782").val()));
                                        }
                                    }).TouchSpin({
                                        min: 1,
                                        max: 1000000000,
                                        forcestepdivisibility: 'round',
                                        buttondown_class: "btn btn-outline-white",
                                        buttonup_class: "btn btn-outline-white",
                                        buttondown_txt: '<i class="bi bi-dash-lg"></i>',
                                        buttonup_txt: '<i class="bi bi-plus-lg"></i>'
                                    }).on('touchspin.on.startspin', function () {
                                        if (enableDonation64d9b8f42053f92ca2745782) {
                                            $("#predefined-values-div-64d9b8f42053f92ca2745782").find("a").removeClass("active");
                                            $("#preAmount_64d9b8f42053f92ca2745782").val("0");
                                            $("#EnteredPrice_64d9b8f42053f92ca2745782").val($("#DonationAmount_64d9b8f42053f92ca2745782").val());

                                            if ($("#DonationCount_64d9b8f42053f92ca2745782").length > 0) {
                                                $("#DonationCount_64d9b8f42053f92ca2745782").val("1");
                                            }

                        attribute_change_handler_64d9b8f42053f92ca2745782();
                                        }
                                    }).on('blur', function () {
                                        if (enableDonation64d9b8f42053f92ca2745782) {
                                            $("#predefined-values-div_64d9b8f42053f92ca2745782").find("a").removeClass("active");
                                            $("#preAmount_64d9b8f42053f92ca2745782").val("0");
                                            $("#EnteredPrice_64d9b8f42053f92ca2745782").val($("#DonationAmount_64d9b8f42053f92ca2745782").val());

                                            if ($("#DonationCount_64d9b8f42053f92ca2745782").length > 0) {
                                                $("#DonationCount_64d9b8f42053f92ca2745782").val("1");
                                            }

                        attribute_change_handler_64d9b8f42053f92ca2745782();
                                        }
                                    });
                                }
                            });


                        function setPredefinedAmount(obj, objVal, productId) {

                            if (objVal != null && objVal != "") {
                                var childrens = document.getElementById("predefined-values-div-" + productId).querySelectorAll('a');

                                for (var i = 0; i < childrens.length; i++) {
                                    childrens[i].classList.remove("active");
                                }

                                obj.classList.add("active");

                                document.getElementById("preAmount_" + productId).value = objVal;
                                document.getElementById("EnteredPrice_" + productId).value = "0";


                                    eval("attribute_change_handler_" + productId + "();");
                            }
                        }

                        function attribute_change_handler_64d9b8f42053f92ca2745782() {

                            if (true) {
                                if (document.getElementById("dvCount_64d9b8f42053f92ca2745782") != undefined && document.getElementById("dvCount_64d9b8f42053f92ca2745782") != null) {
                                    if (document.getElementById("ShareId_64d9b8f42053f92ca2745782").value == "") {
                                        document.getElementById("dvCount_64d9b8f42053f92ca2745782").style.display = "none";
                                    } else {
                                        document.getElementById("dvCount_64d9b8f42053f92ca2745782").style.display = "block";
                                    }
                                }
                            }

                            var form = document.getElementById('homepage-product-details-form-64d9b8f42053f92ca2745782');

                            var tempFormData = new FormData(form);
                            axios({
                                url: "/actioncart/productdetails_attributechange?productId=64d9b8f42053f92ca2745782",
                                method: 'post',
                                data: tempFormData,
                                headers: {
                                    'Accept': 'application/json',
                                    'Content-Type': 'application/json',
                                    'X-Response-View': 'Json'
                                }
                            }).then(function (response) {
                                if (response.data != null) {
                                    if (document.getElementById('DonationAmount_64d9b8f42053f92ca2745782') != null) {

                                        if (response.data.price == 0) {
                                            document.getElementById('DonationAmount_64d9b8f42053f92ca2745782').value = "";
                                        } else {
                                            document.getElementById('DonationAmount_64d9b8f42053f92ca2745782').value = response.data.price;
                                        }
                                    }

                                    if (response.data.message) {

                                        if (response.data.message != null && response.data.message.length > 0) {
                                            for (var i = 0; i < response.data.message.length; i++) {
                                                displayBarNotification(response.data.message[i], 'error', 3500);
                                            }
                                        }
                                    }

                                    if (response.data.pictureUrl) {
                                        document.getElementById('homepagemain-product-img-64d9b8f42053f92ca2745782').setAttribute("src", response.data.pictureUrl);
                                    }

                                    if (response.data.showDonorNameField) {
                                        document.getElementById('dvDonarName64d9b8f42053f92ca2745782').classList.remove("d-none");
                                    } else {
                                        document.getElementById('dvDonarName64d9b8f42053f92ca2745782').classList.add("d-none");
                                    }
                                    if (response.data.showDonorEmailField) {
                                        document.getElementById('dvDonarEmail64d9b8f42053f92ca2745782').classList.remove("d-none");
                                    } else {
                                        document.getElementById('dvDonarEmail64d9b8f42053f92ca2745782').classList.add("d-none");
                                    }

                                    document.getElementById('ShowMobileNumberField_64d9b8f42053f92ca2745782').value = response.data.showMobileNumberField;

                                    if (response.data.showMobileNumberField) {
                                        document.getElementById('dvDonarMobile64d9b8f42053f92ca2745782').classList.remove("d-none");
                                    } else {
                                        document.getElementById('dvDonarMobile64d9b8f42053f92ca2745782').classList.add("d-none");
                                    }
                                    if (response.data.measureName) {
                                        document.getElementById('spDonationAmount64d9b8f42053f92ca2745782').innerHTML = response.data.measureName;
                                    } else {
                                        document.getElementById('spDonationAmount64d9b8f42053f92ca2745782').innerHTML = "";
                                    }
                                    if (response.data.allowAnyDonationAmount) {
                        enableDonation64d9b8f42053f92ca2745782 = response.data.allowAnyDonationAmount;
                                        document.getElementById('DonationAmount_64d9b8f42053f92ca2745782').removeAttribute("disabled");
                            
                                                document.getElementById('predefined-values-div-64d9b8f42053f92ca2745782').classList.remove("d-none");
                                                                                                                } else {
                                        document.getElementById('DonationAmount_64d9b8f42053f92ca2745782').setAttribute("disabled", "disabled");
                            
                                                document.getElementById('predefined-values-div-64d9b8f42053f92ca2745782').classList.add("d-none");
                                                                                                                }
                                    if (response.data.showProjectCount) {
                                        document.getElementById('dvCount_64d9b8f42053f92ca2745782').classList.remove("d-none");
                                    } else {
                                        document.getElementById('dvCount_64d9b8f42053f92ca2745782').classList.add("d-none");
                                    }



                                }
                            }).catch(function (error) {
                                alert(error);
                            });
                        }

                        document.addEventListener("DOMContentLoaded", function (event) {
                        attribute_change_handler_64d9b8f42053f92ca2745782();
                        document.getElementById('DonationCount_64d9b8f42053f92ca2745782').addEventListener('blur', function(){attribute_change_handler_64d9b8f42053f92ca2745782();});
;

                            //If shares is used and share are not ignorable
                                                                        });

