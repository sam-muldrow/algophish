    <script>
    function checkCarouselWidth() {
        var windowsize = window.outerWidth;

        if (windowsize > 800) {
            if (document.querySelector('#NooriCarousel') != null)
                document.querySelector('#NooriCarousel').style.display = "block";

            //In case there is mobile view
            if (document.querySelector('#NooriCarouselMob') != null)
                document.querySelector('#NooriCarouselMob').style.display = "none";
        } else {
            if (document.querySelector('#NooriCarousel') != null)
                document.querySelector('#NooriCarousel').style.display = "none";

            //In case there is mobile view
            if (document.querySelector('#NooriCarouselMob') != null)
                document.querySelector('#NooriCarouselMob').style.display = "block";
        }
    }

    window.addEventListener("resize", function () {
        //console.log(window.outerWidth);
        checkCarouselWidth();
    });

    document.addEventListener('DOMContentLoaded', function () {
        // fire if first slide has set full width
        if (document.querySelector('#NooriCarousel') != null) {
            if (document.querySelector('#NooriCarousel .carousel-item').getAttribute('data-width') == 'full') {
                var mainContainer = document.querySelector(".main-container");
                var NooriCarousel = document.getElementById("NooriCarousel");
                var parentDiv = mainContainer.parentNode;

                parentDiv.insertBefore(NooriCarousel, mainContainer);
            }
        }

        //In case there is mobile view
        // fire if first slide has set full width
        if (document.querySelector('#NooriCarouselMob') != null) {
            if (document.querySelector('#NooriCarouselMob .carousel-item').getAttribute('data-width') == 'full') {
                var mainContainer = document.querySelector(".main-container");
                var NooriCarouselMob = document.getElementById("NooriCarouselMob");
                var parentDiv = mainContainer.parentNode;

                parentDiv.insertBefore(NooriCarouselMob, mainContainer);
            }
        }

        // Execute on load
        checkCarouselWidth();
    });
</script>
    
