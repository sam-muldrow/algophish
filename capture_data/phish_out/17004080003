	var catTitle = '–ö–∞—Ç–µ–≥–æ—Ä—ñ—ó';
	var prodTitle = '–¢–æ–≤–∞—Ä–∏';
	var viewAllTitle = '–£—Å—ñ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–∏ –ø–æ—à—É–∫—É';
