<script type="text/javascript">
var insuranceType='G';
function resetAllForm(id) {
	if(document.getElementById('doctor').value != 'NAME') {
		document.getElementById('doctor').value = 'NAME';
	}
	if(document.getElementById('speciality').value != '') {
		document.getElementById('speciality').value = '';
		getReasons('');
	}
	
	if(document.getElementById('insurance'+id).value != '') {
		document.getElementById('insurance'+id).value = '';
		getInsurancePlans('','insurance'+id);
	}
	if(document.getElementById('zip'+id).value != 'AREA OR ZIP CODE') {
		document.getElementById('zip'+id).value = 'AREA OR ZIP CODE';
	}	
}
function resetAllFormSearch(id) {
	if(document.getElementById('doctor').value != 'NAME') {
		document.getElementById('doctor').value = 'NAME';
	}
	if(document.getElementById('speciality').value != '') {
		document.getElementById('speciality').value = '';
		getReasons('');
	}
	
	if(document.getElementById('insurance'+id).value != '') {
		document.getElementById('insurance'+id).value = '';
		getInsurancePlans('','insurance'+id);
	}
	if(document.getElementById('zip'+id).value != 'AREA OR ZIP CODE') {
		document.getElementById('zip'+id).value = 'AREA OR ZIP CODE';
	}
	if(document.getElementById('gender').value != '') {
		document.getElementById('gender').value = '';
	}	
}
function hideDiv(id){
	$('#insurancediv'+id).hide("200");
	$('#divplan'+id).hide("200");
	$('#more'+id).show();
	$('#less'+id).hide();	
}
	function showDiv(id){
	$('#insurancediv'+id).show("200");
	$('#divplan'+id).show("200");
	$('#more'+id).hide();
	$('#less'+id).show();	
}
Array.prototype.in_array = function(p_val) {
for(var i = 0, l = this.length; i < l; i++) {
if(this[i] == p_val) {
return true;
}
}
return false;
}


function getReasons(speciality_id){	
 	  var xmlhttp;
		if (window.XMLHttpRequest)
			  {// code for IE7+, Firefox, Chrome, Opera, Safari
			  xmlhttp=new XMLHttpRequest();
			  }
			else
			  {// code for IE6, IE5
			  xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
			  }
			xmlhttp.onreadystatechange=function()
			  {
				if (xmlhttp.readyState==4 && xmlhttp.status==200)
				{

				 document.getElementById("reasonDiv").style.display="block";
				 document.getElementById("reasonDiv").innerHTML=xmlhttp.responseText;				  
				}
				else {
				 // document.getElementById("reasonloading").innerHTML='<img src="/images/loadingsmall.gif" style="float: right; margin: -20px -30px 0px 0px;" />';				 
				}
			  }
			xmlhttp.open("GET","https://docchecker.com/ajaxphpcall/searchresult/getReasonsNew.php?id="+speciality_id,true);
			xmlhttp.send();
	 }
	 
	 function funOnChangeSpel(splID){
			var spe_array = [33,30,26,16,41,25];
			if(spe_array.in_array(splID) && insuranceType=='G') {
					insuranceType = 'D';			
			}
			else if(spe_array.in_array(splID) && insuranceType == 'D') {
			  return false;
			} 
			else { 
				if(insuranceType == 'G') {
					return false;
				} 
				else {  
					insuranceType = 'G';
				}
			}	

			  // $('#more1').hide();
			  // $('#less1').show();
			  callSpecialtyInsurance('0',splID,'insurance1');
			  //getInsurancePlans('0','insurance1');
	}	
	
function callSpecialtyInsurance(insurance, specialty, divid)
{	  
// document.getElementById('reasonDiv').style.display='block';
// document.getElementById('insurancediv1').style.display='block';

	var xmlhttp;
	if (window.XMLHttpRequest)
		{
			xmlhttp=new XMLHttpRequest();
		}
	else
	{
		xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
	}
	xmlhttp.onreadystatechange=function()
	{
		if (xmlhttp.readyState==4 && xmlhttp.status==200)
		{
			document.getElementById(divid).innerHTML=xmlhttp.responseText;			
		}
	  }
	xmlhttp.open("GET","https://docchecker.com/ajaxphpcall/searchresult/callSpecialtyInsuranceforIndex.php?insurance="+insurance+"&specialty="+specialty,true);
	xmlhttp.send();
}
function getInsurancePlans(insurance_id,control_id){
	if(control_id=='insurance1'){
	var suff=1;
	}
	else if(control_id=='insurance2'){
	var suff=2;
	}
	document.getElementById('Plan'+suff).style.display='block';
	  var speciality=document.getElementById('speciality').value;
	  var xmlhttp;
		if (window.XMLHttpRequest)
			  {// code for IE7+, Firefox, Chrome, Opera, Safari
			  xmlhttp=new XMLHttpRequest();
			  }
			else
			  {// code for IE6, IE5
			  xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
			  }
			xmlhttp.onreadystatechange=function()
			  {
				if (xmlhttp.readyState==4 && xmlhttp.status==200)
				{
				document.getElementById("selplan"+suff).innerHTML=xmlhttp.responseText;
				document.getElementById("planloading"+suff).innerHTML='';
				} 
				else {
				document.getElementById("planloading"+suff).innerHTML='<img src="/images/loadingsmall.gif" style="float: right; margin: -20px -30px 0px 0px;" />';	
				}
			  }
			 
			xmlhttp.open("GET","https://docchecker.com/ajaxphpcall/searchresult/getinsuranceplansforIndexfornew.php?id="+insurance_id+"&speciality="+speciality,true);
			xmlhttp.send();
}

</script>
