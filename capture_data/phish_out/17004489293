                                if ('On' != 'On'){
                                    $(document).find('#ays-pb-screen-shade_2').css({'opacity': '0', 'display': 'none !important', 'pointer-events': 'none'});
                                    $(document).find('.ays-pb-modal_2').css('pointer-events', 'auto');
                                    $(document).find('.av_pop_modals_2').css('pointer-events','none');
