								elementorFrontend.hooks.addAction("frontend/element_ready/global", function ( $scope ) {
									if ( $scope.find( "#forminator-module-330" ).length > 0 ) {
										if (typeof ($.fn.forminatorLoader) !== 'undefined') {
											renderForminatorAjax( 330, {"action":"forminator_load_form","type":"forminator_forms","id":330,"render_id":0,"is_preview":false,"preview_data":[],"last_submit_data":[],"nonce":"ac4860935d","extra":{"_wp_http_referer":"","page_id":12,"referer_url":""}}, 0 );
										}
									}
								});
							}
							// Elementor Popup
                            $( document ).on( 'elementor/popup/show', () => {
                                if (typeof ($.fn.forminatorLoader) !== 'undefined') {
                                    renderForminatorAjax( 330, {"action":"forminator_load_form","type":"forminator_forms","id":330,"render_id":0,"is_preview":false,"preview_data":[],"last_submit_data":[],"nonce":"ac4860935d","extra":{"_wp_http_referer":"","page_id":12,"referer_url":""}}, 0 );
                                }
                            } );
						}

						if (typeof ($.fn.forminatorLoader) === 'undefined') {
							console.log('forminator scripts not loaded');
						} else {
							renderForminatorAjax( 330, {"action":"forminator_load_form","type":"forminator_forms","id":330,"render_id":0,"is_preview":false,"preview_data":[],"last_submit_data":[],"nonce":"ac4860935d","extra":{"_wp_http_referer":"","page_id":12,"referer_url":""}}, 0 );
						}
					});
					function renderForminatorAjax ( id, frontLoaderConfig, renderId ) {
    					var front_loader_config = frontLoaderConfig;
    					front_loader_config.extra.referer_url = document.referrer;
    					$('#forminator-module-' + id + '[data-forminator-render="' + renderId + '"]')
    						.forminatorLoader(front_loader_config);
				    }
				})();
			}(jQuery, document, window));
/* ]]> */
</script>
