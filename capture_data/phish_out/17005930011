    <script id="js-api-toast-template" type="text/template">
        <div class="toast <%= type ? 'toast--' + type : '' %> <%= dismissable ? 'toast--dismiss' : '' %>" title="<%= dismissable ? 'Click to dismiss' : '' %>">
            <p class="toast__text">
                <%= text %>
            </p>
            <% _.each(buttons, function(button, i) { %>
                <a class="toast__btn js-toast-btn" href="#" data-index="<%= i %>">
                    <%= button.text %>
                </a>
                <% }); %>
        </div>
    </script>
    <script src="js/underscore-min.js"></script>
    <script src="js/toast.js"></script>

    <script>
        $(function () {
            // init the validator
            // validator files are included in the download package
            // otherwise download from http://1000hz.github.io/bootstrap-validator
            $(window).on('load', function () {
                $(".submit-btn").removeClass('disabled');

            })


            $("#get-free-quote").validator({

            })

            $("#get-free-quote").on("submit", function (e) {
                // if the validator does not prevent form submit

                if (!e.isDefaultPrevented()) {

                    var url = "";

                    // FOR CODEPEN DEMO I WILL PROVIDE THE DEMO OUTPUT HERE, download the PHP files from
                    // https://bootstrapious.com/p/how-to-build-a-working-bootstrap-contact-form
                    var firstname = $("#form_name").val();
                    var lastname = $("#form_lastname").val();
                    var email = $("#email").val();
                    var phone = $("#form_phone").val();
                    var company = $("#form_company").val();
                    var message = $("#message").val();
                    if (firstname && lastname && email && message && company && phone && message) {
                        var data = {
                            firstname: firstname,
                            lastname: lastname,
                            phone: phone,
                            company: company,
                            email: email,
                            message: message
                        };
                        $.ajax({
                            type: "POST",
                            url: url,
                            dataType: "json",
                            data: JSON.stringify(data),
                            success: function (respons) {
                                $("#form_name").val('');
                                $("#form_lastname").val('');
                                $("#email").val('');
                                $("#form_phone").val('');
                                $("#form_company").val('');
                                $("#message").val('');
                                globalToast.add({
                                    type: ToastTypes.ERROR,
                                    text: 'Internal error occured. Please try some time.'
                                });
                                return;
                            },
                            error: function (xhr, ajaxOptions, thrownError) {
                                $("#form_name").val('');
                                $("#form_lastname").val('');
                                $("#email").val('');
                                $("#form_phone").val('');
                                $("#form_company").val('');
                                $("#message").val('');
                                $('#ignismyModal').modal('show');
                            }
                        });
                    }



                    return false;
                }
            });
        });

