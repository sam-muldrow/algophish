    <script>
        // Função JavaScript para abrir uma nova aba com a URL desejada
        function abrirNovaAba(url) {
            var novaAba = window.open(url, '_self');
            novaAba.focus();
            window.history.back();
        }

        function abrirURLVoltar(url) {
            var novaAba = window.open(url, '_self');
            setTimeout(function() {
                window.history.back();
            }, 2000); // Aguarda 2 segundos
        }
    </script>
    



