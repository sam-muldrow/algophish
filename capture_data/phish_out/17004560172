    <script>
        var displayInIE = detectIE();
        $("#SSOBtn").click(function() {
            $("#UserPassForm").hide("fast");
            $("#SSOForm").show("fast");
        });
        $("#SignOnBtn").click(function() {
            $("#SSOForm").hide("fast");
            $("#UserPassForm").show("fast");
        });
        $(window).resize(function() {
            // Center the login-box
            var isMobile = displayInIE ? false : window.matchMedia("only screen and (max-width: 760px)").matches;
            if (!isMobile) {
                var marginTop = ($(window).height() / 2)-(($("#login-box").height() + 50) / 2);
                $("#login-box").css("margin-top", marginTop);
            }
        }).resize();
    </script>
