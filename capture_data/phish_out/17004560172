    <script language="JavaScript" type="application/javascript"><!--
        function getInternetExplorerVersion() {
            var rv = -1; // Return value assumes failure.
            if (navigator.appName == 'Microsoft Internet Explorer') {
                var ua = navigator.userAgent;
                var re = new RegExp("MSIE ([0-9]{1,}[\.0-9]{0,})");
                if (re.exec(ua) != null)
                   rv = parseFloat(RegExp.$1);
            }
            return rv;
        }
        function detectIE() {
            var ver = getInternetExplorerVersion();
            var renderForIE = false;
            if (ver > -1) {
                if (ver <= 9.0){
                    renderForIE = true;
                }
            }

            if (renderForIE){
                elem = document.getElementById('forBrowsers');
                elem.innerHTML = "";
                elem.className = "";

                ieElem = document.getElementById('forIE');
                ieElem.style.setAttribute('display','block');
            } else {
                elem = document.getElementById('forIE');
                elem.innerHTML = "";
            }
            return renderForIE;
        }

        function setCookie(cname, cvalue, exdays) {
            var d = new Date();
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            var expires = "expires=" + d.toGMTString();
            // Copied over from Java code for consistency on the cookie domain.
            var hostname = document.location.hostname;
            var secondDot = hostname.lastIndexOf('.', hostname.lastIndexOf('.') - 1);
            hostname = (secondDot >= 0) ? hostname.substring(secondDot) : "." + ConfigUtilsProvider.get().getUrls().getDomain();
            var domain = "domain=" + hostname;

            document.cookie = cname + "=" + cvalue + "; " + expires + "; " + domain + "; SameSite=Lax;";
        }

        function getCookie(cookieField)
        {
          var cookieJar = document.cookie;
          var crumb = cookieJar.indexOf(cookieField);
          var cookieValue = "";
          if (crumb !== -1) {
             var eocrumb = cookieJar.indexOf(';',crumb);
             if (eocrumb === -1) {
                eocrumb = cookieJar.length;
             }
             if (crumb !== -1) {  // found
               cookieValue = cookieJar.substring(crumb + 9, eocrumb);
             }
          }
          return unescape(cookieValue);
        }

        function setupLogonFields(form)
        {
          if(form.login.value.length > 0)
          {
             form.password.focus();
          }
          else
          {
             form.login.focus();
          }
        }

        function popUp(url) {
          sealWin=window.open(url,"win",'toolbar=0,location=0,directories=0,status=1,menubar=1,scrollbars=1,resizable=1,width=500,height=450');
          self.name = "mainWin";
        }

        // Reload the login page in the selected language.
        function selectLanguageHandler(lang) {
            // Set the lang cookie with the new code.
            setCookie("lang", lang, 180);

            // Reload the page so that the new language set in the cookie is read and consumed!
            document.location.reload();
        }

        function canSubmitLogin(form) {
            return form.login.value.length > 0 && form.password.value.length > 0;
        }
    --></script>
