function nos2() {
  var a = document.getElementById("register2").value;
  document.getElementById("register2").value = a.replace(/[^a-zA-Z0-9\-.]/gim, '');
   return;
}
</script>
<script>
var input4 = document.getElementById('register2');
if(!input4){
} else {
   var activePlaceholder4 = new ActivePlaceholder(
   input4, 'eg. ',
   ['Absolute.ly', 'Trend.ly', 'Dream.ly'],
   1, 48, 24, 12);
   activePlaceholder4.start();
}
