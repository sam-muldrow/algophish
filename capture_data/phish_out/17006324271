var OLD_MARKETPLACE_CONFIGURATION_PARAMS = {
  referrer: '',
  referer: '',
  page_referrer: '',
  page_referer: '',
  cookie_prefix: '_mshops',
  linker: {
    name: 'destination_name',
    accept_incoming: true
  }
