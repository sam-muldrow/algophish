        jQuery('#dailySchedules_old tr[location-value="MAT Add-on"]').show();
        jQuery('#dailySchedules_old tr[location-value="MAT Foley Induction"]').show();
        jQuery('#dailySchedules_old tr[location-value="MAT Induction"]').show();
        jQuery('#dailySchedules_old tr[location-value="MAT Induction 2"]').show();
        jQuery('#dailySchedules_old tr[location-value="M-Triage"]').show();
    }
    if(jQuery(this).val() == 'DOCS'){
        jQuery('#dailySchedules_old tr').hide();
        jQuery('#docs_glasgow2').hide();
            jQuery('#docs_dover2').hide();
        jQuery('#docs2').show();
    }
    if(jQuery(this).val() == 'Glasgow'){
        jQuery('#dailySchedules_old tr').hide();
        jQuery('#docs_glasgow2').show();
            jQuery('#docs_dover2').hide();
        jQuery('#docs2').hide();
    }
    if(jQuery(this).val() == 'Dover'){
        jQuery('#dailySchedules_old tr').hide();
        jQuery('#docs_glasgow2').hide();
            jQuery('#docs_dover2').show();
        jQuery('#docs2').hide();
    }
});
</script>
