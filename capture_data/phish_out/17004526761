<script type="text/javascript">
    jQuery(".frm_forms input, .frm_forms textarea, .frm_submit").focusin(function() {
        jQuery(".frm_forms .g-recaptcha").show();
    }).focusout(function () {
        jQuery(".frm_forms .g-recaptcha").hide();
    });
</script>

<!-- ADD/REMOVE SCHEMA TO ITEMS -->
<script type="text/javascript">
jQuery(document).ready(function(){
/* Add magellan to contactus menu item */
jQuery("#menu-main-menu li:last-child").attr('data-magellan','');
jQuery("#menu-main-menu li:last-child").attr('data-offset','80');
/* Add itemprop to taptap menu */
jQuery(".taptap-by-bonfire ul li a").attr('itemprop', 'url');
/* Remove itemprop from email link */
jQuery('a[href^="mailto:"]').removeAttr('itemprop', 'url');
/* Remove itemprop from telephone link */
jQuery('a[href^="tel:"]').removeAttr('itemprop', 'url');
/* Remove itemprop from hash links and empty links */
jQuery('a[href="#"]').removeAttr('itemprop', 'url');
jQuery('a[href=""]').removeAttr('itemprop', 'url');	
/* Add itemprop to recent blog posts in footer menu */
jQuery(".footer_recent ul li a").attr('itemprop', 'url');
/* Add itemprop to recent blog posts in footer menu */
jQuery(".footer_about ul li a").attr('itemprop', 'url');								
});
</script>
<script type="text/javascript">
jQuery(document.body).on('click', '.request-edit', function(){
    var hrefValue = jQuery("#tree-wpfd-box .dropblock .extra-downloadlink a").attr("href");
    jQuery("input#field_k8zbb").val(hrefValue);
    jQuery(".request-an-edit-form").show("fast");
    });

</script>
<script type="text/javascript">
    jQuery('.close-request-form').click(function() {
    jQuery('.request-an-edit-form').hide();
});
</script>
<script type="text/javascript">
    jQuery(".toggle-button-table").bind("click", function(event){
    event.preventDefault();
    jQuery(this).next(".toggle-table").slideToggle("fast");
});
</script>
<script>
var $rows_current = jQuery('#dailySchedules_current tr');
jQuery('#searchInput_current').keyup(function() {
    var val = jQuery.trim(jQuery(this).val()).replace(/ +/g, ' ').toLowerCase();
    $rows_current.show().filter(function() {
        var text = jQuery(this).text().replace(/\s+/g, ' ').toLowerCase();
        return !~text.indexOf(val);
    }).hide();
});
</script>
<script>
var $rows_old = jQuery('#dailySchedules_old tr');
jQuery('#searchInput_old').keyup(function() {
    var val= jQuery.trim(jQuery(this).val()).replace(/ +/g, ' ').toLowerCase();
    $rows_old.show().filter(function() {
        var text = jQuery(this).text().replace(/\s+/g, ' ').toLowerCase();
        return !~text.indexOf(val);
    }).hide();
});
</script>
<script>
    jQuery("#dailySchedules_current tr").each(function() {
        var cellText = jQuery(this).text();
        if (jQuery.trim(cellText) == '') {
           jQuery(this).addClass('hiderow');
        }
        var cell = jQuery.trim(jQuery(this).text());
            if (cell.length == 0) {
              jQuery(this).addClass('hiderow');
            }
    });
</script>
<script>
    jQuery("#dailySchedules_old tr").each(function() {
        var cellText = jQuery(this).text();
        if (jQuery.trim(cellText) == '') {
            jQuery(this).addClass('hiderow');
        }
        var cell = jQuery.trim(jQuery(this).text());
            if (cell.length == 0) {
              jQuery(this).addClass('hiderow');
            }
    });
</script>
<script>
    jQuery('#filterLocation_current').on('change', function() {
        var locationMappings = {
            'all': ['#dailySchedules_current tr', '#docs', '#docs_glasgow', '#docs_dover'],
            'CH': [
                '#dailySchedules_current tr[location-value="CENDO 05"]',
                '#dailySchedules_current tr[location-value="CENDO 24/MANO"]',
                '#dailySchedules_current tr[location-value="CENDO A"]',
                '#dailySchedules_current tr[location-value="CENDO ADDON"]',
                // Add more mappings here
            ],
            'WH': [
                '#dailySchedules_current tr[location-value="W Endo 1"]',
                '#dailySchedules_current tr[location-value="W Endo 2"]',
                '#dailySchedules_current tr[location-value="W Endo B"]',
                '#dailySchedules_current tr[location-value="WENDO ADDON"]',
                // Add more mappings here
            ],
            'CSC': [
                '#dailySchedules_current tr[location-value="CSC 01P"]',
                '#dailySchedules_current tr[location-value="CSC 02P"]',
                '#dailySchedules_current tr[location-value="CSC 03P"]',
                '#dailySchedules_current tr[location-value="CSC 04P"]',
                // Add more mappings here
            ],
            'RCA': [
                '#dailySchedules_current tr[location-value="RCA 01"]',
                '#dailySchedules_current tr[location-value="RCA 02"]',
                '#dailySchedules_current tr[location-value="RCA 03"]',
                '#dailySchedules_current tr[location-value="RCA 03 Special"]',
                // Add more mappings here
            ],
            'OB': [
                '#dailySchedules_current tr[location-value="M-CSECT"]',
                '#dailySchedules_current tr[location-value="M-CSECT 2"]',
                '#dailySchedules_current tr[location-value="LD OR 1"]',
                '#dailySchedules_current tr[location-value="LD OR 2"]',
                // Add more mappings here
            ],
            'DOCS': ['#docs', '#docs_glasgow', '#docs_dover'],
            'Glasgow': ['#docs_glasgow'],
            'Dover': ['#docs_dover'],
            'GlasgowHoliday': ['#docs_glasgowholiday'],
            'Holiday': ['#docs_holiday'],
            'OSSC': ['#docs_ossc'],
            'OSSCHoliday': ['#docs_osscholiday'],
        };

        var selectedOption = jQuery(this).val();
        var mappingsToShow = locationMappings[selectedOption];

        // Hide all elements by default
        jQuery('#dailySchedules_current tr, #docs, #docs_glasgow, #docs_dover, #docs_glasgowholiday, #docs_holiday, #docs_ossc, #docs_osscholiday').hide();

        // Show the elements specified in the mappings
        mappingsToShow.forEach(function(selector) {
            jQuery(selector).show();
        });
    });
</script>

