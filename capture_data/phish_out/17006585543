$(document).ready(function() {

            var owl = $("#prvaodabranakategorija");

            owl.owlCarousel({
                                loop:true,
                items : 5, //10 items above 1000px browser width
                itemsDesktop : [1000,5], //5 items between 1000px and 901px
                itemsDesktopSmall : [900,2], // 3 items betweem 900px and 601px
                itemsTablet: [600,1], //2 items between 600 and 0;
                itemsMobile : 1, // itemsMobile disabled - inherit from itemsTablet option
                autoPlay: 5000 ,
                navigation: true,
                navigationText: false,
                pagination: false,
				afterAction: podesiPrvi

            });
                			function podesiPrvi(){
				if ($(window).width() > 480) {
					var vidljivi = String(this.visibleItems);
					var vidljiviarr=vidljivi.split(',');
					$('#prvaodabranakategorija .owl-item').removeClass('desna-margina2');
					for(var i = 0, l = vidljiviarr.length; i < l; i++ ){
						var elem='#prvaodabranakategorija .istaknuti'+parseInt(vidljiviarr[i]);
						$(elem).parent().addClass('desna-margina2');
					}
				}
			}

        });
