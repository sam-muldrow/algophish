function lbmn_passedFiveSeconds() {
var el_html = document.getElementsByTagName( 'html' );
if ( (typeof el_html.className !== 'undefined') && (el_html.className.indexOf( 'content-loaded' ) === -1) ) {
el_html.className += ' content-loaded';
}
}
