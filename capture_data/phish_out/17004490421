		<script>
	
			jQuery(document).ready(function() { 
				
				setTimeout(function () {
                    jQuery(".mphb_room_type_check_in_datepicker").val('');
                jQuery(".mphb_datepicker").val('');
                 }, 2000);
				
				jQuery("#hear_from_us").change(function(){
					if(jQuery(this).val()=='Other'){
						jQuery("#other_hear_from_us_field").removeClass('hide');
						jQuery("#other_hear_from_us").attr('required','required');
					}else{
						jQuery("#other_hear_from_us_field").addClass('hide');
						jQuery("#other_hear_from_us").removeAttr('required');
					} 
				});
			
				jQuery(".mphb-children-chooser select").on('change',function(){
					jQuery(this).closest(".guests_container").find(".mphb-adults-chooser select").trigger('change');
				});
				
				if( jQuery("body").hasClass('page-id-3890') && jQuery("#customise_checkout_field").html()!=undefined ) {
					
					jQuery( window ).scroll(function() {
						
						var scroll = jQuery(window).scrollTop();
						
						if( jQuery(this).scrollTop() > 300 ) {
							restrict = 1700;
							if( jQuery(this).scrollTop() < restrict ) { 
								jQuery("#order_review").css( 'top', (jQuery(this).scrollTop()-100) );
							}
						}else{
							jQuery("#order_review").css( 'top', 70 );
						}
						
						
					});
					
				}
				
				jQuery(".mphb_sc_checkout-guests-chooser").change(function(){
					
					jQuery(this).closest(".guests_container").find(".extra_guest_room").remove();
					if( jQuery(this).val() > 1 ) {
						var loopEnd  = jQuery(this).val();
						var position = jQuery(this).closest(".guests_container").index();
						
						for(var i=1; i < loopEnd; i++ ) {
							jQuery(this).closest(".guests_container").append('<input placeholder="Guest Name '+(i+1)+'" type="text" name="mphb_room_details['+parseInt(jQuery(this).closest('.mphb-room-details').attr('data-index'))+'][extra_guest_name]['+i+']" id="mphb_room_details-'+position+'-guest-name['+i+']" class="extra_guest_room"/>');
						}
					}
				});
				
				jQuery(".mphb-reserve-btn.button").click(function(){
					
					var interval = setInterval( function(){ 
						if( jQuery(".button.mphb-button.mphb-confirm-reservation").val()!=undefined ) {
							jQuery(".custom_dates_confirmed").html('<strong> These dates  '+jQuery(".mphb-check-in-date-wrapper input[type='text']").val()+' - '+jQuery(".mphb-check-out-date-wrapper input[type='text']").val()+' are available. Would You like to proceed with your booking?</strong>');
							clearInterval(interval);
						}
					},1000);
					
				});
				
				jQuery("#content").on("mouseover",function(){
					jQuery(".tooltipsy.white").text("prices without taxes");
				});
				
				var interval2 = setInterval(function(){
					if( jQuery(".tooltipsy.white").text()!='' ) {
						jQuery(".tooltipsy.white").text("prices without taxes");
						clearInterval(interval2);
					}
				},100);
				
				jQuery(".mphb-room-type-images").attr("style","display:block !important;");
				
								
							});
	
		</script>
	
		
