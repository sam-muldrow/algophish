			jQuery(".mphb-room-rate-variant input[type='radio']").click(function(){
				
				/* alert(jQuery(this).val()+" === "+memberRates.indexOf(jQuery(this).val())+" ==== "+memberRates); */
				var text = jQuery.trim(jQuery(this).closest('.mphb-room-rate-variant').text());
				/* if ( jQuery.inArray(jQuery(this).val(),memberRates) >= 0 ) { */
				if ( jQuery.trim(jQuery(this).closest('.mphb-room-rate-variant').text()).indexOf('Non Members') >= 0 ) {
					
					jQuery(".select_membership").hide();
					
					jQuery("select[name='membership_name']").removeAttr('required','required');
					jQuery("input[name='member_ship_number']").removeAttr('required','required');
					
					jQuery("select[name='membership_name']").val('');
					jQuery("input[name='member_ship_number']").val('');
					
				}else{
					
					jQuery(".select_membership").show();
					
					jQuery("select[name='membership_name']").attr('required','required');
					jQuery("input[name='member_ship_number']").attr('required','required');
					
				}
			});
			
			jQuery('.mphb-terms-and-conditions-link').on('click',function(){
				window.open(jQuery(this).attr('href'),'_blank');
			});
			
			jQuery('select[name="membership_name"]').change(function(){
				if( jQuery(this).val()=='other' ){
					jQuery("input[name='other_membership_name']").show();
				}else{
					jQuery("input[name='other_membership_name']").val('');
					jQuery("input[name='other_membership_name']").hide();
				}
			});
			
			Date.prototype.addDays = function(days) {
				var date = new Date(this.valueOf());
				date.setDate(date.getDate() + days);
				return date;
			};

			setTimeout(function(){
			
				var date  = new Date();
				
				var currentMonth = date.getMonth();
					currentMonth++;
					
				if(currentMonth<10) currentMonth = '0'+currentMonth;
					
				var currentDay   = (date.getDate()+1);
				if(currentDay<10) currentDay = '0'+currentDay;
					
				var currentDate  = currentMonth+'/'+currentDay+'/'+date.getFullYear();
				
				var futureDate   =  date.addDays(3);
				var futureMonth  =  futureDate.getMonth();
					futureMonth++;
				var checkoutDate =  futureMonth+'/'+(futureDate.getDate()+1)+'/'+futureDate.getFullYear();
				
				/* var checkoutDate = "11/24/2023"; */
				
				jQuery("body.home .mphb_sc_search-check-in-date input[name='mphb_check_in_date']").val(currentDate);
				
				jQuery("#availability_search-3 .mphb_sc_search-check-in-date input[name='mphb_check_in_date']").val(currentDate);
				
				jQuery(".mphb_room_type_check_in_datepicker").val(currentDate);
				jQuery(".mphb-check-out-date-wrapper>input[type='text']").val(checkoutDate);
				
				//jQuery("body.home .mphb_sc_search-check-out-date input[name='mphb_check_out_date']").val(checkoutDate);
				
				
								
					jQuery(".mphb_widget_search-form #mphb_check_in_date-availability_search-1").val(currentDate);
					
					jQuery(".mphb_widget_search-form #mphb_check_out_date-availability_search-1").val(checkoutDate);
					
								
				/** hidden fields **/
				var hiddenCurrentDate  = "2023-11-21";
				var hiddenCheckoutDate = "2023-11-24";
				
				jQuery("body.home .mphb_sc_search-form>input[name='mphb_check_in_date']").val(hiddenCurrentDate);
				jQuery("body.home .mphb_sc_search-form>input[name='mphb_check_out_date']").val(hiddenCheckoutDate);
				
				jQuery(".mphb-booking-form input[name='mphb_check_in_date']").val(hiddenCurrentDate);
				jQuery(".mphb-booking-form input[name='mphb_check_out_date']").val(hiddenCheckoutDate);
				
				jQuery(".mphb_sc_search-form input[type='hidden'][name='mphb_check_in_date']").val(hiddenCurrentDate);
				jQuery(".mphb_sc_search-form input[type='hidden'][name='mphb_check_out_date']").val(hiddenCheckoutDate);
				
				/** hidden fields **/
				
									jQuery(".mphb_widget_search-form #mphb_check_in_date-availability_search-1-hidden").val(hiddenCurrentDate);
					jQuery(".mphb_widget_search-form #mphb_check_out_date-availability_search-1-hidden").val(hiddenCheckoutDate);
							
			},1500); 
			
			jQuery(".rpt_plan a").click(function(){
				
				if( jQuery(this).hasClass("rpt_foot_0") ){
					jQuery("#input_9_14").val("Beach - $30.00");
				}else if( jQuery(this).hasClass("rpt_foot_1") ){
					jQuery("#input_9_14").val("Sunset - $45.00");
				}else if( jQuery(this).hasClass("rpt_foot_2") ){
					jQuery("#input_9_14").val("Sand - $60.00");
				}else if( jQuery(this).hasClass("rpt_foot_3") ){
					jQuery("#input_9_14").val("Moonlight - $65.00");
				}else if( jQuery(this).hasClass("rpt_foot_4") ){
					jQuery("#input_9_14").val("Breeze - $80.00");
				}
				
				jQuery('html, body').animate({ scrollTop: jQuery("#section2").offset().top - 100 }, 800, function(){
				});
				
				jQuery(".rpt_plan a").removeClass('selected');
				jQuery(this).addClass('selected');
			});
			
			jQuery("body, html").on("mouseover","#mphb_check_in_date-availability_search-1",function(){
				jQuery(this).on('change',function(){
					jQuery("#mphb_check_out_date-availability_search-1").val("");
				});
				
			});
			
			jQuery("#input_9_14").change(function(){
				
				jQuery(".rpt_plan a").removeClass('selected');
				
				if( jQuery(this).val() == "Beach - $30.00" ){
					jQuery(".rpt_foot_0").addClass('selected');
				}else if( jQuery(this).val() == "Sunset - $45.00" ){
					jQuery(".rpt_foot_1").addClass('selected');
				}else if( jQuery(this).val() == "Sand - $60.00" ){
					jQuery(".rpt_foot_2").addClass('selected');
				}else if( jQuery(this).val() == "Moonlight - $65.00" ){
					jQuery(".rpt_foot_3").addClass('selected');
				}else if( jQuery(this).val() == "Breeze - $80.00" ){
					jQuery(".rpt_foot_4").addClass('selected');
				}
				
				calculate_prices();
			});
			
			jQuery("input[name='input_9']").click(function(){
				calculate_prices();
			});
			
						
						
			var roomList = {"144":5,"145":5,"161":5,"162":5,"201":5,"202":5,"203":5,"204":5,"205":5,"206":5,"207":5,"208":5,"244":5,"245":5,"261":5,"262":5,"301":5,"302":5,"303":5,"304":5,"305":5,"306":5,"307":5,"308":5,"401":5,"402":5,"403":5,"404":5,"405":5,"406":5,"407":5,"408":5,"143":6,"146":6,"160":6,"163":6,"209":6,"222":6,"243":6,"246":6,"260":6,"263":6,"110":4,"111":4,"112":4,"113":4,"114":4,"115":4,"116":4,"117":4,"118":4,"119":4,"120":4,"121":4,"122":4,"123":4,"124":4,"125":4,"126":4,"127":4,"128":4,"129":4,"130":4,"131":4,"132":4,"133":4,"134":4,"135":4,"136":4,"137":4,"138":4,"139":4,"140":4,"141":4,"142":4,"147":4,"148":4,"149":4,"150":4,"151":4,"152":4,"153":4,"154":4,"155":4,"156":4,"157":4,"158":4,"159":4,"210":4,"212":4,"213":4,"214":4,"215":4,"216":4,"217":4,"218":4,"219":4,"220":4,"221":4,"223":4,"225":4,"226":4,"227":4,"228":4,"229":4,"230":4,"231":4,"232":4,"233":4,"234":4,"235":4,"236":4,"237":4,"238":4,"239":4,"240":4,"241":4,"242":4,"247":4,"248":4,"249":4,"250":4,"251":4,"252":4,"253":4,"254":4,"255":4,"256":4,"257":4,"258":4,"259":4};
			
			jQuery("#input_14_51").change(function(){
				var roomNumber = roomList[jQuery(this).val()];
				if(roomList[jQuery(this).val()]!=undefined){
					jQuery("#input_14_27").val(roomList[jQuery(this).val()]);
				}else{
					jQuery("#input_14_27").val("");
				}
			});
			
		});
		
		function calculate_prices() {
			
			if( (jQuery("input[name='input_9']:checked").val()!=undefined) && (jQuery("#input_9_14").val()!=undefined) ) {
				
				var pricePart = jQuery("#input_9_14").val().split('-');
			    var prices    = parseFloat(pricePart[1].replace('$',''));												  
			
				var finalPrices = parseFloat(prices)*parseFloat(jQuery("input[name='input_9']:checked").val());
				    finalPrices = parseFloat(finalPrices)+parseFloat(finalPrices*(.06));
				
				jQuery("#input_9_11").val("$"+finalPrices.toFixed(2));
			}
			return false;
		}
    </script>	  
<script>
	jQuery( ".tooltipsy.white" ).hover(
	  function() {
		jQuery(".tooltipsy.white").append( jQuery( "<p>Price excludes taxes and surcharges</p>" ) );
	  }, function() {
	   jQuery(".tooltipsy.white").find( "Choose dates to see relevant prices" ).last().remove();
	  }
	); 
	
	setTimeout(function(){
		jQuery(".tooltipsy.white").html("<p>Price excludes taxes and surcharges</p>");
	},3000);
 </script>
