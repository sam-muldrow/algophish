<script type="rocketlazyloadscript">
  document.addEventListener("DOMContentLoaded", function () {
    // Function to check if the page is in Italian or Spanish translation
    function isItalianOrSpanish() {
      var italianElement = document.querySelector('[lang="it-IT"]');
      var spanishElement = document.querySelector('[lang="es-ES"]');
      return italianElement || spanishElement;
    }

    // Check if the page is in Italian or Spanish translation
    if (isItalianOrSpanish()) {
      // Hide the elements for Italian and Spanish
      document.getElementById("my-element").style.display = "none";
      var otherElements = document.querySelectorAll(".other-element");
      otherElements.forEach(function (element) {
        element.style.display = "none";
      });
    } else {
      // Show the elements for other languages
      document.getElementById("my-element").style.display = "block";
      var otherElements = document.querySelectorAll(".other-element");
      otherElements.forEach(function (element) {
        element.style.display = "block";
      });
    }
  });
</script>

<script type="rocketlazyloadscript">
    document.addEventListener("DOMContentLoaded", function () {
        // Function to check if the page is in Italian or Spanish translation
        function isItalianOrSpanish() {
            var italianElement = document.querySelector('[lang="it-IT"]')
            var spanishElement = document.querySelector('[lang="es-ES"]')
            var englishElement = document.querySelector('[lang="en-GB"]')
            var germanElement = document.querySelector('[lang="de-DE"]')
            var frenchElement = document.querySelector('[lang="fr-FR"]')
            var swedishElement = document.querySelector('[lang="sv-SE"]')
            var dutchElement = document.querySelector('[lang="nl-NL"]')
            var polishElement = document.querySelector('[lang="pl-PL"]')
            var finishElement = document.querySelector('[lang="fi"]')
            var brasilianElement = document.querySelector('[lang="pt-BR"]')
            return italianElement || spanishElement || englishElement || germanElement || frenchElement || swedishElement || dutchElement || polishElement || finishElement || brasilianElement
        }

        // Check if the page is in Italian or Spanish translation
        if (isItalianOrSpanish()) {
            // Hide the elements for Italian and Spanish
            document.getElementById("showApac").style.display = "none"
            var otherElements = document.querySelectorAll(".other-element")
            otherElements.forEach(function (element) {
                element.style.display = "none"
            })
        } else {
            // Show the elements for other languages
            document.getElementById("showApac").style.display = "block"
            var otherElements = document.querySelectorAll(".other-element")
            otherElements.forEach(function (element) {
                element.style.display = "block"
            })
        }
    })
</script>
        <script type="rocketlazyloadscript">
        document.addEventListener("DOMContentLoaded", function () {
            // Function to check if the page is in Italian or Spanish translation
            function isItalianOrSpanish() {
                var italianElement = document.querySelector('[lang="it-IT"]')
                var spanishElement = document.querySelector('[lang="es-ES"]')
                var englishElement = document.querySelector('[lang="en-GB"]')
                var germanElement = document.querySelector('[lang="de-DE"]')
                var frenchElement = document.querySelector('[lang="fr-FR"]')
                var swedishElement = document.querySelector('[lang="sv-SE"]')
                var dutchElement = document.querySelector('[lang="nl-NL"]')
                var polishElement = document.querySelector('[lang="pl-PL"]')
                var finishElement = document.querySelector('[lang="fi"]')
                var brasilianElement = document.querySelector('[lang="pt-BR"]')
                return italianElement || spanishElement || englishElement || germanElement || frenchElement || swedishElement || dutchElement || polishElement || finishElement || brasilianElement
            }

            // Check if the page is in Italian or Spanish translation
            if (isItalianOrSpanish()) {
                // Hide the elements for Italian and Spanish
                document.getElementById("newApac").style.display = "none"
                var otherElements = document.querySelectorAll(".other-element")
                otherElements.forEach(function (element) {
                    element.style.display = "none"
                })
            } else {
                // new the elements for other languages
                document.getElementById("newApac").style.display = "block"
                var otherElements = document.querySelectorAll(".other-element")
                otherElements.forEach(function (element) {
                    element.style.display = "block"
                })
            }
        })
    </script>
    <script type="rocketlazyloadscript">
        document.addEventListener("DOMContentLoaded", function () {
            // Function to check if the page is in Italian or Spanish translation
            function isItalianOrSpanish() {
                var singaporeElement = document.querySelector('[lang="en-SG"]')
                var africaElement = document.querySelector('[lang="en-ZA"]')
                var australiaElement = document.querySelector('[lang="en-AU"]')
                var zealandElement = document.querySelector('[lang="en-NZ"]')
                var thaiElement = document.querySelector('[lang="th"]')
                var honkElement = document.querySelector('[lang="zh-HK"]')
                var taiwanElement = document.querySelector('[lang="zh-TW"]')
                var indonesiaElement = document.querySelector('[lang="en-ID"]')
                var malyasiaElement = document.querySelector('[lang="en-MY"]')
                var philippinesElement = document.querySelector('[lang="en-PH"]')

                return singaporeElement || africaElement || australiaElement || zealandElement || thaiElement || honkElement || taiwanElement || indonesiaElement || malyasiaElement || philippinesElement
            }

            // Check if the page is in Italian or Spanish translation
            if (isItalianOrSpanish()) {
                // Hide the elements for Italian and Spanish
                document.getElementById("hideApac").style.display = "none"
                var otherElements = document.querySelectorAll(".other-element")
                otherElements.forEach(function (element) {
                    element.style.display = "none"
                })
            } else {
                // Show the elements for other languages
                document.getElementById("hideApac").style.display = "block"
                var otherElements = document.querySelectorAll(".other-element")
                otherElements.forEach(function (element) {
                    element.style.display = "block"
                })
            }
        })
    </script><style id="wpr-lazyload-bg"></style><style id="wpr-lazyload-bg-exclusion"></style>
