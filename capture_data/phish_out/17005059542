			<img onclick = "mod_flashShare('g',document.getElementById('mod_current_playlist_table').value,document.getElementById('mod_current_track').value)" style="cursor:pointer" src="/components/com_spidervideoplayer/views/spidervideoplayer/images/googleplus.png" /><br>
			<img onclick="jQuery('#embed_Url_div').css('display','');embed_url(document.getElementById('mod_current_playlist_table').value,document.getElementById('mod_current_track').value)" style="cursor:pointer" src="/components/com_spidervideoplayer/views/spidervideoplayer/images/embed.png" />
