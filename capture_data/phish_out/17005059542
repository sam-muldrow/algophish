	<script type="text/javascript">
	function mod_flashShare(type,b,c){
		mod_u = location.href;
		if(mod_u=='https://calambamisocc.gov.ph/index.php?' || mod_u=='https://calambamisocc.gov.ph/' )
			mod_u='https://calambamisocc.gov.ph/index.php?';
		else if(!location.search)
			mod_u=mod_u+'?';
		else
			mod_u=mod_u+'&';

		t = document.title;
		switch (type) {
			case 'fb':	
				window.open('http://www.facebook.com/sharer.php?mod_u='+encodeURIComponent(mod_u+'AlbumId='+b+'&TrackId='+c)+'&t='+encodeURIComponent(t), "Facebook","menubar=1,resizable=1,width=350,height=250");
			break;
			
			case 'g':
				window.open('http://plus.google.com/share?url='+encodeURIComponent(mod_u+'AlbumId='+b+'&TrackId='+c)+'&t='+encodeURIComponent(t), "Google","menubar=1,resizable=1,width=350,height=250");
			break;

			case 'tw':
				window.open('http://twitter.com/home/?status='+encodeURIComponent(mod_u+'&AlbumId='+b+'&TrackId='+c), "Twitter","menubar=1,resizable=1,width=350,height=250");
			break;
		}
	}	
	var mod_so = new SWFObject("https://calambamisocc.gov.ph/components/com_spidervideoplayer/videoPlayer.swf?wdrand=47263641", "Player", "100%", "100%", "8", "#000000");

	mod_so.addParam("FlashVars", "settingsUrl=/index.php/component/spidervideoplayer/?view=settings@format=row@typeselect=1@video=@theme=1&playlistUrl=/index.php/component/spidervideoplayer/?view=singlevideo@format=row@trackID=@show_trackid=1&defaultAlbumId=&defaultTrackId=");
	mod_so.addParam("quality", "high");
	mod_so.addParam("menu", "false");
	mod_so.addParam("wmode", "transparent");
	mod_so.addParam("loop", "false");
	mod_so.addParam("allowfullscreen", "true");
	mod_so.write("flashcontent167");
	</script>

