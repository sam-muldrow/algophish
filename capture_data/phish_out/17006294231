  @media (min-width: 1024px) { section.insurance.pinstripe div.row { display: grid !important; grid-template-columns: repeat(auto-fit, minmax(0, 1fr)); align-items: center; justify-content: center; width: 100% !important; max-width: 1380px; margin: 0 auto !important; } section.insurance.pinstripe div.row::after { display: none !important; } section.insurance.pinstripe div.span6.left div.inner { display: grid; gap: 1.4em; padding: 1em !important; } section.insurance.pinstripe div.span6.left div.inner div { grid-column: 2; } section.insurance div.span6 { float: none !important; width: 100% !important; } section.insurance.pinstripe div.span6.right { display: grid; place-items: center; } }
section.service .btn-book-online { margin: 50px auto 30px; display: none !important; }

.meet.pinstripe {
background-color: #FFFFFF !important;
}

