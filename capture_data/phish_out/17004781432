        $(document).ajaxError(function (event, jqxhr, settings, thrownError) {
            if (jqxhr.responseText != undefined) {
                var response = jqxhr.responseText.match(/.*Description([\s\S]*)<br><br>/);
                console.log("\nError requesting " + settings.url);
                console.log("\nError Code: " + jqxhr.status + ' ' + thrownError);
                console.log("\nMessage: \n" + response);
            }
        });

