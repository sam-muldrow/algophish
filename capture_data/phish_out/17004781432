		<script type="text/javascript">
			$(function() {
				var $userName = $('#Email');

				$userName.bind('blur', function() {
					if ($userName.val().indexOf('pradco.com') > 0) {
						window.location = 'https://welcome.pradco.com/account/DirectoryLogin';
					}
				});

			    if ('False' === 'True') {
			        var domainLabel = 'PRADCO';
			        var supportDomain = 'support@pradco.com';
			        var msg = GDPRText =
                        "<p>In the course of using " + domainLabel + "\'s online services, you may be asked to provide information " +
                        "that is personal and sensitive. " + domainLabel + " retains this information and uses it to " +
                        "develop a personal development assessment on behalf of the requesting organization.</p>" +
                        "<p>You may request a copy of your development assessment(s) by contacting " +
                        "<a href='" + supportDomain + "'>" + supportDomain + "</a>. Please indicate that this is a GDPR data request.</p>" +
                        "<p>If you agree to authorize " + domainLabel + " to collect and retain this information, please click \"I Agree\" below.</p>" +
			            "<p>You may revoke this authorization at any time by clicking the GDPR link on the login page, or " +
			            "navigating to <a href='" + '/GDPR/OptOut' + "'>" + 'https://beta.pradco.com/GDPR/OptOut'
                        + "</a>.<p>Thank you,<br />" + domainLabel + "</p>";

			        new BootstrapDialog({
			            title: 'GDPR Notice',
			            message: msg,
			            draggable: true,
			            closable: false,
			            buttons: [{
			                label: 'I Refuse',
			                action: function (dialog) {
			                    dialog.close();
			                }
			            }, {
			                label: 'I Agree',
			                action: function (dialog) {
			                    $('#GDPROptIn').val(true);
			                    $('form').submit();
			                }
			            }]
			        }).open();
			    }

			    $(window).load(function () {
			        $('#Timezone').val(moment.tz.guess());
			    });
			});
		</script>



