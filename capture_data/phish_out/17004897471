<script>
  // Function that loads recaptcha on form input focus
  function reCaptchaOnFocus() {
    let head = document.getElementsByTagName('head')[0];
    let script = document.createElement('script');
    script.type = 'text/javascript';
    script.src = 'https://www.google.com/recaptcha/api.js?render=6LdoXwYgAAAAAFwD1j0L3o67fY06rN9svYob9r0Z'
    head.appendChild(script);
    // remove focus to avoid js error:
    document.getElementById('newsletter-email')?.removeEventListener('focus', reCaptchaOnFocus);
    document.getElementById('newsletter-forename')?.removeEventListener('focus', reCaptchaOnFocus);
    document.getElementById('newsletter-surname')?.removeEventListener('focus', reCaptchaOnFocus);
    document.querySelector('#contact-form [name=name]')?.removeEventListener('focus', reCaptchaOnFocus);
    document.querySelector('#contact-form [name=surname]')?.removeEventListener('focus', reCaptchaOnFocus);
  };
  // add initial event listener to the form inputs
  document.getElementById('newsletter-forename')?.removeEventListener('focus', reCaptchaOnFocus);
  document.getElementById('newsletter-surname')?.removeEventListener('focus', reCaptchaOnFocus);
  document.getElementById('newsletter-email')?.addEventListener('focus', reCaptchaOnFocus, false);
  document.querySelector('#contact-form [name=name]')?.addEventListener('focus', reCaptchaOnFocus, false);
  document.querySelector('#contact-form [name=surname]')?.addEventListener('focus', reCaptchaOnFocus, false);
</script>

