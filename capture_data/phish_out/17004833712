<script>
var newsletterSent = 0;
function ajax_newsletter_signup51029(){

if(newsletterSent != 1){

	newsletterSent = 1;

    jQuery.ajax({
        type: "POST",
        url: 'https://www.biotrax.com/',		
		dataType: 'json',
		data: {
            action: "newsletter_join",
			email: jQuery('#ppt_newsletter_mailme51029').val(),	 
        },
        success: function(r) {
			
			if(r.status == "ok"){
				jQuery('#newsletterthankyou51029').show();
				jQuery('#mailinglist-form51029').html('');
			}else{
				jQuery('#mailinglist-form51029').html("Invalid Email Address");
			}
			
        },
        error: function(e) {
            //console.log(e)
        }
    });
	
	}

}
</script>

