			function shadowBoxClick(s,event){
				event.stopPropagation();
				if ((" " + s.className + " ").replace(/[\n\t]/g, " ").indexOf(" closed ") > -1 ){
					document.getElementsByClassName("shadowBox")[0].classList.remove( 'closed' );
					document.getElementsByClassName("shadowBox")[0].classList.add( 'opened' );
					document.getElementsByClassName("navbar-toggle")[0].classList.remove( 'closed' );
					document.getElementsByClassName("navbar-toggle")[0].classList.add( 'opened' );
					document.getElementsByClassName("navbar-collapse")[0].classList.add( 'in' );
				} else {
					document.getElementsByClassName("shadowBox")[0].classList.remove( 'opened' );
					document.getElementsByClassName("shadowBox")[0].classList.add( 'closed' );
					document.getElementsByClassName("navbar-toggle")[0].classList.remove( 'opened' );
					document.getElementsByClassName("navbar-toggle")[0].classList.add( 'closed' );
					document.getElementsByClassName("navbar-collapse")[0].classList.remove( 'in' );
				}
				return false;
			}
		</script>
