          } else if ($(e).hasClass('fh-fixed--bottom')) {
            type = 'fixed-bottom';
          } else if ($(e).hasClass('fh-fixed--top')) {
            type = 'fixed-top';
          } else if ($(e).hasClass('fh-fixed--side')) {
            type = 'fixed-side';
          } else {
            type = 'generic-lightframe-link';
          }

