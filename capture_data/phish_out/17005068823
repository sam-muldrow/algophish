  <script type="text/javascript">
    var d = new Date();
    var dateFormated = d.getDate() + "/" + (d.getMonth()+1) + "/" + d.getFullYear();

    $("#btnPDF").click(function(){
      $("#tipo").val("pdf");
      $("#titulo").val($(".page-header").html());
      if ($(".field-name-body").html() !== undefined && $(".field-name-body").html() !== null) {
        $("#texto").val($(".field-name-body").html());
      } else if ($(".table-responsive").html() !== undefined && $(".table-responsive").html() !== null) {
        $("#texto").val($(".table-responsive").html());
      } else if($("#page-content").html() !== undefined && $("#page-content").html() !== null) {
        $("#texto").val($("#page-content").html());
      }
      $("#url").val($(location).attr('href'));
      $("#dataexport").val(dateFormated);
    });  
    
    $("#btnCSV").click(function(){
      $("#tipo").val("csv");
      $("#titulo").val($(".page-header").html());
      if ($(".field-name-body").html() !== undefined && $(".field-name-body").html() !== null) {
        $("#texto").val($(".field-name-body").html());
      } else if ($(".table-responsive").html() !== undefined && $(".table-responsive").html() !== null) {
        $("#texto").val($(".table-responsive").html());
      } else if($("#page-content").html() !== undefined && $("#page-content").html() !== null) {
        $("#texto").val($("#page-content").html());
      }
      $("#url").val($(location).attr('href'));
      $("#dataexport").val(dateFormated);
    }); 
	
    (function($) {
       $.cookieBubble({
           messageText:"Utilizamos cookies e tecnologias semelhantes em nosso site. Ao prosseguir com a utilização do nosso site, você concorda com o uso de cookies, conforme estabelecido nesta política.",
	       buttonText:'Prosseguir',
	       messageFontSize:'12px',
	       iconColor:'rgba(19, 154, 241, 1)',
	       buttonColor:'rgba(19, 154, 241, 1)',
	       iconVisibility:true,
	       cookiePolicyButtonText:'Política de Privacidade',
	       cookiePolicyButtonUrl:'/politica',
	       cookiePolicyButtonTarget:'_blank',
	       boxPosition:'bottom-center'
       });
   })(jQuery);
   
  </script>
