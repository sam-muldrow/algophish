<script>
$(window).scroll(function(){
     var scroll = $(window).scrollTop();
    if(scroll > 100){
        $('.champ__header_div').addClass('active');
        $('.champ__header_logo .__white').hide();
        $('.champ__header_logo .__black').show();
    }
    else {
      $('.champ__header_div').removeClass('active');
        $('.champ__header_logo .__white').show();
        $('.champ__header_logo .__black').hide();
    }
  });
</script>

