	function get_id(id_data)
	{

//alert(id_data);
jQuery.ajax({
	type:"POST",
	url: "https://championshipquest.com/wp-admin/admin-ajax.php",
	data : {action: "ajax_home_video", post_id : id_data},
	success:function(data)
	{
		$('.modal_dy_'+id_data).html(data);
//$('.modal_dy_'+id_data)
$('.modal_dy_'+id_data).modal('show');
///// End output
}
});

}

