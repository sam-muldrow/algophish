<section id="FAQ" class="mbr-section accordion1" style="background-color: rgb(255, 255, 255); padding-top: 0px; padding-bottom: 100px;">


        <div class="container">
            <div class="row">
                <div class="col-md-8 col-md-offset-2">
                    <div class="section-head text-center">
                        <h1 class="mbr-section-title display-2 text-center mbr-editable-content">FAQ - ASQ CQE</h1>
                    </div>
                    <div class="clearfix"></div>
                    <div id="accordion1-2-init" class="panel-group accordionStyles accordion" role="tablist" aria-multiselectable="true">
                      <div class="accordion-group">


                        

                        <div class="panel panel-default colorBG" style="display: block;">
                            <div class="panel-heading" role="tab" id="heading_1">
