        $(document).ready(function() {
        
    $.supersized({

		// Functionality
		slideshow               :   0,			// Slideshow on/off
		autoplay				:	0,			// Slideshow starts playing automatically
		start_slide             :   1,			// Start slide (0 is random)
		stop_loop				:	0,			// Pauses slideshow on last slide
		random					:	0,			// Randomize slide order (Ignores start slide)
		slide_interval          :   5000,		// Length between transitions
		transition				:	2, 			// 0-None, 1-Fade, 2-Slide Top, 3-Slide Right, 4-Slide Bottom, 5-Slide Left, 6-Carousel Right, 7-Carousel Left
		transition_speed		:	600,		// Speed of transition
		new_window				:	1,			// Image links open in new window/tab
		pause_hover             :   0,			// Pause slideshow on hover
		keyboard_nav            :   1,			// Keyboard navigation on/off
		performance				:	1,			// 0-Normal, 1-Hybrid speed/quality, 2-Optimizes image quality, 3-Optimizes transition speed // (Only works for Firefox/IE, not Webkit)
		image_protect			:	1,			// Disables image dragging and right click with Javascript

		// Size & Position						   
		min_width		        :   0,			// Min width allowed (in pixels)
		min_height		        :   0,			// Min height allowed (in pixels)
		vertical_center         :   1,			// Vertically center background
		horizontal_center       :   1,			// Horizontally center background
		fit_always				:	0,			// Image will never exceed browser width or height (Ignores min. dimensions)
		fit_portrait         	:	1,			// Portrait images will not exceed browser height
		fit_landscape			:   0,			// Landscape images will not exceed browser width

		// Components							
		slide_links				:	'blank',	// Individual links for each slide (Options: false, 'num', 'name', 'blank')
		thumb_links				:	0,			// Individual thumb links for each slide
		thumbnail_navigation    :   0,			// Thumbnail navigation
		slides 					:	[			// Slideshow Images
			{ 
					"image" : "https://dhlwelcomepack.it/sites/default/files/surreal/slide/HP_New-Img_2019.jpg",
					"title" : "<h2><span></span>             </h2>",
				}
		],
									
		// Theme Options			   
		progress_bar			:	0,			// Timer for each slide							
		mouse_scrub				:	0

	});
	});        function moveTo(contentArea){
            var goPosition = $(contentArea).offset().top;
            $('html,body').animate({ scrollTop: goPosition}, 'slow');
        }

        $('body').prepend('<div id="loader"><div id="loading"></div></div>');
        $('body').imagesLoaded(function () {
            $('#loader').remove();
        });

//        $("img").lazyload({
//            effect : "fadeIn"
//        });
                    //Main menu configure
            $("nav").sticky({topSpacing:0});
            if (($('#home-landing').length > 0) && (isMobile)){
                //$('.slider-text').css({'position':'relative','top':'0','margin-top':'60px','padding-bottom':'30px'});
                $('.slider-text').css({'position':'relative'});
                $('#home-landing').css('height',+$('#home-landing .container').height()+'px');
                if ($(window).height() < $('#home-landing .container').height()){
                    $('.sticky-wrapper').css('margin-top','0');
                    $('#jq_ez_bg').css('height', $('#home-landing .container').height()+'px');
                }else{
                    $('.slider-text').css({'position':'absolute','top':'60px','margin-top':'0px','padding-bottom':'0px'});
                    $('#home-landing').css('height','100%');
                }
            }

            $('#nav li').each(function(){
                var search = '/';
                var $a = $(this).find('a');
                var href = $a.attr('href');
                if(href.indexOf(search) === 0){
                    href = href.substr(search.length);
                    $a.attr('href', href);
                }
            });

            $(".menu li").not('.disabled').click(function () {
                if (!$(this).hasClass('disabled')){
                    $("html, body").animate({
                        scrollTop: $($(this).find('a').attr("href")).offset().top + "px"
                    }, {
                        duration: 1000,
                        easing: "swing"
                    });
                    return false;
                }
            });
        
        $(window).load(function(){
            //                if (isMobile){
//                    $('.button-5mosse').css('position','absolute').css('top',($(window).height()*0.3)+'px');
//                    $('#footer-home').css('position','relative');
//                }
            
            $('#back-top').hide();
            if($(window).scrollTop() + $(window).height() >= $(document).height()-85) {
                $('#scroll-down').fadeOut();
            }

            $(window).scroll(function () {
                if ($(this).scrollTop() > 300) {
                    $('#back-top').fadeIn();
                } else {
                    $('#back-top').fadeOut();
                }
                if($(window).scrollTop() + $(window).height() >= $(document).height()-85) {
                    $('#scroll-down').fadeOut();
                }else{
                    $('#scroll-down').fadeIn();
                }
            });
            $('#back-top a').click(function () {
                $('body, html').animate({
                    scrollTop: 0
                }, 800);
                return false;
            });
            $('#scroll-down a').click(function () {
                $('body, html').animate({
                    scrollTop: $(window).scrollTop()+$(window).height()-80
                }, 800);
                return false;
            });


            $("ul.slides > li > img").removeAttr('width');
            $("ul.slides > li > img").removeAttr('height');

          if (isMobile == true) {
              $('.header').addClass('mobileHeader');	//add mobile header class
          } else {
              $('.page').addClass('desktop');
              $('.parallax').addClass('fixed-desktop');
          }

            $("#drupal_messages").mouseout(function(){
                $("#drupal_messages").delay(2000).fadeOut('slow');
            });
            new UISearch(document.getElementById('sb-search'));
        });
    </script>
