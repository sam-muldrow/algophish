<script type="text/javascript">
    $(document).ready(function($) {
        var div_ancho = $("div.cuadro").width();
        $('div.cuadro').css({
            'height': div_ancho
        });
        $('div.cuadro-transparente').css({
            'height': div_ancho,
            'background-size': 'cover'
        });

        $(window).resize(function() {
            var div_ancho = $("div.cuadro").width();
            $('div.cuadro').css({
                'height': div_ancho
            });
            $('div.cuadro-transparente').css({
                'height': div_ancho,
                'background-size': 'cover'
            });
        });

        $('#ultron').on('change', function() {
            if (this.value == 1) {
                $('#notultron').removeAttr('disabled', '');
            } else {
                $('#notultron').attr('disabled', 'disabled');
            }
        });
    });

</script>

