	<div class="so_newletter_custom_popup_bg"></div>
	<div class="custom-pop main-newsleter-popup so_newletter_custom_popup so_newletter_oca_popup" id="so_newletter_custom_popup">
	    <div class="so-custom-popup so-custom-oca-popup" style="width: 800px   ; background-image: url(image/catalog/banners/bg-popup.jpg  ) ;  ">
			
			 

			
			<div class="modcontent">
				<div class="oca_popup" id="test-popup">
					<div class="popup-content">
