    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCH2M6LMgrZtaWHPvxSRcGa7td6Pb7bumQ&callback=myMap"></script>
    <script>

        function myMap() {
			var lat = -6.23080;
			var lng = 106.82416;

            var mapProp = {
                center:new google.maps.LatLng(lat, lng),
                zoom:16,
            };

            var map=new google.maps.Map(document.getElementById("googleMap"),mapProp);
            
            var uluru = {lat: lat, lng: lng};
            var marker = new google.maps.Marker({
              position: uluru,
              map: map,
            });
        }
        myMap();
    </script>

    <script>
        window.onscroll = function() {
            myFunction();
        };

        var header = document.getElementById("myHeader");
        var headerlogo = document.getElementById("headerlogo");
        
        function myFunction() {
            var sticky = $("#myHeader").height();
            console.log(sticky);
            
          if (window.pageYOffset > sticky) {
            header.classList.add("sticky");
            headerlogo.classList.add("header-sticky");

            $(".header-tambahan").attr("style","width:100%;height:" + sticky + "px;");
          } else {
            header.classList.remove("sticky");
            headerlogo.classList.remove("header-sticky");
            $(".header-tambahan").attr("style","width:100%;height:0px;");
          }
        }
    </script>
