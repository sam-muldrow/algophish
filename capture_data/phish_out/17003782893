		<script>
function fncReadmore() {
		    $("#longtext").removeClass("parrafContract ");
		    $("#readless").show();
		    $("#readmore").hide();
}
		
function fncReadless() {
    $("#longtext").addClass("parrafContract ");
    $("#readless").hide();
    $("#readmore").show();
}

$(function() {
        $('.container ul .lazy').lazy({
            appendScroll: $('.container ul')
        });

});

function fncReadless(id) {
			    $("#longtext" + id).addClass("parrafContract ");
			    $("#longtext"+id+" #readless").hide();
			    $(".readless" + id).hide();
			    $("#longtext"+id+" #readmore").css('display','inline-block');
			    $(".readmore" + id).css('display','inline-block');
}

function fncReadmore(id) {
			    $("#longtext" + id).removeClass("parrafContract ");
			    $("#longtext"+id+" #readless").css('display','inline-block');
			    $(".readless" + id).css('display','inline-block');
			    $("#longtext"+id+" #readmore").hide();
			    $(".readmore" + id).hide();
}


function scrollElRight(scrollerid) {
	event.preventDefault();
  document.getElementById(scrollerid).scrollBy({  top: 0,  left: 600,  behavior: 'smooth'});

    //Right button checks...
    var newScrollLeft=$("#" + scrollerid).scrollLeft();
    var divWidth = $("#" + scrollerid).outerWidth();
    var divWidthcheck = divWidth + 601;
    var scrollwidth =$("#" + scrollerid).get(0).scrollWidth;
    if(newScrollLeft > scrollwidth - divWidthcheck){
        $("." + scrollerid + " .rightscroller").fadeOut();
      }

    //Left button checks...
    $("." + scrollerid + " .leftscroller").not(':visible').fadeIn();
}

function scrollElLeft(scrollerid) {
event.preventDefault();
  document.getElementById(scrollerid).scrollBy({  top: 0,  left: -600,  behavior: 'smooth'});

    var newScrollLeft=$("#" + scrollerid).scrollLeft();
    var divWidth = $("#" + scrollerid).outerWidth();
    var divWidthcheck = divWidth - 100;
    var scrollwidth =$("#" + scrollerid).get(0).scrollWidth;
    if(newScrollLeft < scrollwidth - divWidthcheck){
        //alert('right end');
        $("." + scrollerid + " .rightscroller").fadeIn();
      }

    //Left button checks...
    var newScrollLeftcheck=$("#" + scrollerid).scrollLeft();
    if(newScrollLeftcheck < 601){
        $("." + scrollerid + " .leftscroller").fadeOut();
      }
}

$("#mainctaid").click(function() {
	event.preventDefault();
    $('html,body').animate({
        scrollTop: $(".start").offset().top},
        'slow');
});

function opentabs(evt, tabName) {
  var i, tabcontent, tablinks;
  tabcontent = document.getElementsByClassName("tabcontent");
  for (i = 0; i < tabcontent.length; i++) {
    tabcontent[i].style.display = "none";
  }
  tablinks = document.getElementsByClassName("tablinks");
  for (i = 0; i < tablinks.length; i++) {
    tablinks[i].className = tablinks[i].className.replace(" activetab", "");
  }
  document.getElementById(tabName).style.display = "block";
  evt.currentTarget.className += " activetab";
}

// Get the element with id="defaultOpen" and click on it
//document.getElementById("defaultOpen").click();

$.fn.isInViewport = function() {
    var elementTop = $(this).offset().top;
    var elementBottom = elementTop + $(this).outerHeight();

    var viewportTop = $(window).scrollTop();
    var viewportBottom = viewportTop + $(window).height();

    return elementBottom > viewportTop && elementTop < viewportBottom;
};
</script>		
		
					
