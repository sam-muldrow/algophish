<script type="text/javascript">

    
$(".mobilesearchtoggle").click(function(event) {
        event.preventDefault();
        $("#searchform").toggle();
        $('body').addClass('no-scroll');
    
    });
    
function makenav(){
	var width = $(window).width();
	
	if(width < 906){
		
		var a_href = $(".nav").attr('href');
		if(a_href != ""){
			$('.nav').each(function(i, obj) {
				$(obj).attr("href", "#");
			});

			$( ".nav" ).click(function(e) {
				e.preventDefault();
				$(this).toggleClass("active");
				$(this).next('.content-menu').slideToggle();
			});
			
		}
			
		var a_href_sub = $(".sub-menu-toggle").attr('href');
		if(a_href_sub != ""){
			$('.sub-menu-toggle').each(function(i, obj) {
				$(obj).attr("href", "#");
			});
	
			$( ".sub-menu-toggle" ).click(function(e) {
				e.preventDefault();
				$(this).toggleClass("active");
				$(this).next('.sub-menu-outer').slideToggle();
			});
			
		}
		
		
	}
	
	
	
}


	
$(document).ready(function(){	
	makenav();
	 $('.lazy').lazy();
});

$(".mobnavtoggle").click(function() {
$(this).html(function(i,h){
return ($("#mainnav").is(':visible') ? 'Show tool ranges <i class="fa fa-caret-down"></i>' : 'Hide tool ranges <i class="fa fa-caret-up"></i>');
});
$("#mainnav").slideToggle();
});


function loadmore (){
	  event.preventDefault();
	  $(".mini-content-outer").toggleClass("all-categorys");
	  $(".list div").animate({ opacity: 1 });
	  //var text = $('.smbtn').text();
     //$('.smbtn').text() == 'Show more' ? 'Close' : 'Email';
    
  $('.smbtn').text($('.smbtn').text() == "Show more" ? "Show less" : "Show more");
 //if(  ($('.smbtn').text() == "Show less")   ){$(".list div").animate({ opacity: 0 });};
;

}

