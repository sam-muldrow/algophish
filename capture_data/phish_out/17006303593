<script>
	["load", "resize"].forEach((event) => {
		window.addEventListener(event, () => {
			if (window.innerWidth >= 1024) {
				// Zjisti výšku <main>
				const height = window.innerHeight -
					document.querySelector("header").offsetHeight -
					document.querySelector("footer.slider-footer").offsetHeight;

				// console.log(document.getElementsByTagName("main").item(0));
				console.log(height);

				// Vyber v mainu a. obrázky a b. div-děti
				const imgs = document.querySelectorAll("main .adc-slider img");
				const divs = document.querySelectorAll("main .adc-slider");

				// Nastav výšku v mainu pro elementy a. obrázky a b. div-děti
				for (let img of imgs) {
					img.style.height = height + 'px';
				}
				for (let div of divs) {
					div.style.height = height + 'px';
				}
				document.querySelector('main').style.maxHeight = height + 'px';
				document.querySelector('main').style.overflowY = 'auto';
			} else {
				document.querySelector('main').style.maxHeight = '';
			}
		})
	});
</script>

