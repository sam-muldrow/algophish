.wp-block-columns[class*="fullwidth-cols"]:not(.is-not-stacked-on-mobile)
    > .wp-block-column:not(:last-child) {
  /* no suggestion */
  margin-bottom: var(--wp--style--block-gap, 1.75rem) !important;
