<script type="text/javascript">
  jQuery(document).ready(function($) {
    $(".profile-content").each(function() {
      $(this).attr('data-toggle', 'modal');
      

      let modal_box = $(this).parents('.ekit-wid-con').find('.zoom-anim-dialog');
      modal_box.addClass('modal');
      modal_box.find('.ekit-team-modal-close').attr('data-dismiss', 'modal');
      let target_id = '#'+modal_box.attr('id');

      $(this).attr('data-target', target_id);
    });
  });
</script>