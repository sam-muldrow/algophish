<script type="text/javascript">
/* Default comment here */ 
jQuery(document).ready(function($){
	function showContent(val) {
		$(".cheque").hide();
		$(".pyanow").hide();
		$(".inter-bank").hide();
		
		if (val == 'Cheque') {
			$(".cheque").show();
		} else if (val == 'Inter-bank') {
			$(".inter-bank").show();
		} else {
			$(".pyanow").show();
		}
	}
	let mySelection = $(".give-dropdown-btn").find('#myselection');
	let myList = $(".give-dropdown-btn").find('ul > li');
	mySelection.on('change', function() {
		let sel_val = $(this).val();
		showContent(sel_val);
		/*
		if (sel_val == 'Cheque') {
			$(".cheque").show();
		} else if (sel_val == 'Inter-bank') {
			$(".inter-bank").show();
		} else {
			$(".pyanow").show();
		} */
	});
	mySelection.trigger('change');
	
	myList.on('click', function() {
		let dataVal = $(this).attr('data-value');
		showContent(dataVal);
	});
	/*
  jQuery("#Inter-bank").click(function(){
    jQuery(".cheque").hide();
	  jQuery(".pyanow").hide();
	  jQuery(".inter-bank").show();
  });
  jQuery("#Cheque").click(function(){
    jQuery(".cheque").show();
	  jQuery(".pyanow").hide();
	  jQuery(".inter-bank").hide();
  });
	jQuery("#paynow").click(function(){
    jQuery(".cheque").hide();
	  jQuery(".pyanow").show();
	  jQuery(".inter-bank").hide();
  });
	*/
});

</script>
